         TITLE 'AMDSABUF    BUFFER SEARCH AND SELECTION ROUTINE        *00001000
                       '                                                00002000
AMDSABUF CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'AMDSABUF  74.107'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @11,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@11                                      01S0001 00013000
         ST    @13,@SA00001+4                                   01S0001 00014000
         LA    @14,@SA00001                                     01S0001 00015000
         ST    @14,8(,@13)                                      01S0001 00016000
         LR    @13,@14                                          01S0001 00017000
*   SAVEFORM(1:4) = 'LBUF';         /* MODULE ID TO SAVE AREA        */ 00018000
*                                                               01S0193 00019000
         MVC   SAVEFORM(4,R13),@CC00957                         01S0193 00020000
*   /*****************************************************************/ 00021000
*   /*                                                               */ 00022000
*   /* CHECK FOR VALID CTIOTYPE - ONLY WORKFILE OR PAGE DATA SET     */ 00023000
*   /* REQUESTS ARE ALLOWED                                          */ 00024000
*   /*                                                               */ 00025000
*   /*****************************************************************/ 00026000
*                                                               01S0194 00027000
*/* D (YES,,NO,%BUFRC4) CTIOTYPE VALID                               */ 00028000
*   IF CTIOWK = OFF & CTIODA = OFF THEN /* VALID REQUEST             */ 00029000
         LH    @10,@CH00756                                     01S0194 00030000
         TM    CTIOWK(@10),B'00110000'                          01S0194 00031000
         BZ    @RT00194                                         01S0194 00032000
*     GO TO NOBUFF;                 /* NO - ERROR                    */ 00033000
*                                                               01S0196 00034000
*   /*****************************************************************/ 00035000
*   /*                                                               */ 00036000
*   /* CHECK FOR SWITCH IN ASID. IF SO, PURGE ALL BUFFERS CONTAINING */ 00037000
*   /* INVALID DATA OR DATA FOR PREVIOUS MEMORY.                     */ 00038000
*   /*                                                               */ 00039000
*   /*****************************************************************/ 00040000
*                                                               01S0196 00041000
*/* D (YES,,NO,%BUFASID) ASID DIFFERENT FROM LAST ENTRY              */ 00042000
*/* P SAVE ASID                                                      */ 00043000
*/* P OBTAIN FIRST BCT FROM BCTHFRST                                 */ 00044000
*/*%BUFPRG1: D (NO,,YES,%BUFNEXT) BCTASM SET                         */ 00045000
*/* D (NO,%BUFRSET,YES,) BCTWKFLE SET                                */ 00046000
*/* D (NO,%BUFNEXT,YES,) BCTIOERR SET                                */ 00047000
*/*%BUFRSET: P SET BCTASID TO 0                                      */ 00048000
*/* P SET BCTVADDR TO 0                                              */ 00049000
*/* P RESET BCTFLGT, BCTFLGI TO 0                                    */ 00050000
*/*%BUFNEXT: D (NO,,YES,%BUFASID) PROCESSING LAST BCT                */ 00051000
*/* P (,%BUFPRG1) GET NEXT BCT                                       */ 00052000
*   IF VCTASID ^= BUFASID THEN      /* NO - ASID SWITCH              */ 00053000
@RF00194 CLC   VCTASID(2,VCCTREG),BUFASID                       01S0196 00054000
         BE    @RF00196                                         01S0196 00055000
*     DO;                           /* YES - PURGE BUFFERS RELATED TO   00056000
*                                      PREVIOUS MEMORY               */ 00057000
*       BUFASID = VCTASID;          /* SAVE NEW ASID                 */ 00058000
         MVC   BUFASID(2),VCTASID(VCCTREG)                      01S0198 00059000
*       DO BUFBCTAD = BCTHFRST TO BCTHLAST BY LENGTH(BCT); /* PURGE     00060000
*                                      BUFFERS                       */ 00061000
         L     @10,VCTBCTH(,VCCTREG)                            01S0199 00062000
         L     BUFBCTAD,BCTHFRST(,@10)                          01S0199 00063000
         B     @DE00199                                         01S0199 00064000
@DL00199 DS    0H                                               01S0200 00065000
*         IF (BCTASM = OFF & BCTWKFLE = OFF) | BCTIOERR = ON THEN /*    00066000
*                                      IS THIS AN INVALID OR    01S0200 00067000
*                                      MEMORY-RELATED BUFFER         */ 00068000
         TM    BCTASM(BUFBCTAD),B'01010000'                     01S0200 00069000
         BZ    @RT00200                                         01S0200 00070000
         TM    BCTIOERR(BUFBCTAD),B'00100000'                   01S0200 00071000
         BNO   @RF00200                                         01S0200 00072000
@RT00200 DS    0H                                               01S0201 00073000
*           DO;                     /* YES - FREE BUFFER             */ 00074000
*             BCTASID = 0;          /* RESET ASID                    */ 00075000
         SR    @10,@10                                          01S0202 00076000
         STH   @10,BCTASID(,BUFBCTAD)                           01S0202 00077000
*             BCTVADDR = 0;         /* RESET VIRTUAL ADDRESS         */ 00078000
         ST    @10,BCTVADDR(,BUFBCTAD)                          01S0203 00079000
*             BCTFLGT = OFF;        /* RESET BUFFER TYPE FLAGS       */ 00080000
         MVI   BCTFLGT(BUFBCTAD),B'00000000'                    01S0204 00081000
*             BCTFLGI = OFF;        /* RESET BUFFER I/O FLAGS        */ 00082000
         MVI   BCTFLGI(BUFBCTAD),B'00000000'                    01S0205 00083000
*           END;                                                01S0206 00084000
*       END;                        /* CONTINUE BUFFER PURGE         */ 00085000
@RF00200 AH    BUFBCTAD,@CH00974                                01S0207 00086000
@DE00199 L     @10,VCTBCTH(,VCCTREG)                            01S0207 00087000
         C     BUFBCTAD,BCTHLAST(,@10)                          01S0207 00088000
         BNH   @DL00199                                         01S0207 00089000
*     END;                                                      01S0208 00090000
*                                                               01S0209 00091000
*   /*****************************************************************/ 00092000
*   /*                                                               */ 00093000
*   /* SEE IF ANY BUFFER CONTAINS REQUESTED ADDRESS. SKIP PROCESSING */ 00094000
*   /* IF PAGE TABLE BUFFER IS REQUESTED SINCE THESE ONLY OCCUR ON   */ 00095000
*   /* SWITCH OF ASID. PROCESSING IS ALSO BYPASSED IF CURRENT        */ 00096000
*   /* INTERRUPT CODE IS FOR SEGMENT EXCEPTION.                      */ 00097000
*   /*                                                               */ 00098000
*   /*****************************************************************/ 00099000
*                                                               01S0209 00100000
*/*%BUFASID: D (NO,,YES,%BUFLOOP) CTPGT ON                           */ 00101000
*/* D (NO,,YES,%BUFLOOP) PROCESSING SEGMENT EXCEPTION                */ 00102000
*/* P OBTAIN BCTH FROM VCTBCTH                                       */ 00103000
*/* P GET FIRST BCT FROM BCTHFRST                                    */ 00104000
*/*%BUFIN1: D (NO,%BUFFUL2,YES,) BCTVADDR = FLCTEAA                  */ 00105000
*/* D (NO,,YES,%BUFFUL1) CTASM ON                                    */ 00106000
*/* D (NO,,YES,%BUFFUL2) BCTASM ON                                   */ 00107000
*/*%BUFRC8: P SET UP RC = 8                                          */ 00108000
*/* P PLACE ACTIVE MDCB ADDRESS IN BCT                               */ 00109000
*/* P (,BUFRET1) SET BCTCOMP                                         */ 00110000
*/*%BUFFUL1: D (YES,%BUFRC8,NO,%BUFFUL2) BCTASM ON                   */ 00111000
*/*%BUFFUL2: D (NO,,YES,%BUFLOOP) PROCESSING LAST BCT                */ 00112000
*/* P (,%BUFIN1) GET NEXT BCT                                        */ 00113000
*   IF CTPGT = OFF & MDCBPI ^= SEGEXCPT THEN /* PAGE TABLE REQUEST   */ 00114000
@RF00196 LH    @10,@CH00756                                     01S0209 00115000
         TM    CTPGT(@10),B'00100000'                           01S0209 00116000
         BNZ   @RF00209                                         01S0209 00117000
         L     @10,VCTMDCE(,VCCTREG)                            01S0209 00118000
         L     @10,MDCEACTV(,@10)                               01S0209 00119000
         CLC   MDCBPI(2,@10),@CH00953                           01S0209 00120000
         BE    @RF00209                                         01S0209 00121000
*     DO BUFBCTAD = BCTHFRST TO BCTHLAST BY LENGTH(BCT); /* NO -        00122000
*                                      CHECK FOR ADDRESS IN A BUFFER */ 00123000
         L     @10,VCTBCTH(,VCCTREG)                            01S0210 00124000
         L     BUFBCTAD,BCTHFRST(,@10)                          01S0210 00125000
         B     @DE00210                                         01S0210 00126000
@DL00210 DS    0H                                               01S0211 00127000
*       IF BCTVADDR = FLCTEA THEN   /* VIRTUAL ADDRESS IN BUFFER     */ 00128000
         CLC   BCTVADDR(4,BUFBCTAD),FLCTEA                      01S0211 00129000
         BNE   @RF00211                                         01S0211 00130000
*         IF (CTASM = ON & BCTASM = ON) |                       01S0212 00131000
*           (CTASM = OFF & BCTASM = OFF) THEN /* YES-ENSURE THIS IS ASM 00132000
*                                      BUFFER ONLY IF ASM REQUESTED  */ 00133000
         LH    @10,@CH00756                                     01S0212 00134000
         TM    CTASM(@10),B'00010000'                           01S0212 00135000
         BNO   @GL00003                                         01S0212 00136000
         TM    BCTASM(BUFBCTAD),B'00010000'                     01S0212 00137000
         BO    @RT00212                                         01S0212 00138000
@GL00003 LH    @10,@CH00756                                     01S0212 00139000
         TM    CTASM(@10),B'00010000'                           01S0212 00140000
         BNZ   @RF00212                                         01S0212 00141000
         TM    BCTASM(BUFBCTAD),B'00010000'                     01S0212 00142000
         BNZ   @RF00212                                         01S0212 00143000
@RT00212 DS    0H                                               01S0213 00144000
*           DO;                     /* ADDRESS IS IN BUFFER          */ 00145000
*             SAVEBACK -> SAVER15 = ADINBUFR; /* INDICATE ADDRESS IN    00146000
*                                      BUFFER                        */ 00147000
         L     @10,SAVEBACK(,R13)                               01S0214 00148000
         MVC   SAVER15(4,@10),@CF00058                          01S0214 00149000
*             BCTMDCB = MDCEACTV;   /* GET MDCB ADDRESS              */ 00150000
         L     @10,VCTMDCE(,VCCTREG)                            01S0215 00151000
         MVC   BCTMDCB(4,BUFBCTAD),MDCEACTV(@10)                01S0215 00152000
*             BCTCOMP = ON;         /* INDICATE BUFFER IS FULL       */ 00153000
         OI    BCTCOMP(BUFBCTAD),B'10000000'                    01S0216 00154000
*             GO TO BUFRET1;        /* PREPARE TO EXIT               */ 00155000
         B     BUFRET1                                          01S0217 00156000
*           END;                                                01S0218 00157000
*     END;                          /* CONTINUE BUFFER SEARCH        */ 00158000
@RF00212 DS    0H                                               01S0219 00159000
@RF00211 AH    BUFBCTAD,@CH00974                                01S0219 00160000
@DE00210 L     @10,VCTBCTH(,VCCTREG)                            01S0219 00161000
         C     BUFBCTAD,BCTHLAST(,@10)                          01S0219 00162000
         BNH   @DL00210                                         01S0219 00163000
*                                                               01S0220 00164000
*   /*****************************************************************/ 00165000
*   /*                                                               */ 00166000
*   /* ADDRESS NOT IN ANY BUFFER. INITIALIZE FOR BUFFER SEARCH AND   */ 00167000
*   /* SELECTION. REPEAT BUFFER SEARCH 10 TIMES IF BUFFER CANNOT BE  */ 00168000
*   /* FOUND AND I/O IS ACTIVE ON THE OUTPUT QUEUE.                  */ 00169000
*   /*                                                               */ 00170000
*   /*****************************************************************/ 00171000
*                                                               01S0220 00172000
*/*%BUFLOOP: P INITIALIZE LOOP COUNTER                               */ 00173000
*/*BUFSRCH: P  INITIALIZE SEARCH PRIORITY                            */ 00174000
*/* N ORDER OF PRIORITY IS WKFLE, ASM, LSQA, PGT, DUMP               */ 00175000
*/* P RESET PROCESSED FLAGS                                          */ 00176000
*/* P RESET CHOSEN BUFFER ADDRESS                                    */ 00177000
*   DO I = 1 TO NUMWAIT;            /* REPEAT BUFFER SEARCH 10 TIMES */ 00178000
@RF00209 LA    I,1                                              01S0220 00179000
@DL00220 DS    0H                                               01S0221 00180000
*     BUFPRTY = 'FF'X;              /* INITIALIZE BUFFER PRIORITY FOR   00181000
*                                      SEARCH                        */ 00182000
         LA    BUFPRTY,255                                      01S0221 00183000
*     BUFFIRST = OFF;               /* INDICATE BCTHFRST HAS NOT BEEN   00184000
*                                      PROCESSED                     */ 00185000
         NI    BUFFIRST,B'01111111'                             01S0222 00186000
*     BUFCHSEN = 0;                 /* RESET CHOSEN BUFFER ADDRESS   */ 00187000
         SR    BUFCHSEN,BUFCHSEN                                01S0223 00188000
*                                                               01S0224 00189000
*   /*****************************************************************/ 00190000
*   /*                                                               */ 00191000
*   /* SEARCH BCTS FOR EMPTY BUFFER OR LOWEST PRIORITY, NON-ALLOCATED*/ 00192000
*   /* BUFFER IF NONE ARE EMPTY.  SEARCH IN CIRCULAR MANNER BEGINNING*/ 00193000
*   /* THE BUFFER FOLLOWING THE CURRENT BUFFER.                      */ 00194000
*   /*                                                               */ 00195000
*   /*****************************************************************/ 00196000
*                                                               01S0224 00197000
*/*%BUFSR1: P GET VCTBCTC (INITIALIZED TO BCTHFRST)                  */ 00198000
*/*%BUFSR2: D (YES,,NO,%BUFSR1A) PROCESSING BCTHFRST                 */ 00199000
*/* P INDICATE BCTHFRST PROCESSED                                    */ 00200000
*/*%BUFSR1A: D (NO,,YES,%BUFSR3) BCTALLOC ON                         */ 00201000
*/* D (NO,,YES,%BUFSR3) BCTPGTF ON                                   */ 00202000
*/* D (NO,,YES,BUFFOUND) BCTCOMP OFF                                 */ 00203000
*/* D (NO,,YES,%BUFSR3) BUFFER PRIORITY > SEARCH PRIORITY            */ 00204000
*/* P SET SEARCH TO LOWER PRIORITY                                   */ 00205000
*/* P SAVE ADDR OF BUFFER                                            */ 00206000
*/*%BUFSR3: D (NO,,YES,%BUFSR4) REACHED BCTHLAST                     */ 00207000
*/* P (,%BUFSR2) POINT TO NEXT BCT                                   */ 00208000
*/*%BUFSR4: D (NO,,YES,BUFNONE) PROCESSED BCTHFRST YET               */ 00209000
*/* P (,%BUFSR2) POINT TO BCTHFRST                                   */ 00210000
*     IF VCTBCTC ^= BCTHLAST THEN   /* IS LAST BUFFER CURRENT        */ 00211000
         L     @10,VCTBCTC(,VCCTREG)                            01S0224 00212000
         L     @02,VCTBCTH(,VCCTREG)                            01S0224 00213000
         C     @10,BCTHLAST(,@02)                               01S0224 00214000
         BE    @RF00224                                         01S0224 00215000
*       BUFBCTAD = VCTBCTC + LENGTH(BCT); /* NO - CHOOSE BUFFER 01S0225 00216000
*                                      FOLLOWING CURRENT             */ 00217000
         AH    @10,@CH00974                                     01S0225 00218000
         LR    BUFBCTAD,@10                                     01S0225 00219000
*     ELSE                                                      01S0226 00220000
*       BUFBCTAD = BCTHFRST;        /* YES - BEGIN SEARCH AT FIRST      00221000
*                                      BUFFER                        */ 00222000
         B     @RC00224                                         01S0226 00223000
@RF00224 L     @10,VCTBCTH(,VCCTREG)                            01S0226 00224000
         L     BUFBCTAD,BCTHFRST(,@10)                          01S0226 00225000
*     DO BUFBCTAD BY LENGTH(BCT);   /* BEGIN SEARCH                  */ 00226000
@RC00224 DS    0H                                               01S0227 00227000
@DL00227 DS    0H                                               01S0228 00228000
*       IF BUFBCTAD = BCTHFRST THEN /* PROCESSING BCTHFRST           */ 00229000
         L     @10,VCTBCTH(,VCCTREG)                            01S0228 00230000
         C     BUFBCTAD,BCTHFRST(,@10)                          01S0228 00231000
         BNE   @RF00228                                         01S0228 00232000
*         BUFFIRST = ON;            /* YES - INDICATE BCTHFRST  01S0229 00233000
*                                      PROCESSED                     */ 00234000
         OI    BUFFIRST,B'10000000'                             01S0229 00235000
*       IF (BCTALLOC = OFF) & (BCTPGTF = OFF) THEN /* BUFFER ALLOCATED  00236000
*                                      FOR I/O ALREADY, OR CONTAINS     00237000
*                                      FIXED PAGE TABLES             */ 00238000
@RF00228 TM    BCTALLOC(BUFBCTAD),B'01000000'                   01S0230 00239000
         BNZ   @RF00230                                         01S0230 00240000
         TM    BCTPGTF(BUFBCTAD),B'00000010'                    01S0230 00241000
         BNZ   @RF00230                                         01S0230 00242000
*         DO;                       /* NO -                          */ 00243000
*           IF BCTCOMP = OFF THEN   /* CURRENT BUFFER EMPTY          */ 00244000
         TM    BCTCOMP(BUFBCTAD),B'10000000'                    01S0232 00245000
         BNZ   @RF00232                                         01S0232 00246000
*             DO;                   /* YES -                         */ 00247000
*               BUFCHSEN = BUFBCTAD; /* GET CHOSEN BUFFER ADDRESS    */ 00248000
         LR    BUFCHSEN,BUFBCTAD                                01S0234 00249000
*               GO TO BUFFOUND;     /* USE THIS BUFFER               */ 00250000
         B     BUFFOUND                                         01S0235 00251000
*             END;                                              01S0236 00252000
*           IF BCTFLGT < BUFPRTY THEN /* NO-IS IT LOWER PRIORITY THAN   00253000
*                                      SEARCH                        */ 00254000
@RF00232 SR    @10,@10                                          01S0237 00255000
         IC    @10,BCTFLGT(,BUFBCTAD)                           01S0237 00256000
         CR    @10,BUFPRTY                                      01S0237 00257000
         BNL   @RF00237                                         01S0237 00258000
*             DO;                   /* BUFFER IS AVAILABLE FOR  01S0238 00259000
*                                      SELECTION                     */ 00260000
*               BUFPRTY = BCTFLGT;  /* RESET SEARCH PRIORITY TO 01S0239 00261000
*                                      LOWER VALUE                   */ 00262000
         LR    BUFPRTY,@10                                      01S0239 00263000
*               BUFCHSEN = BUFBCTAD; /* SAVE BCT ADDRESS CHOSEN      */ 00264000
         LR    BUFCHSEN,BUFBCTAD                                01S0240 00265000
*             END;                                              01S0241 00266000
*         END;                                                  01S0242 00267000
@RF00237 DS    0H                                               01S0243 00268000
*       IF BUFBCTAD = BCTHLAST THEN /* PROCESSING LAST BCT           */ 00269000
@RF00230 L     @10,VCTBCTH(,VCCTREG)                            01S0243 00270000
         C     BUFBCTAD,BCTHLAST(,@10)                          01S0243 00271000
         BNE   @RF00243                                         01S0243 00272000
*         IF BUFFIRST = ON THEN     /* YES - PROCESSED FIRST BCT YET */ 00273000
         TM    BUFFIRST,B'10000000'                             01S0244 00274000
         BO    @RT00244                                         01S0244 00275000
*           GO TO BUFNONE;          /* YES - GET OUT OF LOOP         */ 00276000
*         ELSE                                                  01S0246 00277000
*           BUFBCTAD = BCTHFRST - LENGTH(BCT); /* POINT IN FRONT OF     00278000
*                                      FIRST BCT - LOOP CONTROL WILL    00279000
*                                      UPDATE POINTER TO BCT         */ 00280000
         L     BUFBCTAD,BCTHFRST(,@10)                          01S0246 00281000
         SH    BUFBCTAD,@CH00974                                01S0246 00282000
*     END;                          /* CONTINUE BUFFER SEARCH        */ 00283000
@RF00243 AH    BUFBCTAD,@CH00974                                01S0247 00284000
         B     @DL00227                                         01S0247 00285000
*                                                               01S0248 00286000
*   /*****************************************************************/ 00287000
*   /*                                                               */ 00288000
*   /* FINISHED SEARCH OF BUFFERS. CHECK IF ANY BUFFER WAS FOUND. IF */ 00289000
*   /* A BUFFER WAS FOUND, GO INITIALIZE IT.                         */ 00290000
*   /*                                                               */ 00291000
*   /*****************************************************************/ 00292000
*                                                               01S0248 00293000
*/*BUFNONE: P RESET PROCESSED FLAGS                                  */ 00294000
*/* D (NO,,YES,BUFFOUND) WAS A BUFFER CHOSEN                         */ 00295000
*                                                               01S0248 00296000
*BUFNONE:                                                       01S0248 00297000
*     BUFFIRST = OFF;               /* RESET BCTHFRST PROCESSED FLAG */ 00298000
BUFNONE  NI    BUFFIRST,B'01111111'                             01S0248 00299000
*     IF BUFCHSEN ^= 0 THEN         /* WAS A BUFFER CHOSEN           */ 00300000
         SR    @10,@10                                          01S0249 00301000
         CR    BUFCHSEN,@10                                     01S0249 00302000
         BNE   @RT00249                                         01S0249 00303000
*       GO TO BUFFOUND;             /* YES - INITIALIZE CHOSEN BCT   */ 00304000
*                                                               01S0250 00305000
*   /*****************************************************************/ 00306000
*   /*                                                               */ 00307000
*   /* NO BUFFERS ARE AVAILABLE. SEE IF ANY BUFFERS ARE QUEUED FOR   */ 00308000
*   /* OUTPUT. IF SO, CALL AMDSAWAT TO FREE A BUFFER FOR WHICH OUTPUT*/ 00309000
*   /* I/O HAS COMPLETED. AND REPEAT BUFFER SEARCH. OTHERWISE, SET UP*/ 00310000
*   /* A RETURN CODE OF 4 AND EXIT SINCE NO BUFFERS ARE AVAILABLE.   */ 00311000
*   /*                                                               */ 00312000
*   /*****************************************************************/ 00313000
*                                                               01S0251 00314000
*/*BUFFREE: D (NO,,YES,%BUFRC4) OUTPUT IODBBCT = 0                   */ 00315000
*/* P SAVE CTIOTYPE                                                  */ 00316000
*/* P SET CTIOOUT IN CTIOTYPE                                        */ 00317000
*/* P SET CTIOINT IN CTIOTYPE                                        */ 00318000
*/* L AMDSAWAT:  FREE COMPLETED OUTPUT BUFFERS                       */ 00319000
*/* P RESTORE CTIOTYPE                                               */ 00320000
*/* P INCREMENT WAIT COUNT                                           */ 00321000
*/* D (YES,,NO,BUFSRCH) MAX. NO. OF WAITS                            */ 00322000
*     RESPECIFY IODB BASED(ADDR(IODBOUT));                      01S0251 00323000
*     IF IODBBCT ^= 0 THEN          /* ANY BUFFERS QUEUED FOR OUTPUT */ 00324000
         L     @04,@CA00930                                     01S0252 00325000
         C     @10,IODBBCT(,@04)                                01S0252 00326000
         BE    @RF00252                                         01S0252 00327000
*       DO;                         /* YES - PREPARE TO FREE A BUFFER*/ 00328000
*         BUFCTIO = CTIOTYPE;       /* SAVE I/O TYPE REQUEST         */ 00329000
         LH    @10,@CH00756                                     01S0254 00330000
         SR    BUFCTIO,BUFCTIO                                  01S0254 00331000
         IC    BUFCTIO,CTIOTYPE(,@10)                           01S0254 00332000
*         CTIOTYPE = 0;             /* RESET I/O REQUEST FLAGS       */ 00333000
         MVI   CTIOTYPE(@10),X'00'                              01S0255 00334000
*         CTIOOUT = ON;             /* REQUEST OUTPUT DEVICE BE 01S0256 00335000
*                                      CHECKED                       */ 00336000
*         CTIOINT = ON;             /* REQUEST RETURN ON FIRST  01S0257 00337000
*                                      INTERRUPT                     */ 00338000
         OI    CTIOOUT(@10),B'01000100'                         01S0257 00339000
*         PFBUFWAT = PFBUFWAT + 1;  /* INCREMENT COUNTER - NUMBER OF    00340000
*                                      TIMES BUF HAD TO WAIT FOR        00341000
*                                      BUFFERS                       */ 00342000
*                                                               01S0258 00343000
         LA    @10,1                                            01S0258 00344000
         AH    @10,PFBUFWAT                                     01S0258 00345000
         STH   @10,PFBUFWAT                                     01S0258 00346000
*         SVC(WATSVC);              /* CALL AMDSAWAT TO FREE AN 01S0259 00347000
*                                      OUTPUT BUFFER                 */ 00348000
         SVC   1                                                01S0259 00349000
*         CTIOTYPE = BUFCTIO;       /* RESTORE ORIGINAL I/O REQUEST  */ 00350000
         LH    @10,@CH00756                                     01S0260 00351000
         STC   BUFCTIO,CTIOTYPE(,@10)                           01S0260 00352000
*       END;                                                    01S0261 00353000
*     ELSE                                                      01S0262 00354000
*       GO TO NOBUFF;               /* ERROR - NO WORK ON OUTPUT        00355000
*                                      QUEUE                         */ 00356000
*   END;                                                        01S0263 00357000
*                                                               01S0263 00358000
         AH    I,@CH00036                                       01S0263 00359000
         CH    I,@CH00778                                       01S0263 00360000
         BNH   @DL00220                                         01S0263 00361000
*   /*****************************************************************/ 00362000
*   /*                                                               */ 00363000
*   /* WAITED 10 TIMES FOR BUFFERS TO BE FREED BUT NONE WERE. SET    */ 00364000
*   /* ERROR RETURN CODE AND EXIT.                                   */ 00365000
*   /*                                                               */ 00366000
*   /*****************************************************************/ 00367000
*/*%BUFRC4: P SET UP RC = 4                                          */ 00368000
*/* R RETURN TO CALLER                                               */ 00369000
*                                                               01S0264 00370000
*                                                               01S0264 00371000
*NOBUFF:                                                        01S0264 00372000
*   SAVEBACK -> SAVER15 = NOBUFFRS; /* SET UP RETURN CODE            */ 00373000
NOBUFF   L     @10,SAVEBACK(,R13)                               01S0264 00374000
         MVC   SAVER15(4,@10),@CF00038                          01S0264 00375000
*   RETURN;                         /* RETURN TO CALLER              */ 00376000
@EL00001 L     @13,4(,@13)                                      01S0265 00377000
