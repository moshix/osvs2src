*     DO;                           /* YES - UPDATE PTRT             */ 00435000
*       RESPECIFY BCT BASED(BUFBCTAD);                          01S0274 00436000
*       DO BUFBCTAD = BCTHFRST TO BCTHLAST BY LENGTH(BCT); /* SEARCH AL 00437000
*                                      BCTS FOR BCTPGTE              */ 00438000
