   LM R11,R14,LOWGRS+44 RESTORE THE CONSOLE REGISTERS                   00384000
*                                                                  0272 00385000
*   /*****************************************************************/ 00386000
*   /*                                                               */ 00387000
*   /* DETERMINE STATUS OF THE CONSOLE I/O OPERATION. IF THE I/O     */ 00388000
*   /* COMPLETED SUCCESSFULLY RETURN TO THE CALLER. IF AN ERROR      */ 00389000
*   /* OCCURRED, SET UP FOR ERROR RECOVERY                           */ 00390000
*   /*                                                               */ 00391000
*   /*****************************************************************/ 00392000
*/* P IODBDEV = R10 (SAVE DEVICE ADDRESS)                            */ 00393000
*/* P CTWAIT = 0 SET ZERO ERROR CODE                                 */ 00394000
*/* P SAVE FLCCAW IN IODBCAW FOR ERROR RECOVERY                      */ 00395000
*/* D (YES,CONRETN,NO,) RETURN CODE FROM CON = 0?                    */ 00396000
*                                                                  0272 00397000
*   IODBDEV = R10;                                                 0272 00398000
         L     @07,@CA00924                                        0272 00399000
         STH   R10,IODBDEV(,@07)                                   0272 00400000
*   R12 -> CTWAIT = 0;              /* SET UP CTWAIT FOR NO ERROR  0273 00401000
*                                      RECOVERY REQUIRED             */ 00402000
         MVI   CTWAIT(R12),X'00'                                   0273 00403000
*   IODBCAW = FLCCAW;               /* SAVE CAW IF AN ERROR OCCURRED */ 00404000
         MVC   IODBCAW(4,@07),FLCCAW                               0274 00405000
*   IF RETCODE = 0 THEN                                            0275 00406000
         LTR   RETCODE,RETCODE                                     0275 00407000
         BZ    @RT00275                                            0275 00408000
*     GO TO CONIORET;               /* RETURN TO CALLER IF I/O TO  0276 00409000
*                                      CONSOLE WENT AS NORMAL        */ 00410000
*                                                                  0277 00411000
*   /*****************************************************************/ 00412000
*   /*                                                               */ 00413000
*   /* AN I/O ERROR OCCURRED, DETERMINE IF IT IS CORRECTABLE OR NOT, */ 00414000
*   /* THEN TAKE THE STEPS TO CORRECT OR BYPASS THE ERROR            */ 00415000
*   /*                                                               */ 00416000
*   /*****************************************************************/ 00417000
*/* P CTWAIT = INPUT MSGREQ VALUE (MSG ID)                           */ 00418000
*/* D (YES,CONERREC,NO,CONRCBE) RETURN CODE = 8?                     */ 00419000
*/*CONERREC: P SAVE FLCCSW IN IODBCSW                                */ 00420000
*                                                                  0277 00421000
*   R12 -> CTWAIT = MSGREQ;         /* SAVE THE ERROR CODE IF      0277 00422000
*                                      UNRECOVERABLE                 */ 00423000
         STC   MSGREQ,CTWAIT(,R12)                                 0277 00424000
*   IF RETCODE ^= 8 THEN                                           0278 00425000
         CH    RETCODE,@CH00037                                    0278 00426000
         BE    @RF00278                                            0278 00427000
*     RETURN TO CONRCBE;            /* IF TERMINAL ERROR EXIT WITH NO   00428000
*                                      RETRIES                       */ 00429000
         LA    @14,CONRCBE                                         0279 00430000
         BR    @14                                                 0279 00431000
*   RESPECIFY (R10,R11,R12,R14,R15) UNRESTRICTED;                  0280 00432000
