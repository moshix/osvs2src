         TITLE 'AMDSAGTF    GTF VIRTUAL BUFFER DUMP MODULE             *00001000
                       '                                                00002000
AMDSAGTF CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'AMDSAGTF  73.230'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @11,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@11                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
*   SAVEFORM(1:4) = GTFHEAD;        /* MOVE MODULE ID TO SAVE AREA   */ 00018000
         MVC   SAVEFORM(4,R13),GTFHEAD                             0208 00019000
*   RCBNEXT = VCTRCB;               /* LINK TO RCB CHAIN             */ 00020000
         LA    @10,GTFRCB                                          0209 00021000
         MVC   RCBNEXT(4,@10),VCTRCB(VCCTREG)                      0209 00022000
*   RCBEXIT = ADDR(GTFEXIT1);       /* POINT TO ERROR EXIT           */ 00023000
         LA    @15,GTFEXIT1                                        0210 00024000
         ST    @15,RCBEXIT(,@10)                                   0210 00025000
*   RCBSAVE = R13;                  /* SAVE AREA ADDR TO RCB         */ 00026000
         ST    R13,RCBSAVE(,@10)                                   0211 00027000
*   RCBBASE = BASEREG;              /* BASE ADDRESS TO RCB           */ 00028000
         ST    BASEREG,RCBBASE(,@10)                               0212 00029000
*   GENERATE REFS(R1) SETS(GTFSTOR) (STCTL R1,R1,GTFSTOR); /* SEGMENT   00030000
*                                      TABLE ORIGIN TO RCB           */ 00031000
         STCTL R1,R1,GTFSTOR                                            00032000
*   VCTRCB = ADDR(GTFRCB);          /* MAKE RCB ACTIVE               */ 00033000
         LA    @10,GTFRCB                                          0214 00034000
         ST    @10,VCTRCB(,VCCTREG)                                0214 00035000
*   SAVEBACK -> SAVER15 = 0;        /* SET UP RETURN CODE OF 0       */ 00036000
*                                                                  0215 00037000
         SR    @10,@10                                             0215 00038000
         L     @01,SAVEBACK(,R13)                                  0215 00039000
         ST    @10,SAVER15(,@01)                                   0215 00040000
*   /*****************************************************************/ 00041000
*   /*                                                               */ 00042000
*   /* CHECK FOR PROCESSING OR FIRST PASS COMPLETE                   */ 00043000
*   /*                                                               */ 00044000
*   /*****************************************************************/ 00045000
*                                                                  0216 00046000
*/* D (NO,,YES,GTFRET1) PROCESSING COMPLETE                          */ 00047000
*/* D (NO,,YES,GTFENT2) FIRST PASS COMPLETE                          */ 00048000
*   IF GTFCOMPF = ON THEN           /* PROCESSING COMPLETE           */ 00049000
         TM    GTFCOMPF,B'10000000'                                0216 00050000
         BO    @RT00216                                            0216 00051000
*     GO TO GTFRET1;                /* YES - TAKE RETURN PATH        */ 00052000
*   IF GTFPASS1 = ON THEN           /* FIRST PASS COMPLETE           */ 00053000
         TM    GTFPASS1,B'01000000'                                0218 00054000
         BO    @RT00218                                            0218 00055000
*     GO TO GTFENT2;                /* YES - SKIP INITIALIZATION     */ 00056000
*                                                                  0220 00057000
*   /*****************************************************************/ 00058000
*   /*                                                               */ 00059000
*   /* INITIALIZATION PASS IS REQUIRED TO DETERMINE THE GTF MEMORY.  */ 00060000
*   /* VALIDIITY CHECK THE CURRENT MCCE POINTED TO BY MCHEAD. MCHEAD */ 00061000
*   /* IS POINTED TO BY THE CVT.                                     */ 00062000
*   /*                                                               */ 00063000
*   /*****************************************************************/ 00064000
*                                                                  0220 00065000
*/* P OBTAIN CVT ADDRESS FROM X'10' (FLCCVT)                         */ 00066000
*/* P OBTAIN MCHEAD ADDRESS FROM CVTGTF                              */ 00067000
*/* D (NO,,YES,%GTFCOMP) MCHCUR ADDR IN MCHEAD = 0                   */ 00068000
*/* P SET VCTCBADR TO MCHCUR TO CHECK MCCE                           */ 00069000
*/* P SET VCTCBID TO MCCE ID                                         */ 00070000
*/* L AMDSAVCK: VALIDITY CHECK MCCE                                  */ 00071000
*/* D (YES,,NO,GTFEXIT1) RC = 0                                      */ 00072000
*   RESPECIFY MCCE BASED(MCHCUR);                                  0220 00073000
*   IF MCHCUR = 0 THEN              /* IS THERE A CURRENT MCCE       */ 00074000
