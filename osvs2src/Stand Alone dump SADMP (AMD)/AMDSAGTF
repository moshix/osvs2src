*   DO I = 1 TO NUMMCQE;            /* SEARCH ALL MCQES              */ 00120000
         LA    I,1                                                 0232 00121000
@DL00232 DS    0H                                                  0233 00122000
*     VCTCBADR = GTFMCQEA;          /* MCQE ADDRESS FOR VALIDITY   0233 00123000
*                                      CHECK                         */ 00124000
         MVC   VCTCBADR(4,VCCTREG),GTFMCQEA                        0233 00125000
*     VCTCBIDN = VMCQEID;           /* MCQE CONTROL BLOCK ID         */ 00126000
