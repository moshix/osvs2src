*         BINLOOP = ON;             /* INDICATE SECOND HALF PROCESSED*/ 00894000
         OI    BINLOOP,B'10000000'                              01S0466 00895000
*         BINWORK = BINICHAR & SECNHALF; /*PICK UP REMAINDER OF BYTE */ 00896000
