         TITLE 'AMDSAPGE  VIRTUAL DUMP INITIALIZATION AND CONTROL      *00001000
                        '                                               00002000
AMDSAPGE CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                          01S0001 00008000
         DC    C'AMDSAPGE  77.011'                              01S0001 00009000
AMDSABIN DS    0H                                               01S0001 00010000
         USING *,@15                                            01S0001 00011000
         B     @PROLOG                                          01S0001 00012000
         DC    AL2(@EP00450-AMDSABIN)                                   00013000
         ENTRY AMDSABIN                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00016000
         BALR  @11,0                                            01S0001 00017000
@PSTART  LA    @10,4095(,@11)                                   01S0001 00018000
         USING @PSTART,@11                                      01S0001 00019000
         USING @PSTART+4095,@10                                 01S0001 00020000
