*   /*                                                               */ 00819000
*   /* BINARY TO EBCDIC CONVERT ROUTINE - RECEIVES CONTROL VIA SVC   */ 00820000
*   /* 28. INPUT IS PARAMETER LIST POINTED TO BY REGISTER 1. FORMAT  */ 00821000
*   /* OF PARAMETER LIST IS 0 - ADDRESS OF BINARY AREA, 4 - ADDRESS  */ 00822000
*   /* OF EBCDIC OUTPUT AREA, 8 - 2 BYTE LENGTH OF BINARY AREA IN    */ 00823000
*   /* BYTES.                                                        */ 00824000
*   /*                                                               */ 00825000
*   /*****************************************************************/ 00826000
*/*AMDSABIN: E AMDSABIN                                              */ 00827000
*/* N BINARY TO EBCDIC CONVERT ROUTINE                               */ 00828000
*/* N REGISTER 1 CONTAINS ADDRESS OF CONVERT PARM LIST               */ 00829000
*/* N BINARY AREA, EBCDIC AREA, LENGTH OF BINARY AREA IN BYTES       */ 00830000
*/* P SAVE REGISTERS                                                 */ 00831000
*/*%CONLP1: P OBTAIN FIRST HALF OF BINARY BYTE                       */ 00832000
*/*CONLP2: D (YES,,NO,%NUMBER) IS 9 < HEX DIGIT ?                    */ 00833000
*/* P DECREMENT A - F BY 1                                           */ 00834000
*/* P MOVE THE CHARACTER TO OUTPUT AREA                              */ 00835000
*/* P 'AND' THE CHARACTER WITH X'07' TO RETAIN LOW 3 ORDER BITS      */ 00836000
*/* P (,%MOVECHR) 'OR' IN X'C0' TO COMPLETE BYTE FOR A - F CHARACTER */ 00837000
*/*%NUMBER: P MOVE THE NUMBER TO OUTPUT AREA                         */ 00838000
*/* P 'OR' IN X'F0' TO COMPLETE EBCDIC NUMBER                        */ 00839000
*/*%MOVECHR: P BUMP PTR TO NEXT BYTE IN EBCDIC AREA                  */ 00840000
*/* D (NO,CONLP2,YES,) BOTH HALVES OF BYTE DONE ?                    */ 00841000
*/* P BUMP PTR TO NEXT SET OF HEX DIGITS IN INPUT AREA               */ 00842000
*/* D (NO,%CONLP1,YES,) ALL INPUT PROCESSED ?                        */ 00843000
*/* P RESTORE REGISTERS                                              */ 00844000
*/* R RETURN TO CALLER                                               */ 00845000
*                                                               01S0450 00846000
*                                                               01S0450 00847000
*AMDSABIN:                                                      01S0450 00848000
*   ENTRY;                                                      01S0450 00849000
@EP00450 DS    0H                                               01S0451 00850000
*   RESPECIFY (R1,I) RESTRICTED;                                01S0451 00851000
*   BINOPTR = BINEBCAD;             /* POINT TO EBCDIC OUTPUT AREA   */ 00852000
         L     BINOPTR,BINEBCAD(,R1)                            01S0452 00853000
*   BINIPTR = BINBINAD;             /* POINT TO BINARY INPUT AREA    */ 00854000
         L     BINIPTR,BINBINAD(,R1)                            01S0453 00855000
*   DO I = BINBINLN TO 1 BY -1;     /* CONVERT ALL INPUT TO EBCDIC   */ 00856000
         LH    I,BINBINLN(,R1)                                  01S0454 00857000
         B     @DE00454                                         01S0454 00858000
@DL00454 DS    0H                                               01S0455 00859000
*     BINLOOP = OFF;                /* FIRST HALF OF BYTE PROCESSED  */ 00860000
         NI    BINLOOP,B'01111111'                              01S0455 00861000
*     BINWORK = BINICHAR / 16;      /* GET FIRST HALF OF BYTE        */ 00862000
*                                                               01S0456 00863000
         SLR   @14,@14                                          01S0456 00864000
         IC    @14,BINICHAR(,BINIPTR)                           01S0456 00865000
         SRL   @14,4                                            01S0456 00866000
         STC   @14,BINWORK                                      01S0456 00867000
*CONLP2:                                                        01S0457 00868000
*     IF BINWORK > MAXNUM THEN      /* IS IT A NUMERIC               */ 00869000
CONLP2   CLI   BINWORK,9                                        01S0457 00870000
         BNH   @RF00457                                         01S0457 00871000
*       DO;                         /* NO - CHARACTER                */ 00872000
*         BINOCHAR = (BINWORK - 1) & LOW3BITS; /* DECREMENT CHAR VALUE  00873000
*                                      BY 1. RETAIN LOW ORDER 3 BITS */ 00874000
         SLR   @14,@14                                          01S0459 00875000
         IC    @14,BINWORK                                      01S0459 00876000
         BCTR  @14,0                                            01S0459 00877000
         N     @14,@CF01704                                     01S0459 00878000
         STC   @14,BINOCHAR(,BINOPTR)                           01S0459 00879000
*         BINOCHAR = BINOCHAR | EBCMASK; /* CONVERT CHAR TO EBCDIC   */ 00880000
         OI    BINOCHAR(BINOPTR),X'C0'                          01S0460 00881000
*       END;                                                    01S0461 00882000
*     ELSE                          /* NUMERIC VALUE TO BE CONVERTED */ 00883000
*       BINOCHAR = BINWORK | NUMMASK; /* CONVERT NUMERIC TO EBCDIC   */ 00884000
         B     @RC00457                                         01S0462 00885000
@RF00457 MVC   BINOCHAR(1,BINOPTR),BINWORK                      01S0462 00886000
