         CR    R0,@02                                           01S0493 01058000
         BNE   @RF00493                                         01S0493 01059000
*       IF R1=0|PCBRBN=R1 THEN      /* RBN MATCH OR 0 (DON'T CARE)      01060000
*                                                            @Z40WI3F*/ 01061000
         CR    R1,@06                                           01S0494 01062000
         BE    @RT00494                                         01S0494 01063000
         SLR   @06,@06                                          01S0494 01064000
         ICM   @06,3,PCBRBN(PCBPTR)                             01S0494 01065000
         CR    R1,@06                                           01S0494 01066000
         BNE   @RF00494                                         01S0494 01067000
@RT00494 DS    0H                                               01S0495 01068000
*         DO;                                                   01S0495 01069000
*           RETCODE=0;              /* YES - GOOD RETURN             */ 01070000
         SLR   RETCODE,RETCODE                                  01S0496 01071000
*           RETURN;                 /* RETURN TO CALLER              */ 01072000
@EL00006 DS    0H                                               01S0497 01073000
@EF00006 DS    0H                                               01S0497 01074000
@ER00006 BR    @14                                              01S0497 01075000
*         END;                                                  01S0498 01076000
*     PCBPTR=PCBFQPA;               /* GET NEXT PCB                  */ 01077000
@RF00494 DS    0H                                               01S0499 01078000
@RF00493 SLR   @06,@06                                          01S0499 01079000
         ICM   @06,7,PCBFQPA(PCBPTR)                            01S0499 01080000
         LR    PCBPTR,@06                                       01S0499 01081000
*   END;                            /* CONTINUE PCB SEARCH           */ 01082000
