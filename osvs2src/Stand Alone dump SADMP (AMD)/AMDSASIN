         TITLE 'AMDSASIN SWAP-IN FUNCTION FOR VIRTUAL DUMP             *00001000
                        '                                               00002000
AMDSASIN CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'AMDSASIN  76.074'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @11,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@11                                      01S0001 00013000
         ST    @13,@SA00001+4                                   01S0001 00014000
         LA    @14,@SA00001                                     01S0001 00015000
         ST    @14,8(,@13)                                      01S0001 00016000
         LR    @13,@14                                          01S0001 00017000
*   SAVEFORM(1:4)='LSIN';           /* SET UP SWAP-IN ROUTINE ID IN     00018000
*                                      SAVE AREA                     */ 00019000
         MVC   SAVEFORM(4,R13),@CC01846                         01S0221 00020000
*   RCBNEXT=VCTRCB;                 /* SAVE THE ADDRESS OF THE LAST     00021000
*                                      RCB ON THE VCTRCB CHAIN       */ 00022000
         LA    @10,SINRCB                                       01S0222 00023000
         L     @15,VCTRCB(,VCCTREG)                             01S0222 00024000
         ST    @15,RCBNEXT(,@10)                                01S0222 00025000
*   GEN REFS(R1) SETS(SINSTOR)(STCTL R1,R1,SINSTOR);/* SAVE THE 01S0223 00026000
*                                      MEMORY SEGMENT TABLE ADDRESS  */ 00027000
         STCTL R1,R1,SINSTOR                                            00028000
*   EXITADDR=ADDR(SINERRTN);        /* ERROR EXIT ADDR               */ 00029000
         LA    @10,SINERRTN                                     01S0224 00030000
         ST    @10,EXITADDR                                     01S0224 00031000
*   RCBSAVE=R13;                    /* SAVE SWAP'S SAVE AREA ADDRESS */ 00032000
         LA    @10,SINRCB                                       01S0225 00033000
         ST    R13,RCBSAVE(,@10)                                01S0225 00034000
*   RCBBASE=BASEREG;                /* SAVE THE BASE REGISTER FOR RCB   00035000
*                                      ADDRESSABILITY                */ 00036000
         ST    BASEREG,RCBBASE(,@10)                            01S0226 00037000
*   VCTRCB=ADDR(SINRCB);            /* QUEUE THE SWAP-IN RCB ON THE     00038000
*                                      VCTRCB CHAIN                  */ 00039000
         ST    @10,VCTRCB(,VCCTREG)                             01S0227 00040000
*                                                               01S0228 00041000
*   /*****************************************************************/ 00042000
*   /*                                                               */ 00043000
*   /* DETERMINE IF THE MEMORY BEING PROCESSED IS IN STORAGE OR      */ 00044000
*   /* SWAPPED OUT                                                   */ 00045000
*   /*                                                               */ 00046000
*   /*****************************************************************/ 00047000
*                                                               01S0228 00048000
*   VCTLRCLM=OFF;                   /* SET RECLAIM SWITCH OFF        */ 00049000
         NI    VCTLRCLM(VCCTREG),B'11110111'                    01S0228 00050000
*   VCTCBADR=RSMHDPTR;              /* SET UP RSM ADDRESS FOR VCK    */ 00051000
         L     @10,ASCBPTR(,VCCTREG)                            01S0229 00052000
         L     @10,RSMHDPTR(,@10)                               01S0229 00053000
         ST    @10,VCTCBADR(,VCCTREG)                           01S0229 00054000
*   VCTCBIDN=VRSMHID;               /* SET UP RSM ID                 */ 00055000
         MVI   VCTCBIDN(VCCTREG),X'0E'                          01S0230 00056000
*   SVC(VCKSVC);                    /* VCK THE RSM                   */ 00057000
         SVC   25                                               01S0231 00058000
*   RFY                                                         01S0232 00059000
*     RETCODE RSTD;                                             01S0232 00060000
*   IF RETCODE^=0 THEN                                          01S0233 00061000
         LTR   RETCODE,RETCODE                                  01S0233 00062000
         BZ    @RF00233                                         01S0233 00063000
*     VCTLRCLM=ON;                  /* SET RECLAIM SW ON IF RSM 01S0234 00064000
*                                      INVALID                       */ 00065000
         OI    VCTLRCLM(VCCTREG),B'00001000'                    01S0234 00066000
*   VCTMEMIN=ON;                    /* SET UP NO SWAP IN REQUIRED    */ 00067000
@RF00233 OI    VCTMEMIN(VCCTREG),B'01000000'                    01S0235 00068000
*   IF(ASCBNOQ=OFF)|(OUCBOUT=OFF) THEN/*                     @YM06829*/ 00069000
         L     @14,ASCBPTR(,VCCTREG)                            01S0236 00070000
         TM    ASCBNOQ(@14),B'10000000'                         01S0236 00071000
         BZ    @RT00236                                         01S0236 00072000
         L     @10,OUCBPTR(,@14)                                01S0236 00073000
         TM    OUCBOUT(@10),B'00000100'                         01S0236 00074000
         BZ    @RT00236                                         01S0236 00075000
*     GO TO SGTADDR;                /* ON QUEUE OR NOT OUT THEN 01S0237 00076000
*                                      MEMORY IS IN                  */ 00077000
*   IF(VCTLRCLM=ON)|(RSMSPCT=0) THEN                            01S0238 00078000
         TM    VCTLRCLM(VCCTREG),B'00001000'                    01S0238 00079000
         BO    @RT00238                                         01S0238 00080000
         L     @14,RSMHDPTR(,@14)                               01S0238 00081000
         L     @14,RSMSPCT(,@14)                                01S0238 00082000
         LTR   @14,@14                                          01S0238 00083000
         BZ    @RT00238                                         01S0238 00084000
*     GO TO SINERRTN;               /* MEMORY NOT IN, NO RECLAIM OR     00085000
*                                      SPCT THEN CTL BLK IS IN ERROR */ 00086000
*   VCTCBADR=RSMSPCT;               /* SET UP SPCT ADDRESS FOR VCK      00087000
*                                                            @Z40WI3P*/ 00088000
         ST    @14,VCTCBADR(,VCCTREG)                           01S0240 00089000
*   VCTCBIDN=VSPCTID;               /* SET UP SPCT ID        @Z40WI3P*/ 00090000
         MVI   VCTCBIDN(VCCTREG),X'01'                          01S0241 00091000
*   SVC(VCKSVC);                    /* VCK THE SPCT          @Z40WI3P*/ 00092000
         SVC   25                                               01S0242 00093000
*   IF RETCODE^=0 THEN              /* SPCT VALID?           @Z40WI3P*/ 00094000
         LTR   RETCODE,RETCODE                                  01S0243 00095000
         BNZ   @RT00243                                         01S0243 00096000
*     GO TO SINERRTN;               /* NO--CANNOT PROCESS A SWAPPED     00097000
*                                      OUT MEMORY WITHOUT THE SPCT      00098000
*                                                            @Z40WI3P*/ 00099000
*   IF VCTSARTB=ON&SPCTPSET=OFF THEN/* IS SART INVALID AND LSQA ON      00100000
*                                      SWAP DS?              @Z40WI3P*/ 00101000
         TM    VCTSARTB(VCCTREG),B'00010000'                    01S0245 00102000
         BNO   @RF00245                                         01S0245 00103000
         L     @14,ASCBPTR(,VCCTREG)                            01S0245 00104000
         L     @14,RSMHDPTR(,@14)                               01S0245 00105000
         L     @14,SPCTPTR(,@14)                                01S0245 00106000
         TM    SPCTPSET(@14),B'00001000'                        01S0245 00107000
         BZ    @RT00245                                         01S0245 00108000
*     GO TO SINERRTN;               /* YES-CANNOT PROCESS MEMORY        00109000
*                                      BECAUSE ALL ITS LSQA IS ON A     00110000
*                                      SWAP DATA SET, BUT THE SART IS   00111000
*                                      INVALID SO WE CANNOT GET 01S0246 00112000
*                                      ANYTHING FROM THE SWAP DS.       00113000
*                                                            @Z40WI3P*/ 00114000
*   RFY                                                         01S0247 00115000
*     RETCODE UNRSTD;               /*                       @Z40WI3P*/ 00116000
@RF00245 DS    0H                                               01S0248 00117000
*                                                               01S0248 00118000
*   /*****************************************************************/ 00119000
*   /*                                                               */ 00120000
*   /* SET UP TO INITIALIZE THE ENTIRE PTRT TABLE SINCE THIS IS A    */ 00121000
*   /* MEMORY SWAP-IN OPERATION                                      */ 00122000
*   /*                                                               */ 00123000
*   /*****************************************************************/ 00124000
*                                                               01S0248 00125000
*   RESPECIFY                                                   01S0248 00126000
*     PTRTRSA BASED(SINRSA);                                    01S0248 00127000
*   RESPECIFY                                                   01S0249 00128000
*     PTRT BASED(VCTPTRT+VCTLPA);                               01S0249 00129000
*   PTRTEND=ADDR(PTRTVS(1));        /* CALCULATE THE END OF THE PTRT    00130000
*                                      FOR LATER USE                 */ 00131000
         L     @10,VCTPTRT(,VCCTREG)                            01S0250 00132000
         AL    @10,VCTLPA(,VCCTREG)                             01S0250 00133000
         LA    PTRTEND,PTRTVS(,@10)                             01S0250 00134000
*   VCTMEMIN=OFF;                   /* MEMORY MUST BE SWAPPED INTO      00135000
*                                      REAL STORAGE                  */ 00136000
         NI    VCTMEMIN(VCCTREG),B'10111111'                    01S0251 00137000
*   DO SINRSA=ADDR(PTRTRS(1)) BY LENGTH(PTRTRSA) WHILE SINRSA<PTRTEND;  00138000
*                                                               01S0252 00139000
         LA    @10,PTRTRS(,@10)                                 01S0252 00140000
         ST    @10,SINRSA                                       01S0252 00141000
         B     @DE00252                                         01S0252 00142000
@DL00252 DS    0H                                               01S0253 00143000
*/* SET UP TO INITIALIZE THE ENTIRE PTRT TABLE                       */ 00144000
*                                                               01S0253 00145000
*     PTRTRSA='FFFFFFFF'X;          /* INVALIDATE EACH PTRT ENTRY    */ 00146000
         L     @10,SINRSA                                       01S0253 00147000
         MVC   PTRTRSA(4,@10),@CF01853                          01S0253 00148000
*   END;                                                        01S0254 00149000
         AL    @10,@CF00038                                     01S0254 00150000
         ST    @10,SINRSA                                       01S0254 00151000
@DE00252 C     PTRTEND,SINRSA                                   01S0254 00152000
         BH    @DL00252                                         01S0254 00153000
*                                                               01S0255 00154000
*   /*****************************************************************/ 00155000
*   /*                                                               */ 00156000
*   /* INITIALIZE THE SEGMENT TABLE ENTRIES IN THE PTRT TO THE       */ 00157000
*   /* CORRESPONDING MASTER MEMORY ENTRIES AND VALIDATE              */ 00158000
*   /*                                                               */ 00159000
*   /*****************************************************************/ 00160000
*                                                               01S0255 00161000
*   SINRSA=ADDR(PTRTRS(1));                                     01S0255 00162000
         L     @10,VCTPTRT(,VCCTREG)                            01S0255 00163000
         AL    @10,VCTLPA(,VCCTREG)                             01S0255 00164000
         LA    @10,PTRTRS(,@10)                                 01S0255 00165000
         ST    @10,SINRSA                                       01S0255 00166000
*   RESPECIFY                                                   01S0256 00167000
*     R1 RESTRICTED;                                            01S0256 00168000
*   R1=SINSTOR;                     /* GET REAL SEGMENT TABLE ADDRESS*/ 00169000
         L     R1,SINSTOR                                       01S0257 00170000
*   SVC(VRTSVC);                    /* CALL AMDSAVRT TO GET VIRTUAL     00171000
*                                      SEGMENT TABLE ADDRESS         */ 00172000
         SVC   30                                               01S0258 00173000
*   WKSNSTOR=R1;                    /* SAVE VIRTUAL SEGMENT TABLE       00174000
*                                      ADDRESS                       */ 00175000
         LR    WKSNSTOR,R1                                      01S0259 00176000
*   DO I=CVTNUCB/SEGSIZE TO 1 BY-1;                             01S0260 00177000
         L     @10,CVTPTR                                       01S0260 00178000
         L     I,CVTNUCB(,@10)                                  01S0260 00179000
         SRL   I,16                                             01S0260 00180000
         B     @DE00260                                         01S0260 00181000
@DL00260 DS    0H                                               01S0261 00182000
*     SGTSTE=WKSNSTOR->SGTSTE;                                  01S0261 00183000
         L     @10,STEPTR                                       01S0261 00184000
         MVC   SGTSTE(4,@10),SGTSTE(WKSNSTOR)                   01S0261 00185000
*     SINRSA=SINRSA+LENGTH(PTRTRSA);                            01S0262 00186000
         LA    @10,4                                            01S0262 00187000
         L     @00,SINRSA                                       01S0262 00188000
         ALR   @00,@10                                          01S0262 00189000
         ST    @00,SINRSA                                       01S0262 00190000
*     WKSNSTOR=WKSNSTOR+LENGTH(PTRTRSA);                        01S0263 00191000
         ALR   WKSNSTOR,@10                                     01S0263 00192000
*   END;                                                        01S0264 00193000
         BCTR  I,0                                              01S0264 00194000
@DE00260 LTR   I,I                                              01S0264 00195000
         BP    @DL00260                                         01S0264 00196000
*   SINRSA=ADDR(PTRTRS(1))+(CVTSHRVM/SEGSIZE)*LENGTH(SGTSTE);/* 01S0265 00197000
*                                                            @Z40WI3P*/ 00198000
         L     @10,VCTPTRT(,VCCTREG)                            01S0265 00199000
         AL    @10,VCTLPA(,VCCTREG)                             01S0265 00200000
         LA    @10,PTRTRS(,@10)                                 01S0265 00201000
         L     @03,@CF01806                                     01S0265 00202000
         L     @15,CVTPTR                                       01S0265 00203000
         L     @15,CVTSHRVM(,@15)                               01S0265 00204000
         LR    @05,@15                                          01S0265 00205000
         SLR   @04,@04                                          01S0265 00206000
         DR    @04,@03                                          01S0265 00207000
         SLA   @05,2                                            01S0265 00208000
         ALR   @10,@05                                          01S0265 00209000
         ST    @10,SINRSA                                       01S0265 00210000
*   WKSNSTOR=R1+(CVTSHRVM/SEGSIZE)*LENGTH(SGTSTE);/*         @Z40WI3P*/ 00211000
         ALR   @05,R1                                           01S0266 00212000
         LR    WKSNSTOR,@05                                     01S0266 00213000
*   DO I=(NUMSEG-CVTSHRVM/SEGSIZE) TO 1 BY-1;/*              @Z40WI3P*/ 00214000
         L     @10,@CF01806                                     01S0267 00215000
         SLR   @14,@14                                          01S0267 00216000
         DR    @14,@10                                          01S0267 00217000
         LCR   @15,@15                                          01S0267 00218000
         AL    @15,@CF00913                                     01S0267 00219000
         LR    I,@15                                            01S0267 00220000
         B     @DE00267                                         01S0267 00221000
@DL00267 DS    0H                                               01S0268 00222000
*     SGTSTE=WKSNSTOR->SGTSTE;                                  01S0268 00223000
         L     @10,STEPTR                                       01S0268 00224000
         MVC   SGTSTE(4,@10),SGTSTE(WKSNSTOR)                   01S0268 00225000
*     SINRSA=SINRSA+LENGTH(PTRTRSA);                            01S0269 00226000
         LA    @10,4                                            01S0269 00227000
         L     @00,SINRSA                                       01S0269 00228000
         ALR   @00,@10                                          01S0269 00229000
         ST    @00,SINRSA                                       01S0269 00230000
*     WKSNSTOR=WKSNSTOR+LENGTH(PTRTRSA);                        01S0270 00231000
         ALR   WKSNSTOR,@10                                     01S0270 00232000
*   END;                                                        01S0271 00233000
         BCTR  I,0                                              01S0271 00234000
@DE00267 LTR   I,I                                              01S0271 00235000
         BP    @DL00267                                         01S0271 00236000
*   RESPECIFY                                                   01S0272 00237000
*     R1 UNRESTRICTED;                                          01S0272 00238000
*                                                               01S0273 00239000
*   /*****************************************************************/ 00240000
*   /*                                                               */ 00241000
*   /* CALCULATE THE LSID AND INITIALIZE THE PTRT FOR EACH   @Z40WI3P*/ 00242000
*   /* PAGE TABLE ENTRY.                                     @Z40WI3P*/ 00243000
*   /*                                                               */ 00244000
*   /*****************************************************************/ 00245000
*                                                               01S0273 00246000
*   DO SEGSPTR=ADDR(SPCTSEGS(1)) BY LENGTH(SPCTSEGE) WHILE SEGSPTR<ADDR 00247000
*         (SPCTSEGS(1))+(SPCTSSEG*LENGTH(SPCTSEGE));            01S0273 00248000
         L     @10,ASCBPTR(,VCCTREG)                            01S0273 00249000
         L     @10,RSMHDPTR(,@10)                               01S0273 00250000
         L     SEGSPTR,SPCTPTR(,@10)                            01S0273 00251000
         LA    SEGSPTR,SPCTSEGS(,SEGSPTR)                       01S0273 00252000
         B     @DE00273                                         01S0273 00253000
@DL00273 DS    0H                                               01S0274 00254000
*     SINRSA=ADDR(PTRTRS(1))+(SPCTSEGX*LENGTH(PTRTRSA));/* INDEX TO     00255000
*                                      PROPER RSA PTRT ENTRY         */ 00256000
         L     @10,VCTPTRT(,VCCTREG)                            01S0274 00257000
         AL    @10,VCTLPA(,VCCTREG)                             01S0274 00258000
         LA    @10,PTRTRS(,@10)                                 01S0274 00259000
         SLR   @06,@06                                          01S0274 00260000
         IC    @06,SPCTSEGX(,SEGSPTR)                           01S0274 00261000
         SLA   @06,2                                            01S0274 00262000
         ALR   @10,@06                                          01S0274 00263000
         ST    @10,SINRSA                                       01S0274 00264000
*     PTRTRSA=0;                    /* ZERO THE RSA ADDRESS          */ 00265000
         SLR   @06,@06                                          01S0275 00266000
         ST    @06,PTRTRSA(,@10)                                01S0275 00267000
*     SGTPAM=ON;                    /* INVALIDATE THE ENTRY          */ 00268000
         L     @06,STEPTR                                       01S0276 00269000
         OI    SGTPAM(@06),B'00000001'                          01S0276 00270000
*     PTRTVSA=SPCTPGT;              /* OBTAIN THE VIRTUAL STORAGE       00271000
*                                      ADDR                          */ 00272000
         SLR   @06,@06                                          01S0277 00273000
         ICM   @06,7,SPCTPGT(SEGSPTR)                           01S0277 00274000
         ST    @06,PTRTVSA+1024(,@10)                           01S0277 00275000
*     RECLMSW=OFF;                  /* INDICATE NO RECLAIM           */ 00276000
         NI    RECLMSW,B'01111111'                              01S0278 00277000
*     PGECAL=(SPCTPGT&'FFF000'X);   /* CALCULATE THE VBN OF THE PAGE    00278000
*                                      ENTRY                         */ 00279000
         MVC   PGECAL(3),SPCTPGT(SEGSPTR)                       01S0279 00280000
         NC    PGECAL(3),@CB01855                               01S0279 00281000
*     RESPECIFY                                                 01S0280 00282000
*      (R0,                                                     01S0280 00283000
*       R1) RESTRICTED;             /*                       @Z40WI3P*/ 00284000
*     R0=PGEVBN;                    /* GET VBN PORTION OF PGECAL        00285000
*                                                            @Z40WI3P*/ 00286000
         SLR   R0,R0                                            01S0281 00287000
         ICM   R0,3,PGEVBN                                      01S0281 00288000
*     CALL SRCHSPCT;                /* FIND LSQA ENTRY IN SPCT FOR      00289000
*                                      THIS PAGE. ON RETURN, R1 01S0282 00290000
*                                      POINTS TO THE ENTRY.  @Z40WI3P*/ 00291000
         BAL   @14,SRCHSPCT                                     01S0282 00292000
*     RESPECIFY                                                 01S0283 00293000
*       RETCODE RESTRICTED;         /*                       @Z40WI3P*/ 00294000
*     IF RETCODE^=0 THEN            /* LSQA ENTRY FOUND?     @Z40WI3P*/ 00295000
         LTR   RETCODE,RETCODE                                  01S0284 00296000
         BNZ   @RT00284                                         01S0284 00297000
*       GO TO BADLSQA;              /* NO-ERROR, PAGE TABLES MUST BE    00298000
*                                      IN LSQA               @Z40WI3P*/ 00299000
*     RESPECIFY                                                 01S0286 00300000
*       RETCODE UNRESTRICTED;       /*                       @Z40WI3P*/ 00301000
*     RESPECIFY                                                 01S0287 00302000
*       SPCTSWPE BASED(R1);         /*                       @Z40WI3P*/ 00303000
*     IF SPCTLVAL=OFF THEN          /* LSID IN SPCT STILL VALID?        00304000
*                                                            @Z40WI3P*/ 00305000
         TM    SPCTLVAL(R1),B'10000000'                         01S0288 00306000
         BNZ   @RF00288                                         01S0288 00307000
*       DO;                         /* NO- BETTER BE ABLE TO RECLAIM    00308000
*                                                            @Z40WI3P*/ 00309000
*         SAVESPCT=R1;              /* SAVE SPCT ENTRY ADDR  @Z40WI3P*/ 00310000
         LR    SAVESPCT,R1                                      01S0290 00311000
*         CALL CHKPFTE;             /* CHECK PFTE FOR RECLAIM. ON       00312000
*                                      RETURN, R1 CONTAINS RBN  01S0291 00313000
*                                                            @Z40WI3P*/ 00314000
         BAL   @14,CHKPFTE                                      01S0291 00315000
*         RESPECIFY                                             01S0292 00316000
*           RETCODE RESTRICTED;     /*                       @Z40WI3P*/ 00317000
*         IF RETCODE^=0 THEN        /* PAGE RECLAIMABLE?     @Z40WI3P*/ 00318000
         LTR   RETCODE,RETCODE                                  01S0293 00319000
         BZ    @RF00293                                         01S0293 00320000
*           DO;                     /* NO-ERROR              @Z40WI3P*/ 00321000
*             RESPECIFY                                         01S0295 00322000
*               RETCODE UNRESTRICTED;/*                      @Z40WI3P*/ 00323000
*BADLSQA:                           /*                       @Z40WI3P*/ 00324000
*             R1=0;                 /* REQUEST JUST ERR MSG  @Z40WI3P*/ 00325000
BADLSQA  SLR   R1,R1                                            01S0296 00326000
*             CALL SINDUMP;         /* ISSUE ERROR MESSAGE   @Z40WI3P*/ 00327000
         BAL   @14,SINDUMP                                      01S0297 00328000
*             GO TO NEXTENTY;       /* GET NEXT SEGMNT ENTRY @Z40WI3P*/ 00329000
         B     NEXTENTY                                         01S0298 00330000
*           END;                    /*                       @Z40WI3P*/ 00331000
*         RECLMSW=ON;               /* INDICATE RECLAIM      @Z40WI3P*/ 00332000
@RF00293 OI    RECLMSW,B'10000000'                              01S0300 00333000
*         PTRTRSA=PTRTVSA&'00000FFF'X;/* GET DISPLACEMENT PORTION       00334000
*                                                            @Z40WI3P*/ 00335000
         L     @10,SINRSA                                       01S0301 00336000
         L     @15,PTRTVSA+1024(,@10)                           01S0301 00337000
         N     @15,@CF01863                                     01S0301 00338000
         ST    @15,PTRTRSA(,@10)                                01S0301 00339000
*         SINVADDR(2:3)=SINVADDR(2:3)|R1;/* 'OR' THE RBN INTO ENTRY     00340000
*                                                            @Z40WI3P*/ 00341000
         SLR   @15,@15                                          01S0302 00342000
         ICM   @15,3,SINVADDR+1(@10)                            01S0302 00343000
         OR    @15,R1                                           01S0302 00344000
         STCM  @15,3,SINVADDR+1(@10)                            01S0302 00345000
*         SINVADDR(1)='F0'X;        /* SET PAGE TABLE LENGTH @Z40WI3P*/ 00346000
         MVI   SINVADDR(@10),X'F0'                              01S0303 00347000
*         RESPECIFY                                             01S0304 00348000
*           PGTPTE BASED(VCTPGT+VCTLPA);/*                   @Z40WI3P*/ 00349000
*         PGTRSA=R1;                /* SET UP VCTLPA AS VIRT ADDR OF    00350000
*                                      THIS REAL FRAME       @Z40WI3P*/ 00351000
         L     @10,VCTPGT(,VCCTREG)                             01S0305 00352000
         AL    @10,VCTLPA(,VCCTREG)                             01S0305 00353000
         STCM  R1,3,PGTRSA(@10)                                 01S0305 00354000
*         GEN(PTLB)                                             01S0306 00355000
*         NOREFS NOSETS;            /* PURGE HARDWARE TLB    @Z40WI3P*/ 00356000
         PTLB                                                           00357000
*         R1=SAVESPCT;              /* RESTORE SAVED SPCT ENTRY 01S0307 00358000
*                                      ADDRESS               @Z40WI3P*/ 00359000
         LR    R1,SAVESPCT                                      01S0307 00360000
*       END;                        /*                       @Z40WI3P*/ 00361000
*     DO SINSEGPT=ADDR(SPCTSEGS(1)) BY LENGTH(SPCTSEGE) WHILE SINSEGPT< 00362000
*           SEGSPTR;                /* SEARCH FOR IDENTICAL ENTRIES  */ 00363000
