         TITLE 'AMDSASIO VIRTUAL DUMP START I/O ROUTINE                *00001000
                        '                                               00002000
AMDSASIO CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'AMDSASIO  75.280'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @11,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@11                                      01S0001 00013000
         ST    @13,@SA00001+4                                   01S0001 00014000
         LA    @14,@SA00001                                     01S0001 00015000
         ST    @14,8(,@13)                                      01S0001 00016000
         LR    @13,@14                                          01S0001 00017000
*   SAVEFORM(1:4) = 'LSIO';         /* MOVE MODULE ID TO SAVE AREA   */ 00018000
         MVC   SAVEFORM(4,R13),@CC01095                         01S0212 00019000
*   CCTREG = CCTADDR;               /* SET CCT POINTER               */ 00020000
         L     CCTREG,@CF00742                                  01S0213 00021000
*   SRCBNEXT = VCTRCB;              /* SET UP RCB BACKWARD CHAIN     */ 00022000
         L     @10,VCTRCB(,VCCTREG)                             01S0214 00023000
         ST    @10,SRCBNEXT                                     01S0214 00024000
*   SRCBSAVE = R13;                 /* SET UP SAVE AREA PTR FOR RCB     00025000
*                                      EXIT                          */ 00026000
         ST    R13,SRCBSAVE                                     01S0215 00027000
*   SRCBBASE = BASEREG;             /* SAVE BASE REGISTER FOR RCB       00028000
*                                      EXIT                          */ 00029000
         ST    BASEREG,SRCBBASE                                 01S0216 00030000
*   VCTRCB =ADDR(SIORCB);           /* QUEUE SIO RCB ON CHAIN        */ 00031000
         LA    @10,SIORCB                                       01S0217 00032000
         ST    @10,VCTRCB(,VCCTREG)                             01S0217 00033000
*                                                               01S0218 00034000
*   /*****************************************************************/ 00035000
*   /*                                                               */ 00036000
*   /* ISOLATE THE TYPE OF INPUT REQUEST AND INITIALIZE. IF THE INPUT*/ 00037000
*   /* REQUEST IS INVALID CALL AUDIT SINCE THIS IS DUE TO AN INTERNAL*/ 00038000
*   /* AMDSADMP ERROR                                                */ 00039000
*   /*                                                               */ 00040000
*   /*****************************************************************/ 00041000
*                                                               01S0218 00042000
*/* D (YES,DAIO,NO,) DA I/O REQUEST ?                                */ 00043000
*/* D (YES,WKIO,NO,) WORKFILE I/O REQUEST ?                          */ 00044000
*/* D (YES,OUTIO,NO,) OUTPUT I/O REQUEST ?                           */ 00045000
*/* D (YES,OUTIO,NO,AUDEXIT) CTIOTERM REQUEST ?                      */ 00046000
*/*AUDEXIT: L (,EREXIT) AMDSAAUD: SVC 13 TO OBTAIN DUMP              */ 00047000
*/*DAIO: P (,%SPROC) SET IODBREG = ADDR OF DA IODB                   */ 00048000
*/*WKIO: P (,%SPROC) SET IODBREG = ADDR OF WKFILE IODB               */ 00049000
*/*OUTIO: P (,%SPROC) SET IODBREG = ADDR OF OUTPUT IODB              */ 00050000
*   IF CTIODA = ON THEN             /* IF DA REQUEST USE DA IODB     */ 00051000
         TM    CTIODA(CCTREG),B'00100000'                       01S0218 00052000
         BNO   @RF00218                                         01S0218 00053000
*     IODBREG = ADDR(IODBDA);                                   01S0219 00054000
         L     IODBREG,@CA00943                                 01S0219 00055000
*   ELSE                                                        01S0220 00056000
*     IF CTIOWK = ON THEN           /* IF WORKFILE REQUEST USE  01S0220 00057000
*                                      WORKFILE IODB                 */ 00058000
         B     @RC00218                                         01S0220 00059000
@RF00218 TM    CTIOWK(CCTREG),B'00010000'                       01S0220 00060000
         BNO   @RF00220                                         01S0220 00061000
*       IODBREG = ADDR(IODBWORK);                               01S0221 00062000
         L     IODBREG,@CA00944                                 01S0221 00063000
*     ELSE                                                      01S0222 00064000
*       IF CTIOOUT = ON | CTIOTERM = ON THEN                    01S0222 00065000
         B     @RC00220                                         01S0222 00066000
@RF00220 TM    CTIOOUT(CCTREG),B'11000000'                      01S0222 00067000
         BZ    @RF00222                                         01S0222 00068000
*         IODBREG = ADDR(IODBOUT);  /* IF OUTPUT OR TERMINATION 01S0223 00069000
*                                      REQUEST USE THE OUTPUT IODB   */ 00070000
         L     IODBREG,@CA00945                                 01S0223 00071000
*       ELSE                                                    01S0224 00072000
*         DO;                                                   01S0224 00073000
         B     @RC00222                                         01S0224 00074000
@RF00222 DS    0H                                               01S0225 00075000
*           VCTAUDIT = SIOAUD1;     /* SET SIO AUDIT ERROR CODE TO      00076000
*                                      UNIQUELY IDENTIFY THIS ERROR     00077000
*                                      CONDITION AS AN INVALID INPUT    00078000
*                                      REQUEST                       */ 00079000
         MVC   VCTAUDIT(2,VCCTREG),SIOAUD1                      01S0225 00080000
*           SVC (AUDSVC);           /* CALL AUDIT AS THIS IS A SADMP    00081000
*                                      ERROR                         */ 00082000
         SVC   13                                               01S0226 00083000
*           GEN NOSEQFLOW EXIT;     /* TO INDICATE NO RETURN FROM SVC   00084000
*                                     ABOVE                         */  00085000
*         END;                                                  01S0228 00086000
*                                                               01S0229 00087000
*   /*****************************************************************/ 00088000
*   /*                                                               */ 00089000
*   /* DETERMINE IF THE DEVICE WAS OPERATIONAL PREVIOUSLY. IF        */ 00090000
*   /* CATASTROPHIC ERRORS PREVIOUSLY OCCURRED THERE IS NO NEED TO   */ 00091000
*   /* ATTEMPT I/O. IF THE DEVICE HAS NOT PREVIOUSLY BEEN USED OR    */ 00092000
*   /* ERRORS HAVE NOT OCCURRED, PREPARE TO PERFORM I/O              */ 00093000
*   /*                                                               */ 00094000
*   /*****************************************************************/ 00095000
*                                                               01S0229 00096000
*/*%SPROC: D (YES,TERMREQ,NO,) CTIOTERM = 1 ?                        */ 00097000
*/* D (YES,EREXIT,NO,) IODBTERM = 1 ?                                */ 00098000
*/* P TURN OFF BCTCOMP IN BCT POINTED TO BY THE ACTIVE IODB          */ 00099000
*   IF CTIOTERM = ON THEN                                       01S0229 00100000
@RC00222 DS    0H                                               01S0229 00101000
@RC00220 DS    0H                                               01S0229 00102000
@RC00218 TM    CTIOTERM(CCTREG),B'10000000'                     01S0229 00103000
         BO    @RT00229                                         01S0229 00104000
*     GO TO TERMREQ;                /* IF TERMINATION REQUEST HANDLE    00105000
*                                      DIRECTLY                      */ 00106000
*   IF IODBTERM = ON THEN                                       01S0231 00107000
         TM    IODBTERM(IODBREG),B'10000000'                    01S0231 00108000
         BO    @RT00231                                         01S0231 00109000
*     GO TO EREXIT;                 /* IF CATASTROPHIC I/O ERROR - DO   00110000
*                                      NOT PERFORM I/O               */ 00111000
*   BCTCOMP = OFF;                  /* TURN OFF I/O COMPLETED BIT IN    00112000
*                                      BUFFER                        */ 00113000
         L     @10,IODBBCT(,IODBREG)                            01S0233 00114000
         NI    BCTCOMP(@10),B'01111111'                         01S0233 00115000
*                                                               01S0234 00116000
*   /*****************************************************************/ 00117000
*   /*                                                               */ 00118000
*   /* NOW TO THE HEART OF THE SIO CODE. AS A FUNCTION OF THE DEVICE,*/ 00119000
*   /* THE CHANNEL PROGRAMS TO PERFORM THE I/O MUST BE SET UP AND    */ 00120000
*   /* INITIALIZED. THIS SECTION OF CODE PROCESSES DIRECT ACCESS I/O */ 00121000
*   /* REQUESTS                                                      */ 00122000
*   /*                                                               */ 00123000
*   /*****************************************************************/ 00124000
*                                                               01S0234 00125000
*/* D (YES,DAIOTYPE,NO,WKFILECK) DA I/O ?                            */ 00126000
*/*DAIOTYPE: P USE DA CCW'S FOR I/O                                  */ 00127000
*/* P IODBCAW = SEEKADDR                                             */ 00128000
*/* N FORMAT OF DA CCW'S SEEK, SRCH, TIC, READ                       */ 00129000
*/* N SEEKADDR IN IODBCCHH                                           */ 00130000
*/* P (,COMENTRY) STORE BCTRADDR INTO READ CCW ADDRESS               */ 00131000
*   IF IODBREG = ADDR(IODBDA) THEN                              01S0234 00132000
         L     @09,@CA00943                                     01S0234 00133000
         CR    IODBREG,@09                                      01S0234 00134000
         BNE   @RF00234                                         01S0234 00135000
*     DO;                           /* PROCESS DA IO                 */ 00136000
*       IODBCAW = ADDR(DACCW);      /* SET UP CCW CHAIN IN CAW       */ 00137000
         LA    @09,DACCW                                        01S0236 00138000
         ST    @09,IODBCAW(,IODBREG)                            01S0236 00139000
*       DASEEKAD = ADDR(IODBSEEK);  /* INITIALIZE SEEK ADDRESS       */ 00140000
         LA    @09,IODBSEEK(,IODBREG)                           01S0237 00141000
         STCM  @09,7,DASEEKAD                                   01S0237 00142000
*       DASRCHAD = ADDR(IODBSRCH);  /* INITIALIZE SEARCH ADDRESS     */ 00143000
         LA    @09,IODBSRCH(,IODBREG)                           01S0238 00144000
         STCM  @09,7,DASRCHAD                                   01S0238 00145000
*       DAREADAD = BCTRADDR;        /* INITIALIZE CCW INPUT AREA        00146000
*                                      ADDRESS                       */ 00147000
         MVC   DAREADAD(3),BCTRADDR+1(@10)                      01S0239 00148000
*       CTIORSET = ON;              /* INDICATE TRANSFER TO A NEW CPU*/ 00149000
         OI    CTIORSET(CCTREG),B'00000010'                     01S0240 00150000
*       PFSIODA = PFSIODA + 1;      /* COUNT NUMBER OF DA SIO'S      */ 00151000
*                                                               01S0241 00152000
         LA    @10,1                                            01S0241 00153000
         AH    @10,PFSIODA                                      01S0241 00154000
         STH   @10,PFSIODA                                      01S0241 00155000
*       GO TO COMENTRY;             /* PERFORM COMMON I/O FUNCTION   */ 00156000
         B     COMENTRY                                         01S0242 00157000
*     END;                                                      01S0243 00158000
*                                                               01S0244 00159000
*   /*****************************************************************/ 00160000
*   /*                                                               */ 00161000
*   /* IF NOT DA THEN CHECK IF THIS IS WORKFILE. IF YES CHECK THE    */ 00162000
*   /* VALIDITY OF THE REQUESTED ADDRESS. IF INVALID, THE ERROR EXIT */ 00163000
*   /* IS TAKEN OTHERWISE PROCESSING IS SPLIT UP FOR DA WORKFILE AND */ 00164000
*   /* TAPE WORKFILE                                                 */ 00165000
*   /*                                                               */ 00166000
*   /*****************************************************************/ 00167000
*                                                               01S0244 00168000
*/*WKFILECK: D (YES,,NO,%OUTP) WORKFILE I/O ?                        */ 00169000
*/* P OBTAIN REQUESTED VBN FROM IODBCCHH                             */ 00170000
*/* D (YES,EREXIT,NO,) REQUESTED VBN > VCTWKHI ?                     */ 00171000
*   IF IODBREG = ADDR(IODBWORK) THEN                            01S0244 00172000
@RF00234 L     @10,@CA00944                                     01S0244 00173000
         CR    IODBREG,@10                                      01S0244 00174000
         BNE   @RF00244                                         01S0244 00175000
*     DO;                           /* PROCESSING FOR WORKFILE I/O   */ 00176000
*       IF IODBR > VCTWKHI THEN                                 01S0246 00177000
         CLC   IODBR(1,IODBREG),VCTWKHI(VCCTREG)                01S0246 00178000
         BH    @RT00246                                         01S0246 00179000
*         GO TO EREXIT;             /* IF RECORD IS UNAVAILABLE SKIP    00180000
*                                      I/O                           */ 00181000
*                                                               01S0247 00182000
*   /*****************************************************************/ 00183000
*   /*                                                               */ 00184000
*   /* THIS SECTION DYNAMICALLY GENERATES THE TAPE WORKFILE CCW'S    */ 00185000
*   /*                                                               */ 00186000
*   /*****************************************************************/ 00187000
*                                                               01S0248 00188000
*/* D (YES,WKTAPE,NO,WKDA) IODBCLAS = TAPE ?                         */ 00189000
*/*WKTAPE: P OBTAIN CURRENT VBN FROM VCTWKC                          */ 00190000
*/* P BUILD DYNAMIC CCW TO BSR OR FSR THE TAPE TO PROPER VBN         */ 00191000
*/* N CCW FORMAT IS BSR/FSR ... BSR/FSR, READ                        */ 00192000
*/* P MOVE ADDRESS OF INPUT AREA FROM BCTRADDR INTO READ CCW         */ 00193000
*/* P (,COMENTRY) IODBCAW = BSR/FRS CCW ADDRESS                      */ 00194000
*       IF IODBCLAS = TAPE THEN                                 01S0248 00195000
         CLI   IODBCLAS(IODBREG),128                            01S0248 00196000
         BNE   @RF00248                                         01S0248 00197000
*         DO;                       /* FOR TAPE WORKFILE             */ 00198000
*           DYNAPTR = ADDR (DYNAREA); /* SET UP TO START OF DYNAMIC     00199000
*                                      AREA                          */ 00200000
         LA    DYNAPTR,DYNAREA                                  01S0250 00201000
*           VCTWKC = VCTWKC + 1;    /* POINT TO RECORD TO WHICH TAPE    00202000
*                                      IS POSITIONED                 */ 00203000
         LA    @10,1                                            01S0251 00204000
         SLR   @09,@09                                          01S0251 00205000
         IC    @09,VCTWKC(,VCCTREG)                             01S0251 00206000
         ALR   @10,@09                                          01S0251 00207000
         STC   @10,VCTWKC(,VCCTREG)                             01S0251 00208000
*           DO WHILE IODBR ^= VCTWKC; /* DYNAMICALLY GENERATE CCW'S  */ 00209000
         B     @DE00252                                         01S0252 00210000
@DL00252 DS    0H                                               01S0253 00211000
*             IF IODBR > VCTWKC THEN                            01S0253 00212000
         CLC   IODBR(1,IODBREG),VCTWKC(VCCTREG)                 01S0253 00213000
         BNH   @RF00253                                         01S0253 00214000
*               DO;                                             01S0254 00215000
*                 DYNACCW = WKTPFSR; /* MOVE IN FSR CCW              */ 00216000
         MVC   DYNACCW(8,DYNAPTR),WKTPFSR                       01S0255 00217000
*                 VCTWKC = VCTWKC + 1; /* BUMP VCT POINTER FOR FSR   */ 00218000
         LA    @10,1                                            01S0256 00219000
         SLR   @09,@09                                          01S0256 00220000
         IC    @09,VCTWKC(,VCCTREG)                             01S0256 00221000
         ALR   @10,@09                                          01S0256 00222000
         STC   @10,VCTWKC(,VCCTREG)                             01S0256 00223000
*               END;                                            01S0257 00224000
*             ELSE                                              01S0258 00225000
*               DO;                                             01S0258 00226000
         B     @RC00253                                         01S0258 00227000
@RF00253 DS    0H                                               01S0259 00228000
*                 DYNACCW = WKTPBSR; /* MOVE IN BSR CCW              */ 00229000
         MVC   DYNACCW(8,DYNAPTR),WKTPBSR                       01S0259 00230000
*                 VCTWKC = VCTWKC - 1; /* DECREMENT VCT POINTER FOR     00231000
*                                      BSR                           */ 00232000
         SLR   @10,@10                                          01S0260 00233000
         IC    @10,VCTWKC(,VCCTREG)                             01S0260 00234000
         BCTR  @10,0                                            01S0260 00235000
         STC   @10,VCTWKC(,VCCTREG)                             01S0260 00236000
*               END;                                            01S0261 00237000
*             DYNAPTR = DYNAPTR + LENGTH(CCW); /* BUMP TO NEXT  01S0262 00238000
*                                      AVAILABLE AREA FOR THE CCW    */ 00239000
@RC00253 AL    DYNAPTR,@CF00036                                 01S0262 00240000
*           END;                                                01S0263 00241000
@DE00252 CLC   IODBR(1,IODBREG),VCTWKC(VCCTREG)                 01S0263 00242000
         BNE   @DL00252                                         01S0263 00243000
*           WKTPRDAD = BCTRADDR;    /* SET UP INPUT BUFFER ADDRESS   */ 00244000
         L     @10,IODBBCT(,IODBREG)                            01S0264 00245000
         MVC   WKTPRDAD(3),BCTRADDR+1(@10)                      01S0264 00246000
*           DYNACCW2 = WKTPREAD;    /* SET UP READ CCW'S             */ 00247000
         MVC   DYNACCW2(16,DYNAPTR),WKTPREAD                    01S0265 00248000
*           IODBCAW = ADDR(DYNAREA); /* SET UP AREA ADDRESS          */ 00249000
         LA    @10,DYNAREA                                      01S0266 00250000
         ST    @10,IODBCAW(,IODBREG)                            01S0266 00251000
*           PFSIOWK = PFSIOWK + 1;  /* COUNT NUMBER OF DA WORKFILE      00252000
*                                      SIO'S                         */ 00253000
*                                                               01S0267 00254000
         LA    @10,1                                            01S0267 00255000
         AH    @10,PFSIOWK                                      01S0267 00256000
         STH   @10,PFSIOWK                                      01S0267 00257000
*           GO TO COMENTRY;         /* PERFORM COMMON I/O FUNCTION   */ 00258000
         B     COMENTRY                                         01S0268 00259000
*         END;                                                  01S0269 00260000
*                                                               01S0270 00261000
*   /*****************************************************************/ 00262000
*   /*                                                               */ 00263000
*   /* THIS SECTION HANDLES DIRECT ACCESS WORKFILE CCW'S             */ 00264000
*   /*                                                               */ 00265000
*   /*****************************************************************/ 00266000
*                                                               01S0270 00267000
*/*WKDA: D (YES,,NO,%AUDS) IODBCLAS = D.A. ?                         */ 00268000
*/* P OBTAIN START OF WORKFILE DATA SET FROM CTCCHHW                 */ 00269000
*/* P OBTAIN DEVICE TYPE FROM IODBTYPE                               */ 00270000
*/* P MATCH DEVICE TYPE IN TABLE                                     */ 00271000
*/* D (YES,,NO,EREXIT) DEVICE IN TABLE ?                             */ 00272000
*/* P DIVIDE VBN BY NUMBER OF WKFILE RECORDS PER TRACK               */ 00273000
*/* P CTCCHHW + QUOTIENT = CCHH OF WKFILE RECORD                     */ 00274000
*/* P R = REMAINDER FROM ABOVE DIVISION + 1                          */ 00275000
*/* P STORE CCHHR INTO IODBCCHH                                      */ 00276000
*/*WKHHMAX: D (YES,,NO,WKEND) HH >= DEVICE MAXIMUM ?                 */ 00277000
*/* P CC = CC + 1                                                    */ 00278000
*/* P (,WKHHMAX) HH = HH - DEVICE MAXIMUM                            */ 00279000
*/*WKEND: D (YES,EREXIT,NO,) IODBCCHH > CTCCHHE ?                    */ 00280000
*/* P IODBCAW = SEEK CCW                                             */ 00281000
*/* N FORMAT OF WKFILE CCW IS SEEK, SRCH, TIC, READ                  */ 00282000
*/* N SEEKADDR IS IN IODBCCHH                                        */ 00283000
*/* P (,COMENTRY) STORE BCTRADDR INTO CCW READ ADDRESS               */ 00284000
*/*%AUDS: L SVC 13 CALL AUDIT TO TAKE DUMP                           */ 00285000
*       IF IODBCLAS = DA THEN                                   01S0270 00286000
@RF00248 CLI   IODBCLAS(IODBREG),32                             01S0270 00287000
         BNE   @RF00270                                         01S0270 00288000
*         DO;                                                   01S0271 00289000
*           WKDCTPTR = ADDR(WKDCT); /* OBTAIN ADDRESS OF DEVICE TABLE*/ 00290000
         LA    WKDCTPTR,WKDCT                                   01S0272 00291000
*           DO WHILE WKDCTPTR ^= ADDR(DEND); /* SEARCH ENTIRE TABLE  */ 00292000
         B     @DE00273                                         01S0273 00293000
@DL00273 DS    0H                                               01S0274 00294000
*             IF IODBTYPE = WKDCTID THEN                        01S0274 00295000
         CLC   IODBTYPE(1,IODBREG),WKDCTID(WKDCTPTR)            01S0274 00296000
         BNE   @RF00274                                         01S0274 00297000
*               DO;                 /* IF MATCH FOUND IN TABLE  01S0275 00298000
*                                      PROCESS DEVICE                */ 00299000
*                 IODBSRCH = CTCCHHW + (IODBR / WKDCTTK); /* CCHH =     00300000
*                                      STARTING WORKFILE CCHH + 01S0276 00301000
*                                      OFFSET INTO WORKFILE DATA AREA*/ 00302000
         SLR   @10,@10                                          01S0276 00303000
         IC    @10,IODBR(,IODBREG)                              01S0276 00304000
         SLR   @09,@09                                          01S0276 00305000
         IC    @09,WKDCTTK(,WKDCTPTR)                           01S0276 00306000
         LR    @01,@10                                          01S0276 00307000
         SLR   @00,@00                                          01S0276 00308000
         DR    @00,@09                                          01S0276 00309000
         AL    @01,CTCCHHW(,CCTREG)                             01S0276 00310000
         STCM  @01,15,IODBSRCH(IODBREG)                         01S0276 00311000
*                 IODBR = (IODBR//WKDCTTK) + 1; /* R IS REMAINDER OF    00312000
*                                      ABOVE                         */ 00313000
         LA    @15,1                                            01S0277 00314000
         LR    @00,@10                                          01S0277 00315000
         SRDA  @00,32                                           01S0277 00316000
         DR    @00,@09                                          01S0277 00317000
         ALR   @00,@15                                          01S0277 00318000
         STC   @00,IODBR(,IODBREG)                              01S0277 00319000
*                 IODBCC = IODBCC + IODBHH/WKDCTTM; /* UPDATE   01S0278 00320000
*                                      CYLINDER COUNT                */ 00321000
         SLR   @10,@10                                          01S0278 00322000
         ICM   @10,3,IODBHH(IODBREG)                            01S0278 00323000
         SLR   @09,@09                                          01S0278 00324000
         IC    @09,WKDCTTM(,WKDCTPTR)                           01S0278 00325000
         LR    @01,@10                                          01S0278 00326000
         SLR   @00,@00                                          01S0278 00327000
         DR    @00,@09                                          01S0278 00328000
         SLR   @00,@00                                          01S0278 00329000
         ICM   @00,3,IODBCC(IODBREG)                            01S0278 00330000
         ALR   @01,@00                                          01S0278 00331000
         STCM  @01,3,IODBCC(IODBREG)                            01S0278 00332000
*                 IODBHH = IODBHH//WKDCTTM; /* RESET HEAD COUNT      */ 00333000
         LR    @00,@10                                          01S0279 00334000
         SRDA  @00,32                                           01S0279 00335000
         DR    @00,@09                                          01S0279 00336000
         STCM  @00,3,IODBHH(IODBREG)                            01S0279 00337000
*                 IF IODBSRCH > CTCCHHE THEN                    01S0280 00338000
         CLC   IODBSRCH(4,IODBREG),CTCCHHE(CCTREG)              01S0280 00339000
         BH    @RT00280                                         01S0280 00340000
*                   GO TO EREXIT;   /* IF OUT OF DATA EXTENTS - ERROR*/ 00341000
*                 IODBCAW = ADDR(WKDACCW); /* SET UP WORKFILE CCW       00342000
*                                      ADDRESS                       */ 00343000
         LA    @10,WKDACCW                                      01S0282 00344000
         ST    @10,IODBCAW(,IODBREG)                            01S0282 00345000
*                 WKDASKAD = ADDR(IODBSEEK); /* SET UP SEEK ADDRESS  */ 00346000
         LA    @10,IODBSEEK(,IODBREG)                           01S0283 00347000
         STCM  @10,7,WKDASKAD                                   01S0283 00348000
*                 WKDASRAD = ADDR(IODBSRCH); /* SET UP SEARCH ADDRESS*/ 00349000
         LA    @10,IODBSRCH(,IODBREG)                           01S0284 00350000
         STCM  @10,7,WKDASRAD                                   01S0284 00351000
*                 WKDARDAD = BCTRADDR; /* SET UP INPUT AREA ADDRESS  */ 00352000
         L     @10,IODBBCT(,IODBREG)                            01S0285 00353000
         MVC   WKDARDAD(3),BCTRADDR+1(@10)                      01S0285 00354000
*                 PFSIOWK = PFSIOWK + 1; /* COUNT NUMBER OF TAPE        00355000
*                                      WORKFILE SIO'S                */ 00356000
*                                                               01S0286 00357000
         AH    @15,PFSIOWK                                      01S0286 00358000
         STH   @15,PFSIOWK                                      01S0286 00359000
*                 GO TO COMENTRY;   /* GO DO I/O FOR WORKFILE        */ 00360000
         B     COMENTRY                                         01S0287 00361000
*               END;                /* ENDS PROCESSING FOR WORKFILE     00362000
*                                      DEVICE                        */ 00363000
*             WKDCTPTR = WKDCTPTR + LENGTH(WKDCTYPE); /* BUMP TO NEXT   00364000
*                                      TABLE ENTRY                   */ 00365000
@RF00274 AL    WKDCTPTR,@CF00072                                01S0289 00366000
*           END;                    /* ENDS DEVICE TABLE SEARCH      */ 00367000
@DE00273 LA    @10,DEND                                         01S0290 00368000
         CR    WKDCTPTR,@10                                     01S0290 00369000
         BNE   @DL00273                                         01S0290 00370000
*           GO TO EREXIT;           /* THIS DEVICE IS NOT SUPPORTED  */ 00371000
         B     EREXIT                                           01S0291 00372000
*         END;                      /* ENDS WORKFILE DEVICE CHECK    */ 00373000
*       VCTAUDIT = SIOAUD2;         /* SET SIO AUDIT ERROR TO   01S0293 00374000
*                                      IDENTIFY THIS CONDITION AS AN    00375000
*                                      INVALID WORKFILE DEVICE TYPE  */ 00376000
@RF00270 MVC   VCTAUDIT(2,VCCTREG),SIOAUD2                      01S0293 00377000
*       SVC (AUDSVC);               /* CALL AUDIT THIS IS A SADMP       00378000
*                                      ERROR                         */ 00379000
         SVC   13                                               01S0294 00380000
*     END;                          /* END OF WORKFILE PROCESSING    */ 00381000
*                                                               01S0296 00382000
*   /*****************************************************************/ 00383000
*   /*                                                               */ 00384000
*   /* THIS SECTION PROCESSES NORMAL OUTPUT REQUESTS. FIRST A        */ 00385000
*   /* DETERMINATION IS MADE AS TO THE AVAILABILITY OF CCW'S. IF     */ 00386000
*   /* AVAILABLE, THE CCW IS SET UP FOR I/O. IF UNAVAILABLE, A CALL  */ 00387000
*   /* TO AMDSAWAT IS MADE TO FREE SOME CCW'S.                       */ 00388000
*   /*                                                               */ 00389000
*   /*****************************************************************/ 00390000
*                                                               01S0296 00391000
*/*%OUTP: P OBTAIN CURRENT AVAILABLE CCW FROM VCTCPOUT               */ 00392000
*/* D (YES,%TOPC,NO,) I/O ACTIVE ?                                   */ 00393000
*/*CPAVAIL: D (YES,%CCWA,NO,) ANY CCW'S AVAILABLE ?                  */ 00394000
*/* P CTIOINT = 1                                                    */ 00395000
