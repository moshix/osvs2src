*                                   /* RESET TO NOP COMMAND AT THE      00487000
*                                      END OF CCWETY                 */ 00488000
*       CCWCMD = NOPCMD;            /* INSURE THIS IS A NOP COMMAND  */ 00489000
@RF00324 MVI   CCWCMD(CCWADDR),X'03'                            01S0326 00490000
*       GO TO COMENTRY;             /* PERFORM I/O OPERATION         */ 00491000
         B     COMENTRY                                         01S0327 00492000
*     END;                                                      01S0328 00493000
*                                                               01S0329 00494000
*   /*****************************************************************/ 00495000
*   /*                                                               */ 00496000
*   /* SET UP AVAILABLE CCW IN ORDER TO WRITE THE OUTPUT RECORDS     */ 00497000
*   /*                                                               */ 00498000
*   /*****************************************************************/ 00499000
*                                                               01S0329 00500000
*/*%CCWA: P SET UP TO CCW FOLLOWING LAST ACTIVE CCW                  */ 00501000
*/*%TOPC: P STORE BCTRADDR INTO 2ND WRITE                            */ 00502000
*/* N FORMAT OF CCW'S WRITE HDR DATA CHAINED TO WRITE AND TIC/NOP    */ 00503000
