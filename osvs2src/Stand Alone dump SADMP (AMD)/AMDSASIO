*       IODBBCT = BCTHOLD;          /*   LAST BCT            @YM03675*/ 00458000
         ST    BCTHOLD,IODBBCT(,IODBREG)                        01S0317 00459000
*       CCWADDR = VCTCPOUT;         /* BACK UP TO HDR CMD            */ 00460000
         L     CCWADDR,VCTCPOUT(,VCCTREG)                       01S0318 00461000
*       CCWDADDR = ADDR(BCTKEY1);   /* SET UP HEADER PORTION         */ 00462000
         L     @10,IODBBCT(,IODBREG)                            01S0319 00463000
         LA    @09,BCTKEY1(,@10)                                01S0319 00464000
