         STH   @14,PFSIOWAT                                     01S0305 00433000
*         SVC (WATSVC);             /* FREE A CHANNEL PROGRAM        */ 00434000
         SVC   1                                                01S0306 00435000
*         CTIOINT = OFF;            /* RESET FLAG                    */ 00436000
         NI    CTIOINT(CCTREG),B'11111011'                      01S0307 00437000
*        IF RETCODE = ZERO THEN DO;                        /*@YM04503*/ 00438000
