*   RESPECIFY (R8,R15) RESTRICTED;                                 0271 00281000
*   R8 = R1;                        /* GET WORK AREA POINTER         */ 00282000
         LR    R8,R1                                               0272 00283000
*   GENERATE REFS(R8) (USING @DATD,R8); /* MAKE WORK AREA ADDRESSABLE*/ 00284000
