         MVC   MDCBGRS(64,MDCBADDR),SVISAVE                        0252 00220000
*   GENERATE REFS(R1,R1) SETS(WORKSTOR) (STCTL R1,R1,WORKSTOR); /*STORE 00221000
*                                      CONTROL REG 1                 */ 00222000
         STCTL R1,R1,WORKSTOR                                           00223000
*   MDCBSTOR = WORKSTOR;            /* SAVE CONTROL REG 1 IN MDCB    */ 00224000
         MVC   MDCBSTOR(4,MDCBADDR),WORKSTOR                       0254 00225000
