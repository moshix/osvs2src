*   SVC(GTMSVC);                    /* CALL AMDSAGTM FOR A SAVE AREA */ 00016000
         SVC   4                                                01S0225 00017000
*   IF RETCODE ^= 0 THEN            /* STORAGE OBTAINED              */ 00018000
         LTR   RETCODE,RETCODE                                  01S0226 00019000
         BZ    @RF00226                                         01S0226 00020000
*     DO;                           /* NO - RETURN WITH ERROR CODE   */ 00021000
*        GENERATE REFS(R13,RETREG,RETCODE) SETS(R14,R12);       01S0228 00022000
         LM    R14,R12,12(R13)         RESTORE REGISTERS                00023000
         LA    RETCODE,4               SET ERROR RETURN CODE            00024000
         BR    RETREG                  RETURN TO CALLER                 00025000
*     END;                                                      01S0229 00026000
*   R8 = R1;                        /* GET WORK AREA ADDRESS         */ 00027000
@RF00226 LR    R8,R1                                            01S0230 00028000
*   GENERATE REFS(R8,R13) SETS(R15);                            01S0231 00029000
         USING @DATD,R8                MAKE WORK AREA ADDRESSABLE       00030000
         ST    R13,@SA00001+4          SAVE CALLERS SAVE AREA ADDR      00031000
         LA    R15,@SA00001            GET SAVE AREA ADDRESS            00032000
*   SAVENEXT = R15;                 /* LINK SAVE AREA TO CALLER'S    */ 00033000
         ST    R15,SAVENEXT(,R13)                               01S0232 00034000
*   R13 = R15;                      /* POINT TO ACQUIRED SAVE AREA   */ 00035000
         LR    R13,R15                                          01S0233 00036000
*   RESPECIFY (R1,RETCODE) UNRESTRICTED;                        01S0234 00037000
*   SAVENAME = 'UCB';               /* FILL IN MODULE ID             */ 00038000
