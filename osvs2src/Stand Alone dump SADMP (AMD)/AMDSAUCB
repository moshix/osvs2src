*   CTASM = ON;                     /* REQUEST ASM BUFFERS           */ 00066000
         OI    CTASM(@15),B'00010000'                           01S0246 00067000
*                                                               01S0247 00068000
*   /*****************************************************************/ 00069000
*   /*                                                               */ 00070000
*   /* GO FROM DIRECT ACCESS IODB TO BCT TO AMB TO DEB TO UCB.       */ 00071000
*   /* VALIDITY CHECK THE UCB.                                       */ 00072000
*   /*                                                               */ 00073000
*   /*****************************************************************/ 00074000
*                                                               01S0247 00075000
*   RESPECIFY IODB BASED(UCBIODBA); /* VIRTUAL ADDRESS OF IODB       */ 00076000
*   RESPECIFY BCT BASED(IODBBCT+VCTLPA); /* VIRTUAL ADDRESS OF BCT   */ 00077000
*   UCBIODBA = ADDR(IODBDA) + VCTLPA; /* GET VIRTUAL ADDR OF IODB    */ 00078000
         L     UCBIODBA,@CA02029                                01S0249 00079000
         AL    UCBIODBA,VCTLPA(,VCCTREG)                        01S0249 00080000
*   VCTCBADR = UCBPTR;              /* SET UP UCB ADDRESS FOR   01S0250 00081000
*                                      VALIDITY CHECK                */ 00082000
         ST    UCBPTR,VCTCBADR(,VCCTREG)                        01S0250 00083000
*   VCTCBIDN = VUCBID;              /* SET UP UCB CONTROL BLOCK ID   */ 00084000
         MVI   VCTCBIDN(VCCTREG),X'10'                          01S0251 00085000
*   SVC(VCKSVC);                    /* CALL AMDSAVCK TO VALIDITY        00086000
*                                      CHECK UCB                     */ 00087000
         SVC   25                                               01S0252 00088000
*   RESPECIFY RETCODE RESTRICTED;                               01S0253 00089000
*   IF RETCODE ^= 0 THEN            /* IS UCB INVALID                */ 00090000
         LTR   RETCODE,RETCODE                                  01S0254 00091000
         BNZ   @RT00254                                         01S0254 00092000
*     GO TO UCBEXIT;                /* YES - ERROR                   */ 00093000
*                                                               01S0255 00094000
*   /*****************************************************************/ 00095000
*   /*                                                               */ 00096000
*   /* CHECK FOR PROCESSING ON MP SYSTEM. IF NOT, SET UP             */ 00097000
*   /* IPLED CPU ADDRESS AND DEVICE ADDRESS FROM UCB.                */ 00098000
*   /*                                                               */ 00099000
*   /*****************************************************************/ 00100000
*                                                               01S0256 00101000
*   WORKCPU = CTCPUI;               /* IPLED CPU DEVICE ADDRESS      */ 00102000
         L     @14,VCTCCTV(,VCCTREG)                            01S0256 00103000
         MVC   WORKCPU(2),CTCPUI(@14)                           01S0256 00104000
*   WORKDEV = UCBCHAN;              /* SET UP DEVICE ADDRESS         */ 00105000
         MVC   WORKDEV(2),UCBCHAN(UCBPTR)                       01S0257 00106000
*   IF CTMP = OFF THEN              /* PROCESSING ON MP SYSTEM       */ 00107000
