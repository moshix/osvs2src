*   VPATH16I = DEFINE16;            /* NO PATH DEFINED               */ 00228000
         L     @14,@CA02020                                     01S0295 00229000
         ALR   @14,@06                                          01S0295 00230000
         MVC   VPATH16I(9,@14),DEFINE16                         01S0295 00231000
*   IF RETCODE ^= 0 THEN            /* WAS IT NO PATH AVAILABLE      */ 00232000
         LTR   RETCODE,RETCODE                                  01S0296 00233000
         BZ    @RF00296                                         01S0296 00234000
*     VPATH16I = AVAIL16;           /* YES-INDICATE NO PATH AVAILABLE*/ 00235000
         MVC   VPATH16I(9,@14),AVAIL16                          01S0297 00236000
*   RESPECIFY R1 RESTRICTED;                                    01S0298 00237000
@RF00296 DS    0H                                               01S0299 00238000
*   R1 = AMD016I;                   /* REQUEST MESSAGE AMD016I       */ 00239000
         LA    R1,16                                            01S0299 00240000
*   SVC (CONSVC);                   /* ISSUE MESSAGE AMD016I         */ 00241000
         SVC   3                                                01S0300 00242000
*   IODBTERM = ON;                  /* INDICATE DEVICE UNAVAILABLE   */ 00243000
         OI    IODBTERM(UCBIODBA),B'10000000'                   01S0301 00244000
*   IODBDEV = WORKDEV;              /* SET UP LAST DEVICE ADDRESS    */ 00245000
         MVC   IODBDEV(2,UCBIODBA),WORKDEV                      01S0302 00246000
*   GO TO UCBEXIT;                  /* CANNOT GET ON ANY REQUIRED CPU*/ 00247000
         B     UCBEXIT                                          01S0303 00248000
*                                                               01S0304 00249000
*   /*****************************************************************/ 00250000
*   /*                                                               */ 00251000
*   /* ON REQUESTED CPU. SEE IF DEVICE HAD A PERMANENT ERROR BEFORE. */ 00252000
*   /* IF NOT, INITIALIZE THE IODB AND SET UP A RETURN CODE OF 0.    */ 00253000
*   /*                                                               */ 00254000
*   /*****************************************************************/ 00255000
*                                                               01S0304 00256000
*UCBTERM:                                                       01S0304 00257000
*   IF IODBTERM = ON & IODBDEV = WORKDEV THEN /* PERMANENT ERROR ON     00258000
*                                      REQUIRED DEVICE               */ 00259000
UCBTERM  TM    IODBTERM(UCBIODBA),B'10000000'                   01S0304 00260000
         BNO   @RF00304                                         01S0304 00261000
         SLR   @14,@14                                          01S0304 00262000
         ICM   @14,3,WORKDEV                                    01S0304 00263000
         CH    @14,IODBDEV(,UCBIODBA)                           01S0304 00264000
         BE    @RT00304                                         01S0304 00265000
*     GO TO UCBEXIT;                /* YES - INDICATE ERROR          */ 00266000
*   IODBDEV = WORKDEV;              /* SET UP DEVICE ADDRESS         */ 00267000
@RF00304 MVC   IODBDEV(2,UCBIODBA),WORKDEV                      01S0306 00268000
*   IODBCPU = WORKCPU;              /* SET UP CPU ADDRESS            */ 00269000
         MVC   IODBCPU(2,UCBIODBA),WORKCPU                      01S0307 00270000
*   IODBCLAS = DACLASS;             /* SET DIRECT ACCESS CLASS       */ 00271000
         MVI   IODBCLAS(UCBIODBA),X'20'                         01S0308 00272000
*   IODBTYPE = UCBTBYT4;            /* SET UP UNIT TYPE              */ 00273000
         MVC   IODBTYPE(1,UCBIODBA),UCBTBYT4(UCBPTR)            01S0309 00274000
*   IODBTERM = OFF;                 /* INDICATE DEVICE AVAILABLE     */ 00275000
         NI    IODBTERM(UCBIODBA),B'01111111'                   01S0310 00276000
*   SAVEBACK -> SAVER15 = 0;        /* SET UP 0 RETURN CODE          */ 00277000
*                                                               01S0311 00278000
         L     @14,SAVEBACK(,R13)                               01S0311 00279000
         SLR   @06,@06                                          01S0311 00280000
         ST    @06,SAVER15(,@14)                                01S0311 00281000
*   /*****************************************************************/ 00282000
*   /*                                                               */ 00283000
*   /* COMMON RETURN PATH - DEQUEUE RCB, FREE SAVE AREA, AND RETURN  */ 00284000
*   /* TO CALLER.                                                    */ 00285000
*   /*                                                               */ 00286000
*   /*****************************************************************/ 00287000
*                                                               01S0312 00288000
*UCBRET:                                                        01S0312 00289000
*   VCTRCB = RCBNEXT;               /* DEQUEUE RCB                   */ 00290000
UCBRET   LA    @14,UCBRCB                                       01S0312 00291000
         MVC   VCTRCB(4,VCCTREG),RCBNEXT(@14)                   01S0312 00292000
*   CTBUFTYP = UCBCTBU;             /* RESTORE BUFFER REQUEST FLAGS  */ 00293000
         L     @14,VCTCCTV(,VCCTREG)                            01S0313 00294000
         STC   UCBCTBU,CTBUFTYP(,@14)                           01S0313 00295000
*   RESPECIFY R1 RESTRICTED;                                    01S0314 00296000
*   R1 = R8;                        /* GET ADDRESS TO BE FREED       */ 00297000
         LR    R1,R8                                            01S0315 00298000
*   SVC(FRMSVC);                    /* CALL AMDSAFRM TO FREE SAVE       00299000
*                                      AREA                          */ 00300000
         SVC   5                                                01S0316 00301000
*   RETURN;                         /* RETURN TO CALLER              */ 00302000
@EL00001 L     @13,4(,@13)                                      01S0317 00303000
@EF00001 DS    0H                                               01S0317 00304000
@ER00001 LM    @14,@12,12(@13)                                  01S0317 00305000
         BR    @14                                              01S0317 00306000
*   RESPECIFY R1 UNRESTRICTED;                                  01S0318 00307000
*                                                               01S0318 00308000
*   /*****************************************************************/ 00309000
*   /*                                                               */ 00310000
*   /* RCB ERROR EXIT                                                */ 00311000
*   /*                                                               */ 00312000
*   /*****************************************************************/ 00313000
*                                                               01S0319 00314000
*UCBEXIT:                                                       01S0319 00315000
*   SAVEBACK -> SAVER15 = 4;        /* SET UP ERROR RETURN CODE      */ 00316000
UCBEXIT  L     @14,SAVEBACK(,R13)                               01S0319 00317000
         MVC   SAVER15(4,@14),@CF00054                          01S0319 00318000
*   VCTASMIC = OFF;                 /* RESET VSAM IN CONTROL FLAG    */ 00319000
         NI    VCTASMIC(VCCTREG),B'11011111'                    01S0320 00320000
*   GO TO UCBRET;                   /* RETURN TO CALLER              */ 00321000
         B     UCBRET                                           01S0321 00322000
*                                                               01S0322 00323000
*   /*****************************************************************/ 00324000
*   /*                                                               */ 00325000
*   /* SWITCH CPUS - INPUT IS CPU ADDRESS IN WORKCPU IN WORK AREA.   */ 00326000
*   /* RETURN CODE FROM AMDSATAP IS RETURNED TO CALLER.              */ 00327000
*   /*                                                               */ 00328000
*   /*****************************************************************/ 00329000
*                                                               01S0322 00330000
*SWTCHCPU:                                                      01S0322 00331000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                            01S0322 00332000
SWTCHCPU DS    0H                                               01S0323 00333000
*   RESPECIFY (R5,RETREG,RETCODE) RESTRICTED;                   01S0323 00334000
*   SWTCHSAV = RETREG;              /* SAVE RETURN ADDRESS           */ 00335000
         ST    RETREG,SWTCHSAV                                  01S0324 00336000
*   UCBCTIO = CTIOTYPE;             /* SAVE CTIOTYPE                 */ 00337000
         L     @04,VCTCCTV(,VCCTREG)                            01S0325 00338000
         SLR   UCBCTIO,UCBCTIO                                  01S0325 00339000
         IC    UCBCTIO,CTIOTYPE(,@04)                           01S0325 00340000
*   CTIOTYPE = OFF;                 /* RESET I/O REQUEST FLAGS       */ 00341000
         MVI   CTIOTYPE(@04),B'00000000'                        01S0326 00342000
*   CTIODA = ON;                    /* REQUEST D.A. I/O              */ 00343000
*   CTIORSET = ON;                  /* INDICATE CPU RESET IS NEEDED  */ 00344000
         OI    CTIODA(@04),B'00100010'                          01S0328 00345000
*   IODBCPU = WORKCPU;              /* PLACE CPU ADDRESS IN IODB     */ 00346000
         MVC   IODBCPU(2,UCBIODBA),WORKCPU                      01S0329 00347000
*   SVC(TAPSVC);                    /* CALL AMDSATAP TO SWITCH CPUS  */ 00348000
         SVC   2                                                01S0330 00349000
*   CTIOTYPE = UCBCTIO;             /* RESTORE I/O REQUEST FLAGS     */ 00350000
         L     @04,VCTCCTV(,VCCTREG)                            01S0331 00351000
         STC   UCBCTIO,CTIOTYPE(,@04)                           01S0331 00352000
*   RETREG = SWTCHSAV;              /* RESTORE RETURN ADDRESS        */ 00353000
         L     RETREG,SWTCHSAV                                  01S0332 00354000
*   RETURN;                         /* RETURN TO CALLER              */ 00355000
@EL00002 DS    0H                                               01S0333 00356000
@EF00002 DS    0H                                               01S0333 00357000
@ER00002 BR    @14                                              01S0333 00358000
*   RESPECIFY (R5,RETREG,RETCODE) UNRESTRICTED;                 01S0334 00359000
*   END SWTCHCPU;                                               01S0335 00360000
*                                                               01S0335 00361000
*   /*****************************************************************/ 00362000
*   /*                                                               */ 00363000
*   /* THIS IS AN EXPANSION OF THE IOSGEN MACRO (MAP OPTION). IT     */ 00364000
*   /* DETERMINES THE AVAILABILITY OF ALL PATHS TO A DEVICE.         */ 00365000
*   /*                                                               */ 00366000
*   /*****************************************************************/ 00367000
*                                                               01S0336 00368000
*IOSGRTN:                                                       01S0336 00369000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                            01S0336 00370000
IOSGRTN  DS    0H                                               01S0337 00371000
