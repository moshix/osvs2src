*   RESPECIFY RCB BASED(ADDR(UCBRCB));                          01S0236 00040000
*   RCBNEXT = VCTRCB;               /* LINK TO EXISTING RCB CHAIN    */ 00041000
         LA    @06,UCBRCB                                       01S0237 00042000
         MVC   RCBNEXT(4,@06),VCTRCB(VCCTREG)                   01S0237 00043000
*   RCBEXIT = ADDR(UCBEXIT);        /* POINT TO EXIT                 */ 00044000
         LA    @15,UCBEXIT                                      01S0238 00045000
         ST    @15,RCBEXIT(,@06)                                01S0238 00046000
*   RCBBASE = BASEREG;              /* FILL IN BASE REGISTER         */ 00047000
         ST    BASEREG,RCBBASE(,@06)                            01S0239 00048000
*   RCBSAVE = R13;                  /* FILL IN SAVE AREA POINTER     */ 00049000
         ST    R13,RCBSAVE(,@06)                                01S0240 00050000
*   GENERATE REFS(R1) SETS(UCBWORK) (STCTL R1,R1,UCBWORK); /* GET       00051000
*                                      SEGMENT TABLE ORIGIN          */ 00052000
         STCTL R1,R1,UCBWORK                                            00053000
*   RCBSTOR = UCBWORK;              /* PUT SEGMENT TABLE ORIGIN IN      00054000
*                                      RCB                           */ 00055000
         LA    @06,UCBRCB                                       01S0242 00056000
         MVC   RCBSTOR(4,@06),UCBWORK                           01S0242 00057000
*   VCTRCB = ADDR(UCBRCB);          /* MAKE RCB ACTIVE               */ 00058000
         ST    @06,VCTRCB(,VCCTREG)                             01S0243 00059000
*   UCBCTBU = CTBUFTYP;             /* SAVE BUFFER REQUEST FLAGS     */ 00060000
         L     @15,VCTCCTV(,VCCTREG)                            01S0244 00061000
