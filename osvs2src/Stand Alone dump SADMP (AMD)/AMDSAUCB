         BZ    @RT00258                                         01S0258 00109000
*     GO TO UCBTERM;                /* NO - FINISHED                 */ 00110000
*                                                               01S0259 00111000
*   /*****************************************************************/ 00112000
*   /*                                                               */ 00113000
*   /* UCB IS VALID. CALL IOSGEN SUBROUTINE TO DETERMINE CPU         */ 00114000
*   /* ADDRESSES. CHECK FOR AVAILABLE PATHS.                         */ 00115000
*   /*                                                               */ 00116000
*   /*****************************************************************/ 00117000
*                                                               01S0260 00118000
*   RESPECIFY R5 RESTRICTED;                                    01S0260 00119000
*   R13 = ADDR(UCBSAVE1);           /* POINT TO IOSGEN REGISTER SAVE    00120000
*                                      AREA                          */ 00121000
         LA    R13,UCBSAVE1                                     01S0261 00122000
*   R5 = ADDR(IOSGOUT);             /* POINT TO OUTPUT AREA          */ 00123000
         LA    R5,IOSGOUT                                       01S0262 00124000
*   VCTASMIC = ON;                  /* INDICATE SADMP NOT IN CONTROL */ 00125000
         OI    VCTASMIC(VCCTREG),B'00100000'                    01S0263 00126000
*   CALL IOSGRTN;                   /* CALL IOSGEN SUBROUTINE        */ 00127000
         BAL   @14,IOSGRTN                                      01S0264 00128000
*   VCTASMIC = OFF;                 /* SADMP IN CONTROL              */ 00129000
         NI    VCTASMIC(VCCTREG),B'11011111'                    01S0265 00130000
*   R13 = RCBSAVE;                  /* POINT TO ORIGINAL SAVE AREA   */ 00131000
         LA    @14,UCBRCB                                       01S0266 00132000
         L     R13,RCBSAVE(,@14)                                01S0266 00133000
*                                                               01S0267 00134000
*   /*****************************************************************/ 00135000
*   /*                                                               */ 00136000
*   /* THIS SECTION OF CODE IS DEPENDENT ON THE FACT THAT THERE ARE 2*/ 00137000
*   /* CPUS IN THE SYSTEM, WITH CPU ADDRESSES OF 0 AND 1. THIS MUST  */ 00138000
*   /* BE CHANGED IF MORE CPUS ARE SUPPORTED.                        */ 00139000
*   /*                                                               */ 00140000
*   /*****************************************************************/ 00141000
*                                                               01S0267 00142000
*   RESPECIFY RETCODE RESTRICTED;                               01S0267 00143000
*   RETCODE = 0;                    /* SET UP 0 RETURN CODE          */ 00144000
         SLR   RETCODE,RETCODE                                  01S0268 00145000
*   DO IOSGPTR = ADDR(PRIMARY) TO ADDR(SECNDARY) BY LENGTH(IOSGFRM); /* 00146000
*                                      CHECK IOSGEN OUTPUT           */ 00147000
         LA    IOSGPTR,PRIMARY                                  01S0269 00148000
         B     @DE00269                                         01S0269 00149000
@DL00269 DS    0H                                               01S0270 00150000
*     WORKDEV = IOSGDEV;            /* SAVE DEVICE ADDRESS           */ 00151000
         MVC   WORKDEV(2),IOSGDEV(IOSGPTR)                      01S0270 00152000
*     IF IOSGAVAL(CTCPUI+1) = '01'X THEN /* PATH AVAILABLE FROM IPLED   00153000
*                                      CPU                           */ 00154000
         L     @14,VCTCCTV(,VCCTREG)                            01S0271 00155000
         LH    @14,CTCPUI(,@14)                                 01S0271 00156000
         ST    @14,@TF00001                                     01S0271 00157000
         ALR   @14,IOSGPTR                                      01S0271 00158000
         CLI   IOSGAVAL(@14),X'01'                              01S0271 00159000
         BNE   @RF00271                                         01S0271 00160000
*       DO;                         /* YES -                         */ 00161000
*         WORKCPU = CTCPUI;         /* INDICATE IPLED CPU            */ 00162000
         MVC   WORKCPU(2),@TF00001+2                            01S0273 00163000
*         GO TO UCBTERM;            /* EXIT                          */ 00164000
         B     UCBTERM                                          01S0274 00165000
*       END;                                                    01S0275 00166000
*      IF CVTCRCA ^= 0 THEN DO; /* CINTAR CHECK              @Y30CI3B*/ 00167000
@RF00271 L     @14,CVTPTR                                       01S0276 00168000
         ICM   @06,15,CVTCRCA(@14)                              01S0276 00169000
         BZ    @RF00276                                         01S0276 00170000
*        RETCODE = 4;                                      /*@Y30CI3B*/ 00171000
         LA    RETCODE,4                                        01S0278 00172000
*        GO TO UCBM16; /* WRITE NO PATH MESSAGE              @Y30CI3B*/ 00173000
         B     UCBM16                                           01S0279 00174000
*        END;                                              /*@Y30CI3B*/ 00175000
*     DO R5 = 1 TO NUMCPUS;         /* CHECK ALL CPUS                */ 00176000
@RF00276 LA    R5,1                                             01S0281 00177000
@DL00281 DS    0H                                               01S0282 00178000
*       IF IOSGAVAL(R5) = '01'X THEN /* PATH AVAILABLE               */ 00179000
         LR    @14,IOSGPTR                                      01S0282 00180000
         ALR   @14,R5                                           01S0282 00181000
         CLI   IOSGAVAL-1(@14),X'01'                            01S0282 00182000
         BNE   @RF00282                                         01S0282 00183000
*         DO;                       /* YES -                         */ 00184000
*           WORKCPU = R5 - 1;       /* INDICATE CPU NUMBER           */ 00185000
         LR    @14,R5                                           01S0284 00186000
         BCTR  @14,0                                            01S0284 00187000
         STH   @14,WORKCPU                                      01S0284 00188000
*           CALL SWTCHCPU;          /* IS CPU ACCESSIBLE             */ 00189000
         BAL   @14,SWTCHCPU                                     01S0285 00190000
*           IF RETCODE = 0 THEN     /* ON REQUESTED CPU              */ 00191000
         LTR   RETCODE,RETCODE                                  01S0286 00192000
         BZ    @RT00286                                         01S0286 00193000
*             GO TO UCBTERM;        /* YES - CONTINUE PROCESSING     */ 00194000
*         END;                                                  01S0288 00195000
*     END;                                                      01S0289 00196000
@RF00282 AH    R5,@CH00062                                      01S0289 00197000
         CH    R5,@CH00074                                      01S0289 00198000
         BNH   @DL00281                                         01S0289 00199000
*   END;                                                        01S0290 00200000
         AH    IOSGPTR,@CH00054                                 01S0290 00201000
@DE00269 LA    @14,SECNDARY                                     01S0290 00202000
         CR    IOSGPTR,@14                                      01S0290 00203000
         BNH   @DL00269                                         01S0290 00204000
*   RESPECIFY R5 UNRESTRICTED;                                  01S0291 00205000
*                                                               01S0291 00206000
*   /*****************************************************************/ 00207000
*   /*                                                               */ 00208000
*   /* ISSUE MESSAGE AMD016I (NO PATH AVAILABLE/DEFINED).            */ 00209000
*   /*                                                               */ 00210000
*   /*****************************************************************/ 00211000
*                                                               01S0292 00212000
*   IF IODBTERM = ON & IODBDEV = WORKDEV THEN /* PERMANENT ERROR ON     00213000
*                                      REQUIRED DEVICE               */ 00214000
         TM    IODBTERM(UCBIODBA),B'10000000'                   01S0292 00215000
         BNO   @RF00292                                         01S0292 00216000
         SLR   @14,@14                                          01S0292 00217000
         ICM   @14,3,WORKDEV                                    01S0292 00218000
         CH    @14,IODBDEV(,UCBIODBA)                           01S0292 00219000
         BE    @RT00292                                         01S0292 00220000
*     GO TO UCBEXIT;                /* YES - INDICATE ERROR          */ 00221000
*UCBM16: VDEV16I = UCBNAME;   /* YES - INDICATE ERROR        @Y30CI3B*/ 00222000
@RF00292 DS    0H                                               01S0294 00223000
UCBM16   L     @14,@CA02019                                     01S0294 00224000
