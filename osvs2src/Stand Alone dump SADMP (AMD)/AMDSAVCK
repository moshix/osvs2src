         TITLE 'AMDSAVCK VIRTUAL DUMP VALIDITY CHECK ROUTINE           *00001000
                        '                                               00002000
AMDSAVCK CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'AMDSAVCK  76.074'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @11,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@11                                      01S0001 00013000
*   SVC (GTMSVC);                   /* OBTAIN A WORK AREA FOR   01S0286 00014000
*                                      RE-ENTRANT CODE               */ 00015000
         SVC   4                                                01S0286 00016000
*   IF RETCODE ^= 0 THEN                                        01S0287 00017000
         LTR   RETCODE,RETCODE                                  01S0287 00018000
         BZ    @RF00287                                         01S0287 00019000
*     DO;                           /* IF CORE UNAVAILABLE- EXIT WITH   00020000
*                                      AN ERROR RETURN CODE TO CALLER*/ 00021000
*       GENERATE REFS(R12,R13,R14)SETS(R15);                    01S0289 00022000
       LM R14,R12,12(R13)                                               00023000
       LA R15,4                                                         00024000
       BR RETREG                                                        00025000
*     END;                                                      01S0290 00026000
*   R8=R1;                                                      01S0291 00027000
@RF00287 LR    R8,R1                                            01S0291 00028000
*   GEN REFS(R8,R13,R15);                                       01S0292 00029000
   USING @DATD,R8                                                       00030000
   ST R13,@SA00001+4                                                    00031000
   LA R15,@SA00001                                                      00032000
*   SAVENEXT=R15;                                               01S0293 00033000
         ST    R15,SAVENEXT(,R13)                               01S0293 00034000
*   R13=R15;                                                    01S0294 00035000
         LR    R13,R15                                          01S0294 00036000
*   SAVENAME='VCK';                                             01S0295 00037000
         MVC   SAVENAME(3,R13),@CC03325                         01S0295 00038000
*   RESPECIFY (R1,R15,RETCODE) UNRESTRICTED;                    01S0296 00039000
*   RCBNEXT = VCTRCB;               /* SAVE BACKWARD RCB CHAIN  01S0297 00040000
*                                      POINTER                       */ 00041000
         LA    @10,VRCB                                         01S0297 00042000
         L     @09,VCTRCB(,VCCTREG)                             01S0297 00043000
         ST    @09,RCBNEXT(,@10)                                01S0297 00044000
*   IF VCCTREG=VCTVCCTV THEN        /* IN TRANSLATED MODE            */ 00045000
         C     VCCTREG,VCTVCCTV(,VCCTREG)                       01S0298 00046000
         BNE   @RF00298                                         01S0298 00047000
*     DO;                                                       01S0299 00048000
*       VCKRELOC=VCTLPA;            /* SET UP FOR TRANSLATED    01S0300 00049000
*                                      ADDRESSING                    */ 00050000
         L     VCKRELOC,VCTLPA(,VCCTREG)                        01S0300 00051000
*       GEN REFS(R1) SETS(VCKSTOR);                             01S0301 00052000
       STCTL R1,R1,VCKSTOR SAVE MEMORY SEGMENT TABLE ADDRESS            00053000
*     END;                                                      01S0302 00054000
*   ELSE                                                        01S0303 00055000
*     VCKRELOC=0;                   /* NON TRANSLATED NO RELOC FACTOR*/ 00056000
         B     @RC00298                                         01S0303 00057000
@RF00298 SLR   VCKRELOC,VCKRELOC                                01S0303 00058000
*   RCBSAVE = R13;                  /* SAVE VALIDITY CHECK'S SAVE       00059000
*                                      AREA ADDRESS                  */ 00060000
@RC00298 LA    @10,VRCB                                         01S0304 00061000
         ST    R13,RCBSAVE(,@10)                                01S0304 00062000
*   VCKCB = OFF;                    /* TURN OFF ERROR SWITCH         */ 00063000
         SLR   VCKCB,VCKCB                                      01S0305 00064000
*   RCBBASE = BASEREG;              /* SAVE BASE REGISTER            */ 00065000
         ST    BASEREG,RCBBASE(,@10)                            01S0306 00066000
*   RCBEXIT = ADDR(EREXIT);         /* SET UP ERROR EXIT ADDRESS     */ 00067000
         LA    @09,EREXIT                                       01S0307 00068000
         ST    @09,RCBEXIT(,@10)                                01S0307 00069000
*   VCTRCB = ADDR(VRCB);            /* QUEUE THE VALIDITY CHECK RCB     00070000
*                                      ON VCTRCB CHAIN               */ 00071000
         ST    @10,VCTRCB(,VCCTREG)                             01S0308 00072000
*                                                               01S0309 00073000
*   /*****************************************************************/ 00074000
*   /*                                                               */ 00075000
*   /* DETERMINE THE VALIDITY OF THE INPUT REQUEST. IF IN ERROR WE   */ 00076000
*   /* HAVE A AMDSADMP BUG AND WILL TAKE A DUMP. OTHERWISE OBTAIN THE*/ 00077000
*   /* ENTRY ADDRESS                                                 */ 00078000
*   /*                                                               */ 00079000
*   /*****************************************************************/ 00080000
*                                                               01S0309 00081000
*   IF VCTCBIDN > LENGTH(VCKCBVT) / LENGTH(VCKCBVTE) THEN       01S0309 00082000
         CLI   VCTCBIDN(VCCTREG),21                             01S0309 00083000
         BNH   @RF00309                                         01S0309 00084000
*     DO;                           /* AMDSADMP ERROR IF OUT OF TABLE*/ 00085000
*       VCTAUDIT = VCKAUD1;         /* INVALID CONTROL BLOCK REQUEST */ 00086000
         MVC   VCTAUDIT(2,VCCTREG),VCKAUD1                      01S0311 00087000
*       SVC (AUDSVC);               /* TAKE A DUMP                   */ 00088000
         SVC   13                                               01S0312 00089000
*     END;                                                      01S0313 00090000
*   VCTSADMP = OFF;                 /* SET UP FOR NO DUMP CONDITION  */ 00091000
@RF00309 NI    VCTSADMP(VCCTREG),B'01111111'                    01S0314 00092000
*   INPUTCB = ((VCTCBIDN - 1) * LENGTH(VCKCBVTE)) + ADDR(VCKCBVT); /*   00093000
*                                      SET UP TO THE PROPER CONTROL     00094000
*                                      BLOCK ENTRY                   */ 00095000
         SLR   INPUTCB,INPUTCB                                  01S0315 00096000
         IC    INPUTCB,VCTCBIDN(,VCCTREG)                       01S0315 00097000
         BCTR  INPUTCB,0                                        01S0315 00098000
         SLA   INPUTCB,4                                        01S0315 00099000
         LA    @10,VCKCBVT                                      01S0315 00100000
         ALR   INPUTCB,@10                                      01S0315 00101000
*   VCTCBID = CBVTID;               /* MOVE CONTROL BLOCK NAME INTO     00102000
*                                      VCCT                          */ 00103000
         MVC   VCTCBID(8,VCCTREG),CBVTID(INPUTCB)               01S0316 00104000
*   WORKCBID=CBVTID;                /* SAVE CONTROL BLOCK NAME IN       00105000
*                                      WORK AREA                     */ 00106000
         MVC   WORKCBID(8),CBVTID(INPUTCB)                      01S0317 00107000
*   WORKCBAD=VCTCBADR;                                          01S0318 00108000
         L     @10,VCTCBADR(,VCCTREG)                           01S0318 00109000
         ST    @10,WORKCBAD                                     01S0318 00110000
*   IF VCTCBADR=0 THEN                                          01S0319 00111000
         LTR   @10,@10                                          01S0319 00112000
         BNZ   @RF00319                                         01S0319 00113000
*     DO;                           /* ZERO IS INVALID CTL BLK ADDR  */ 00114000
*       VCKCB=ON;                                               01S0321 00115000
         LA    VCKCB,1                                          01S0321 00116000
*       GO TO EREXIT;               /* EXIT ON ZERO ADDRESS          */ 00117000
         B     EREXIT                                           01S0322 00118000
*     END;                                                      01S0323 00119000
*                                                               01S0324 00120000
*   /*****************************************************************/ 00121000
*   /*                                                               */ 00122000
*   /* DOES THIS CONTROL BLOCK HAVE AN EBCDIC/CHARACTER CHECK? IF SO,*/ 00123000
*   /* CHECK THE CONTROL BLOCK FOR VALIDITY.                         */ 00124000
*   /*                                                               */ 00125000
*   /*****************************************************************/ 00126000
*                                                               01S0324 00127000
*   IF CHARCK = ON THEN                                         01S0324 00128000
@RF00319 TM    CHARCK(INPUTCB),B'10000000'                      01S0324 00129000
         BNO   @RF00324                                         01S0324 00130000
*     DO;                           /* IF IN CHARACTER CHECK MODE    */ 00131000
*       VCKCB = ON;                 /* CONTROL BLOCK CHECKING IN        00132000
*                                      PROGRESS                      */ 00133000
         LA    VCKCB,1                                          01S0326 00134000
*       RESPECIFY CHARCOMP BASED (WORKCBAD+TABOFF);             01S0327 00135000
*       IF CHARCOMP(1:TABLNG) ^= TABCHR(1:TABLNG) THEN          01S0328 00136000
         L     @10,CBVTEADR-1(,INPUTCB)                         01S0328 00137000
         LA    @10,0(,@10)                                      01S0328 00138000
         ALR   @10,VCKRELOC                                     01S0328 00139000
         LH    @09,TABOFF(,@10)                                 01S0328 00140000
         AL    @09,WORKCBAD                                     01S0328 00141000
         SLR   @04,@04                                          01S0328 00142000
         IC    @04,TABLNG(,@10)                                 01S0328 00143000
         BCTR  @04,0                                            01S0328 00144000
         EX    @04,@SC03359                                     01S0328 00145000
         BNE   @RT00328                                         01S0328 00146000
*         GO TO EREXIT;                                         01S0329 00147000
*/* IF THE VALIDITY CHECK FAILS--PROVIDE A RETURN CODE OF 4, OTHERWISE  00148000
*CONTINUE THE VALIDITY CHECKING (IF ANY REMAINS)                     */ 00149000
*       VCKCB = OFF;                /* BACK INTO SIMPLY VALIDITY        00150000
*                                      CHECK CODE                    */ 00151000
         SLR   VCKCB,VCKCB                                      01S0330 00152000
*     END;                                                      01S0331 00153000
*                                                               01S0332 00154000
*   /*****************************************************************/ 00155000
*   /*                                                               */ 00156000
*   /* NOW A CHECK FOR UNIQUE CONTROL BLOCK PROCESSING OCCURS. EACH  */ 00157000
*   /* ROUTINE UNIQUELY PROCESSES THE CONTROL BLOCK                  */ 00158000
*   /*                                                               */ 00159000
*   /*****************************************************************/ 00160000
*                                                               01S0332 00161000
*   IF SPECIAL = OFF THEN                                       01S0332 00162000
@RF00324 TM    SPECIAL(INPUTCB),B'01000000'                     01S0332 00163000
         BZ    @RT00332                                         01S0332 00164000
*     GO TO VCKRC0;                 /* IF NO SPECIAL CHECKS ARE TO BE   00165000
*                                      PERFORMED, THE CONTROL BLOCK     00166000
*                                      HAS BEEN PROPERLY VALIDITY       00167000
*                                      CHECKED                       */ 00168000
*   RESPECIFY R15 RESTRICTED;                                   01S0334 00169000
*   R15 = CBVTESPC+VCKRELOC;        /* ADDRESS OF SPECIAL PROCESSING    00170000
*                                      ROUTINE                       */ 00171000
         LR    R15,VCKRELOC                                     01S0335 00172000
         AL    R15,CBVTESPC(,INPUTCB)                           01S0335 00173000
*   GEN REFS(R15) FLOWS(VCKDQE,VCKSPQE,VCKSGTB,VCKRSMH,VCKUCB); 01S0336 00174000
   BR R15 GIVE CONTROL TO SPECIAL HANDLER                               00175000
*                                                               01S0337 00176000
*   /*****************************************************************/ 00177000
*   /*                                                               */ 00178000
*   /* FIRST WE HAVE THE SPQE CHECK. THE CORRESPONDING DQE IS        */ 00179000
*   /* OBTAINED FROM THE SPQE AND THE NORMAL DQE CHECK IS THEN USED  */ 00180000
*   /*                                                               */ 00181000
*   /*****************************************************************/ 00182000
*                                                               01S0337 00183000
*VCKSPQE:                                                       01S0337 00184000
*   RESPECIFY SPQESECT BASED(WORKCBAD);                         01S0337 00185000
VCKSPQE  DS    0H                                               01S0338 00186000
*   VCKCB = ON;                     /* PROCESSING THE SPQE ADDRESS   */ 00187000
         LA    VCKCB,1                                          01S0338 00188000
*   DQESVE = SPDQEPTR;              /* OBTAIN THE DQE FROM THE SPQE  */ 00189000
         L     @14,WORKCBAD                                     01S0339 00190000
         L     DQESVE,SPDQEPTR(,@14)                            01S0339 00191000
*   IF SPSHARE=ON THEN                                          01S0340 00192000
         TM    SPSHARE(@14),B'10000000'                         01S0340 00193000
         BNO   @RF00340                                         01S0340 00194000
*     DQESVE=DQESVE->SPDQEPTR;      /* GET OWNING SPQE               */ 00195000
         L     DQESVE,SPDQEPTR(,DQESVE)                         01S0341 00196000
*   VCKCB = OFF;                    /* BACK INTO VALIDITY CHECK CODE */ 00197000
@RF00340 SLR   VCKCB,VCKCB                                      01S0342 00198000
*   IF DQESVE=0 THEN                                            01S0343 00199000
         LTR   DQESVE,DQESVE                                    01S0343 00200000
         BZ    @RT00343                                         01S0343 00201000
*     GO TO VCKRC0;                 /* DQE PTR MAY BE 0              */ 00202000
*   GO TO VCKDQEB;                  /* ENTER DQE CHECKING            */ 00203000
         B     VCKDQEB                                          01S0345 00204000
*                                                               01S0346 00205000
*   /*****************************************************************/ 00206000
*   /*                                                               */ 00207000
*   /* THIS SECTION OF CODE TESTS THE VALIDITY OF A DQE. A SECONDARY */ 00208000
*   /* ENTRY EXISTS FOR SPQE PROCESSING                              */ 00209000
*   /*                                                               */ 00210000
*   /*****************************************************************/ 00211000
*                                                               01S0346 00212000
*VCKDQE:                                                        01S0346 00213000
*   DQESVE = WORKCBAD;              /* PICK UP DQE ADDRESS TO BE        00214000
*                                      TESTED                        */ 00215000
*                                                               01S0346 00216000
VCKDQE   L     DQESVE,WORKCBAD                                  01S0346 00217000
*VCKDQEB:                                                       01S0347 00218000
*   RESPECIFY DQESECT BASED (DQESVE);                           01S0347 00219000
VCKDQEB  DS    0H                                               01S0348 00220000
*   VCKCB = ON;                     /* READY TO TEST THE DQE         */ 00221000
         LA    VCKCB,1                                          01S0348 00222000
*   IF (DQEBLKAD // PGEBDY = ZERO) & (DQELNTH // PGEBDY = ZERO) THEN    00223000
         SLR   @14,@14                                          01S0349 00224000
         L     @10,@CF03281                                     01S0349 00225000
         L     @00,DQEBLKAD(,DQESVE)                            01S0349 00226000
         SRDA  @00,32                                           01S0349 00227000
         DR    @00,@10                                          01S0349 00228000
         CR    @00,@14                                          01S0349 00229000
         BNE   @RF00349                                         01S0349 00230000
         L     @00,DQELNTH(,DQESVE)                             01S0349 00231000
         SRDA  @00,32                                           01S0349 00232000
         DR    @00,@10                                          01S0349 00233000
         CR    @00,@14                                          01S0349 00234000
         BE    @RT00349                                         01S0349 00235000
*     GO TO VCKRC0;                 /* IF ADDRESS AND LENGTH IN DQE     00236000
*                                      ARE MULTIPLE OF 4K THEN THE      00237000
*                                      DQE IS VALID                  */ 00238000
*   GO TO EREXIT;                   /* OTHERWISE AN ERROR EXISTS     */ 00239000
         B     EREXIT                                           01S0351 00240000
*                                                               01S0352 00241000
*   /*****************************************************************/ 00242000
*   /*                                                               */ 00243000
*   /* THE SEGMENT TABLE CHECK DETERMINES IF LOW CORE INDEED MAPS 1:1*/ 00244000
*   /*                                                               */ 00245000
*   /*****************************************************************/ 00246000
*                                                               01S0352 00247000
*VCKSGTB:                                                       01S0352 00248000
*   STEPTR = WORKCBAD;              /* SET UP SEGMENT TABLE ADDRESS  */ 00249000
VCKSGTB  L     STEPTR,WORKCBAD                                  01S0352 00250000
*   IF VCKRELOC ^= 0 THEN                                       01S0353 00251000
         LTR   VCKRELOC,VCKRELOC                                01S0353 00252000
         BZ    @RF00353                                         01S0353 00253000
*     DO;                           /* SEE IF RUNNING TRANSLATED     */ 00254000
*       RFY PGTPTE BASED(VCTPGT+VCTLPA);                        01S0355 00255000
*       GEN (PTLB) NOREFS NOSETS;                          /*@Z40WI3P*/ 00256000
         PTLB                                                           00257000
*       PGTRSA=EVAL(WORKCBAD/PAGESIZE)*16; /* GET THE RBN            */ 00258000
         L     @14,VCTLPA(,VCCTREG)                             01S0357 00259000
         L     @10,VCTPGT(,VCCTREG)                             01S0357 00260000
         ALR   @10,@14                                          01S0357 00261000
         L     @09,WORKCBAD                                     01S0357 00262000
         LR    @04,@09                                          01S0357 00263000
         SRL   @04,12                                           01S0357 00264000
         SLA   @04,4                                            01S0357 00265000
         STCM  @04,3,PGTRSA(@10)                                01S0357 00266000
*       STEPTR=VCTLPA+(WORKCBAD&'00000FFF'X); /* SET SEGMENT TBL PTR    00267000
*                                      TO VIRTUAL ADDRESS            */ 00268000
         N     @09,@CF03340                                     01S0358 00269000
         ALR   @14,@09                                          01S0358 00270000
         LR    STEPTR,@14                                       01S0358 00271000
*     END;                                                      01S0359 00272000
*   VCKCB = ON;                     /* PROCESSING THE SEGMENT TABLE  */ 00273000
@RF00353 LA    VCKCB,1                                          01S0360 00274000
*   IF SGTLK^='F0'X|EVAL(STEPTR+4)->SGTLK^='F0'X|               01S0361 00275000
*         EVAL(STEPTR+(NUMSEG-2)*LENGTH(SGTSTE))->SGTLK^='F0'X| 01S0361 00276000
*         EVAL(STEPTR+(NUMSEG-1)*LENGTH(SGTSTE))->SGTLK^='F0'X THEN     00277000
         CLI   SGTLK(STEPTR),X'F0'                              01S0361 00278000
         BNE   @RT00361                                         01S0361 00279000
         LA    @14,4                                            01S0361 00280000
         ALR   @14,STEPTR                                       01S0361 00281000
         CLI   SGTLK(@14),X'F0'                                 01S0361 00282000
         BNE   @RT00361                                         01S0361 00283000
         LA    @14,1016                                         01S0361 00284000
         ALR   @14,STEPTR                                       01S0361 00285000
         CLI   SGTLK(@14),X'F0'                                 01S0361 00286000
         BNE   @RT00361                                         01S0361 00287000
         LA    @14,1020                                         01S0361 00288000
         ALR   @14,STEPTR                                       01S0361 00289000
         CLI   SGTLK(@14),X'F0'                                 01S0361 00290000
         BE    @RF00361                                         01S0361 00291000
@RT00361 DS    0H                                               01S0362 00292000
*     DO;                                                       01S0362 00293000
*       IF VCKRELOC ^= 0 THEN       /* RUNNING TRANSLATED            */ 00294000
         LTR   VCKRELOC,VCKRELOC                                01S0363 00295000
         BZ    @RF00363                                         01S0363 00296000
*         PGTPVM=ON;                /* YES - INVALIDATE THE PAGE        00297000
*                                      ENTRY                         */ 00298000
         L     @14,VCTPGT(,VCCTREG)                             01S0364 00299000
         AL    @14,VCTLPA(,VCCTREG)                             01S0364 00300000
         OI    PGTPVM(@14),B'00001000'                          01S0364 00301000
*       GO TO EREXIT;               /* IF NOT, THEN HAVE AN INVALID     00302000
*                                      SEGMENT TABLE                 */ 00303000
         B     EREXIT                                           01S0365 00304000
*     END;                                                      01S0366 00305000
*   IF VCKRELOC ^= 0 THEN           /* RUNNING TRANSLATED            */ 00306000
@RF00361 LTR   VCKRELOC,VCKRELOC                                01S0367 00307000
         BZ    @RF00367                                         01S0367 00308000
*     PGTPVM=ON;                    /* YES - INVALIDATE THE PAGE        00309000
*                                      ENTRY                         */ 00310000
         L     @14,VCTPGT(,VCCTREG)                             01S0368 00311000
         AL    @14,VCTLPA(,VCCTREG)                             01S0368 00312000
         OI    PGTPVM(@14),B'00001000'                          01S0368 00313000
*   GO TO VCKRC0;                                               01S0369 00314000
         B     VCKRC0                                           01S0369 00315000
*                                                               01S0370 00316000
*   /*****************************************************************/ 00317000
*   /*                                                               */ 00318000
*   /* RSMHDR VALIDITY CHECKING VERIFIES IF THE ASCB IN THE RSM      */ 00319000
*   /* HEADER MATCHES THE ASCB BEING PROCESSED                       */ 00320000
*   /*                                                               */ 00321000
*   /*****************************************************************/ 00322000
*                                                               01S0370 00323000
*VCKRSMH:                                                       01S0370 00324000
*   RSMHDPTR = WORKCBAD;            /* POINT TO THE RSM HEADER TO BE    00325000
*                                      VALIDITY CHECKED              */ 00326000
VCKRSMH  L     RSMHDPTR,WORKCBAD                                01S0370 00327000
*   VCKCB = ON;                     /* READY TO CHECK THE RSM HEADER */ 00328000
         LA    VCKCB,1                                          01S0371 00329000
*   IF VCTASCB = RSMASCB THEN                                   01S0372 00330000
         CLC   VCTASCB(4,VCCTREG),RSMASCB(RSMHDPTR)             01S0372 00331000
         BE    @RT00372                                         01S0372 00332000
*     GO TO VCKRC0;                 /* IF VALID-ZERO RETURN CODE     */ 00333000
*   GO TO EREXIT;                   /* ERROR RETURN CODE             */ 00334000
         B     EREXIT                                           01S0374 00335000
*                                                               01S0375 00336000
*   /*****************************************************************/ 00337000
*   /*                                                               */ 00338000
*   /* UCB VALIDITY CHECK FUNCTION                                   */ 00339000
*   /*                                                               */ 00340000
*   /*****************************************************************/ 00341000
*                                                               01S0375 00342000
*VCKUCB:                                                        01S0375 00343000
*   RESPECIFY R1 RESTRICTED;                                    01S0375 00344000
VCKUCB   DS    0H                                               01S0376 00345000
*   RESPECIFY UCBOB BASED (WORKCBAD);                           01S0376 00346000
*   CVTBINA = ADDR(UCBUA);          /* POINTER TO DEVICE TO CONVERT  */ 00347000
         L     @14,WORKCBAD                                     01S0377 00348000
         LA    @14,UCBUA(,@14)                                  01S0377 00349000
         ST    @14,CVTBINA                                      01S0377 00350000
*   CVTEBCA = ADDR(CVTAREA);        /* OUTPUT AREA ADDRESS           */ 00351000
         LA    @14,CVTAREA                                      01S0378 00352000
         ST    @14,CVTEBCA                                      01S0378 00353000
*   CVTINNL = 1;                    /* LENGTH OF DEVICE ADDRESS      */ 00354000
         MVC   CVTINNL(2),@CH00076                              01S0379 00355000
*   R1 = ADDR(CVTBLK);              /* SET UP CONVERT PARAMETER LIST */ 00356000
         LA    R1,CVTBLK                                        01S0380 00357000
*   VCKCB = ON;                     /* UCB VALIDITY CHECK IN PROGRESS*/ 00358000
         LA    VCKCB,1                                          01S0381 00359000
*   SVC (BINSVC);                   /* GO COVERT THE DEVICE ADDRESS  */ 00360000
         SVC   28                                               01S0382 00361000
*   RESPECIFY R1 UNRESTRICTED;                                  01S0383 00362000
*   IF CVTAREA ^= UCBNAME(2:3) | UCBTBYT3 ^= DACLASS THEN       01S0384 00363000
         L     @14,WORKCBAD                                     01S0384 00364000
         CLC   CVTAREA(2),UCBNAME+1(@14)                        01S0384 00365000
         BNE   @RT00384                                         01S0384 00366000
         CLI   UCBTBYT3(@14),X'20'                              01S0384 00367000
         BNE   @RT00384                                         01S0384 00368000
*     GO TO EREXIT;                 /* IF DEVICE IS NOT VALID, OR NOT   00369000
*                                      DA, RETURN ERROR              */ 00370000
*   DO UTYPEPTR = ADDR(WKDCT) + VCTLPA BY LENGTH(WKDCTYPE)      01S0386 00371000
*        WHILE WKDCTID ^= 'FF'X;    /* SEARCH DEVICE CHARACTERISTICS    00372000
*                                      TABLE FOR SUPPORTED UNIT TYPE */ 00373000
         L     UTYPEPTR,@CA03315                                01S0386 00374000
         AL    UTYPEPTR,VCTLPA(,VCCTREG)                        01S0386 00375000
         B     @DE00386                                         01S0386 00376000
@DL00386 DS    0H                                               01S0387 00377000
*     IF UCBTBYT4 = WKDCTID THEN                                01S0387 00378000
         L     @14,WORKCBAD                                     01S0387 00379000
         CLC   UCBTBYT4(1,@14),WKDCTID(UTYPEPTR)                01S0387 00380000
         BE    @RT00387                                         01S0387 00381000
*        GO TO VCKRC0;              /* YES - UCB IS OKAY             */ 00382000
