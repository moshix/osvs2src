         TITLE 'AMDSAWAT VIRTUAL DUMP WAIT ROUTINE                     *00001000
                       '                                                00002000
AMDSAWAT CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'AMDSAWAT  73.234'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @11,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@11                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
*   SAVEFORM(1:4) = 'LWAT';         /* MODULE ID TO SAVE AREA        */ 00018000
         MVC   SAVEFORM(4,R13),@CC00968                            0195 00019000
*   WRCBNEXT = VCTRCB;              /* CHAIN WAIT'S RCB'S TO NEXT ON    00020000
*                                      QUEUE                         */ 00021000
         MVC   WRCBNEXT(4),VCTRCB(VCCTREG)                         0196 00022000
*   WRCBSAVE = R13;                 /* SAVE AREA ADDRESS TO RCB      */ 00023000
         ST    R13,WRCBSAVE                                        0197 00024000
*   WRCBASE = BASEREG;              /* SAVE BASE REGISTER FOR THE  0198 00025000
*                                      RCBEXIT TO HAVE ADDRESSABILITY   00026000
*                                      WHEN IT GAINS CONTROL         */ 00027000
         ST    BASEREG,WRCBASE                                     0198 00028000
*   VCTRCB = ADDR(WATRCB);          /* PLACE WAIT'S RCB ON THE VCTRCB   00029000
*                                      QUEUE                         */ 00030000
         LA    @10,WATRCB                                          0199 00031000
         ST    @10,VCTRCB(,VCCTREG)                                0199 00032000
*   CCTREG = CCTADDR;               /* SET CCT BASE                  */ 00033000
         LH    CCTREG,@CH00738                                     0200 00034000
*                                                                  0201 00035000
*   /*****************************************************************/ 00036000
*   /*                                                               */ 00037000
*   /* NOW WAIT MUST PERFORM TWO TYPES OF INITIALIZATION TO PROCESS  */ 00038000
*   /* THE INPUT REQUEST. THE FIRST MODIFIES THE WAIT BIT IN THE WAIT*/ 00039000
*   /* ENABLED PSW. THIS IS DONE TO ALLOW WAIT TO PROCESS AS MANY    */ 00040000
*   /* PENDING INTERRUPTS AS POSSIBLE BEFORE ENTERING A WAIT STATE TO*/ 00041000
*   /* WAIT UPON THE COMPLETION OF THE REQUESTED I/O EVENT. THE      */ 00042000
*   /* SECOND STAGE IN PROCESSING THE INPUT REQUEST ANALYZES AND SETS*/ 00043000
*   /* WAIT UP TO ADDRESS THE TARGET IODB                            */ 00044000
*   /*                                                               */ 00045000
*   /*****************************************************************/ 00046000
*/* P TURN OFF WAIT BIT IN ENABLED PSW                               */ 00047000
*/* D (YES,WATDA,NO,) CTIOTYPE = DA ?                                */ 00048000
*/* D (YES,WATWK,NO,) CTIOTYPE = WORKFILE ?                          */ 00049000
*/* D (YES,WATOUT,NO,AUDEXIT) CTIOTYPE = OUTPUT ?                    */ 00050000
*/*WATDA: P (,WATBCTCK) IODBREG = ADDRESS OF DA IODB                 */ 00051000
*/*WATWK: P (,WATBCTCK) IODBREG = ADDRESS OF WORKFILE IODB           */ 00052000
*/*WATOUT: P (,WATBCTCK) IODBREG = ADDRESS OF OUTPUT IODB            */ 00053000
*/*AUDEXIT: L (,EREXIT) AMDSAAUD: SVC 13 DUMP ON ERROR CONDITION     */ 00054000
*                                                                  0201 00055000
*   PSWAIT = OFF;                   /* INSURE WAIT BIT OF PSW IS OFF */ 00056000
         LA    @10,EWAITPSW                                        0201 00057000
         NI    PSWAIT(@10),B'11111101'                             0201 00058000
*   IF CTIODA = ON THEN                                            0202 00059000
         TM    CTIODA(CCTREG),B'00100000'                          0202 00060000
         BNO   @RF00202                                            0202 00061000
*     IODBREG = ADDR(IODBDA);       /* DA REQUESTS USE THE DA IODB   */ 00062000
         L     IODBREG,@CA00957                                    0203 00063000
*   ELSE                                                           0204 00064000
*     IF CTIOWK = ON THEN                                          0204 00065000
         B     @RC00202                                            0204 00066000
@RF00202 TM    CTIOWK(CCTREG),B'00010000'                          0204 00067000
         BNO   @RF00204                                            0204 00068000
*       IODBREG = ADDR(IODBWORK);   /* WORKFILE REQUESTS USE THE   0205 00069000
*                                      WORKFILE IODB                 */ 00070000
         L     IODBREG,@CA00959                                    0205 00071000
*     ELSE                                                         0206 00072000
*       IF CTIOOUT = ON THEN                                       0206 00073000
         B     @RC00204                                            0206 00074000
@RF00204 TM    CTIOOUT(CCTREG),B'01000000'                         0206 00075000
         BNO   @RF00206                                            0206 00076000
*         IODBREG = ADDR(IODBOUT);  /* OUTPUT REQUESTS USE THE OUTPUT   00077000
*                                      IODB                          */ 00078000
         L     IODBREG,@CA00958                                    0207 00079000
*       ELSE                                                       0208 00080000
*         DO;                                                      0208 00081000
         B     @RC00206                                            0208 00082000
@RF00206 DS    0H                                                  0209 00083000
*           VCTAUDIT = WATAUD1;     /* SET UP UNIQUE AUDIT CODE TO 0209 00084000
*                                      INDICATE AN INVALID INPUT   0209 00085000
*                                      REQUEST                       */ 00086000
         MVC   VCTAUDIT(2,VCCTREG),WATAUD1                         0209 00087000
*           SVC (AUDSVC);           /* ERROR INPUT REQUEST - SADMP 0210 00088000
*                                      ERROR                         */ 00089000
         SVC   13                                                  0210 00090000
*           GEN NOSEQFLOW EXIT;     /* NO RETURN FROM AMDSAAUD       */ 00091000
*         END;                                                     0212 00092000
*                                                                  0213 00093000
*   /*****************************************************************/ 00094000
*   /*                                                               */ 00095000
*   /* NOW THE STATE OF AFFAIRS MUST BE EXAMINED. FIRST WE ASK IF    */ 00096000
*   /* THERE REALLY IS ANYTHING TO WAIT FOR ANYWAY. THE CALLER MAY BE*/ 00097000
*   /* CALLING US JUST TO INSURE NO I/O IS ACTIVE. IF THERE IS NO    */ 00098000
*   /* ACTIVE I/O LIFE CAN CONTUNUE AS NORMAL, AND WE CAN CHECK THE  */ 00099000
*   /* DEVICE STATE                                                  */ 00100000
*   /*                                                               */ 00101000
*   /*****************************************************************/ 00102000
*/*WATBCTCK: D (YES,WATERMCK,NO,RCSET0) IODBACTV = 1 ?               */ 00103000
*/*WATERMCK: D (YES,PURGEQ,NO,WATINTCK) IODBTERM = 1 ?               */ 00104000
*                                                                  0213 00105000
*   IF IODBACTV = OFF & IODBINT = OFF THEN /* IF NO I/O ACTIVE TO  0213 00106000
*                                      WAIT ON RETURN                */ 00107000
@RC00206 DS    0H                                                  0213 00108000
@RC00204 DS    0H                                                  0213 00109000
@RC00202 TM    IODBACTV(IODBREG),B'00100100'                       0213 00110000
         BZ    @RT00213                                            0213 00111000
*     GO TO RCSET0;                 /* NORMAL RETURN TO CALLER AS ALL   00112000
*                                      I/O ACTIVITY HAS FINISHED     */ 00113000
*   IF IODBTERM = ON THEN                                          0215 00114000
@RF00213 TM    IODBTERM(IODBREG),B'10000000'                       0215 00115000
         BO    @RT00215                                            0215 00116000
*     GO TO PURGEQ;                 /* TERMINAL ERROR ON DEVICE- SET    00117000
*                                      I/O ERROR FLAGS               */ 00118000
*                                                                  0217 00119000
*   /*****************************************************************/ 00120000
*   /*                                                               */ 00121000
*   /* NOW THE QUESTION IS HAS AN INTERRUPT OCCURRED FOR THIS DEVICE,*/ 00122000
*   /* IF NO, WE MUST WAIT FOR THE INTERRUPT                         */ 00123000
*   /*                                                               */ 00124000
*   /*****************************************************************/ 00125000
*/*WATINTCK: D (YES,PROINT,NO,ENADIS) IODBINT = 1 ?                  */ 00126000
*/*ENADIS: L AMDSATAP: ISSUE SVC 2 TO GET ON PROPER CPU              */ 00127000
*/* D (YES,,NO,EREXIT) ON PROPER CPU ?                               */ 00128000
*/* I ENABLE/DISABLE FOR I/O INTERRUPT                               */ 00129000
*/* D (YES,,NO,NOIOINT) DID AN I/O INTERRUPT OCCUR                   */ 00130000
*/* L AMDSAIOI: LPSW TO IOI TO PROCESS INTERRUPT                     */ 00131000
*                                                                  0217 00132000
*   IF IODBINT = OFF THEN                                          0217 00133000
         TM    IODBINT(IODBREG),B'00100000'                        0217 00134000
         BNZ   @RF00217                                            0217 00135000
*     DO;                           /* NO INTERRUPT PATH             */ 00136000
*                                                                  0218 00137000
*ENADIS:                                                           0219 00138000
*       SVC (TAPSVC);               /* GET ON PROPER CPU             */ 00139000
ENADIS   SVC   2                                                   0219 00140000
*       RESPECIFY RETCODE RESTRICTED;                              0220 00141000
*       IF RETCODE ^= ZERO THEN                                    0221 00142000
         LTR   RETCODE,RETCODE                                     0221 00143000
         BNZ   @RT00221                                            0221 00144000
*         GO TO EREXIT;             /* CANNOT GET ON PROPER CPU      */ 00145000
*       RESPECIFY RETCODE UNRESTRICTED;                            0223 00146000
*       IONPSW = FLCINPSW;          /* SAVE I/O NEW PSW              */ 00147000
         MVC   IONPSW(8),FLCINPSW                                  0224 00148000
*       FLCINPSW = WATPSW;          /* CHANGE TO RETURN CONTROL TO 0225 00149000
*                                      WAT                           */ 00150000
         MVC   FLCINPSW(8),WATPSW                                  0225 00151000
*       IF PSWAIT = ON THEN         /* IS THIS A WAIT PSW            */ 00152000
         LA    @10,EWAITPSW                                        0226 00153000
         TM    PSWAIT(@10),B'00000010'                             0226 00154000
         BNO   @RF00226                                            0226 00155000
*         DO;                                                      0227 00156000
*           EWAITCD = 'F0'X;        /* YES--SET WAIT CODE            */ 00157000
         MVI   EWAITCD,X'F0'                                       0228 00158000
*           EWAITDEV = IODBDEV;     /* SET CUU INTO WAIT PSW         */ 00159000
         MVC   EWAITDEV(2),IODBDEV(IODBREG)                        0229 00160000
*         END;                                                     0230 00161000
*       ELSE                                                       0231 00162000
*         EWAITAD = ADDR(DISABRET); /* NO--SET ADDRESS OF NEXT     0231 00163000
*                                      INSTRUCTION                   */ 00164000
         B     @RC00226                                            0231 00165000
@RF00226 LA    @10,DISABRET                                        0231 00166000
         STCM  @10,7,EWAITAD                                       0231 00167000
*       PFWATEN = PFWATEN + 1;      /* INCREMENT ENABLED DISABLED  0232 00168000
*                                      COUNTER                       */ 00169000
*                                                                  0232 00170000
@RC00226 LA    @10,1                                               0232 00171000
         AH    @10,PFWATEN                                         0232 00172000
         STH   @10,PFWATEN                                         0232 00173000
*       LPSW (EWAITPSW);            /* ENABLE FOR I/O INTERRUPTS     */ 00174000
         LPSW  EWAITPSW                                            0233 00175000
*       GEN FLOWS(DISABRET,IOINT);                                 0234 00176000
*                                                                  0234 00177000
*   /*****************************************************************/ 00178000
*   /*                                                               */ 00179000
*   /*   NO I/O INTERRUPT OCCURRED.                                  */ 00180000
*   /*                                                               */ 00181000
*   /*****************************************************************/ 00182000
*                                                                  0235 00183000
*DISABRET:                                                         0235 00184000
*       FLCINPSW = IONPSW;          /* RESTORE I/O NEW PSW           */ 00185000
DISABRET MVC   FLCINPSW(8),IONPSW                                  0235 00186000
*       GO TO NOIOINT;              /* CHECK FOR OTHER DEVICES       */ 00187000
*                                                                  0236 00188000
         B     NOIOINT                                             0236 00189000
*   /*****************************************************************/ 00190000
*   /*                                                               */ 00191000
*   /* AN I/O INTERRUPT HAS OCCURRED. CONTROL IS PASSED TO           */ 00192000
*   /* AMDSAIOI TO PROCESS THE INTERRUPT.                            */ 00193000
*   /*                                                               */ 00194000
*   /*****************************************************************/ 00195000
*                                                                  0237 00196000
*                                                                  0237 00197000
*IOINT:                                                            0237 00198000
*       FLCINPSW = IONPSW;          /* RESTORE I/O NEW PSW           */ 00199000
IOINT    MVC   FLCINPSW(8),IONPSW                                  0237 00200000
*       FLCIOPSW = WATRET;          /* SET I/O OLD PSW TO SHOW HOW 0238 00201000
*                                      CONTROL WANTED BACK           */ 00202000
         MVC   FLCIOPSW(8),WATRET                                  0238 00203000
*       LPSW(FLCINPSW);             /* GIVE CONTROL TO IOI           */ 00204000
*                                                                  0239 00205000
         LPSW  FLCINPSW                                            0239 00206000
*   /*****************************************************************/ 00207000
*   /*                                                               */ 00208000
*   /* AT THIS POINT IN TIME AMDSAIOI WILL GET CONTROL.              */ 00209000
*   /* IT WILL PROCESS THE INTERRUPT, HANDLE ERRORS (IF              */ 00210000
*   /* ANY), EXAMINE AND MARK BUFFERS COMPLETE THEN RESTORE CONTROL  */ 00211000
*   /* VIA LPSW                                                      */ 00212000
*   /* AMDSAIOI SETS IODBINT FOR THE DEVICE IF AN INTERRUPT HAS      */ 00213000
*   /* OCCURRED FOR THE DEVICE WE ARE WAITING ON.                    */ 00214000
*   /*                                                               */ 00215000
*   /*****************************************************************/ 00216000
*/* D (YES,PROINT,NO,) IODBINT = 1 ?                                 */ 00217000
*/* D (YES,,NO,NOIOINT) INTERRUPT OCCUR FOR OUTPUT */              0240 00218000
*/* P SAVE ORIGINAL REQUEST */                                     0240 00219000
*/* S WATDQOUT: FREE BUFFERS AND CHANNEL PROGRAMS */               0240 00220000
*/* P INDICATE INTERRUPT PROCESSED */                              0240 00221000
*/* P RESTORE ORIGINAL REQUEST */                                  0240 00222000
*                                                                  0240 00223000
*                                                                  0240 00224000
*DISABRE2:                                                         0240 00225000
*       IF IODBINT = ON THEN                                       0240 00226000
DISABRE2 TM    IODBINT(IODBREG),B'00100000'                        0240 00227000
         BO    @RT00240                                            0240 00228000
*         GO TO PROINT;             /* IF INTERRUPT OCCURRED FOR THIS   00229000
*                                      DEVICE WE CAN NOW GO OFF TO 0241 00230000
*                                      PROCESS THE INTERRUPT         */ 00231000
*       IF (IODBREG ^= ADDR(IODBOUT)) &                            0242 00232000
*        (EVAL(ADDR(IODBOUT)) -> IODBINT = ON) THEN /* DID AN INTERRUPT 00233000
*                                      OCCUR FOR OUTPUT              */ 00234000
         L     @10,@CA00958                                        0242 00235000
         CR    IODBREG,@10                                         0242 00236000
         BE    @RF00242                                            0242 00237000
         TM    IODBINT(@10),B'00100000'                            0242 00238000
         BNO   @RF00242                                            0242 00239000
*         DO;                       /* YES - FREE BUFFERS AND CHANNEL   00240000
*                                      PROGRAMS                      */ 00241000
*           TEMPIODB = IODBREG;     /* SAVE REQUESTED IODB           */ 00242000
         LR    TEMPIODB,IODBREG                                    0244 00243000
*           IODBREG = ADDR(IODBOUT); /* POINT TO OUTPUT IODB         */ 00244000
         LR    IODBREG,@10                                         0245 00245000
*           CALL WATDQOUT;          /* FREE BUFFERS AND CHANNEL PGM  */ 00246000
         BAL   @14,WATDQOUT                                        0246 00247000
*           IODBINT = OFF;          /* RESET INTERRUPT-PROCESSED FLAG*/ 00248000
         NI    IODBINT(IODBREG),B'11011111'                        0247 00249000
*           IODBREG = TEMPIODB;     /* RESTORE ORIGINAL IODB         */ 00250000
         LR    IODBREG,TEMPIODB                                    0248 00251000
*           PFWATOIP = PFWATOIP + 1;    /* INCREMENT WAIT COUNTER    */ 00252000
*                                                                  0249 00253000
         LA    @10,1                                               0249 00254000
         AH    @10,PFWATOIP                                        0249 00255000
         STH   @10,PFWATOIP                                        0249 00256000
*         END;                                                     0250 00257000
*                                                                  0250 00258000
*   /*****************************************************************/ 00259000
*   /*                                                               */ 00260000
*   /* THE EXPECTED INTERRUPT DID NOT OCCUR.  WE WILL ENTER AN       */ 00261000
*   /* ENABLED WAIT STATE FOR THE ORIGINAL REQUEST.                  */ 00262000
*   /*                                                               */ 00263000
*   /*****************************************************************/ 00264000
*/*NOIOINT: P (,ENADIS) SET UP FOR ENABLED WAIT STATE                */ 00265000
*                                                                  0251 00266000
*                                                                  0251 00267000
*NOIOINT:                                                          0251 00268000
*       PSWAIT = ON;                /* ENABLE WAIT STATE             */ 00269000
@RF00242 DS    0H                                                  0251 00270000
NOIOINT  LA    @10,EWAITPSW                                        0251 00271000
         OI    PSWAIT(@10),B'00000010'                             0251 00272000
*       PFWATENW = PFWATENW + 1;    /* INCREMENT ENABLED WAIT COUNTER*/ 00273000
*                                                                  0252 00274000
         LA    @10,1                                               0252 00275000
         AH    @10,PFWATENW                                        0252 00276000
         STH   @10,PFWATENW                                        0252 00277000
*       GO TO ENADIS;               /* ALLOW INTERRUPTS              */ 00278000
         B     ENADIS                                              0253 00279000
*     END;                          /* THIS ENDS PROCESSING FOR NO 0254 00280000
*                                      EXISTING INTERRUPTS           */ 00281000
*                                                                  0255 00282000
*   /*****************************************************************/ 00283000
*   /*                                                               */ 00284000
*   /* WE CAN NOW PROCESS INTERRUPTS FOR A DEVICE.                   */ 00285000
*   /*                                                               */ 00286000
*   /*****************************************************************/ 00287000
*/*PROINT: D (NO,,YES,ORGINT) PROCESSING OUTPUT                      */ 00288000
*/* D (YES,,NO,ORGINT) INTERRUPT OCCUR FOR OUTPUT                    */ 00289000
*/* P SAVE ORIGINAL REQUEST                                          */ 00290000
*/* S WATDQOUT: FREE BUFFERS AND CHANNEL PROGRAMS                    */ 00291000
*/* P INDICATE INTERRUPT PROCESSED                                   */ 00292000
*/* P RESTORE ORIGINAL REQUEST                                       */ 00293000
*/*ORGINT: P IODBINT = 0                                             */ 00294000
*/* D (YES,,NO,IOCCK) OUTPUT IODB ?                                  */ 00295000
*/* S WATDQOUT: FREE BUFFERS AND CHANNEL PROGRAMS                    */ 00296000
*/* D (NO,,YES,ENADIS) WAIT FOR I/O COMPLETION                       */ 00297000
*                                                                  0255 00298000
*                                                                  0255 00299000
*PROINT:                                                           0255 00300000
*   IF (IODBREG ^= ADDR(IODBOUT)) &                                0255 00301000
*    (EVAL(ADDR(IODBOUT)) -> IODBINT = ON) THEN /* DID AN INTERRUPT     00302000
*                                      OCCUR FOR OUTPUT              */ 00303000
@RF00217 DS    0H                                                  0255 00304000
PROINT   L     @10,@CA00958                                        0255 00305000
         CR    IODBREG,@10                                         0255 00306000
         BE    @RF00255                                            0255 00307000
         TM    IODBINT(@10),B'00100000'                            0255 00308000
         BNO   @RF00255                                            0255 00309000
*     DO;                           /* YES - FREE BUFFERS AND CHANNEL   00310000
*                                      PROGRAMS                      */ 00311000
*       TEMPIODB = IODBREG;         /* SAVE REQUESTED IODB           */ 00312000
         LR    TEMPIODB,IODBREG                                    0257 00313000
*       IODBREG = ADDR(IODBOUT);    /* POINT TO OUTPUT IODB          */ 00314000
         LR    IODBREG,@10                                         0258 00315000
*       CALL WATDQOUT;              /* FREE BUFFERS AND CHANNEL PGM  */ 00316000
         BAL   @14,WATDQOUT                                        0259 00317000
*       IODBINT = OFF;              /* RESET INTERRUPT-PROCESSED FLAG*/ 00318000
         NI    IODBINT(IODBREG),B'11011111'                        0260 00319000
*       IODBREG = TEMPIODB;         /* RESTORE ORIGINAL IODB         */ 00320000
         LR    IODBREG,TEMPIODB                                    0261 00321000
*       PFWATOIP = PFWATOIP + 1;    /* INCREMENT WAIT COUNTER        */ 00322000
         LA    @10,1                                               0262 00323000
         AH    @10,PFWATOIP                                        0262 00324000
         STH   @10,PFWATOIP                                        0262 00325000
*     END;                                                         0263 00326000
*   IODBINT = OFF;                  /* RESET INTERRUPT FLAG          */ 00327000
@RF00255 NI    IODBINT(IODBREG),B'11011111'                        0264 00328000
*   IF IODBREG = ADDR(IODBOUT) THEN /* PROCESSING OUTPUT             */ 00329000
         L     @10,@CA00958                                        0265 00330000
         CR    IODBREG,@10                                         0265 00331000
         BNE   @RF00265                                            0265 00332000
*     DO;                           /* YES - FREE BUFFERS AND CHANNEL   00333000
*                                      PROGRAMS                      */ 00334000
*       RESPECIFY RETCODE RESTRICTED;                              0267 00335000
*       CALL WATDQOUT;              /* FREE BUFFERS AND CHANNEL PGM  */ 00336000
         BAL   @14,WATDQOUT                                        0268 00337000
*       IF RETCODE = 8 THEN         /* SHOULD WE WAIT FOR OUTPUT TO     00338000
*                                      COMPLETE                      */ 00339000
         CH    RETCODE,@CH00036                                    0269 00340000
         BE    @RT00269                                            0269 00341000
*         GO TO ENADIS;             /* YES - ENABLE FOR INTERRUPTS   */ 00342000
*       RESPECIFY RETCODE UNRESTRICTED;                            0271 00343000
*     END;                                                         0272 00344000
*                                                                  0273 00345000
*   /*****************************************************************/ 00346000
*   /*                                                               */ 00347000
*   /* NOW SET UP RETURN CODES BASED UPON I/O COMPLETION             */ 00348000
*   /*                                                               */ 00349000
*   /*****************************************************************/ 00350000
*/*IOCCK: D (YES,EREXIT,NO,RCSET0) IODBIOC = 1 ?                     */ 00351000
*/*PURGEQ: D (YES,EREXIT,NO,) CTIOTERM = 1 ?                         */ 00352000
*/* N PURGE IODBBCT QUEUE                                            */ 00353000
*/* P TURN BCTCOMP ON                                                */ 00354000
*/* P TURN BCTIOERR ON                                               */ 00355000
*/*EREXIT: P (,DEQRCB) RETURN CODE = 4                               */ 00356000
*/*RCSET0: P (,DEQRCB) RETURN CODE = 0                               */ 00357000
*                                                                  0273 00358000
*   IF IODBIOC = OFF THEN                                          0273 00359000
@RF00265 TM    IODBIOC(IODBREG),B'00010000'                        0273 00360000
         BZ    @RT00273                                            0273 00361000
*     GO TO RCSET0;                 /* IF NO I/O ERROR SET UP NORMAL    00362000
*                                      RETURN CODE                   */ 00363000
*   ELSE                                                           0275 00364000
*     GO TO RCSET4;                 /* FOR I/O ERRORS SET ERROR    0275 00365000
*                                      RETURN CODE                   */ 00366000
*                                                                  0275 00367000
         B     RCSET4                                              0275 00368000
*PURGEQ:                                                           0276 00369000
*   IF CTIOTERM = OFF& IODBBCT ^= ZERO THEN                        0276 00370000
PURGEQ   TM    CTIOTERM(CCTREG),B'10000000'                        0276 00371000
         BNZ   @RF00276                                            0276 00372000
         L     @10,IODBBCT(,IODBREG)                               0276 00373000
         LTR   @10,@10                                             0276 00374000
         BZ    @RF00276                                            0276 00375000
*     DO;                           /* IF TERMINATION,SKIP BUFFER  0277 00376000
*                                      DEQUEUEING                    */ 00377000
*       RESPECIFY BCT BASED(IODBBCT);                              0278 00378000
*       BCTCOMP = ON;               /* INDICATE I/O COMPLETE FOR   0279 00379000
*                                      BUFFER                        */ 00380000
*       BCTIOERR = ON;              /* I/O ERROR OCCURRED            */ 00381000
         OI    BCTCOMP(@10),B'10100000'                            0280 00382000
*     END;                                                         0281 00383000
*                                                                  0281 00384000
*EREXIT:                                                           0282 00385000
*                                                                  0282 00386000
*RCSET4:                                                           0282 00387000
*   SAVEBACK -> SAVER15 = FOUR;     /* SET UP ERROR RETURN CODE      */ 00388000
@RF00276 DS    0H                                                  0282 00389000
EREXIT   DS    0H                                                  0282 00390000
RCSET4   L     @10,SAVEBACK(,R13)                                  0282 00391000
         MVC   SAVER15(4,@10),@CF00044                             0282 00392000
*   GO TO DEQRCB;                                                  0283 00393000
*                                                                  0283 00394000
         B     DEQRCB                                              0283 00395000
*RCSET0:                                                           0284 00396000
*   SAVEBACK ->SAVER15 = ZERO;      /* SET UP NORMAL RETURN CODE     */ 00397000
*                                                                  0284 00398000
RCSET0   L     @10,SAVEBACK(,R13)                                  0284 00399000
         SR    @15,@15                                             0284 00400000
         ST    @15,SAVER15(,@10)                                   0284 00401000
*   /*****************************************************************/ 00402000
*   /*                                                               */ 00403000
*   /* THIS SECTION SETS WAIT UP TO RETURN TO THE CALLER             */ 00404000
*   /*                                                               */ 00405000
*   /*****************************************************************/ 00406000
*/*DEQRCB: P DEQUEUE RCB FROM VCTRCB                                 */ 00407000
