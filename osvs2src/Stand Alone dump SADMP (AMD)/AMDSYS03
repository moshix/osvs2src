*   IF IOASCB=ASTERR THEN                                          0182 00281000
@RC00169 L     @12,DATAPTR(,PREG)                                  0182 00282000
         CLC   IOASCB(4,@12),@CC00339                              0182 00283000
         BNE   @RF00182                                            0182 00284000
*     DO;                           /* **** IN ASCB ?                */ 00285000
*       IF IOTCB=ASTERR THEN        /* **** IN TCB TOO ?             */ 00286000
