         LA    @06,IOSENSE(,@06)                                   0178 00273000
         ST    @06,DATAPT(,@12)                                    0178 00274000
*       REG0=ADDR(IOSNSOK);         /* FMTPTRN ADDR                  */ 00275000
         LA    REG0,IOSNSOK                                        0179 00276000
