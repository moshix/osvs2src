         ALR   @10,@15                                             0100 00262000
         C     @10,@CF00406                                        0100 00263000
         BNH   @RF00100                                            0100 00264000
*     DO;                                                          0101 00265000
*       CNVRTLNG=MAXLNG;            /* LOAD LENGTH OF AREA TO BE   0102 00266000
*                                      CONVERTED                     */ 00267000
         MVI   CNVRTLNG(WAPTR),X'20'                               0102 00268000
*       CALL HEXCNVRT;              /* CONVERT INPUT AREA TO EBCDIC     00269000
*                                      AND MOVE INTO OUTPUT BUFFER   */ 00270000
         BAL   @14,HEXCNVRT                                        0103 00271000
*       RETCODE=PRTRET;             /* SET RETURN CODE TO PRINT    0104 00272000
*                                      OUTPUT BUFFER AND RETURN      */ 00273000
