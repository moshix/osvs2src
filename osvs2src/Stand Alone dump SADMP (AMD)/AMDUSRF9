@RC00100 L     RET,RSAVEA(,WAPTR)                                  0113 00308000
*HEXCNVRT:                                                         0114 00309000
*                                                                  0114 00310000
*/* ******************************************************************/ 00311000
*/*                                                                  */ 00312000
*/*  FUNCTION= THIS ROUTINE CONVERTS A SPECIFIED AREA OF MAIN        */ 00313000
*/*            STORAGE FROM HEXIDECIMAL TO THE PRINTABLE EBCDIC      */ 00314000
*/*            REPRESENTATION FOR THE HEXIDECIMAL DIGITS.            */ 00315000
*/*                                                                  */ 00316000
*/*  INPUTS= LENGTH OF AREA TO BE CONVERTED(CNVRTLNG)                */ 00317000
*/*          ADDRESS OF AREA TO BE CONVERTED(CNVRTPTR)               */ 00318000
*/*                                                                  */ 00319000
*/*  OUTPUTS= EBCDIC REPRESENTION OF AREA CONVERTED IN OUTPUT BUFFER */ 00320000
*/*                                                                  */ 00321000
*/* ******************************************************************/ 00322000
*                                                                  0114 00323000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0114 00324000
         B     @PB00004                                            0114 00325000
HEXCNVRT DS    0H                                                  0115 00326000
*   RFY                                                            0115 00327000
*    (CNVREG,                                                      0115 00328000
*     WKREG6) RESTRICTED;           /* RESTRICT WORK REGS    @ZA16026*/ 00329000
*   RSAVEB=RET;                     /* SAVE RETURN ADDRESS           */ 00330000
         ST    RET,RSAVEB(,WAPTR)                                  0116 00331000
*   DO I=1 TO MAXLNG BY 16 WHILE(CNVRTLNG>0);                      0117 00332000
         LA    @10,1                                               0117 00333000
         STC   @10,I(,WAPTR)                                       0117 00334000
@DL00117 CLI   CNVRTLNG(WAPTR),0                                   0117 00335000
         BNH   @DC00117                                            0117 00336000
*     DO J=1 TO 16 BY 4 WHILE(CNVRTLNG>0);                         0118 00337000
         LA    @10,1                                               0118 00338000
         STC   @10,J(,WAPTR)                                       0118 00339000
@DL00118 CLI   CNVRTLNG(WAPTR),0                                   0118 00340000
         BNH   @DC00118                                            0118 00341000
*       DO K=1 TO 4 WHILE(CNVRTLNG>0);                             0119 00342000
         LA    @10,1                                               0119 00343000
         STC   @10,K(,WAPTR)                                       0119 00344000
@DL00119 CLI   CNVRTLNG(WAPTR),0                                   0119 00345000
         BNH   @DC00119                                            0119 00346000
*         WKREG6=I+J+K-2;           /* DETERMINE INDEX       @ZA16026*/ 00347000
         SLR   WKREG6,WKREG6                                       0120 00348000
         IC    WKREG6,I(,WAPTR)                                    0120 00349000
         SLR   @10,@10                                             0120 00350000
         IC    @10,J(,WAPTR)                                       0120 00351000
         ALR   WKREG6,@10                                          0120 00352000
         SLR   @10,@10                                             0120 00353000
         IC    @10,K(,WAPTR)                                       0120 00354000
         ALR   WKREG6,@10                                          0120 00355000
         BCTR  WKREG6,0                                            0120 00356000
         BCTR  WKREG6,0                                            0120 00357000
*         CNVAREA=CNVMASK(WKREG6);  /* MOVE BYTE INTO CNVRT AREA   0121 00358000
*                                                            @ZA16026*/ 00359000
         LR    @10,CNVRTPTR                                        0121 00360000
         ALR   @10,WKREG6                                          0121 00361000
         BCTR  @10,0                                               0121 00362000
         MVC   CNVAREA(1,WAPTR),CNVMASK(@10)                       0121 00363000
*         CNVAREA=CNVAREA&'F0'X;    /* ZERO SECOND HALF OF BYTE      */ 00364000
         NI    CNVAREA(WAPTR),X'F0'                                0122 00365000
*         CNVREG=CNVAREA;           /* MOVE BYTE INTO REGISTER       */ 00366000
         SLR   CNVREG,CNVREG                                       0123 00367000
         IC    CNVREG,CNVAREA(,WAPTR)                              0123 00368000
*         SRL(CNVREG,4);            /* SHIFT TO LOW ORDER BIT POS  0124 00369000
*                                                            @ZA16026*/ 00370000
         SRL   CNVREG,4                                            0124 00371000
*         CNVAREA=CNVREG;           /* STORE BYTE BACK INTO AREA   0125 00372000
*                                                            @ZA16026*/ 00373000
         STC   CNVREG,CNVAREA(,WAPTR)                              0125 00374000
*         TR(CNVAREA,CNVTABLE);     /* CONVERT TO EBCDIC             */ 00375000
         TR    CNVAREA(1,WAPTR),@CC00547                           0126 00376000
*         OUTBFR(OUTPTR)=CNVAREA;   /* MOVE CHAR INTO OUTPUT BFR     */ 00377000
         SLR   @10,@10                                             0127 00378000
         IC    @10,OUTPTR(,WAPTR)                                  0127 00379000
         L     @07,OUTBFPTR(,PARMPTR)                              0127 00380000
         ALR   @10,@07                                             0127 00381000
         BCTR  @10,0                                               0127 00382000
         MVC   OUTBFR(1,@10),CNVAREA(WAPTR)                        0127 00383000
*         OUTPTR=OUTPTR+1;          /* INCR TO NEXT POSITION         */ 00384000
         LA    @10,1                                               0128 00385000
         SLR   @05,@05                                             0128 00386000
         IC    @05,OUTPTR(,WAPTR)                                  0128 00387000
         ALR   @05,@10                                             0128 00388000
         STC   @05,OUTPTR(,WAPTR)                                  0128 00389000
*         CNVAREA=CNVMASK(WKREG6);  /* MOVE BYTE BACK INTO CNVRT AREA   00390000
*                                                            @ZA16026*/ 00391000
         LR    @01,CNVRTPTR                                        0129 00392000
         ALR   @01,WKREG6                                          0129 00393000
         BCTR  @01,0                                               0129 00394000
         MVC   CNVAREA(1,WAPTR),CNVMASK(@01)                       0129 00395000
