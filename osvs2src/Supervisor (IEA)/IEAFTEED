         TITLE 'IEAFTEED-EED CONTROL BLOCK FORMAT MODULE               *00001000
                        '                                               00002000
IEAFTEED CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAFTEED  77.006'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @11,0                                               0001 00011000
@PSTART  LA    @12,4095(,@11)                                      0001 00012000
         USING @PSTART,@11                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
         L     @00,@SIZDATD                                        0001 00015000
         GETMAIN  R,LV=(0)                                              00016000
         LR    @10,@01                                             0001 00017000
         USING @DATD,@10                                           0001 00018000
         ST    @13,@SA00001+4                                      0001 00019000
         LM    @00,@01,20(@13)                                     0001 00020000
         ST    @10,8(,@13)                                         0001 00021000
         LR    @13,@10                                             0001 00022000
*                                                                  0105 00023000
*/********************************************************************/ 00024000
*/* FUNCTION-                                                      0105 00025000
*       ROUTINE WHICH CHECKS WHICH TYPE OF EED IS TO BE FORMATTED.  IT  00026000
*     SETS UP THE NEEDED POINTERS TO BE PASSED TO THE FORMAT ROUTINE    00027000
*     SENT IN THE PARAMETER LIST.  FOR ANY ERROR ACCESSING A NEEDED     00028000
*     FIELD IN THE EED OR FOR NON-ZERO RETURN CODE FROM THE FORMATTING  00029000
*     SERVICE, THE FOLLOWING MESSAGES WILL BE PRINTED:             0105 00030000
*                       *** FORMATTING SERVICE ROUTINE ERROR DURING EED 00031000
*                           FORMATTING (POSSIBLE CAUSE: DATA NOT IN     00032000
*                           DUMP ) ***.                            0105 00033000
*     AT THIS POINT EED PROCESSING WOULD TERMINATE. IF THE EED IS  0105 00034000
*     FORMATTED WITHOUT ACCESS ERROR, A BIT SUMMARY IS PRINTED USING    00035000
*     THE ANALYZE PROC.                                            0105 00036000
*                                                                  0105 00037000
*   INPUT-                                                         0105 00038000
*       REGISTER 1 POINTS TO THE PARMLIST WHICH CONTAINS THE ADDRESS    00039000
*     OF THE CONTROL BLOCK TO BE FORMATTED IN ADPLFMT2. ADPLFMT1 ALSO   00040000
*     DEFINED AS START, CONTAINS THE OFFSET TO BE PLACED ALONG SIDE     00041000
*     THE FIRST FIELD IN THE CONTROL BLOCK AND ADPLCOM1 CONTAINS THE    00042000
*     ADDRESS OF THE ESTAE PARAMETER LIST.                         0105 00043000
*                                                                  0105 00044000
*   CALLS-                                                         0105 00045000
*       CALLS ACCESRTN,CONVERT,EEDBITS,FMTRTN,AND PRINT            0105 00046000
*                                                                  0105 00047000
*   CALLED BY -                                                    0105 00048000
*       IEAVTFMT TO FORMAT EEDS                                    0105 00049000
*                                                                  0105 00050000
*   RETURNS TO -                                                   0105 00051000
*       IEAVTFMT WITH A RETURN CODE SET BY ACCESS AND FORMAT       0105 00052000
*       SERVICE ROUTINES                                             */ 00053000
*/********************************************************************/ 00054000
*                                                                  0105 00055000
*   ERRSW=OFF;                      /* ERROR INDICATOR               */ 00056000
         NI    ERRSW,B'01111111'                                   0105 00057000
*   ESTAPTR=ADPLCOM1;               /* PTR TO ESTAE PARM LIST        */ 00058000
         L     ESTAPTR,ADPLCOM1(,ABDPLPTR)                         0106 00059000
*   STM(VADR,RCREG,LCALREGS);       /* SAVE REGS FOR RETRY           */ 00060000
         STM   VADR,RCREG,LCALREGS(ESTAPTR)                        0107 00061000
*   ADPLCOM1=ADDR(EEDFMT);          /* ADDRESS TO BE USED BY FORMAT     00062000
*                                      ROUTINE FOR ERROR MESSAGE     */ 00063000
         LA    @14,EEDFMT                                          0108 00064000
         ST    @14,ADPLCOM1(,ABDPLPTR)                             0108 00065000
*   EEDPTR=ADPLFMT2;                /* PTR PASSED IN PARMLIST        */ 00066000
         L     @14,ADPLFMT2(,ABDPLPTR)                             0109 00067000
         LR    EEDPTR,@14                                          0109 00068000
*   SAVEPTR=ADPLFMT2-START;         /* ADDRESS OF BEGINNING OF BLOCK    00069000
*                                      (IF PASSED CONTROL BLOCK IS 0110 00070000
*                                      EMBEDDED IN ANOTHER)          */ 00071000
         SL    @14,START(,ABDPLPTR)                                0110 00072000
         ST    @14,SAVEPTR                                         0110 00073000
*   VADR=ADDR(EEDID);               /* EED ID FIELD                  */ 00074000
         LA    VADR,EEDID(,EEDPTR)                                 0111 00075000
*   CALL ACCESRTN;                  /* ACCESS ROUTINE                */ 00076000
         BAL   @14,ACCESRTN                                        0112 00077000
*   IF RCREG^=ZERO THEN             /* COULD NOT ACCESS ID           */ 00078000
         LTR   RCREG,RCREG                                         0113 00079000
         BZ    @RF00113                                            0113 00080000
*     BUFFER(1:LENGTH(EEDFMT))=EEDFMT;                             0114 00081000
         L     @14,ADPLBUF(,ABDPLPTR)                              0114 00082000
         MVC   BUFFER(120,@14),EEDFMT                              0114 00083000
*   ELSE                            /* FOUND EEDID                   */ 00084000
*     DO;                                                          0115 00085000
         B     @RC00113                                            0115 00086000
@RF00113 DS    0H                                                  0116 00087000
*       CALL PRINT;                 /* 2 BLANK LINES                 */ 00088000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0116 00089000
         BALR  @14,@15                                             0116 00090000
*       CALL PRINT;                 /* BLANK                         */ 00091000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0117 00092000
         BALR  @14,@15                                             0117 00093000
*       IDSAVE=DATABYTE;            /* SAVE EEDID                    */ 00094000
         MVC   IDSAVE(1),DATABYTE(DATAPTR)                         0118 00095000
*       PACKAREA=EEDPTR;            /* PLACE PTR IN CHAR AREA        */ 00096000
         ST    EEDPTR,PACKAREA                                     0119 00097000
*       GARBAGE='04'X;              /* GARBAGE BYTE FOR THE UNPK     */ 00098000
         MVI   GARBAGE,X'04'                                       0120 00099000
*       UNPK(ADDRESS,UNPACK);       /* 31 BIT ADDRESSING             */ 00100000
         L     @14,ADPLBUF(,ABDPLPTR)                              0121 00101000
         UNPK  ADDRESS(9,@14),UNPACK(5)                            0121 00102000
*       TR(ADDRESS,TRANS);          /* TRANSLATE TO PRINTABLE HEX    */ 00103000
*                                                                  0122 00104000
         TR    ADDRESS(9,@14),TRANS                                0122 00105000
*       /*************************************************************/ 00106000
*       /*                                                           */ 00107000
*       /* TYPE 1 EED - REGS AND PSW                                 */ 00108000
*       /*                                                           */ 00109000
*       /*************************************************************/ 00110000
*                                                                  0123 00111000
*       IF IDSAVE=REGSTYPE THEN     /* REGS AND PSW EED              */ 00112000
         CLC   IDSAVE(1),REGSTYPE                                  0123 00113000
         BNE   @RF00123                                            0123 00114000
*         DO;                                                      0124 00115000
*           BUFFER(1:LENGTH(EED1HDR))=EED1HDR;                     0125 00116000
         MVC   BUFFER(54,@14),EED1HDR                              0125 00117000
*           CALL PRINT;             /* PRINT TITLE                   */ 00118000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0126 00119000
         BALR  @14,@15                                             0126 00120000
*           BUFFER(1:LENGTH(EEDUS))=EEDUS;/* UNDER SCORE             */ 00121000
         L     @14,ADPLBUF(,ABDPLPTR)                              0127 00122000
         MVC   BUFFER(4,@14),EEDUS                                 0127 00123000
*           CALL PRINT;             /* PRINT IT                      */ 00124000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0128 00125000
         BALR  @14,@15                                             0128 00126000
*           CALL PRINT;             /* 2 BLANK LINES                 */ 00127000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0129 00128000
         BALR  @14,@15                                             0129 00129000
*           CALL PRINT;             /* BLANK                         */ 00130000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0130 00131000
         BALR  @14,@15                                             0130 00132000
*           DO ARRAYCNT=1 TO DIM(EED1NAME) WHILE(ERRSW=OFF);/* FORMAT   00133000
*                                      LOOP                          */ 00134000
         LA    ARRAYCNT,1                                          0131 00135000
@DL00131 TM    ERRSW,B'10000000'                                   0131 00136000
         BNZ   @DC00131                                            0131 00137000
*             LINEPTR=EED1NAME(ARRAYCNT);/* PTR TO FORMAT PATTERN    */ 00138000
         LR    @14,ARRAYCNT                                        0132 00139000
         SLA   @14,2                                               0132 00140000
         L     LINEPTR,EED1NAME-4(@14)                             0132 00141000
*             CALL CONVERT;         /* GENERATE OFFSET               */ 00142000
         BAL   @14,CONVERT                                         0133 00143000
*             CALL FMTRTN;          /* FORMAT ROUTINE                */ 00144000
         BAL   @14,FMTRTN                                          0134 00145000
*           END;                    /* END LOOP                      */ 00146000
         AH    ARRAYCNT,@CH00041                                   0135 00147000
         CH    ARRAYCNT,@CH00112                                   0135 00148000
         BNH   @DL00131                                            0135 00149000
@DC00131 DS    0H                                                  0136 00150000
*         END;                      /* END REGS AND PSW              */ 00151000
*                                                                  0137 00152000
*       /*************************************************************/ 00153000
*       /*                                                           */ 00154000
*       /* TYPE 2 EED - DUMP OPTIONS                                 */ 00155000
*       /*                                                           */ 00156000
*       /*************************************************************/ 00157000
*                                                                  0137 00158000
*       ELSE                        /* NOT REGS TYPE                 */ 00159000
*         IF IDSAVE=DUMPTYPE THEN   /* DUMP OPTIONS TYPE             */ 00160000
         B     @RC00123                                            0137 00161000
@RF00123 CLC   IDSAVE(1),DUMPTYPE                                  0137 00162000
         BNE   @RF00137                                            0137 00163000
*           DO;                                                    0138 00164000
*             BUFFER(1:LENGTH(EED2HDR))=EED2HDR;/* TITLE LINE        */ 00165000
         L     @14,ADPLBUF(,ABDPLPTR)                              0139 00166000
         MVC   BUFFER(54,@14),EED2HDR                              0139 00167000
*             CALL PRINT;           /* PRINT IT                      */ 00168000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0140 00169000
         BALR  @14,@15                                             0140 00170000
*             BUFFER(1:LENGTH(EEDUS))=EEDUS;/* UNDER SCORE           */ 00171000
         L     @14,ADPLBUF(,ABDPLPTR)                              0141 00172000
         MVC   BUFFER(4,@14),EEDUS                                 0141 00173000
*             CALL PRINT;           /* PRINT IT                      */ 00174000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0142 00175000
         BALR  @14,@15                                             0142 00176000
*             CALL PRINT;           /* 2 BLANK LINES                 */ 00177000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0143 00178000
         BALR  @14,@15                                             0143 00179000
*             CALL PRINT;           /* BLANK                         */ 00180000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0144 00181000
         BALR  @14,@15                                             0144 00182000
*             DO ARRAYCNT=1 TO EED2MAX WHILE(ERRSW=OFF);/* FORMAT  0145 00183000
*                                      LOOP                          */ 00184000
         LA    ARRAYCNT,1                                          0145 00185000
@DL00145 TM    ERRSW,B'10000000'                                   0145 00186000
         BNZ   @DC00145                                            0145 00187000
*               LINEPTR=EED2NAME(ARRAYCNT);/* PTR TO FORMAT PATTERN  */ 00188000
         LR    @14,ARRAYCNT                                        0146 00189000
         SLA   @14,2                                               0146 00190000
         L     LINEPTR,EED2NAME-4(@14)                             0146 00191000
*               CALL CONVERT;       /* GENERATE OFFSET               */ 00192000
         BAL   @14,CONVERT                                         0147 00193000
*               CALL FMTRTN;        /* FORMAT THE LINE               */ 00194000
         BAL   @14,FMTRTN                                          0148 00195000
*             END;                  /* END FORMAT LOOP               */ 00196000
         AH    ARRAYCNT,@CH00041                                   0149 00197000
         CH    ARRAYCNT,@CH00065                                   0149 00198000
         BNH   @DL00145                                            0149 00199000
@DC00145 DS    0H                                                  0150 00200000
*             IF ERRSW=OFF THEN     /* NO PROBLEMS FORMATTING BASIC     00201000
*                                      SECT                          */ 00202000
         TM    ERRSW,B'10000000'                                   0150 00203000
         BNZ   @RF00150                                            0150 00204000
*               DO;                                                0151 00205000
*                 VADR=ADDR(EEDERFL);/* ERROR ID FIELD ADDED FOR SU17*/ 00206000
         LA    VADR,EEDERFL(,EEDPTR)                               0152 00207000
*                 CALL ACCESRTN;    /* ACCESS IT                     */ 00208000
         BAL   @14,ACCESRTN                                        0153 00209000
*                 IF RCREG^=ZERO THEN/* ACCESS ERROR                 */ 00210000
         LTR   RCREG,RCREG                                         0154 00211000
         BZ    @RF00154                                            0154 00212000
*                   BUFFER(1:LENGTH(EEDFMT))=EEDFMT;               0155 00213000
         L     @14,ADPLBUF(,ABDPLPTR)                              0155 00214000
         MVC   BUFFER(120,@14),EEDFMT                              0155 00215000
*                 ELSE              /* FOUND FLAG FIELD              */ 00216000
*                   DO;                                            0156 00217000
         B     @RC00154                                            0156 00218000
@RF00154 DS    0H                                                  0157 00219000
*                     IF ERRFL=ON THEN/* EXTRA SECTION OF EED TO BE     00220000
*                                      FORMATTED                     */ 00221000
         TM    ERRFL(DATAPTR),B'10000000'                          0157 00222000
         BNO   @RF00157                                            0157 00223000
*                       DO ARRAYCNT=ARRAYCNT TO DIM(EED2NAME) WHILE(    00224000
*                             ERRSW=OFF);                          0158 00225000
         B     @DE00158                                            0158 00226000
@DL00158 TM    ERRSW,B'10000000'                                   0158 00227000
         BNZ   @DC00158                                            0158 00228000
*                         LINEPTR=EED2NAME(ARRAYCNT);/* FORMAT     0159 00229000
*                                      PATTERN PTR                   */ 00230000
         LR    @14,ARRAYCNT                                        0159 00231000
         SLA   @14,2                                               0159 00232000
         L     LINEPTR,EED2NAME-4(@14)                             0159 00233000
*                         CALL CONVERT;/* GENRATE OFFSET             */ 00234000
         BAL   @14,CONVERT                                         0160 00235000
*                         CALL FMTRTN;/* FORMAT                      */ 00236000
         BAL   @14,FMTRTN                                          0161 00237000
*                       END;        /* END LOOP                      */ 00238000
         AH    ARRAYCNT,@CH00041                                   0162 00239000
@DE00158 CH    ARRAYCNT,@CH00184                                   0162 00240000
         BNH   @DL00158                                            0162 00241000
@DC00158 DS    0H                                                  0163 00242000
*                     ELSE                                         0163 00243000
*                       ;           /* ERROR ID NOT TO BE FORMATTED  */ 00244000
@RF00157 DS    0H                                                  0164 00245000
*                   END;            /* END FOUND FLAG FIELD          */ 00246000
*               END;                /* END NO ERROR DURING BASIC   0165 00247000
*                                      FORMATTING                    */ 00248000
*             ELSE                                                 0166 00249000
*               ;                   /* ERROR FORMATTING BASIC SECTION*/ 00250000
@RF00150 DS    0H                                                  0167 00251000
*           END;                    /* END DUMP OPTIONS TYPE         */ 00252000
*                                                                  0168 00253000
*       /*************************************************************/ 00254000
*       /*                                                           */ 00255000
*       /* MACHINE CHECK EED - HARDWARE STATUS                       */ 00256000
*       /*                                                           */ 00257000
*       /*************************************************************/ 00258000
*                                                                  0168 00259000
*         ELSE                      /* NOT DUMP OPTIONS TYPE         */ 00260000
*           IF IDSAVE=MACHINE THEN  /* MACHINE CHECK EED             */ 00261000
         B     @RC00137                                            0168 00262000
@RF00137 CLC   IDSAVE(1),MACHINE                                   0168 00263000
         BNE   @RF00168                                            0168 00264000
*             DO;                                                  0169 00265000
*               BUFFER(1:LENGTH(EED3HDR))=EED3HDR;/* TITLE LINE      */ 00266000
         L     @14,ADPLBUF(,ABDPLPTR)                              0170 00267000
