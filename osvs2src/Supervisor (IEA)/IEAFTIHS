*                   CALL PRINT;     /* PRINT IT                      */ 00586000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0260 00587000
         BALR  @14,@15                                             0260 00588000
*                   ERRSW=ON;       /* ERROR INDICATOR               */ 00589000
         OI    ERRSW,B'01000000'                                   0261 00590000
*                 END;              /* END ACCESS ERROR              */ 00591000
*               ELSE                /* FOUND ENTRY LENGTH            */ 00592000
*                 IF DATAWORD=ZERO THEN/* TEST FOR ZERO STACK      0263 00593000
*                                                            @ZA27290*/ 00594000
         B     @RC00257                                            0263 00595000
@RF00257 ICM   @14,15,DATAWORD(DATAPTR)                            0263 00596000
         BNZ   @RF00263                                            0263 00597000
