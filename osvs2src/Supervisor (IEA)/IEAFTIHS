*/****************************************************************** */ 01598000
*/* FUNCTION                                                       0502 01599000
*     ROUTINE TO ANALYZE THE BITS IN THE BYTE PASSED AND PRINT OUT 0502 01600000
*     THE MESSAGE THAT CORRESPONDS TO THE BITS THAT ARE ON. IF A BLANK  01601000
*     LINE IS PASSED IN THE MESSAGE ARRAY (INDICATING A RESERVED BIT)   01602000
*     NO OUTPUT WILL CORRESPOND TO THAT BIT.                       0502 01603000
*                                                                  0502 01604000
*   INPUT-                                                         0502 01605000
*     AN ARRAY (SUMMARY) POINTED TO BY THE PARMLIST FIELD          0502 01606000
*     ADPLCOM2 WHICH CONTAINS THE ADDRESS OF THE BYTE TO BE        0502 01607000
*     ANALYZED, THE ADDRESS OF THE ARRAY OF CORRESPONDING          0502 01608000
*     MESSAGES, THE COLUMN NUMBER THE MESSAGE IS TO BEGIN IN,      0502 01609000
*     AND THE LENGTH OF THE MESSAGE ARRAY PASSED.                  0502 01610000
*                                                                  0502 01611000
*   NOTE- COLNUM + MSGLNGTH MUST BE LESS THAN OR EQUAL TO 120 BYTES     01612000
*         WHICH IS THE OUTPUT BUFFER LENGTH.                       0502 01613000
*                                                                  0502 01614000
*                                                                  0502 01615000
*   CALLS-                                                         0502 01616000
*       CALLS PRINT                                                0502 01617000
*                                                                  0502 01618000
*   CALLED BY -                                                    0502 01619000
*       BYTEMODE,EEDBITS,IEAFTESA,IEAFTSCB,RT2BITS                 0502 01620000
*                                                                  0502 01621000
*   RETURNS CONTROL TO -                                           0502 01622000
*       CALLER WITHOUT SETTING A RETURN CODE                       0502 01623000
*                                                                  0502 01624000
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE)-------  0502 01625000
*                                                                    */ 01626000
*/****************************************************************** */ 01627000
*                                                                  0502 01628000
*   DO INDEX=1 TO BYTELNTH;         /* LOOP TO FIND WHICH BITS ARE ON*/ 01629000
         LA    INDEX,1                                             0502 01630000
@DL00502 DS    0H                                                  0503 01631000
*     LNGTHEQU=((INDEX-1)*(MSGLNGTH)+1);/* OFFSET INTO MSGARY OF   0503 01632000
*                                      CORRESPONDING MESSAGE         */ 01633000
         L     @14,MSGLNGTH                                        0503 01634000
         LR    @03,INDEX                                           0503 01635000
         BCTR  @03,0                                               0503 01636000
         MR    @02,@14                                             0503 01637000
         AH    @03,@CH00041                                        0503 01638000
         LR    LNGTHEQU,@03                                        0503 01639000
*     IF(BITSTR&TESTBITS(INDEX))^=ZEROBYTE&/* FOUND A BIT ON         */ 01640000
*         MSGARY(LNGTHEQU:MSGLNGTH+LNGTHEQU-1)^=BLANKS(1:MSGLNGTH) THEN 01641000
         L     @09,BYTEADDR                                        0504 01642000
         SLR   @04,@04                                             0504 01643000
         IC    @04,BITSTR(,@09)                                    0504 01644000
         SLR   @09,@09                                             0504 01645000
         IC    @09,TESTBITS-1(INDEX)                               0504 01646000
         NR    @04,@09                                             0504 01647000
         SLR   @09,@09                                             0504 01648000
         IC    @09,ZEROBYTE                                        0504 01649000
         CR    @04,@09                                             0504 01650000
         BE    @RF00504                                            0504 01651000
         L     @09,MSGADDR                                         0504 01652000
         LR    @04,@14                                             0504 01653000
         BCTR  @04,0                                               0504 01654000
         LR    @03,@09                                             0504 01655000
         ALR   @03,LNGTHEQU                                        0504 01656000
         BCTR  @03,0                                               0504 01657000
         EX    @04,@SC02114                                        0504 01658000
         BE    @RF00504                                            0504 01659000
*       DO;                         /* CORRESPONDING MESSAGE WAS NOT    01660000
*                                      BLANK(RESERVED)               */ 01661000
*         BUFFER(COLNUM:(MSGLNGTH+COLNUM-1))=MSGARY(LNGTHEQU:LNGTHEQU+  01662000
*             MSGLNGTH-1);          /* PLACE MSG IN BUFFER           */ 01663000
         L     @04,COLNUM                                          0506 01664000
         L     @03,ADPLBUF(,ABDPLPTR)                              0506 01665000
         ALR   @03,@04                                             0506 01666000
         BCTR  @03,0                                               0506 01667000
         ALR   @14,@04                                             0506 01668000
         SLR   @14,@04                                             0506 01669000
         BCTR  @14,0                                               0506 01670000
         ALR   @09,LNGTHEQU                                        0506 01671000
         BCTR  @09,0                                               0506 01672000
         EX    @14,@SM02116                                        0506 01673000
*         CALL PRINT;               /* PRINT IT                      */ 01674000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0507 01675000
         BALR  @14,@15                                             0507 01676000
*       END;                        /* END FOUND THE MESSAGE         */ 01677000
*     ELSE                                                         0509 01678000
*       ;                           /* EITHER BIT NOT ON OR RESERVED    01679000
*                                      BIT-NO MESSAGE                */ 01680000
@RF00504 DS    0H                                                  0510 01681000
*   END;                            /* END SEARCH LOOP               */ 01682000
         AH    INDEX,@CH00041                                      0510 01683000
         CH    INDEX,@CH00086                                      0510 01684000
         BNH   @DL00502                                            0510 01685000
*   END ANALYZE;                                                   0511 01686000
@EL00008 DS    0H                                                  0511 01687000
@EF00008 DS    0H                                                  0511 01688000
@ER00008 L     @14,@SA00008                                        0511 01689000
         BR    @14                                                 0511 01690000
*ACCESRTN:                                                         0512 01691000
*   PROC OPTIONS(SAVE(14));                                        0512 01692000
*                                                                  0512 01693000
ACCESRTN ST    @14,@SA00009                                        0512 01694000
*/********************************************************************/ 01695000
*/* FUNCTION-                                                      0513 01696000
*       ROUTINE WHICH CALLS THE ACCESS SERVICE ROUTINE AND MOVES THE    01697000
*     VALUE RETURNED IN R0 INTO DATAPTR.                           0513 01698000
*                                                                  0513 01699000
*                                                                  0513 01700000
*   INPUT -                                                        0513 01701000
*         VADR(REGISTER 0) CONTAINS THE VIRTUAL ADDRESS OF THE FIELD    01702000
*         TO BE ACCESSED.                                          0513 01703000
*                                                                  0513 01704000
*    CALLS-                                                        0513 01705000
*         ACCESS SERVICE ROUTINE PASSED IN PARMLIST                0513 01706000
*                                                                  0513 01707000
*    CALLED BY -                                                   0513 01708000
*         EEDBITS, IEAFTEED, IEAFTFRR, IEAFTRTC, IEAFTRT2,         0513 01709000
*         IEAFTSCB, RT1BITS, AND RT2BITS                           0513 01710000
*                                                                  0513 01711000
*    RETURNS TO -                                                  0513 01712000
*         CALLER WITH THE RETURN CODE SET BY THE ACCESS            0513 01713000
*         SERVICE ROUTINE.                                         0513 01714000
*                                                                  0513 01715000
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE) ------  0513 01716000
*                                                                    */ 01717000
*/********************************************************************/ 01718000
*                                                                  0513 01719000
*   IF ADPLSNPR=OFF THEN            /* SNAP WAS INVOKER              */ 01720000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0513 01721000
         BNZ   @RF00513                                            0513 01722000
*     DO;                                                          0514 01723000
*       RTYADR=ADDR(RETRY);         /* RETRY ADDR FOR ESTAE IN THE 0515 01724000
*                                      CASE OF EXPECTED ERRORS       */ 01725000
         LA    @14,RETRY                                           0515 01726000
         ST    @14,RTYADR(,ESTAPTR)                                0515 01727000
*       DATAPTR=VADR;               /* USED TO BASE THE STORAGE ON   */ 01728000
         LR    DATAPTR,VADR                                        0516 01729000
*       RCREG=ZERO;                 /* SUCCESS UNLESS AN ABEND OCCURS   01730000
*                                      DURING THE MOVE INTO A BUFFER */ 01731000
         SLR   RCREG,RCREG                                         0517 01732000
*       ERREXP=ON;                  /* AN ERROR IS EXPECTED          */ 01733000
         OI    ERREXP(ESTAPTR),B'10000000'                         0518 01734000
*       VERIFY=DATADWRD;            /* MOVE A DOUBLE WORD INTO     0519 01735000
*                                      INTERNAL BUFFER               */ 01736000
*                                                                  0519 01737000
         MVC   VERIFY(8),DATADWRD(DATAPTR)                         0519 01738000
*       /*************************************************************/ 01739000
*       /*                                                           */ 01740000
*       /* THIS MOVE WILL CAUSE AN ABEND IF THE DUMP DOES NOT CONTAIN*/ 01741000
*       /* THE AREA REFERENCED OR IF THE AREA IS PROTECTED. IF AN    */ 01742000
*       /* ABEND OCCURS, THE ESTAE WILL GET CONTROL AND RETRY AT THE */ 01743000
*       /* POINT WHICH SETS A BAD RETURN CODE AND RETURNS TO THE     */ 01744000
*       /* CALLER. THIS THEN SIMULATES THE PRINT DUMP ACCESS ROUTINE.*/ 01745000
*       /* IF THE AREA ASKED FOR IS IN THE DUMP, A DOUBLE WORD IS    */ 01746000
*       /* MOVED INTO AN INTERNAL BUFFER TO PREVENT AN UNEXPECTED    */ 01747000
*       /* ERROR WHEN REFERENCING THE AREA AGAIN                     */ 01748000
*       /*                                                           */ 01749000
*       /*************************************************************/ 01750000
*                                                                  0520 01751000
*       ERREXP=OFF;                 /* AN ERROR IS NO LONGER EXPECTED*/ 01752000
         NI    ERREXP(ESTAPTR),B'01111111'                         0520 01753000
*       DATAPTR=ADDR(VERIFY);       /* REBASE DATA OFF INTERNAL    0521 01754000
*                                      BUFFER                        */ 01755000
         LA    DATAPTR,VERIFY                                      0521 01756000
*     END;                          /* END SNAP WAS THE INVOKER      */ 01757000
*   ELSE                            /* CALLER HAS OWN ACCESS ROUTINE    01758000
*                                      LIKE PRINT DUMP               */ 01759000
*     DO;                                                          0523 01760000
         B     @RC00513                                            0523 01761000
@RF00513 DS    0H                                                  0524 01762000
*       CALL ACCESS;                /* ACCESS SERVICE ROUTINE PASSED    01763000
*                                      BY CALLER                     */ 01764000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0524 01765000
         BALR  @14,@15                                             0524 01766000
*       IF RCREG=ZERO THEN                                         0525 01767000
         LTR   RCREG,RCREG                                         0525 01768000
         BNZ   @RF00525                                            0525 01769000
*         DATAPTR=VADR;             /* SAVE PTR RETURNED BY ACCESS 0526 01770000
*                                      SERVICE ROUTINE               */ 01771000
         LR    DATAPTR,VADR                                        0526 01772000
*       ELSE                        /* BAD RETURN CODE               */ 01773000
*RETRY:                             /* RETRY ADDR FOR THE ESTAE      */ 01774000
*         RCREG=4;                  /* SET A BAD RETURN CODE         */ 01775000
         B     @RC00525                                            0527 01776000
@RF00525 DS    0H                                                  0527 01777000
RETRY    LA    RCREG,4                                             0527 01778000
*     END;                          /* END CALLER WAS NOT SNAP       */ 01779000
@RC00525 DS    0H                                                  0529 01780000
*   RCCODE=RCREG;                   /* SAVE RETURN CODE FROM ACCESS     01781000
*                                      SERVICE RTN                   */ 01782000
@RC00513 ST    RCREG,RCCODE                                        0529 01783000
*   RETURN CODE(RCCODE);                                           0530 01784000
         L     @04,RCCODE                                          0530 01785000
         LR    @15,@04                                             0530 01786000
@EL00009 DS    0H                                                  0530 01787000
@EF00009 DS    0H                                                  0530 01788000
@ER00009 L     @14,@SA00009                                        0530 01789000
         BR    @14                                                 0530 01790000
*   END ACCESRTN;                                                  0531 01791000
*   DCL                                                            0532 01792000
*     IHSPATCH CHAR(200) INIT(                                     0532 01793000
