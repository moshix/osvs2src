*           IF RCREG^=ZERO THEN     /* ERROR ACCESSING FIRST POSSIBLE   00553000
*                                      ADR                           */ 00554000
         LTR   RCREG,RCREG                                         0247 00555000
         BZ    @RF00247                                            0247 00556000
*             DO;                                                  0248 00557000
*               BUFFER(1:LENGTH(FRRSFMT))=FRRSFMT;/* ERROR MSG       */ 00558000
         L     @14,ADPLBUF(,ABDPLPTR)                              0249 00559000
         MVC   BUFFER(120,@14),FRRSFMT                             0249 00560000
*               CALL PRINT;         /* PRINT IT                      */ 00561000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0250 00562000
         BALR  @14,@15                                             0250 00563000
*               ERRSW=ON;           /* ERROR INDICATOR               */ 00564000
         OI    ERRSW,B'01000000'                                   0251 00565000
*             END;                  /* ERROR ACCESSING               */ 00566000
*           ELSE                    /* FOUND FIRST POSSIBLE ADDRESS  */ 00567000
*             DO;                                                  0253 00568000
         B     @RC00247                                            0253 00569000
@RF00247 DS    0H                                                  0254 00570000
*               ENTRYMAX=ENTRYMAX-DATAWORD;/* LENGTH OF SPACE FOR  0254 00571000
*                                      ENTRIES                       */ 00572000
         SL    ENTRYMAX,DATAWORD(,DATAPTR)                         0254 00573000
*               VADR=ADDR(FRRSELEN);/* LENGTH OF EACH ENTRY          */ 00574000
         L     @14,FRRSPTR                                         0255 00575000
         LA    VADR,FRRSELEN(,@14)                                 0255 00576000
*               CALL ACCESRTN;      /* ACCESS IT                     */ 00577000
         BAL   @14,ACCESRTN                                        0256 00578000
*               IF RCREG^=ZERO THEN /* ERROR ACCESSING LENGTH        */ 00579000
         LTR   RCREG,RCREG                                         0257 00580000
