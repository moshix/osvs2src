         TITLE 'IEAFTIHS-IHSA-FRRS CONTROL BLOCK FORMAT MODULE         *00001000
                        '                                               00002000
IEAFTIHS CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'IEAFTIHS  78.033'                                 0001 00009000
IEAFTFRR DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00203-IEAFTFRR)                                   00013000
         ENTRY IEAFTFRR                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00016000
         BALR  @11,0                                               0001 00017000
@PSTART  LA    @12,4095(,@11)                                      0001 00018000
         USING @PSTART,@11                                         0001 00019000
         USING @PSTART+4095,@12                                    0001 00020000
         L     @00,@SIZDATD                                        0001 00021000
         GETMAIN  R,LV=(0)                                              00022000
         LR    @10,@01                                             0001 00023000
         USING @DATD,@10                                           0001 00024000
         ST    @13,@SA00001+4                                      0001 00025000
         LM    @15,@01,16(@13)                                     0001 00026000
         ST    @10,8(,@13)                                         0001 00027000
         LR    @13,@10                                             0001 00028000
         AH    @15,4(,@15)                                         0001 00029000
         BR    @15                                                 0001 00030000
@EP00001 DS    0H                                                  0002 00031000
*                                                                  0120 00032000
*/********************************************************************/ 00033000
*/* FUNCTION -                                                     0120 00034000
*      ROUTINE WHICH FORMATS THE INTERRUPT HANDLER SAVE AREA.      0120 00035000
*      IT SETS UP THE NEEDED POINTERS TO BE PASSED TO THE FORMAT   0120 00036000
*      ROUTINE SENT IN THE PARAMETER LIST.  FOR ANY ERROR ACCESSING     00037000
*      A NEEDED FIELD IN THE IHSA OR RT1W OR FOR NON-ZERO RETURN CODE   00038000
*      FROM THE FORMATTING SERVICE, THE FOLLOWING MESSAGE WILL BE  0120 00039000
*      PRINTED:                                                    0120 00040000
*               *** FORMATTING SERVICE ROUTINE ERROR DURING        0120 00041000
*               IHSA FORMATTING(POSSIBLE CAUSE:  DATA NOT IN       0120 00042000
*               DUMP) ***                                          0120 00043000
*      AT THIS POINT PROCESSING WOULD TERMINATE. IF THE IHSA IS    0120 00044000
*      FORMATTED WITHOUT ERROR, A BIT SUMMARY OF THE RT1W IS PRINTED    00045000
*      USING THE ANALYZE INTERNAL PROCEDURE.                       0120 00046000
*                                                                  0120 00047000
*   INPUT -                                                        0120 00048000
*      REGISTER 1 POINTS TO THE PARMLIST WHICH CONTAINS THE        0120 00049000
*      ADDRESS OF THE CONTROL BLOCK TO BE FORMATTED IN ADPLFMT2.   0120 00050000
*      ADPLFMT1, ALSO DEFINED AS START, CONTAINS THE OFFSET TO BE  0120 00051000
*      PLACED ALONG THE FIRST FIELD IN THE CONTROL BLOCK AND ADPLCOM1   00052000
*      CONTAINS THE ADDRESS OF THE ESTAE PARAMETER LIST.           0120 00053000
*                                                                  0120 00054000
*   CALLS -                                                        0120 00055000
*      CALLS CONVERT, FMTRTN, PRINT, AND RT1RTN                    0120 00056000
*                                                                  0120 00057000
*   CALLED BY -                                                    0120 00058000
*       IEAVTFMT TO FORMAT IHSA                                    0120 00059000
*                                                                  0120 00060000
*   RETURNS TO -                                                   0120 00061000
*       IEAVTFMT WITH RETURN CODE SET BY THE ACCESS OR FORMAT SERVICE   00062000
*       ROUTINES OR BY RT1RTN.                                       */ 00063000
*/********************************************************************/ 00064000
*                                                                  0120 00065000
*   ESTAPTR=ADPLCOM1;               /* PTR TO ESTAE PARM LIST        */ 00066000
         L     ESTAPTR,ADPLCOM1(,ABDPLPTR)                         0120 00067000
*   STM(VADR,RCREG,LCALREGS);       /* SAVE REGS FOR RETRY           */ 00068000
         STM   VADR,RCREG,LCALREGS(ESTAPTR)                        0121 00069000
*   ADPLCOM1=ADDR(IHSAFMT);         /* FORMAT ROUTINE ERROR MSG      */ 00070000
         LA    @14,IHSAFMT                                         0122 00071000
         ST    @14,ADPLCOM1(,ABDPLPTR)                             0122 00072000
*   IHSAPTR=ADPLFMT2;               /* PTR TO IHSA                   */ 00073000
         L     IHSAPTR,ADPLFMT2(,ABDPLPTR)                         0123 00074000
*   BITS=BITS&&BITS;                /* ZERO ALL INTERNAL BITS        */ 00075000
*                                                                  0124 00076000
         XC    BITS(1),BITS                                        0124 00077000
*   /*****************************************************************/ 00078000
*   /*                                                               */ 00079000
*   /* IN RELEASE 2 THE POINTER TO THE SUBITS WAS NOT DEFINED.       */ 00080000
*   /* THEREFORE BEFORE ACCESSING THE SUBIT POINTER, CHECK FOR       */ 00081000
*   /* RELEASE 2                                                     */ 00082000
*   /*                                                               */ 00083000
*   /*****************************************************************/ 00084000
*                                                                  0125 00085000
*   VADR=ADDR(CVTNUMB);             /* RELEASE NUMBER                */ 00086000
         L     @14,CVTPTR(,ABDPLPTR)                               0125 00087000
         AL    @14,@CF02109                                        0125 00088000
         LR    VADR,@14                                            0125 00089000
*   CALL ACCESRTN;                  /* ACCESS IT                     */ 00090000
         BAL   @14,ACCESRTN                                        0126 00091000
*   IF RCREG^=ZERO THEN             /* ACCESS ERROR                  */ 00092000
         LTR   RCREG,RCREG                                         0127 00093000
         BZ    @RF00127                                            0127 00094000
*     BUFFER(1:LENGTH(IHSAFMT))=IHSAFMT;/* ERROR MESSAGE             */ 00095000
         L     @14,ADPLBUF(,ABDPLPTR)                              0128 00096000
         MVC   BUFFER(120,@14),IHSAFMT                             0128 00097000
*   ELSE                            /* FOUND RELEASE NUMBER          */ 00098000
*     DO;                                                          0129 00099000
         B     @RC00127                                            0129 00100000
@RF00127 DS    0H                                                  0130 00101000
*       IF DATAHALF^=REL2 THEN      /* NOT A RELEASE 2 MVS SYSTEM    */ 00102000
         CLC   DATAHALF(2,DATAPTR),REL2                            0130 00103000
         BE    @RF00130                                            0130 00104000
*         DO;                                                      0131 00105000
*                                                                  0131 00106000
*           /*********************************************************/ 00107000
*           /*                                                       */ 00108000
*           /* FIND WHICH SUS ARE ON SYSTEM                          */ 00109000
*           /*                                                       */ 00110000
*           /*********************************************************/ 00111000
*                                                                  0132 00112000
*           VADR=ADDR(CVTIHASU);    /* SUBIT PTR                     */ 00113000
         L     @14,CVTPTR(,ABDPLPTR)                               0132 00114000
         LA    VADR,CVTIHASU(,@14)                                 0132 00115000
*           CALL ACCESRTN;          /* ACCESS IT                     */ 00116000
         BAL   @14,ACCESRTN                                        0133 00117000
*           IF RCREG^=ZERO THEN     /* ACCESS ERROR                  */ 00118000
         LTR   RCREG,RCREG                                         0134 00119000
         BZ    @RF00134                                            0134 00120000
*             DO;                                                  0135 00121000
*               BUFFER(1:LENGTH(IHSAFMT))=IHSAFMT;                 0136 00122000
         L     @14,ADPLBUF(,ABDPLPTR)                              0136 00123000
         MVC   BUFFER(120,@14),IHSAFMT                             0136 00124000
*               ERRSW=ON;           /* INDICATE ACCESS ERROR         */ 00125000
         OI    ERRSW,B'01000000'                                   0137 00126000
*             END;                  /* END ACCESS ERROR              */ 00127000
*           ELSE                    /* FOUND SUBIT PTR               */ 00128000
*             DO;                                                  0139 00129000
         B     @RC00134                                            0139 00130000
@RF00134 DS    0H                                                  0140 00131000
*               IF DATAWORD^=ZERO THEN/* THIS IS AN SU SYSTEM        */ 00132000
         L     @14,DATAWORD(,DATAPTR)                              0140 00133000
         LTR   @14,@14                                             0140 00134000
         BZ    @RF00140                                            0140 00135000
*                 DO;                                              0141 00136000
*                   VADR=DATAWORD;  /* ADDRESS OF SUBIT STRING       */ 00137000
         LR    VADR,@14                                            0142 00138000
*                   CALL ACCESRTN;  /* ACCESS IT                     */ 00139000
         BAL   @14,ACCESRTN                                        0143 00140000
*                   IF RCREG^=ZERO THEN/* ACCESS ERROR               */ 00141000
         LTR   RCREG,RCREG                                         0144 00142000
         BZ    @RF00144                                            0144 00143000
*                     DO;                                          0145 00144000
*                       BUFFER(1:LENGTH(IHSAFMT))=IHSAFMT;         0146 00145000
         L     @14,ADPLBUF(,ABDPLPTR)                              0146 00146000
         MVC   BUFFER(120,@14),IHSAFMT                             0146 00147000
*                       ERRSW=ON;   /* INDICATE ERROR HAS OCCURED    */ 00148000
         OI    ERRSW,B'01000000'                                   0147 00149000
*                     END;          /* END ACCESS ERROR              */ 00150000
*                   ELSE            /* FOUND SUBITS                  */ 00151000
*                     DO;                                          0149 00152000
         B     @RC00144                                            0149 00153000
@RF00144 DS    0H                                                  0150 00154000
*                       IF SU50BIT=ON THEN                         0150 00155000
         TM    SU50BIT(DATAPTR),B'00100000'                        0150 00156000
         BNO   @RF00150                                            0150 00157000
*                         SU50SAVE=ON;/* INDICATE SU50 ON SYSTEM     */ 00158000
         OI    SU50SAVE,B'10000000'                                0151 00159000
*                       ELSE                                       0152 00160000
*                         ;         /* SU50 NOT ON SYSTEM            */ 00161000
@RF00150 DS    0H                                                  0153 00162000
*                     END;          /* END FOUND SUBITS              */ 00163000
*                 END;              /* END THIS IS AN SU SYSTEM      */ 00164000
*               ELSE                                               0155 00165000
*                 ;                 /* PRE SU SYSTEM -NO BIT NEEDED  */ 00166000
@RF00140 DS    0H                                                  0156 00167000
*             END;                  /* END COULD ACCESS SUBIT POINTER*/ 00168000
*         END;                      /* END NOT A RELEASE 2 MVS SYSTEM*/ 00169000
*       ELSE                                                       0158 00170000
*         ;                         /* IS A RELEASE 2 MVS SYSTEM, NO    00171000
*                                      SU                            */ 00172000
@RF00130 DS    0H                                                  0159 00173000
*       IF ERRSW=OFF THEN           /* FOUND SYSTEM OK               */ 00174000
@RC00130 TM    ERRSW,B'01000000'                                   0159 00175000
         BNZ   @RF00159                                            0159 00176000
*         DO;                                                      0160 00177000
*                                                                  0160 00178000
*           /*********************************************************/ 00179000
*           /*                                                       */ 00180000
*           /* PUT OUT IHSA HEADER                                   */ 00181000
*           /*                                                       */ 00182000
*           /*********************************************************/ 00183000
*                                                                  0161 00184000
*           SAVEPTR=ADPLFMT2-START; /* VALUE FOR OFFSET GENERATION   */ 00185000
         L     @14,ADPLFMT2(,ABDPLPTR)                             0161 00186000
         SL    @14,START(,ABDPLPTR)                                0161 00187000
         ST    @14,SAVEPTR                                         0161 00188000
*           BUFFER(1:LENGTH(IHSAHDR))=IHSAHDR;/* TITLE LINE          */ 00189000
         L     @14,ADPLBUF(,ABDPLPTR)                              0162 00190000
         MVC   BUFFER(54,@14),IHSAHDR                              0162 00191000
*           PACKAREA=IHSAPTR;       /* PLACE ADDR IN CHAR AREA       */ 00192000
         ST    IHSAPTR,PACKAREA                                    0163 00193000
*           GARBAGE='04'X;          /* GARBAGE BYTE FOR UNPACK       */ 00194000
         MVI   GARBAGE,X'04'                                       0164 00195000
*           UNPK(ADDRESS,UNPACK);   /* PLACE INTO BUFFER             */ 00196000
         UNPK  ADDRESS(9,@14),UNPACK(5)                            0165 00197000
*           TR(ADDRESS,TRANS);      /* TRANSLATE TO PRINTABLE HEX    */ 00198000
         TR    ADDRESS(9,@14),TRANS                                0166 00199000
*           CALL PRINT;             /* PRINT TITLE LINE              */ 00200000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0167 00201000
         BALR  @14,@15                                             0167 00202000
*           BUFFER(1:LENGTH(IHSAUS))=IHSAUS;/* UNDERSCORE            */ 00203000
         L     @14,ADPLBUF(,ABDPLPTR)                              0168 00204000
         MVC   BUFFER(28,@14),IHSAUS                               0168 00205000
*           CALL PRINT;             /* PRINT IT                      */ 00206000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0169 00207000
         BALR  @14,@15                                             0169 00208000
*           CALL PRINT;             /* BLANK                         */ 00209000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0170 00210000
         BALR  @14,@15                                             0170 00211000
*           CALL PRINT;             /* BLANK                         */ 00212000
*                                                                  0171 00213000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0171 00214000
         BALR  @14,@15                                             0171 00215000
*           /*********************************************************/ 00216000
*           /*                                                       */ 00217000
*           /* SECTION UNIQUE TO IHSA                                */ 00218000
*           /*                                                       */ 00219000
*           /*********************************************************/ 00220000
*                                                                  0172 00221000
*           DO ARRAYCNT=1 TO DIM(IHSANAME) WHILE(ERRSW=OFF);/* FORMAT   00222000
*                                      IHSA UP TO FRRS               */ 00223000
         LA    ARRAYCNT,1                                          0172 00224000
@DL00172 TM    ERRSW,B'01000000'                                   0172 00225000
         BNZ   @DC00172                                            0172 00226000
*             LINEPTR=IHSANAME(ARRAYCNT);/* FORMAT PATTERN           */ 00227000
         LR    @14,ARRAYCNT                                        0173 00228000
         SLA   @14,2                                               0173 00229000
         L     LINEPTR,IHSANAME-4(@14)                             0173 00230000
*             CALL CONVERT;         /* OFFSET GENERATOR              */ 00231000
         BAL   @14,CONVERT                                         0174 00232000
*             CALL FMTRTN;          /* FORMAT                        */ 00233000
         BAL   @14,FMTRTN                                          0175 00234000
*           END;                    /* END FORMAT LOOP               */ 00235000
         AH    ARRAYCNT,@CH00041                                   0176 00236000
         CH    ARRAYCNT,@CH00080                                   0176 00237000
         BNH   @DL00172                                            0176 00238000
@DC00172 DS    0H                                                  0177 00239000
*           IF ERRSW=OFF THEN       /* NO ERRORS SO FAR              */ 00240000
         TM    ERRSW,B'01000000'                                   0177 00241000
         BNZ   @RF00177                                            0177 00242000
*             DO;                                                  0178 00243000
*               CALL PRINT;         /* BLANK                         */ 00244000
*                                                                  0179 00245000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0179 00246000
         BALR  @14,@15                                             0179 00247000
*               /*****************************************************/ 00248000
*               /*                                                   */ 00249000
*               /* IHSA SECTION WHICH CHANGES WITH SU50              */ 00250000
*               /*                                                   */ 00251000
*               /*****************************************************/ 00252000
*                                                                  0180 00253000
*               BUFFER(31:(LENGTH(IHSAFMSG)+30))=IHSAFMSG;/* FRRS  0180 00254000
*                                      STACK HDR                     */ 00255000
         L     @14,ADPLBUF(,ABDPLPTR)                              0180 00256000
         MVC   BUFFER+30(9,@14),IHSAFMSG                           0180 00257000
*               CALL PRINT;         /* PRINT IT                      */ 00258000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0181 00259000
         BALR  @14,@15                                             0181 00260000
*               BUFFER(31:(LENGTH(IHSAFRUS)+30))=IHSAFRUS;/*       0182 00261000
*                                      UNDERSCORE                    */ 00262000
         L     @14,ADPLBUF(,ABDPLPTR)                              0182 00263000
         MVC   BUFFER+30(9,@14),IHSAFRUS                           0182 00264000
*               CALL PRINT;         /* PRINT IT                      */ 00265000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0183 00266000
         BALR  @14,@15                                             0183 00267000
*               CALL PRINT;         /* BLANK                         */ 00268000
*                                                                  0184 00269000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0184 00270000
         BALR  @14,@15                                             0184 00271000
*               /*****************************************************/ 00272000
*               /*                                                   */ 00273000
*               /* THIS LINE OF THE FRRS IS FORMATTED HERE BECAUSE   */ 00274000
*               /* SU50 CHANGED THE CONTENTS OF THIS AREA            */ 00275000
*               /*                                                   */ 00276000
*               /*****************************************************/ 00277000
*                                                                  0185 00278000
*               FRRSPTR=ADPLFMT2;   /* SET PTR FOR FIGURING NUM    0185 00279000
*                                      ENTRIES                       */ 00280000
         MVC   FRRSPTR(4),ADPLFMT2(ABDPLPTR)                       0185 00281000
*               ARRAYCNT=1;                                        0186 00282000
         LA    ARRAYCNT,1                                          0186 00283000
*               IF SU50SAVE=ON THEN /* SU50 ON SYSTEM                */ 00284000
         TM    SU50SAVE,B'10000000'                                0187 00285000
         BNO   @RF00187                                            0187 00286000
*                 LINEPTR=IHSASU50(ARRAYCNT);/* SU50 VERSION         */ 00287000
         LR    @14,ARRAYCNT                                        0188 00288000
         SLA   @14,2                                               0188 00289000
         L     LINEPTR,IHSASU50-4(@14)                             0188 00290000
*               ELSE                /* NON SU50 VERSION              */ 00291000
*                 LINEPTR=FRRNAMES(ARRAYCNT);/* COMMON FRRS LINE     */ 00292000
         B     @RC00187                                            0189 00293000
@RF00187 LR    @14,ARRAYCNT                                        0189 00294000
         SLA   @14,2                                               0189 00295000
         L     LINEPTR,FRRNAMES-4(@14)                             0189 00296000
*               CALL CONVERT;       /* OFFSET                        */ 00297000
@RC00187 BAL   @14,CONVERT                                         0190 00298000
*               CALL FMTRTN;        /* FORMAT                        */ 00299000
         BAL   @14,FMTRTN                                          0191 00300000
*               IF ERRSW=OFF THEN                                  0192 00301000
         TM    ERRSW,B'01000000'                                   0192 00302000
         BNZ   @RF00192                                            0192 00303000
*                 CALL RT1RTN;      /* IF STILL NO ERRORS FINISH   0193 00304000
*                                      PROCESSING                    */ 00305000
         BAL   @14,RT1RTN                                          0193 00306000
*               ELSE                                               0194 00307000
*                 ;                 /* FRRS ERROR                    */ 00308000
@RF00192 DS    0H                                                  0195 00309000
*             END;                  /* END NO IHSA ERROR             */ 00310000
*           ELSE                                                   0196 00311000
*             ;                     /* IHSA ERROR                    */ 00312000
@RF00177 DS    0H                                                  0197 00313000
*         END;                      /* END FOUND SYSTEM              */ 00314000
*       ELSE                                                       0198 00315000
*         ;                         /* ERROR FINDING SYSTEM          */ 00316000
@RF00159 DS    0H                                                  0199 00317000
*     END;                          /* END COULD ACCESS REL NUMBER   */ 00318000
*   CALL PRINT;                     /* ERROR MSG OR BLANK            */ 00319000
@RC00127 L     @15,ADPLPRNT(,ABDPLPTR)                             0200 00320000
         BALR  @14,@15                                             0200 00321000
*   LCALREGS=RTYREGS;               /* RESTORE RETRY REGS OF CALLER  */ 00322000
         MVC   LCALREGS(64,ESTAPTR),RTYREGS(ESTAPTR)               0201 00323000
*   RETURN CODE(RCCODE);            /* DO NOT ENTER FRR CODE         */ 00324000
         L     @09,RCCODE                                          0202 00325000
         L     @13,4(,@13)                                         0202 00326000
         L     @00,@SIZDATD                                        0202 00327000
         LR    @01,@10                                             0202 00328000
         FREEMAIN R,LV=(0),A=(1)                                        00329000
         LR    @15,@09                                             0202 00330000
         L     @14,12(,@13)                                        0202 00331000
         LM    @00,@12,20(@13)                                     0202 00332000
         BR    @14                                                 0202 00333000
*IEAFTFRR:                                                         0203 00334000
*   ENTRY;                                                         0203 00335000
*                                                                  0203 00336000
@EP00203 DS    0H                                                  0204 00337000
*/********************************************************************/ 00338000
*/* FUNCTION-                                                      0204 00339000
*       ROUTINE WHICH FORMATS THE FRR STACK TITLE AND LINES WHICH ARE   00340000
*     DIFFERENT THAN THOSE IN THE INTERRUPT HANDLER SAVE AREA.     0204 00341000
*     IT SETS UP THE NEEDED POINTERS TO BE PASSED TO THE FORMAT ROUTINE 00342000
*     SENT IN THE PARAMETER LIST.  FOR ANY ERROR ACCESSING A NEEDED     00343000
*     FIELD IN THE FRRS OR FOR NON-ZERO RETURN CODE FROM THE       0204 00344000
*     FORMATTING SERVICE, THE FOLLOWING MESSAGES WILL BE PRINTED:  0204 00345000
*                       *** FORMATTING SERVICE ROUTINE ERROR DURING FRR 00346000
*                           FORMATTING (POSSIBLE CAUSE: DATA NOT IN     00347000
*                           DUMP ) ***.                            0204 00348000
*     AT THIS POINT, FOR EITHER REASON, THE FRR PROCESSING WOULD   0204 00349000
*     TERMINATE. IF THE FRR LINES ARE FORMATTED WITHOUT AN ACCESS  0204 00350000
*     ERROR, THE ROUTINE TO FORMAT THE FRRS-IHSA COMMON AREAS IS   0204 00351000
*     CALLED.                                                      0204 00352000
*                                                                  0204 00353000
*   INPUT-                                                         0204 00354000
*       REGISTER 1 POINTS TO THE PARMLIST WHICH CONTAINS THE ADDRESS    00355000
*     OF THE CONTROL BLOCK TO BE FORMATTED IN ADPLFMT2. ADPLFMT1 ALSO   00356000
*     DEFINED AS START, CONTAINS THE OFFSET TO BE PLACED ALONG SIDE     00357000
*     THE FIRST FIELD IN THE CONTROL BLOCK AND ADPLCOM1 CONTAINS THE    00358000
*     ADDRESS OF THE ESTAE PARAMETER LIST.                         0204 00359000
*                                                                  0204 00360000
*   CALLS-                                                         0204 00361000
*       CALLS ACCESRTN,CONVERT,FMTRTN,RT1RTN,AND PRINT             0204 00362000
*                                                                  0204 00363000
*   CALLED BY -                                                    0204 00364000
*       IEAVTFMT TO FORMAT FRRS AND RT1W.                          0204 00365000
*                                                                  0204 00366000
*   RETURNS TO -                                                   0204 00367000
*       CALLER WITH A RETURN CODE SET BY THE ACCESS OR FORMAT      0204 00368000
*       SERVICE ROUTINES OR BY RT1RTN.                               */ 00369000
*/********************************************************************/ 00370000
*                                                                  0204 00371000
*   ESTAPTR=ADPLCOM1;               /* PTR TO ESTAE PARM LIST        */ 00372000
         L     ESTAPTR,ADPLCOM1(,ABDPLPTR)                         0204 00373000
*   STM(VADR,RCREG,LCALREGS);       /* SAVE REGS FOR RETRY           */ 00374000
         STM   VADR,RCREG,LCALREGS(ESTAPTR)                        0205 00375000
*   ADPLCOM1=ADDR(FRRSFMT);         /* FORMAT ROUTINE ERROR MESSAGE  */ 00376000
         LA    @14,FRRSFMT                                         0206 00377000
         ST    @14,ADPLCOM1(,ABDPLPTR)                             0206 00378000
*   FRRSPTR=ADPLFMT2;               /* PTR TO FRRS CONTROL BLOCK     */ 00379000
         L     @14,ADPLFMT2(,ABDPLPTR)                             0207 00380000
         ST    @14,FRRSPTR                                         0207 00381000
*   BITS=BITS&&BITS;                /* ZERO THE INTERNAL BITS        */ 00382000
         XC    BITS(1),BITS                                        0208 00383000
*   SAVEPTR=ADPLFMT2-START;         /* VALUE FOR DYNAMIC OFFSET    0209 00384000
*                                      GENERATOR                     */ 00385000
         LR    @08,@14                                             0209 00386000
         SL    @08,START(,ABDPLPTR)                                0209 00387000
         ST    @08,SAVEPTR                                         0209 00388000
*   BUFFER(1:LENGTH(FRRHDR))=FRRHDR;/* TITLE LINE                    */ 00389000
         L     @08,ADPLBUF(,ABDPLPTR)                              0210 00390000
         MVC   BUFFER(54,@08),FRRHDR                               0210 00391000
*   PACKAREA=FRRSPTR;               /* PLACE ADDR INTO CHAR AREA     */ 00392000
         ST    @14,PACKAREA                                        0211 00393000
*   GARBAGE='04'X;                  /* GARBAGE BYTE FOR THE UNPACK   */ 00394000
         MVI   GARBAGE,X'04'                                       0212 00395000
*   UNPK(ADDRESS,UNPACK);           /* PLACE ADDRESS INTO BUFFER LINE*/ 00396000
         UNPK  ADDRESS(9,@08),UNPACK(5)                            0213 00397000
*   TR(ADDRESS,TRANS);              /* TRANSLATE TO PRINTABLE HEX    */ 00398000
         TR    ADDRESS(9,@08),TRANS                                0214 00399000
*   CALL PRINT;                     /* PRINT TITLE                   */ 00400000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0215 00401000
         BALR  @14,@15                                             0215 00402000
*   BUFFER(1:LENGTH(FRRSUS))=FRRSUS;/* UNDERSCORE                    */ 00403000
         L     @14,ADPLBUF(,ABDPLPTR)                              0216 00404000
         MVC   BUFFER(18,@14),FRRSUS                               0216 00405000
*   CALL PRINT;                     /* PRINT IT                      */ 00406000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0217 00407000
         BALR  @14,@15                                             0217 00408000
*   CALL PRINT;                     /* BLANK                         */ 00409000
*                                                                  0218 00410000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0218 00411000
         BALR  @14,@15                                             0218 00412000
*   /*****************************************************************/ 00413000
*   /*                                                               */ 00414000
*   /* SECTION OF THE FRR STACK PRECEDING THE FRR ENTRIES            */ 00415000
*   /*                                                               */ 00416000
*   /*****************************************************************/ 00417000
*                                                                  0219 00418000
*   ARRAYCNT=1;                     /* FIRST FORMAT PATTERN          */ 00419000
         LA    ARRAYCNT,1                                          0219 00420000
*   LINEPTR=FRRNAMES(ARRAYCNT);     /* ADDRESS OF FIRST FORMAT     0220 00421000
*                                      PATTERN                       */ 00422000
         LR    @14,ARRAYCNT                                        0220 00423000
         SLA   @14,2                                               0220 00424000
         L     LINEPTR,FRRNAMES-4(@14)                             0220 00425000
*   CALL CONVERT;                   /* GENERATE OFFSET               */ 00426000
         BAL   @14,CONVERT                                         0221 00427000
*   CALL FMTRTN;                    /* FORMAT                        */ 00428000
         BAL   @14,FMTRTN                                          0222 00429000
*   IF ERRSW=OFF THEN               /* CONTINUE ONLY IF FORMATTED OK */ 00430000
         TM    ERRSW,B'01000000'                                   0223 00431000
         BNZ   @RF00223                                            0223 00432000
*     CALL RT1RTN;                  /* ROUTINE TO FORMAT FRR IHSA  0224 00433000
*                                      COMMON AREAS                  */ 00434000
         BAL   @14,RT1RTN                                          0224 00435000
*   ELSE                                                           0225 00436000
*     ;                             /* DID NOT FORMAT OK             */ 00437000
@RF00223 DS    0H                                                  0226 00438000
*   LCALREGS=RTYREGS;               /* RESTORE RETRY REGS OF CALLER  */ 00439000
         MVC   LCALREGS(64,ESTAPTR),RTYREGS(ESTAPTR)               0226 00440000
*   RETURN CODE(RCCODE);            /* END ENTRY POINT IEAFTFRR      */ 00441000
         L     @09,RCCODE                                          0227 00442000
         L     @13,4(,@13)                                         0227 00443000
         L     @00,@SIZDATD                                        0227 00444000
         LR    @01,@10                                             0227 00445000
         FREEMAIN R,LV=(0),A=(1)                                        00446000
         LR    @15,@09                                             0227 00447000
         L     @14,12(,@13)                                        0227 00448000
         LM    @00,@12,20(@13)                                     0227 00449000
         BR    @14                                                 0227 00450000
*RT1RTN:                                                           0228 00451000
*   PROC OPTIONS(SAVE(14));                                        0228 00452000
*                                                                  0228 00453000
@EL00001 L     @13,4(,@13)                                         0228 00454000
@EF00001 L     @00,@SIZDATD                                        0228 00455000
         LR    @01,@10                                             0228 00456000
         FREEMAIN R,LV=(0),A=(1)                                        00457000
@ER00001 LM    @14,@12,12(@13)                                     0228 00458000
         BR    @14                                                 0228 00459000
RT1RTN   ST    @14,@SA00002                                        0228 00460000
*/********************************************************************/ 00461000
*/* FUNCTION-                                                      0229 00462000
*       ROUTINE WHICH FORMATS THE FRR STACK FROM THE RT1W ON.      0229 00463000
*     THIS SECTION OF THE CONTROL BLOCK IS COMMON BETWEEN THE IHSA 0229 00464000
*     AND FRR STACK.                                               0229 00465000
*     IT SETS UP THE NEEDED POINTERS TO BE PASSED TO THE FORMAT ROUTINE 00466000
*     SENT IN THE PARAMETER LIST.  FOR ANY ERROR ACCESSING A NEEDED     00467000
*     FIELD IN THE RT1W OR FRRS OR FOR NON-ZERO RETURN CODE FROM THE    00468000
*     FORMATTING SERVICE, THE FOLLOWING MESSAGES WILL BE PRINTED:  0229 00469000
*                       *** FORMATTING SERVICE ROUTINE ERROR DURING FRR 00470000
*                           FORMATTING (POSSIBLE CAUSE: DATA NOT IN     00471000
*                           DUMP ) ***.                            0229 00472000
*     IF THE NEEDED FIELDS FOR DETERMINING THE NUMBER OF ENTRIES COULD  00473000
*     BE ACCESSED BUT THE NUMBER OF ENTRIES IS NOT BETWEEN 0 AND 16-THE 00474000
*     VALUES MUST BE IN ERROR AND THE FOLLOWING MESSAGE WILL BE    0229 00475000
*     PRINTED:                                                     0229 00476000
*                       *** ERROR IN THE FIELDS USED TO DETERMINE  0229 00477000
*                           THE NUMBER OF FRR ENTRIES ***          0229 00478000
*     AT THIS POINT, FOR EITHER REASON, THE FRR PROCESSING WOULD   0229 00479000
*     TERMINATE. IF THE FRR STACK IS FORMATTED WITHOUT AN ACCESS   0229 00480000
*     ERROR, A BIT SUMMARY OF THE RT1W IS PRINTED USING THE ANALYZE     00481000
*     PROC.                                                        0229 00482000
*                                                                  0229 00483000
*                                                                  0229 00484000
*   CALLS-                                                         0229 00485000
*       CALLS ACCESRTN,CONVERT,FMTRTN,RT1BITS,AND PRINT            0229 00486000
*                                                                  0229 00487000
*   CALLED BY -                                                    0229 00488000
*             IEAFTFRR AND IEAFTIHS TO FORMAT THE IHSA FRRS COMMON 0229 00489000
*             AREAS.                                               0229 00490000
*                                                                  0229 00491000
*   RETURNS TO -                                                   0229 00492000
*              CALLER WITH A RETURN CODE SET BY THE ACCESS OR FORMAT    00493000
*             SERVICE ROUTINES.                                      */ 00494000
*/********************************************************************/ 00495000
*                                                                  0229 00496000
*   ERRSW=OFF;                                                     0229 00497000
         NI    ERRSW,B'10111111'                                   0229 00498000
*   CALL PRINT;                     /* BLANK LINE                    */ 00499000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0230 00500000
         BALR  @14,@15                                             0230 00501000
*   RT1PTR=ADPLFMT2;                /* RT1W ADDRESS                  */ 00502000
         MVC   RT1PTR(4),ADPLFMT2(ABDPLPTR)                        0231 00503000
*                                                                  0232 00504000
*   /*****************************************************************/ 00505000
*   /*                                                               */ 00506000
*   /* FIGURE OUT THE NUMBER OF POSSIBLE ENTRIES IN THE FRR STACK.   */ 00507000
*   /* FOR RELEASE 2 OF MVS THE NUMBER OF ENTRIES WAS 8. FOR RELEASE */ 00508000
*   /* 3 THIS NUMBER CHANGED TO 16. THE NUMBER CAN BE DETERMINED BY  */ 00509000
*   /* TAKING THE LAST POSSIBLE ENTRY ADDRESS AND SUBTRACTING FROM IT*/ 00510000
*   /* THE FIRST POSSIBLE ADDRESS AND THEN DIVIDING BY THE LENGTH OF */ 00511000
*   /* OF THE ENTRY.                                                 */ 00512000
*   /*                                                               */ 00513000
*   /*****************************************************************/ 00514000
*                                                                  0232 00515000
*   IF SU50SAVE=ON THEN             /* SET NUMBER OF FRR ENTRIES -16 */ 00516000
         TM    SU50SAVE,B'10000000'                                0232 00517000
         BNO   @RF00232                                            0232 00518000
*     ENTRYMAX=SU50MAX;                                            0233 00519000
         LA    ENTRYMAX,16                                         0233 00520000
*   ELSE                            /* DYNAMICALLY FIGURE NUM ENTRIES*/ 00521000
*     DO;                                                          0234 00522000
         B     @RC00232                                            0234 00523000
@RF00232 DS    0H                                                  0235 00524000
*       VADR=ADDR(FRRSLAST);        /* LAST POSSIBLE ENTRY ADDRESS   */ 00525000
         L     @14,FRRSPTR                                         0235 00526000
         LA    VADR,FRRSLAST(,@14)                                 0235 00527000
*       CALL ACCESRTN;              /* ACCESS IT                     */ 00528000
         BAL   @14,ACCESRTN                                        0236 00529000
*       IF RCREG^=ZERO THEN         /* ERROR ACCESSING THE FRR SATCK */ 00530000
         LTR   RCREG,RCREG                                         0237 00531000
         BZ    @RF00237                                            0237 00532000
*         DO;                                                      0238 00533000
*           BUFFER(1:LENGTH(FRRSFMT))=FRRSFMT;/* ERROR MESSAGE       */ 00534000
         L     @14,ADPLBUF(,ABDPLPTR)                              0239 00535000
         MVC   BUFFER(120,@14),FRRSFMT                             0239 00536000
*           CALL PRINT;             /* PRINT IT                      */ 00537000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0240 00538000
         BALR  @14,@15                                             0240 00539000
*           ERRSW=ON;               /* ERROR INDICATOR               */ 00540000
         OI    ERRSW,B'01000000'                                   0241 00541000
*         END;                      /* END ERROR ACCESSING           */ 00542000
*       ELSE                        /* FOUND LAST ENTRY ADDRESS      */ 00543000
*         DO;                                                      0243 00544000
