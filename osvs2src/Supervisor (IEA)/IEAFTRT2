*       MSGLNGTH=LENGTH(FLAGSMSG);  /* MSG ARRAY LENGTH              */ 01389000
         MVC   MSGLNGTH(4),@CF03617                                0563 01390000
*       CALL CONVERT;               /* GENERATE OFFSET               */ 01391000
         BAL   @14,CONVERT                                         0564 01392000
*       OFFSET=CONVERTD;            /* PUT OFFSET IN BUFFER          */ 01393000
         L     @14,ADPLBUF(,ABDPLPTR)                              0565 01394000
         MVC   OFFSET(6,@14),CONVERTD                              0565 01395000
*       COLNUM=21;                  /* WHERE IN BUFFER TO START      */ 01396000
         MVC   COLNUM(4),@CF02753                                  0566 01397000
*       CALL ANALYZE;               /* ANALYZE THE BYTE ANALYZE BYTE */ 01398000
         BAL   @14,ANALYZE                                         0567 01399000
*     END;                          /* END ANALYZE BYTE              */ 01400000
*   ELSE                                                           0569 01401000
*     ;                             /* BYTE DID NOT NEED TO BE     0569 01402000
*                                      ANALYZED                      */ 01403000
*                                                                  0569 01404000
@RF00550 DS    0H                                                  0570 01405000
*   /*****************************************************************/ 01406000
*   /*                                                               */ 01407000
*   /* ANALYZE RECURSION FLAGS                                       */ 01408000
*   /*                                                               */ 01409000
*   /*****************************************************************/ 01410000
*                                                                  0570 01411000
*   VADR=ADDR(ESARFLAG);            /* RECURSION FLAGS               */ 01412000
         L     @14,ESAPTR                                          0570 01413000
         LA    VADR,ESARFLAG(,@14)                                 0570 01414000
*   CALL ACCESRTN;                  /* ACCESS IT                     */ 01415000
         BAL   @14,ACCESRTN                                        0571 01416000
*   IF(RCREG=ZERO)&(DATABYTE^=ZEROBYTE) THEN/* ANALYZE BYTE          */ 01417000
