*/********************************************************************/ 01169000
*/* FUNCTION -                                                     0498 01170000
*      ROUTINE WHICH FORMATS THE PART OF THE RTM2 WORK AREA THAT   0498 01171000
*      CHANGES FOR DIFFERENT SU CHANGES. IT CHECKS THE ARRAY NUMBER     01172000
*      TO DETERMINE WHAT DECISION HAS TO BE MADE AND FORMATS THE OUTPUT 01173000
*      LINE ACCORDINGLY.                                           0498 01174000
*                                                                  0498 01175000
*   CALLS -                                                        0498 01176000
*      CALLS CONVERT AND FMTRTN                                    0498 01177000
*                                                                  0498 01178000
*   INPUT -                                                        0498 01179000
*      EXPECTS SUSAVE BITS AND THE ARRAYCNT FILLED IN.             0498 01180000
*                                                                  0498 01181000
*   CALLED BY -                                                    0498 01182000
*      IEAFTRT2 TO FORMAT THE RTM2WA FIELDS CHANGED BY AN SU       0498 01183000
*                                                                  0498 01184000
*   RETURNS TO -                                                   0498 01185000
*      IEAFTRT2 WITH RETURN CODE SET BY FORMAT SERVICE             0498 01186000
*      ROUTINE                                                       */ 01187000
*/********************************************************************/ 01188000
*                                                                  0498 01189000
*   IF RT2ARRAY(ARRAYCNT)=102 THEN  /* FIELDS ADDED AT END FOR BOTH     01190000
*                                      SU17 & SU33                   */ 01191000
         LR    @14,ARRAYCNT                                        0498 01192000
         SLA   @14,2                                               0498 01193000
         L     @14,RT2ARRAY-4(@14)                                 0498 01194000
         CH    @14,@CH01679                                        0498 01195000
         BNE   @RF00498                                            0498 01196000
*     DO;                                                          0499 01197000
*       IF SU33SAVE=ON THEN         /* SU33 ADDED 25 WORDS OVER SU0     01198000
*                                      BASE(3 OF WHICH WERE ADDED BY    01199000
*                                      SU17                          */ 01200000
         TM    SU33SAVE,B'01000000'                                0500 01201000
         BNO   @RF00500                                            0500 01202000
*         DO;                                                      0501 01203000
*           LINEPTR=RTM2NAME(8);    /* SINCE 25 WORDS OF OUTPUT ARE     01204000
*                                      NEEDED AND 8 IS MAXIMUM LIMIT    01205000
*                                      FOR A LINE-NEED FOUR LINES OF    01206000
*                                      OUTPUT                @ZM45190*/ 01207000
         L     LINEPTR,RTM2NAME+28                                 0502 01208000
*           CALL CONVERT;           /* GENERATE OFFSET               */ 01209000
         BAL   @14,CONVERT                                         0503 01210000
*           CALL FMTRTN;            /* FORMAT THE LINE               */ 01211000
         BAL   @14,FMTRTN                                          0504 01212000
*           IF ERRSW=OFF THEN       /* NO PROBLEM PUTTING OUT LINE 1 */ 01213000
         TM    ERRSW,B'00100000'                                   0505 01214000
         BNZ   @RF00505                                            0505 01215000
*             DO;                                                  0506 01216000
*               CALL CONVERT;       /* GENERATE OFFSET               */ 01217000
         BAL   @14,CONVERT                                         0507 01218000
*               CALL FMTRTN;        /* FORMAT THE LINE               */ 01219000
         BAL   @14,FMTRTN                                          0508 01220000
*               IF ERRSW=OFF THEN   /* NO PROBLEM PUTTING OUT LINE 2 */ 01221000
         TM    ERRSW,B'00100000'                                   0509 01222000
         BNZ   @RF00509                                            0509 01223000
*                 DO;                                              0510 01224000
*                   CALL CONVERT;   /* OFFSET                        */ 01225000
         BAL   @14,CONVERT                                         0511 01226000
*                   CALL FMTRTN;    /* FORMAT                        */ 01227000
         BAL   @14,FMTRTN                                          0512 01228000
*                   IF ERRSW=OFF THEN/* NO PROBLEM PUTTING OUT LINE 3   01229000
*                                                            @ZM45190*/ 01230000
         TM    ERRSW,B'00100000'                                   0513 01231000
         BNZ   @RF00513                                            0513 01232000
*                     DO;           /*                       @ZM45190*/ 01233000
*                       LINEPTR=RTM2NAME(1);/* ONE WORD OF DATA    0515 01234000
*                                                            @ZM45190*/ 01235000
         L     LINEPTR,RTM2NAME                                    0515 01236000
*                       CALL CONVERT;/* OFFSET               @ZM95190*/ 01237000
         BAL   @14,CONVERT                                         0516 01238000
*                       CALL FMTRTN;/* FORMAT                @ZM45190*/ 01239000
         BAL   @14,FMTRTN                                          0517 01240000
*                     END;          /* END NO PROBLEM WITH LINE 3  0518 01241000
*                                                            @ZM45190*/ 01242000
*                   ELSE                                           0519 01243000
*                     ;             /* PROBLEM FORMATTING THIRD LINE    01244000
*                                                            @ZM45190*/ 01245000
@RF00513 DS    0H                                                  0520 01246000
*                 END;              /* END NO PROBLEM WITH LINE 2    */ 01247000
*               ELSE                                               0521 01248000
*                 ;                 /* PROBLEM FORMATTING SECOND LINE*/ 01249000
@RF00509 DS    0H                                                  0522 01250000
*             END;                  /* FORMATTED LINE 1 OK           */ 01251000
*           ELSE                                                   0523 01252000
*             ;                     /* PROBLEM FORMATTING FIRST LINE */ 01253000
@RF00505 DS    0H                                                  0524 01254000
*         END;                      /* END SU33 ENDING               */ 01255000
*       ELSE                                                       0525 01256000
*         IF SU17SAVE=ON THEN       /* SU17 ADDED ONLY 3 WORDS TO THE   01257000
*                                      END                           */ 01258000
         B     @RC00500                                            0525 01259000
@RF00500 TM    SU17SAVE,B'10000000'                                0525 01260000
         BNO   @RF00525                                            0525 01261000
*           DO;                                                    0526 01262000
*             LINEPTR=RTM2NAME(3);                                 0527 01263000
         L     LINEPTR,RTM2NAME+8                                  0527 01264000
*             CALL CONVERT;         /* GENERATE OFFSET               */ 01265000
         BAL   @14,CONVERT                                         0528 01266000
*             CALL FMTRTN;          /* FORMAT                        */ 01267000
         BAL   @14,FMTRTN                                          0529 01268000
*           END;                    /* END SU17 ENDING               */ 01269000
*         ELSE                                                     0531 01270000
*           ;                       /* NO NEED TO ADD ANY MORE-BASE     01271000
*                                      TYPE CONTROL BLOCK            */ 01272000
@RF00525 DS    0H                                                  0532 01273000
*     END;                          /* END FIELDS AT END CHECK       */ 01274000
*   ELSE                                                           0533 01275000
*     ;                             /* UNKNOWN CHECK VALUE           */ 01276000
@RF00498 DS    0H                                                  0534 01277000
*   RETURN CODE(RCCODE);                                           0534 01278000
@RC00498 L     @15,RCCODE                                          0534 01279000
@EL00005 DS    0H                                                  0534 01280000
@EF00005 DS    0H                                                  0534 01281000
@ER00005 L     @14,@SA00005                                        0534 01282000
         BR    @14                                                 0534 01283000
*   END SPECIAL;                                                   0535 01284000
*IEAFTESA:                                                         0536 01285000
*   ENTRY;                                                         0536 01286000
*                                                                  0536 01287000
@EP00536 DS    0H                                                  0537 01288000
*/********************************************************************/ 01289000
*/* FUNCTION -                                                     0537 01290000
*      ROUTINE WHICH FORMATS THE BIT FLAGS FROM THE ESA RELATED TO 0537 01291000
*      THE RTM2WA JUST FORMATTED.                                  0537 01292000
*                                                                  0537 01293000
*   INPUT -                                                        0537 01294000
*      REGISTER 1 POINTS TO THE PARMLIST WHICH CONTAINS THE        0537 01295000
*      ADDRESS OF THE CONTROL BLOCK TO BE FORMATTED IN ADPLFMT2    0537 01296000
*      AND THE ADDRESS OF THE ESTAE PARAMETER LIST IN ADPLCOM1.    0537 01297000
*                                                                  0537 01298000
*   CALLS -                                                        0537 01299000
*      CALLS ACCESRTN, ANALYZE, BYTEMODE, CONVERT, AND PRINT       0537 01300000
*                                                                  0537 01301000
*   CALLED BY -                                                    0537 01302000
*      IEAVTFMT TO FORMAT ESA BIT SUMMARY                          0537 01303000
*                                                                  0537 01304000
*   RETURNS TO -                                                   0537 01305000
*      IEAVTFMT WITH RETURN CODE SET BY ACCESS SERVICE             0537 01306000
*      ROUTINES                                                      */ 01307000
*/********************************************************************/ 01308000
*                                                                  0537 01309000
*   ESTAPTR=ADPLCOM1;               /* SAVE PTR TO ESTAE PARM LIST   */ 01310000
         L     @14,ADPLCOM1(,ABDPLPTR)                             0537 01311000
         ST    @14,ESTAPTR                                         0537 01312000
*   STM(VADR,RCREG,LCALREGS);       /* SAVE REGS FOR RETRY           */ 01313000
         STM   VADR,RCREG,LCALREGS(@14)                            0538 01314000
*   SAVEPTR=ADPLFMT2-START;         /* USED IN GENERATING OFFSETS    */ 01315000
         L     @14,ADPLFMT2(,ABDPLPTR)                             0539 01316000
         LR    @08,@14                                             0539 01317000
         SL    @08,START(,ABDPLPTR)                                0539 01318000
         ST    @08,SAVEPTR                                         0539 01319000
*   RBSECPTR=ADPLFMT2;              /* PTR TO SVRB RELATED TO RTM2WA */ 01320000
         LR    RBSECPTR,@14                                        0540 01321000
*   HDRBIT=OFF;                     /* HEADER INDICATOR              */ 01322000
         NI    HDRBIT,B'11101111'                                  0541 01323000
*   HDR(1:LENGTH(ESAHDR))=ESAHDR;   /* PLACE STATIC HDR IN HDR       */ 01324000
         MVC   HDR(61),ESAHDR                                      0542 01325000
*   PACKAREA=RBSECPTR;              /* CHAR AREA FOR ADDRESS TO UNPK */ 01326000
         ST    RBSECPTR,PACKAREA                                   0543 01327000
*   GARBAGE='04'X;                  /* GARBAGE BYTE FOR THE UNPK     */ 01328000
         MVI   GARBAGE,X'04'                                       0544 01329000
*   UNPK(SVRBADR,UNPACK);           /* PLACE IN HEADER               */ 01330000
         UNPK  SVRBADR(9),UNPACK(5)                                0545 01331000
*   TR(SVRBADR,TRANS);              /* TRANSLATE TO PRINTABLE HEX    */ 01332000
         TR    SVRBADR(9),TRANS                                    0546 01333000
*   ESAPTR=ADDR(RBEXSAVE);          /* ESA EMBEDDED IN SVRB          */ 01334000
*                                                                  0547 01335000
         LA    @14,RBEXSAVE(,RBSECPTR)                             0547 01336000
         ST    @14,ESAPTR                                          0547 01337000
*   /*****************************************************************/ 01338000
*   /*                                                               */ 01339000
*   /* ENTRY RTM2 REASON                                             */ 01340000
*   /*                                                               */ 01341000
*   /*****************************************************************/ 01342000
*                                                                  0548 01343000
*   VADR=ADDR(ESAFLAGS);            /* FIRST BYTE OF FLAGS           */ 01344000
         LA    VADR,ESAFLAGS(,@14)                                 0548 01345000
*   CALL ACCESRTN;                  /* ACCESS IT                     */ 01346000
         BAL   @14,ACCESRTN                                        0549 01347000
*   IF(RCREG=ZERO)&(DATABYTE^=ZEROBYTE) THEN/* BYTE NEEDS TO BE    0550 01348000
*                                      ANALYZED                      */ 01349000
         LTR   RCREG,RCREG                                         0550 01350000
         BNZ   @RF00550                                            0550 01351000
         CLC   DATABYTE(1,DATAPTR),ZEROBYTE                        0550 01352000
         BE    @RF00550                                            0550 01353000
*     DO;                                                          0551 01354000
*       HDRBIT=ON;                  /* HEADR BEING PUT OUT           */ 01355000
         OI    HDRBIT,B'00010000'                                  0552 01356000
*       BUFFER(31:(LENGTH(DUMMYHDR)+30))=DUMMYHDR;/* PLACE HDR INTO     01357000
*                                      BUFFER                        */ 01358000
         L     @14,ADPLBUF(,ABDPLPTR)                              0553 01359000
         MVC   BUFFER+30(70,@14),DUMMYHDR                          0553 01360000
*       CALL PRINT;                 /* PRINT TITLE                   */ 01361000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0554 01362000
         BALR  @14,@15                                             0554 01363000
*       BUFFER(31:(LENGTH(ESAUS)+30))=ESAUS;/* UNDER SCORE           */ 01364000
         L     @14,ADPLBUF(,ABDPLPTR)                              0555 01365000
         MVC   BUFFER+30(60,@14),ESAUS                             0555 01366000
*       CALL PRINT;                 /* PRINT IT                      */ 01367000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0556 01368000
         BALR  @14,@15                                             0556 01369000
*       CALL PRINT;                 /* BLANK                         */ 01370000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0557 01371000
         BALR  @14,@15                                             0557 01372000
*       BUFFER(8:(LENGTH(ESASTART)+7))=ESASTART;/* BEGINNING OF    0558 01373000
*                                      MESSAGE                       */ 01374000
         L     @14,ADPLBUF(,ABDPLPTR)                              0558 01375000
         MVC   BUFFER+7(13,@14),ESASTART                           0558 01376000
*       ADPLFMT2=ADDR(ESAFLAGS);    /* ADDRESS FOR GENING OFFSET     */ 01377000
         L     @14,ESAPTR                                          0559 01378000
