*/********************************************************************/ 01979000
*/* FUNCTION -                                                     0716 01980000
*      ROUTINE TO DYNAMICALLY GENERATE THE OFFSETS TO BE PUT ALONGSIDE  01981000
*      THE CONTROL BLOCK LINE.  IT FIGURES THE OFFSET FROM THE     0716 01982000
*      BEGINNING OF THE CONTROL BLOCK, CONVERTS IT TO PRINTABLE HEX,    01983000
*      PLACES IT INTO A BUFFER AREA AND PUTS THE ADDRESS OF THIS   0716 01984000
*      BUFFER AREA INTO THE PARAMETER LIST TO BE PICKED UP IN THE  0716 01985000
*      FORMAT PATTERNS BY THE FORMAT SERVICE ROUTINE.              0716 01986000
*                                                                  0716 01987000
*   INPUT -                                                        0716 01988000
*      SAVEPTR MUST HAVE THE ADDRESS OF THE BEGINNING OF THE CONTROL    01989000
*      BLOCK, REGISTER 1 POINTS TO THE PARMLIST WHICH CONTAINS THE 0716 01990000
*      CURRENT CONTROL BLOCK ADDRESS IN ADPLFMT2.                  0716 01991000
*                                                                  0716 01992000
*   CALLS -                                                        0716 01993000
*      MAKES NO OUTSIDE CALLS                                      0716 01994000
*                                                                  0716 01995000
*   CALLED BY -                                                    0716 01996000
*      BYTEMODE, EEDBITS, ENTRYRT1, IEAFTEED, IEAFTESA, IEAFTFRR,  0716 01997000
*      IEAFTIHS, IEAFTRTC, IEAFTRT2, IEAFTSCB, IEAFTSDW, RT1BITS,  0716 01998000
*      RT2BITS, RT2SUM, AND SPECIAL                                0716 01999000
*                                                                  0716 02000000
*   RETURNS TO -                                                   0716 02001000
*      CALLER.                                                     0716 02002000
*                                                                  0716 02003000
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE) ------  0716 02004000
*                                                                    */ 02005000
*/********************************************************************/ 02006000
*                                                                  0716 02007000
*   IF SAVEPTR>ADPLFMT2 THEN        /* THIS WILL OCCUR FOR A PREFIX     02008000
*                                      WITH A NEGATIVE OFFSET        */ 02009000
         L     @14,SAVEPTR                                         0716 02010000
         L     @07,ADPLFMT2(,ABDPLPTR)                             0716 02011000
         CR    @14,@07                                             0716 02012000
         BNH   @RF00716                                            0716 02013000
*     DO;                                                          0717 02014000
*       NEGBIT=ON;                  /* INDICATE NEGATIVE OFFSET      */ 02015000
         OI    NEGBIT,B'10000000'                                  0718 02016000
*       CONVRT=SAVEPTR-ADPLFMT2;    /* NEGATIVE OFFSET VALUE         */ 02017000
         L     @04,ADPLFMT2(,ABDPLPTR)                             0719 02018000
         SLR   @14,@04                                             0719 02019000
         LR    CONVRT,@14                                          0719 02020000
*     END;                          /* END NEGATIVE OFFSET           */ 02021000
*   ELSE                            /* POSITIVE OR ZERO OFFSET       */ 02022000
*     DO;                                                          0721 02023000
         B     @RC00716                                            0721 02024000
@RF00716 DS    0H                                                  0722 02025000
*       NEGBIT=OFF;                 /* INDICATE POS VALUE            */ 02026000
         NI    NEGBIT,B'01111111'                                  0722 02027000
*       CONVRT=ADPLFMT2-SAVEPTR;    /* RELATIVE OFFSET IN CONTROL  0723 02028000
*                                      BLOCK                         */ 02029000
         L     CONVRT,ADPLFMT2(,ABDPLPTR)                          0723 02030000
         SL    CONVRT,SAVEPTR                                      0723 02031000
*     END;                          /* END POS VALUE                 */ 02032000
*   DIGIT=BLANKS(1:5);              /* BLANK OUT DIGITS              */ 02033000
@RC00716 MVC   DIGIT(5),BLANKS                                     0725 02034000
*   PACKAREA=CONVRT;                /* PLACE OFFSET INTO CHAR AREA 0726 02035000
*                                      FOR THE UNPACK                */ 02036000
         ST    CONVRT,PACKAREA                                     0726 02037000
*   GARBAGE='04'X;                  /* GARBAGE BYTE SO OFFSET WILL 0727 02038000
*                                      UNPACK OK                     */ 02039000
*                                                                  0727 02040000
         MVI   GARBAGE,X'04'                                       0727 02041000
*   /*****************************************************************/ 02042000
*   /*                                                               */ 02043000
*   /* FOR AN ODD NUMBER OF DIGITS THE OUTPUT SHOULD LOOK LIKE +C OR */ 02044000
*   /* +13C. IF THE VALUE WAS UNPACKED DIRECTLY INTO DIGITS THE      */ 02045000
*   /* OUTPUT WOULD HAVE UNWANTED LEADING ZEROS SUCH AS +0C. TO      */ 02046000
*   /* ELIMINATE THIS(ONLY FOR AN ODD NUMBER OF DIGITS) UNPACK ODD   */ 02047000
*   /* NUMBER OF DIGITS INTO CONVERTD WHERE THE LEADING ZERO WILL BE */ 02048000
*   /* OVERWRITTEN BY THE '+' OR '-' SIGN                            */ 02049000
*   /*                                                               */ 02050000
*   /*****************************************************************/ 02051000
*                                                                  0728 02052000
*   IF CONVRT<TWODGT THEN           /* ONLY 1 DIGIT TO OFFSET        */ 02053000
         CH    CONVRT,@CH00781                                     0728 02054000
         BNL   @RF00728                                            0728 02055000
*     UNPK(CONVERTD(1:3),UNPACK(4:5));/* UNPACK ONLY 1 DIGIT AND   0729 02056000
*                                      DUMMY                         */ 02057000
         UNPK  CONVERTD(3),UNPACK+3(2)                             0729 02058000
*   ELSE                                                           0730 02059000
*     IF CONVRT<THREDGT THEN        /* 2 DIGITS TO OFFSET            */ 02060000
         B     @RC00728                                            0730 02061000
@RF00728 CH    CONVRT,@CH00704                                     0730 02062000
         BNL   @RF00730                                            0730 02063000
*       UNPK(DIGIT(1:3),UNPACK(4:5));/* UNPACK LAST 2 DIGITS & DUMMY */ 02064000
         UNPK  DIGIT(3),UNPACK+3(2)                                0731 02065000
*     ELSE                                                         0732 02066000
*       IF CONVRT<FOURDGT THEN      /* 3 DIGITS TO OFFSET            */ 02067000
         B     @RC00730                                            0732 02068000
@RF00730 CH    CONVRT,@CH03858                                     0732 02069000
         BNL   @RF00732                                            0732 02070000
*         UNPK(CONVERTD(1:5),UNPACK(3:5));/* UNPACK LAST 3 DIGITS &     02071000
*                                      DUMMY                         */ 02072000
         UNPK  CONVERTD(5),UNPACK+2(3)                             0733 02073000
*       ELSE                        /* 4 DIGITS (MAX)                */ 02074000
*         UNPK(DIGIT(1:5),UNPACK(3:5));/* LAST 4 DIGITS & DUMMY BYTE */ 02075000
         B     @RC00732                                            0734 02076000
@RF00732 UNPK  DIGIT(5),UNPACK+2(3)                                0734 02077000
*   TR(DIGIT,TRANS);                /* MAKE PRINTABLE                */ 02078000
@RC00732 DS    0H                                                  0735 02079000
@RC00730 DS    0H                                                  0735 02080000
@RC00728 TR    DIGIT(5),TRANS                                      0735 02081000
*   IF NEGBIT=ON THEN               /* WAS A NEGATIVE OFFSET         */ 02082000
         TM    NEGBIT,B'10000000'                                  0736 02083000
         BNO   @RF00736                                            0736 02084000
*     PLUS=NEGCHAR;                 /* MINUS SIGN                    */ 02085000
         MVI   PLUS,C'-'                                           0737 02086000
*   ELSE                            /* POSITIVE OFFSET               */ 02087000
*     PLUS=PLUSCHAR;                /* MOVE IN PLUS SIGN             */ 02088000
         B     @RC00736                                            0738 02089000
@RF00736 MVI   PLUS,C'+'                                           0738 02090000
*   ADPLFMT1=ADDR(CONVERTD);        /* SET UP FOR FORMAT             */ 02091000
@RC00736 LA    @14,CONVERTD                                        0739 02092000
         ST    @14,ADPLFMT1(,ABDPLPTR)                             0739 02093000
*   END CONVERT;                                                   0740 02094000
@EL00008 DS    0H                                                  0740 02095000
@EF00008 DS    0H                                                  0740 02096000
@ER00008 L     @14,12(,@13)                                        0740 02097000
         BR    @14                                                 0740 02098000
*FMTRTN:                                                           0741 02099000
*   PROC OPTIONS(SAVE(14));                                        0741 02100000
*                                                                  0741 02101000
FMTRTN   ST    @14,@SA00009                                        0741 02102000
*/****************************************************************** */ 02103000
*/* FUNCTION                                                       0742 02104000
*       ROUTINE TO SET UP CALLING OF THE FORMAT SERVICE ROUTINE PASSED  02105000
*     IN THE PARMLIST AND CHECKS RETURN CODE.  IF A RETURN CODE IS BAD  02106000
*     IT TAKES THE MESSAGE WHOSE ADDRESS IS IN ADPLCOM1. AND PUTS IT    02107000
*     IN THE BUFFER AND PRINTS IT. IT SETS THE ERRSW TO INDICATE BAD    02108000
*     RETURN CODE AND RETURNS. IF THE RETURN CODE WAS GOOD, IT PRINTS   02109000
*     THE FORMATTED LINE.                                          0742 02110000
*                                                                  0742 02111000
*   CALLS-                                                         0742 02112000
*       CALLS FORMAT AND PRINT                                     0742 02113000
*                                                                  0742 02114000
*   INPUT -                                                        0742 02115000
*     LINEPTR(REGISTER 0) HAS THE ADDRESS OF THE FORMAT PATTERN TO BE   02116000
*    PASSED TO THE FORMAT SERVICE ROUTINE. ADPLFMT2 CONTAINS THE CURREN 02117000
*    ADDRESS OF THE CONTROL BLOCK TO BE FORMATTED AND ,IN MOST CASES,   02118000
*    ADPLFMT1 CONTAINS THE ADDRESS OF THE FIELD 'CONVERTD' WHICH IS     02119000
*    THE EBCDIC REPRESENTATION OF THE OFFSET TO BE PLACED ALONG SIDE    02120000
*    THE OUTPUT LINE.                                              0742 02121000
*                                                                  0742 02122000
*   CALLED BY -                                                    0742 02123000
*     IEAFTEED,IEAFTFRR,IEAFTIHS,IEAFTRTC,IEAFTRT2,IEAFTSCB,       0742 02124000
*     IEAFTSDW, RT2SUM,AND SPECIAL                                 0742 02125000
*                                                                  0742 02126000
*   RETURNS CONTROL TO -                                           0742 02127000
*     CALLER WITH RETURN CODE RECIEVED FROM FORMAT                 0742 02128000
*     SERVICE ROUTINE                                              0742 02129000
*                                                                  0742 02130000
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE) ------  0742 02131000
*                                                                    */ 02132000
*/****************************************************************** */ 02133000
*                                                                  0742 02134000
*   IF ADPLSNPR=OFF THEN            /* IF SNAP IS THE CALLER, AN   0742 02135000
*                                      ACCESS ERROR CAN BE EXPECTED  */ 02136000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0742 02137000
         BNZ   @RF00742                                            0742 02138000
*     DO;                                                          0743 02139000
*       RTYADR=ADDR(FMTRTY);        /* WHERE TO RETRY                */ 02140000
         L     @14,ESTAPTR                                         0744 02141000
         LA    @07,FMTRTY                                          0744 02142000
         ST    @07,RTYADR(,@14)                                    0744 02143000
*       ERREXP=ON;                  /* INDICATE AN ERROR IS EXPECTED */ 02144000
         OI    ERREXP(@14),B'10000000'                             0745 02145000
*     END;                          /* END SNAP IS THE CALLER        */ 02146000
*   ELSE                                                           0747 02147000
*     ;                             /* SNAP WAS NOT INVOKER          */ 02148000
@RF00742 DS    0H                                                  0748 02149000
*   CALL FORMAT;                    /* FORMAT ROUTINE PASSED BY    0748 02150000
*                                      CALLER                        */ 02151000
         L     @15,ADPLFRMT(,ABDPLPTR)                             0748 02152000
         BALR  @14,@15                                             0748 02153000
*   ERREXP=OFF;                     /* ERROR IS NO LONGER EXPECTED   */ 02154000
         L     @14,ESTAPTR                                         0749 02155000
         NI    ERREXP(@14),B'01111111'                             0749 02156000
*   IF RCREG^=ZERO THEN                                            0750 02157000
         LTR   RCREG,RCREG                                         0750 02158000
         BZ    @RF00750                                            0750 02159000
*     DO;                           /* FORMAT ROUTINE ERROR          */ 02160000
*FMTRTY:                            /* RETRY POINT FROM ERROR        */ 02161000
*       ERRSW=ON;                   /* SET ERROR INDICATOR           */ 02162000
FMTRTY   OI    ERRSW,B'00100000'                                   0752 02163000
*       CALL PRINT;                 /* SPACE A LINE                  */ 02164000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0753 02165000
         BALR  @14,@15                                             0753 02166000
*       BUFFER=MSG;                 /* ERROR MSG PASSED BY CALLER  0754 02167000
*                                      WHOSE ADDR IS PLACED IN     0754 02168000
*                                      ADPLCOM1                      */ 02169000
         L     @14,ADPLBUF(,ABDPLPTR)                              0754 02170000
         L     @07,ADPLCOM1(,ABDPLPTR)                             0754 02171000
         MVC   BUFFER(120,@14),MSG(@07)                            0754 02172000
*       RCREG=4;                    /* INDICATE BAD RETURN CODE FROM    02173000
*                                      FORMATTER                     */ 02174000
         LA    RCREG,4                                             0755 02175000
*     END;                          /* END FORMAT ROUTINE ERROR      */ 02176000
*   ELSE                                                           0757 02177000
*     ;                             /* FORMATTED LINE IS IN BUFFER   */ 02178000
@RF00750 DS    0H                                                  0758 02179000
*   RCCODE=RCREG;                   /* SAVE RETURN CODE FROM FORMAT     02180000
*                                      SERVICE ROUTINE               */ 02181000
         ST    RCREG,RCCODE                                        0758 02182000
*   CALL PRINT;                     /* PRINT THE ERROR MSG OR      0759 02183000
*                                      FORMATTED LINE                */ 02184000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0759 02185000
         BALR  @14,@15                                             0759 02186000
*   RETURN CODE(RCCODE);                                           0760 02187000
         L     @15,RCCODE                                          0760 02188000
@EL00009 DS    0H                                                  0760 02189000
@EF00009 DS    0H                                                  0760 02190000
@ER00009 L     @14,@SA00009                                        0760 02191000
         BR    @14                                                 0760 02192000
*   END FMTRTN;                                                    0761 02193000
*ANALYZE:                                                          0762 02194000
*   PROC OPTIONS(SAVE(14));                                        0762 02195000
*                                                                  0762 02196000
ANALYZE  ST    @14,@SA00010                                        0762 02197000
*/****************************************************************** */ 02198000
*/* FUNCTION                                                       0763 02199000
*     ROUTINE TO ANALYZE THE BITS IN THE BYTE PASSED AND PRINT OUT 0763 02200000
*     THE MESSAGE THAT CORRESPONDS TO THE BITS THAT ARE ON. IF A BLANK  02201000
*     LINE IS PASSED IN THE MESSAGE ARRAY (INDICATING A RESERVED BIT)   02202000
*     NO OUTPUT WILL CORRESPOND TO THAT BIT.                       0763 02203000
*                                                                  0763 02204000
*   INPUT-                                                         0763 02205000
*     AN ARRAY (SUMMARY) POINTED TO BY THE PARMLIST FIELD          0763 02206000
*     ADPLCOM2 WHICH CONTAINS THE ADDRESS OF THE BYTE TO BE        0763 02207000
*     ANALYZED, THE ADDRESS OF THE ARRAY OF CORRESPONDING          0763 02208000
*     MESSAGES, THE COLUMN NUMBER THE MESSAGE IS TO BEGIN IN,      0763 02209000
*     AND THE LENGTH OF THE MESSAGE ARRAY PASSED.                  0763 02210000
*                                                                  0763 02211000
*   NOTE- COLNUM + MSGLNGTH MUST BE LESS THAN OR EQUAL TO 120 BYTES     02212000
*         WHICH IS THE OUTPUT BUFFER LENGTH.                       0763 02213000
*                                                                  0763 02214000
*                                                                  0763 02215000
*   CALLS-                                                         0763 02216000
*       CALLS PRINT                                                0763 02217000
*                                                                  0763 02218000
*   CALLED BY -                                                    0763 02219000
*       BYTEMODE,EEDBITS,IEAFTESA,IEAFTSCB,RT2BITS                 0763 02220000
*                                                                  0763 02221000
*   RETURNS CONTROL TO -                                           0763 02222000
*       CALLER WITHOUT SETTING A RETURN CODE                       0763 02223000
*                                                                  0763 02224000
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE)-------  0763 02225000
*                                                                    */ 02226000
*/****************************************************************** */ 02227000
*                                                                  0763 02228000
*   DO INDEX=1 TO BYTELNTH;         /* LOOP TO FIND WHICH BITS ARE ON*/ 02229000
         LA    INDEX,1                                             0763 02230000
@DL00763 DS    0H                                                  0764 02231000
*     LNGTHEQU=((INDEX-1)*(MSGLNGTH)+1);/* OFFSET INTO MSGARY OF   0764 02232000
*                                      CORRESPONDING MESSAGE         */ 02233000
         L     @14,MSGLNGTH                                        0764 02234000
         LR    @03,INDEX                                           0764 02235000
         BCTR  @03,0                                               0764 02236000
         MR    @02,@14                                             0764 02237000
         AH    @03,@CH00042                                        0764 02238000
         LR    LNGTHEQU,@03                                        0764 02239000
*     IF(BITSTR&TESTBITS(INDEX))^=ZEROBYTE&/* FOUND A BIT ON         */ 02240000
*         MSGARY(LNGTHEQU:MSGLNGTH+LNGTHEQU-1)^=BLANKS(1:MSGLNGTH) THEN 02241000
         L     @04,BYTEADDR                                        0765 02242000
         SLR   @03,@03                                             0765 02243000
         IC    @03,BITSTR(,@04)                                    0765 02244000
         SLR   @04,@04                                             0765 02245000
         IC    @04,TESTBITS-1(INDEX)                               0765 02246000
         NR    @03,@04                                             0765 02247000
         SLR   @04,@04                                             0765 02248000
         IC    @04,ZEROBYTE                                        0765 02249000
         CR    @03,@04                                             0765 02250000
         BE    @RF00765                                            0765 02251000
         L     @04,MSGADDR                                         0765 02252000
         LR    @03,@14                                             0765 02253000
         BCTR  @03,0                                               0765 02254000
         LR    @02,@04                                             0765 02255000
         ALR   @02,LNGTHEQU                                        0765 02256000
         BCTR  @02,0                                               0765 02257000
         EX    @03,@SC03895                                        0765 02258000
         BE    @RF00765                                            0765 02259000
*       DO;                         /* CORRESPONDING MESSAGE WAS NOT    02260000
*                                      BLANK(RESERVED)               */ 02261000
*         BUFFER(COLNUM:(MSGLNGTH+COLNUM-1))=MSGARY(LNGTHEQU:LNGTHEQU+  02262000
*             MSGLNGTH-1);          /* PLACE MSG IN BUFFER           */ 02263000
         L     @03,COLNUM                                          0767 02264000
         L     @02,ADPLBUF(,ABDPLPTR)                              0767 02265000
         ALR   @02,@03                                             0767 02266000
         BCTR  @02,0                                               0767 02267000
         ALR   @14,@03                                             0767 02268000
         SLR   @14,@03                                             0767 02269000
         BCTR  @14,0                                               0767 02270000
         ALR   @04,LNGTHEQU                                        0767 02271000
         BCTR  @04,0                                               0767 02272000
         EX    @14,@SM03897                                        0767 02273000
*         CALL PRINT;               /* PRINT IT                      */ 02274000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0768 02275000
         BALR  @14,@15                                             0768 02276000
*       END;                        /* END FOUND THE MESSAGE         */ 02277000
*     ELSE                                                         0770 02278000
*       ;                           /* EITHER BIT NOT ON OR RESERVED    02279000
*                                      BIT-NO MESSAGE                */ 02280000
@RF00765 DS    0H                                                  0771 02281000
*   END;                            /* END SEARCH LOOP               */ 02282000
         AH    INDEX,@CH00042                                      0771 02283000
         CH    INDEX,@CH00087                                      0771 02284000
         BNH   @DL00763                                            0771 02285000
*   END ANALYZE;                                                   0772 02286000
@EL00010 DS    0H                                                  0772 02287000
@EF00010 DS    0H                                                  0772 02288000
@ER00010 L     @14,@SA00010                                        0772 02289000
         BR    @14                                                 0772 02290000
*ACCESRTN:                                                         0773 02291000
*   PROC OPTIONS(SAVE(14));                                        0773 02292000
*                                                                  0773 02293000
ACCESRTN ST    @14,@SA00011                                        0773 02294000
*/********************************************************************/ 02295000
*/* FUNCTION-                                                      0774 02296000
*       ROUTINE WHICH CALLS THE ACCESS SERVICE ROUTINE AND MOVES THE    02297000
*     VALUE RETURNED IN R0 INTO DATAPTR.                           0774 02298000
*                                                                  0774 02299000
*                                                                  0774 02300000
*   INPUT -                                                        0774 02301000
*         VADR(REGISTER 0) CONTAINS THE VIRTUAL ADDRESS OF THE FIELD    02302000
*         TO BE ACCESSED.                                          0774 02303000
*                                                                  0774 02304000
*    CALLS-                                                        0774 02305000
*         ACCESS SERVICE ROUTINE PASSED IN PARMLIST                0774 02306000
*                                                                  0774 02307000
*    CALLED BY -                                                   0774 02308000
*         EEDBITS, IEAFTEED, IEAFTFRR, IEAFTRTC, IEAFTRT2,         0774 02309000
*         IEAFTSCB, RT1BITS, AND RT2BITS                           0774 02310000
*                                                                  0774 02311000
*    RETURNS TO -                                                  0774 02312000
*         CALLER WITH THE RETURN CODE SET BY THE ACCESS            0774 02313000
*         SERVICE ROUTINE.                                         0774 02314000
*                                                                  0774 02315000
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE) ------  0774 02316000
*                                                                    */ 02317000
*/********************************************************************/ 02318000
*                                                                  0774 02319000
*   IF ADPLSNPR=OFF THEN            /* SNAP WAS INVOKER              */ 02320000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0774 02321000
         BNZ   @RF00774                                            0774 02322000
*     DO;                                                          0775 02323000
*       RTYADR=ADDR(RETRY);         /* RETRY ADDR FOR ESTAE IN THE 0776 02324000
*                                      CASE OF EXPECTED ERRORS       */ 02325000
         L     @14,ESTAPTR                                         0776 02326000
         LA    @07,RETRY                                           0776 02327000
         ST    @07,RTYADR(,@14)                                    0776 02328000
*       DATAPTR=VADR;               /* USED TO BASE THE STORAGE ON   */ 02329000
         LR    DATAPTR,VADR                                        0777 02330000
*       RCREG=ZERO;                 /* SUCCESS UNLESS AN ABEND OCCURS   02331000
*                                      DURING THE MOVE INTO A BUFFER */ 02332000
         SLR   RCREG,RCREG                                         0778 02333000
*       ERREXP=ON;                  /* AN ERROR IS EXPECTED          */ 02334000
         OI    ERREXP(@14),B'10000000'                             0779 02335000
*       VERIFY=DATADWRD;            /* MOVE A DOUBLE WORD INTO     0780 02336000
*                                      INTERNAL BUFFER               */ 02337000
*                                                                  0780 02338000
         MVC   VERIFY(8),DATADWRD(DATAPTR)                         0780 02339000
*       /*************************************************************/ 02340000
*       /*                                                           */ 02341000
*       /* THIS MOVE WILL CAUSE AN ABEND IF THE DUMP DOES NOT CONTAIN*/ 02342000
*       /* THE AREA REFERENCED OR IF THE AREA IS PROTECTED. IF AN    */ 02343000
*       /* ABEND OCCURS, THE ESTAE WILL GET CONTROL AND RETRY AT THE */ 02344000
*       /* POINT WHICH SETS A BAD RETURN CODE AND RETURNS TO THE     */ 02345000
*       /* CALLER. THIS THEN SIMULATES THE PRINT DUMP ACCESS ROUTINE.*/ 02346000
*       /* IF THE AREA ASKED FOR IS IN THE DUMP, A DOUBLE WORD IS    */ 02347000
*       /* MOVED INTO AN INTERNAL BUFFER TO PREVENT AN UNEXPECTED    */ 02348000
*       /* ERROR WHEN REFERENCING THE AREA AGAIN                     */ 02349000
*       /*                                                           */ 02350000
*       /*************************************************************/ 02351000
*                                                                  0781 02352000
*       ERREXP=OFF;                 /* AN ERROR IS NO LONGER EXPECTED*/ 02353000
         NI    ERREXP(@14),B'01111111'                             0781 02354000
*       DATAPTR=ADDR(VERIFY);       /* REBASE DATA OFF INTERNAL    0782 02355000
*                                      BUFFER                        */ 02356000
         LA    DATAPTR,VERIFY                                      0782 02357000
*     END;                          /* END SNAP WAS THE INVOKER      */ 02358000
*   ELSE                            /* CALLER HAS OWN ACCESS ROUTINE    02359000
*                                      LIKE PRINT DUMP               */ 02360000
*     DO;                                                          0784 02361000
         B     @RC00774                                            0784 02362000
@RF00774 DS    0H                                                  0785 02363000
*       CALL ACCESS;                /* ACCESS SERVICE ROUTINE PASSED    02364000
*                                      BY CALLER                     */ 02365000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0785 02366000
         BALR  @14,@15                                             0785 02367000
*       IF RCREG=ZERO THEN                                         0786 02368000
         LTR   RCREG,RCREG                                         0786 02369000
         BNZ   @RF00786                                            0786 02370000
*         DATAPTR=VADR;             /* SAVE PTR RETURNED BY ACCESS 0787 02371000
*                                      SERVICE ROUTINE               */ 02372000
         LR    DATAPTR,VADR                                        0787 02373000
*       ELSE                        /* BAD RETURN CODE               */ 02374000
*RETRY:                             /* RETRY ADDR FOR THE ESTAE      */ 02375000
*         RCREG=4;                  /* SET A BAD RETURN CODE         */ 02376000
         B     @RC00786                                            0788 02377000
@RF00786 DS    0H                                                  0788 02378000
RETRY    LA    RCREG,4                                             0788 02379000
*     END;                          /* END CALLER WAS NOT SNAP       */ 02380000
@RC00786 DS    0H                                                  0790 02381000
*   RCCODE=RCREG;                   /* SAVE RETURN CODE FROM ACCESS     02382000
*                                      SERVICE RTN                   */ 02383000
@RC00774 ST    RCREG,RCCODE                                        0790 02384000
*   RETURN CODE(RCCODE);                                           0791 02385000
         L     @07,RCCODE                                          0791 02386000
         LR    @15,@07                                             0791 02387000
@EL00011 DS    0H                                                  0791 02388000
@EF00011 DS    0H                                                  0791 02389000
@ER00011 L     @14,@SA00011                                        0791 02390000
