*       ADPLCOM2=ADDR(SUMMARY);     /* SUMMARY PTR IN PARMLIST       */ 01381000
         LA    @14,SUMMARY                                         0560 01382000
         ST    @14,ADPLCOM2(,ABDPLPTR)                             0560 01383000
*       BYTEADDR=ADDR(DATABYTE);    /* ADDR OF BYTE TO BE RESOLVED   */ 01384000
         ST    DATAPTR,BYTEADDR                                    0561 01385000
*       MSGADDR=ADDR(FLAGSMSG);     /* MESSAGE ARRAY                 */ 01386000
         LA    @14,FLAGSMSG                                        0562 01387000
