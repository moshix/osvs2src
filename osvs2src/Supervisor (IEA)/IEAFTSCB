*                                                                    */ 01227000
*/****************************************************************** */ 01228000
*                                                                  0415 01229000
*   DO INDEX=1 TO BYTELNTH;         /* LOOP TO FIND WHICH BITS ARE ON*/ 01230000
         LA    INDEX,1                                             0415 01231000
@DL00415 DS    0H                                                  0416 01232000
*     LNGTHEQU=((INDEX-1)*(MSGLNGTH)+1);/* OFFSET INTO MSGARY OF   0416 01233000
*                                      CORRESPONDING MESSAGE         */ 01234000
         L     @14,MSGLNGTH                                        0416 01235000
         LR    @03,INDEX                                           0416 01236000
         BCTR  @03,0                                               0416 01237000
         MR    @02,@14                                             0416 01238000
         AH    @03,@CH00062                                        0416 01239000
         LR    LNGTHEQU,@03                                        0416 01240000
*     IF(BITSTR&TESTBITS(INDEX))^=ZEROBYTE&/* FOUND A BIT ON         */ 01241000
*         MSGARY(LNGTHEQU:MSGLNGTH+LNGTHEQU-1)^=BLANKS(1:MSGLNGTH) THEN 01242000
         L     @04,BYTEADDR                                        0417 01243000
         SLR   @03,@03                                             0417 01244000
         IC    @03,BITSTR(,@04)                                    0417 01245000
         SLR   @04,@04                                             0417 01246000
         IC    @04,TESTBITS-1(INDEX)                               0417 01247000
         NR    @03,@04                                             0417 01248000
         SLR   @04,@04                                             0417 01249000
         IC    @04,ZEROBYTE                                        0417 01250000
         CR    @03,@04                                             0417 01251000
         BE    @RF00417                                            0417 01252000
         L     @04,MSGADDR                                         0417 01253000
         LR    @03,@14                                             0417 01254000
         BCTR  @03,0                                               0417 01255000
         LR    @02,@04                                             0417 01256000
