*/********************************************************************/ 01296000
*/* FUNCTION-                                                      0426 01297000
*       ROUTINE WHICH CALLS THE ACCESS SERVICE ROUTINE AND MOVES THE    01298000
*     VALUE RETURNED IN R0 INTO DATAPTR.                           0426 01299000
*                                                                  0426 01300000
*                                                                  0426 01301000
*   INPUT -                                                        0426 01302000
*         VADR(REGISTER 0) CONTAINS THE VIRTUAL ADDRESS OF THE FIELD    01303000
*         TO BE ACCESSED.                                          0426 01304000
*                                                                  0426 01305000
*    CALLS-                                                        0426 01306000
*         ACCESS SERVICE ROUTINE PASSED IN PARMLIST                0426 01307000
*                                                                  0426 01308000
*    CALLED BY -                                                   0426 01309000
*         EEDBITS, IEAFTEED, IEAFTFRR, IEAFTRTC, IEAFTRT2,         0426 01310000
*         IEAFTSCB, RT1BITS, AND RT2BITS                           0426 01311000
*                                                                  0426 01312000
*    RETURNS TO -                                                  0426 01313000
*         CALLER WITH THE RETURN CODE SET BY THE ACCESS            0426 01314000
*         SERVICE ROUTINE.                                         0426 01315000
*                                                                  0426 01316000
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE) ------  0426 01317000
*                                                                    */ 01318000
*/********************************************************************/ 01319000
*                                                                  0426 01320000
*   IF ADPLSNPR=OFF THEN            /* SNAP WAS INVOKER              */ 01321000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0426 01322000
         BNZ   @RF00426                                            0426 01323000
*     DO;                                                          0427 01324000
*       RTYADR=ADDR(RETRY);         /* RETRY ADDR FOR ESTAE IN THE 0428 01325000
*                                      CASE OF EXPECTED ERRORS       */ 01326000
         LA    @14,RETRY                                           0428 01327000
         ST    @14,RTYADR(,ESTAPTR)                                0428 01328000
*       DATAPTR=VADR;               /* USED TO BASE THE STORAGE ON   */ 01329000
         LR    DATAPTR,VADR                                        0429 01330000
*       RCREG=ZERO;                 /* SUCCESS UNLESS AN ABEND OCCURS   01331000
*                                      DURING THE MOVE INTO A BUFFER */ 01332000
         SLR   RCREG,RCREG                                         0430 01333000
*       ERREXP=ON;                  /* AN ERROR IS EXPECTED          */ 01334000
         OI    ERREXP(ESTAPTR),B'10000000'                         0431 01335000
*       VERIFY=DATADWRD;            /* MOVE A DOUBLE WORD INTO     0432 01336000
*                                      INTERNAL BUFFER               */ 01337000
*                                                                  0432 01338000
         MVC   VERIFY(8),DATADWRD(DATAPTR)                         0432 01339000
*       /*************************************************************/ 01340000
*       /*                                                           */ 01341000
*       /* THIS MOVE WILL CAUSE AN ABEND IF THE DUMP DOES NOT CONTAIN*/ 01342000
*       /* THE AREA REFERENCED OR IF THE AREA IS PROTECTED. IF AN    */ 01343000
*       /* ABEND OCCURS, THE ESTAE WILL GET CONTROL AND RETRY AT THE */ 01344000
*       /* POINT WHICH SETS A BAD RETURN CODE AND RETURNS TO THE     */ 01345000
*       /* CALLER. THIS THEN SIMULATES THE PRINT DUMP ACCESS ROUTINE.*/ 01346000
*       /* IF THE AREA ASKED FOR IS IN THE DUMP, A DOUBLE WORD IS    */ 01347000
*       /* MOVED INTO AN INTERNAL BUFFER TO PREVENT AN UNEXPECTED    */ 01348000
*       /* ERROR WHEN REFERENCING THE AREA AGAIN                     */ 01349000
*       /*                                                           */ 01350000
*       /*************************************************************/ 01351000
*                                                                  0433 01352000
*       ERREXP=OFF;                 /* AN ERROR IS NO LONGER EXPECTED*/ 01353000
         NI    ERREXP(ESTAPTR),B'01111111'                         0433 01354000
*       DATAPTR=ADDR(VERIFY);       /* REBASE DATA OFF INTERNAL    0434 01355000
*                                      BUFFER                        */ 01356000
         LA    DATAPTR,VERIFY                                      0434 01357000
*     END;                          /* END SNAP WAS THE INVOKER      */ 01358000
*   ELSE                            /* CALLER HAS OWN ACCESS ROUTINE    01359000
*                                      LIKE PRINT DUMP               */ 01360000
*     DO;                                                          0436 01361000
         B     @RC00426                                            0436 01362000
@RF00426 DS    0H                                                  0437 01363000
*       CALL ACCESS;                /* ACCESS SERVICE ROUTINE PASSED    01364000
*                                      BY CALLER                     */ 01365000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0437 01366000
         BALR  @14,@15                                             0437 01367000
*       IF RCREG=ZERO THEN                                         0438 01368000
         LTR   RCREG,RCREG                                         0438 01369000
         BNZ   @RF00438                                            0438 01370000
*         DATAPTR=VADR;             /* SAVE PTR RETURNED BY ACCESS 0439 01371000
*                                      SERVICE ROUTINE               */ 01372000
         LR    DATAPTR,VADR                                        0439 01373000
*       ELSE                        /* BAD RETURN CODE               */ 01374000
*RETRY:                             /* RETRY ADDR FOR THE ESTAE      */ 01375000
*         RCREG=4;                  /* SET A BAD RETURN CODE         */ 01376000
         B     @RC00438                                            0440 01377000
@RF00438 DS    0H                                                  0440 01378000
RETRY    LA    RCREG,4                                             0440 01379000
*     END;                          /* END CALLER WAS NOT SNAP       */ 01380000
@RC00438 DS    0H                                                  0442 01381000
*   RCCODE=RCREG;                   /* SAVE RETURN CODE FROM ACCESS     01382000
*                                      SERVICE RTN                   */ 01383000
@RC00426 LR    RCCODE,RCREG                                        0442 01384000
*   RETURN CODE(RCCODE);                                           0443 01385000
