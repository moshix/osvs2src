         ST    @14,LOOPCNTR                                        0289 00682000
@DL00289 TM    ERRSW,B'00100000'                                   0289 00683000
         BNZ   @DC00289                                            0289 00684000
*                 CALL CONVERT;     /* GEN OFFSET                    */ 00685000
         BAL   @14,CONVERT                                         0290 00686000
*                 CALL FMTRTN;      /* FORMAT LINE                   */ 00687000
         BAL   @14,FMTRTN                                          0291 00688000
*               END;                                               0292 00689000
         LA    @14,1                                               0292 00690000
         AL    @14,LOOPCNTR                                        0292 00691000
         ST    @14,LOOPCNTR                                        0292 00692000
         CH    @14,@CH00784                                        0292 00693000
         BNH   @DL00289                                            0292 00694000
@DC00289 DS    0H                                                  0293 00695000
*               CALL PRINT;         /* SECTION SEPARATOR             */ 00696000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0293 00697000
         BALR  @14,@15                                             0293 00698000
*               IF SU33SAVE=OFF THEN/* SU33 NOT IN SYSTEM DUMP       */ 00699000
         TM    SU33SAVE,B'01000000'                                0294 00700000
         BNZ   @RF00294                                            0294 00701000
*                 DO;                                              0295 00702000
*                                                                  0295 00703000
*                   /*************************************************/ 00704000
*                   /*                                               */ 00705000
*                   /* LAST LOGICAL SECTION FOR 3.7 VERSION OR SU17  */ 00706000
*                   /* VERSION OF THE RTCT                           */ 00707000
*                   /*                                               */ 00708000
*                   /*************************************************/ 00709000
*                                                                  0296 00710000
*                   IF ERRSW=OFF THEN/* CONTINUE IF FORMATTED SVC  0296 00711000
*                                      ENTRIES OK                    */ 00712000
         TM    ERRSW,B'00100000'                                   0296 00713000
