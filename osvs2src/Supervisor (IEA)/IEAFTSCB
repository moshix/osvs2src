*               CALL PRINT;         /* BLANK                         */ 00672000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0287 00673000
         BALR  @14,@15                                             0287 00674000
*               LINEPTR=RTCT37NM(ARRAYCNT);/* NEXT LINE PATTERN      */ 00675000
