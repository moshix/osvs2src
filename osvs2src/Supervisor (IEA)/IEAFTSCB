         TITLE 'IEAFTSCB-SCB-RTCT CONTROL BLOCK FORMAT MODULE          *00001000
                        '                                               00002000
IEAFTSCB CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'IEAFTSCB  77.025'                                 0001 00009000
IEAFTRTC DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00216-IEAFTRTC)                                   00013000
         ENTRY IEAFTRTC                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00016000
         BALR  @11,0                                               0001 00017000
@PSTART  LA    @12,4095(,@11)                                      0001 00018000
         USING @PSTART,@11                                         0001 00019000
         USING @PSTART+4095,@12                                    0001 00020000
         L     @00,@SIZDATD                                        0001 00021000
         GETMAIN  R,LV=(0)                                              00022000
         LR    @10,@01                                             0001 00023000
         USING @DATD,@10                                           0001 00024000
         ST    @13,@SA00001+4                                      0001 00025000
         LM    @15,@01,16(@13)                                     0001 00026000
         ST    @10,8(,@13)                                         0001 00027000
         LR    @13,@10                                             0001 00028000
         AH    @15,4(,@15)                                         0001 00029000
         BR    @15                                                 0001 00030000
@EP00001 DS    0H                                                  0002 00031000
*                                                                  0112 00032000
*/********************************************************************/ 00033000
*/* FUNCTION -                                                     0112 00034000
*      ROUTINE WHICH FORMATS THE STAE CONTROL BLOCK.               0112 00035000
*      IT SETS UP THE NEEDED POINTERS TO BE PASSED TO THE FORMAT   0112 00036000
*      ROUTINE SENT IN THE PARAMETER LIST.  FOR ANY ERROR ACCESSING     00037000
*      A NEEDED FIELD IN THE SCB OR FOR NON-ZERO RETURN CODE       0112 00038000
*      FROM THE FORMATTING SERVICE, THE FOLLOWING MESSAGE WILL BE  0112 00039000
*      PRINTED:                                                    0112 00040000
*               *** FORMATTING SERVICE ROUTINE ERROR DURING        0112 00041000
*               SCB FORMATTING(POSSIBLE CAUSE:  DATA NOT IN        0112 00042000
*               DUMP) ***                                          0112 00043000
*      AT THIS POINT PROCESSING WOULD TERMINATE. IF THE SCB IS     0112 00044000
*      FORMATTED WITHOUT ERROR, A BIT SUMMARY OF THE SCB IS PRINTED     00045000
*      USING THE ANALYZE PROC.                                     0112 00046000
*                                                                  0112 00047000
*   INPUT -                                                        0112 00048000
*      REGISTER 1 POINTS TO THE PARMLIST WHICH CONTIANS THE        0112 00049000
*      ADDRESS OF THE CONTROL BLOCK TO BE FORMATTED IN ADPLFMT2.   0112 00050000
*      ADPLFMT1 ALSO DEFINED AS START CONTAINS THE OFFSET TO BE    0112 00051000
*      PLACED ALONG THE FIRST FIELD IN THE CONTROL BLOCK AND       0112 00052000
*      ADPLCOM1 CONTAINS THE ADDRESS OF THE ESTAE PARAMETER LIST.  0112 00053000
*                                                                  0112 00054000
*   CALLS -                                                        0112 00055000
*      CALLS ACCESRTN, ANALYZE, CONVERT, FMTRTN,PRINT              0112 00056000
*                                                                  0112 00057000
*   CALLED BY -                                                    0112 00058000
*      IEAVTFMT TO FORMAT SCB                                      0112 00059000
*                                                                  0112 00060000
*   RETURNS TO -                                                   0112 00061000
*      IEAVTFMT WITH RETURN CODE SET BY ACCESS OR FORMAT           0112 00062000
*      SERVICE ROUTINES                                              */ 00063000
*/********************************************************************/ 00064000
*                                                                  0112 00065000
*   ESTAPTR=ADPLCOM1;               /* SAVE PTR TO ESTAE PARM LIST   */ 00066000
         L     ESTAPTR,ADPLCOM1(,ABDPLPTR)                         0112 00067000
*   STM(VADR,RCREG,LCALREGS);       /* SAVE REGS FOR RETRY           */ 00068000
         STM   VADR,RCREG,LCALREGS(ESTAPTR)                        0113 00069000
*   ADPLCOM1=ADDR(SCBFMT);          /* FORMAT ROUTINE ERROR          */ 00070000
         LA    @14,SCBFMT                                          0114 00071000
         ST    @14,ADPLCOM1(,ABDPLPTR)                             0114 00072000
*   SCBPTR=ADPLFMT2;                /* PTR TO SCB                    */ 00073000
         L     @14,ADPLFMT2(,ABDPLPTR)                             0115 00074000
         ST    @14,SCBPTR                                          0115 00075000
*   ERRSW=OFF;                                                     0116 00076000
         NI    ERRSW,B'11011111'                                   0116 00077000
*   SAVEPTR=ADPLFMT2-START;         /* VALUE FOR OFFSET              */ 00078000
         LR    @04,@14                                             0117 00079000
         SL    @04,START(,ABDPLPTR)                                0117 00080000
         ST    @04,SAVEPTR                                         0117 00081000
*   BUFFER(1:LENGTH(SCBHDR))=SCBHDR;/* TITLE                         */ 00082000
         L     @04,ADPLBUF(,ABDPLPTR)                              0118 00083000
         MVC   BUFFER(55,@04),SCBHDR                               0118 00084000
*   PACKAREA=SCBPTR;                /* MOVE ADDR INTO CHAR AREA      */ 00085000
         ST    @14,PACKAREA                                        0119 00086000
*   GARBAGE='04'X;                  /* GARBAGE BYTE SO ADDR UNPACKS  */ 00087000
         MVI   GARBAGE,X'04'                                       0120 00088000
*   UNPK(ADDRESS,UNPACK);           /* MOVE INTO BUFFER              */ 00089000
         UNPK  ADDRESS(9,@04),UNPACK(5)                            0121 00090000
*   TR(ADDRESS,TRANS);              /* MAKE PRINTABLE                */ 00091000
         TR    ADDRESS(9,@04),TRANS                                0122 00092000
*   CALL PRINT;                     /* PRINT IT                      */ 00093000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0123 00094000
         BALR  @14,@15                                             0123 00095000
*   BUFFER(2:LENGTH(SCBUS)+1)=SCBUS;/* UNDERSCORE                    */ 00096000
         L     @14,ADPLBUF(,ABDPLPTR)                              0124 00097000
         MVC   BUFFER+1(18,@14),SCBUS                              0124 00098000
*   CALL PRINT;                     /* PRINT IT                      */ 00099000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0125 00100000
         BALR  @14,@15                                             0125 00101000
*   CALL PRINT;                     /* BLANK                         */ 00102000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0126 00103000
         BALR  @14,@15                                             0126 00104000
*   ARRAYCNT=1;                     /* ARRAY INDEX                   */ 00105000
         LA    ARRAYCNT,1                                          0127 00106000
*   LINEPTR=SCBNAMES(ARRAYCNT);     /* FORMAT PATTERN                */ 00107000
         LR    @14,ARRAYCNT                                        0128 00108000
         SLA   @14,2                                               0128 00109000
         L     LINEPTR,SCBNAMES-4(@14)                             0128 00110000
*   CALL CONVERT;                   /* GENERATE OFFSET               */ 00111000
         BAL   @14,CONVERT                                         0129 00112000
*   CALL FMTRTN;                    /* FORMAT IT                     */ 00113000
*                                                                  0130 00114000
         BAL   @14,FMTRTN                                          0130 00115000
*   /*****************************************************************/ 00116000
*   /*                                                               */ 00117000
*   /* ANALYZE BITS IF FORMATTER COMPLETED SUCESSFULLY               */ 00118000
*   /*                                                               */ 00119000
*   /*****************************************************************/ 00120000
*                                                                  0131 00121000
*   IF ERRSW=OFF THEN               /* NO FORMAT ERRORS              */ 00122000
         TM    ERRSW,B'00100000'                                   0131 00123000
         BNZ   @RF00131                                            0131 00124000
*     DO;                                                          0132 00125000
*       HDRBIT=OFF;                 /* HEADER INDICATOR              */ 00126000
         NI    HDRBIT,B'11101111'                                  0133 00127000
*       CALL PRINT;                 /* BLANK                         */ 00128000
*                                                                  0134 00129000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0134 00130000
         BALR  @14,@15                                             0134 00131000
*       /*************************************************************/ 00132000
*       /*                                                           */ 00133000
*       /* ANALYZE TYPE OF SCB BYTE                                  */ 00134000
*       /*                                                           */ 00135000
*       /*************************************************************/ 00136000
*                                                                  0135 00137000
*       VADR=ADDR(SCBFLGS1);        /* FIRST FLAG FIELD              */ 00138000
         L     @14,SCBPTR                                          0135 00139000
         LA    VADR,SCBFLGS1(,@14)                                 0135 00140000
*       CALL ACCESRTN;              /* ACCESS IT                     */ 00141000
         BAL   @14,ACCESRTN                                        0136 00142000
*       IF RCREG=ZERO THEN          /* BYTE TO ANALYZE               */ 00143000
         LTR   RCREG,RCREG                                         0137 00144000
         BNZ   @RF00137                                            0137 00145000
*         DO;                                                      0138 00146000
*           HDRBIT=ON;              /* HEADER OUT                    */ 00147000
         OI    HDRBIT,B'00010000'                                  0139 00148000
*           BUFFER(31:(LENGTH(SCBSUM)+30))=SCBSUM;/* SUMMARY HDR     */ 00149000
         L     @14,ADPLBUF(,ABDPLPTR)                              0140 00150000
         MVC   BUFFER+30(20,@14),SCBSUM                            0140 00151000
*           CALL PRINT;             /* PRINT IT                      */ 00152000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0141 00153000
         BALR  @14,@15                                             0141 00154000
*           BUFFER(31:(LENGTH(SCBSUS)+30))=SCBSUS;/* UNDERSCORE      */ 00155000
         L     @14,ADPLBUF(,ABDPLPTR)                              0142 00156000
         MVC   BUFFER+30(20,@14),SCBSUS                            0142 00157000
*           CALL PRINT;             /* PRINT IT                      */ 00158000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0143 00159000
         BALR  @14,@15                                             0143 00160000
*           CALL PRINT;             /* BLANK                         */ 00161000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0144 00162000
         BALR  @14,@15                                             0144 00163000
*           ADPLFMT2=ADDR(SCBFLGS1);                               0145 00164000
*                                                                  0145 00165000
         L     @14,SCBPTR                                          0145 00166000
         LA    @14,SCBFLGS1(,@14)                                  0145 00167000
         ST    @14,ADPLFMT2(,ABDPLPTR)                             0145 00168000
*/*           THE SCB TYPE BYTE IS MORE INVOLVED THAN THE OTHER BYTES   00169000
*             BEING ANALYZED. FOR BIT PATTERNS:                    0146 00170000
*                       1..1 ....  -     ESTAI                     0146 00171000
*                       1..0 ....  -     STAI                      0146 00172000
*                       .1.1 ....  -     ESTAR                     0146 00173000
*                       .1.0 ....  -     STAR                      0146 00174000
*                       00.1 ....  -     ESTAE                     0146 00175000
*                       00.0 ....  -     STAE                      0146 00176000
*                       .... ..10  -     BYPASS I/O INTERVENTION   0146 00177000
*                       .... ..01  -     HALT I/O                  0146 00178000
*                       .... ..00  -     QUIESCE I/O               0146 00179000
*                                                                  0146 00180000
*              FOR ALL OTHER BITS THERE IS ONLY ONE MEANING          */ 00181000
*                                                                  0146 00182000
*           IF(DATABYTE&ESTAMSK)=ESTAMSK THEN/* THE SCB IS OF ESTA 0146 00183000
*                                      TYPE                          */ 00184000
         SLR   @14,@14                                             0146 00185000
         IC    @14,ESTAMSK                                         0146 00186000
         SLR   @09,@09                                             0146 00187000
         IC    @09,DATABYTE(,DATAPTR)                              0146 00188000
         NR    @09,@14                                             0146 00189000
         CR    @09,@14                                             0146 00190000
         BNE   @RF00146                                            0146 00191000
*             DO;                                                  0147 00192000
*               ADPLCOM2=ADDR(SUMMARY);/* SUMMARY PTR IN PARMLIST    */ 00193000
         LA    @14,SUMMARY                                         0148 00194000
         ST    @14,ADPLCOM2(,ABDPLPTR)                             0148 00195000
*               BYTEADDR=ADDR(DATABYTE);/* ADDR OF BYTE TO BE      0149 00196000
*                                      RESOLVED                      */ 00197000
         ST    DATAPTR,BYTEADDR                                    0149 00198000
*               MSGADDR=ADDR(PARMEMSG);/* MESSAGE ARRAY              */ 00199000
         LA    @14,PARMEMSG                                        0150 00200000
         ST    @14,MSGADDR                                         0150 00201000
*               MSGLNGTH=LENGTH(PARMEMSG);/* MSG ARRAY LENGTH        */ 00202000
         MVC   MSGLNGTH(4),@CF01294                                0151 00203000
*               CALL CONVERT;       /* GENERATE OFFSET               */ 00204000
         BAL   @14,CONVERT                                         0152 00205000
*               OFFSET=CONVERTD;    /* PUT OFFSET IN BUFFER          */ 00206000
         L     @14,ADPLBUF(,ABDPLPTR)                              0153 00207000
         MVC   OFFSET(6,@14),CONVERTD                              0153 00208000
*               COLNUM=DEFAULT;     /* WHERE IN BUFFER TO START      */ 00209000
         MVC   COLNUM(4),@CF00060                                  0154 00210000
*               CALL ANALYZE;       /* ANALYZE THE BYTE ANALYZE BYTE */ 00211000
         BAL   @14,ANALYZE                                         0155 00212000
*               IF(DATABYTE&MSKIR)=ESTAMSK THEN/* THE SCB IS ESTAE   */ 00213000
         SLR   @14,@14                                             0156 00214000
         IC    @14,DATABYTE(,DATAPTR)                              0156 00215000
         SLR   @09,@09                                             0156 00216000
         IC    @09,MSKIR                                           0156 00217000
         NR    @14,@09                                             0156 00218000
         SLR   @09,@09                                             0156 00219000
         IC    @09,ESTAMSK                                         0156 00220000
         CR    @14,@09                                             0156 00221000
         BNE   @RF00156                                            0156 00222000
*                 DO;                                              0157 00223000
*                   BUFFER(8:(LENGTH(ESTAMSG)+7))=ESTAMSG;/* SPECIAL    00224000
*                                      MSG FOR ESTAE                 */ 00225000
         L     @14,ADPLBUF(,ABDPLPTR)                              0158 00226000
         MVC   BUFFER+7(18,@14),ESTAMSG                            0158 00227000
*                   CALL PRINT;     /* PRINT IT                      */ 00228000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0159 00229000
         BALR  @14,@15                                             0159 00230000
*                 END;              /* END SPECIAL MSG FOR ESTAE     */ 00231000
*               ELSE                                               0161 00232000
*                 ;                 /* EITHER ESTAI OR ESTAR-ALREADY    00233000
*                                      RESOLVED                      */ 00234000
@RF00156 DS    0H                                                  0162 00235000
*             END;                  /* END OF THE ESTA TYPE          */ 00236000
*           ELSE                    /* SCB OF THE STA TYPE           */ 00237000
*             DO;                                                  0163 00238000
         B     @RC00146                                            0163 00239000
@RF00146 DS    0H                                                  0164 00240000
*               ADPLCOM2=ADDR(SUMMARY);/* SUMMARY PTR IN PARMLIST    */ 00241000
         LA    @14,SUMMARY                                         0164 00242000
         ST    @14,ADPLCOM2(,ABDPLPTR)                             0164 00243000
*               BYTEADDR=ADDR(DATABYTE);/* ADDR OF BYTE TO BE      0165 00244000
*                                      RESOLVED                      */ 00245000
         ST    DATAPTR,BYTEADDR                                    0165 00246000
*               MSGADDR=ADDR(PARMRMSG);/* MESSAGE ARRAY              */ 00247000
         LA    @14,PARMRMSG                                        0166 00248000
         ST    @14,MSGADDR                                         0166 00249000
*               MSGLNGTH=LENGTH(PARMRMSG);/* MSG ARRAY LENGTH        */ 00250000
         MVC   MSGLNGTH(4),@CF01294                                0167 00251000
*               CALL CONVERT;       /* GENERATE OFFSET               */ 00252000
         BAL   @14,CONVERT                                         0168 00253000
*               OFFSET=CONVERTD;    /* PUT OFFSET IN BUFFER          */ 00254000
         L     @14,ADPLBUF(,ABDPLPTR)                              0169 00255000
         MVC   OFFSET(6,@14),CONVERTD                              0169 00256000
*               COLNUM=DEFAULT;     /* WHERE IN BUFFER TO START      */ 00257000
         MVC   COLNUM(4),@CF00060                                  0170 00258000
*               CALL ANALYZE;       /* ANALYZE THE BYTE ANALYZE BYTE */ 00259000
         BAL   @14,ANALYZE                                         0171 00260000
*               IF(DATABYTE&MSKIR)=ZEROBYTE THEN/* THE SCB IS STAE   */ 00261000
         SLR   @14,@14                                             0172 00262000
         IC    @14,DATABYTE(,DATAPTR)                              0172 00263000
         SLR   @09,@09                                             0172 00264000
         IC    @09,MSKIR                                           0172 00265000
         NR    @14,@09                                             0172 00266000
         SLR   @09,@09                                             0172 00267000
         IC    @09,ZEROBYTE                                        0172 00268000
         CR    @14,@09                                             0172 00269000
         BNE   @RF00172                                            0172 00270000
*                 DO;                                              0173 00271000
*                   BUFFER(8:(LENGTH(STAEMSG)+7))=STAEMSG;/* SPECIAL    00272000
*                                      MSG FOR STAE                  */ 00273000
         L     @14,ADPLBUF(,ABDPLPTR)                              0174 00274000
         MVC   BUFFER+7(17,@14),STAEMSG                            0174 00275000
*                   CALL PRINT;     /* PRINT IT                      */ 00276000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0175 00277000
         BALR  @14,@15                                             0175 00278000
*                 END;              /* END SPECIAL MSG FOR STAE      */ 00279000
*               ELSE                                               0177 00280000
*                 ;                 /* EITHER STAI OR STAR-ALREADY 0177 00281000
*                                      RESOLVED                      */ 00282000
@RF00172 DS    0H                                                  0178 00283000
*             END;                  /* END OF THE STA TYPE           */ 00284000
*           IF(DATABYTE&QUIESMSK)=ZEROBYTE THEN/* FOR BOTH TYPES   0179 00285000
*                                      CHECK IF QUIESCE I/O BITS ON  */ 00286000
@RC00146 SLR   @14,@14                                             0179 00287000
         IC    @14,DATABYTE(,DATAPTR)                              0179 00288000
         SLR   @09,@09                                             0179 00289000
         IC    @09,QUIESMSK                                        0179 00290000
         NR    @14,@09                                             0179 00291000
         SLR   @09,@09                                             0179 00292000
         IC    @09,ZEROBYTE                                        0179 00293000
         CR    @14,@09                                             0179 00294000
         BNE   @RF00179                                            0179 00295000
*             DO;                                                  0180 00296000
*               BUFFER(8:(LENGTH(QUIESMSG)+7))=QUIESMSG;/* QUIESCE 0181 00297000
*                                      MESSAGE                       */ 00298000
         L     @14,ADPLBUF(,ABDPLPTR)                              0181 00299000
         MVC   BUFFER+7(11,@14),QUIESMSG                           0181 00300000
*               CALL PRINT;         /* PRINT IT                      */ 00301000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0182 00302000
         BALR  @14,@15                                             0182 00303000
*             END;                  /* END QUIESCE MESSAGE           */ 00304000
*           ELSE                                                   0184 00305000
*             ;                     /* QUIESCE INDICATORS NOT ON     */ 00306000
@RF00179 DS    0H                                                  0185 00307000
*         END;                      /* END BYTE ANALYZED             */ 00308000
*       ELSE                                                       0186 00309000
*         ;                         /* NO BYTE TO BE ANALYZED        */ 00310000
*                                                                  0186 00311000
@RF00137 DS    0H                                                  0187 00312000
*       /*************************************************************/ 00313000
*       /*                                                           */ 00314000
*       /* ANALYZE FLGS2 BYTE                                        */ 00315000
*       /*                                                           */ 00316000
*       /*************************************************************/ 00317000
*                                                                  0187 00318000
*       VADR=ADDR(SCBFLGS2);        /* NEXT BYTE TO BE ANALYZED      */ 00319000
         L     @14,SCBPTR                                          0187 00320000
         LA    VADR,SCBFLGS2(,@14)                                 0187 00321000
*       CALL ACCESRTN;              /* ACCESS IT                     */ 00322000
         BAL   @14,ACCESRTN                                        0188 00323000
*       IF RCREG=ZERO&DATABYTE^=ZEROBYTE THEN/* BYTE TO ANALYZE      */ 00324000
         LTR   RCREG,RCREG                                         0189 00325000
         BNZ   @RF00189                                            0189 00326000
         CLC   DATABYTE(1,DATAPTR),ZEROBYTE                        0189 00327000
         BE    @RF00189                                            0189 00328000
*         DO;                                                      0190 00329000
*           IF HDRBIT=OFF THEN      /* HDR NOT ALREADY PRINTED       */ 00330000
         TM    HDRBIT,B'00010000'                                  0191 00331000
         BNZ   @RF00191                                            0191 00332000
*             DO;                                                  0192 00333000
*               HDRBIT=ON;          /* HEADER OUT                    */ 00334000
         OI    HDRBIT,B'00010000'                                  0193 00335000
*               BUFFER(31:(LENGTH(SCBSUM)+30))=SCBSUM;/* SUMMARY HDR */ 00336000
         L     @14,ADPLBUF(,ABDPLPTR)                              0194 00337000
         MVC   BUFFER+30(20,@14),SCBSUM                            0194 00338000
*               CALL PRINT;         /* PRINT IT                      */ 00339000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0195 00340000
         BALR  @14,@15                                             0195 00341000
*               BUFFER(31:(LENGTH(SCBSUS)+30))=SCBSUS;/* UNDERSCORE  */ 00342000
         L     @14,ADPLBUF(,ABDPLPTR)                              0196 00343000
         MVC   BUFFER+30(20,@14),SCBSUS                            0196 00344000
*               CALL PRINT;         /* PRINT IT                      */ 00345000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0197 00346000
         BALR  @14,@15                                             0197 00347000
*               CALL PRINT;         /* BLANK                         */ 00348000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0198 00349000
         BALR  @14,@15                                             0198 00350000
*             END;                  /* HDR PRINTED                   */ 00351000
*           ELSE                                                   0200 00352000
*             ;                     /* HEADER ALREADY PRINTED        */ 00353000
@RF00191 DS    0H                                                  0201 00354000
*           ADPLFMT2=ADDR(SCBFLGS2);                               0201 00355000
         L     @14,SCBPTR                                          0201 00356000
         LA    @14,SCBFLGS2(,@14)                                  0201 00357000
         ST    @14,ADPLFMT2(,ABDPLPTR)                             0201 00358000
*           ADPLCOM2=ADDR(SUMMARY); /* SUMMARY PTR IN PARMLIST       */ 00359000
         LA    @14,SUMMARY                                         0202 00360000
         ST    @14,ADPLCOM2(,ABDPLPTR)                             0202 00361000
*           BYTEADDR=ADDR(DATABYTE);/* ADDR OF BYTE TO BE RESOLVED   */ 00362000
         ST    DATAPTR,BYTEADDR                                    0203 00363000
*           MSGADDR=ADDR(FLGS2MSG); /* MESSAGE ARRAY                 */ 00364000
         LA    @14,FLGS2MSG                                        0204 00365000
         ST    @14,MSGADDR                                         0204 00366000
*           MSGLNGTH=LENGTH(FLGS2MSG);/* MSG ARRAY LENGTH            */ 00367000
         MVC   MSGLNGTH(4),@CF00071                                0205 00368000
*           CALL CONVERT;           /* GENERATE OFFSET               */ 00369000
         BAL   @14,CONVERT                                         0206 00370000
*           OFFSET=CONVERTD;        /* PUT OFFSET IN BUFFER          */ 00371000
         L     @14,ADPLBUF(,ABDPLPTR)                              0207 00372000
         MVC   OFFSET(6,@14),CONVERTD                              0207 00373000
*           COLNUM=DEFAULT;         /* WHERE IN BUFFER TO START      */ 00374000
         MVC   COLNUM(4),@CF00060                                  0208 00375000
*           CALL ANALYZE;           /* ANALYZE THE BYTE ANALYZE BYTE */ 00376000
         BAL   @14,ANALYZE                                         0209 00377000
*         END;                      /* BYTE DOES NOT NEED ANALYZE    */ 00378000
*       ELSE                                                       0211 00379000
*         ;                         /* NOT ANALYZED                  */ 00380000
@RF00189 DS    0H                                                  0212 00381000
*     END;                          /* END NO FORMAT ERRORS          */ 00382000
*   ELSE                                                           0213 00383000
*     ;                             /* FORMAT ERROR                  */ 00384000
@RF00131 DS    0H                                                  0214 00385000
*   LCALREGS=RTYREGS;               /* RESTORE RETRY REGS OF CALLER  */ 00386000
         MVC   LCALREGS(64,ESTAPTR),RTYREGS(ESTAPTR)               0214 00387000
*   RETURN CODE(RCCODE);            /* DO NOT FALL THROUGH TO RTCT 0215 00388000
*                                      CODE                          */ 00389000
         L     @13,4(,@13)                                         0215 00390000
         L     @00,@SIZDATD                                        0215 00391000
         LR    @01,@10                                             0215 00392000
         FREEMAIN R,LV=(0),A=(1)                                        00393000
         LR    @15,@09                                             0215 00394000
         L     @14,12(,@13)                                        0215 00395000
         LM    @00,@12,20(@13)                                     0215 00396000
         BR    @14                                                 0215 00397000
*IEAFTRTC:                                                         0216 00398000
*   ENTRY;                                                         0216 00399000
*                                                                  0216 00400000
@EP00216 DS    0H                                                  0217 00401000
*/********************************************************************/ 00402000
*/* FUNCTION -                                                     0217 00403000
*      ROUTINE WHICH FORMATS THE RECOVERY TERMINATION CONTROL TABLE.    00404000
*      IT SETS UP THE NEEDED POINTERS TO BE PASSED TO THE FORMAT   0217 00405000
*      ROUTINE SENT IN THE PARAMETER LIST.  FOR ANY ERROR ACCESSING     00406000
*      A NEEDED FIELD IN THE RTCT OR FOR NON-ZERO RETURN CODE      0217 00407000
*      FROM THE FORMATTING SERVICE, THE FOLLOWING MESSAGE WILL BE  0217 00408000
*      PRINTED:                                                    0217 00409000
*               *** FORMATTING SERVICE ROUTINE ERROR DURING        0217 00410000
*               RTCT FORMATTING(POSSIBLE CAUSE:  DATA NOT IN       0217 00411000
*               DUMP) ***                                          0217 00412000
*      AT THIS POINT PROCESSING WOULD TERMINATE.                   0217 00413000
*                                                                  0217 00414000
*   INPUT -                                                        0217 00415000
*      REGISTER 1 POINTS TO THE PARMLIST WHICH CONTAINS THE        0217 00416000
*      ADDRESS OF THE CONTROL BLOCK TO BE FORMATTED IN ADPLFMT2.   0217 00417000
*      ADPLFMT1 ALSO DEFINED AS START, CONTAINS THE OFFSET TO BE   0217 00418000
*      PLACED ALONG SIDE THE FIRST FIELD IN THE CONTROL BLOCK AND  0217 00419000
*      ADPLCOM1 CONTAINS THE ADDRESS OF THE ESTAE PARAMETER LIST.  0217 00420000
*                                                                  0217 00421000
*   CALLS -                                                        0217 00422000
*      CALLS ACCESRTN, CONVERT, FMTRTN, AND PRINT                  0217 00423000
*                                                                  0217 00424000
*   CALLED BY -                                                    0217 00425000
*      IEAVTFMT TO FORMAT RTCT                                     0217 00426000
*                                                                  0217 00427000
*   RETURNS TO -                                                   0217 00428000
*      IEAVTFMT WITH RETURN CODE SET BY ACCESS OR FORMAT           0217 00429000
*      SERVICE ROUTINES                                              */ 00430000
*/********************************************************************/ 00431000
*                                                                  0217 00432000
*   ESTAPTR=ADPLCOM1;               /* SAVE PTR TO ESTAE PARM LIST   */ 00433000
         L     ESTAPTR,ADPLCOM1(,ABDPLPTR)                         0217 00434000
*   STM(VADR,RCREG,LCALREGS);       /* SAVE REGS FOR RETRY           */ 00435000
         STM   VADR,RCREG,LCALREGS(ESTAPTR)                        0218 00436000
*   ADPLCOM1=ADDR(RTCFMT);          /* FORMAT ROTINE ERROR MSG       */ 00437000
         LA    @14,RTCFMT                                          0219 00438000
         ST    @14,ADPLCOM1(,ABDPLPTR)                             0219 00439000
*   RTCTPTR=ADPLFMT2;               /* RTCT ADDRESS                  */ 00440000
         L     @14,ADPLFMT2(,ABDPLPTR)                             0220 00441000
         LR    RTCTPTR,@14                                         0220 00442000
*   SAVEPTR=ADPLFMT2-START;         /* VALUE FOR DYNAMIC OFFSET    0221 00443000
*                                      GENERATOR                     */ 00444000
         SL    @14,START(,ABDPLPTR)                                0221 00445000
         ST    @14,SAVEPTR                                         0221 00446000
*   BITS=BITS&&BITS;                /* ZERO INTERNAL BITS            */ 00447000
*                                                                  0222 00448000
         XC    BITS(1),BITS                                        0222 00449000
*   /*****************************************************************/ 00450000
*   /*                                                               */ 00451000
*   /* IN RELEASE 2 OF MVS, THE POINTER TO THE SUBITS WAS NOT        */ 00452000
*   /* DEFINED. THEREFORE, BEFORE ACCESSING THE SUBIT POINTER, CHECK */ 00453000
*   /* FOR RELEASE 2                                                 */ 00454000
*   /*                                                               */ 00455000
*   /*****************************************************************/ 00456000
*                                                                  0223 00457000
*   VADR=ADDR(CVTNUMB);             /* RELEASE NUMBER                */ 00458000
         L     @14,CVTPTR(,ABDPLPTR)                               0223 00459000
         AL    @14,@CF02061                                        0223 00460000
         LR    VADR,@14                                            0223 00461000
*   CALL ACCESRTN;                  /* ACCESS IT                     */ 00462000
         BAL   @14,ACCESRTN                                        0224 00463000
*   IF RCREG^=ZERO THEN             /* ACCESS ERROR                  */ 00464000
         LTR   RCREG,RCREG                                         0225 00465000
         BZ    @RF00225                                            0225 00466000
*     BUFFER(1:LENGTH(RTCFMT))=RTCFMT;                             0226 00467000
         L     @14,ADPLBUF(,ABDPLPTR)                              0226 00468000
         MVC   BUFFER(120,@14),RTCFMT                              0226 00469000
*   ELSE                            /* FOUND RELEASE NUMBER          */ 00470000
*     DO;                                                          0227 00471000
         B     @RC00225                                            0227 00472000
@RF00225 DS    0H                                                  0228 00473000
*       IF DATAHALF^=REL2 THEN      /* NOT A RELEASE 2 MVS SYSTEM    */ 00474000
         CLC   DATAHALF(2,DATAPTR),REL2                            0228 00475000
         BE    @RF00228                                            0228 00476000
*         DO;                                                      0229 00477000
*                                                                  0229 00478000
*           /*********************************************************/ 00479000
*           /*                                                       */ 00480000
*           /* FIND WHICH SUS ARE ON THE SYSTEM                      */ 00481000
*           /*                                                       */ 00482000
*           /*********************************************************/ 00483000
*                                                                  0230 00484000
*           VADR=ADDR(CVTIHASU);    /* SU BIT PTR                    */ 00485000
         L     @14,CVTPTR(,ABDPLPTR)                               0230 00486000
         LA    VADR,CVTIHASU(,@14)                                 0230 00487000
*           CALL ACCESRTN;          /* ACCESS IT                     */ 00488000
         BAL   @14,ACCESRTN                                        0231 00489000
*           IF RCREG^=ZERO THEN     /* ACCESS ERROR                  */ 00490000
         LTR   RCREG,RCREG                                         0232 00491000
         BZ    @RF00232                                            0232 00492000
*             DO;                                                  0233 00493000
*               BUFFER(1:LENGTH(RTCFMT))=RTCFMT;                   0234 00494000
         L     @14,ADPLBUF(,ABDPLPTR)                              0234 00495000
         MVC   BUFFER(120,@14),RTCFMT                              0234 00496000
*               ERRSW=ON;           /* INDICATE ACCESS ERROR         */ 00497000
         OI    ERRSW,B'00100000'                                   0235 00498000
*             END;                  /* END ACCESS ERROR              */ 00499000
*           ELSE                    /* FOUND SU BIT PTR              */ 00500000
*             DO;                                                  0237 00501000
         B     @RC00232                                            0237 00502000
@RF00232 DS    0H                                                  0238 00503000
*               IF DATAWORD^=ZERO THEN/* SU SYSTEM                   */ 00504000
         L     @14,DATAWORD(,DATAPTR)                              0238 00505000
         LTR   @14,@14                                             0238 00506000
         BZ    @RF00238                                            0238 00507000
*                 DO;                                              0239 00508000
*                   VADR=DATAWORD;  /* SU BITS                       */ 00509000
         LR    VADR,@14                                            0240 00510000
*                   CALL ACCESRTN;  /* ACCESS BITS                   */ 00511000
         BAL   @14,ACCESRTN                                        0241 00512000
*                   IF RCREG^=ZERO THEN/* ACCESS ERROR               */ 00513000
         LTR   RCREG,RCREG                                         0242 00514000
         BZ    @RF00242                                            0242 00515000
*                     DO;                                          0243 00516000
*                       BUFFER(1:LENGTH(RTCFMT))=RTCFMT;           0244 00517000
         L     @14,ADPLBUF(,ABDPLPTR)                              0244 00518000
         MVC   BUFFER(120,@14),RTCFMT                              0244 00519000
*                       ERRSW=ON;   /* ERROR INDICATOR TO STOP     0245 00520000
*                                      PROCESSING                    */ 00521000
         OI    ERRSW,B'00100000'                                   0245 00522000
*                     END;          /* END ACCESS ERROR              */ 00523000
*                   ELSE            /* FOUND SU BIT STRING           */ 00524000
*                     DO;                                          0247 00525000
         B     @RC00242                                            0247 00526000
@RF00242 DS    0H                                                  0248 00527000
*                       IF SU33BIT=ON THEN                         0248 00528000
         TM    SU33BIT(DATAPTR),B'01000000'                        0248 00529000
         BNO   @RF00248                                            0248 00530000
*                         SU33SAVE=ON;/* SU 33 ON SYSTEM             */ 00531000
         OI    SU33SAVE,B'01000000'                                0249 00532000
*                       ELSE                                       0250 00533000
*                         ;         /* SU33 NOT ON SYSTEM            */ 00534000
@RF00248 DS    0H                                                  0251 00535000
*                       IF SU17BIT=ON THEN                         0251 00536000
         TM    SU17BIT(DATAPTR),B'01000000'                        0251 00537000
         BNO   @RF00251                                            0251 00538000
*                         SU17SAVE=ON;/* SU 17 ON SYSTEM             */ 00539000
         OI    SU17SAVE,B'10000000'                                0252 00540000
*                       ELSE                                       0253 00541000
*                         ;         /* SU17 NOT ON SYSTEM            */ 00542000
@RF00251 DS    0H                                                  0254 00543000
*                     END;          /* END SU BIT STRING FOUND       */ 00544000
*                 END;              /* END SU SYSTEM                 */ 00545000
*               ELSE                                               0256 00546000
*                 ;                 /* PRE SU SYSTEM-NO SU BITS TO 0256 00547000
*                                      SET                           */ 00548000
@RF00238 DS    0H                                                  0257 00549000
*             END;                  /* END COULD ACCESS SUBIT POINTER*/ 00550000
*         END;                      /* END NOT A RELEASE 2 MVS SYSTEM*/ 00551000
*       ELSE                                                       0259 00552000
*         ;                         /* IS A RELEASE 2 MVS SYSTEM - NO   00553000
*                                      SUS                           */ 00554000
@RF00228 DS    0H                                                  0260 00555000
*       IF ERRSW=OFF THEN           /* CONTINUE IF FOUND SYSTEM OK   */ 00556000
@RC00228 TM    ERRSW,B'00100000'                                   0260 00557000
         BNZ   @RF00260                                            0260 00558000
*         DO;                                                      0261 00559000
*           BUFFER(1:LENGTH(RTCTHDR))=RTCTHDR;/* TITLE LINE          */ 00560000
         L     @14,ADPLBUF(,ABDPLPTR)                              0262 00561000
         MVC   BUFFER(54,@14),RTCTHDR                              0262 00562000
*           PACKAREA=RTCTPTR;       /* MOVE ADDR INTO CHAR AREA      */ 00563000
         ST    RTCTPTR,PACKAREA                                    0263 00564000
*           GARBAGE='04'X;          /* GARBAGE BYTE SO ADDR UNPACKS  */ 00565000
         MVI   GARBAGE,X'04'                                       0264 00566000
*           UNPK(ADDRESS,UNPACK);   /* MOVE INTO BUFFER              */ 00567000
         UNPK  ADDRESS(9,@14),UNPACK(5)                            0265 00568000
*           TR(ADDRESS,TRANS);      /* MAKE PRINTABLE                */ 00569000
         TR    ADDRESS(9,@14),TRANS                                0266 00570000
*           CALL PRINT;             /* PRINT TITLE                   */ 00571000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0267 00572000
         BALR  @14,@15                                             0267 00573000
*           BUFFER(1:LENGTH(RTCTUS))=RTCTUS;/* UNDERSCORE            */ 00574000
         L     @14,ADPLBUF(,ABDPLPTR)                              0268 00575000
         MVC   BUFFER(5,@14),RTCTUS                                0268 00576000
*           CALL PRINT;             /* PRINT IT                      */ 00577000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0269 00578000
         BALR  @14,@15                                             0269 00579000
*           CALL PRINT;             /* BLANK                         */ 00580000
*                                                                  0270 00581000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0270 00582000
         BALR  @14,@15                                             0270 00583000
*           /*********************************************************/ 00584000
*           /*                                                       */ 00585000
*           /* SU 33 CHANGED THE SIZE AND STRUCTURE OF THE RTCT, SO  */ 00586000
*           /* AN SU33 SYSTEM DUMP MUST BE FORMATTED DIFFERENTLY THAN*/ 00587000
*           /* A SYSTEM WITHOUT SU33                                 */ 00588000
*           /*                                                       */ 00589000
*           /*********************************************************/ 00590000
*                                                                  0271 00591000
*           IF SU33SAVE=ON THEN     /* SU 33 IS ON DUMPED SYSTEM - 0271 00592000
*                                      RTCT CHANGED WITH THIS SU     */ 00593000
*                                                                  0271 00594000
         TM    SU33SAVE,B'01000000'                                0271 00595000
         BNO   @RF00271                                            0271 00596000
*             /*******************************************************/ 00597000
*             /*                                                     */ 00598000
*             /* FIRST LOGICAL SECTION OF RTCT WITH SU33 ON THE      */ 00599000
*             /* SYSTEM                                              */ 00600000
*             /*                                                     */ 00601000
*             /*******************************************************/ 00602000
*                                                                  0272 00603000
*             DO ARRAYCNT=1 TO RTSUMAX1 WHILE(ERRSW=OFF);/* FIRST  0272 00604000
*                                      SECTION OF OUTPUT             */ 00605000
         LA    ARRAYCNT,1                                          0272 00606000
@DL00272 TM    ERRSW,B'00100000'                                   0272 00607000
         BNZ   @DC00272                                            0272 00608000
*               LINEPTR=RTCTSUNM(ARRAYCNT);/* PTR TO FORMAT PATTERN  */ 00609000
         LR    @14,ARRAYCNT                                        0273 00610000
         SLA   @14,2                                               0273 00611000
         L     LINEPTR,RTCTSUNM-4(@14)                             0273 00612000
*               CALL CONVERT;       /* GENERATE OFFSET               */ 00613000
         BAL   @14,CONVERT                                         0274 00614000
*               CALL FMTRTN;        /* FORMAT LINE OF OUTPUT         */ 00615000
         BAL   @14,FMTRTN                                          0275 00616000
*             END;                  /* END FIRST SECTION LOOP        */ 00617000
         AH    ARRAYCNT,@CH00062                                   0276 00618000
         CH    ARRAYCNT,@CH00074                                   0276 00619000
         BNH   @DL00272                                            0276 00620000
@DC00272 DS    0H                                                  0277 00621000
*           ELSE                    /* NOT AN SU33 SYSTEM DUMP       */ 00622000
*                                                                  0277 00623000
*             /*******************************************************/ 00624000
*             /*                                                     */ 00625000
*             /* FIRST LOGICAL SECTION OF RTCT- SU33 NOT IN DUMP     */ 00626000
*             /* SYSTEM                                              */ 00627000
*             /*                                                     */ 00628000
*             /*******************************************************/ 00629000
*                                                                  0277 00630000
*             DO ARRAYCNT=1 TO RT37MAX1 WHILE(ERRSW=OFF);/* FIRST  0277 00631000
*                                      SECTION                       */ 00632000
         B     @RC00271                                            0277 00633000
@RF00271 LA    ARRAYCNT,1                                          0277 00634000
@DL00277 TM    ERRSW,B'00100000'                                   0277 00635000
         BNZ   @DC00277                                            0277 00636000
*               LINEPTR=RTCT37NM(ARRAYCNT);/* PTR TO FORMAT PATTERN  */ 00637000
         LR    @14,ARRAYCNT                                        0278 00638000
         SLA   @14,2                                               0278 00639000
         L     LINEPTR,RTCT37NM-4(@14)                             0278 00640000
*               CALL CONVERT;       /* GENERATE OFFSET               */ 00641000
         BAL   @14,CONVERT                                         0279 00642000
*               CALL FMTRTN;        /* FORMAT LINE OF OUTPUT         */ 00643000
         BAL   @14,FMTRTN                                          0280 00644000
*             END;                  /* END FIRST SECTION LOOP        */ 00645000
*                                                                  0281 00646000
         AH    ARRAYCNT,@CH00062                                   0281 00647000
         CH    ARRAYCNT,@CH00074                                   0281 00648000
         BNH   @DL00277                                            0281 00649000
@DC00277 DS    0H                                                  0282 00650000
*           /*********************************************************/ 00651000
*           /*                                                       */ 00652000
*           /* SECOND LOGICAL SECTION - SVC ENTRIES COMMON TO SU33   */ 00653000
*           /* RTCT AND 3.7 VERSION OF THE RTCT                      */ 00654000
*           /*                                                       */ 00655000
*           /*********************************************************/ 00656000
*                                                                  0282 00657000
*           IF ERRSW=OFF THEN       /* FIRST SECTION FORMATTED OK    */ 00658000
@RC00271 TM    ERRSW,B'00100000'                                   0282 00659000
         BNZ   @RF00282                                            0282 00660000
*             DO;                                                  0283 00661000
*               CALL PRINT;         /* BLANK TO SEPARATE SECTIONS    */ 00662000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0284 00663000
