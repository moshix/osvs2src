         N     @12,@CF04671                                        0246 00619000
         STH   @12,RTCTSDID(,@14)                                  0246 00620000
*                             END;                                 0247 00621000
*                           ELSE                                   0248 00622000
*                             DO;                                  0248 00623000
         B     @RC00236                                            0248 00624000
@RF00236 DS    0H                                                  0249 00625000
*                               ERRENDMP='1'B;/* IF CALLER DID NOT 0249 00626000
