*/********************************************************************/ 03051000
*/*   THIS SEGMENT CONTAINS THE ESTAE EXIT FOR SVCDUMP.  THIS        */ 03052000
*/*   EXIT IS ESTABLISHED AFTER SVCDUMP SERIALIES ITS PROCCESSING,   */ 03053000
*/*   VALIDATES PARAMETERS AND ESTABLISHES THE DUMP ENVIRONMENT.     */ 03054000
*/*   THIS EXIT IS ACTIVE UNTIL THE DUMP IS COMPLETE. IT IS          */ 03055000
*/*   DELETED IN SDFRRRTN OR SDENDUP.  IT WILL RECEIVE CONTROL       */ 03056000
*/*   WHENEVER THE FRR IS NOT ACTIVE OR WHEN THE FRR FAILS.  IT      */ 03057000
*/*   WILL RETRY IF POSSIBLE, ELSE CLEANUP AND PERCOLATE.            */ 03058000
*/*                                                                  */ 03059000
*/*   INPUT: ERRORWK(ERRWORK), SDWA, SVC DUMP WORKAREA-SDWORK        */ 03060000
*/*                                                                  */ 03061000
*/*   OUTPUT: RETRY OR PERCOLATE                                     */ 03062000
*/********************************************************************/ 03063000
*                                                                  0834 03064000
*SDESTAEX:                                                         0834 03065000
*   ENTRY;                                                         0834 03066000
@EP00834 DS    0H                                                  0835 03067000
*   RFY                                                            0835 03068000
*    (R1,                                                          0835 03069000
*     R10,                                                         0835 03070000
*     R11) RSTD;                                                   0835 03071000
*                                                                  0835 03072000
*   /*****************************************************************/ 03073000
*   /*                                                               */ 03074000
*   /* RESTORE REGISTERS FROM ERRORWK, WORKA WILL CONTAIN FIRST WORD,*/ 03075000
*   /* WORKB WILL CONTAIN SECOND WORD (RETRY ADDRESS), ETC. DETERMINE*/ 03076000
*   /* IF SDWA EXISTS, IF NOT, CLEANUP AND PERCOLATE                 */ 03077000
*   /*                                                               */ 03078000
*   /*****************************************************************/ 03079000
*                                                                  0836 03080000
*   DO;                             /* ENTESTAE (R10,R11,R7)ERREXIT(E   03081000
*                                      STAPERC)PARMREG(R3)           */ 03082000
*                                                                  0836 03083000
*     /***************************************************************/ 03084000
*     /*                                                             */ 03085000
*     /* ENTRY CODE FOR ESTAE ROUTINE. IF RTCA ALLOCATED THEN RESTORE*/ 03086000
*     /* CODE AND DATA REGS FROM START OF USERS PARM LIST, AND       */ 03087000
*     /* ESTABLISH ADDR TO ESTAE SAVE AREA IF REQUIRED. IF NO RTCA   */ 03088000
*     /* AVAILABLE THEN BRANCH TO ERROR CODE AFTER ESTABLISHING      */ 03089000
*     /* ADDRESSABILITY                                              */ 03090000
*     /*                                                             */ 03091000
*     /***************************************************************/ 03092000
*                                                                  0837 03093000
*     GEN(USING *,GPR15P);                                         0837 03094000
         USING *,GPR15P                                                 03095000
*     GEN(CH    GPR00F,*+26);       /* CHECK IF RTCA ALLOC FAILED    */ 03096000
         CH    GPR00F,*+26                                              03097000
*     GEN(BNE   *+24);              /* NO,SAVE RTM REGS              */ 03098000
         BNE   *+24                                                     03099000
*     GEN(DROP  GPR15P);            /* DROP LOCAL ADDR               */ 03100000
         DROP  GPR15P                                                   03101000
*     GEN(LR    R3,GPR02P);         /* PTR TO USER PARM LIST         */ 03102000
         LR    R3,GPR02P                                                03103000
*     GEN(L     R10,00(R3));                                       0842 03104000
         L     R10,00(R3)                                               03105000
*     GEN(L     R11,04(R3));                                       0843 03106000
         L     R11,04(R3)                                               03107000
*     GEN(L     R7,08(R3));                                        0844 03108000
         L     R7,08(R3)                                                03109000
*     GEN(B     ESTAPERC);          /* BRANCH TO ERROR EXIT          */ 03110000
         B     ESTAPERC                                                 03111000
*     GEN(DC    H'12');             /* NO RTCA AVAIL RC              */ 03112000
         DC    H'12'                                                    03113000
*     GEN(STM   GPR14P,GPR12F,12(GPR13P));/* GOOD R13, SAVE RTM'S  0847 03114000
*                                      REGS                          */ 03115000
         STM   GPR14P,GPR12F,12(GPR13P)                                 03116000
*     GEN(L     R3,0(GPR01P));      /* PTR TO USER PARM LIST         */ 03117000
         L     R3,0(GPR01P)                                             03118000
