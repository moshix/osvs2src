         TITLE 'IEAVAD00 - SVC 51 ENTRY POINT AND SVC DUMP PROCESSOR'   00001000
IEAVAD00 CSECT ,                                                   0001 00002000
@MAINENT BALR  @15,0                                               0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL2(@EP00001-@MAINENT-2)                                 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVAD00  78.208'                                 0001 00008000
SDFRRRTN BALR  @15,0                                               0001 00009000
         USING *,@15                                               0001 00010000
         B     @PROLOG                                             0001 00011000
         DC    AL2(@EP00580-SDFRRRTN-2)                                 00012000
         ENTRY SDFRRRTN                                                 00013000
SDESTAEX BALR  @15,0                                               0001 00014000
         USING *,@15                                               0001 00015000
         B     @PROLOG                                             0001 00016000
         DC    AL2(@EP00834-SDESTAEX-2)                                 00017000
         ENTRY SDESTAEX                                                 00018000
         DROP  @15                                                      00019000
@PROLOG  BALR  @09,0                                               0001 00020000
@PSTART  LA    @06,4095(,@09)                                      0001 00021000
         USING @PSTART,@09                                         0001 00022000
         USING @PSTART+4095,@06                                    0001 00023000
         AH    @15,4(,@15)                                         0001 00024000
         BR    @15                                                 0001 00025000
@EP00001 DS    0H                                                  0002 00026000
*                                                                  0177 00027000
*/********************************************************************/ 00028000
*/*  THIS SECTION PERFORMS INITIALIZATION.  CHECKS ARE MADE TO       */ 00029000
*/*  INSURE THE VALIDITY OF THE CALLERS PARAMETER LIST.  IF THE      */ 00030000
*/*  PARAMETER LIST IS FOUND TO BE INVALID, THE CALLER IS ABENDED    */ 00031000
*/*  WITH COMPLETION CODE 233. A REASON CODE IS STORED IN REG 15     */ 00032000
*/********************************************************************/ 00033000
*                                                                  0177 00034000
*   IF R1=NULL|                     /* IF PTR TO PARM LIST IS 0 OR   */ 00035000
*       R1->R1SDFLAG='1'B THEN      /* PARM LIST IS OSVS2 R1 SDUMP   */ 00036000
         LTR   R1,R1                                               0177 00037000
         BZ    @RT00177                                            0177 00038000
         TM    R1SDFLAG(R1),B'10000000'                            0177 00039000
         BNO   @RF00177                                            0177 00040000
@RT00177 DS    0H                                                  0178 00041000
*     DO;                           /* ABEND CALLER W/233    @G33VPHD*/ 00042000
*       RFY                                                        0179 00043000
*         R15 RSTD;                 /*                       @G33VPHD*/ 00044000
*       R15=RSN04;                  /* PUT REASON CODE IN REG      0180 00045000
*                                                            @G33VPHD*/ 00046000
         LA    R15,4                                               0180 00047000
*       GEN(ABEND X'233',DUMP,,SYSTEM);/* ABEND CALLER WITH INVALID     00048000
*                                      PARMS COMPLETION CODE         */ 00049000
         ABEND X'233',DUMP,,SYSTEM                                      00050000
*       RFY                                                        0182 00051000
*         R15 UNRSTD;               /*                       @G33VPHD*/ 00052000
*     END;                          /* END OF ABEND CALLER WITH 233     00053000
*                                                            @G33VPHD*/ 00054000
*/********************************************************************/ 00055000
*/* THIS SECTION PROCESSES SNAP DUMP REQUESTS. NOTE THAT SNAP WILL   */ 00056000
*/* SAVE AND RESTORE THE BASE REGISTER - REG 9 ON ENTRY AND EXIT     */ 00057000
*/********************************************************************/ 00058000
*                                                                  0184 00059000
*   IF R1->SNPFLAG=NULLCHAR|        /* IF OSVS2 R1 SNAP PARM LST OR  */ 00060000
*       (R1->SNPVS2='1'B&           /* OSVS2 RELEASE 2               */ 00061000
*       R1->SNPSNAP='0'B) THEN      /* SNAP PARAMETER LIST           */ 00062000
@RF00177 CLI   SNPFLAG(R1),X'00'                                   0184 00063000
         BE    @RT00184                                            0184 00064000
         TM    SNPVS2(R1),B'01000000'                              0184 00065000
         BNO   @RF00184                                            0184 00066000
         TM    SNPSNAP(R1),B'10000000'                             0184 00067000
         BNZ   @RF00184                                            0184 00068000
@RT00184 DS    0H                                                  0185 00069000
*     DO;                                                          0185 00070000
*       CALL IEAVAD01;              /* GIVE CONTROL TO SNAP          */ 00071000
         L     @15,@CV04542                                        0186 00072000
         BALR  @14,@15                                             0186 00073000
*       RFY                                                        0187 00074000
*         R15 RSTD;                 /* SAVE RET CODE FOR CALLER      */ 00075000
*     END;                                                         0188 00076000
*   ELSE                                                           0189 00077000
*                                                                  0189 00078000
*/********************************************************************/ 00079000
*/*  THIS SECTION PROCESSES SVC DUMP OR SYSMDUMP REQUESTS            */ 00080000
*/********************************************************************/ 00081000
*                                                                  0189 00082000
*     DO;                                                          0189 00083000
         B     @RC00184                                            0189 00084000
@RF00184 DS    0H                                                  0190 00085000
*       IF R1->DUMPTYPE='1'B THEN   /* IF SDUMP PARM LIST,CHECK      */ 00086000
         TM    DUMPTYPE(R1),B'10000000'                            0190 00087000
         BNO   @RF00190                                            0190 00088000
*         DO;                       /* AUTHORIZATION OF CALLER       */ 00089000
*           RFY                                                    0192 00090000
*            (R10,                                                 0192 00091000
*             R11) RSTD;                                           0192 00092000
*           R10=R1;                 /* SAVE ADDRESS OF PARM LIST     */ 00093000
         LR    R10,R1                                              0193 00094000
*           R11=R3;                 /* SAVE ADDRESS OF CVT           */ 00095000
         LR    R11,R3                                              0194 00096000
*           GEN(TESTAUTH  FCTN=1,STATE=NO,KEY=YES,BRANCH=YES)      0195 00097000
*           REFS(CVT,CVTAPF);       /* VALIDATE AUTHORITY OF CALLER  */ 00098000
         TESTAUTH  FCTN=1,STATE=NO,KEY=YES,BRANCH=YES                   00099000
*           R1=R10;                 /* RESTORE ADDRESS OF PARM LIST  */ 00100000
         LR    R1,R10                                              0196 00101000
*           R3=R11;                 /* RESTORE ADDRESS OF CVT        */ 00102000
         LR    R3,R11                                              0197 00103000
*           RFY                                                    0198 00104000
*            (R10,                                                 0198 00105000
*             R11) UNRSTD;                                         0198 00106000
*           IF R15^=NULL THEN       /* IF CALLER NOT AUTHORIZED      */ 00107000
         LTR   R15,R15                                             0199 00108000
         BZ    @RF00199                                            0199 00109000
*             DO;                                                  0200 00110000
*               GEN(ABEND X'133',DUMP,,SYSTEM);/* ABEND CALLER WITH     00111000
*                                      WITH UNAUTHORIZIED COMPLETION    00112000
*                                      CODE                          */ 00113000
         ABEND X'133',DUMP,,SYSTEM                                      00114000
*             END;                                                 0202 00115000
*           ELSE                    /* CALLER IS AUTHORIZED          */ 00116000
*/********************************************************************/ 00117000
*/*  THIS SECTION PROCESSES A SCHEDULED SVC DUMP REQUEST. ASID OR    */ 00118000
*/*  ASIDLST WAS SPECIFIED IN THE SVC DUMP PARAMETER LIST    @G33VPHD*/ 00119000
*/********************************************************************/ 00120000
*                                                                  0203 00121000
*             DO;                                                  0203 00122000
         B     @RC00199                                            0203 00123000
@RF00199 DS    0H                                                  0204 00124000
*               IF R1->SDUASID='1'B|/* IF ASID OR            @G33VPHD*/ 00125000
*                   (R1->SDUNEW='1'B&/* NEW SDUMP &          @G33VPHD*/ 00126000
*                   R1->SDUASLST='1'B) THEN/* ASIDLST        @G33VPHD*/ 00127000
         TM    SDUASID(R1),B'00000100'                             0204 00128000
         BO    @RT00204                                            0204 00129000
         TM    SDUNEW(R1),B'00110000'                              0204 00130000
         BNO   @RF00204                                            0204 00131000
@RT00204 DS    0H                                                  0205 00132000
*                 DO;               /* CALL SCHEDULE DUMP IEAVTSDX 0205 00133000
*                                                            @G33VPHD*/ 00134000
*                   R13=WSALSDMP;   /* SDUMP LOCAL WORK/SAVE         */ 00135000
         L     @14,ASCBPTR                                         0206 00136000
         L     @14,ASXBPTR(,@14)                                   0206 00137000
         L     @14,ASXBSPSA(,@14)                                  0206 00138000
         L     R13,WSALSDMP(,@14)                                  0206 00139000
*                   CALL IEAVTSDX;  /* CALL SCHEDULE DUMP    @G33VPHD*/ 00140000
         L     @12,CVTPTR                                          0207 00141000
         L     @15,CVTSDMP(,@12)                                   0207 00142000
         BALR  @14,@15                                             0207 00143000
*                 END;                                             0208 00144000
*                                                                  0209 00145000
*/********************************************************************/ 00146000
*/*  THIS SECTION PROCESSES A SYNCRONOUS SVC DUMP REQUEST. SVC 51    */ 00147000
*/*  WAS ISSUED AND ASID AND ASIDLST WERE NOT SPECIFIED      @G33VPHD*/ 00148000
*/********************************************************************/ 00149000
*                                                                  0209 00150000
*               ELSE                                               0209 00151000
*                 DO;               /* SYNCHRONOUS DUMP REQUEST    0209 00152000
*                                                            @G33VPHD*/ 00153000
         B     @RC00204                                            0209 00154000
@RF00204 DS    0H                                                  0210 00155000
*                   RFY                                            0210 00156000
*                    (R15) UNRSTD;                                 0210 00157000
*                                                                  0211 00158000
*/*  START OF SPECIFICATIONS ****************************************** 00159000
*                                                                  0211 00160000
* SEGMENT NAME = SDTOP                                             0211 00161000
*                                                                  0211 00162000
* MODULE NAME = IEAVAD00                                           0211 00163000
*                                                                  0211 00164000
* DESCRIPTIVE NAME = SVC DUMP                                      0211 00165000
*                                                                  0211 00166000
* LOAD MODULE NAME = IGC0005A                                      0211 00167000
* LOAD LIBRARY = SYS1.LPALIB                                       0211 00168000
* SYSGEN MACRO = SGIEA401                                          0211 00169000
*                                                                  0211 00170000
* COPYRIGHT = NONE                                                 0211 00171000
*                                                                  0211 00172000
* STATUS = OS/VS2 RELEASE 3.7 SU 33                            @G33VPHD 00173000
*                                                                  0211 00174000
* FUNCTION = SVC DUMP PROVIDES AN UNFORMATTED DUMP OF AN ADDRESS SPACE  00175000
*            TO A SYS1.DUMP DATASET OR A USER SUPPLIED DATASET.  SVC    00176000
*            DUMP IS CALLED BY SVC 51 WHEN THE SDUMP MACRO, WITH   0211 00177000
*            LINKAGE VIA SVC HAS BEEN ISSUED.  THE REQUEST IS      0211 00178000
*            FOR A SYNCHRONOUS DUMP OF THE CALLER'S ADDRESS SPACE. 0211 00179000
*                                                                  0211 00180000
* OPERATION= THE FOLLOWING VIRTUAL STORAGE AREAS MAY BE DUMPED: PSA'S,  00181000
*            NUCLEUS, SQA, RGN(PRIVATE AREA OF ADDRESS SPACE), LSQA,    00182000
*            SWA, ACTIVE LPA MODULES AND SVC'S FOR THE ADDRESS SPACE,   00183000
*            CSA, AND TRT(EITHER SUPERVISOR TRACE TABLE OR GTF INCORE   00184000
*            TRACE BUFFERS).                                       0211 00185000
*            A 4K SQA BUFFER IS AVAILABLE TO THE CALLER            0211 00186000
*            FOR THE SAVING OF VOLATILE DATA. THE 4K SQA BUFFER    0211 00187000
*            IS RESERVED AND INITIALIZED PRIOR TO CALLING SVC DUMP AND  00188000
*            IS REQUESTED TO BE DUMPED BY AN OPTION ON THE SDUMP MACRO. 00189000
*            STORAGE RANGES OF VIRTUAL STORAGE MAY ALSO BE REQUESTED.   00190000
*                                                                  0211 00191000
*            IF VOLATILE, GLOBAL VIRTUAL STORAGE (SQA, CSA) IS     0211 00192000
*            REQUESTED BY THE CALLER, THE SYSTEM IS SET NON-DISPATCH-   00193000
*            ABLE.  THE ADDRESS SPACE TCB'S, EXCEPT THE TOP TASK, ARE   00194000
*            ALWAYS SET NON-DISPATCHABLE.                          0211 00195000
*                                                                  0211 00196000
*            ONLY ONE SVC DUMP IS DONE AT A TIME.  SVC DUMP SERIALIZES  00197000
*            ITS PROCESSING WITH A WORD IN THE RTMCT. (RTCTSDPL)   0211 00198000
*                                                                  0211 00199000
*            THE DUMP CAN BE WRITTEN TO TAPE (2400 COMPATIBLE) OR TO    00200000
*            CERTAIN DASD (2314, 2319, 3330-A, 3330-C, 2305-1, 2305-2,  00201000
*            3350,                                           @Z40RSTA   00202000
*            3340).  THE DUMP DATASET WILL CONSIST OF UNFORMATTED 4104  00203000
*            BYTE RECORDS WHICH MUST BE PRINTED BY THE PRINT DUMP  0211 00204000
*            SERVICE AID, AMDPRDMP.  THE RECORDS ARE IDENTIFIED BY 0211 00205000
*            VIRTUAL ADDRESS AND ARE IN RANDOM ORDER IN THE DATASET.    00206000
*            THE FIRST RECORD IN THE DATASET IS THE CONTENTS OF THE     00207000
*            4K SQA BUFFER, IF REQUESTED.  THE NEXT RECORD IS THE  0211 00208000
*            HEADER RECORD FOLLOWED VOLATILE, GLOBAL STORAGE AREA  0211 00209000
*            RECORDS (PSA,CSA,SQA).  THE NEXT GROUP OF RECORDS CONTAIN  00210000
*            DATA FROM THE PRIVATE AREA OF THE ADDRESS SPACE (RGN, SWA, 00211000
*            LSQA) FOLLOWED BY THE REST OF THE REQUESTED AREAS (LPA     00212000
*            TRT, NUCLEUS).                                        0211 00213000
*                                                                  0211 00214000
*            THE FOLLOWING CONTROL BLOCKS ARE ALWAYS INCLUDED IN THE    00215000
*            DUMP: CVT, ASVT, ASCB, UCB.  THEY ARE REQUIRED BY AMDPRDMP 00216000
*                                                                  0211 00217000
*            THE BASIC LOGIC OF SVC DUMP IS TO CONVERT ALL THE     0211 00218000
*            REQUESTED AREAS INTO ADDRESS RANGES, STARTING AND ENDING   00219000
*            ON A 4K BOUNDARY.  THE STARTING ADDRESS IS ROUNDED DOWN TO 00220000
*            A 4K BOUNDARY AND THE ENDING ADDRESS IS ROUNDED UP TO A    00221000
*            4K BOUNDARY.  THESE ADDRESSES ARE USED AS LOCATIONS FROM   00222000
*            WHICH TO MOVE DATA INTO THE OUTPUT BUFFER.            0211 00223000
*                                                                  0211 00224000
*            THE 4K SQA BUFFER, IF REQUESTED, IS WRITTEN OUT FIRST.     00225000
*            THE HEADER RECORD IS BUILT IN THE OUTPUT BUFFER AND   0211 00226000
*            WRITTEN OUT.  ALL THE REQUESTED AREAS ARE CONVERTED TO     00227000
*            ADDRESS RANGES AND STORED IN THE ADDRESS RANGE TABLE  0211 00228000
*            LOCATED IN THE VACATED 4K SQA BUFFER.  THE ADDRESSES ARE   00229000
*            SORTED TO REMOVE DUPLICATE ADDRESSES.  THE DATA IS MOVED   00230000
*            TO THE OUTPUT BUFFER FROM WHICH IT IS WRITTEN OUT.    0211 00231000
*            UNALLOCATED STORAGE IN AN ADDRESS RANGE IS IGNORED.   0211 00232000
*            SVC DUMP CALLS A SERIES OF EXTERNAL ROUTINES TO PERFORM    00233000
*            THIS SYNCRONOUS DUMP PROCESSING.                      0211 00234000
* NOTES = NONE                                                     0211 00235000
*                                                                  0211 00236000
*   DEPENDENCIES = THIS MODULE IS ECBDIC CHARACTER CODE DEPENDENT. 0211 00237000
*                                                                  0211 00238000
*   RESTRICTIONS = NONE                                            0211 00239000
*                                                                  0211 00240000
*   REGISTER CONVENTIONS = SEE REGISTER DECLARES.                  0211 00241000
*                                                                  0211 00242000
*   PATCH LABEL = SDUPATCH                                         0211 00243000
*                                                                  0211 00244000
* MODULE TYPE = MODULE                                             0211 00245000
*                                                                  0211 00246000
*   PROCESSOR = PLS2                                               0211 00247000
*                                                                  0211 00248000
*   MODULE SIZE = X'ECC'                                           0211 00249000
*                                                                  0211 00250000
*   ATTRIBUTES = PAGED LPA, ZERO PSW PROTECT KEY, SUPERVISOR MODE, 0211 00251000
*                RENTRANT, REFRESHABLE                             0211 00252000
*                                                                  0211 00253000
* ENTRY POINT = SDTOP                                              0211 00254000
*                                                                  0211 00255000
*   PURPOSE = TO TAKE A DUMP OF THE CURRENT ADDRESS SPACE          0211 00256000
*                                                                  0211 00257000
*                                                                  0211 00258000
*                                                                  0211 00259000
*   LINKAGE = PLS2 INCLUDE                                         0211 00260000
*                                                                  0211 00261000
*   INPUT DATA = REG 1 - ADDRESS OF SVC DUMP PARAMETER LIST        0211 00262000
*                REG 3 - ADDRESS OF CVT                            0211 00263000
*                REG 4 - ADDRESS OF SVC DUMP TCB                   0211 00264000
*                REG 5 - ADDRESS OF SVC DUMP SURB                  0211 00265000
*                REG 7 - ADDRESS OF CURRENT ASCB                   0211 00266000
*                REG 14 - RETURN ADDRESS                           0211 00267000
*                                                                  0211 00268000
*   ENQ CONDITIONS = NONE                                          0211 00269000
*                                                                  0211 00270000
*   LOCK CONDITIONS = LOCAL LOCK HELD ON ENTRY.                    0211 00271000
*                                                                  0211 00272000
*   RETISTERS SAVED = NONE                                         0211 00273000
*                                                                  0211 00274000
*   STATIC REGISTERS = NONE                                        0211 00275000
*                                                                  0211 00276000
*   REGISTERS RESTORED = NONE                                      0211 00277000
*                                                                  0211 00278000
*                                                                  0211 00279000
*                                                                  0211 00280000
*                                                                  0211 00281000
*                                                                  0211 00282000
* ENTRY POINT = SDFRRRTN                                           0211 00283000
*                                                                  0211 00284000
*   PURPOSE = RECOVER OR TERMINATE AN SVC DUMP WHILE A LOCK IS HELD.    00285000
*             THIS ENTRY POINT IS ENTERED WHEN SVC DUMP OWNS A LOCK     00286000
*             AND AN ERROR HAS OCCURRED OR THERE HAS BEEN A REQUEST TO  00287000
*             TERMINATE.                                           0211 00288000
*                                                                  0211 00289000
*   LINKAGE = LOAD PSW FROM RTM1                                   0211 00290000
*                                                                  0211 00291000
*   INPUT DATA = REG  1 - ADDRESS OF WORK AREA (SDWA)              0211 00292000
*                REG 14 - RETURN ADDRESS                           0211 00293000
*                REG 15 - ENTRY POINT                              0211 00294000
*                                                                  0211 00295000
*                                                                  0211 00296000
*   ENQ CONDITIONS = NONE                                          0211 00297000
*                                                                  0211 00298000
*   LOCK CONDITIONS = LOCAL, CMS, DISPATCHER, OR SALLOC IS HELD.   0211 00299000
*                                                                  0211 00300000
*   REGISTERS SAVED = NONE                                         0211 00301000
*                                                                  0211 00302000
*   STATIC REGISTERS = NONE                                        0211 00303000
*                                                                  0211 00304000
*   REGISTERS  RESTORED = NONE                                     0211 00305000
*                                                                  0211 00306000
* ENTRY POINT = SDESTAEX                                           0211 00307000
*                                                                  0211 00308000
*   PURPOSE = RECOVER OR TERMINATE AN SVC DUMP.  THIS ENTRY POINT CAN   00309000
*             BE ENTERED WHEN SVC DUMPS FRR PERCOLATES, OR WHEN AN 0211 00310000
*             ERROR OR A REQUEST TO TERMINATE OCCURS.              0211 00311000
*                                                                  0211 00312000
*                                                                  0211 00313000
*   LINKAGE = SYNCHED TO FROM RTM2                                 0211 00314000
*                                                                  0211 00315000
*   INPUT DATA = REG  1 - ADDRESS OF WORK AREA (SDWA)              0211 00316000
*                REG 13 - ADDRESS OF A SAVE AREA                   0211 00317000
*                REG 14 - RETURN ADDRESS                           0211 00318000
*                REG 15 - ENTRY POINT                              0211 00319000
*                                                                  0211 00320000
*   ENQ CONDITIONS = NONE                                          0211 00321000
*                                                                  0211 00322000
*   LOCK CONDITIONS = NONE                                         0211 00323000
*                                                                  0211 00324000
*   REGISTERS SAVED = NONE                                         0211 00325000
*                                                                  0211 00326000
*   STATIC REGISTERS = NONE                                        0211 00327000
*                                                                  0211 00328000
*   REGISTERS RESTORED = NONE                                      0211 00329000
*                                                                  0211 00330000
*                                                                  0211 00331000
* EXIT-NORMAL = RETURN TO CALLER                                   0211 00332000
*                                                                  0211 00333000
*   CONDITIONS = SVC DUMP HAS COMPLETED                            0211 00334000
*                                                                  0211 00335000
*   OUTPUT DATA = UNFORMATTED VIRTUAL DUMP OF AN ADDRESS SPACE IN A     00336000
*                 SYS1.DUMP DATASET OR A USER SUPPLIED DATASET.    0211 00337000
*                                                                  0211 00338000
*   ENQ CONDITIONS = NONE                                          0211 00339000
*                                                                  0211 00340000
*   LOCK CONDITIONS = NONE HELD                                    0211 00341000
*                                                                  0211 00342000
*   RETURN CODES = 0 - SUCCESSFULLY COMPLETED                      0211 00343000
*                  4 - DUMP DATASET CONTAINS A PARTIAL DUMP        0211 00344000
*                  8 - UNABLE TO DUMP                              0211 00345000
*                                                                  0211 00346000
*                                                                  0211 00347000
* EXIT-NORMAL = RETURN TO RTM1 FROM FRR                            0211 00348000
*                                                                  0211 00349000
*   CONDITIONS = RETRY OR PERCOLATE                                0211 00350000
*                                                                  0211 00351000
*   OUTPUT DATA = SOFTWARE ERROR RECORD                            0211 00352000
*                                                                  0211 00353000
*   ENQ CONDITIONS = NONE                                          0211 00354000
*                                                                  0211 00355000
*   LOCK CONDITIONS = LOCAL, CMS, SALLOC, OR DISPATCHER            0211 00356000
*                                                                  0211 00357000
*   RETURN CODES = NONE                                            0211 00358000
*                                                                  0211 00359000
* EXIT-NORMAL = RETURN TO RTM2 FROM ESTAE EXIT                     0211 00360000
*                                                                  0211 00361000
*   CONDITIONS = RETRY OR PERCOLATE                                0211 00362000
*                                                                  0211 00363000
*   OUTPUT DATA = SOFTWARE ERROR RECORD                            0211 00364000
*                                                                  0211 00365000
*   ENQ CONDITIONS = NONE                                          0211 00366000
*                                                                  0211 00367000
*   LOCK CONDITIONS = NONE                                         0211 00368000
*                                                                  0211 00369000
*   RETURN CODES = NONE                                            0211 00370000
*                                                                  0211 00371000
* EXIT-ERROR = ABEND CALLER                                        0211 00372000
*                                                                  0211 00373000
*   CONDITIONS = INVALID PARAMETERS - COMPLETION CODE X'233'       0211 00374000
*                                                                  0211 00375000
*   OUTPUT DATA = NONE                                             0211 00376000
*                                                                  0211 00377000
*   ENQ CONDITIONS = NONE                                          0211 00378000
*                                                                  0211 00379000
*   LCOK CONDITIONS = NONE                                         0211 00380000
*                                                                  0211 00381000
*   RETURN CODES = NONE                                            0211 00382000
*                                                                  0211 00383000
*   REASON CODES = SEE SVC 51 PROLOGUE FOR DETAILS                 0211 00384000
*                                                                  0211 00385000
* EXTERNAL REFERENCES = NONE                                       0211 00386000
*                                                                  0211 00387000
*   ROUTINES = SVC 60 - ESTABLISH AND REMOVE ESTAE EXIT, BRANCH ENTRY   00388000
*                       AND SVC LINKAGE.                           0211 00389000
*                                                                  0211 00390000
*              STATUS - SET/RESET SYSTEM AND TCB'S IN CURRENT ADDRESS   00391000
*                       SPACE NON-DISPATCHABLE, BRANCH ENTRY AND SVC    00392000
*                       LINKAGE.                                   0211 00393000
*                                                                  0211 00394000
*              IEAVTSDH - SELECT AND INITIALIZE THE DUMP DATA  @G33VPHD 00395000
*                         SET                                  @G33VPHD 00396000
*                                                                  0211 00397000
*              IEAVTSDG - BUILD THE ADDRESS RANGES FOR THE     @G33VPHD 00398000
*                         GLOBAL STORAGE AREA.  OUTPUT THE     @G33VPHD 00399000
*                         GLOBAL STORAGE DATA BY CALLING       @G33VPHD 00400000
*                         MODULE IEAVTSDO                      @G33VPHD 00401000
*                                                                  0211 00402000
*              IEAVTSDF - BUILD THE ADDRESS RANGES FOR THE     @G33VPHD 00403000
*                         GTF RECORDS                          @G33VPHD 00404000
*                                                                  0211 00405000
*              IEAVTSDL - BUILD THE ADDRESS RANGES FOR THE     @G33VPHD 00406000
*                         LOCAL STORAGE AREAS.  OUTPUT THE     @G33VPHD 00407000
*                         LOCAL STORAGE DATA BY CALLING        @G33VPHD 00408000
*                         MODULE IEAVTSDO                      @G33VPHD 00409000
*                                                                  0211 00410000
*              IEAVTSDC - CLEAN-UP SVC DUMP RESOURCES AND      @G33VPHD 00411000
*                         NOTIFY OPERATOR                      @G33VPHD 00412000
*                                                              @G33VPHD 00413000
*              IEAVTSDE - CLEAN UP SVC DUMP RESOURCES FROM     @G33VPHD 00414000
*                         RECOVERY ROUTINES                    @G33VPHD 00415000
*                                                                  0211 00416000
*              IEAVTSSE - PERFORM REQUESTED ENABLED SUMMARY        0211 00417000
*                         DUMP                                 @ZM45052 00418000
*                                                                  0211 00419000
*              LOCK MGR - OBTAIN AND RELEASE LOCAL LOCK OR SALLOC LOCK, 00420000
*                         BRANCH ENTRY                             0211 00421000
*                                                                  0211 00422000
*                                                                  0211 00423000
