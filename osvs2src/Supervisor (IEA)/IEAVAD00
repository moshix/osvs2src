*                               ERRSDBIT='1'B;/* SVC DUMP LOCK BIT 0244 00607000
*                                      TURNED ON                     */ 00608000
*                               ERRBFBIT='1'B;/* BUFFER LOCK BIT   0245 00609000
*                                      TURNED ON                     */ 00610000
         OI    ERRSDBIT(R3),B'00110000'                            0245 00611000
*                               RTCTSDID=ASCBASID;/* SVC DUMP ACTIVE    00612000
*                                      IN THIS ADDR SPACE-FOR      0246 00613000
*                                      IEAVTSDR                      */ 00614000
         L     @14,CVTPTR                                          0246 00615000
         L     @14,CVTRTMCT(,@14)                                  0246 00616000
         L     @12,ASCBPTR                                         0246 00617000
