*                     DO;           /* RELEASE SVC DUMP LOCK FLAGS   */ 01864000
*                       RTCTSDI=RTCTSDI&&RTCTSDI;/* CLEAR RTCT FLAGS    01865000
*                                                            @ZM44940*/ 01866000
         L     @14,CVTPTR                                          0540 01867000
         L     @14,CVTRTMCT(,@14)                                  0540 01868000
