*                       RTCTSDF=RTCTSDF&&RTCTSDF;/* CLEAR RTCT FLAGS    01870000
*                                                            @ZM44940*/ 01871000
         XC    RTCTSDF(4,@14),RTCTSDF(@14)                         0541 01872000
*                       DO RTCTINDX=ONE TO MAXENTRY;/* CLEAR WHOLE 0542 01873000
*                                      ARRAY                 @ZM44940*/ 01874000
