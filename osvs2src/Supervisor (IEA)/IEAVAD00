         L     @14,CVTRTMCT(,@14)                                  0239 00593000
         CS    R15,@01,RTCTSDPL(@14)                               0239 00594000
*                               BC(CSCC0,SWAPPED1);/* NO-GO IND THIS    00595000
*                                      CURR DMP                      */ 00596000
         BC    8,SWAPPED1                                          0240 00597000
*                               R15=RSN38;/* PUT REASON CODE IN REG     00598000
*                                                            @G33VPHD*/ 00599000
         LA    R15,56                                              0241 00600000
*                               GEN(ABEND X'233',DUMP,,SYSTEM);/*  0242 00601000
*                                      YES, BUFFER= BAD PARM         */ 00602000
         ABEND X'233',DUMP,,SYSTEM                                      00603000
*SWAPPED1:                                                         0243 00604000
*                               ;                                  0243 00605000
