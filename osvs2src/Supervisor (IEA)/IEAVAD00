@EL00001 DS    0H                                                  0562 01954000
@EF00001 DS    0H                                                  0562 01955000
@ER00001 BR    @14                                                 0562 01956000
*                                                                  0563 01957000
*/********************************************************************/ 01958000
*/*  INTERNAL PROCEDURE - SDXMPOST                                   */ 01959000
*/*                                                                  */ 01960000
*/*  FUNCTION - THIS INTERNAL PROCEDURE IS USED BY THE FRR AND       */ 01961000
*/*             ESTAE RECOVERY ROUTINES TO POST THE CALLERS ECB,     */ 01962000
*/*             IF ONE WAS SUPPLIED.  AN ECB WILL ONLY BE SUPPLIED   */ 01963000
*/*             FOR SCHEDULED DUMP REQUESTS.                         */ 01964000
*/*                                                                  */ 01965000
*/*  INPUT - SVC DUMP PARAMETER LIST, CALLER SPECIFED ECB            */ 01966000
*/*                                                                  */ 01967000
*/*  CALLERS - SDFRRRTN, SDESTAEX                                    */ 01968000
*/*                                                                  */ 01969000
*/********************************************************************/ 01970000
*                                                                  0563 01971000
*SDXMPOST:                                                         0563 01972000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0563 01973000
SDXMPOST DS    0H                                                  0564 01974000
*   RFY                                                            0564 01975000
*    (R15,                                                         0564 01976000
*     R14) RSTD;                                                   0564 01977000
*   SDUECB='0'B;                    /* INSURE ECB POSTED ONCE      0565 01978000
*                                                            @G33VPHD*/ 01979000
         L     @12,SDWSDPL(,R7)                                    0565 01980000
         NI    SDUECB(@12),B'11110111'                             0565 01981000
*   RFY                                                            0566 01982000
*    (R10,                                                         0566 01983000
*     R11,                                                         0566 01984000
*     R4,                                                          0566 01985000
*     R5,                                                          0566 01986000
*     R1) RSTD;                     /*                       @G33VPHD*/ 01987000
*   IF ASVTAVAL(SDUCASID)='0'B&     /* IF CALLERS ASID       @G33VPHD*/ 01988000
*       SDUECBAD^=NULL THEN         /* AND ECB ARE VALID     @G33VPHD*/ 01989000
         LH    @08,SDUCASID(,@12)                                  0567 01990000
         N     @08,@CF04671                                        0567 01991000
         SLA   @08,2                                               0567 01992000
         L     @02,CVTPTR                                          0567 01993000
         L     @00,ASVTPTR(,@02)                                   0567 01994000
         LR    @12,@00                                             0567 01995000
         ALR   @12,@08                                             0567 01996000
         TM    ASVTAVAL-4(@12),B'10000000'                         0567 01997000
         BNZ   @RF00567                                            0567 01998000
         L     @12,SDWSDPL(,R7)                                    0567 01999000
         L     @12,SDUECBAD(,@12)                                  0567 02000000
         LTR   @12,@12                                             0567 02001000
