*   ESTAEPTR=SDWAPARM;              /* GET POINTER TO ESTAE PARAMETER   02630000
*                                      LIST                          */ 02631000
         L     ESTAEPTR,SDWAPARM(,SDWAPTR)                         0950 02632000
*   MFLESTAE=ESTAELST;              /* MOVE MF-L FORM TO GM STORAGE. */ 02633000
