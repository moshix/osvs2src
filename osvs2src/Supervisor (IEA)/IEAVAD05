         TITLE 'IEAVAD05 - CONTROL BLOCKS II                           *00001000
                        '                                               00002000
IEAVAD05 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVAD05  77.049'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @11,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@11                                         0001 00014000
*                                                                  0080 00015000
*   /*****************************************************************/ 00016000
*   /*                                                               */ 00017000
*   /* INITIALIZATION ON ENTRY FIRST THE ADDRESSING REGISTERS ARE    */ 00018000
*   /* INITIALIZED, THEN A SAVE AREA FOR THE REGISTER SAVE AREA AND  */ 00019000
*   /* AUTOMATIC DATA AREA IS OBTAINED FROM LSQA. THE ADDRESS OF THE */ 00020000
*   /* AREA IS RETURNED IN ABDGMA, AND THE ADDRESS OF THE GETMAIN    */ 00021000
*   /* PARAMETER LIST IS PASSED IN REGISTER 1. NEXT THE SAVE AREAS   */ 00022000
*   /* ARE CHAINED, AND THE TEMPORARY WORK AREA IS CLEARED TO ZEROS  */ 00023000
*   /* SO THAT THE BOUNDARY ADJUSTMENT MOVES CAN BE DONE PROPERLY.   */ 00024000
*   /*                                                               */ 00025000
*   /*****************************************************************/ 00026000
*                                                                  0080 00027000
*   ABDARPTR=PARMPTR;               /* THE ABDAREA POINTER IS      0080 00028000
*                                      INITIALIZED FROM THE VALUE IN    00029000
*                                      REGISTER 1                    */ 00030000
         LR    ABDARPTR,PARMPTR                                    0080 00031000
*   PARMPTR=ADDR(ABDASIZE);         /* REGISTER 1 IS SET TO POINT TO    00032000
*                                      THE GETMAIN PARAMETER LIST    */ 00033000
         LA    PARMPTR,ABDASIZE(,ABDARPTR)                         0081 00034000
*   GENERATE;                                                      0082 00035000
         GETMAIN EC,LV=@ENDDATD-@DATD+72,                              *00036000
               SP=253,                                                 *00037000
               A=ABDGMA(ABDARPTR),                                     *00038000
               MF=(E,(1))                                               00039000
*   IF RETCODE^=0                   /* IF GETMAIN IS UNSUCCESSFUL    */ 00040000
*     THEN                          /* RETURN IMMEDIATELY.           */ 00041000
         LTR   RETCODE,RETCODE                                     0083 00042000
         BZ    @RF00083                                            0083 00043000
*     DO;                           /*                        @Y02705*/ 00044000
*       RETCODE=8;                  /* NO CORE AVAIL.         @Y02705*/ 00045000
         LA    RETCODE,8                                           0085 00046000
*       RETURN;                     /* OTHERWISE CONTINUE            */ 00047000
@EL00001 DS    0H                                                  0086 00048000
@EF00001 DS    0H                                                  0086 00049000
@ER00001 L     @14,12(,@13)                                        0086 00050000
         LM    @00,@12,20(@13)                                     0086 00051000
         BR    @14                                                 0086 00052000
*     END;                          /*                        @Y02705*/ 00053000
*   ELSE                            /*                        @Y02705*/ 00054000
*     DO;                           /*                        @Y02705*/ 00055000
@RF00083 DS    0H                                                  0089 00056000
*       PARMPTR=ABDAAREA;           /* USE PARMPTR AS TEMPORARY    0089 00057000
*                                      POINTER TO SAVE AREA          */ 00058000
         L     PARMPTR,ABDAAREA(,ABDARPTR)                         0089 00059000
*       NEXSAPTR=PARMPTR;           /* INITIALIZE NEXT SAVE AREA   0090 00060000
*                                      POINTER IN OLD SAVE AREA      */ 00061000
         ST    PARMPTR,NEXSAPTR(,SAVEPTR)                          0090 00062000
*       PRESAPTR=SAVEPTR;           /* INITIALIZE PREVIOUS SAVE AREA    00063000
*                                      POINTER IN NEW AREA           */ 00064000
         ST    SAVEPTR,PRESAPTR(,PARMPTR)                          0091 00065000
*       SAVEPTR=PARMPTR;            /* SET SAVE REGISTER TO NEW SAVE    00066000
*                                      AREA ADDRESS                  */ 00067000
         LR    SAVEPTR,PARMPTR                                     0092 00068000
*       AUTOPTR=SAVEPTR+72;         /* SET UP AUTOMATIC DATA       0093 00069000
*                                      ADDRESSABILITY BEYOND SAVE  0093 00070000
*                                      AREA                          */ 00071000
         LA    AUTOPTR,72(,SAVEPTR)                                0093 00072000
*       GENERATE;                                                  0094 00073000
         USING @DATD,AUTOPTR           ESTABLISH ADDRESSABILITY         00074000
         XC    0(@ENDDATD-@DATD,AUTOPTR),0(AUTOPTR)       0 SPACE       00075000
*       PARMPTR=ABDARPTR;           /* SET ADDR OF WORK AREA @YM04767*/ 00076000
         LR    PARMPTR,ABDARPTR                                    0095 00077000
*       SAVERET=0;                  /* INIT. RETCODE REG     @YM04767*/ 00078000
         SLR   SAVERET,SAVERET                                     0096 00079000
*                                                                  0097 00080000
*       /*************************************************************/ 00081000
*       /*                                                           */ 00082000
*       /* THE FIRST TIME THROUGH THE LOOP, THE IQES ARE      @Y02705*/ 00083000
*       /* FORMATTED AND PRINTED. THE SEARCH OF THE QUEUE IS  @Y02705*/ 00084000
*       /* WITH THE LOCAL LOCK RUN DISABLED DUE TO            @Y02705*/ 00085000
*       /* ITS VOLATILE NATURE. THOSE IQES TO BE DISPLAYED    @Y02705*/ 00086000
*       /* HAVE A PURGE BIT ON TO INDICATE THAT THE BLOCK     @Y02705*/ 00087000
*       /* WILL NOT BE FREED WHILE IT IS BEING DISPLAYED. IN THE MAIN*/ 00088000
*       /* LOOP, THE NEXT BLOCK POINTER IS GOTTEN. IF THE HIGH ORDER */ 00089000
*       /* BYTE CONTAINS A HEX FF THE END OF THE QUEUE HAS BEEN      */ 00090000
*       /* REACHED.                                           @Y02705*/ 00091000
*       /* IF THERE WAS ANOTHER BLOCK STILL ON THE QUEUE, THE PURGE  */ 00092000
*       /* BIT IS CHECKED. IF THE BIT IS OFF THE NEXT BLOCK IS       */ 00093000
*       /* OBTAINED. IF THE BIT IS ON THE TCB FIELD IS CHECKED. IF   */ 00094000
*       /* THE TCB ADDRESS IS THE TCB BEING DISPLAYED, THE BLOCK IS  */ 00095000
*       /* FORMATTED AND PRINTED. THE FORMATTING AND PRINTING IS     */ 00096000
*       /* NATURALLY DONE ENABLED.                                   */ 00097000
*       /*                                                           */ 00098000
*       /*************************************************************/ 00099000
*                                                                  0097 00100000
*       TCBPTR=ABDTCB;              /* PICK UP TCB ADDRESS           */ 00101000
         L     TCBPTR,ABDTCB(,ABDARPTR)                            0097 00102000
*IQEDISLP:                          /* OBTAIN LOCK BEFORE SEARCH     */ 00103000
*       GENERATE REFS(PSALITA,ABDWORK);                            0098 00104000
IQEDISLP DS    0H                                                  0098 00105000
*                                       /*                    @Y02705*/ 00106000
         STM   11,14,ABDWORK(ABDARPTR)                                  00107000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(IQE,           C00108000
               IEAVAD05(IQEDISLP))                                      00109000
         LM    11,14,ABDWORK(ABDARPTR)                                  00110000
*       ASCBPTR=PSAAOLD;            /* GET ADDR OF CURRENT ASCB    0099 00111000
*                                                             @Y02705*/ 00112000
         L     ASCBPTR,PSAAOLD                                     0099 00113000
*       ASXBPTR=ASCBASXB;           /* GET ADDR OF ASXB       @Y02705*/ 00114000
         L     ASXBPTR,ASCBASXB(,ASCBPTR)                          0100 00115000
*       IF ASXBFIQE=0 THEN          /* DOES AN IQE EXIST      @Y02705*/ 00116000
         ICM   @14,15,ASXBFIQE(ASXBPTR)                            0101 00117000
         BNZ   @RF00101                                            0101 00118000
*         DO;                       /* NO, FREE LOCK          @Y02705*/ 00119000
*           GENERATE REFS(PSALITA,ABDWORK);                        0103 00120000
*                                         /*                  @Y02705*/ 00121000
         STM    11,14,ABDWORK(ABDARPTR)                                 00122000
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(IQE,IEAVAD05(IEQDISLP))    00123000
         LM     11,14,ABDWORK(ABDARPTR)                                 00124000
*           GOTO VSMOUT;            /* FMT VSM BLKS          @Z40WPXH*/ 00125000
         B     VSMOUT                                              0104 00126000
*         END;                      /*                        @Y02705*/ 00127000
*       IQEPTR=ASXBFIQE;            /* POINT TO FIRST IQE ADDRESS    */ 00128000
@RF00101 L     IQEPTR,ASXBFIQE(,ASXBPTR)                           0106 00129000
*       ABDLINEA(1:4)=IQEMSG;       /* SET UP IQE MESSAGE TEXT       */ 00130000
         MVC   ABDLINEA(4,ABDARPTR),IQEMSG                         0107 00131000
*       HEADBIT='1'B;               /* SET UP TO DO IQE HEADER       */ 00132000
         OI    HEADBIT,B'10000000'                                 0108 00133000
*       GOTO CHPURGE;               /* CHECK PURGE INDICTR    @Y02705*/ 00134000
         B     CHPURGE                                             0109 00135000
*IQELP:                             /* SEARCH LOOP FOR IQE           */ 00136000
*       IQEPTR=IQELNKA;             /* PICK UP NEXT BLOCK TO TEST  0110 00137000
*                                                            @YM06701*/ 00138000
IQELP    SLR   @14,@14                                             0110 00139000
         ICM   @14,7,IQELNKA(IQEPTR)                               0110 00140000
         LR    IQEPTR,@14                                          0110 00141000
*       IF IQEPTR=0                 /* IF END OF Q INDICATD  @YM06701*/ 00142000
*         THEN                                                     0111 00143000
         LTR   IQEPTR,IQEPTR                                       0111 00144000
         BNZ   @RF00111                                            0111 00145000
*         DO;                                                      0112 00146000
*           GENERATE REFS(ABDWORK,PSALITA);                        0113 00147000
*                                      /*                     @Y02705*/ 00148000
         STM   11,14,ABDWORK(ABDARPTR)                                  00149000
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(IQE,IEAVAD05(IQELP))       00150000
         LM    11,14,ABDWORK(ABDARPTR)                                  00151000
*           PARMPTR=ABDARPTR;       /* RESET PARAMETER REG           */ 00152000
         LR    PARMPTR,ABDARPTR                                    0114 00153000
*           ABDLINEA(1:4)=' ';      /* BLANK OUT POSS HEADER         */ 00154000
         MVI   ABDLINEA+1(ABDARPTR),C' '                           0115 00155000
         MVC   ABDLINEA+2(2,ABDARPTR),ABDLINEA+1(ABDARPTR)         0115 00156000
         MVI   ABDLINEA(ABDARPTR),C' '                             0115 00157000
*           GOTO VSMOUT;            /* DO VSM BLOCKS         @Z40WPXH*/ 00158000
         B     VSMOUT                                              0116 00159000
*         END;                                                     0117 00160000
*CHPURGE:                           /*                        @Y02705*/ 00161000
*       IF IQEPURGE='0'B            /* IF BLOCK NOT FIXED            */ 00162000
*         THEN                      /* IGNORE IT                     */ 00163000
@RF00111 DS    0H                                                  0118 00164000
CHPURGE  TM    IQEPURGE(IQEPTR),B'10000000'                        0118 00165000
         BZ    @RT00118                                            0118 00166000
*         GOTO IQELP;               /* CONTINUE DISABLE SEARCH       */ 00167000
*       IF TCBPTR^=IQETCB           /* IF NOT OWNED BY OUR TCB       */ 00168000
*         THEN                      /* IGNORE IT                     */ 00169000
         C     TCBPTR,IQETCB(,IQEPTR)                              0120 00170000
         BNE   @RT00120                                            0120 00171000
*         GOTO IQELP;               /* CONTINUE DISABLED SEARCH      */ 00172000
*       ABDLLINE=ADDR(IQELLINE);    /* POINT TO IQE LAYOUT           */ 00173000
         LA    @14,IQELLINE                                        0122 00174000
         ST    @14,ABDLLINE(,ABDARPTR)                             0122 00175000
*IQEHDR:                                                           0123 00176000
*       GENERATE REFS(PSALITA,ABDWORK);                            0123 00177000
IQEHDR   DS    0H                                                  0123 00178000
*                                       /*                    @Y02705*/ 00179000
         STM   11,14,ABDWORK(ABDARPTR)                                  00180000
          SETLOCK RELEASE,TYPE=LOCAL,RELATED=(IQE,IEAVAD05(IQEHDR))     00181000
         LM    11,14,ABDWORK(ABDARPTR)                                  00182000
*       IF HEADBIT='1'B             /* IF HEADING TO BE PRINTED      */ 00183000
*         THEN                      /* PRINT IT                      */ 00184000
         TM    HEADBIT,B'10000000'                                 0124 00185000
         BNO   @RF00124                                            0124 00186000
*         DO;                                                      0125 00187000
*           CALL IEAVAD11;          /* OUTPUT THE HEADING            */ 00188000
         L     @15,@CV00114                                        0126 00189000
         BALR  @14,@15                                             0126 00190000
*           IF RETCODE^=0           /* IF ERRED                      */ 00191000
*             THEN                  /* LEAVE WITH                    */ 00192000
         LTR   RETCODE,RETCODE                                     0127 00193000
         BNZ   @RT00127                                            0127 00194000
*             GOTO FREERET;         /* BAD CODE                      */ 00195000
*           HEADBIT='0'B;           /* INDICATE HEADING PRINTED      */ 00196000
         NI    HEADBIT,B'01111111'                                 0129 00197000
*           ABDCC='0';              /* DOUBLE SPACE FIRST BLOCK      */ 00198000
         MVI   ABDCC(ABDARPTR),C'0'                                0130 00199000
*         END;                                                     0131 00200000
*       ABDWORKA=IQEPTR;            /* POINT TO IQE TO FORMAT        */ 00201000
@RF00124 ST    IQEPTR,ABDWORKA(,ABDARPTR)                          0132 00202000
*       ABDBPTR=ADDR(ABDWORKB);     /* POINT TO ADDRESS              */ 00203000
         LA    @14,ABDWORKB(,ABDARPTR)                             0133 00204000
         ST    @14,ABDBPTR(,ABDARPTR)                              0133 00205000
*       CALL IEAVAD31;              /* FORMAT THE ADDRESS            */ 00206000
         L     @15,@CV00115                                        0134 00207000
         BALR  @14,@15                                             0134 00208000
*       IF RETCODE^=0               /* IF ERRED                      */ 00209000
*         THEN                      /* LEAVE WITH                    */ 00210000
         LTR   RETCODE,RETCODE                                     0135 00211000
         BNZ   @RT00135                                            0135 00212000
*         GOTO FREERET;             /* BAD CODE                      */ 00213000
*       ABDBPTR=IQEPTR;             /* POINT TO BLOCK TO FORMAT      */ 00214000
         ST    IQEPTR,ABDBPTR(,ABDARPTR)                           0137 00215000
*       CALL IEAVAD31;              /* FORMAT THE IQE                */ 00216000
         L     @15,@CV00115                                        0138 00217000
         BALR  @14,@15                                             0138 00218000
*       IF RETCODE^=0               /* IF ERRED                      */ 00219000
*         THEN                      /* LEAVE WITH                    */ 00220000
         LTR   RETCODE,RETCODE                                     0139 00221000
         BNZ   @RT00139                                            0139 00222000
*         GOTO FREERET;             /* BAD CODE                      */ 00223000
*       CALL IEAVAD11;              /* OUTPUT THE IQE                */ 00224000
         L     @15,@CV00114                                        0141 00225000
         BALR  @14,@15                                             0141 00226000
*       IF RETCODE^=0               /* IF ERRED                      */ 00227000
*         THEN                      /* LEAVE WITH                    */ 00228000
         LTR   RETCODE,RETCODE                                     0142 00229000
         BNZ   @RT00142                                            0142 00230000
*         GOTO FREERET;             /* BAD CODE                      */ 00231000
*       GENERATE REFS(PSALITA,ABDWORK);                            0144 00232000
*                                       /*                    @Y02705*/ 00233000
         STM   11,14,ABDWORK(ABDARPTR)                                  00234000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(IQE,           C00235000
               IEAVAD05(IQEHDR))                                        00236000
         LM    11,14,ABDWORK(ABDARPTR)                                  00237000
*       GOTO IQELP;                 /* CONTINUE THE LOOP             */ 00238000
         B     IQELP                                               0145 00239000
*VSMOUT:                            /*                       @Z40WPXH*/ 00240000
*       IF TCBFC='1'B               /* IF TASK IS COMPLETE           */ 00241000
*         THEN                      /* DONT PRINT                    */ 00242000
VSMOUT   TM    TCBFC(TCBPTR),B'10000000'                           0146 00243000
         BO    @RT00146                                            0146 00244000
*         GOTO PQEOUT;              /* VMM BLOCK                     */ 00245000
*       HEADBIT='1'B;               /* PRINT HEADERS FIRST SPQE      */ 00246000
         OI    HEADBIT,B'10000000'                                 0148 00247000
*       IF TCBPTR=ABDCTCB           /* IF CURRENT TASK DUMPED        */ 00248000
*         THEN                      /* SET NON-DISPATCHABLE          */ 00249000
         C     TCBPTR,ABDCTCB(,ABDARPTR)                           0149 00250000
         BNE   @RF00149                                            0149 00251000
*         DO;                                                      0150 00252000
*           IF TCBLTC^=0&           /* DOES TASK HAVE DAU.    @Y02705*/ 00253000
*               TCBLTC->TCBJSTCB=TCBLTC/* WHICH IS A JS TASK  @Y02705*/ 00254000
*             THEN                                                 0151 00255000
         L     @14,TCBLTC(,TCBPTR)                                 0151 00256000
         LTR   @14,@14                                             0151 00257000
         BZ    @RF00151                                            0151 00258000
         C     @14,TCBJSTCB(,@14)                                  0151 00259000
         BE    @RT00151                                            0151 00260000
*             ;                     /* YES, DO NOT SET ND     @Y02705*/ 00261000
*           ELSE                                                   0153 00262000
*             DO;                                                  0153 00263000
@RF00151 DS    0H                                                  0154 00264000
*               TCBADMP='1'B;       /* INDICATE GOING TO SET BIT     */ 00265000
         OI    TCBADMP(TCBPTR),B'00000010'                         0154 00266000
*               GEN(STATUS SET,ND,STEP,(1));/* SET NON-DISP          */ 00267000
         STATUS SET,ND,STEP,(1)                                         00268000
*               ABDSTAT='1'B;       /* IND STATUS ISSUED     @YM06195*/ 00269000
         OI    ABDSTAT(ABDARPTR),B'00000010'                       0156 00270000
*             END;                  /*                        @Y02705*/ 00271000
*         END;                                                     0158 00272000
@RT00151 DS    0H                                                  0159 00273000
*       SPQESPTR=ADDR(TCBMSS);      /* POINT TO FIRST SPQE ADDR      */ 00274000
@RF00149 LA    SPQESPTR,TCBMSS(,TCBPTR)                            0159 00275000
*       PARMPTR=ABDARPTR;           /* RESET PARAMETER REG           */ 00276000
         LR    PARMPTR,ABDARPTR                                    0160 00277000
*NEXTSPQE:                          /* GET NEXT SPQE                 */ 00278000
*       SPQESPTR=SPQEPTR;           /* PICK UP NEXT SPQE ADDR        */ 00279000
NEXTSPQE L     SPQESPTR,SPQEPTR(,SPQESPTR)                         0161 00280000
*       IF SPQESPTR=0               /* IF NON LEFT LEAVE             */ 00281000
*         THEN                      /* AFTER CHECKING DISPACHABLE    */ 00282000
         SLR   @14,@14                                             0162 00283000
         CR    SPQESPTR,@14                                        0162 00284000
         BNE   @RF00162                                            0162 00285000
*         DO;                                                      0163 00286000
*           IF TCBPTR=ABDCTCB       /* IF SET NON-DISP               */ 00287000
*             THEN                  /* RESET TO DISPACHABLE          */ 00288000
         C     TCBPTR,ABDCTCB(,ABDARPTR)                           0164 00289000
         BNE   @RF00164                                            0164 00290000
*             DO;                                                  0165 00291000
*               IF TCBLTC^=0&       /* DOES TASK HAVE DAU.    @Y02705*/ 00292000
*                   TCBLTC->TCBJSTCB=TCBLTC/* WHICH IS A JS TASK   0166 00293000
