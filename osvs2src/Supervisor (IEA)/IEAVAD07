*   ABDUPRXT=0;                     /* DISALLOW UPR INTERRUPTS       */ 00747000
*                                                                  0269 00748000
         ST    @15,ABDUPRXT(,ABDARPTR)                             0269 00749000
*   /*****************************************************************/ 00750000
*   /*                                                               */ 00751000
*   /* IF LENGTH OF ID IS 0 THEN MESSAGE IS COMPLETE                 */ 00752000
*   /*                                                               */ 00753000
*   /*****************************************************************/ 00754000
*                                                                  0270 00755000
*   IF LNG=0                        /* MODULE ID 0 LONG              */ 00756000
*     THEN                          /* YES                           */ 00757000
         CLI   LNG,0                                               0270 00758000
         BE    @RT00270                                            0270 00759000
*     GOTO MSGDONE1;                /* NOTHING TO MOVE               */ 00760000
*   ELSE                                                           0272 00761000
*     ;                             /* MOVE ID                       */ 00762000
*                                                                  0272 00763000
*   /*****************************************************************/ 00764000
*   /*                                                               */ 00765000
*   /* MAX ID LENGTH FOR MESSAGE IS 70                               */ 00766000
*   /*                                                               */ 00767000
*   /*****************************************************************/ 00768000
*                                                                  0273 00769000
*   IF LNG>70                       /* OVER SIZE ID                  */ 00770000
*     THEN                          /* YES                           */ 00771000
