*   HEAD=STANDMSG;                  /* SET HEADING INTO LINE         */ 00704000
*                                                                  0260 00705000
         MVC   HEAD(20,ABDARPTR),STANDMSG                          0260 00706000
*   /*****************************************************************/ 00707000
*   /*                                                               */ 00708000
*   /* POSSIBLE ENTRY POINT MUST LIE WITHIN THE BOUNDARIES OF THE    */ 00709000
*   /* MACHINE                                                       */ 00710000
*   /*                                                               */ 00711000
*   /*****************************************************************/ 00712000
*                                                                  0261 00713000
*   IF SA15=0|                      /* ADDRESS ZER                   */ 00714000
*       SA15>CVTMZ00                /* VALUE HIGHER THAN MAX ADDR    */ 00715000
