*       ELSE                        /* OTHERWISE, NO MSG     @ZA03859*/ 00910000
*         GOTO SA1EXITA;            /* BYPASS MESSAGE        @ZA03859*/ 00911000
*     END;                          /* END LLEPTR ZERO       @ZA03859*/ 00912000
*   ELSE                                                           0310 00913000
*     ;                             /* CHECK LLE FOR EP              */ 00914000
