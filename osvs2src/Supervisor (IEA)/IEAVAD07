         ST    @14,ABDUPRXT(,ABDARPTR)                             0264 00730000
*   /*****************************************************************/ 00731000
*   /*                                                               */ 00732000
*   /* CONSIDERING THE VALUE IN SA15 AS THE ENTRY POINT OF THE       */ 00733000
*   /* MODULE, DETERMINE IF STANDARD MODULE IDS ARE USED             */ 00734000
*   /*                                                               */ 00735000
*   /*****************************************************************/ 00736000
*                                                                  0265 00737000
*   IF EPI^='47F0'X                 /* INSTRUCTION AT SA15 A BRANCH  */ 00738000
*     THEN                          /* NO                            */ 00739000
         CLC   EPI(2,@10),@CB02440                                 0265 00740000
         BNE   @RT00265                                            0265 00741000
*     GOTO MSGDONE;                 /* LEAVE EP STUFF OUT            */ 00742000
*   ELSE                                                           0267 00743000
*     ;                             /* TRY TO PUT EP IN MSG          */ 00744000
*   LNG=CNT;                        /* GET LENGTH OF EP IDENT        */ 00745000
