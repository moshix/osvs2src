*   /*****************************************************************/ 01191000
*   /*                                                               */ 01192000
*   /* ENTRY POINT TO SUBROUTINE IF LLE SUPPLIED                     */ 01193000
*   /*                                                               */ 01194000
*   /*****************************************************************/ 01195000
*                                                                  0385 01196000
*TESTEP2:                           /* START LOGIC                   */ 01197000
*   ENTRY;                                                         0385 01198000
TESTEP2  DS    0H                                                  0386 01199000
*   TESTR14=RETREG;                 /* SAVE RET ADDR                 */ 01200000
