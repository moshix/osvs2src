         BE    @RT00252                                            0252 00670000
*     GOTO AWAY;                    /* NORMAL EXIT                   */ 00671000
*   ELSE                            /* MORE TO DO                    */ 00672000
*     GOTO PRBLOOP;                 /* REPEAT ONCE MORE              */ 00673000
         B     PRBLOOP                                             0254 00674000
*                                                                  0255 00675000
*   /*****************************************************************/ 00676000
*   /*                                                               */ 00677000
*   /* THIS SUBROUTINE CONTAINS THREE ENTRY POINTS WHICH PERFORM THE */ 00678000
*   /* FUNCTIONS OUTLINED BELOW SAPRINT1 CONSTRUCT AND PRINT THE     */ 00679000
*   /* STANDARD MESSAGES PRECEDING THE DISPLAY OF SAVE AREA. PASS    */ 00680000
*   /* CONTROL TO SAPRINT2. SAPRINT2 CHECK IF CALLER REQUESTED THAT  */ 00681000
*   /* ONLY THE HEADINGS BE DISPLAYED. IF SO RETURN TO THE CALLER    */ 00682000
*   /* WITH A CODE OF 0. OTHERWISE, PASS CONTROL TO SAPRINT3.        */ 00683000
*   /* SAPRINT3 DISPLAY THE CONTENTS OF THE CURRENT SAVE AREA        */ 00684000
*   /*                                                               */ 00685000
*   /*****************************************************************/ 00686000
*                                                                  0255 00687000
*SAPRINT1:                          /* START OF SUBROUTINE           */ 00688000
*   PROCEDURE OPTIONS(NOSAVEAREA,   /* DO NOT OBTAIN SAVE AREA       */ 00689000
*       DONTSAVE);                  /* DO NOT SAVE REGISTERS         */ 00690000
SAPRINT1 DS    0H                                                  0256 00691000
*   SA1RET=RETREG;                  /* SAVE RETURN ADDR              */ 00692000
         ST    RETREG,SA1RET                                       0256 00693000
*   ABDUPRXT=ADDR(SAEXIT8);         /* ESTABLISH UPR ROUTINE         */ 00694000
         LA    @10,SAEXIT8                                         0257 00695000
         ST    @10,ABDUPRXT(,ABDARPTR)                             0257 00696000
*   SA15=R15A;                      /* GET POSSIBLE EP ADDR          */ 00697000
