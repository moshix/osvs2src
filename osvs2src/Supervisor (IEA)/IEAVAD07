         TITLE 'IEAVAD07 - SAVE AREAS                                  *00001000
                        '                                               00002000
IEAVAD07 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVAD07  77.049'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @11,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@11                                         0001 00014000
RBPRFXLN EQU 64                                                         00015000
RBPRFLNA EQU 32                                                         00016000
PRBLEN   EQU 136                                                        00017000
SIRBLEN  EQU 200                                                        00018000
TIRBLEN  EQU 136                                                        00019000
IRBLEN   EQU 128                                                        00020000
SVRBLEN  EQU 224                                                        00021000
*                                                            @G33CPHE*/ 00022000
*                                                                  0109 00023000
*   /*****************************************************************/ 00024000
*   /*                                                               */ 00025000
*   /* INITIALIZATION SAVES THE CALLERS REGISTERS IN THE PROVIDED    */ 00026000
*   /* SAVE AREA, BUILDS A CONDITIONAL GETMAIN PARAMETER LIST IN     */ 00027000
*   /* ABDGMA FOR SPACE FOR ITS OWN SAVE AREA AND FOR SPACE FOR THE  */ 00028000
*   /* COMPILERS REQUIREMENTS. IF THE GETMAIN FAILS, REGISTERS ARE   */ 00029000
*   /* RESTORED, GPR 15 IS SET TO 8 AND CONTROL IS RETURNED TO THE   */ 00030000
*   /* CALLER. OTHERWISE, THE SAVE AREAS ARE CHAINED TOGETHER IN     */ 00031000
*   /* STANDARD FASHION.                                             */ 00032000
*   /*                                                               */ 00033000
*   /*****************************************************************/ 00034000
*                                                                  0110 00035000
*   ABDARPTR=PARMPTR;               /* ESTABLISH ADDR OF ABDAREA     */ 00036000
         LR    ABDARPTR,PARMPTR                                    0110 00037000
*   TCBPTR=ABDTCB;                  /* INIT ADDR OF DUMPED TCB       */ 00038000
         L     TCBPTR,ABDTCB(,ABDARPTR)                            0111 00039000
*   PARMPTR=ADDR(ABDASIZE);         /* REG 1 POINTS TO PARM LIST FOR    00040000
*                                      GETMAIN                       */ 00041000
*                                                                  0112 00042000
         LA    PARMPTR,ABDASIZE(,ABDARPTR)                         0112 00043000
*   /*****************************************************************/ 00044000
*   /*                                                               */ 00045000
*   /* GENERATE THE GETMAIN SVC                                      */ 00046000
*   /*                                                               */ 00047000
*   /*****************************************************************/ 00048000
*                                                                  0113 00049000
*   GENERATE;                                                      0113 00050000
*                                                                  0113 00051000
         GETMAIN EC,LV=@ENDDATD-@DATD+72,                              *00052000
               SP=253,                                                 *00053000
               A=ABDGMA(ABDARPTR),                                     *00054000
               MF=(E,(1))                                               00055000
*   /*****************************************************************/ 00056000
*   /*                                                               */ 00057000
*   /* IF SPACE IS NOT AVAILABLE ROUTINE CANNOT FUNCTION             */ 00058000
*   /*                                                               */ 00059000
*   /*****************************************************************/ 00060000
*                                                                  0114 00061000
*   IF RETCODE^=0                   /* DID GETMAIN FAIL              */ 00062000
*     THEN                          /* YES                           */ 00063000
         LTR   RETCODE,RETCODE                                     0114 00064000
         BZ    @RF00114                                            0114 00065000
*     DO;                                                          0115 00066000
*       RETCODE=8;                  /* SET RETURN CODE               */ 00067000
         LA    RETCODE,8                                           0116 00068000
*       RETURN;                     /* RETURN TO IEAVAD01            */ 00069000
@EL00001 DS    0H                                                  0117 00070000
@EF00001 DS    0H                                                  0117 00071000
@ER00001 L     @14,12(,@13)                                        0117 00072000
         LM    @00,@12,20(@13)                                     0117 00073000
         BR    @14                                                 0117 00074000
*     END;                                                         0118 00075000
*   ELSE                                                           0119 00076000
*     ;                             /* CONTINUE PROCESSING           */ 00077000
@RF00114 DS    0H                                                  0120 00078000
*   PARMPTR=ABDAAREA;               /* RETRIEVE ADDR GOTTEN CORE     */ 00079000
         L     PARMPTR,ABDAAREA(,ABDARPTR)                         0120 00080000
*   SAVEREG->NEXTSAVE=PARMPTR;      /* OLD SAVE POINTS TO NEW SAVE 0121 00081000
*                                      AREA                          */ 00082000
         STCM  PARMPTR,7,NEXTSAVE(SAVEREG)                         0121 00083000
*   PARMPTR->PREVSAVE=SAVEREG;      /* NEW SAVE POINTS BACK TO OLD 0122 00084000
*                                      SAVE AREA                     */ 00085000
         STCM  SAVEREG,7,PREVSAVE(PARMPTR)                         0122 00086000
*   SAVEREG=PARMPTR;                /* ESTABLISH ADDR OF NEW SAVE  0123 00087000
*                                      AREA IN REGISTER 13           */ 00088000
*                                                                  0123 00089000
         LR    SAVEREG,PARMPTR                                     0123 00090000
*   /*****************************************************************/ 00091000
*   /*                                                               */ 00092000
*   /* ESTABLISH BASE ADDR FOR TEMPORARY STORAGE                     */ 00093000
*   /*                                                               */ 00094000
*   /*****************************************************************/ 00095000
*                                                                  0124 00096000
*   AUTOPTR=PARMPTR+72;             /* ADDR OF TEMP IS GM AREA+72    */ 00097000
*                                                                  0124 00098000
         LA    AUTOPTR,72                                          0124 00099000
         ALR   AUTOPTR,PARMPTR                                     0124 00100000
*   /*****************************************************************/ 00101000
*   /*                                                               */ 00102000
*   /* GENERATE A USING FOR THE COMPILED CODE AND CLEAR IT           */ 00103000
*   /*                                                               */ 00104000
*   /*****************************************************************/ 00105000
*                                                                  0125 00106000
*   GENERATE;                                                      0125 00107000
*                                                                  0125 00108000
         USING @DATD,AUTOPTR           ESTABLISH ADDRESSABILITY         00109000
         XC    0(@ENDDATD-@DATD,AUTOPTR),0(AUTOPTR) 0 TEMP SPACE        00110000
*   /*****************************************************************/ 00111000
*   /*                                                               */ 00112000
*   /* IF THE TASK BEING DUMPED IS FLAGGED COMPLETE THERE CAN BE NO  */ 00113000
*   /* SAVE AREAS TO DUMP. THEREFORE, THE ROUTINE RETURNS CONTROL TO */ 00114000
*   /* THE CALLER. OTHERWISE THE RB QUEUE IS SCANNED FOR IRBS. FOR   */ 00115000
*   /* EACH IRB, THE ROUTINE DISPLAYS THE PP SAVE AREA ASSOCIATED    */ 00116000
*   /* WITH IT. AFTER THE LAST SUCH SAVE AREA HAS BEEN DISPLAYED,    */ 00117000
*   /* CONTROL PASSES TO THE NEXT SECTION OF THE ROUTINE             */ 00118000
*   /*                                                               */ 00119000
*   /*****************************************************************/ 00120000
*                                                                  0126 00121000
*   SAVE=SAVEMSG1;                  /* SET UP INIT MSG IN DUMP       */ 00122000
         MVC   SAVE(16,ABDARPTR),SAVEMSG1                          0126 00123000
*   PARMPTR=ABDARPTR;               /* SET ADDRESS OF PARAM LIST     */ 00124000
         LR    PARMPTR,ABDARPTR                                    0127 00125000
*   CALL IEAVAD11;                  /* PRINT THE HEADING             */ 00126000
*                                                                  0128 00127000
         L     @15,@CV02411                                        0128 00128000
         BALR  @14,@15                                             0128 00129000
*   /*****************************************************************/ 00130000
*   /*                                                               */ 00131000
*   /* IF THE ROUTINE FAILED FOR ANY REASON KILL THE DUMP            */ 00132000
*   /*                                                               */ 00133000
*   /*****************************************************************/ 00134000
*                                                                  0129 00135000
*   IF RETCODE^=0                   /* OUTPUT FAIL                   */ 00136000
*     THEN                          /* YES                           */ 00137000
         LTR   RETCODE,RETCODE                                     0129 00138000
         BNZ   @RT00129                                            0129 00139000
*     GOTO DIE;                     /* END OF ROUTINE AND DUMP       */ 00140000
*   ELSE                                                           0131 00141000
*     ;                             /* CONTINUE PROCESSING           */ 00142000
*                                                                  0131 00143000
*   /*****************************************************************/ 00144000
*   /*                                                               */ 00145000
*   /* IF THE TASK IS FLAGGED COMPLETE RETURN TO CALLER CODE 0       */ 00146000
*   /*                                                               */ 00147000
*   /*****************************************************************/ 00148000
*                                                                  0132 00149000
*   IF TCBFC='1'B                   /* TASK COMPLETE                 */ 00150000
*     THEN                          /* YES                           */ 00151000
         TM    TCBFC(TCBPTR),B'10000000'                           0132 00152000
         BO    @RT00132                                            0132 00153000
*     GOTO AWAY;                    /* START EXITING                 */ 00154000
*   ELSE                                                           0134 00155000
*     ;                             /* CONTINUE                      */ 00156000
*   RBSECPTR=TCBRBP;                /* ADDR OF TOP RB                */ 00157000
*                                                                  0135 00158000
         L     RBSECPTR,TCBRBP(,TCBPTR)                            0135 00159000
*   /*****************************************************************/ 00160000
*   /*                                                               */ 00161000
*   /* SEARCH FOR IRB. IGNORE ALL OTHERS                             */ 00162000
*   /*                                                               */ 00163000
*   /*****************************************************************/ 00164000
*                                                                  0136 00165000
*TESTIRB:                           /* REENTER FOR EACH IRB          */ 00166000
*   IF RBFTP^='010'B                /* IS RB AN IRB                  */ 00167000
*     THEN                          /* NO                            */ 00168000
TESTIRB  TM    RBFTP(RBSECPTR),B'01000000'                         0136 00169000
         BNO   @RT00136                                            0136 00170000
         TM    RBFTP(RBSECPTR),B'10100000'                         0136 00171000
         BNZ   @RT00136                                            0136 00172000
*     GOTO NEXTRB;                  /* IGNORE THIS RB                */ 00173000
*   ELSE                                                           0138 00174000
*     ;                             /* PROCESS THE IRB               */ 00175000
*   CURSA=RBPPSAV1;                 /* GET ADR PP SAVE YM2693        */ 00176000
         SLR   CURSA,CURSA                                         0139 00177000
         ICM   CURSA,7,RBPPSAV1(RBSECPTR)                          0139 00178000
*   IF CURSA=0                      /* DOES IT EXIST YM2693          */ 00179000
*     THEN                          /* NO YM2693                     */ 00180000
         LTR   CURSA,CURSA                                         0140 00181000
         BZ    @RT00140                                            0140 00182000
*     GOTO NEXTRB;                  /* IGNORE THIS IRB YM2693        */ 00183000
*   CALL SAPRINT3;                  /* PRINT SAVE AREA               */ 00184000
*                                                                  0142 00185000
         BAL   @14,SAPRINT3                                        0142 00186000
*   /*****************************************************************/ 00187000
*   /*                                                               */ 00188000
*   /* IF SUBROUTINE FAILED FOR LACK OF SPACE TERMINATE THE TASK     */ 00189000
*   /*                                                               */ 00190000
*   /*****************************************************************/ 00191000
*                                                                  0143 00192000
*   IF RETCODE=4                    /* OUT OF SPACE                  */ 00193000
*     THEN                          /* YES                           */ 00194000
         CH    RETCODE,@CH00070                                    0143 00195000
         BE    @RT00143                                            0143 00196000
*     GOTO DIE;                     /* END OF DUMP                   */ 00197000
*   ELSE                                                           0145 00198000
*     ;                             /* UPR EVENT DOES NOT KILL THE 0145 00199000
*                                      DUMP. ONLY THE DISPLAY OF THE    00200000
*                                      CURRENT SAVE AREA             */ 00201000
*                                                                  0145 00202000
*   /*****************************************************************/ 00203000
*   /*                                                               */ 00204000
*   /* IRB SEARCH LOOP CONTROL                                       */ 00205000
*   /*                                                               */ 00206000
*   /*****************************************************************/ 00207000
*                                                                  0146 00208000
*NEXTRB:                            /* SKIP RB ENTERS HERE           */ 00209000
*   IF RBTCBNXT='1'B                /* IS THIS OLDEST RB ON QUEUE    */ 00210000
*     THEN                          /* YES                           */ 00211000
NEXTRB   TM    RBTCBNXT(RBSECPTR),B'10000000'                      0146 00212000
         BO    @RT00146                                            0146 00213000
*     GOTO FORTRC;                  /* TO NEXT DISPLAY               */ 00214000
*   ELSE                                                           0148 00215000
*     ;                             /* CONTINUE RB CHECKING          */ 00216000
*   RBSECPTR=RBLINKB;               /* ADDR NEXT RB ON QUEUE         */ 00217000
         SLR   @10,@10                                             0149 00218000
         ICM   @10,7,RBLINKB(RBSECPTR)                             0149 00219000
         LR    RBSECPTR,@10                                        0149 00220000
*   GOTO TESTIRB;                   /* LOOP                          */ 00221000
*                                                                  0150 00222000
         B     TESTIRB                                             0150 00223000
*   /*****************************************************************/ 00224000
*   /*                                                               */ 00225000
*   /* NORMAL COMPLETION OF SAVE AREA DISPLAY                        */ 00226000
*   /*                                                               */ 00227000
*   /*****************************************************************/ 00228000
*                                                                  0151 00229000
*AWAY:                                                             0151 00230000
*   RETCODE=0;                      /* NORMAL COMPLETION             */ 00231000
*                                                                  0151 00232000
AWAY     SLR   RETCODE,RETCODE                                     0151 00233000
*   /*****************************************************************/ 00234000
*   /*                                                               */ 00235000
*   /* ABNORMAL COMPLETION OF DISPLAY                                */ 00236000
*   /*                                                               */ 00237000
*   /*****************************************************************/ 00238000
*                                                                  0152 00239000
*DIE:                                                              0152 00240000
*   SAVERET=RETCODE;                /* SAVE RETURN CODE OVER FREEMAIN*/ 00241000
DIE      LR    SAVERET,RETCODE                                     0152 00242000
*   PARMPTR=SAVEREG;                /* ADDR OF SPACE TO BE FREED     */ 00243000
         LR    PARMPTR,SAVEREG                                     0153 00244000
*   SAVEREG=SAVEREG->PREVSAVE;      /* RESTORE CALLERS SAVE AREA   0154 00245000
*                                      ADDRESS                       */ 00246000
*                                                                  0154 00247000
         SLR   @10,@10                                             0154 00248000
         ICM   @10,7,PREVSAVE(SAVEREG)                             0154 00249000
         LR    SAVEREG,@10                                         0154 00250000
*   /*****************************************************************/ 00251000
*   /*                                                               */ 00252000
*   /* FREE THE SAVE AREA AND DYNAMIC STORAGE                        */ 00253000
*   /*                                                               */ 00254000
*   /*****************************************************************/ 00255000
*                                                                  0155 00256000
*   GENERATE;                                                      0155 00257000
         LA    0,@ENDDATD-@DATD+72     SIZE OF SPACE TO BE FREED        00258000
         ICM   0,8,K253                SUBPOOL NUMBER                   00259000
         SVC   10                      FREEMAIN SVC                     00260000
*   RETCODE=SAVERET;                /* RETSORE RETURN CODE           */ 00261000
         LR    RETCODE,SAVERET                                     0156 00262000
*   RETURN;                         /* RETURN TO MIAINLINE           */ 00263000
*                                                                  0157 00264000
         B     @EL00001                                            0157 00265000
*   /*****************************************************************/ 00266000
*   /*                                                               */ 00267000
*   /* THIS SECTION OF THE MODULE DISPLAYS THE SAVE AREAS STARTING   */ 00268000
*   /* WITH THE SAVE AREA ADDRESS IN TCBFSA. EACH ADDRESS IS VALIDITY*/ 00269000
*   /* CHECKED TO INSURE THAT THE SPACE IS ADDRESSABLE BY THE TASK   */ 00270000
*   /* BEING DUMPED                                                  */ 00271000
*   /*                                                               */ 00272000
*   /*****************************************************************/ 00273000
*                                                                  0158 00274000
*FORTRC:                                                           0158 00275000
*   LASTSA=0;                       /* INIT FIELD AND INDICATE NO  0158 00276000
*                                      BACK CHAIN CHECK              */ 00277000
FORTRC   SLR   @10,@10                                             0158 00278000
         ST    @10,LASTSA                                          0158 00279000
*   SANUM=0;                        /* INIT SAVEAREA NO      @ZA01900*/ 00280000
         STH   @10,SANUM                                           0159 00281000
*   FRSTENT='1'B;                   /* SIGNAL FIRST TIME     @ZA01900*/ 00282000
         OI    FRSTENT,B'10000000'                                 0160 00283000
*   EPINMSG='0'B;                   /* NO EP FOUND YET       @ZA03859*/ 00284000
         NI    EPINMSG,B'01111111'                                 0161 00285000
*   CURSA=TCBFSAB;                  /* SET TO FIRST PP SAVE AREA     */ 00286000
         SLR   CURSA,CURSA                                         0162 00287000
         ICM   CURSA,7,TCBFSAB(TCBPTR)                             0162 00288000
*FORLOOP:                           /* ENTERED FOR EACH SAVE AREA ON    00289000
*                                      THE FORWARD CHAIN             */ 00290000
*                                                                  0163 00291000
*   /*****************************************************************/ 00292000
*   /*                                                               */ 00293000
*   /* TO NEXT SECTION OF THE DISPLAY WHEN THE LAST SAVE AREA ON THE */ 00294000
*   /* FORWARD QUEUE HAS BEEN DISPLAYED                              */ 00295000
*   /*                                                               */ 00296000
*   /*****************************************************************/ 00297000
*                                                                  0163 00298000
*   IF CURSA=0                      /* LAST SAVE AREA DISPLAYED      */ 00299000
*     THEN                          /* YES                           */ 00300000
FORLOOP  LTR   CURSA,CURSA                                         0163 00301000
         BZ    @RT00163                                            0163 00302000
*     GOTO PRBCODE;                 /* NEXT SECTION OF DISPLAY       */ 00303000
*   ELSE                                                           0165 00304000
*     ;                             /* START DISPLAY OF SAVE AREA    */ 00305000
*   CALL VALID;                     /* VALID CHECK SAVE AREA ADDR    */ 00306000
*                                                                  0166 00307000
         BAL   @14,VALID                                           0166 00308000
*   /*****************************************************************/ 00309000
*   /*                                                               */ 00310000
*   /* TEST RESULTS OF VALIDITY CHECK. A CODE OF FOUR MEANS THAT AN  */ 00311000
*   /* INVALID BACK CHAIN WAS DETECTED. A MESSAGE AND THE CONTENTS OF*/ 00312000
*   /* THE SAVE AREA WILL BE DISPLAYED BUT THAT NO MORE SAVE AREAS   */ 00313000
*   /* WILL BE DUMPED. A CODE OF EIGHT MEANS THAT THE SAVE AREA IS   */ 00314000
*   /* NOT ADDRESSABLE AND THE SAVE AREA IS NOT DISPLAYED            */ 00315000
*   /*                                                               */ 00316000
*   /*****************************************************************/ 00317000
*                                                                  0167 00318000
*   IF RETCODE=4                    /* BACK CHAIN INVALID            */ 00319000
*     THEN                          /* YES                           */ 00320000
         CH    RETCODE,@CH00070                                    0167 00321000
         BE    @RT00167                                            0167 00322000
*     GOTO BADBACK;                 /* PROCESS THE CONDITION         */ 00323000
*   ELSE                                                           0169 00324000
*     ;                             /* CONTINUE CHECKS               */ 00325000
*   IF RETCODE=8                    /* NOT ADDRESSABLE               */ 00326000
*     THEN                          /* YES                           */ 00327000
         CH    RETCODE,@CH00072                                    0170 00328000
         BE    @RT00170                                            0170 00329000
*     GOTO PRBCODE;                 /* NEXT SECTION OF DISPLAY       */ 00330000
*   ELSE                                                           0172 00331000
*     ;                             /* DISPLAY THE SAVE AREA         */ 00332000
*   CALL SAPRINT1;                  /* PRINT SAVE AREA               */ 00333000
*                                                                  0173 00334000
         BAL   @14,SAPRINT1                                        0173 00335000
*   /*****************************************************************/ 00336000
*   /*                                                               */ 00337000
*   /* THIS SUBROUTINE RETURNS A CODE OF FOUR IF ABDUMP FAILED FOR   */ 00338000
*   /* LACK OF STORAGE. IF THIS HAPPENS THE DISPLAY AND THE DUMP ARE */ 00339000
*   /* TERMINATED. A CODE OF EIGHT IMPLIES THAT THE SAVE AREA        */ 00340000
*   /* DISAPPEARED WHILE IT WAS BEING DISPLAYED. SINCE THIS IS       */ 00341000
*   /* EQUIVALENT TO IT NOT BEING ADDRESSABLE BEFORE AN ATTEMPT TO   */ 00342000
*   /* DISPLAY IT WAS MADE, THE FORWARD TRACE IS ENDED AND THE NEXT  */ 00343000
*   /* SECTION OF THE DISPLAY IS IS ENTERED                          */ 00344000
*   /*                                                               */ 00345000
*   /*****************************************************************/ 00346000
*                                                                  0174 00347000
*   IF RETCODE=4                    /* ABDUMP FAIL                   */ 00348000
*     THEN                          /* YES                           */ 00349000
         CH    RETCODE,@CH00070                                    0174 00350000
         BE    @RT00174                                            0174 00351000
*     GOTO DIE;                     /* KILL DISPLAY AND EXIT         */ 00352000
*   ELSE                                                           0176 00353000
*     ;                             /* CONTINUE CODE CHECK           */ 00354000
*   IF RETCODE=8                    /* UPR EVENT HAPPEN              */ 00355000
*     THEN                          /* YES                           */ 00356000
         CH    RETCODE,@CH00072                                    0177 00357000
         BE    @RT00177                                            0177 00358000
*     GOTO PRBCODE;                 /* END OF FORWARD TRACE          */ 00359000
*   ELSE                                                           0179 00360000
*     ;                             /* EVERYTHING OK. CONTINUE       */ 00361000
*   LASTSA=CURSA;                   /* LAST ADDR IS CURRENT ADDR     */ 00362000
         ST    CURSA,LASTSA                                        0180 00363000
*   ABDUPRXT=ADDR(PRBCODE);         /* IF UPR THEN NEXT DISPLAY      */ 00364000
         LA    @10,PRBCODE                                         0181 00365000
         ST    @10,ABDUPRXT(,ABDARPTR)                             0181 00366000
*   CURSA=NEXTSAVE;                 /* CURRENT SAVE AREA BECOMES THE    00367000
*                                      NEXT SAVE AREA                */ 00368000
         SLR   @10,@10                                             0182 00369000
         ICM   @10,7,NEXTSAVE(CURSA)                               0182 00370000
         LR    CURSA,@10                                           0182 00371000
*   ABDUPRXT=0;                     /* DISALLOW UPR                  */ 00372000
         SLR   @10,@10                                             0183 00373000
         ST    @10,ABDUPRXT(,ABDARPTR)                             0183 00374000
*   GOTO FORLOOP;                   /* PROCESS NEXT SAVE AREA        */ 00375000
*                                                                  0184 00376000
         B     FORLOOP                                             0184 00377000
*   /*****************************************************************/ 00378000
*   /*                                                               */ 00379000
*   /* WHEN AN INVALID BACK CHAIN IS DETECTED IN THE SAVE AREA CHAIN */ 00380000
*   /* IT PROBABLY MEANS THAT THE CURRENT SAVE AREA IS INVALID. A    */ 00381000
*   /* MESSAGE IS PRINTED INDICATING THE INVALID CHAINING AND THE    */ 00382000
*   /* CONTENTS OF THE SAVE AREA ARE DISPLAYED HOWEVER, NO ATTEMPT IS*/ 00383000
*   /* MADE TO PRODUCE THE WAS ENTERED MESSAGE SINCE THIS WOULD MEAN */ 00384000
*   /* TREATING THE WHATEVER AS A SAVE AREA                          */ 00385000
*   /*                                                               */ 00386000
*   /*****************************************************************/ 00387000
*                                                                  0185 00388000
*BADBACK:                                                          0185 00389000
*   IBC=IBCMSG;                     /* INCORRECT BACK CHAIN MSG      */ 00390000
BADBACK  MVC   IBC(19,ABDARPTR),IBCMSG                             0185 00391000
*   PARMPTR=ABDARPTR;               /* SET ADDRESS OF PARAM LIST     */ 00392000
         LR    PARMPTR,ABDARPTR                                    0186 00393000
*   CALL IEAVAD11;                  /* GO TO PRINT THE MESSAGE       */ 00394000
*                                                                  0187 00395000
         L     @15,@CV02411                                        0187 00396000
         BALR  @14,@15                                             0187 00397000
*   /*****************************************************************/ 00398000
*   /*                                                               */ 00399000
*   /* IF OUTPUT FAILED THEN KILL THE DISPLAY AND THE DUMP           */ 00400000
*   /*                                                               */ 00401000
*   /*****************************************************************/ 00402000
*                                                                  0188 00403000
*   IF RETCODE^=0                   /* ABDUMP FAIL                   */ 00404000
*     THEN                          /* YES                           */ 00405000
         LTR   RETCODE,RETCODE                                     0188 00406000
         BNZ   @RT00188                                            0188 00407000
*     GOTO DIE;                     /* BEGIN TERMINATION             */ 00408000
*   ELSE                                                           0190 00409000
*     ;                                                            0190 00410000
*   CALL SAPRINT2;                  /* DISPLAY SAVE AREA CONTENTS    */ 00411000
*                                                                  0191 00412000
         BAL   @14,SAPRINT2                                        0191 00413000
*   /*****************************************************************/ 00414000
*   /*                                                               */ 00415000
*   /* ONLY CODE THAT IS MEANINGFUL UNDER THE CURRENT CONDITIONS IS  */ 00416000
*   /* FOUR WHICH MEANS THAT ABDUMP FAILED. IF THIS HAPPENS THE      */ 00417000
*   /* DISPLAY AND THE DUMP ARE TERMINATED                           */ 00418000
*   /*                                                               */ 00419000
*   /*****************************************************************/ 00420000
*                                                                  0192 00421000
*   IF RETCODE=4                    /* ABDUMP FAIL                   */ 00422000
*     THEN                          /* YES                           */ 00423000
         CH    RETCODE,@CH00070                                    0192 00424000
         BE    @RT00192                                            0192 00425000
*     GOTO DIE;                     /* KILL DISPLAY AND DUMP         */ 00426000
*   ELSE                                                           0194 00427000
*     ;                             /* ENTER NEXT SECTION OF DISPLAY */ 00428000
*                                                                  0194 00429000
*   /*****************************************************************/ 00430000
*   /*                                                               */ 00431000
*   /* THIS SECTION OF THE ROUTINE DISPLAYS THE TWO MOST RECENT SAVE */ 00432000
*   /* AREAS ASSOCIATED WITH THE MOST RECENT PRB ON THE ACTIVE       */ 00433000
*   /* PROGRAM QUEUE. AGAIN, NO ATTEMPT IS MADE TO DISPLAY THE       */ 00434000
*   /* SUPPOSED SAVE AREAS IF VALIDITY CHECK FAILS                   */ 00435000
*   /*                                                               */ 00436000
*   /*****************************************************************/ 00437000
*                                                                  0195 00438000
*PRBCODE:                                                          0195 00439000
*   RBSECPTR=TCBRBP;                /* GET ADDR MOST RECENT RB       */ 00440000
PRBCODE  L     RBSECPTR,TCBRBP(,TCBPTR)                            0195 00441000
*   SANUM=SANUM-1;                  /* DECREMENT TO LST RB@ZA01900   */ 00442000
         LH    @10,SANUM                                           0196 00443000
         BCTR  @10,0                                               0196 00444000
         STH   @10,SANUM                                           0196 00445000
*RBLOOP:                            /* CONTINUE SEARCH FOR PRB       */ 00446000
*                                                                  0197 00447000
*   /*****************************************************************/ 00448000
*   /*                                                               */ 00449000
*   /* SEARCH FOR MOST RECENT PRB ON Q                               */ 00450000
*   /*                                                               */ 00451000
*   /*****************************************************************/ 00452000
*                                                                  0197 00453000
*   IF RBFTP^='000'B                /* CURRENT RB A PRB              */ 00454000
*     THEN                          /* NO                            */ 00455000
