*   END RBSEARCH;                   /* END SUBROUTINE        @ZA01900*/ 01633000
         B     @EL00004                                            0475 01634000
*                                                                  0476 01635000
*   /*****************************************************************/ 01636000
*   /*                                                               */ 01637000
*   /* SUBROUTINE VALID VALIDITY CHECKS THE ADDRESS OF A POSSIBLE    */ 01638000
*   /* SAVE AREA. AFTER DISABLING, IEA0VL00 IS CALLED TO CHECK THE   */ 01639000
*   /* INITIAL AND LAST ADDRESSES OF THE SAVE AREA TO INSURE THAT THE*/ 01640000
*   /* SPACE WOULD BE ADDRESSABLE BY THE USER. IF NOT, THE ROUTINE   */ 01641000
*   /* RETURNS TO THE CALLER WITH A CODE OF EIGHT. THE ROUTINE THEN  */ 01642000
*   /* CHECKS IF THE SAVE AREA FORWARD POINTER IS EQUAL TO THE SAVE  */ 01643000
*   /* AREA BACK POINTER. IF SO, A CODE OF EIGHT IS RETURNED TO THE  */ 01644000
*   /* CALLER. THEN, IF THE CALLER REQUESTED BACK CHAINING BE        */ 01645000
*   /* CHECKED, THE ROUTINE CHECKS IF THE CURRENT SAVE AREA'S BACK   */ 01646000
*   /* POINTER IS TO THE SAVE AREA POINTED TO BY LASTSA. IF NOT A    */ 01647000
*   /* RETURN CODE OF FOUR IS PASSED TO THE CALLER. OTHERWISE A CODE */ 01648000
*   /* OF ZERO IS GIVEN THE CALLER                                   */ 01649000
*   /*                                                               */ 01650000
*   /*****************************************************************/ 01651000
*                                                                  0476 01652000
*VALID:                             /* START OF SUBROUTINE           */ 01653000
*   PROCEDURE OPTIONS(NOSAVEAREA,   /* PROVIDE NO SAVE AREA          */ 01654000
*       DONTSAVE);                  /* SAVE NO REGISTERS             */ 01655000
VALID    DS    0H                                                  0477 01656000
*   VALIDR14=RETREG;                /* SAVE RETURN ADDRESS           */ 01657000
         ST    RETREG,VALIDR14                                     0477 01658000
*   GENERATE REFS(PSALITA,ABDWORK);                                0478 01659000
*                                        /*                   @Y02705*/ 01660000
         STM   11,14,ABDWORK(ABDARPTR)                          @Y02705 01661000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                 @Y02705C01662000
               RELATED=(SAVEAREA,IEAVAD07(INVALID))             @Y02705 01663000
         LM    11,14,ABDWORK(ABDARPTR)                          @Y02705 01664000
*   RFY                                                            0479 01665000
*    (TCBADDR,                                                     0479 01666000
*     INADDR,                                                      0479 01667000
*     SVWORK) RSTD;                 /*                       @YM05065*/ 01668000
*   INADDR=CURSA;                   /* SET INPUT ADDR FOR 0VL00      */ 01669000
         LR    INADDR,CURSA                                        0480 01670000
*   SVWORK=ABDARPTR;                /* SAVE ADDR OF WKAREA    @Y02705*/ 01671000
         LR    SVWORK,ABDARPTR                                     0481 01672000
