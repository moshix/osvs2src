*   /*****************************************************************/ 00501000
*   /*                                                               */ 00502000
*   /* IF FORMAT FAILED THEN TERMINATE DISPLAY AND DUMP              */ 00503000
*   /*                                                               */ 00504000
*   /*****************************************************************/ 00505000
*                                                                  0212 00506000
*   IF RETCODE^=0                   /* ABDUMP FAIL                   */ 00507000
*     THEN                          /* YES                           */ 00508000
         LTR   RETCODE,RETCODE                                     0212 00509000
         BNZ   @RT00212                                            0212 00510000
*     GOTO DIE;                     /* START TERMINATION             */ 00511000
*   ELSE                                                           0214 00512000
