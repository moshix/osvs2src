*                                                                  0373 01129000
*   /*****************************************************************/ 01130000
*   /*                                                               */ 01131000
*   /* THIS SUBROUTINE DETERMINES IF THE SUPPOSED ENTRY POINT, THE   */ 01132000
*   /* ADDRESS FROM R15 IN THE SAVE AREA, IS CONTAINED WITHIN THE    */ 01133000
*   /* LIMITS OF THE PROGRAM DESCRIBED BY THE RB OR THE LLE SUPPLIED */ 01134000
*   /* BY THE CALLER. IF NOT, RETURN TO THE CALLER WITH A CODE.      */ 01135000
*   /* OTHERWISE, MOVE THE MODULE NAME TO THE MESSAGE BEING BUILT,   */ 01136000
*   /* COMPLETE THE INFORMATION IN THE MESSAGE PERTAINING TO HOW     */ 01137000
*   /* ENTERED AND WRITE THE HEADING. SET RETURN CODE TO CALLER TO   */ 01138000
*   /* ZERO                                                          */ 01139000
*   /*                                                               */ 01140000
*   /*****************************************************************/ 01141000
*                                                                  0373 01142000
*TESTEP1:                           /* START OF SUBROUTINE           */ 01143000
*   PROCEDURE OPTIONS(NOSAVEAREA,   /* DO NOT OBTAIN SAVE AREA       */ 01144000
*       DONTSAVE);                  /* DO NOT SAVE ANY REGISTERS     */ 01145000
TESTEP1  DS    0H                                                  0374 01146000
*   TESTR14=RETREG;                 /* SAVE RETURN ADDRESS           */ 01147000
*                                                                  0374 01148000
         ST    RETREG,TESTR14                                      0374 01149000
*   /*****************************************************************/ 01150000
*   /*                                                               */ 01151000
*   /* IF SUPPLIED RB IS NOT PRB BACK TO CALLER                      */ 01152000
*   /*                                                               */ 01153000
*   /*****************************************************************/ 01154000
*                                                                  0375 01155000
*   IF RBFTP^='000'B                /* RB A PRB                      */ 01156000
*     THEN                          /* NO                            */ 01157000
         TM    RBFTP(RBSECPTR),B'11100000'                         0375 01158000
         BNZ   @RT00375                                            0375 01159000
*     GOTO RC12;                    /* BACK TO CALLER                */ 01160000
*   ELSE                                                           0377 01161000
*     ;                             /* CONTINUE CHECKS               */ 01162000
*   CDENTPTR=RBCDE1;                /* GET ADDR OF CDE FOR PGM       */ 01163000
*                                                                  0378 01164000
         SLR   CDENTPTR,CDENTPTR                                   0378 01165000
         ICM   CDENTPTR,7,RBCDE1(RBSECPTR)                         0378 01166000
*   /*****************************************************************/ 01167000
*   /*                                                               */ 01168000
*   /* IF PRB HAS NO CDE THEN IT CANNOT BE PROGRAM FOR EP            */ 01169000
*   /*                                                               */ 01170000
*   /*****************************************************************/ 01171000
*                                                                  0379 01172000
*   IF CDENTPTR^=0                  /* CDE WITH THE PRB              */ 01173000
*     THEN                          /* YES                           */ 01174000
         LTR   CDENTPTR,CDENTPTR                                   0379 01175000
         BNZ   @RT00379                                            0379 01176000
*     GOTO TEST2A;                  /* CONTINUE CHECKS               */ 01177000
*   ELSE                                                           0381 01178000
*     ;                             /* BACK TO CALLER                */ 01179000
*RC12:                              /* SET WRONG PRB OR LLE CODE     */ 01180000
