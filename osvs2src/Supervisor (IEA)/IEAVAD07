         BE    @RT00312                                            0312 00930000
         CH    RETCODE,@CH00072                                    0312 00931000
         BE    @RT00312                                            0312 00932000
*     GOTO TERMINAL;                /* END OF SUBROUTINE             */ 00933000
*   ELSE                                                           0314 00934000
*     ;                             /* CONTINUE CHECKS               */ 00935000
*                                                                  0314 00936000
*   /*****************************************************************/ 00937000
*   /*                                                               */ 00938000
*   /* A RETURN CODE OF ZERO IMPLIES RIGHT LLE FOUND AND HEADING WAS */ 00939000
*   /* DISPLAYED                                                     */ 00940000
*   /*                                                               */ 00941000
*   /*****************************************************************/ 00942000
*                                                                  0315 00943000
*   IF RETCODE=0                    /* LLE FOUND                     */ 00944000
