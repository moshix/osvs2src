         BZ    @RT00398                                            0398 01274000
*     GOTO RC12;                    /* BACK TO CALLER                */ 01275000
*   ELSE                                                           0400 01276000
*     ;                             /* CHECK CURRENT EXTENT          */ 01277000
*   EXEND=EXSTART+EXSIZE;           /* CALC END ADDR OF EXTENT       */ 01278000
*                                                                  0401 01279000
         L     @10,EXSTARTP                                        0401 01280000
         SLR   @15,@15                                             0401 01281000
         ICM   @15,7,EXSTART(@10)                                  0401 01282000
         L     @10,EXENDP                                          0401 01283000
         SLR   @14,@14                                             0401 01284000
         ICM   @14,7,EXSIZE(@10)                                   0401 01285000
         ALR   @14,@15                                             0401 01286000
         ST    @14,EXEND                                           0401 01287000
*   /*****************************************************************/ 01288000
*   /*                                                               */ 01289000
*   /* IF POSSIBLE ENTRY POINT IS ENTIRELY WITHIN EXTENT THEN THE    */ 01290000
*   /* DESIRED CDE HAS BEEN FOUND                                    */ 01291000
*   /*                                                               */ 01292000
*   /*****************************************************************/ 01293000
*                                                                  0402 01294000
*   IF SA15>=EXSTART&               /* ENTRY ABOVE LOW END           */ 01295000
