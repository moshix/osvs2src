         ST    @10,ABDUPRXT(,ABDARPTR)                             0281 00809000
*MSGDONE1:                          /* ENTERED WHEN UPR OCCURRED     */ 00810000
*   RBSECPTR=TCBRBP;                /* ADDR OF TOP RB                */ 00811000
MSGDONE1 L     RBSECPTR,TCBRBP(,TCBPTR)                            0282 00812000
*CKRBLOOP:                          /* ENTERED FOR EACH RB           */ 00813000
*   CALL TESTEP1;                   /* CHECK RB                      */ 00814000
*                                                                  0283 00815000
CKRBLOOP BAL   @14,TESTEP1                                         0283 00816000
*   /*****************************************************************/ 00817000
*   /*                                                               */ 00818000
*   /* A RETURN CODE OF 4 IMPLIES THAT ABDUMP FAILED. A RETURN CODE  */ 00819000
*   /* OF EIGHT IMPLIES AN UNEXPECTED UPR. IN EITHER CASE THE DISPLAY*/ 00820000
*   /* AND THE DUMP ARE TERMINATED                                   */ 00821000
*   /*                                                               */ 00822000
*   /*****************************************************************/ 00823000
*                                                                  0284 00824000
*   IF RETCODE=4|                   /* ABDUMP FAIL                   */ 00825000
*       RETCODE=8                   /* UPR                           */ 00826000
*     THEN                          /* YES                           */ 00827000
         CH    RETCODE,@CH00070                                    0284 00828000
         BE    @RT00284                                            0284 00829000
         CH    RETCODE,@CH00072                                    0284 00830000
         BE    @RT00284                                            0284 00831000
*     GOTO TERMINAL;                /* END OF DUMP                   */ 00832000
*   ELSE                                                           0286 00833000
*     ;                             /* CHECK IF DESIRED RB FND       */ 00834000
*                                                                  0286 00835000
*   /*****************************************************************/ 00836000
*   /*                                                               */ 00837000
*   /* IF RIGHT RB FOUND THEN BREAK LOOP                             */ 00838000
*   /*                                                               */ 00839000
*   /*****************************************************************/ 00840000
*                                                                  0287 00841000
*   IF RETCODE=0                    /* RIGHT RB FOUND                */ 00842000
*     THEN                          /* YES                           */ 00843000
         LTR   RETCODE,RETCODE                                     0287 00844000
         BZ    @RT00287                                            0287 00845000
*     GOTO CKHEAD;                  /* BREAK LOOP                    */ 00846000
*   ELSE                                                           0289 00847000
*     ;                             /* CONTINUE SEARCH               */ 00848000
*                                                                  0289 00849000
*   /*****************************************************************/ 00850000
*   /*                                                               */ 00851000
*   /* IF LAST RB CHECKED GO TRY LLE                                 */ 00852000
*   /*                                                               */ 00853000
*   /*****************************************************************/ 00854000
*                                                                  0290 00855000
*   IF RBTCBNXT='1'B                /* END OF RB CHAIN REACHED       */ 00856000
*     THEN                          /* YES                           */ 00857000
         TM    RBTCBNXT(RBSECPTR),B'10000000'                      0290 00858000
         BO    @RT00290                                            0290 00859000
*     GOTO TRYLLE;                  /* SEARCH LLE FOR ENTRY POINT    */ 00860000
*   ELSE                                                           0292 00861000
*     ;                             /* TRY NEXT RB                   */ 00862000
*   RBSECPTR=RBLINKB;               /* UPDATE RB POINTER             */ 00863000
         SLR   @10,@10                                             0293 00864000
         ICM   @10,7,RBLINKB(RBSECPTR)                             0293 00865000
         LR    RBSECPTR,@10                                        0293 00866000
*   GOTO CKRBLOOP;                  /* CONTINUE SEARCH               */ 00867000
         B     CKRBLOOP                                            0294 00868000
*SAEXIT8:                           /* A UPR DID NOT OCCUR           */ 00869000
*   ABDUPRXT=0;                     /* DISALLOW UPR                  */ 00870000
SAEXIT8  SLR   @10,@10                                             0295 00871000
         ST    @10,ABDUPRXT(,ABDARPTR)                             0295 00872000
*   RETCODE=8;                      /* INDICATE UPR EVENT            */ 00873000
         LA    RETCODE,8                                           0296 00874000
*TERMINAL:                          /* END OF SUBROUTINE             */ 00875000
*   RETREG=SA1RET;                  /* RESTORE RETURN ADDR           */ 00876000
