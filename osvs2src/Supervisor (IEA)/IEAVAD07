*   LNG=LNG-1;                      /* DECREMENT FOR EXEC OF MVC     */ 00779000
         SLR   @10,@10                                             0276 00780000
         IC    @10,LNG                                             0276 00781000
         BCTR  @10,0                                               0276 00782000
         STC   @10,LNG                                             0276 00783000
*   ATEPPART='AT EP';               /* SET UP MSG                    */ 00784000
         MVC   ATEPPART(5,ABDARPTR),@CC02443                       0277 00785000
*   ABDUPRXT=ADDR(SA1EXITA);        /* ESTABLISH UPR ROUTINE         */ 00786000
         LA    @10,SA1EXITA                                        0278 00787000
         ST    @10,ABDUPRXT(,ABDARPTR)                             0278 00788000
*   EPINMSG='1'B;                   /* EP FOUND FOR MSG      @ZA03859*/ 00789000
*                                                                  0279 00790000
         OI    EPINMSG,B'10000000'                                 0279 00791000
*   /*****************************************************************/ 00792000
*   /*                                                               */ 00793000
*   /* BAL TO MOVE ID                                                */ 00794000
*   /*                                                               */ 00795000
*   /*****************************************************************/ 00796000
*                                                                  0280 00797000
*   GENERATE SETS(DESC);                                           0280 00798000
         SR    15,15            0 COUNT                                 00799000
         IC    15,LNG           GET LENGTH OF ID                        00800000
         L     1,SA15           OBTAIN ADDR OF PGM ENTRY POINT          00801000
         EX    15,MOVEID        MOVE EP ID TO OUTPUT LINE               00802000
         B     MSGDONE          '47F0'X AROUND EXECUTED INSTRUCTION     00803000
MOVEID   MVC   DESC(1,ABDARPTR),5(1)   MOVE ID                          00804000
K253     DC    X'FD00'                 SUBPOOL NUMBER                   00805000
*MSGDONE:                           /* ENTERED IF NO UPR             */ 00806000
*   ABDUPRXT=0;                     /* DISALLOW UPR INTERRUPTS       */ 00807000
