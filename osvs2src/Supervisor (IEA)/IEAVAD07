*     GOTO AWAY;                    /* NORMAL END OF ROUTINE         */ 00644000
*   ELSE                                                           0248 00645000
*     ;                             /* CONTINUE PROCESSING           */ 00646000
*   ABDUPRXT=ADDR(AWAY);            /* EXIT NORMAL IF UPR HAPPENS    */ 00647000
         LA    @10,AWAY                                            0249 00648000
         ST    @10,ABDUPRXT(,ABDARPTR)                             0249 00649000
*   CURSA=PREVSAVE;                 /* SET CURRENT SAVE AREA TO SAVE    00650000
*                                      AREA BACK CHAINED TO CURRENT     00651000
