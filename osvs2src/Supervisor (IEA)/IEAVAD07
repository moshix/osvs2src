*LLELOOP:                           /* REENTER FOR EACH LLE          */ 00885000
*                                                                  0300 00886000
*   /*****************************************************************/ 00887000
*   /*                                                               */ 00888000
*   /* WHEN LAST LLE HAS BEEN CHECKED THEN EP CANNOT BE FOUND AND NO */ 00889000
*   /* HEADING IS DISPLAYED                                          */ 00890000
*   /*                                                               */ 00891000
*   /*****************************************************************/ 00892000
*                                                                  0300 00893000
*   IF LLEPTR=0                     /* AT END OF LLE QUEUE           */ 00894000
*     THEN                          /* YES                           */ 00895000
LLELOOP  LTR   LLEPTR,LLEPTR                                       0300 00896000
         BNZ   @RF00300                                            0300 00897000
*     DO;                           /* LOAD MODULE UNKNOW    @ZA03859*/ 00898000
*       IF EPINMSG='1'B THEN        /* IF EP WAS FOUND       @ZA03859*/ 00899000
         TM    EPINMSG,B'10000000'                                 0302 00900000
         BNO   @RF00302                                            0302 00901000
*         DO;                       /* THEN PUT OUT MSG      @ZA03859*/ 00902000
*           MODNAME='UNKNOWN ';     /* SET NAME IN MSG       @ZA03859*/ 00903000
