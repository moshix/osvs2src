*   /*****************************************************************/ 00563000
*   /*                                                               */ 00564000
*   /* ONLY MEANINGFUL RETURN CODE IS EIGHT WHICH IMPLIES THAT THE   */ 00565000
*   /* SAVE AREA IS NOT ADDRESSABLE. NO DISPLAY IS MADE BUT NORMAL   */ 00566000
*   /* RETURN IS TAKEN                                               */ 00567000
*   /*                                                               */ 00568000
*   /*****************************************************************/ 00569000
*                                                                  0228 00570000
*   IF RETCODE^=0                   /* INVALID ADDRESS               */ 00571000
*     THEN                          /* YES                           */ 00572000
         SLR   @14,@14                                             0228 00573000
         CR    RETCODE,@14                                         0228 00574000
         BNE   @RT00228                                            0228 00575000
*     GOTO AWAY;                    /* NORMAL COMPLETION             */ 00576000
*   ELSE                                                           0230 00577000
*     ;                             /* DISPLAY SAVE AREA             */ 00578000
*   COUNT=COUNT-1;                  /* DECREMENT LOOP CONTROL        */ 00579000
*                                                                  0231 00580000
         LH    @10,COUNT                                           0231 00581000
         BCTR  @10,0                                               0231 00582000
         STH   @10,COUNT                                           0231 00583000
*   /*****************************************************************/ 00584000
*   /*                                                               */ 00585000
*   /* BEFORE DISPLAYING FIRST SAVE AREA PRINT THE PROCEEDING BACK   */ 00586000
*   /* MESSAGE                                                       */ 00587000
*   /*                                                               */ 00588000
*   /*****************************************************************/ 00589000
*                                                                  0232 00590000
*   IF COUNT^=0                     /* FIRST DISPLAY                 */ 00591000
*     THEN                          /* YES                           */ 00592000
         CR    @10,@14                                             0232 00593000
         BE    @RF00232                                            0232 00594000
*     DO;                           /* PRINT HEADING                 */ 00595000
*       PB=PBMSG;                   /* SET MESSAGE IN LINE           */ 00596000
         MVC   PB(27,ABDARPTR),PBMSG                               0234 00597000
*       PARMPTR=ABDARPTR;           /* SET ADDRESS OF PARAM LIST     */ 00598000
         LR    PARMPTR,ABDARPTR                                    0235 00599000
*       CALL IEAVAD11;              /* PRINT THE LINE                */ 00600000
*                                                                  0236 00601000
         L     @15,@CV02411                                        0236 00602000
         BALR  @14,@15                                             0236 00603000
*       /*************************************************************/ 00604000
*       /*                                                           */ 00605000
*       /* IF OUTPUT FAILED KILL DISPLAY AND DUMP                    */ 00606000
*       /*                                                           */ 00607000
*       /*************************************************************/ 00608000
*                                                                  0237 00609000
*       IF RETCODE^=0               /* ABDUMP FAILURE                */ 00610000
*         THEN                      /* YES                           */ 00611000
         LTR   RETCODE,RETCODE                                     0237 00612000
         BNZ   @RT00237                                            0237 00613000
*         GOTO DIE;                 /* KILL DUMP AND DISPLAY         */ 00614000
*       ELSE                                                       0239 00615000
*         ;                         /* END OF HEADING LOGIC          */ 00616000
*     END;                                                         0240 00617000
*   ELSE                                                           0241 00618000
*     ;                             /* DISPLAY SAVE AREA             */ 00619000
@RF00232 DS    0H                                                  0242 00620000
*   CALL SAPRINT1;                  /* PRINT SAVE AREA DATA          */ 00621000
*                                                                  0242 00622000
         BAL   @14,SAPRINT1                                        0242 00623000
*   /*****************************************************************/ 00624000
*   /*                                                               */ 00625000
*   /* RETURN CODES ARE AS BEFORE. A FOUR MEANS ABDUMP FAILED        */ 00626000
*   /* SOMEWHERE. THE DISPLAY AND DUMP WILL BE TERMINATED. EIGHT     */ 00627000
*   /* MEANS THE SAVE AREA DISAPPEARED. THE DISPLAY WILL BE STOPPED  */ 00628000
*   /* BUT RETURN TO CALLER IS NORMAL                                */ 00629000
*   /*                                                               */ 00630000
*   /*****************************************************************/ 00631000
*                                                                  0243 00632000
*   IF RETCODE=4                    /* ABDUMP FAILURE                */ 00633000
*     THEN                          /* YES                           */ 00634000
         CH    RETCODE,@CH00070                                    0243 00635000
         BE    @RT00243                                            0243 00636000
*     GOTO DIE;                     /* KILL DISPLAY AND DUMP         */ 00637000
*   ELSE                                                           0245 00638000
*     ;                             /* CHECK FOR BAD SAVE AREA       */ 00639000
*   IF RETCODE=8                    /* SAVE AREA BECOME BAD          */ 00640000
*     THEN                          /* YES                           */ 00641000
