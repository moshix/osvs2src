         BNZ   @DL00149                                            0164 00317000
*         END;                      /* END... MULTI TABLES   @G33SPHW*/ 00318000
*       PARMPTR=ADDR(ABDASIZE);     /* SET UP FREEMAIN LIST REG      */ 00319000
@RF00144 LA    PARMPTR,ABDASIZE(,ABDARPTR)                         0166 00320000
*       ABDAAREA=SAVEPTR;           /* SET UP ADDRESS TO FREE        */ 00321000
         ST    SAVEPTR,ABDAAREA(,ABDARPTR)                         0167 00322000
*       SAVEPTR=SAVEPTR->PRESAPTR;  /* SET UP PREVIOUS SAVE AREA     */ 00323000
