*       NEXSAPTR=0;                 /* CLEAR POINTER TO AREA TO BE 0169 00325000
*                                      FREED                         */ 00326000
         SLR   @08,@08                                             0169 00327000
         ST    @08,NEXSAPTR(,SAVEPTR)                              0169 00328000
*       GENERATE;                                                  0170 00329000
         FREEMAIN E,LV=@ENDDATD-@DATD+72+3208,                         *00330000
               SP=253,                                                 *00331000
               A=ABDGMA(ABDARPTR),                                     *00332000
               MF=(E,(1))                                               00333000
*     END;                                                         0171 00334000
*   RETCODE=SAVERET;                /* SET UP RETURN CODE            */ 00335000
@RC00072 LR    RETCODE,SAVERET                                     0172 00336000
*   RETURN;                                                        0173 00337000
@EL00001 DS    0H                                                  0173 00338000
