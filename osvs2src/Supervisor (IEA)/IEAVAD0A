         TITLE 'IEAVAD0A - SQA,LSQA DISPLAY                            *00001000
                        '                                               00002000
IEAVAD0A CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVAD0A  77.077'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @06,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@06                                         0001 00014000
*                                                                  0068 00015000
*/*                    END OF TCB                                    */ 00016000
*                                                                  0069 00017000
*                                                                  0069 00018000
*   /*****************************************************************/ 00019000
*   /*                                                               */ 00020000
*   /* INITIALIZATION ON ENTRY FIRST THE ADDRESSING REGISTERS ARE    */ 00021000
*   /* INITIALIZED, THEN A SAVE AREA FOR THE REGISTER SAVE AREA AND  */ 00022000
*   /* AUTOMATIC DATA AREA IS OBTAINED FROM LSQA. THE ADDRESS OF THE */ 00023000
*   /* AREA IS RETURNED IN ABDGMA, AND THE ADDRESS OF THE GETMAIN    */ 00024000
*   /* PARAMETER LIST IS PASSED IN REGISTER 1. NEXT THE SAVE AREAS   */ 00025000
*   /* ARE CHAINED, AND THE TEMPORARY WORK AREA ZTEMPS IS CLEARED TO */ 00026000
*   /* ZEROS SO THAT THE BOUNDARY ADJUSTMENT MOVES CAN BE DONE       */ 00027000
*   /* PROPERLY.                                                     */ 00028000
*   /*                                                               */ 00029000
*   /*****************************************************************/ 00030000
*                                                                  0069 00031000
*   ABDARPTR=PARMPTR;               /* THE ABDAREA SAVE AREA IS    0069 00032000
*                                      INITIALIZED FROM THE VALUE IN    00033000
*                                      REGISTER 1                    */ 00034000
         LR    ABDARPTR,PARMPTR                                    0069 00035000
*   PARMPTR=ADDR(ABDASIZE);         /* REGISTER 1 IS SET TO POINT TO    00036000
*                                      THE GETMAIN PARAMETER LIST    */ 00037000
         LA    PARMPTR,ABDASIZE(,ABDARPTR)                         0070 00038000
*   GENERATE;                                                      0071 00039000
         GETMAIN EC,LV=@ENDDATD-@DATD+72+3208,                         *00040000
               SP=253,                                                 *00041000
               A=ABDGMA(ABDARPTR),                                     *00042000
               MF=(E,(1))                                               00043000
*   IF RETCODE^=0 THEN              /* GETMAIN SUCCESSFUL            */ 00044000
         LTR   RETCODE,RETCODE                                     0072 00045000
         BZ    @RF00072                                            0072 00046000
*     SAVERET=8;                    /* NO, SET RETURN CODE           */ 00047000
         LA    SAVERET,8                                           0073 00048000
*   ELSE                                                           0074 00049000
*     DO;                                                          0074 00050000
         B     @RC00072                                            0074 00051000
@RF00072 DS    0H                                                  0075 00052000
*       PARMPTR=ABDAAREA;           /* USE PARMPTR AS TEMPORARY    0075 00053000
*                                      POINTER TO SAVE AREA          */ 00054000
         L     PARMPTR,ABDAAREA(,ABDARPTR)                         0075 00055000
*       NEXSAPTR=PARMPTR;           /* INITIALIZE NEXT SAVE AREA   0076 00056000
*                                      POINTER IN OLD SAVE AREA      */ 00057000
         ST    PARMPTR,NEXSAPTR(,SAVEPTR)                          0076 00058000
*       PRESAPTR=SAVEPTR;           /* INITIALIZE PREVIOUS SAVE AREA    00059000
*                                      POINTER IN NEW AREA           */ 00060000
         ST    SAVEPTR,PRESAPTR(,PARMPTR)                          0077 00061000
*       SAVEPTR=PARMPTR;            /* SET SAVE REGISTER TO NEW SAVE    00062000
*                                      AREA ADDRESS                  */ 00063000
         LR    SAVEPTR,PARMPTR                                     0078 00064000
*       RFY                                                        0079 00065000
*         R5 RSTD;                                                 0079 00066000
*       R5=SAVEPTR+72;              /* TABLE STARTS AT END OF      0080 00067000
*                                      SAVEAREA STORAGE      @G33SPHW*/ 00068000
         LA    R5,72                                               0080 00069000
         ALR   R5,SAVEPTR                                          0080 00070000
*       AUTOPTR=R5+TABSZ;           /* DATA AREA BEGINS AT END OF  0081 00071000
*                                      STORAGE TABLE         @G33SPHW*/ 00072000
         LA    AUTOPTR,3208                                        0081 00073000
         ALR   AUTOPTR,R5                                          0081 00074000
*       GENERATE;                                                  0082 00075000
         USING @DATD,AUTOPTR       ESTABLISH ADDRESSABILITY             00076000
         XC 0(@ENDDATD-@DATD,AUTOPTR),0(AUTOPTR) ZERO STORAGE           00077000
*       FRSTTABL=R5;                /* INITIALIZE TABLE ANCHOR     0083 00078000
*                                                            @G33SPHW*/ 00079000
         ST    R5,FRSTTABL                                         0083 00080000
*       STORPTR=FRSTTABL;           /* INITIALIZE TABLE POINTER    0084 00081000
*                                                            @G33SPHW*/ 00082000
         L     @08,FRSTTABL                                        0084 00083000
         ST    @08,STORPTR                                         0084 00084000
*       FWDPTR=0;                   /* INITIALIZE FORWARD POINTER TO    00085000
*                                      ZERO                  @G33SPHW*/ 00086000
         SLR   @07,@07                                             0085 00087000
         ST    @07,FWDPTR(,@08)                                    0085 00088000
*       BWDPTR=0;                   /* INITIALIZE BACKWARD POINTER TO   00089000
*                                      ZERO                  @G33SPHW*/ 00090000
         ST    @07,BWDPTR(,@08)                                    0086 00091000
*       RFY                                                        0087 00092000
*         R5 UNRSTD;                /*                       @G33SPHW*/ 00093000
*       ASCBPTR=PSAAOLD;            /* GET PTR TO CURRENT ASCB     0088 00094000
*                                                            @G33SPHW*/ 00095000
         MVC   ASCBPTR(4),PSAAOLD                                  0088 00096000
*       PARMPTR=ABDARPTR;           /* INITIALIZE PARAMETER REG      */ 00097000
         LR    PARMPTR,ABDARPTR                                    0089 00098000
*       SAVERET=0;                  /* INIT RETURN CODE              */ 00099000
         SLR   SAVERET,SAVERET                                     0090 00100000
*       ENTLIMIT=0;                 /* INITIALIZE TOTAL ENTRY      0091 00101000
*                                      COUNTER.              @G33SPHW*/ 00102000
         STH   @07,ENTLIMIT                                        0091 00103000
*       CTR=0;                      /* INITIALIZE CURRENT ENTRY    0092 00104000
*                                      COUNTER.              @G33SPHW*/ 00105000
         STH   @07,CTR                                             0092 00106000
*       IF TCBPTR=ABDTCB THEN       /* DUMP OF CURRENT TASK          */ 00107000
         C     TCBPTR,ABDTCB(,ABDARPTR)                            0093 00108000
         BNE   @RF00093                                            0093 00109000
*         DO;                       /* YES                           */ 00110000
*           IF TCBLTC^=0&           /* DOES TASK HAVE DAUGHTER       */ 00111000
*               TCBLTC->TCBJSTCB=TCBLTC/* WHICH IS A JOB STEP TASK   */ 00112000
*             THEN                                                 0095 00113000
         L     @08,TCBLTC(,TCBPTR)                                 0095 00114000
         CR    @08,@07                                             0095 00115000
         BE    @RF00095                                            0095 00116000
         C     @08,TCBJSTCB(,@08)                                  0095 00117000
         BE    @RT00095                                            0095 00118000
*             ;                     /* YES, DO NOT SET TASK ND       */ 00119000
*           ELSE                                                   0097 00120000
*             DO;                                                  0097 00121000
@RF00095 DS    0H                                                  0098 00122000
*               TCBADMP='1'B;       /* INDICATE TASK REQUESTING    0098 00123000
*                                                              STATUS*/ 00124000
         OI    TCBADMP(TCBPTR),B'00000010'                         0098 00125000
*               ABDSTAT='1'B;       /* IND STATUS ISSUED     @YM06195*/ 00126000
         OI    ABDSTAT(ABDARPTR),B'00000010'                       0099 00127000
*               GENERATE;                                          0100 00128000
          STATUS SET,ND,STEP,(1)   SET JOB NON-DISPATCHABLE             00129000
*             END;                                                 0101 00130000
*         END;                                                     0102 00131000
@RT00095 DS    0H                                                  0103 00132000
*       IF ABDOLSQA='1'B THEN       /* LSQA REQUESTED        @G33SPHW*/ 00133000
@RF00093 TM    ABDOLSQA(ABDARPTR),B'00100000'                      0103 00134000
         BNO   @RF00103                                            0103 00135000
*         DO;                       /* YES                   @G33SPHW*/ 00136000
*                                                                  0104 00137000
*           /*********************************************************/ 00138000
*           /*                                                       */ 00139000
*           /* THIS SECTION OF CODE DISPLAYES LSQA                   */ 00140000
*           /*                                                       */ 00141000
*           /*********************************************************/ 00142000
*                                                                  0105 00143000
*           PARMPTR=ABDARPTR;       /* SET R1 TO ABDA ADDRESS      0105 00144000
*                                                            @G33SPHW*/ 00145000
         LR    PARMPTR,ABDARPTR                                    0105 00146000
*           LDAPTR=ASCBLDA;         /* GET PTR TO LDA                */ 00147000
         L     @08,ASCBPTR                                         0106 00148000
         L     @08,ASCBLDA(,@08)                                   0106 00149000
         ST    @08,LDAPTR                                          0106 00150000
*           IF LSQAPTR^=0 THEN      /* DOES SPQE EXIST FOR LSQA      */ 00151000
         ICM   @07,15,LSQAPTR(@08)                                 0107 00152000
         BZ    @RF00107                                            0107 00153000
*             DO;                   /* YES                           */ 00154000
*               ABDLINEA(1:15)=LSQAMSG;/* SET UP LSQA HEADING        */ 00155000
         MVC   ABDLINEA(15,ABDARPTR),LSQAMSG                       0109 00156000
*               CALL IEAVAD11;      /* PUT OUT HEADING               */ 00157000
         L     @15,@CV00059                                        0110 00158000
         BALR  @14,@15                                             0110 00159000
*               SPQESPTR=LSQAPTR;   /* GET SPQE PTR FOR LSQA         */ 00160000
         L     @08,LDAPTR                                          0111 00161000
         MVC   SPQESPTR(4),LSQAPTR(@08)                            0111 00162000
*               CALL D0ADSPLY;      /* BUILD STORAGE TABLE           */ 00163000
         BAL   @14,D0ADSPLY                                        0112 00164000
*               CALL D0ASORT;       /* SORT STORAGE TABLE    @G33SPHW*/ 00165000
         BAL   @14,D0ASORT                                         0113 00166000
*               CALL D0APRNT;       /* DISPLAY STORAGE       @G33SPHW*/ 00167000
         BAL   @14,D0APRNT                                         0114 00168000
*             END;                                                 0115 00169000
*         END;                                                     0116 00170000
@RF00107 DS    0H                                                  0117 00171000
*       IF ABDSQA='1'B THEN         /* SQA REQUESTED                 */ 00172000
@RF00103 TM    ABDSQA(ABDARPTR),B'00010000'                        0117 00173000
         BNO   @RF00117                                            0117 00174000
*         DO;                                                      0118 00175000
*                                                                  0118 00176000
*           /*********************************************************/ 00177000
*           /*                                                       */ 00178000
*           /* THIS SECTION OF CODE DISPLAYS SQA                     */ 00179000
*           /*                                                       */ 00180000
*           /*********************************************************/ 00181000
*                                                                  0119 00182000
*           PARMPTR=ABDARPTR;       /* SET R1 TO ABDA ADDRESS      0119 00183000
*                                                            @G33SPHW*/ 00184000
         LR    PARMPTR,ABDARPTR                                    0119 00185000
*           GDAPTR=CVTGDA;          /* GET ADDR OF GDA               */ 00186000
         L     @08,CVTPTR                                          0120 00187000
         L     @08,CVTGDA(,@08)                                    0120 00188000
         ST    @08,GDAPTR                                          0120 00189000
*           IF SQASPQEP^=0 THEN     /* DOES SPQE EXIST FOR SQA       */ 00190000
         ICM   @07,15,SQASPQEP(@08)                                0121 00191000
         BZ    @RF00121                                            0121 00192000
*             DO;                   /* YES                           */ 00193000
*               ABDSQSDM='1'B;      /* INDICATE SQA BEING DUMPED     */ 00194000
         OI    ABDSQSDM(ABDARPTR),B'00001000'                      0123 00195000
*               ABDLINEA(1:15)=LSQAMSG;/* SET SQA HEADING            */ 00196000
         MVC   ABDLINEA(15,ABDARPTR),LSQAMSG                       0124 00197000
*               ABDLINEA(12)=' ';   /* CHANGE L TO BLANK             */ 00198000
         MVI   ABDLINEA+11(ABDARPTR),C' '                          0125 00199000
*               CALL IEAVAD11;      /* PUT OUT HEADING               */ 00200000
         L     @15,@CV00059                                        0126 00201000
         BALR  @14,@15                                             0126 00202000
*               SPQESPTR=SQASPQEP;  /* GET SPQE PTR FOR SQA          */ 00203000
         L     @08,GDAPTR                                          0127 00204000
         MVC   SPQESPTR(4),SQASPQEP(@08)                           0127 00205000
*               CALL D0ADSPLY;      /* BUILD STORAGE TABLE   @G33SPHW*/ 00206000
         BAL   @14,D0ADSPLY                                        0128 00207000
*               CALL D0ASORT;       /* SORT STORAGE LIST     @G33SPHW*/ 00208000
         BAL   @14,D0ASORT                                         0129 00209000
*               CALL D0APRNT;       /* PRINT STORAGE         @G33SPHW*/ 00210000
         BAL   @14,D0APRNT                                         0130 00211000
*             END;                                                 0131 00212000
*         END;                                                     0132 00213000
@RF00121 DS    0H                                                  0133 00214000
*       IF TCBPTR=ABDTCB THEN       /* WAS DUMP OF CURRENT TCB       */ 00215000
@RF00117 C     TCBPTR,ABDTCB(,ABDARPTR)                            0133 00216000
         BNE   @RF00133                                            0133 00217000
*         DO;                       /* YES                           */ 00218000
*           IF TCBLTC^=0&           /* DOES TASK HAVE DAUGHTER       */ 00219000
*               TCBLTC->TCBJSTCB=TCBLTC/* WHICH IS A JOB STEP TASK   */ 00220000
*             THEN                                                 0135 00221000
         L     @08,TCBLTC(,TCBPTR)                                 0135 00222000
         LTR   @08,@08                                             0135 00223000
         BZ    @RF00135                                            0135 00224000
         C     @08,TCBJSTCB(,@08)                                  0135 00225000
         BE    @RT00135                                            0135 00226000
*             ;                     /* YES, DO NOT SET TASK ND       */ 00227000
*           ELSE                                                   0137 00228000
*             DO;                                                  0137 00229000
@RF00135 DS    0H                                                  0138 00230000
*               ABDSTAT='0'B;       /* IND STATUS RESET      @YM06195*/ 00231000
         NI    ABDSTAT(ABDARPTR),B'11111101'                       0138 00232000
*               GENERATE;                                          0139 00233000
         STATUS RESET,ND,STEP,(1) RESET JOB STEP DISPATCHABLE           00234000
*               TCBADMP='0'B;       /* TURN OFF STATUS REQUESTOR   0140 00235000
*                                      INDICATOR                     */ 00236000
         NI    TCBADMP(TCBPTR),B'11111101'                         0140 00237000
*             END;                                                 0141 00238000
*         END;                                                     0142 00239000
@RT00135 DS    0H                                                  0143 00240000
*                                                                  0143 00241000
*       /*************************************************************/ 00242000
*       /*                                                           */ 00243000
*       /* FREE THE STORAGE USED TO HOLD STORAGE ADDRESSES. THE FIRST*/ 00244000
*       /* TABLE POINTED TO BY FRSTTABL, IS FREED BY EXIT CLEAN UP.  */ 00245000
*       /* IF MORE THAN ONE TABLE WAS AQCQUIRED THEY WILL FREED BY   */ 00246000
*       /* THIS ROUTINE.                                     @G33SPHW*/ 00247000
*       /*                                                           */ 00248000
*       /*************************************************************/ 00249000
*                                                                  0143 00250000
*       STORPTR=FRSTTABL;           /* GET FIRST TABLE ADDRESS     0143 00251000
*                                                            @G33SPHW*/ 00252000
@RF00133 L     @08,FRSTTABL                                        0143 00253000
         ST    @08,STORPTR                                         0143 00254000
*       IF FWDPTR^=0 THEN           /* MORE THAN ONE TABLE   @G33SPHW*/ 00255000
         ICM   @07,15,FWDPTR(@08)                                  0144 00256000
         BZ    @RF00144                                            0144 00257000
*         DO;                       /* YES.. FREE ALL        @G33SPHW*/ 00258000
*           DO WHILE FWDPTR^=0;     /* FIND END OF CHAIN     @G33SPHW*/ 00259000
         B     @DE00146                                            0146 00260000
@DL00146 DS    0H                                                  0147 00261000
*             STORPTR=FWDPTR;       /* NEXT TABLE ADDRESS    @G33SPHW*/ 00262000
         L     @08,STORPTR                                         0147 00263000
         L     @08,FWDPTR(,@08)                                    0147 00264000
         ST    @08,STORPTR                                         0147 00265000
*           END;                    /* END ..DO WHILE LAST TBL     0148 00266000
*                                                            @G33SPHW*/ 00267000
@DE00146 L     @08,STORPTR                                         0148 00268000
         ICM   @07,15,FWDPTR(@08)                                  0148 00269000
         BNZ   @DL00146                                            0148 00270000
*           DO WHILE BWDPTR^=0;     /* PROCESS TBL FROM BACK @G33SPHW*/ 00271000
         B     @DE00149                                            0149 00272000
@DL00149 DS    0H                                                  0150 00273000
*             R0=TABSZ;             /* PUT SIZE OF TABLE IN R0     0150 00274000
*                                                            @G33SPHW*/ 00275000
         LA    R0,3208                                             0150 00276000
*             R1=STORPTR;           /* PUT ADDRESS OF TBL IN R1    0151 00277000
*                                                            @G33SPHW*/ 00278000
         L     @08,STORPTR                                         0151 00279000
         LR    R1,@08                                              0151 00280000
*             STORPTR=BWDPTR;       /* SAVE PREVIOUS TABLE ADDR    0152 00281000
*                                                            @G33SPHW*/ 00282000
         L     @08,BWDPTR(,@08)                                    0152 00283000
         ST    @08,STORPTR                                         0152 00284000
*             FREEAREA=R1;          /* GET AREA TO BE FREED  @G33SPHW*/ 00285000
         ST    R1,FREEAREA                                         0153 00286000
*             DO;                   /* FREEMAIN(RC)                0154 00287000
*                                      LV(R0)A(FREEAREA->I256C)    0154 00288000
*                                      SP(253)                       */ 00289000
*               RESPECIFY                                          0155 00290000
*                (GPR01F,                                          0155 00291000
*                 GPR15F,                                          0155 00292000
*                 GPR00F) RESTRICTED;                              0155 00293000
*               GPR01F=0;           /* REG 1 MUST BE ZERO            */ 00294000
         SLR   GPR01F,GPR01F                                       0156 00295000
*               GPR15F=1;           /* SET TYPE                      */ 00296000
         LA    GPR15F,1                                            0157 00297000
*               GPR00F=R0;          /* LENGTH REQUESTED              */ 00298000
*               GPR01F=ADDR(FREEAREA->I256C);/* SET REGISTER ONE     */ 00299000
         L     @08,FREEAREA                                        0159 00300000
         LR    GPR01F,@08                                          0159 00301000
*               GPR15F=GPR15F+((253)*256);/* SUBPOOL VALUE           */ 00302000
         AL    GPR15F,@CF02135                                     0160 00303000
*               SVC(120);           /* RC/RU-FORM OF GETMAIN         */ 00304000
         SVC   120                                                 0161 00305000
*               RESPECIFY                                          0162 00306000
*                (GPR01F,                                          0162 00307000
*                 GPR15F,                                          0162 00308000
*                 GPR00F) UNRESTRICTED;                            0162 00309000
*             END;                  /* FREEMAIN(RC)                0163 00310000
*                                      LV(R0)A(FREEAREA->I256C)    0163 00311000
*                                      SP(253)                       */ 00312000
*           END;                    /* END.. DO WHILE FREEMAIN     0164 00313000
*                                                            @G33SPHW*/ 00314000
@DE00149 L     @08,STORPTR                                         0164 00315000
