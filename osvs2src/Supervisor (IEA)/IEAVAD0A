*D0ADSPLY:                                                         0174 00343000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0174 00344000
D0ADSPLY DS    0H                                                  0175 00345000
*   SAVE14=RETERN;                  /* SAVE RETURN ADDR              */ 00346000
*                                                                  0175 00347000
         ST    RETERN,SAVE14                                       0175 00348000
*   /*****************************************************************/ 00349000
*   /*                                                               */ 00350000
*   /* OBTAIN LOCAL LOCK WHILE BUILDING THE STORAGE TABLE    @G33SPHW*/ 00351000
*   /*                                                               */ 00352000
*   /*****************************************************************/ 00353000
*                                                                  0176 00354000
*   GEN REFS(CVTPTR,CVTMAP,CVTCRMN,ABDWORK,PSALITA) SETS(R11,R14); 0176 00355000
     STM  R11,R14,ABDWORK(ABDARPTR) SAVE REGS OVER SETLOCK      G33SPHW 00356000
     SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=('IEAVAD0A')         00357000
     LM   R11,R14,ABDWORK(ABDARPTR)  RESTORE REGS AFTER SETLOCK G33SPHW 00358000
*   DQELNSVE=0;                     /* INIT LENGTH SAVE FIELD TO ZERO*/ 00359000
         SLR   @08,@08                                             0177 00360000
         ST    @08,DQELNSVE                                        0177 00361000
*   DQESEPTR=SPDQEPTR;              /* GET FIRST DQE PTR             */ 00362000
