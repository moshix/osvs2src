*     END;                                                         0298 00958000
*   ELSE                                                           0299 00959000
*     ;                             /* CONTINUE HUNT FOR SEGMENT     */ 00960000
@RF00294 DS    0H                                                  0300 00961000
*   SEGLO=DQELO;                    /* SEGMNT IS DEFINED BY DQELO    */ 00962000
