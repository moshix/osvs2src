         TITLE 'IEAVAD10 DISPLAY PSW AND REGISTERS                     *00001000
                        '                                               00002000
IEAVAD10 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVAD10  77.049'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @11,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@11                                         0001 00014000
RBPRFXLN EQU 64                                                         00015000
RBPRFLNA EQU 32                                                         00016000
PRBLEN   EQU 136                                                        00017000
SIRBLEN  EQU 200                                                        00018000
TIRBLEN  EQU 136                                                        00019000
IRBLEN   EQU 128                                                        00020000
SVRBLEN  EQU 224                                                        00021000
*/* **************************************************************   */ 00022000
*/*                                                                  */ 00023000
*/*                 INITIALIZATION ON ENTRY                          */ 00024000
*/*    FIRST THE ADDRESSING REGISTERS ARE INITIALIZED, THEN A        */ 00025000
*/*    SAVEAREA FOR THE REGISTERS AND AUTOMATIC DATA                 */ 00026000
*/*    AREA IS OBTAINED FROM LSQA. THE SAVEAREAS ARE                 */ 00027000
*/*    CHAINED AND THE GETMAINED STORAGE IS CLEARED TO               */ 00028000
*/*    ZEROS.                                                        */ 00029000
*/*                                                                  */ 00030000
*/* **************************************************************   */ 00031000
*                                                                  0058 00032000
*   ABDARPTR=PARMPTR;               /* SAVE PARMLIST ADDRESS         */ 00033000
         LR    ABDARPTR,PARMPTR                                    0058 00034000
*   TCBPTR=ABDTCB;                  /* GET DUMPED TCB ADDRESS      0059 00035000
*                                                            @G33SPHW*/ 00036000
         L     TCBPTR,ABDTCB(,ABDARPTR)                            0059 00037000
*   PARMPTR=ADDR(ABDASIZE);                                        0060 00038000
         LA    PARMPTR,ABDASIZE(,ABDARPTR)                         0060 00039000
*   GENERATE;                                                      0061 00040000
     GETMAIN EC,LV=@ENDDATD-@DATD+72,SP=253,A=ABDGMA(ABDARPTR),        X00041000
               MF=(E,(1))                                               00042000
*   IF RETCODE^=0 THEN              /* GETMAIN FAILED                */ 00043000
         LTR   RETCODE,RETCODE                                     0062 00044000
         BZ    @RF00062                                            0062 00045000
*     DO;                           /* YES ...                       */ 00046000
*       RETCODE=8;                  /* SEND RETURN CODE 8 BACK TO  0064 00047000
*                                      CALLER                        */ 00048000
         LA    RETCODE,8                                           0064 00049000
*       RETURN;                     /* RETURN TO CALLER              */ 00050000
@EL00001 DS    0H                                                  0065 00051000
@EF00001 DS    0H                                                  0065 00052000
@ER00001 L     @14,12(,@13)                                        0065 00053000
         LM    @00,@12,20(@13)                                     0065 00054000
         BR    @14                                                 0065 00055000
*     END;                          /* END... GETMAIN FAILED         */ 00056000
*   ELSE                            /* GETMAIN OK                    */ 00057000
*     DO;                           /*                               */ 00058000
@RF00062 DS    0H                                                  0068 00059000
*       PARMPTR=ABDAAREA;           /* POINT TO SAVEAREA             */ 00060000
         L     PARMPTR,ABDAAREA(,ABDARPTR)                         0068 00061000
*       NEXSAPTR=PARMPTR;           /* NEXT SAVEAREA IN OLD          */ 00062000
         ST    PARMPTR,NEXSAPTR(,SAVEPTR)                          0069 00063000
*       PRESAPTR=SAVEPTR;           /* PREVIOUS SAVEAREA IN NEW      */ 00064000
         ST    SAVEPTR,PRESAPTR(,PARMPTR)                          0070 00065000
*       SAVEPTR=PARMPTR;            /* R13 TO NEW SAVEAREA           */ 00066000
         LR    SAVEPTR,PARMPTR                                     0071 00067000
*       AUTOPTR=SAVEPTR+72;         /* DATA AREA AFTER SAVEAREA      */ 00068000
         LA    AUTOPTR,72                                          0072 00069000
         ALR   AUTOPTR,SAVEPTR                                     0072 00070000
*       GENERATE;                                                  0073 00071000
         USING @DATD,AUTOPTR               GET ADDRESSABILITY           00072000
         XC     0(@ENDDATD-@DATD,AUTOPTR),0(AUTOPTR) ZERO STORAGE       00073000
*       PARMPTR=ABDARPTR;           /* SET R1 TO IHAABDA ADDRESS   0074 00074000
*                                                            @G33SPHW*/ 00075000
         LR    PARMPTR,ABDARPTR                                    0074 00076000
*       IF ABDPSW=ON THEN           /* PSW REQUESTED                 */ 00077000
         TM    ABDPSW(ABDARPTR),B'10000000'                        0075 00078000
         BNO   @RF00075                                            0075 00079000
*         DO;                       /* YES...OUTPUT PSW              */ 00080000
*                                                                  0076 00081000
*           /*********************************************************/ 00082000
*           /*                                                       */ 00083000
*           /* LINE 1- PSW, ILC, INTERRUPT CODE- IF THE PSW WAS      */ 00084000
*           /* SPECIFIED TO BE DUMPED, THE PSW, ILC, AND INTERRUPT   */ 00085000
*           /* CODE ARE DISPLAYED FROM THE RB IN CONTROL AT THE TIME */ 00086000
*           /* OF THE SNAP OR ABEND PSW                              */ 00087000
*           /*                                                       */ 00088000
*           /*********************************************************/ 00089000
*                                                                  0077 00090000
*           ABDLINEA(1:21)=PSWMSG;  /* MOVE IN THE DOUBLE SPACE    0077 00091000
*                                      CHARACTER AND PSW MESSAGE     */ 00092000
         MVC   ABDLINEA(21,ABDARPTR),PSWMSG                        0077 00093000
*           RBSECPTR=ABDCRB;        /* PICK UP ABDUMP SVRB POINTER   */ 00094000
         L     RBSECPTR,ABDCRB(,ABDARPTR)                          0078 00095000
*           RBSECPTR=RBLINK;        /* PICK UP USER RB IF SNAP ABEND    00096000
*                                      SVRB IF ABEND                 */ 00097000
         L     RBSECPTR,RBLINK(,RBSECPTR)                          0079 00098000
*           IF APFABEND=OFF         /* IF ABEND IS CALLER PICK UP    */ 00099000
*             THEN                  /* USER RB AND MOVE IN 'ABEND'   */ 00100000
         TM    APFABEND(ABDARPTR),B'00010000'                      0080 00101000
         BNZ   @RF00080                                            0080 00102000
*             DO;                                                  0081 00103000
*               RBSECPTR=RBLINK;    /* GET USER RB                   */ 00104000
         L     RBSECPTR,RBLINK(,RBSECPTR)                          0082 00105000
*               ABDLINEA(18:22)=ABEND;/* MOVE IN ABEND INSTEAD OF  0083 00106000
*                                      SNAP                          */ 00107000
         MVC   ABDLINEA+17(5,ABDARPTR),ABEND                       0083 00108000
*             END;                  /* END OF ABEND CASE             */ 00109000
*           ABDBPTR=ADDR(RBOPSW);   /* SET UP FOR FORMAT PSW         */ 00110000
@RF00080 LA    @14,RBOPSW(,RBSECPTR)                               0085 00111000
         ST    @14,ABDBPTR(,ABDARPTR)                              0085 00112000
*           ABDLLINE=ADDR(PSWLINE); /* SET UP LAYOUT LINE            */ 00113000
         LA    @14,PSWLINE                                         0086 00114000
         ST    @14,ABDLLINE(,ABDARPTR)                             0086 00115000
*           PARMPTR=ABDARPTR;       /* SET R1 TO ABDA ADDRESS      0087 00116000
*                                                            @G33SPHW*/ 00117000
         LR    PARMPTR,ABDARPTR                                    0087 00118000
*           CALL IEAVAD31;          /* FORMAT FIRST HALF OF PSW      */ 00119000
         L     @15,@CV00065                                        0088 00120000
         BALR  @14,@15                                             0088 00121000
*           IF RETCODE^=0           /* IF ERROR CONDITION            */ 00122000
*             THEN                  /* RETURN AFTER FREE CORE        */ 00123000
         LTR   RETCODE,RETCODE                                     0089 00124000
         BNZ   @RT00089                                            0089 00125000
*             GOTO FREERET;                                        0090 00126000
*           PARMPTR=ABDARPTR;       /* SET R1 TO ABDA ADDRESS      0091 00127000
*                                                            @G33SPHW*/ 00128000
         LR    PARMPTR,ABDARPTR                                    0091 00129000
*           CALL IEAVAD31;          /* FORMAT SECOND HALF OF PSW     */ 00130000
         L     @15,@CV00065                                        0092 00131000
         BALR  @14,@15                                             0092 00132000
*           IF RETCODE^=0           /* IF ERROR CONDITION            */ 00133000
*             THEN                  /* RETURN AFTER FREE CORE        */ 00134000
         LTR   RETCODE,RETCODE                                     0093 00135000
         BNZ   @RT00093                                            0093 00136000
*             GOTO FREERET;                                        0094 00137000
*                                                                  0094 00138000
*           /*********************************************************/ 00139000
*           /*                                                       */ 00140000
*           /* ILC                                                   */ 00141000
*           /*                                                       */ 00142000
*           /*********************************************************/ 00143000
*                                                                  0095 00144000
*           RESPECIFY                                              0095 00145000
*             SAVERET UNRESTRICTED; /* LET COMPILER USE SAVERET FOR     00146000
*                                      RB PREFIX WORK                */ 00147000
*           ABDLINEA(50:52)=ILCMSG; /* SET UP ILC MESSAGE            */ 00148000
         MVC   ABDLINEA+49(3,ABDARPTR),ILCMSG                      0096 00149000
*           ABDLINEA(54)=RBINLNTH;  /* INSERT ILC INTO LINE          */ 00150000
*                                                                  0097 00151000
         LR    @14,RBSECPTR                                        0097 00152000
         SL    @14,@CF00423                                        0097 00153000
         MVC   ABDLINEA+53(1,ABDARPTR),RBINLNTH(@14)               0097 00154000
*           /*********************************************************/ 00155000
*           /*                                                       */ 00156000
*           /* SET UP ZONE BITS FOR ILC                              */ 00157000
*           /*                                                       */ 00158000
*           /*********************************************************/ 00159000
*                                                                  0098 00160000
*           ABDLINEA(54)=ABDLINEA(54)|FOXO;                        0098 00161000
*                                                                  0098 00162000
         SLR   @10,@10                                             0098 00163000
         IC    @10,ABDLINEA+53(,ABDARPTR)                          0098 00164000
         SLR   @08,@08                                             0098 00165000
         IC    @08,FOXO                                            0098 00166000
         OR    @10,@08                                             0098 00167000
         STC   @10,ABDLINEA+53(,ABDARPTR)                          0098 00168000
*           /*********************************************************/ 00169000
*           /*                                                       */ 00170000
*           /* INTERRUPT CODE                                        */ 00171000
*           /*                                                       */ 00172000
*           /*********************************************************/ 00173000
*                                                                  0099 00174000
*           ABDBPTR=ADDR(RBINTCOD); /* POINT TO INTERRUPT CODE       */ 00175000
         LA    @14,RBINTCOD(,@14)                                  0099 00176000
         ST    @14,ABDBPTR(,ABDARPTR)                              0099 00177000
*           RESPECIFY                                              0100 00178000
*             SAVERET RESTRICTED;   /* END OF RB PREFIX WORK GET   0100 00179000
*                                      REGISTER BACK FROM COMPILER   */ 00180000
*           ABDLLINE=ADDR(INTCLINE);/* SET LAYOUT LINE POINTER       */ 00181000
         LA    @14,INTCLINE                                        0101 00182000
         ST    @14,ABDLLINE(,ABDARPTR)                             0101 00183000
*           PARMPTR=ABDARPTR;       /* SET R1 TO ABDA ADDRESS      0102 00184000
*                                                            @G33SPHW*/ 00185000
         LR    PARMPTR,ABDARPTR                                    0102 00186000
*           CALL IEAVAD31;          /* FORMAT THE INTERRUPT CODE     */ 00187000
         L     @15,@CV00065                                        0103 00188000
         BALR  @14,@15                                             0103 00189000
*           IF RETCODE^=0           /* IF ERROR CONDITION            */ 00190000
*             THEN                  /* RETURN AFTER FREE CORE        */ 00191000
         LTR   RETCODE,RETCODE                                     0104 00192000
         BNZ   @RT00104                                            0104 00193000
*             GOTO FREERET;                                        0105 00194000
*           PARMPTR=ABDARPTR;       /* SET R1 TO ABDA ADDRESS      0106 00195000
*                                                            @G33SPHW*/ 00196000
         LR    PARMPTR,ABDARPTR                                    0106 00197000
*           CALL IEAVAD11;          /* OUTPUT LINE 3                 */ 00198000
         L     @15,@CV00064                                        0107 00199000
         BALR  @14,@15                                             0107 00200000
*           RETCODE=0;              /* SET CODE TO 0 FOR EXIT        */ 00201000
         SLR   RETCODE,RETCODE                                     0108 00202000
*         END;                      /* END PSW FORMAT                */ 00203000
*       ELSE                        /* NOT PSW THEN                  */ 00204000
*         DO;                       /* CHECK FOR REGISTERS REQUESTED */ 00205000
         B     @RC00075                                            0110 00206000
@RF00075 DS    0H                                                  0111 00207000
*           IF ABDREGS=ON THEN      /* REGISTERS REQUESTED           */ 00208000
         TM    ABDREGS(ABDARPTR),B'01000000'                       0111 00209000
         BNO   @RF00111                                            0111 00210000
*             DO;                   /* YES... DISPLAY REGS           */ 00211000
*                                                                  0112 00212000
*               /*****************************************************/ 00213000
*               /*                                                   */ 00214000
*               /* THE NEXT SECTION OF CODE OUTPUTS THE FLOATING     */ 00215000
*               /* POINT REGS AND THE GENERAL PURPOSE REGISTERS IF   */ 00216000
*               /* THEY WERE REQUESTED. IF THE TASK BEING DUMPED IS  */ 00217000
*               /* THE CURRENT TASK, THE FLOATING REGISTERS HAVE THE */ 00218000
*               /* VALUES TO BE DUMPED, AND THEY ARE STORED INTO THE */ 00219000
*               /* CURRENT TCB FOR FORMATTING AND OUTPUT. THE GENERAL*/ 00220000
*               /* REGISTERS ARE FOUND IN ABDUMP SVRB IF SNAP CALLED */ 00221000
*               /* ABDUMP OR IN THE ABEND SVRB IF ABEND CALLED       */ 00222000
*               /* ABDUMP. IF THE CURRENT TASK IS NOT BEING DUMPED,  */ 00223000
*               /* THE FLOATING POINT REGISTERS AND GENERAL REGISTERS*/ 00224000
*               /* ARE BOTH FOUND IN THE TCB OF THE TASK BEING       */ 00225000
*               /* DUMPED. THE REGISTERS ARE FORMATTED USING         */ 00226000
*               /* IEAVAD61, FORMAT22, WHICH UNPACKS THE FIELD IN    */ 00227000
*               /* HEX. THE LINES ARE PRINTED USING IEAVAD11, OUTPUT.*/ 00228000
*               /*                                                   */ 00229000
*               /*****************************************************/ 00230000
*                                                                  0113 00231000
*               ABDLINEA(1:23)=REGMESS;/* PUT REGISTER MESSAGE OUT   */ 00232000
         MVC   ABDLINEA(23,ABDARPTR),REGMESS                       0113 00233000
*               IF TCBPTR=ABDCTCB   /* IF TCB IS CURRENT TCB THEN    */ 00234000
*                 THEN              /* USE FLOAT REGS IN REGS        */ 00235000
         C     TCBPTR,ABDCTCB(,ABDARPTR)                           0114 00236000
         BNE   @RF00114                                            0114 00237000
*                 DO;                                              0115 00238000
*                   TCBPTR=TCBPTR-32;/* LOCATE FLOATING REG SAVE AREA*/ 00239000
         SL    TCBPTR,@CF00335                                     0116 00240000
*                   GEN(STD 0,0(TCBPTR));/* STORE FLOAT REG 0        */ 00241000
         STD 0,0(TCBPTR)                                                00242000
*                   GEN(STD 2,8(TCBPTR));/* STORE FLOAT REG 2        */ 00243000
         STD 2,8(TCBPTR)                                                00244000
*                   GEN(STD 4,16(TCBPTR));/* STORE FLOAT REG 4       */ 00245000
         STD 4,16(TCBPTR)                                               00246000
*                   GEN(STD 6,24(TCBPTR));/* STORE FLOAT REG 6       */ 00247000
         STD 6,24(TCBPTR)                                               00248000
*                   TCBPTR=ABDTCB;  /* RESTORE TCB REG               */ 00249000
         L     TCBPTR,ABDTCB(,ABDARPTR)                            0121 00250000
*                   RBSECPTR=ABDCRB;/* GET ABDUMP SVRB POINTER       */ 00251000
         L     RBSECPTR,ABDCRB(,ABDARPTR)                          0122 00252000
*                   IF APFABEND=OFF /* IF ABEND IS THE CALLER        */ 00253000
*                     THEN          /* USE ABEND SPECIAL CODE        */ 00254000
         TM    APFABEND(ABDARPTR),B'00010000'                      0123 00255000
         BNZ   @RF00123                                            0123 00256000
*                     DO;                                          0124 00257000
*                                                                  0124 00258000
*                       /*********************************************/ 00259000
*                       /*                                           */ 00260000
*                       /* OVERLAY SNAP WITH ABEND                   */ 00261000
*                       /*                                           */ 00262000
*                       /*********************************************/ 00263000
*                                                                  0125 00264000
*                       ABDLINEA(19:23)=ABENDMSG;                  0125 00265000
*                                                                  0125 00266000
         MVC   ABDLINEA+18(5,ABDARPTR),ABENDMSG                    0125 00267000
*                       /*********************************************/ 00268000
*                       /*                                           */ 00269000
*                       /* GET ABEND RB PTR                          */ 00270000
*                       /*                                           */ 00271000
*                       /*********************************************/ 00272000
*                                                                  0126 00273000
*                       RBSECPTR=RBLINKB;                          0126 00274000
         L     @14,RBLINKB-1(,RBSECPTR)                            0126 00275000
         LA    @14,0(,@14)                                         0126 00276000
         LR    RBSECPTR,@14                                        0126 00277000
*                     END;                                         0127 00278000
*                                                                  0127 00279000
*                   /*************************************************/ 00280000
*                   /*                                               */ 00281000
*                   /* SET UP POINTER TO GPRS TO                     */ 00282000
*                   /*                                               */ 00283000
*                   /*************************************************/ 00284000
*                                                                  0128 00285000
*                   REGPTR=ADDR(RBGRSAVE);/* PRINT                   */ 00286000
@RF00123 LA    REGPTR,RBGRSAVE(,RBSECPTR)                          0128 00287000
*                 END;                                             0129 00288000
*               ELSE                /* USE TCB REGS                  */ 00289000
*                 REGPTR=ADDR(TCBGRS);                             0130 00290000
         B     @RC00114                                            0130 00291000
@RF00114 LA    REGPTR,TCBGRS(,TCBPTR)                              0130 00292000
*               PARMPTR=ABDARPTR;   /* SET R1 TO ABDA ADDRESS      0131 00293000
*                                                            @G33SPHW*/ 00294000
@RC00114 LR    PARMPTR,ABDARPTR                                    0131 00295000
*               CALL IEAVAD11;      /* OUTPUT THE REG HEADING        */ 00296000
         L     @15,@CV00064                                        0132 00297000
         BALR  @14,@15                                             0132 00298000
*               IF RETCODE^=0       /* IF ERRED                      */ 00299000
*                 THEN              /* LEAVE WITH                    */ 00300000
         LTR   RETCODE,RETCODE                                     0133 00301000
         BNZ   @RT00133                                            0133 00302000
*                 GOTO FREERET;     /* BAD CODE                      */ 00303000
*               ABDCC=ZIP;          /* SET UP DOUBLE SPACE           */ 00304000
         MVI   ABDCC(ABDARPTR),X'00'                               0135 00305000
*               ABDLINEA(7:14)=FLOATMES;/* SET UP FLOATING REG     0136 00306000
*                                      MESSAGE                       */ 00307000
         MVC   ABDLINEA+6(8,ABDARPTR),FLOATMES                     0136 00308000
*               ABDBPTR=TCBPTR-32;  /* POINT TO FLOAT REG SAVE       */ 00309000
         LR    @14,TCBPTR                                          0137 00310000
         SL    @14,@CF00335                                        0137 00311000
         ST    @14,ABDBPTR(,ABDARPTR)                              0137 00312000
*               ABDLLINE=ADDR(FLOATLIN);/* POINT TO LAYOUT LINE      */ 00313000
         LA    @14,FLOATLIN                                        0138 00314000
         ST    @14,ABDLLINE(,ABDARPTR)                             0138 00315000
*               PARMPTR=ABDARPTR;   /* SET R1 TO ABDA ADDRESS      0139 00316000
*                                                            @G33SPHW*/ 00317000
         LR    PARMPTR,ABDARPTR                                    0139 00318000
*               CALL IEAVAD61;      /* UNPACK THE FLOAT REGS         */ 00319000
         L     @15,@CV00066                                        0140 00320000
         BALR  @14,@15                                             0140 00321000
*               IF RETCODE^=0       /* IF ERRED                      */ 00322000
*                 THEN              /* LEAVE WITH                    */ 00323000
         LTR   RETCODE,RETCODE                                     0141 00324000
         BNZ   @RT00141                                            0141 00325000
*                 GOTO FREERET;     /* BAD CODE                      */ 00326000
*               PARMPTR=ABDARPTR;   /* SET R1 TO ABDA ADDRESS      0143 00327000
*                                                            @G33SPHW*/ 00328000
         LR    PARMPTR,ABDARPTR                                    0143 00329000
*               CALL IEAVAD11;      /* OUTPUT THE FLOAT REGS         */ 00330000
         L     @15,@CV00064                                        0144 00331000
         BALR  @14,@15                                             0144 00332000
*               IF RETCODE^=0       /* IF ERRED                      */ 00333000
*                 THEN              /* LEAVE WITH                    */ 00334000
         LTR   RETCODE,RETCODE                                     0145 00335000
         BNZ   @RT00145                                            0145 00336000
*                 GOTO FREERET;     /* BAD CODE                      */ 00337000
*               ABDCC=ZIP;          /* SET TO DOUBLE SPACE           */ 00338000
         MVI   ABDCC(ABDARPTR),X'00'                               0147 00339000
*               ABDLINEA(7:14)=REG1MESS;/* SET IN REGISTER MESSAGE   */ 00340000
         MVC   ABDLINEA+6(8,ABDARPTR),REG1MESS                     0148 00341000
*               ABDBPTR=REGPTR;     /* SET UP TO FORMAT REGS         */ 00342000
         ST    REGPTR,ABDBPTR(,ABDARPTR)                           0149 00343000
*               ABDLLINE=ADDR(LAYOUT);/* INITIALIZE LAYOUT LINE      */ 00344000
         LA    @14,LAYOUT                                          0150 00345000
         ST    @14,ABDLLINE(,ABDARPTR)                             0150 00346000
*               PARMPTR=ABDARPTR;   /* SET R1 TO ABDA ADDRESS      0151 00347000
*                                                            @G33SPHW*/ 00348000
         LR    PARMPTR,ABDARPTR                                    0151 00349000
*               CALL IEAVAD61;      /* UNPACK THE REGS 0-7           */ 00350000
         L     @15,@CV00066                                        0152 00351000
         BALR  @14,@15                                             0152 00352000
*               IF RETCODE^=0       /* IF ERRED                      */ 00353000
*                 THEN              /* LEAVE WITH                    */ 00354000
         LTR   RETCODE,RETCODE                                     0153 00355000
         BNZ   @RT00153                                            0153 00356000
*                 GOTO FREERET;     /* BAD CODE                      */ 00357000
*               PARMPTR=ABDARPTR;   /* SET R1 TO ABDA ADDRESS      0155 00358000
*                                                            @G33SPHW*/ 00359000
         LR    PARMPTR,ABDARPTR                                    0155 00360000
*               CALL IEAVAD11;      /* OUTPUT REGS 0-7               */ 00361000
         L     @15,@CV00064                                        0156 00362000
         BALR  @14,@15                                             0156 00363000
*               IF RETCODE^=0       /* IF ERRED                      */ 00364000
*                 THEN              /* LEAVE WITH                    */ 00365000
         LTR   RETCODE,RETCODE                                     0157 00366000
         BNZ   @RT00157                                            0157 00367000
*                 GOTO FREERET;     /* BAD CODE                      */ 00368000
*               ABDLINEA(7:15)=REG2MESS;/* SET UP 8-15 MESSAGE       */ 00369000
         MVC   ABDLINEA+6(9,ABDARPTR),REG2MESS                     0159 00370000
*               ABDLLINE=ADDR(LAYOUT);/* SET UP LAYOUT LINE PTR      */ 00371000
         LA    @14,LAYOUT                                          0160 00372000
         ST    @14,ABDLLINE(,ABDARPTR)                             0160 00373000
*               PARMPTR=ABDARPTR;   /* SET R1 TO ABDA ADDRESS      0161 00374000
*                                                            @G33SPHW*/ 00375000
         LR    PARMPTR,ABDARPTR                                    0161 00376000
*               CALL IEAVAD61;      /* UNPACK REGS 8-15              */ 00377000
         L     @15,@CV00066                                        0162 00378000
         BALR  @14,@15                                             0162 00379000
*               IF RETCODE^=0       /* IF ERRED                      */ 00380000
*                 THEN              /* LEAVE WITH                    */ 00381000
         LTR   RETCODE,RETCODE                                     0163 00382000
         BNZ   @RT00163                                            0163 00383000
*                 GOTO FREERET;     /* BAD CODE                      */ 00384000
*               PARMPTR=ABDARPTR;   /* SET R1 TO ABDA ADDRESS      0165 00385000
*                                                            @G33SPHW*/ 00386000
         LR    PARMPTR,ABDARPTR                                    0165 00387000
*               CALL IEAVAD11;      /* OUTPUT THE REGS 8-15          */ 00388000
         L     @15,@CV00064                                        0166 00389000
         BALR  @14,@15                                             0166 00390000
*               IF RETCODE^=0       /* IF ERRED                      */ 00391000
*                 THEN              /* LEAVE WITH                    */ 00392000
         LTR   RETCODE,RETCODE                                     0167 00393000
         BNZ   @RT00167                                            0167 00394000
*                 GOTO FREERET;     /* BAD CODE                      */ 00395000
*             END;                  /* END.. REG DISPLAY             */ 00396000
*         END;                      /*                               */ 00397000
@RF00111 DS    0H                                                  0171 00398000
*FREERET:                           /* CLEANUP AND EXIT              */ 00399000
*       SAVERET=RETCODE;            /* SETUP RETURN CODE             */ 00400000
@RC00075 DS    0H                                                  0171 00401000
FREERET  LR    SAVERET,RETCODE                                     0171 00402000
*       PARMPTR=ADDR(ABDASIZE);     /* SETUP FREEMAIN LIST           */ 00403000
         LA    PARMPTR,ABDASIZE(,ABDARPTR)                         0172 00404000
