*   RSTADDWD=R4;                    /* SET UP AS SAVE AREA POINTER   */ 00049000
         ST    R4,RSTADDWD(,PVTPTR)                                0121 00050000
*   RFY                                                            0122 00051000
*     REGSAVER BASED(RSTADDWD);                                    0122 00052000
*   REG15WD=0;                      /* INITIALIZE REG15 SAVE AREA    */ 00053000
         SLR   @14,@14                                             0123 00054000
