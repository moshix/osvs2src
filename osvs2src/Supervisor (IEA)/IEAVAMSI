*   GENERATE REFS(FADDR) SETS(FADDR,WRK1R,WRK2R);                  0115 00028000
SETSTK SETFRR A,FRRAD=(FADDR),PARMAD=(FADDR),WRKREGS=(WRK1R,WRK2R),   **00029000
               RELATED=(RSM,IEAVAMSI(RELSTK))                           00030000
*   FADDR->RCAVAMSI='1'B;           /* SET ENTRY POINT FLAG          */ 00031000
