               RELATED=(RSM,IEAVAMSI(RELSLK))                           00044000
*   FADDR->RCASAL='1'B;             /* SET THE LOCK HELD FLAG        */ 00045000
         OI    RCASAL(FADDR),B'10000000'                           0119 00046000
*   PVTPRCA=FADDR;                  /* STORE THE RSMRCA POINTER      */ 00047000
