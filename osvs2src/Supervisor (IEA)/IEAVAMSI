*                                      IEAVPFTE                      */ 00767000
         L     R0,PARMBLD(,PVTPTR)                                 0302 00768000
*           R1=ASCBPTR;             /* LOAD ASCB ADDRESS FOR IEAVPFTE*/ 00769000
         LR    R1,@11                                              0303 00770000
*           CALL IEAVPFTE;          /* CALL PFTE MANAGER TO MOVE PFTE*/ 00771000
         L     @15,PVTPFTE(,PVTPTR)                                0304 00772000
         BALR  @14,@15                                             0304 00773000
*           RFY                                                    0305 00774000
*            (R0,                                                  0305 00775000
*             R1) UNRSTD;                                          0305 00776000
*         END;                                                     0306 00777000
*                                                                  0307 00778000
*       /*************************************************************/ 00779000
*       /*                                                           */ 00780000
*       /* RESET THE CHANGE BITS                                     */ 00781000
*       /*                                                           */ 00782000
*       /*************************************************************/ 00783000
*                                                                  0307 00784000
*       RFY                                                        0307 00785000
*        (WRK1R,                                                   0307 00786000
*         WRK2R) RSTD;                                             0307 00787000
@RF00296 DS    0H                                                  0308 00788000
*       WRK2R=VCBRSN;               /* LOAD THE RBN FOR THE SSK    0308 00789000
*                                      INSTRUCTION                   */ 00790000
         LH    WRK2R,VCBRSN(,VCBPTR)                               0308 00791000
         N     WRK2R,@CF02294                                      0308 00792000
*       WRK1R=XPTPROT;              /* LOAD THE KEY FOR SSK        0309 00793000
*                                      INSTRUCTION                   */ 00794000
         SLR   WRK1R,WRK1R                                         0309 00795000
         IC    WRK1R,XPTPROT(,XPTEPTR)                             0309 00796000
*       GENERATE REFS(WRK1R,WRK2R);                                0310 00797000
      SLL    WRK2R,8(0)            MAKE RBN A REAL ADDRESS              00798000
      SSK    WRK1R,WRK2R           RESET THE CHANGE BIT                 00799000
      LA     WRK2R,2048(,WRK2R)    UPDATE TO SECOND HALF PAGE           00800000
      SSK    WRK1R,WRK2R           RESET THE CHANGE BIT                 00801000
*       RFY                                                        0311 00802000
*        (WRK1R,                                                   0311 00803000
*         WRK2R) UNRSTD;                                           0311 00804000
*       PGTRSA=VCBRSN;              /* LOAD REAL STORAGE NUMBER AND     00805000
*                                      MARK PAGE VALID               */ 00806000
         MVC   PGTRSA(2,PTEPTR),VCBRSN(VCBPTR)                     0312 00807000
*       PGTPAM='1'B;                /* SET GETMAIN ASSIGNED BIT BACK    00808000
*                                      ON                            */ 00809000
         OI    PGTPAM(PTEPTR),B'00000001'                          0313 00810000
*     END;                                                         0314 00811000
*                                                                  0314 00812000
*   /*****************************************************************/ 00813000
*   /*                                                               */ 00814000
*   /* RCLMCNTS - THE FRAME IS BEING RECLAIMED, SO THE VIO COUNT IS  */ 00815000
*   /* UPDATED AND THE LOCAL RECLAIM COUNT IS UPDATED                */ 00816000
*   /*                                                               */ 00817000
*   /*****************************************************************/ 00818000
*                                                                  0315 00819000
*RCLMCNTS:                                                         0315 00820000
*   PVTVAMR=PVTVAMR+1;              /* UPDATE THE VIO COUNT, PAGE WAS   00821000
*                                      RECLAIMED                     */ 00822000
RCLMCNTS LA    @11,1                                               0315 00823000
         L     @10,PVTVAMR(,PVTPTR)                                0315 00824000
         ALR   @10,@11                                             0315 00825000
         ST    @10,PVTVAMR(,PVTPTR)                                0315 00826000
*   OUXBVAMR=OUXBVAMR+1;            /* UPDATE THE LOCAL VIO RECLAIM     00827000
*                                      COUNT                         */ 00828000
         L     @01,ASCBPTR                                         0316 00829000
         L     @10,OUXBPTR(,@01)                                   0316 00830000
         AL    @11,OUXBVAMR(,@10)                                  0316 00831000
         ST    @11,OUXBVAMR(,@10)                                  0316 00832000
*   RFY                                                            0317 00833000
*    (PFTEPTR) UNRSTD;                                             0317 00834000
*                                                                  0318 00835000
*   /*****************************************************************/ 00836000
*   /*                                                               */ 00837000
*   /* REGASIGN-- THIS IS CODE FOR REGULAR ASSIGNMENT. THE EXTERNAL  */ 00838000
*   /* PAGE TABLE IS UPDATED AND A CHECK MADE TO SEE IF A LOAD WAS   */ 00839000
*   /* REQUESTED. IF A LOAD WAS REQUESTED AND THE PAGE IS NOT VALID, */ 00840000
*   /* PAGE SERVICES INTERFACE (PSI) MUST BE CALLED TO DO A PAGELOAD */ 00841000
*   /* BEFORE RETURNING TO VAMSI.                                    */ 00842000
*   /*                                                               */ 00843000
*   /*****************************************************************/ 00844000
*                                                                  0318 00845000
*REGASIGN:                                                         0318 00846000
*   XPTLPID=VCBLPID;                /* LOAD THE LOGICAL PAGE I D     */ 00847000
REGASIGN MVC   XPTLPID(8,XPTEPTR),VCBLPID(VCBPTR)                  0318 00848000
*   XPTXAV='1'B;                    /* MARK EXTERNAL STG VALID       */ 00849000
*   XPTVIOLP='1'B;                  /* SET THE SAVE AUX FLAG @Z40WPYC*/ 00850000
*   XPTVIO='1'B;                    /* SET VIO WINDOW PAGE FLAG    0321 00851000
*                                                            @Z40WPYC*/ 00852000
*                                                                  0321 00853000
         OI    XPTXAV(XPTEPTR),B'11001000'                         0321 00854000
*   /*****************************************************************/ 00855000
*   /*                                                               */ 00856000
*   /* CHECK FOR PAGE LOAD REQUEST                                   */ 00857000
*   /*                                                               */ 00858000
*   /*****************************************************************/ 00859000
*                                                                  0322 00860000
*   IF VCBLOAD='1'B THEN            /* IF LOAD IS REQUESTED          */ 00861000
         TM    VCBLOAD(VCBPTR),B'00000100'                         0322 00862000
         BNO   @RF00322                                            0322 00863000
*     DO;                                                          0323 00864000
*       RFY                                                        0324 00865000
*        (TESTPTR) RSTD;                                           0324 00866000
*       TESTPTR=VCBVSA;             /* LOAD ADDRESS FOR LRA TEST     */ 00867000
         L     TESTPTR,VCBVSA(,VCBPTR)                             0325 00868000
*       GENERATE REFS(TESTPTR) SETS(TESTPTR);                      0326 00869000
      LRA   TESTPTR,0(TESTPTR)     IS VSN VALID IN REAL STORAGE         00870000
*       BC(8,ASIGNRTN);             /* IF TRANSLATION WAS SUCCESSFUL,   00871000
*                                      RETURN TRANSLATION WILL BE  0327 00872000
*                                      SUCCESSFUL HERE IF RECLAIM WAS   00873000
*                                      SUCCESSFUL ABOVE.             */ 00874000
         BC    8,ASIGNRTN                                          0327 00875000
*       RFY                                                        0328 00876000
*        (TESTPTR) UNRSTD;                                         0328 00877000
*                                                                  0328 00878000
*       /*************************************************************/ 00879000
*       /*                                                           */ 00880000
*       /* IF THE PAGE IS NOT VALID IN REAL STORAGE AND A LOAD WAS   */ 00881000
*       /* REQUESTED, PSI MUST BE CALLED TO INITIATE THE LOAD        */ 00882000
*       /*                                                           */ 00883000
*       /*************************************************************/ 00884000
*                                                                  0329 00885000
*       RFY                                                        0329 00886000
*        (R0,                                                      0329 00887000
*         R1,                                                      0329 00888000
*         R12,                                                     0329 00889000
*         R13) RSTD;                                               0329 00890000
*       PARMBLD2=VCBVSA;            /* SET PAGE VSA                  */ 00891000
         L     @08,VCBVSA(,VCBPTR)                                 0330 00892000
         ST    @08,PARMBLD2(,PVTPTR)                               0330 00893000
*       PARMFLGS=PGLOAD;            /* SET LOAD REQUEST FLAG         */ 00894000
         MVI   PARMFLGS(PVTPTR),X'10'                              0331 00895000
*       R0=0;                       /* CLEAR FOR PSI CALL            */ 00896000
         SLR   R0,R0                                               0332 00897000
*       R1=PARMBLD2;                /* LOAD FOR PSI CALL             */ 00898000
         L     R1,PARMBLD2(,PVTPTR)                                0333 00899000
*       R12=R2;                     /* SAVE ACROSS CALL              */ 00900000
         LR    R12,R2                                              0334 00901000
*       R13=R4;                     /* SAVE ACROSS CALL              */ 00902000
         LR    R13,R4                                              0335 00903000
*       R2=VCBVSA+BLOCKSIZ;         /* LOAD FOR PSI CALL--END ADDRESS*/ 00904000
         AL    @08,@CF02045                                        0336 00905000
         LR    R2,@08                                              0336 00906000
*       R4=REG0WD;                  /* LOAD FOR PSI CALL--TCB ADDRESS*/ 00907000
         L     @08,RSTADDWD(,PVTPTR)                               0337 00908000
         L     R4,REG0WD(,@08)                                     0337 00909000
*       CALL IEAVPSII;              /* CALL PSI FOR PAGE LOAD        */ 00910000
         L     @15,PVTPSINT(,PVTPTR)                               0338 00911000
         BALR  @14,@15                                             0338 00912000
*       RCACSECT=RCACVAM;           /* RESTORE CSECT NAME AFTER CALL */ 00913000
         L     @08,RCAPTR(,PVTPTR)                                 0339 00914000
         MVI   RCACSECT(@08),X'18'                                 0339 00915000
*       R2=R12;                     /* RESTORE AFTER CALL            */ 00916000
         LR    R2,R12                                              0340 00917000
*       R4=R13;                     /* RESTORE AFTER CALL            */ 00918000
         LR    R4,R13                                              0341 00919000
*     END;                                                         0342 00920000
*                                                                  0342 00921000
*   /*****************************************************************/ 00922000
*   /*                                                               */ 00923000
*   /* RETURN POINT FOR THE ASSIGN SUB-ROUTINE                       */ 00924000
*   /*                                                               */ 00925000
*   /*****************************************************************/ 00926000
*                                                                  0343 00927000
*ASIGNRTN:                                                         0343 00928000
*   RETCODR=0;                      /* SUCCESSFUL ASSIGN             */ 00929000
*                                                                  0343 00930000
@RF00322 DS    0H                                                  0343 00931000
ASIGNRTN SLR   RETCODR,RETCODR                                     0343 00932000
*   /*****************************************************************/ 00933000
*   /*                                                               */ 00934000
*   /* ERROR RETURN POINT FOR ASSIGN SUB-ROUTINE                     */ 00935000
*   /*                                                               */ 00936000
*   /*****************************************************************/ 00937000
*                                                                  0344 00938000
*ERRRTN:                                                           0344 00939000
*   RFY                                                            0344 00940000
*    (R14) RSTD;                                                   0344 00941000
ERRRTN   DS    0H                                                  0345 00942000
*   R14=SUBRETWD;                   /* LOAD RETURN ADDRESS           */ 00943000
         L     R14,SUBRETWD(,PVTPTR)                               0345 00944000
*   RFY                                                            0346 00945000
*    (R0,                                                          0346 00946000
*     R1,                                                          0346 00947000
*     PTEPTR,                                                      0346 00948000
*     XPTEPTR,                                                     0346 00949000
*     R12,                                                         0346 00950000
*     R13,                                                         0346 00951000
*     R14) UNRSTD;                                                 0346 00952000
*   END ASSIGN;                                                    0347 00953000
@EL00002 DS    0H                                                  0347 00954000
@EF00002 DS    0H                                                  0347 00955000
@ER00002 BR    @14                                                 0347 00956000
*                                                                  0348 00957000
*   /*****************************************************************/ 00958000
*   /*                                                               */ 00959000
*   /* MOVEOUT -- THIS IS THE MOVE OUT SUB-ROUTINE OF VAMSI. FINDPAGE*/ 00960000
*   /* IS CALLED TO SUPPLY PAGE TABLE AND EXTERNAL PAGE TABLE        */ 00961000
*   /* ADDRESSES. IF THE PAGE IS NOT GETMAIN ASSIGNED, IF IT IS IN   */ 00962000
*   /* THE NUCLEUS, IF IT IS IN SQA OR IF IT IS NOT A VIO WINDOW     */ 00963000
*   /* PAGE, AN ERROR IS SIGNALED. IF THE PAGE IS MARKED INVALID, THE*/ 00964000
*   /* OUTOFSTG SUB-ROUTINE IS CALLED. IF NO LPID WAS SUPPLIED, THIS */ 00965000
*   /* IS A NULL MOVEOUT REQUEST AND CONTROL WILL PASS TO THE        */ 00966000
*   /* NULLMOVE ROUTINE. IF AUX STORAGE IS NOT ASSIGNED OR THE SAVE  */ 00967000
*   /* AUX BIT IS NOT SET, CONTROL IS PASSED TO TEST CHANGE BIT      */ 00968000
*   /* (TCHGBIT), OTHERWISE THE PAGEOUT ROUTINE WILL NOW EXECUTE.    */ 00969000
*   /*                                                               */ 00970000
*   /*****************************************************************/ 00971000
*                                                                  0348 00972000
*MOVEOUT:                                                          0348 00973000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0348 00974000
MOVEOUT  DS    0H                                                  0349 00975000
*   RFY                                                            0349 00976000
*    (R0,                                                          0349 00977000
*     R1,                                                          0349 00978000
*     PTEPTR,                                                      0349 00979000
*     XPTEPTR,                                                     0349 00980000
*     R14) RSTD;                                                   0349 00981000
*   SUBRETWD=R14;                   /* SAVE RETURN ADDRESS           */ 00982000
         ST    R14,SUBRETWD(,PVTPTR)                               0350 00983000
*   POINPROC='0'B;                  /* CLEAR PAGEOUT IN PROCESS FLAG    00984000
*                                                            @Z40WPYC*/ 00985000
*   PIINPROC='0'B;                  /* CLEAR PAGEIN IN PROCESS FLAG     00986000
*                                                            @Z40WPYC*/ 00987000
         NI    POINPROC(PVTPTR),B'11111100'                        0352 00988000
*   IOSVAIA=0;                      /* CLEAR I/O SAVE AREA FOR AIA 0353 00989000
*                                                            @Z40WPYC*/ 00990000
*                                                                  0353 00991000
         SLR   @12,@12                                             0353 00992000
         STCM  @12,7,IOSVAIA(PVTPTR)                               0353 00993000
*   /*****************************************************************/ 00994000
*   /*                                                               */ 00995000
*   /* GET PGTE AND XPTE ADDRESSES                                   */ 00996000
*   /*                                                               */ 00997000
*   /*****************************************************************/ 00998000
*                                                                  0354 00999000
*   R1=VCBVSA;                      /* LOAD PARM REG FOR FINDPAGE    */ 01000000
         L     R1,VCBVSA(,VCBPTR)                                  0354 01001000
*   CALL IEAVFP2;                   /* REQUEST PGTE AND XPTE ADDRESS */ 01002000
         L     @15,PVTPFP2(,PVTPTR)                                0355 01003000
         BALR  @14,@15                                             0355 01004000
*   IF RETCODR^=0 THEN              /* IF FINDPAGE FAILED            */ 01005000
         LTR   RETCODR,RETCODR                                     0356 01006000
         BZ    @RF00356                                            0356 01007000
*     DO;                                                          0357 01008000
*       IF RETCODR=8 THEN           /* IF AN INTERNAL RSM ERROR      */ 01009000
         C     RETCODR,@CF00059                                    0358 01010000
         BNE   @RF00358                                            0358 01011000
*         DO;                                                      0359 01012000
*           RCARCRD='01'X;          /* SET RECOVERY CODE             */ 01013000
         L     @12,RCAPTR(,PVTPTR)                                 0360 01014000
         MVI   RCARCRD(@12),X'01'                                  0360 01015000
*           GENERATE SETS(R1);                                     0361 01016000
      ABEND X'C0D',,,SYSTEM        RECORD THE ERROR                     01017000
*         END;                                                     0362 01018000
*       VCBINVSA='1'B;              /* SET INVALID VSA FLAG IN VCB   */ 01019000
@RF00358 OI    VCBINVSA(VCBPTR),B'01000000'                        0363 01020000
*       RETCODR=4;                  /* SET THE RETURN CODE           */ 01021000
         LA    RETCODR,4                                           0364 01022000
*       GOTO MOERRTN;               /* GOING TO RETURN TO VAMSI WITH    01023000
*                                      ERROR CODE                    */ 01024000
         B     MOERRTN                                             0365 01025000
*     END;                                                         0366 01026000
*   PTEPTR=R0;                      /* LOAD POINTERS AS RETURNED BY     01027000
*                                      FINDPAGE                      */ 01028000
@RF00356 LR    PTEPTR,R0                                           0367 01029000
*   XPTEPTR=R1;                     /* LOAD POINTERS AS RETURNED BY     01030000
*                                      FINDPAGE                      */ 01031000
         LR    XPTEPTR,R1                                          0368 01032000
*   RFY                                                            0369 01033000
*    (R0,                                                          0369 01034000
*     R1) UNRSTD;                                                  0369 01035000
*                                                                  0369 01036000
*   /*****************************************************************/ 01037000
*   /*                                                               */ 01038000
*   /* TEST FOR ERROR CONDITIONS                                     */ 01039000
*   /*                                                               */ 01040000
*   /*****************************************************************/ 01041000
*                                                                  0370 01042000
*   IF CURVSNR<PVTLPRIV THEN        /* IF A NUCLEUS PAGE             */ 01043000
         LH    @12,PVTLPRIV(,PVTPTR)                               0370 01044000
         N     @12,@CF02294                                        0370 01045000
         CLR   CURVSNR,@12                                         0370 01046000
         BNL   @RF00370                                            0370 01047000
*     DO;                                                          0371 01048000
*EFIX:                                                             0372 01049000
*       VCBEFIX='1'B;               /* SET THE FIXED ERROR FLAG      */ 01050000
EFIX     OI    VCBEFIX(VCBPTR),B'00001000'                         0372 01051000
*       RETCODR=4;                  /* SET THE ERROR RETURN CODE     */ 01052000
         LA    RETCODR,4                                           0373 01053000
*       GOTO MOERRTN;               /* GOING TO RETURN               */ 01054000
         B     MOERRTN                                             0374 01055000
*     END;                                                         0375 01056000
*   IF PGTPAM='0'B|XPTVIO='0'B THEN /* IF PAGE IS NOT GETMAINED OR 0376 01057000
*                                      PAGE IS NOT A VIO WINDOW PAGE    01058000
*                                                            @Z40WPYC*/ 01059000
@RF00370 TM    PGTPAM(PTEPTR),B'00000001'                          0376 01060000
         BZ    @RT00376                                            0376 01061000
         TM    XPTVIO(XPTEPTR),B'00001000'                         0376 01062000
         BNZ   @RF00376                                            0376 01063000
@RT00376 DS    0H                                                  0377 01064000
*     DO;                                                          0377 01065000
*       VCBINVSA='1'B;              /* SET INVALID VSA FLAG IN VCB   */ 01066000
         OI    VCBINVSA(VCBPTR),B'01000000'                        0378 01067000
*       RETCODR=4;                  /* SET THE RETURN CODE           */ 01068000
         LA    RETCODR,4                                           0379 01069000
*       GOTO MOERRTN;               /* GOING TO RETURN               */ 01070000
         B     MOERRTN                                             0380 01071000
*     END;                                                         0381 01072000
*                                                                  0381 01073000
*   /*****************************************************************/ 01074000
*   /*                                                               */ 01075000
*   /* TEST FOR PAGE VALIDITY, NULLMOVE, ETC.                        */ 01076000
*   /*                                                               */ 01077000
*   /*****************************************************************/ 01078000
*                                                                  0382 01079000
*   IF PGTPVM='1'B THEN             /* IF PAGE IS INVALID            */ 01080000
@RF00376 TM    PGTPVM(PTEPTR),B'00001000'                          0382 01081000
         BNO   @RF00382                                            0382 01082000
*     DO;                           /* PROCESS OUT OF STORAGE CASE 0383 01083000
*                                                            @Z40WPYC*/ 01084000
*       IF XPTPOINP='1'B THEN       /* IF A PAGEOUT IS IN PROCESS FOR   01085000
*                                      THIS VSA LOCATE THE OUTPUT PCB   01086000
*                                                            @Z40WPYC*/ 01087000
         TM    XPTPOINP(XPTEPTR),B'00100000'                       0384 01088000
         BNO   @RF00384                                            0384 01089000
*         DO;                       /* START SEARCH OF LIOQ  @Z40WPYC*/ 01090000
*           RESPECIFY                                              0386 01091000
*            (PCBPTR) RESTRICTED;   /*                       @Z40WPYC*/ 01092000
*           PCBPTR=RSMLIOQF;        /* LOCATE START OF CHAIN @Z40WPYC*/ 01093000
         L     PCBPTR,RSMLIOQF(,RSMHDPTR)                          0387 01094000
*           DO WHILE PCBPTR^=0;     /* SEARCH ENTIRE LOCAL I/O QUEUE    01095000
*                                      FOR THE OUTPUT PCB    @Z40WPYC*/ 01096000
         B     @DE00388                                            0388 01097000
@DL00388 DS    0H                                                  0389 01098000
*             IF PCBVBN^=CURVSNR THEN/* IF NOT CORRECT PCB   @Z40WPYC*/ 01099000
         LH    @12,PCBVBN(,PCBPTR)                                 0389 01100000
         N     @12,@CF02294                                        0389 01101000
         CLR   CURVSNR,@12                                         0389 01102000
         BE    @RF00389                                            0389 01103000
*               PCBPTR=PCBFQPA;     /* GET NEXT PTR          @Z40WPYC*/ 01104000
         L     @12,PCBFQPA-1(,PCBPTR)                              0390 01105000
         LA    @12,0(,@12)                                         0390 01106000
         LR    PCBPTR,@12                                          0390 01107000
*             ELSE                  /* MATCH FOUND           @Z40WPYC*/ 01108000
*               DO;                 /* SET INDICATORS FOR TRANSFER 0391 01109000
*                                      PAGE (XFERP)          @Z40WPYC*/ 01110000
         B     @RC00389                                            0391 01111000
@RF00389 DS    0H                                                  0392 01112000
*                 POINPROC='1'B;    /* INDICATE PAGEOUT IN PROCESS 0392 01113000
*                                                            @Z40WPYC*/ 01114000
         OI    POINPROC(PVTPTR),B'00000010'                        0392 01115000
*                 IOSVAIA=ADDR(PCBAIA);/* SAVE AIA ADDR FOR THIS   0393 01116000
*                                      OUTPUT PCB            @Z40WPYC*/ 01117000
         LA    @12,PCBAIA(,PCBPTR)                                 0393 01118000
         STCM  @12,7,IOSVAIA(PVTPTR)                               0393 01119000
*                 IF VCBNDISC='0'B&PCBNOREC='1'B THEN/* IF THE     0394 01120000
*                                      NON-DISC OPTION WAS NOT     0394 01121000
*                                      SPECIFIED AND THE OUTPUT PCB     01122000
*                                      HAS BEEN MARKED             0394 01123000
*                                      NON-RECLAIMABLE, I.E, A     0394 01124000
*                                      PAGEOUT IS IN PROCESS FOR AN     01125000
*                                      INTERCEPTED FRAME, NOP THE  0394 01126000
*                                      OUTPUT PCB. THIS IS REQUIRED     01127000
*                                      BECAUSE A SUBSEQUENT ASSIGN 0394 01128000
*                                      THEN PAGE FAULT WHILE THE   0394 01129000
*                                      OUTPUT I/O IS STILL IN PROCESS   01130000
*                                      WILL RESULT IN DUPLICATE    0394 01131000
*                                      PCBVBNS (ONE INPUT THE OTHER     01132000
*                                      OUTPUT).              @Z40WPYC*/ 01133000
         TM    VCBNDISC(VCBPTR),B'00001000'                        0394 01134000
         BNZ   @RF00394                                            0394 01135000
         TM    PCBNOREC(PCBPTR),B'00000100'                        0394 01136000
         BNO   @RF00394                                            0394 01137000
*                   DO;             /* PERFORM SPECIAL PROCESSING  0395 01138000
*                                                            @Z40WPYC*/ 01139000
*                     PCBVBN=0;     /* NOP THE PCB           @Z40WPYC*/ 01140000
         SLR   @12,@12                                             0396 01141000
         STH   @12,PCBVBN(,PCBPTR)                                 0396 01142000
*                     PCBFREAL='1'B;/* ENSURE FRAME IS FREED WHEN I/O   01143000
*                                      COMPETES              @Z40WPYC*/ 01144000
         OI    PCBFREAL(PCBPTR),B'10000000'                        0397 01145000
*                   END;            /* END NO-RECLAIM PROCESSING   0398 01146000
*                                                            @Z40WPYC*/ 01147000
*                 ELSE                                             0399 01148000
*                   ;               /* NORMAL PROCESSING     @Z40WPYC*/ 01149000
@RF00394 DS    0H                                                  0400 01150000
*                 GOTO SRCHCOMP;    /* BRANCH OUT OF LOOP    @Z40WPYC*/ 01151000
         B     SRCHCOMP                                            0400 01152000
*               END;                /* END OF PAGEOUT FOUND        0401 01153000
*                                      PROCESSING            @Z40WPYC*/ 01154000
*           END;                    /* END OF SEARCH LOOP    @Z40WPYC*/ 01155000
@RC00389 DS    0H                                                  0402 01156000
@DE00388 LTR   PCBPTR,PCBPTR                                       0402 01157000
         BNZ   @DL00388                                            0402 01158000
*         END;                      /* END PAGEOUT PROCESSING      0403 01159000
*                                                            @Z40WPYC*/ 01160000
*SRCHCOMP:                                                         0404 01161000
*       CALL OUTOFSTG;              /* CALL THE OUT OF STORAGE     0404 01162000
*                                      ROUTINE -- CONTROL WILL NOT 0404 01163000
*                                      RETURN TO THIS SUB-PROC -- BUT   01164000
*                                      WILL BE TO VAMSI              */ 01165000
@RF00384 DS    0H                                                  0404 01166000
SRCHCOMP BAL   @14,OUTOFSTG                                        0404 01167000
*     END;                          /* END INVALID PAGE PROCESSING 0405 01168000
*                                                            @Z40WPYC*/ 01169000
*   ELSE                                                           0406 01170000
*     ;                             /* PAGE IS VALID IN STORAGE    0406 01171000
*                                                            @Z40WPYC*/ 01172000
@RF00382 DS    0H                                                  0407 01173000
*   RFY                                                            0407 01174000
*    (PFTEPTR) RSTD;                                               0407 01175000
*   PFTEPTR=PVTPFTP+(PGTRSA&RSNMASK);/* LOAD PFTE POINTER            */ 01176000
         L     PFTEPTR,@CF02042                                    0408 01177000
         SLR   @12,@12                                             0408 01178000
         ICM   @12,3,PGTRSA(PTEPTR)                                0408 01179000
         NR    PFTEPTR,@12                                         0408 01180000
         AL    PFTEPTR,PVTPFTP(,PVTPTR)                            0408 01181000
*   IF PFTLSQA='1'B|PFTBADPG='1'B THEN/* IF AN L/SQA FRAME OR FRAME     01182000
*                                      CONTAINS A STORAGE ERROR    0409 01183000
*                                                            @ZA19960*/ 01184000
         TM    PFTLSQA(PFTEPTR),B'00100100'                        0409 01185000
         BNZ   @RT00409                                            0409 01186000
*     GOTO EFIX;                    /* THIS IS A FIXED PAGE          */ 01187000
*   IF MYVCBLGN=0&MYVCBLPN=0 THEN   /* IF LPID WAS SPECIFIED AS ZERO,   01188000
*                                      A NULL MOVE WAS REQUESTED     */ 01189000
         SLR   @12,@12                                             0411 01190000
         CL    @12,MYVCBLGN(,VCBPTR)                               0411 01191000
         BNE   @RF00411                                            0411 01192000
         CL    @12,MYVCBLPN(,VCBPTR)                               0411 01193000
         BE    @RT00411                                            0411 01194000
*     GOTO NULLMOVE;                /* GOING TO NULLMOVE CODE        */ 01195000
*   IF XPTXAV='0'B|XPTVIOLP='0'B THEN/* IF AUX STORAGE IS NOT      0413 01196000
*                                      ASSIGNED, OR IF THE SAVE AUX     01197000
*                                      FLAG IS NOT SET       @Z40WPYC*/ 01198000
@RF00411 TM    XPTXAV(XPTEPTR),B'11000000'                         0413 01199000
         BNO   @RT00413                                            0413 01200000
*     GOTO TCHGBIT;                 /* GOING TO SEE IF PAGE HAS    0414 01201000
*                                      CHANGED                       */ 01202000
*                                                                  0415 01203000
*   /*****************************************************************/ 01204000
*   /*                                                               */ 01205000
*   /* PAGEOUT -- THIS IS THE PAGE OUT CODE OF THE MOVEOUT           */ 01206000
*   /* SUB-ROUTINE. IEAVINV IS CALLED TO INVALIDATE THE PAGE, AND A  */ 01207000
*   /* PCB IS OBTAINED FOR THE PAGEOUT. IF THE PCB MANAGER CANNOT    */ 01208000
*   /* SUPPLY A PCB, AN ABEND WILL BE ISSUED WHEN CONTROL RETURNS TO */ 01209000
*   /* THE MAINLINE; OTHERWISE THE PCB IS FILLED IN AND THE PCB      */ 01210000
*   /* DEFINED FLAG TURNED ON IN PFTE. IF NEITHER THE NON-DISCONNECT */ 01211000
*   /* NOR THE IN-STORAGE OPTIONS WERE REQUESTED, THE FREE REAL FLAG */ 01212000
*   /* IS TURNED ON IN THE PCB. THE PAGEOUT AIA IS CHAINED TO THE    */ 01213000
*   /* INTERNAL QUEUE (VAMAIAQ) FOR LATER CALL TO ASM. THE PROPER    */ 01214000
*   /* COUNTS ARE UPDATED AND THE PCB SENT TO THE PROPER IO QUEUE. IF*/ 01215000
*   /* THE NON-DISCONNECT OPTION WAS SPECIFIED THE SAVE AUX AND AUX  */ 01216000
*   /* ASSIGNED FLAGS ARE SET, THE XPTLPID IS SET TO THE VCBLPID     */ 01217000
*   /* VALUE AND THE RSN IN THE VCB IS CLEARED. RETURN IS THEN MADE  */ 01218000
*   /* TO THE CALLER. IF A RECLAIM I D WAS NOT SUPPLIED THE VCBRSN IS*/ 01219000
*   /* ZEROED, ELSE THE VCBRSN IS SET, THE PAGE FRAME IRREGULAR BIT  */ 01220000
*   /* IS SET ON, AND THE I D IS SET IN THE PFTE.                    */ 01221000
*   /*                                                               */ 01222000
*   /*****************************************************************/ 01223000
*                                                                  0415 01224000
*PAGEOUT:                                                          0415 01225000
*   RFY                                                            0415 01226000
*    (R0,                                                          0415 01227000
*     R1,                                                          0415 01228000
*     PCBPTR) RSTD;                                                0415 01229000
*                                                                  0415 01230000
PAGEOUT  DS    0H                                                  0416 01231000
*   /*****************************************************************/ 01232000
*   /*                                                               */ 01233000
*   /* INVALIDATE THE PAGE                                           */ 01234000
*   /*                                                               */ 01235000
*   /*****************************************************************/ 01236000
*                                                                  0416 01237000
*   R1=PTEPTR;                      /* LOAD PAGE TABLE POINTER FOR 0416 01238000
*                                      INVALIDATE                    */ 01239000
         LR    R1,PTEPTR                                           0416 01240000
*   CALL IEAVINV;                   /* CALL FOR INVALIDATION         */ 01241000
*                                                                  0417 01242000
         L     @15,PVTPINV(,PVTPTR)                                0417 01243000
         BALR  @14,@15                                             0417 01244000
*   /*****************************************************************/ 01245000
*   /*                                                               */ 01246000
*   /* OBTAIN A PCB FOR PAGEOUT                                      */ 01247000
*   /*                                                               */ 01248000
*   /*****************************************************************/ 01249000
*                                                                  0418 01250000
*   R0=1;                           /* REQUEST ONE PCB FROM PCB    0418 01251000
*                                      MANAGER                       */ 01252000
         LA    R0,1                                                0418 01253000
*   R1=0;                           /* SPECIFY A BUILD REQUEST       */ 01254000
         SLR   R1,R1                                               0419 01255000
*   CALL IEAVPCB;                   /* CALL PCB MANAGER FOR A BUILD  */ 01256000
         L     @15,PVTPCB(,PVTPTR)                                 0420 01257000
         BALR  @14,@15                                             0420 01258000
*   IF RETCODR^=0 THEN              /* IF NO PCBS ARE AVAILABLE      */ 01259000
         LTR   RETCODR,RETCODR                                     0421 01260000
         BZ    @RF00421                                            0421 01261000
*     DO;                                                          0422 01262000
*       RETCODR=16;                 /* RETURN CODE OF 16 FOR ABEND 0423 01263000
*                                      SITUATION                     */ 01264000
         LA    RETCODR,16                                          0423 01265000
*       GOTO MOERRTN;               /* GOING TO RETURN TO CALLER WITH   01266000
*                                      ERROR                         */ 01267000
         B     MOERRTN                                             0424 01268000
*     END;                                                         0425 01269000
*   PCBPTR=R1;                      /* LOAD PCB POINTER AS RETURNED     01270000
*                                      BY MANAGER                    */ 01271000
@RF00421 LR    PCBPTR,R1                                           0426 01272000
*   PCBRTPA=REG0WD;                 /* LOAD THE TCB ADDRESS          */ 01273000
         L     @12,RSTADDWD(,PVTPTR)                               0427 01274000
         MVC   PCBRTPA(3,PCBPTR),REG0WD+1(@12)                     0427 01275000
*   PCBVIO='1'B;                    /* THIS IS A PAGEOUT FOR VIO   0428 01276000
*                                                            @Z40WPYC*/ 01277000
         OI    PCBVIO(PCBPTR),B'00000100'                          0428 01278000
*   PCBIOI='1'B;                    /* MARK A PAGEOUT OPERATION      */ 01279000
         OI    PCBIOI(PCBPTR),B'00000010'                          0429 01280000
*   PCBASCB=RSMASCB;                /* LOAD ADDRESS SPACE IN PCB     */ 01281000
         L     @12,RSMASCB(,RSMHDPTR)                              0430 01282000
         ST    @12,PCBASCB(,PCBPTR)                                0430 01283000
*   PCBXPTA=XPTEPTR;                /* LOAD XPTE ADDRESS             */ 01284000
         STCM  XPTEPTR,7,PCBXPTA(PCBPTR)                           0431 01285000
*   PCBPGTA=PTEPTR;                 /* LOAD PGTE ADDRESS             */ 01286000
         STCM  PTEPTR,7,PCBPGTA(PCBPTR)                            0432 01287000
*   PCBRBN=PGTRSA&RSNMASK;          /* LOAD REAL ADDRESS             */ 01288000
         MVC   PCBRBN(2,PCBPTR),PGTRSA(PTEPTR)                     0433 01289000
         NC    PCBRBN(2,PCBPTR),@CB02042                           0433 01290000
*   PFTPCBSI='1'B;                  /* SET PCB DEFINED FLAG IN PAGE     01291000
*                                      FRAME TABLE                   */ 01292000
*                                                                  0434 01293000
         OI    PFTPCBSI(PFTEPTR),B'00001000'                       0434 01294000
*   /*****************************************************************/ 01295000
*   /*                                                               */ 01296000
*   /* CHAIN AIA TO INTERNAL QUEUE                                   */ 01297000
*   /*                                                               */ 01298000
*   /*****************************************************************/ 01299000
*                                                                  0435 01300000
*   AIANXAIA=VAMAIAQ;               /* TAG ONTO CHAIN                */ 01301000
         L     @12,VAMAIAQ(,PVTPTR)                                0435 01302000
         ST    @12,AIANXAIA(,PCBPTR)                               0435 01303000
*   VAMAIAQ=ADDR(PCBAIA);           /* UPDATE THE HEADER             */ 01304000
         LA    @12,PCBAIA(,PCBPTR)                                 0436 01305000
         ST    @12,VAMAIAQ(,PVTPTR)                                0436 01306000
*   AIAWRITE='1'B;                  /* MARK WRITE OPERATION  @Z40WPYC*/ 01307000
         OI    AIAWRITE(PCBPTR),B'10000000'                        0437 01308000
*   AIALPID=VCBLPID;                /* LOAD THE LOGICAL PAGE ID      */ 01309000
         MVC   AIALPID(8,PCBPTR),VCBLPID(VCBPTR)                   0438 01310000
*   AIAPRIV='1'B;                   /* INDICATE PRIVATE AREA PAGE  0439 01311000
*                                                            @Z40WPYC*/ 01312000
*   AIAVIO='1'B;                    /* INDICATE VIO PAGEOUT  @Z40WPYC*/ 01313000
*                                                                  0440 01314000
         OI    AIAPRIV(PCBPTR),B'01100000'                         0440 01315000
*   /*****************************************************************/ 01316000
*   /*                                                               */ 01317000
*   /* CHECK FOR NON-DISCONNECT OPTION                               */ 01318000
*   /*                                                               */ 01319000
*   /*****************************************************************/ 01320000
*                                                                  0441 01321000
*   IF VCBNDISC='1'B THEN           /* IF THE NON-DISCONNECT OPTION     01322000
*                                      IS SPECIFIED                  */ 01323000
         TM    VCBNDISC(VCBPTR),B'00001000'                        0441 01324000
         BNO   @RF00441                                            0441 01325000
*     PCBVBN=CURVSNR;               /* LOAD THE VIRTUAL BLOCK NUMBER */ 01326000
         STH   CURVSNR,PCBVBN(,PCBPTR)                             0442 01327000
*   ELSE                            /* IF NON-DISCONNECT IS NOT    0443 01328000
*                                      SPECIFIED                     */ 01329000
*     IF VCBINSTG='0'B THEN         /* IF THE IN-STORAGE OPTION NOT     01330000
*                                      SPECIFIED                     */ 01331000
         B     @RC00441                                            0443 01332000
@RF00441 TM    VCBINSTG(VCBPTR),B'00010000'                        0443 01333000
         BNZ   @RF00443                                            0443 01334000
*       PCBFREAL='1'B;              /* SET THE FREE REAL BIT         */ 01335000
*                                                                  0444 01336000
         OI    PCBFREAL(PCBPTR),B'10000000'                        0444 01337000
*   /*****************************************************************/ 01338000
*   /*                                                               */ 01339000
*   /* PCB TRACE                                             @Z40WPYC*/ 01340000
*   /*                                                               */ 01341000
*   /*****************************************************************/ 01342000
*                                                                  0445 01343000
*   GEN REFS(R1,PCBPTR,VAMSIMO) SETS(R1,PCB);                      0445 01344000
*                                                                  0445 01345000
@RF00443 DS    0H                                                  0445 01346000
@RC00441 DS    0H                                                  0445 01347000
*                                               /* @Z40WPYC*/           01348000
TRACEZV1 EQU   *         INDICATE MOVEOUT PCB @Z40WPYC                  01349000
         B     *+16      BRANCH AROUND PCB TRACE @Z40WPYC               01350000
         L     R1,68(PCBPTR) GET CURRENT COUNT @Z40WPYC                 01351000
         AL    R1,VAMSIMO INCREMENT BY 1 @Z40WPYC                       01352000
         ST    R1,68(PCBPTR) STORE NEW COUNT @Z40WPYC                   01353000
*   /*****************************************************************/ 01354000
*   /*                                                               */ 01355000
*   /* UPDATE COUNTS, AND SEND PCB TO PROPER QUEUE                   */ 01356000
*   /*                                                               */ 01357000
*   /*****************************************************************/ 01358000
*                                                                  0446 01359000
*   PVTVAMO=PVTVAMO+1;              /* UPDATE VIO PAGE OUT COUNT     */ 01360000
         LA    @12,1                                               0446 01361000
         L     @11,PVTVAMO(,PVTPTR)                                0446 01362000
         ALR   @11,@12                                             0446 01363000
         ST    @11,PVTVAMO(,PVTPTR)                                0446 01364000
*   OUXBVAMO=OUXBVAMO+1;            /* UPDATE LOCAL AREA VIO PAGE OUT   01365000
*                                      COUNT                         */ 01366000
         L     @11,ASCBPTR                                         0447 01367000
         L     @11,OUXBPTR(,@11)                                   0447 01368000
         AL    @12,OUXBVAMO(,@11)                                  0447 01369000
         ST    @12,OUXBVAMO(,@11)                                  0447 01370000
*   R0=PCBLIOQN;                    /* SEND PCB TO LOCAL QUEUE       */ 01371000
         LA    R0,136                                              0448 01372000
*   R1=PCBPTR;                      /* LOAD PCB ADDRESS FOR PCB    0449 01373000
*                                      MANAGER                       */ 01374000
         LR    R1,PCBPTR                                           0449 01375000
*   CALL IEAVPCB;                   /* SEND PCB TO PROPER QUEUE      */ 01376000
         L     @15,PVTPCB(,PVTPTR)                                 0450 01377000
         BALR  @14,@15                                             0450 01378000
*   RFY                                                            0451 01379000
*    (R0,                                                          0451 01380000
*     R1,                                                          0451 01381000
*     PCBPTR) UNRSTD;                                              0451 01382000
*                                                                  0451 01383000
*   /*****************************************************************/ 01384000
*   /*                                                               */ 01385000
*   /* CHECK FOR NON-DISCONNECT OPTION                               */ 01386000
*   /*                                                               */ 01387000
*   /*****************************************************************/ 01388000
*                                                                  0452 01389000
*   IF VCBNDISC='1'B THEN           /* IF THE NON-DISCONNECT OPTION     01390000
*                                      WAS SPECIFIED                 */ 01391000
         TM    VCBNDISC(VCBPTR),B'00001000'                        0452 01392000
         BNO   @RF00452                                            0452 01393000
*     DO;                                                          0453 01394000
*CONNECT:                                                          0454 01395000
*       XPTLPID=VCBLPID;            /* LOAD THE LOGICAL PAGE ID      */ 01396000
CONNECT  MVC   XPTLPID(8,XPTEPTR),VCBLPID(VCBPTR)                  0454 01397000
*       XPTXAV='1'B;                /* SET THE AUX ASSIGNED FLAG     */ 01398000
*       XPTVIOLP='1'B;              /* SET THE SAVE AUX FLAG @Z40WPYC*/ 01399000
*                                                                  0456 01400000
         OI    XPTXAV(XPTEPTR),B'11000000'                         0456 01401000
*       /*************************************************************/ 01402000
*       /*                                                           */ 01403000
*       /* CLEAR FLAGS NORMALLY CLEARED BY ASM BUT NOT FOR MOVEOUTS  */ 01404000
*       /* OR TRANSFER PAGE OPERATIONS.                      @Z40WPYC*/ 01405000
*       /*                                                           */ 01406000
*       /*************************************************************/ 01407000
*                                                                  0457 01408000
*       XPTPOINP='0'B;              /* CLEAR PAGEOUT IN PROCESS FLAG    01409000
*                                                            @Z40WPYC*/ 01410000
*       XPTIOERR='0'B;              /* CLEAR I/O ERROR FLAG  @Z40WPYC*/ 01411000
*       XPTVALID='0'B;              /* CLEAR LSID VALID FLAG @Z40WPYC*/ 01412000
         NI    XPTPOINP(XPTEPTR),B'01001111'                       0459 01413000
*       VCBRSN=0;                   /* CLEAR THE RSN                 */ 01414000
         SLR   @12,@12                                             0460 01415000
         STH   @12,VCBRSN(,VCBPTR)                                 0460 01416000
*       GOTO MORRTN;                /* GOING BACK TO CALLER, MOVE OUT   01417000
*                                      COMPLETE                      */ 01418000
         B     MORRTN                                              0461 01419000
*     END;                                                         0462 01420000
*   IF VCBDSPID=0 THEN              /* IF NO RECLAIM I D WAS SUPPLIED*/ 01421000
@RF00452 SLR   @12,@12                                             0463 01422000
         CL    @12,VCBDSPID(,VCBPTR)                               0463 01423000
         BNE   @RF00463                                            0463 01424000
*     VCBRSN=0;                     /* RESET THE RSN                 */ 01425000
         STH   @12,VCBRSN(,VCBPTR)                                 0464 01426000
*   ELSE                            /* IF RECLAIM ID WAS SUPPLIED    */ 01427000
*     DO;                           /* IF RECLAIM I D WAS SUPPLIED   */ 01428000
         B     @RC00463                                            0465 01429000
@RF00463 DS    0H                                                  0466 01430000
*       PFTIRRG='1'B;               /* INDICATE A VIO FRAME          */ 01431000
         OI    PFTIRRG(PFTEPTR),B'00010000'                        0466 01432000
*       PFTPGID=VCBDSPID;           /* STORE DATASET I D IN PFTE     */ 01433000
         L     @12,VCBDSPID(,VCBPTR)                               0467 01434000
         ST    @12,PFTPGID(,PFTEPTR)                               0467 01435000
*       VCBRSN=PGTRSA&RSNMASK;      /* STORE THE REAL STORAGE NUMBER */ 01436000
         L     @12,@CF02042                                        0468 01437000
         SLR   @11,@11                                             0468 01438000
         ICM   @11,3,PGTRSA(PTEPTR)                                0468 01439000
         NR    @12,@11                                             0468 01440000
         STH   @12,VCBRSN(,VCBPTR)                                 0468 01441000
*     END;                                                         0469 01442000
*   GOTO CLRPGTE;                   /* GOING TO CLEAN UP TABLES AND     01443000
*                                      RETURN                        */ 01444000
         B     CLRPGTE                                             0470 01445000
*                                                                  0471 01446000
*   /*****************************************************************/ 01447000
*   /*                                                               */ 01448000
*   /* TCHGBIT -- THIS CODE TESTS THE CHANGE BIT TO SEE IF A PAGE HAS*/ 01449000
*   /* BEEN CHANGED IN REAL STORAGE. IF A CHANGE HAS TAKEN PLACE AND */ 01450000
*   /* AUX IS ASSIGNED, RELEASE PAGE MUST BE CALLED AND CONTROL      */ 01451000
*   /* TRANSFERED TO PAGEOUT. IF NO CHANGE IS INDICATED AND THE AUX  */ 01452000
*   /* ASSIGNED BIT IS OFF, THE VCBNOAUX FLAG IS SET. IF THE         */ 01453000
*   /* NON-DISCONNECT OPTION WAS SPECIFIED THE VCBRSN IS CLEARED AND */ 01454000
*   /* CONTROL RETURNS TO THE CALLER. IF NON-DISCONNECT WAS NOT SET, */ 01455000
*   /* INVALIDATION MUST TAKE PLACE AND CONTROL XFERS TO NORECLM. -- */ 01456000
*   /* ELSE -- IF AUX STORAGE WAS ASSIGNED THE PAGE IS TRANSFERED BY */ 01457000
*   /* THE ILRCALL MACRO. IF THE NON-DISCONNECT OPTION IS IN EFFECT  */ 01458000
*   /* CONTROL IS TRANSFERED TO THE LABEL CONNECT IN THE PAGEOUT     */ 01459000
*   /* CODE. IF NOT THE NON-DISCONNECT OPTION IEAVINV IS CALLED TO   */ 01460000
*   /* INVALIDATE THE PAGE. IF THE DATASET PAGE I D IN THE VCB IS    */ 01461000
*   /* ZERO, CONTROL TRANSFERS TO NORECLM, BUT IF A PAGE I D WAS     */ 01462000
*   /* SUPPLIED CONTROL TRANSFERS TO RECLAIM.                        */ 01463000
*   /*                                                               */ 01464000
*   /*****************************************************************/ 01465000
*                                                                  0471 01466000
*TCHGBIT:                                                          0471 01467000
*   RFY                                                            0471 01468000
*    (TESTPTR) RSTD;                                               0471 01469000
TCHGBIT  DS    0H                                                  0472 01470000
*   TESTPTR=PGTRSA;                 /* LOAD TEST POINTER FOR CHANGE     01471000
*                                      BIT TEST                      */ 01472000
         SLR   TESTPTR,TESTPTR                                     0472 01473000
         ICM   TESTPTR,3,PGTRSA(PTEPTR)                            0472 01474000
*   GENERATE REFS(TESTPTR);                                        0473 01475000
      SRL   TESTPTR,4(0)           CLEAR THE FLAGS                      01476000
      SLL   TESTPTR,12(0)          ADJUST TO A REAL ADDRESS             01477000
      RRB   0(TESTPTR)             TEST CHANGE BIT                      01478000
*   BC(5,CKAUX);                    /* IF PAGE CHANGED, GOING TO   0474 01479000
*                                      CHECK AUX                     */ 01480000
         BC    5,CKAUX                                             0474 01481000
*   GENERATE REFS(TESTPTR);                                        0475 01482000
      RRB   2048(TESTPTR)          TEST CHANGE BIT IN 2ND HALF BLOCK    01483000
*   BC(10,NOCHG);                   /* IF PAGE DID NOT CHANGE, GOING    01484000
*                                      TO NO CHANGE                  */ 01485000
         BC    10,NOCHG                                            0476 01486000
*   RFY                                                            0477 01487000
*    (TESTPTR) UNRSTD;                                             0477 01488000
*                                                                  0477 01489000
*   /*****************************************************************/ 01490000
*   /*                                                               */ 01491000
*   /* THIS CODE EXECUTES WHEN IT IS FOUND THAT A CHANGE OCCURED     */ 01492000
*   /*                                                               */ 01493000
*   /*****************************************************************/ 01494000
*                                                                  0478 01495000
*CKAUX:                                                            0478 01496000
*   IF XPTXAV='1'B THEN             /* IF AUX STORAGE IS ASSIGNED  0478 01497000
*                                      FREE THE LSID. GOT HERE     0478 01498000
*                                      BECAUSE XPTXAV=0 OR         0478 01499000
*                                      XPTVIOLP=0. THEREFORE IF    0478 01500000
*                                      XPTXAV=1 THEN XPTVIOLP=0 AND     01501000
*                                      CURRENT OCCUPANT OF XPTE IS AN   01502000
*                                      LSID.                 @Z40WPYC*/ 01503000
CKAUX    TM    XPTXAV(XPTEPTR),B'01000000'                         0478 01504000
         BNO   @RF00478                                            0478 01505000
*     CALL RELEASEP;                /* CALL FOR AUX RELEASE          */ 01506000
         BAL   @14,RELEASEP                                        0479 01507000
*   GOTO PAGEOUT;                   /* GOING TO THE PAGE OUT ROUTINE */ 01508000
         B     PAGEOUT                                             0480 01509000
*                                                                  0481 01510000
*   /*****************************************************************/ 01511000
*   /*                                                               */ 01512000
*   /* THIS CODE EXECUTES WHEN IT IS FOUND THAT NO CHANGE OCCURED.   */ 01513000
*   /*                                                               */ 01514000
*   /*****************************************************************/ 01515000
*                                                                  0481 01516000
*NOCHG:                                                            0481 01517000
*   IF XPTXAV='0'B THEN             /* IF AUX STORAGE IS NOT ASSIGNED*/ 01518000
NOCHG    TM    XPTXAV(XPTEPTR),B'01000000'                         0481 01519000
         BNZ   @RF00481                                            0481 01520000
*     DO;                                                          0482 01521000
*       VCBNOAUX='1'B;              /* SET NO AUX ASSIGNED BIT IN VCB*/ 01522000
         OI    VCBNOAUX(VCBPTR),B'00010000'                        0483 01523000
*       IF VCBNDISC='1'B THEN       /* IF THE NON-DISCONNECT OPTION     01524000
*                                      WAS SET                       */ 01525000
         TM    VCBNDISC(VCBPTR),B'00001000'                        0484 01526000
         BNO   @RF00484                                            0484 01527000
*         DO;                                                      0485 01528000
*           VCBRSN=0;               /* SET REAL STORAGE NUMBER TO  0486 01529000
*                                      ZERO                          */ 01530000
         SLR   @12,@12                                             0486 01531000
         STH   @12,VCBRSN(,VCBPTR)                                 0486 01532000
*           GOTO MORRTN;            /* GOING TO RETUTN               */ 01533000
         B     MORRTN                                              0487 01534000
*         END;                                                     0488 01535000
*       RFY                                                        0489 01536000
*        (R1) RSTD;                                                0489 01537000
@RF00484 DS    0H                                                  0490 01538000
*       R1=PTEPTR;                  /* LOAD PAGE TABLE POINTER FOR 0490 01539000
*                                      INVALIDATE CALL               */ 01540000
         LR    R1,PTEPTR                                           0490 01541000
*       CALL IEAVINV;               /* CALL FOR INVALIDATION         */ 01542000
         L     @15,PVTPINV(,PVTPTR)                                0491 01543000
         BALR  @14,@15                                             0491 01544000
*       RFY                                                        0492 01545000
*        (R1) UNRSTD;                                              0492 01546000
*     END;                                                         0493 01547000
*                                                                  0493 01548000
*   /*****************************************************************/ 01549000
*   /*                                                               */ 01550000
*   /* ELSE -- NO CHANGE OCCURED AND AUX STORAGE WAS ASSIGNED.       */ 01551000
*   /*                                                               */ 01552000
*   /*****************************************************************/ 01553000
*                                                                  0494 01554000
*   ELSE                                                           0494 01555000
*     DO;                                                          0494 01556000
         B     @RC00481                                            0494 01557000
@RF00481 DS    0H                                                  0495 01558000
*       CALL XFERP;                 /* CALL FOR AUX TRANSFER         */ 01559000
         BAL   @14,XFERP                                           0495 01560000
*       IF RETCODR^=0 THEN          /* IF PAGE TRANSFER WAS NOT    0496 01561000
*                                      SUCCESSFUL                    */ 01562000
         LTR   RETCODR,RETCODR                                     0496 01563000
         BZ    @RF00496                                            0496 01564000
*         DO;                                                      0497 01565000
*           IF RETCODR=4 THEN       /* IF THE SOURCE WAS INVALID     */ 01566000
         C     RETCODR,@CF00053                                    0498 01567000
         BNE   @RF00498                                            0498 01568000
*             DO;                                                  0499 01569000
*               RCARCRD='04'X;      /* SET RECOVERY CODE             */ 01570000
         L     @12,RCAPTR(,PVTPTR)                                 0500 01571000
         MVI   RCARCRD(@12),X'04'                                  0500 01572000
*               GENERATE SETS(R1);                                 0501 01573000
      ABEND X'C0D',,,SYSTEM        LOG THE ERROR                        01574000
*               GOTO PAGEOUT;       /* GOING TO DO A PAGEOUT         */ 01575000
         B     PAGEOUT                                             0502 01576000
*             END;                                                 0503 01577000
*           VCBELPID='1'B;          /* SET THE LPID ERROR FLAG IN THE   01578000
*                                      VCB                           */ 01579000
@RF00498 OI    VCBELPID(VCBPTR),B'00100000'                        0504 01580000
*           RETCODR=4;              /* SET THE ERROR CODE            */ 01581000
         LA    RETCODR,4                                           0505 01582000
*           GOTO MOERRTN;           /* GOING TO ERROR HANDLING     0506 01583000
*                                      ROUTINE                       */ 01584000
         B     MOERRTN                                             0506 01585000
*         END;                                                     0507 01586000
*       IF VCBNDISC='1'B THEN       /* IF THE NON-DISCONNECT OPTION     01587000
*                                      IS SET                        */ 01588000
@RF00496 TM    VCBNDISC(VCBPTR),B'00001000'                        0508 01589000
         BO    @RT00508                                            0508 01590000
*         GOTO CONNECT;             /* GOING TO SET UP XPTE          */ 01591000
*       RFY                                                        0510 01592000
*        (R1) RSTD;                                                0510 01593000
*       R1=PTEPTR;                  /* LOAD PAGE TABLE ADDRESS FOR 0511 01594000
*                                      INVALIDATE                    */ 01595000
         LR    R1,PTEPTR                                           0511 01596000
*       CALL IEAVINV;               /* CALL FOR INVALIDATION         */ 01597000
         L     @15,PVTPINV(,PVTPTR)                                0512 01598000
         BALR  @14,@15                                             0512 01599000
*       RFY                                                        0513 01600000
*        (R1) UNRSTD;                                              0513 01601000
*       IF VCBDSPID^=0 THEN         /* IF DATASET PAGE I D WAS     0514 01602000
*                                      SUPPLIED                      */ 01603000
         L     @12,VCBDSPID(,VCBPTR)                               0514 01604000
         LTR   @12,@12                                             0514 01605000
         BNZ   @RT00514                                            0514 01606000
*         GOTO RECLAIM;             /* GO TO RECLAIM IN THE NULLMOVE    01607000
*                                      CODE                          */ 01608000
*     END;                                                         0516 01609000
*   GOTO NORECLM;                   /* GO TO NORECLM IN THE NULLMOVE    01610000
*                                      CODE                          */ 01611000
         B     NORECLM                                             0517 01612000
*                                                                  0518 01613000
*   /*****************************************************************/ 01614000
*   /*                                                               */ 01615000
*   /* NULLMOVE -- THIS IS THE NULLMOVE CODE FOR THE MOVEOUT         */ 01616000
*   /* SUB-ROUTINE. THE PAGE IS IMMEDIATELY INVALIDATED. IF SAVE AUX */ 01617000
*   /* IS NOT SET AND AUX IS ASSIGNED, THE PAGE MUST BE RELEASED, AND*/ 01618000
*   /* NO RECLAIM IS ATTEMPTED. IF SAVE AUX IS SET AND A PAGE I D WAS*/ 01619000
*   /* SUPPLIED A TEST IS MADE TO SEE IF THE PAGE HAS CHANGED, AND IF*/ 01620000
*   /* NOT, A RECLAIM IS MADE. IF THE IN-STORAGE OPTION WAS SPECIFIED*/ 01621000
*   /* CONTROL TRANSFERS TO CLRPGTE, OTHERWISE THE PFTE MUST FIRST BE*/ 01622000
*   /* SENT TO THE AFQ. IF NO PAGE I D WAS SUPPLIED, NO RECLAIM IS   */ 01623000
*   /* POSSIBLE, AND THE FRAME IS SENT TO THE AFQ. FOR ALL CASES     */ 01624000
*   /* WHERE THE PFTE IS MOVED, THE APPROPRIATE FRAME COUNT IS       */ 01625000
*   /* DECREMENTED AND CONTROL TRANSFERS TO CLRPGTE FOR CLEANUP      */ 01626000
*   /*                                                               */ 01627000
*   /*****************************************************************/ 01628000
*                                                                  0518 01629000
*NULLMOVE:                                                         0518 01630000
*   RFY                                                            0518 01631000
*    (R1) RSTD;                                                    0518 01632000
NULLMOVE DS    0H                                                  0519 01633000
*   R1=PTEPTR;                      /* LOAD PARAMETER FOR INVALIDATE    01634000
*                                      ROUTINE                       */ 01635000
         LR    R1,PTEPTR                                           0519 01636000
*   CALL IEAVINV;                   /* CALL FOR INVALIDATION         */ 01637000
         L     @15,PVTPINV(,PVTPTR)                                0520 01638000
         BALR  @14,@15                                             0520 01639000
*   RFY                                                            0521 01640000
*    (R1) UNRSTD;                                                  0521 01641000
*   IF XPTVIOLP='0'B THEN           /* IF SAVE AUX FLAG IS NOT SET 0522 01642000
*                                                            @Z40WPYC*/ 01643000
         TM    XPTVIOLP(XPTEPTR),B'10000000'                       0522 01644000
         BNZ   @RF00522                                            0522 01645000
*     DO;                                                          0523 01646000
*       IF XPTXAV='1'B THEN         /* IF AUX IS ASSIGNED            */ 01647000
         TM    XPTXAV(XPTEPTR),B'01000000'                         0524 01648000
         BNO   @RF00524                                            0524 01649000
*         CALL RELEASEP;            /* CALL FOR PAGE RELEASE         */ 01650000
         BAL   @14,RELEASEP                                        0525 01651000
*       GOTO NORECLM;               /* RECLAIM IS NOT POSSIBLE       */ 01652000
         B     NORECLM                                             0526 01653000
*     END;                                                         0527 01654000
*   IF VCBDSPID^=0 THEN             /* IF A PAGE I D WAS SUPPLIED    */ 01655000
@RF00522 L     @12,VCBDSPID(,VCBPTR)                               0528 01656000
         LTR   @12,@12                                             0528 01657000
         BZ    @RF00528                                            0528 01658000
*     DO;                                                          0529 01659000
*       RFY                                                        0530 01660000
*        (TESTPTR) RSTD;                                           0530 01661000
*       TESTPTR=PGTRSA;             /* LOAD FOR CHANGE BIT TEST      */ 01662000
         SLR   TESTPTR,TESTPTR                                     0531 01663000
         ICM   TESTPTR,3,PGTRSA(PTEPTR)                            0531 01664000
*       GENERATE REFS(TESTPTR);                                    0532 01665000
      SRL   TESTPTR,4(0)           CLEAR THE FLAGS                      01666000
      SLL   TESTPTR,12(0)          ADJUST TO A REAL ADDRESS             01667000
      RRB   0(TESTPTR)             TEST CHANGE BIT                      01668000
*       BC(5,NORECLM);              /* IF PAGE CHANGED, GOING TO NO     01669000
*                                      RECLAIM CODE                  */ 01670000
         BC    5,NORECLM                                           0533 01671000
*       GENERATE REFS(TESTPTR);                                    0534 01672000
      RRB   2048(TESTPTR)          TEST CHANGE BIT IN 2ND HALF BLOCK    01673000
*       BC(5,NORECLM);              /* IF PAGE CHANGED, GOING TO NO     01674000
*                                      RECLAIM CODE                  */ 01675000
         BC    5,NORECLM                                           0535 01676000
*                                                                  0536 01677000
*       /*************************************************************/ 01678000
*       /*                                                           */ 01679000
*       /* A DATA SET I D WAS SUPPLIED, SAVE AUX WAS SPECIFIED, AND  */ 01680000
*       /* THE PAGE HAS NOT CHANGED, SO THE FRAME WILL BE MARKED AS A*/ 01681000
*       /* VIO FRAME (PFTIRRG). IF THE IN-STORAGE OPTION WAS NOT     */ 01682000
*       /* REQUESTED, THE FRAME WILL THEN GO TO THE END OF THE       */ 01683000
*       /* AVAILABLE FRAME QUEUE.                                    */ 01684000
*       /*                                                           */ 01685000
*       /*************************************************************/ 01686000
*                                                                  0536 01687000
*RECLAIM:                                                          0536 01688000
*       RFY                                                        0536 01689000
*        (TESTPTR) UNRSTD;                                         0536 01690000
RECLAIM  DS    0H                                                  0537 01691000
*       PFTIRRG='1'B;               /* SET THE VIO FRAME FLAG        */ 01692000
         OI    PFTIRRG(PFTEPTR),B'00010000'                        0537 01693000
*       PFTPGID=VCBDSPID;           /* SET PFTE PAGE I D             */ 01694000
         L     @12,VCBDSPID(,VCBPTR)                               0538 01695000
         ST    @12,PFTPGID(,PFTEPTR)                               0538 01696000
*       VCBRSN=PGTRSA&RSNMASK;      /* SET REAL STORAGE NUMBER IN  0539 01697000
*                                      VCB, STRIPPING FLAG BITS      */ 01698000
         L     @12,@CF02042                                        0539 01699000
         SLR   @11,@11                                             0539 01700000
         ICM   @11,3,PGTRSA(PTEPTR)                                0539 01701000
         NR    @12,@11                                             0539 01702000
         STH   @12,VCBRSN(,VCBPTR)                                 0539 01703000
*       IF VCBINSTG='1'B THEN       /* IF IN STORAGE OPTION WAS    0540 01704000
*                                      SPECIFIED                     */ 01705000
         TM    VCBINSTG(VCBPTR),B'00010000'                        0540 01706000
         BO    @RT00540                                            0540 01707000
*         GOTO CLRPGTE;             /* GOING TO CLEAN UP TABLES AND     01708000
*                                      RETURN                        */ 01709000
*       RFY                                                        0542 01710000
*        (R0,                                                      0542 01711000
*         R1) RSTD;                                                0542 01712000
*                                                                  0542 01713000
*       /*************************************************************/ 01714000
*       /*                                                           */ 01715000
*       /* --NOTE-- THIS PARAMETER IS VALID ONLY AS LONG AS THE AFQ  */ 01716000
*       /* NUMBER REMAINS EQUAL TO X 00.                             */ 01717000
*       /*                                                           */ 01718000
*       /*************************************************************/ 01719000
*                                                                  0543 01720000
*       R0=PGTRSA&RSNMASK;          /* LOAD THE PARM REG FOR PFTE MGR   01721000
*                                      -NOTE-                        */ 01722000
         LR    R0,@12                                              0543 01723000
*       R1=ASCBPTR;                 /* LOAD FOR PFTE CALL            */ 01724000
         L     R1,ASCBPTR                                          0544 01725000
*       CALL IEAVPFTE;              /* CALL PFTE MGR TO QUE PFTE     */ 01726000
         L     @15,PVTPFTE(,PVTPTR)                                0545 01727000
         BALR  @14,@15                                             0545 01728000
*     END;                                                         0546 01729000
*   ELSE                            /* IF NO PAGE I D WAS SUPPLIED   */ 01730000
*     DO;                                                          0547 01731000
*                                                                  0547 01732000
         B     @RC00528                                            0547 01733000
@RF00528 DS    0H                                                  0548 01734000
*       /*************************************************************/ 01735000
*       /*                                                           */ 01736000
*       /* NO DATA SET PAGE I D WAS SUPPLIED, SAVE AUX WAS NOT       */ 01737000
*       /* SPECIFIED, OR THE PAGE CHANGED, SO THE FRAME WILL GO TO   */ 01738000
*       /* THE FRONT OF THE AVAILABLE FRAME QUEUE.                   */ 01739000
*       /*                                                           */ 01740000
*       /*************************************************************/ 01741000
*                                                                  0548 01742000
*NORECLM:                                                          0548 01743000
*       VCBRSN=0;                   /* SET VCBRSN TO ZERO TO INDICATE   01744000
*                                      RECLAIM OF THIS FRAME IS NOT     01745000
*                                      POSSIBLE.                     */ 01746000
NORECLM  SLR   @12,@12                                             0548 01747000
         STH   @12,VCBRSN(,VCBPTR)                                 0548 01748000
*       QRBN=PGTRSA&RSNMASK;        /* REAL STORAGE NUMBER FOR PFTE     01749000
*                                      MGR                           */ 01750000
         L     @12,@CF02042                                        0549 01751000
         SLR   @11,@11                                             0549 01752000
         ICM   @11,3,PGTRSA(PTEPTR)                                0549 01753000
         NR    @12,@11                                             0549 01754000
         STH   @12,QRBN(,PVTPTR)                                   0549 01755000
*       QHEADFLG=QHEADBIT;          /* INDICATE FRONT OF QUE         */ 01756000
         MVI   QHEADFLG(PVTPTR),X'80'                              0550 01757000
*       QINDEX=PFTAFQN;             /* SEND PFTE TO AVAILABLE FRAME     01758000
*                                      QUE                           */ 01759000
         MVI   QINDEX(PVTPTR),X'00'                                0551 01760000
*       R0=PARMBLD;                 /* LOAD THE PARM REG FOR PFTE MGR*/ 01761000
         L     R0,PARMBLD(,PVTPTR)                                 0552 01762000
*       R1=ASCBPTR;                 /* LOAD FOR PSI CALL             */ 01763000
         L     R1,ASCBPTR                                          0553 01764000
*       CALL IEAVPFTE;              /* CALL PFTE MGR TO QUE PFTE     */ 01765000
         L     @15,PVTPFTE(,PVTPTR)                                0554 01766000
         BALR  @14,@15                                             0554 01767000
*       RFY                                                        0555 01768000
*        (R0,                                                      0555 01769000
*         R1,                                                      0555 01770000
*         R13) UNRSTD;                                             0555 01771000
*     END;                                                         0556 01772000
*   RFY                                                            0557 01773000
*    (PFTEPTR) UNRSTD;                                             0557 01774000
@RC00528 DS    0H                                                  0558 01775000
*                                                                  0558 01776000
*   /*****************************************************************/ 01777000
*   /*                                                               */ 01778000
*   /* CLRPGTE -- THIS CODE CLEARS THE PAGE TABLE ENTRY WITH THE     */ 01779000
*   /* EXCEPTION OF THE GETMAIN ASSIGNED BIT AND THE INVALID BIT. THE*/ 01780000
*   /* AUX ASSIGNED AND SAVE AUX BITS ARE RESET AND THE LPID IS      */ 01781000
*   /* CLEARED. CONTROL THEN RETURNS TO THE CALLER OF THE MOVEOUT    */ 01782000
*   /* SUB-ROUTINE, VAMSI.                                           */ 01783000
*   /*                                                               */ 01784000
*   /*****************************************************************/ 01785000
*                                                                  0558 01786000
*CLRPGTE:                                                          0558 01787000
*   PGTPTE=RESETHWD;                /* CLEAR THE PAGE TABLE ENTRY  0558 01788000
*                                      LEAVING GETMAIN ASSIGNED AND     01789000
*                                      INVALID BITS ON               */ 01790000
CLRPGTE  MVC   PGTPTE(2,PTEPTR),@CB02048                           0558 01791000
*   XPTXAV='0'B;                    /* RESET THE AUX ASSIGNED FLAG   */ 01792000
*   XPTVIOLP='0'B;                  /* RESET THE SAVE AUX FLAG     0560 01793000
*                                                            @Z40WPYC*/ 01794000
         NI    XPTXAV(XPTEPTR),B'00111111'                         0560 01795000
*   XPTLPID=''B;                    /* CLEAR THE LPID        @Z40WPYC*/ 01796000
         XC    XPTLPID(8,XPTEPTR),XPTLPID(XPTEPTR)                 0561 01797000
*   XPTVALID='0'B;                  /* MARK LSID AS INVALID  @Z40WPYC*/ 01798000
*   XPTIOERR='0'B;                  /* CLEAR I/O ERROR FLAG IN CASE     01799000
*                                      WAS SET ON DURING LAST PAGE-IN   01800000
*                                      BY ASM. THIS IS NORMALLY DONE    01801000
*                                      BY ASM EXCEPT FOR VIO       0563 01802000
*                                      MOVEOUTS.             @Z40WPYC*/ 01803000
*   XPTPOINP='0'B;                  /* CLEAR PAGEOUT IN PROCESS FLAG.   01804000
*                                      AGAIN THIS WOULD NORMALLY BE     01805000
*                                      DONE BY ASM           @Z40WPYC*/ 01806000
*                                                                  0564 01807000
         NI    XPTVALID(XPTEPTR),B'01001111'                       0564 01808000
*   /*****************************************************************/ 01809000
*   /*                                                               */ 01810000
*   /* MOVEOUT SUB-ROUTINE RETURN POINT                              */ 01811000
*   /*                                                               */ 01812000
*   /*****************************************************************/ 01813000
*                                                                  0565 01814000
*   RFY                                                            0565 01815000
*    (R14) RSTD;                                                   0565 01816000
*MORRTN:                                                           0566 01817000
*   RETCODR=0;                      /* SET THE RETURN CODE,        0566 01818000
*                                      SUCCESSFUL MOVE OUT           */ 01819000
MORRTN   SLR   RETCODR,RETCODR                                     0566 01820000
*MOERRTN:                                                          0567 01821000
*   R14=SUBRETWD;                   /* RESTORE THE RETURN ADDRESS    */ 01822000
MOERRTN  L     R14,SUBRETWD(,PVTPTR)                               0567 01823000
*   RFY                                                            0568 01824000
*    (PTEPTR,                                                      0568 01825000
*     XPTEPTR,                                                     0568 01826000
*     R14) UNRSTD;                                                 0568 01827000
*   END MOVEOUT;                                                   0569 01828000
@EL00003 DS    0H                                                  0569 01829000
@EF00003 DS    0H                                                  0569 01830000
@ER00003 BR    @14                                                 0569 01831000
*                                                                  0570 01832000
*   /*****************************************************************/ 01833000
*   /*                                                               */ 01834000
*   /* OUTOFSTG -- THE OUT OF STORAGE SUB-ROUTINE OF VAMSI. THIS     */ 01835000
*   /* ROUTINE IS CALLED BY THE MOVEOUT SUB-ROUTINE IF A PAGE IS     */ 01836000
*   /* INVALID WHEN A MOVEOUT WAS REQUESTED, AND RETURNS DIRECTLY TO */ 01837000
*   /* VAMSI. THE PTEPTR AND XPTEPTR ARE PASSED TO THIS ROUTINE WHEN */ 01838000
*   /* ENTRY IS MADE. IF THE NON-DISCONNECT OPTION WAS REQUESTED AND */ 01839000
*   /* A LOGICAL PAGE I D WAS SUPPLIED THE AUX ASSIGNED BIT IS TESTED*/ 01840000
*   /* FOR BEING ON. IF SET THE SAVE AUX BIT IS TESTED FOR BEING OFF.*/ 01841000
*   /* WHEN THESE TWO CONDITIONS ARE MET THE PAGE IS TRANSFERED VIA  */ 01842000
*   /* THE ILRCALL MACRO. IF SAVE AUX WAS REQUESTED, THE VCB AND XPTE*/ 01843000
*   /* LOGICAL PAGE I D'S ARE TESTED FOR BEING EQUAL. IF THEY DO NOT */ 01844000
*   /* MATCH AN ERROR IS SIGNALED AND AN ERROR RETURN MADE. IF EQUAL,*/ 01845000
*   /* THE VCBRSN IS RESET AND A NORMAL RETURN IS MADE.              */ 01846000
*   /*                                                               */ 01847000
*   /*****************************************************************/ 01848000
*                                                                  0570 01849000
*OUTOFSTG:                                                         0570 01850000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0570 01851000
*                                                                  0570 01852000
OUTOFSTG DS    0H                                                  0571 01853000
*   /*****************************************************************/ 01854000
*   /*                                                               */ 01855000
*   /* CHECK FOR NON-DISCONNECT OPTION                               */ 01856000
*   /*                                                               */ 01857000
*   /*****************************************************************/ 01858000
*                                                                  0571 01859000
*   RFY                                                            0571 01860000
*    (PTEPTR,                                                      0571 01861000
*     XPTEPTR) RSTD;                                               0571 01862000
*   IF VCBNDISC='1'B&(MYVCBLGN^=0|MYVCBLPN^=0) THEN/* IF LPID WAS  0572 01863000
*                                      SUPPLIED AND THE            0572 01864000
*                                      NON-DISCONNECT OPTION IS    0572 01865000
*                                      SPECIFIED                     */ 01866000
         TM    VCBNDISC(VCBPTR),B'00001000'                        0572 01867000
         BNO   @RF00572                                            0572 01868000
         SLR   @14,@14                                             0572 01869000
         CL    @14,MYVCBLGN(,VCBPTR)                               0572 01870000
         BNE   @RT00572                                            0572 01871000
         CL    @14,MYVCBLPN(,VCBPTR)                               0572 01872000
         BE    @RF00572                                            0572 01873000
@RT00572 DS    0H                                                  0573 01874000
*     DO;                                                          0573 01875000
*       IF XPTXAV='1'B THEN         /* IF AUX STORAGE IS ASSIGNED    */ 01876000
         TM    XPTXAV(XPTEPTR),B'01000000'                         0574 01877000
         BNO   @RF00574                                            0574 01878000
*         DO;                                                      0575 01879000
*           IF XPTVIOLP='0'B THEN   /* IF THE SAVE AUX FLAG IS NOT 0576 01880000
*                                      SET                   @Z40WPYC*/ 01881000
         TM    XPTVIOLP(XPTEPTR),B'10000000'                       0576 01882000
         BNZ   @RF00576                                            0576 01883000
*             DO;                                                  0577 01884000
*               CALL XFERP;         /* CALL FOR AUX TRANSFER         */ 01885000
         BAL   @14,XFERP                                           0578 01886000
*               IF RETCODR^=0 THEN  /* IF TRANSFER WAS UNSUCCESSFUL  */ 01887000
         LTR   RETCODR,RETCODR                                     0579 01888000
         BNZ   @RT00579                                            0579 01889000
*                 GOTO IDERROR;     /* GOING TO RETURN FOR ERROR     */ 01890000
*               XPTLPID=VCBLPID;    /* UPDATE THE LOGICAL PAGE I D   */ 01891000
         MVC   XPTLPID(8,XPTEPTR),VCBLPID(VCBPTR)                  0581 01892000
*               XPTVIOLP='1'B;      /* MARK THE XPTE AS SAVE AUX   0582 01893000
*                                                            @Z40WPYC*/ 01894000
*                                                                  0582 01895000
         OI    XPTVIOLP(XPTEPTR),B'10000000'                       0582 01896000
*               /*****************************************************/ 01897000
*               /*                                                   */ 01898000
*               /* CLEAR FLAGS NORMALLY CLEARED BY ASM BUT NOT FOR   */ 01899000
*               /* VIO MOVEOUTS OR TRANSFER PAGE OPERATIONS. @Z40WPYC*/ 01900000
*               /*                                                   */ 01901000
*               /*****************************************************/ 01902000
*                                                                  0583 01903000
*               XPTVALID='0'B;      /* CLEAR LSID VALID FLAG @Z40WPYC*/ 01904000
*               XPTIOERR='0'B;      /* CLEAR I/O ERROR FLAG  @Z40WPYC*/ 01905000
*               XPTPOINP='0'B;      /* CLEAR PAGEOUT IN OPER FLAG  0585 01906000
*                                                            @Z40WPYC*/ 01907000
         NI    XPTVALID(XPTEPTR),B'01001111'                       0585 01908000
*             END;                                                 0586 01909000
*           ELSE                    /* IF SAVE AUX WAS SPECIFIED     */ 01910000
*             IF VCBLPID^=XPTLPID THEN/* IF NO LOGICAL PAGE MATCH    */ 01911000
         B     @RC00576                                            0587 01912000
@RF00576 CLC   VCBLPID(8,VCBPTR),XPTLPID(XPTEPTR)                  0587 01913000
         BE    @RF00587                                            0587 01914000
*               DO;                                                0588 01915000
*IDERROR:                                                          0589 01916000
*                 VCBELPID='1'B;    /* MARK THE VCB AS AN LPID ERROR */ 01917000
IDERROR  OI    VCBELPID(VCBPTR),B'00100000'                        0589 01918000
*                 RETCODR=4;        /* SET THE ERROR RETURN CODE     */ 01919000
         LA    RETCODR,4                                           0590 01920000
*                 GOTO OUTERRTN;    /* GOING TO THE ERROR RETURN CODE*/ 01921000
         B     OUTERRTN                                            0591 01922000
*               END;                                               0592 01923000
*         END;                                                     0593 01924000
*       ELSE                        /* IF NO AUX STORAGE WAS ASSIGNED*/ 01925000
*         VCBNOAUX='1'B;            /* SET THE VCB NO AUX FLAG       */ 01926000
         B     @RC00574                                            0594 01927000
@RF00574 OI    VCBNOAUX(VCBPTR),B'00010000'                        0594 01928000
*       VCBRSN=0;                   /* NO REAL STORAGE EXISTS        */ 01929000
@RC00574 SLR   @14,@14                                             0595 01930000
         STH   @14,VCBRSN(,VCBPTR)                                 0595 01931000
*       GOTO OUTRTN;                /* FINISHED PROCESSING, GOING  0596 01932000
*                                      BACK                          */ 01933000
         B     OUTRTN                                              0596 01934000
*     END;                                                         0597 01935000
*                                                                  0598 01936000
*   /*****************************************************************/ 01937000
*   /*                                                               */ 01938000
*   /* PCBSRCH -- THIS CODE CONSTITUTES THE PCB SEARCH CODE OF THE   */ 01939000
*   /* OUTOFSTG SUB-ROUTINE. IF THE NON-DISCONNECT OPTION WAS NOT    */ 01940000
*   /* SPECIFIED, OR IF NO LOGICAL PAGE I D WAS SUPPLIED, THIS CODE  */ 01941000
*   /* WILL SEARCH THE QUEUES FOR THE PCB FOR THE PAGE TO BE MOVED   */ 01942000
*   /* OUT. IF THE DEFER OR FORCE RELATE FLAGS ARE ON, THE GFA DEFER */ 01943000
*   /* QUEUE IS SEARCHED FIRST. IF THE CORRECT PCB IS NOT FOUND THE  */ 01944000
*   /* IO QUEUE WILL THEN BE SEARCHED. IF BOTH DEFER AND FREE REAL   */ 01945000
*   /* WERE OFF, THE GFA QUEUE IS NOT SEARCHED. IF THE CORRECT PCB   */ 01946000
*   /* CANNOT BE FOUND, CONTROL WILL TRANSFER TO THE AUXTEST CODE,   */ 01947000
*   /* ELSE WHEN THE PCB IS FOUND (VBN MATCH AND IF A LOCAL PAGE AN  */ 01948000
*   /* ADDRESS SPACE MATCH) A CHECK IS MADE TO DETERMINE WHICH QUEUE */ 01949000
*   /* THE PCB IS ON. IF ON THE GFA QUEUE, CONTROL PASSES TO GFADEFR,*/ 01950000
*   /* OTHERWISE THE VBN IS ZEROED AND THE FREE REAL FLAG IS TURNED  */ 01951000
*   /* ON IN ALL THE PCBS (REGULAR AND ALL RELATEDS).                */ 01952000
*   /*                                                               */ 01953000
*   /*****************************************************************/ 01954000
*                                                                  0598 01955000
*PCBSRCH:                                                          0598 01956000
*   IF XPTDEFER='1'B THEN           /* IF THE DEFER FLAG IS ON     0598 01957000
*                                                            @Z40WPYC*/ 01958000
@RF00572 DS    0H                                                  0598 01959000
PCBSRCH  TM    XPTDEFER(XPTEPTR),B'00000010'                       0598 01960000
         BNO   @RF00598                                            0598 01961000
*     DO;                                                          0599 01962000
*       RFY                                                        0600 01963000
*        (GFASW,                                                   0600 01964000
*         PCBPTR) RSTD;                                            0600 01965000
*       GFASW='01'X;                /* INDICATE SEARCHING GFAQ-A   0601 01966000
*                                      SWITCH                        */ 01967000
         LA    GFASW,1                                             0601 01968000
*       PCBPTR=PVTGFADF;            /* GET PCB FROM DEFER QUE        */ 01969000
         L     PCBPTR,PVTGFADF(,PVTPTR)                            0602 01970000
*     END;                                                         0603 01971000
*   ELSE                                                           0604 01972000
*     DO;                                                          0604 01973000
         B     @RC00598                                            0604 01974000
@RF00598 DS    0H                                                  0605 01975000
*IOQSRCH:                                                          0605 01976000
*       PCBPTR=RSMLIOQF;            /* GET FIRST PCB ON LOCAL QUE    */ 01977000
IOQSRCH  L     PCBPTR,RSMLIOQF(,RSMHDPTR)                          0605 01978000
*       GFASW='00'X;                /* INDICATE SEARCHING I0 QUE - A    01979000
*                                      SWITCH                        */ 01980000
         SLR   GFASW,GFASW                                         0606 01981000
*     END;                                                         0607 01982000
*PCBLOOP:                                                          0608 01983000
*   IF PCBPTR=0 THEN                /* IF AT THE END OF THE CHAIN,   */ 01984000
@RC00598 DS    0H                                                  0608 01985000
PCBLOOP  LTR   PCBPTR,PCBPTR                                       0608 01986000
         BNZ   @RF00608                                            0608 01987000
*     IF GFASW='01'X THEN           /* AND WE JUST SEARCHED THE GFA     01988000
*                                      QUE                           */ 01989000
         C     GFASW,@CF01765                                      0609 01990000
         BE    @RT00609                                            0609 01991000
*       GOTO IOQSRCH;               /* SEARCH THE IO QUE             */ 01992000
*     ELSE                          /* IF WE JUST SEARCHED THE IO QUE*/ 01993000
*       GOTO AUXTEST;               /* LEAVE THE ROUTINE, GOING TO 0611 01994000
*                                      AUX STORAGE TEST ROUTINE      */ 01995000
         B     AUXTEST                                             0611 01996000
*   IF PCBVBN^=CURVSNR|PCBASCB^=PSAAOLD THEN/* IF VBNS DO NOT MATCH,    01997000
*                                      OR IF PAGE IS NOT FOR THIS  0612 01998000
*                                      ADDRESS SPACE         @Z40WPYC*/ 01999000
@RF00608 LH    @14,PCBVBN(,PCBPTR)                                 0612 02000000
         N     @14,@CF02294                                        0612 02001000
         CLR   CURVSNR,@14                                         0612 02002000
         BNE   @RT00612                                            0612 02003000
         CLC   PCBASCB(4,PCBPTR),PSAAOLD                           0612 02004000
         BE    @RF00612                                            0612 02005000
@RT00612 DS    0H                                                  0613 02006000
*     DO;                                                          0613 02007000
*       PCBPTR=PCBFQPA;             /* GET NEXT PCB ON CHAIN         */ 02008000
         L     @14,PCBFQPA-1(,PCBPTR)                              0614 02009000
         LA    @14,0(,@14)                                         0614 02010000
         LR    PCBPTR,@14                                          0614 02011000
*       GOTO PCBLOOP;               /* TEST NEXT PCB                 */ 02012000
         B     PCBLOOP                                             0615 02013000
*     END;                                                         0616 02014000
*                                                                  0616 02015000
*   /*****************************************************************/ 02016000
*   /*                                                               */ 02017000
*   /* FOUND THE PCB, DETERMINE QUEUE AND TRANSFER CONTROL IF ON THE */ 02018000
*   /* GFA DEFER QUEUE                                               */ 02019000
*   /*                                                               */ 02020000
*   /*****************************************************************/ 02021000
*                                                                  0617 02022000
*   IF GFASW='01'X THEN             /* IF PCB WAS ON GFA QUE         */ 02023000
@RF00612 C     GFASW,@CF01765                                      0617 02024000
         BE    @RT00617                                            0617 02025000
*     GOTO GFADEFR;                 /* GO TO THE GFA HANDLING ROUTINE*/ 02026000
*   IF PCBIOI='0'B THEN             /* IF FOUND PCB IS INPUT @Z40WPYC*/ 02027000
         TM    PCBIOI(PCBPTR),B'00000010'                          0619 02028000
         BNZ   @RF00619                                            0619 02029000
*     DO;                           /* DETERMINE STATE OF I/O      0620 02030000
*                                                            @Z40WPYC*/ 02031000
*       IF PCBIOCMP='0'B THEN       /* IF INPUT NOT COMPLETE, SET BIT   02032000
*                                      SO AUXTEST SUBR. CAN SET    0621 02033000
*                                      AIAFRAUX              @Z40WPYC*/ 02034000
         TM    PCBIOCMP(PCBPTR),B'00000001'                        0621 02035000
         BNZ   @RF00621                                            0621 02036000
*         PIINPROC='1'B;            /* SET BIT INDICATING PAGE-IN  0622 02037000
*                                                            @Z40WPYC*/ 02038000
         OI    PIINPROC(PVTPTR),B'00000001'                        0622 02039000
*       ELSE                        /* I/O IS COMPLETE.      @Z40WPYC*/ 02040000
*         GOTO SRCHRLTE;            /* BYPASS I/O IN PROCESS       0623 02041000
*                                      PROCESSING            @Z40WPYC*/ 02042000
*     END;                          /* END OF INPUT PCB PROCESSING 0624 02043000
*                                                            @Z40WPYC*/ 02044000
*   ELSE                            /* NOT PAGE-IN           @Z40WPYC*/ 02045000
*     POINPROC='1'B;                /* INDICATE PAGE-OUT IN PROCESS     02046000
*                                                            @Z40WPYC*/ 02047000
         B     @RC00619                                            0625 02048000
@RF00619 OI    POINPROC(PVTPTR),B'00000010'                        0625 02049000
*   IOSVAIA=ADDR(PCBAIA);           /* SAVE THE ADDRESS OF THE AIA 0626 02050000
*                                      FOR WHICH I/O IS OUTSTANDING     02051000
*                                                            @Z40WPYC*/ 02052000
@RC00619 LA    @14,PCBAIA(,PCBPTR)                                 0626 02053000
         STCM  @14,7,IOSVAIA(PVTPTR)                               0626 02054000
*SRCHRLTE:                          /*                       @Z40WPYC*/ 02055000
*   DO WHILE PCBPTR^=0;             /* LOOP DOWN RELATED CHAIN,    0627 02056000
*                                      RESETING THE VBN AND FREAL  0627 02057000
*                                      FLAG IN THE REGULAR PCB AND 0627 02058000
*                                      ALL THE RELATED PCBS          */ 02059000
SRCHRLTE B     @DE00627                                            0627 02060000
@DL00627 DS    0H                                                  0628 02061000
*     PCBVBN=0;                     /* RESET THE VBN                 */ 02062000
         SLR   @14,@14                                             0628 02063000
         STH   @14,PCBVBN(,PCBPTR)                                 0628 02064000
*     PCBFREAL='1'B;                /* SET THE FREE REAL FLAG        */ 02065000
         OI    PCBFREAL(PCBPTR),B'10000000'                        0629 02066000
*     PCBPTR=PCBRLPA;               /* GET NEXT RELATED PCB          */ 02067000
         L     @14,PCBRLPA-1(,PCBPTR)                              0630 02068000
         LA    @14,0(,@14)                                         0630 02069000
         LR    PCBPTR,@14                                          0630 02070000
*   END;                                                           0631 02071000
@DE00627 LTR   PCBPTR,PCBPTR                                       0631 02072000
         BNZ   @DL00627                                            0631 02073000
*   RFY                                                            0632 02074000
*    (GFASW) UNRSTD;                                               0632 02075000
*                                                                  0633 02076000
*   /*****************************************************************/ 02077000
*   /*                                                               */ 02078000
*   /* AUXTEST -- THIS IS THE AUXILIARY STORAGE ASSIGNMENT TEST OF   */ 02079000
*   /* THE OUTOFSTG SUB-ROUTINE. IF NO AUX STORAGE IS ASSIGNED THE   */ 02080000
*   /* VCBNOAUX FLAG IS SET AND PROCESSING CONTINUES AT THE LABEL    */ 02081000
*   /* FINAL. IF AUX STORAGE IS ASSIGNED AND NO LOGICAL PAGE I D IS  */ 02082000
*   /* SUPPLIED, THE PAGE WILL BE RELEASED UNLESS THE SAVE AUX FLAG  */ 02083000
*   /* IS ON. IF A LPID IS SUPPLIED AND THE SAVE AUX FLAG IS NOT ON, */ 02084000
*   /* THE PAGE WILL BE TRANSFERED (ILRCALL MACRO). IF THE SAVE AUX  */ 02085000
*   /* FLAG IS ON (AND LPID GIVEN) A CHECK IS MADE TO INSURE THAT THE*/ 02086000
*   /* VCBLPID AND XPTLPID MATCH. IF THEY DO NOT AN ERROR IS SIGNALED*/ 02087000
*   /* AND RETURN MADE TO VAMSI. -- FINAL -- THE PAGE TABLE ENTRY IS */ 02088000
*   /* CLEARED WITH THE EXCEPTION OF THE GETMAIN ASSIGNED AND INVALID*/ 02089000
*   /* BITS. THE XPTE IS UPDATED AND RETURN MADE TO THE CALLER.      */ 02090000
*   /*                                                               */ 02091000
*   /*****************************************************************/ 02092000
*                                                                  0633 02093000
*AUXTEST:                                                          0633 02094000
*   IF XPTXAV='0'B THEN             /* IF AUX IS NOT ASSIGNED        */ 02095000
AUXTEST  TM    XPTXAV(XPTEPTR),B'01000000'                         0633 02096000
         BNZ   @RF00633                                            0633 02097000
*     VCBNOAUX='1'B;                /* SET THE NO AUX FLAG IN THE VCB*/ 02098000
         OI    VCBNOAUX(VCBPTR),B'00010000'                        0634 02099000
*   ELSE                                                           0635 02100000
*     DO;                                                          0635 02101000
         B     @RC00633                                            0635 02102000
@RF00633 DS    0H                                                  0636 02103000
*       IF MYVCBLGN=0&MYVCBLPN=0 THEN/* IF NO LOGICAL PAGE I D       */ 02104000
         SLR   @14,@14                                             0636 02105000
         CL    @14,MYVCBLGN(,VCBPTR)                               0636 02106000
         BNE   @RF00636                                            0636 02107000
         CL    @14,MYVCBLPN(,VCBPTR)                               0636 02108000
         BNE   @RF00636                                            0636 02109000
*         DO;                                                      0637 02110000
*           IF XPTVIOLP='0'B THEN   /* IF SAVE AUX IS NOT REQUESTED     02111000
*                                                            @Z40WPYC*/ 02112000
         TM    XPTVIOLP(XPTEPTR),B'10000000'                       0638 02113000
         BNZ   @RF00638                                            0638 02114000
*             DO;                   /* DETERMINE IF I/O IS         0639 02115000
*                                      OUTSTANDING           @Z40WPYC*/ 02116000
*               IF POINPROC='1'B|PIINPROC='1'B THEN/* IF PAGEOUT OR     02117000
*                                      PAGEIN IS IN PROCESS  @Z40WPYC*/ 02118000
         TM    POINPROC(PVTPTR),B'00000011'                        0640 02119000
         BZ    @RF00640                                            0640 02120000
*                 DO;               /* SET INFO FOR ASM      @Z40WPYC*/ 02121000
*                   PCBPTR=IOSVAIA-(LENGTH(PCB)-LENGTH(PCBAIA));/* 0642 02122000
*                                      RECOMPUTE ADDRESS OF PCB    0642 02123000
*                                                            @Z40WPYC*/ 02124000
         L     PCBPTR,IOSVAIA-1(,PVTPTR)                           0642 02125000
         LA    PCBPTR,0(,PCBPTR)                                   0642 02126000
         SL    PCBPTR,@CF02276                                     0642 02127000
*                   AIAFRAUX='1'B;  /* TELL ASM TO FREE THE AUX STOR    02128000
*                                      SLOT WHEN THE I/O COMPLETES.     02129000
*                                                            @Z40WPYC*/ 02130000
         OI    AIAFRAUX(PCBPTR),B'10000000'                        0643 02131000
*                 END;              /* END I/O PROCESSING    @Z40WPYC*/ 02132000
*               ELSE                /* NO I/O IS IN PROCESS  @Z40WPYC*/ 02133000
*                 CALL RELEASEP;    /* FREE AUX STOR NOW     @Z40WPYC*/ 02134000
         B     @RC00640                                            0645 02135000
@RF00640 BAL   @14,RELEASEP                                        0645 02136000
*             END;                  /* END OF LSID IN XPTE         0646 02137000
*                                      PROCESSING. NOTE THAT IF PCB     02138000
*                                      WAS ON GFA DEFER QUEUE, THE 0646 02139000
*                                      PCB WAS PREVIOUSLY FREED.   0646 02140000
*                                                            @Z40WPYC*/ 02141000
*         END;                                                     0647 02142000
*       ELSE                        /* IF THERE IS A LOGICAL PAGE    */ 02143000
*         IF XPTVIOLP='0'B THEN     /* AND SAVE AUX IS NOT REQUESTED    02144000
*                                                            @Z40WPYC*/ 02145000
         B     @RC00636                                            0648 02146000
@RF00636 TM    XPTVIOLP(XPTEPTR),B'10000000'                       0648 02147000
         BNZ   @RF00648                                            0648 02148000
*           DO;                                                    0649 02149000
*             CALL XFERP;           /* CALL FOR AUX TRANSFER         */ 02150000
         BAL   @14,XFERP                                           0650 02151000
*             IF RETCODR^=0 THEN    /* IF THE TRANSFER FAILED        */ 02152000
         LTR   RETCODR,RETCODR                                     0651 02153000
         BNZ   @RT00651                                            0651 02154000
*               GOTO IDERROR;       /* GOING TO LOGICAL PAGE I D   0652 02155000
*                                      ERROR ROUTINE                 */ 02156000
*           END;                                                   0653 02157000
*         ELSE                      /* IF SAVE AUX IS REQUESTED CHECK   02158000
*                                      LOGICAL PAGE MATCH            */ 02159000
*           IF VCBLPID^=XPTLPID THEN/* IF LOGICAL PAGES DO NOT MATCH */ 02160000
         B     @RC00648                                            0654 02161000
@RF00648 CLC   VCBLPID(8,VCBPTR),XPTLPID(XPTEPTR)                  0654 02162000
         BNE   @RT00654                                            0654 02163000
*             GOTO IDERROR;         /* GOING TO LOGICAL PAGE I D   0655 02164000
*                                      ERROR ROUTINE                 */ 02165000
*     END;                                                         0656 02166000
@RC00648 DS    0H                                                  0656 02167000
@RC00636 DS    0H                                                  0657 02168000
*                                                                  0657 02169000
*   /*****************************************************************/ 02170000
*   /*                                                               */ 02171000
*   /* CLEAN UP CONTROL BLOCKS BEFORE RETURN.                        */ 02172000
*   /*                                                               */ 02173000
*   /*****************************************************************/ 02174000
*                                                                  0657 02175000
*FINAL:                                                            0657 02176000
*   PGTPTE=RESETHWD;                /* ZERO THE PGTE, KEEP GM ASSIGN    02177000
*                                      AND INVALID BITS ON           */ 02178000
@RC00633 DS    0H                                                  0657 02179000
FINAL    MVC   PGTPTE(2,PTEPTR),@CB02048                           0657 02180000
*   VCBRSN=0;                       /* CLEAR THE RSN - NO REAL     0658 02181000
*                                      STORAGE                       */ 02182000
         SLR   @14,@14                                             0658 02183000
         STH   @14,VCBRSN(,VCBPTR)                                 0658 02184000
*   XPTXAV='0'B;                    /* RESET THE AUX ASSIGNED FLAG   */ 02185000
*   XPTVIOLP='0'B;                  /* RESET THE LPID VALID FLAG   0660 02186000
*                                                            @Z40WPYC*/ 02187000
         NI    XPTXAV(XPTEPTR),B'00111111'                         0660 02188000
*   XPTVALID='0'B;                  /* RESET THE LSID VALID FLAG   0661 02189000
*                                                            @Z40WPYC*/ 02190000
*   XPTPOINP='0'B;                  /* TURN OFF PAGEOUT IN PROCESS 0662 02191000
*                                      FLAG. ASM WILL NOT TURN OFF 0662 02192000
*                                      WHEN FRAUX IS SET OR OUTPUT 0662 02193000
*                                      OPERATION IS A VIO MOVEOUT. 0662 02194000
*                                                            @Z40WPYC*/ 02195000
*   XPTIOERR='0'B;                  /* TURN OFF I/O ERROR FLAG. ASM     02196000
*                                      WILL NOT TURN OFF WHEN FRAUX     02197000
*                                      IS SET OR OR OUTPUT OPERATION    02198000
*                                      IS A VIO MOVEOUT.     @Z40WPYC*/ 02199000
         NI    XPTVALID(XPTEPTR),B'01001111'                       0663 02200000
*   XPTLPID=''B;                    /* CLEAR THE LPID        @Z40WPYC*/ 02201000
         XC    XPTLPID(8,XPTEPTR),XPTLPID(XPTEPTR)                 0664 02202000
*   GOTO OUTRTN;                    /* GOING TO RETURN               */ 02203000
         B     OUTRTN                                              0665 02204000
*                                                                  0666 02205000
*   /*****************************************************************/ 02206000
*   /*                                                               */ 02207000
*   /* GFADEFR -- THIS CODE HANDLES ALL PCBS THAT WERE FOUND TO BE ON*/ 02208000
*   /* THE GFA DEFER QUEUE. THE DEFER FLAG IS IMMEDIATELY RESET. IF  */ 02209000
*   /* RESET NEED NOT BE ACCOMPLISHED (PCB RESET FLAG OFF), THE ROOT */ 02210000
*   /* BIT IS TESTED. IF A ROOT EXISTS, THE COUNT IS DECREMENTED BY  */ 02211000
*   /* 1. IF THE COUNT IS NOW ZERO AND THIS IS A COMMON AREA PAGE,   */ 02212000
*   /* THE COUNT IS FORCED TO 1. IF NOT A COMMON AREA PAGE, THE ROOT */ 02213000
*   /* EXIT ROUTINE IS CALLED TO PROCESS THAT ROOT PCB. IF THE RESET */ 02214000
*   /* FLAG WAS ON, A TEST IS MADE TO SEE IF THE PCB IS NOT IN SRB   */ 02215000
*   /* MODE AND THAT THE PAGE IS A COMMON PAGE. IF THIS IS TRUE THE  */ 02216000
*   /* FREE REAL, VBN AND RBN FIELDS ARE SET IN THE PCB, AND THE IO  */ 02217000
*   /* COMPLETE FLAG IS TURNED ON. THE SCHEDULE FUNCTION OF PIOP IS  */ 02218000
*   /* THEN CALLED TO SCHEDULE AN SRB. IF THE ABOVE TEST WAS NOT     */ 02219000
*   /* TRUE, RESET MUST BE CALLED TO MAKE ROUTINE DISPATCHABLE AGAIN.*/ 02220000
*   /* AT LABEL SAVRELAT, THE PCB DEFER BIT IS RESET, AND THE PCBS   */ 02221000
*   /* SENT TO THE CIO OR FREE QUEUE AS APPROPRIATE. IF THERE IS ONE */ 02222000
*   /* OR MORE RELATED PCBS, THE ENTIRE LOOP IS REPEATED FOR EACH    */ 02223000
*   /* ONE.                                                          */ 02224000
*   /*                                                               */ 02225000
*   /*****************************************************************/ 02226000
*                                                                  0666 02227000
*GFADEFR:                                                          0666 02228000
*   XPTDEFER='0'B;                  /* RESET THE DEFER FLAG          */ 02229000
*                                                                  0666 02230000
GFADEFR  NI    XPTDEFER(XPTEPTR),B'11111101'                       0666 02231000
*   /*****************************************************************/ 02232000
*   /*                                                               */ 02233000
*   /* CHECK FOR PCB RESET BIT                                       */ 02234000
*   /*                                                               */ 02235000
*   /*****************************************************************/ 02236000
*                                                                  0667 02237000
*RELTEST:                                                          0667 02238000
*   IF PCBRESET='0'B THEN           /* IF RESET IS NOT SPECIFIED     */ 02239000
RELTEST  TM    PCBRESET(PCBPTR),B'00010000'                        0667 02240000
         BNZ   @RF00667                                            0667 02241000
*     DO;                                                          0668 02242000
*       IF PCBROOT='1'B THEN        /* IF A ROOT PCB EXISTS          */ 02243000
         TM    PCBROOT(PCBPTR),B'00000100'                         0669 02244000
         BNO   @RF00669                                            0669 02245000
*         DO;                                                      0670 02246000
*           PCBRCNT=PCBRCNT-1;      /* DECREMENT COUNT IN ROOT PCB   */ 02247000
         L     @14,PCBRPTR-1(,PCBPTR)                              0671 02248000
         LA    @14,0(,@14)                                         0671 02249000
         LH    @12,PCBRCNT(,@14)                                   0671 02250000
         BCTR  @12,0                                               0671 02251000
         STH   @12,PCBRCNT(,@14)                                   0671 02252000
*           IF PCBRCNT=0 THEN       /* IF THE ROOT COUNT = 0 THEN    */ 02253000
         LTR   @12,@12                                             0672 02254000
         BNZ   @RF00672                                            0672 02255000
*             DO;                                                  0673 02256000
*               RFY                                                0674 02257000
*                (R0,                                              0674 02258000
*                 R1) RSTD;                                        0674 02259000
*               R1=PCBRTPA;         /* LOAD THE ROOT PCB ADDRESS     */ 02260000
         L     R1,PCBRTPA-1(,PCBPTR)                               0675 02261000
         LA    R1,0(,R1)                                           0675 02262000
*               CALL ROOTEXIT;      /* CALL THE ROOT EXIT ROUTINE FOR   02263000
*                                      LOCAL PAGES                   */ 02264000
         L     @15,PCBRGOTO(,@14)                                  0676 02265000
         BALR  @14,@15                                             0676 02266000
*               RCACSECT=RCACVAM;   /* RESTORE AFTER CALL            */ 02267000
         L     @14,RCAPTR(,PVTPTR)                                 0677 02268000
         MVI   RCACSECT(@14),X'18'                                 0677 02269000
*             END;                                                 0678 02270000
*         END;                                                     0679 02271000
*     END;                                                         0680 02272000
*                                                                  0681 02273000
*   /*****************************************************************/ 02274000
*   /*                                                               */ 02275000
*   /* ELSE - THIS CODE EXECUTES IF RESET (PCBRESET=1) WAS SET.      */ 02276000
*   /*                                                               */ 02277000
*   /*****************************************************************/ 02278000
*                                                                  0681 02279000
*   ELSE                            /* IF RESET IS SPECIFIED         */ 02280000
*     DO;                                                          0681 02281000
         B     @RC00667                                            0681 02282000
@RF00667 DS    0H                                                  0682 02283000
*       RFY                                                        0682 02284000
*        (R13) RSTD;                                               0682 02285000
*       RSETSV4=R4;                 /* SAVE ACROSS CALL              */ 02286000
         ST    R4,RSETSV4(,PVTPTR)                                 0683 02287000
*       RSETSV5=R5;                 /* SAVE ACROSS CALL              */ 02288000
         ST    R5,RSETSV5(,PVTPTR)                                 0684 02289000
*       RSETSV6=R6;                 /* SAVE ACROSS CALL              */ 02290000
         ST    R6,RSETSV6(,PVTPTR)                                 0685 02291000
*       IF PCBSRBMD='1'B THEN       /* IF PCB IS IN SRB MODE         */ 02292000
         TM    PCBSRBMD(PCBPTR),B'00100000'                        0686 02293000
         BNO   @RF00686                                            0686 02294000
*         DO;                                                      0687 02295000
*           R4=PCBSRB;              /* LOAD THE SRB ADDRESS FOR CALL */ 02296000
         L     R4,PCBSRB(,PCBPTR)                                  0688 02297000
*           R5=0;                   /* CLEAR PARAMETER               */ 02298000
         SLR   R5,R5                                               0689 02299000
*         END;                                                     0690 02300000
*       ELSE                        /* IF PCB IS NOT IN SRB MODE     */ 02301000
*         DO;                                                      0691 02302000
         B     @RC00686                                            0691 02303000
@RF00686 DS    0H                                                  0692 02304000
*           R4=PCBRTPA;             /* LOAD TCB ADDRESS              */ 02305000
         L     R4,PCBRTPA-1(,PCBPTR)                               0692 02306000
         LA    R4,0(,R4)                                           0692 02307000
*           R5=PCBSRB;              /* LOAD RB FOR CALL              */ 02308000
         L     R5,PCBSRB(,PCBPTR)                                  0693 02309000
*         END;                                                     0694 02310000
*       R13=ADDR(PVTSAVE);          /* SUPPLY A SAVE AREA            */ 02311000
@RC00686 LA    R13,PVTSAVE(,PVTPTR)                                0695 02312000
*       R6=0;                       /* CLEAR PARAMETER               */ 02313000
         SLR   R6,R6                                               0696 02314000
*       CALL IEAVRSET;              /* CALL FOR RESET                */ 02315000
         L     @15,PVTPRSET(,PVTPTR)                               0697 02316000
         BALR  @14,@15                                             0697 02317000
*       R4=RSETSV4;                 /* RESTORE AFTER CALL            */ 02318000
         L     R4,RSETSV4(,PVTPTR)                                 0698 02319000
*       R5=RSETSV5;                 /* RESTORE AFTER CALL            */ 02320000
         L     R5,RSETSV5(,PVTPTR)                                 0699 02321000
*       R6=RSETSV6;                 /* RESTORE AFTER CALL            */ 02322000
         L     R6,RSETSV6(,PVTPTR)                                 0700 02323000
*       RFY                                                        0701 02324000
*        (R11,                                                     0701 02325000
*         R12,                                                     0701 02326000
*         R13) UNRSTD;                                             0701 02327000
*     END;                                                         0702 02328000
*                                                                  0703 02329000
*   /*****************************************************************/ 02330000
*   /*                                                               */ 02331000
*   /* SEND THE PCB TO THE FREE QUEUE, THEN HANDLE ANY RELATED PCBS  */ 02332000
*   /* THAT MAY EXIST.                                               */ 02333000
*   /*                                                               */ 02334000
*   /*****************************************************************/ 02335000
*                                                                  0703 02336000
*FREETQN:                                                          0703 02337000
*   R0=PCBFREQN;                    /* LOAD THE QUE NUMBER FOR PCB 0703 02338000
*                                      MANAGER                       */ 02339000
@RC00667 DS    0H                                                  0703 02340000
FREETQN  LA    R0,16                                               0703 02341000
*   R1=PCBPTR;                      /* LOAD THE PCB ADDRESS FOR PCB     02342000
*                                      MANAGER                       */ 02343000
         LR    R1,PCBPTR                                           0704 02344000
*   PCBPTR=PCBRLPA;                 /* GET RELATED PCB BEFORE      0705 02345000
*                                      RETURNING IT                  */ 02346000
         L     @14,PCBRLPA-1(,PCBPTR)                              0705 02347000
         LA    @14,0(,@14)                                         0705 02348000
         LR    PCBPTR,@14                                          0705 02349000
*   R1->PCBRLPA=0;                  /* CLEAR THE RELATED FIELD       */ 02350000
         SLR   @14,@14                                             0706 02351000
         STCM  @14,7,PCBRLPA(R1)                                   0706 02352000
*   CALL IEAVPCB;                   /* CALL MANAGER TO MOVE PCB      */ 02353000
         L     @15,PVTPCB(,PVTPTR)                                 0707 02354000
         BALR  @14,@15                                             0707 02355000
*   RFY                                                            0708 02356000
*    (R0,                                                          0708 02357000
*     R1) UNRSTD;                                                  0708 02358000
*   IF PCBPTR^=0 THEN               /* IF THERE IS A RELATED         */ 02359000
         SLR   @14,@14                                             0709 02360000
         CR    PCBPTR,@14                                          0709 02361000
         BE    @RF00709                                            0709 02362000
*     DO;                                                          0710 02363000
*       PCBFQPA=0;                  /* MAKE THE RELATED PCB A REGULAR   02364000
*                                      PCB                           */ 02365000
         STCM  @14,7,PCBFQPA(PCBPTR)                               0711 02366000
*       PCBBQP=0;                   /* CLEAR THE BACK POINTER        */ 02367000
         ST    @14,PCBBQP(,PCBPTR)                                 0712 02368000
*       GOTO RELTEST;               /* LOOP TO HANDLE THE RELATED PCB*/ 02369000
         B     RELTEST                                             0713 02370000
*     END;                                                         0714 02371000
*   GOTO AUXTEST;                   /* IF NO MORE PCBS, GOING TO AUX    02372000
*                                      TEST ROUTINE                  */ 02373000
         B     AUXTEST                                             0715 02374000
*                                                                  0716 02375000
*   /*****************************************************************/ 02376000
*   /*                                                               */ 02377000
*   /* OUTRTN -- THIS IS THE RETURN POINT FOR THE OUTOFSTG           */ 02378000
*   /* SUB-ROUTINE. BOTH NORMAL AND ERROR RETURNS TO VAMSI ARE MADE  */ 02379000
*   /* FROM THIS POINT.                                              */ 02380000
*   /*                                                               */ 02381000
*   /*****************************************************************/ 02382000
*                                                                  0716 02383000
*OUTRTN:                                                           0716 02384000
*   RETCODR=0;                      /* SET THE NORMAL RETURN CODE    */ 02385000
OUTRTN   SLR   RETCODR,RETCODR                                     0716 02386000
*   RFY                                                            0717 02387000
*    (R14) RSTD;                                                   0717 02388000
*OUTERRTN:                                                         0718 02389000
*   R14=SUBRETWD;                   /* LOAD THE RETURN ADDRESS       */ 02390000
OUTERRTN L     R14,SUBRETWD(,PVTPTR)                               0718 02391000
*   RFY                                                            0719 02392000
*    (PCBPTR,                                                      0719 02393000
*     PTEPTR,                                                      0719 02394000
*     XPTEPTR,                                                     0719 02395000
*     R14) UNRSTD;                                                 0719 02396000
*   END OUTOFSTG;                                                  0720 02397000
@EL00004 DS    0H                                                  0720 02398000
@EF00004 DS    0H                                                  0720 02399000
@ER00004 BR    @14                                                 0720 02400000
*                                                                  0721 02401000
*   /*****************************************************************/ 02402000
*   /*                                                               */ 02403000
*   /* RELEASEP -- THIS IS THE RELEASE AUX SUB-ROUTINE OF VAMSI. IT  */ 02404000
*   /* IS CALLED BY THE MOVEOUT AND OUTOFSTG SUB-ROUTINES TO RELEASE */ 02405000
*   /* AUX VIA A CALL TO ASM FREE SLOT ROUTINE. THE XPTEPTR MUST BE  */ 02406000
*   /* VALID WHEN THE CALL IS MADE.                                  */ 02407000
*   /*                                                               */ 02408000
*   /*****************************************************************/ 02409000
*                                                                  0721 02410000
*RELEASEP:                                                         0721 02411000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0721 02412000
RELEASEP DS    0H                                                  0722 02413000
*   RFY                                                            0722 02414000
*    (PTEPTR,                                                      0722 02415000
*     PFTEPTR,                                                     0722 02416000
*     XPTEPTR,                                                     0722 02417000
*     PCBPTR,                                                      0722 02418000
*     R13,                                                         0722 02419000
*     R14) RSTD;                                                   0722 02420000
*   SUBSUBWD=R14;                   /* SAVE THE RETURN REG           */ 02421000
*                                                                  0723 02422000
         ST    R14,SUBSUBWD(,PVTPTR)                               0723 02423000
*   /*****************************************************************/ 02424000
*   /*                                                               */ 02425000
*   /* ESTABLISH INTERFACE TO ILRFRSLT                       @Z40WPYC*/ 02426000
*   /*                                                               */ 02427000
*   /*****************************************************************/ 02428000
*                                                                  0724 02429000
*   R13=ADDR(PVTWSA4);              /* GIVE MACRO A SAVE AREA        */ 02430000
         LA    R13,PVTWSA4(,PVTPTR)                                0724 02431000
*   R0=4;                           /* INDICATE PRIVATE AREA PAGE  0725 02432000
*                                                            @Z40WPYC*/ 02433000
         LA    R0,4                                                0725 02434000
*   R1=XPTEPTR;                     /* SET PTR TO XPTE - NOTE THAT R2   02435000
*                                      (RSMHDPTR), ALSO INPUT TO FREE   02436000
*                                      SLOT HAS PREVIOUSLY BEEN    0726 02437000
*                                      ESTABLISHED.          @Z40WPYC*/ 02438000
         LR    R1,XPTEPTR                                          0726 02439000
*   CALL ILRFRSLT;                  /* CALL FREE SLOT        @Z40WPYC*/ 02440000
         L     @15,PVTFRSLT(,PVTPTR)                               0727 02441000
         BALR  @14,@15                                             0727 02442000
*   R14=SUBSUBWD;                   /* RESTORE THE RETURN REG        */ 02443000
         L     R14,SUBSUBWD(,PVTPTR)                               0728 02444000
*   RFY                                                            0729 02445000
*    (PTEPTR,                                                      0729 02446000
*     PFTEPTR,                                                     0729 02447000
*     XPTEPTR,                                                     0729 02448000
*     PCBPTR,                                                      0729 02449000
*     R13,                                                         0729 02450000
*     R14) UNRSTD;                                                 0729 02451000
*   END RELEASEP;                   /* END OF SUB-PROC               */ 02452000
@EL00005 DS    0H                                                  0730 02453000
@EF00005 DS    0H                                                  0730 02454000
@ER00005 BR    @14                                                 0730 02455000
*                                                                  0731 02456000
*   /*****************************************************************/ 02457000
*   /*                                                               */ 02458000
*   /* XFERP -- THIS IS THE TRANSFER AUX SUB-ROUTINE OF VAMSI. IT IS */ 02459000
*   /* CALLED BY THE MOVEOUT AND OUTOFSTG SUB-ROUTINES TO TRANSFER   */ 02460000
*   /* AUX VIA THE ILRCALL TRPAGE MACRO. THE XPTEPTR MUST BE VALID   */ 02461000
*   /* WHEN THE CALL IS MADE.                                        */ 02462000
*   /*                                                               */ 02463000
*   /*****************************************************************/ 02464000
*                                                                  0731 02465000
*XFERP:                                                            0731 02466000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0731 02467000
XFERP    DS    0H                                                  0732 02468000
*   RFY                                                            0732 02469000
*    (PTEPTR,                                                      0732 02470000
*     PFTEPTR,                                                     0732 02471000
*     XPTEPTR,                                                     0732 02472000
*     PCBPTR,                                                      0732 02473000
*     R13,                                                         0732 02474000
*     R14) RSTD;                                                   0732 02475000
*   SUBSUBWD=R14;                   /* SAVE THE RETURN REG           */ 02476000
         ST    R14,SUBSUBWD(,PVTPTR)                               0733 02477000
*   R13=ADDR(PVTWSA4);              /* GIVE ASM A SAVEAREA. NOTE THAT   02478000
*                                      IT IS POSSIBLE GETMAIN COULD     02479000
*                                      BE CALLED THRU THIS INTERFACE    02480000
*                                      SO SAVEAREA MUST NOT BE ONE 0734 02481000
*                                      THAT RSM WOULD USE ON THIS  0734 02482000
*                                      CPU.                  @Z40WPYC*/ 02483000
         LA    R13,PVTWSA4(,PVTPTR)                                0734 02484000
*   ACAOP=0;                        /* ZERO OPERATION FIELD  @Z40WPYC*/ 02485000
         MVI   ACAOP(PVTPTR),X'00'                                 0735 02486000
*   ACAFLG1=0;                      /* ZERO FLAG BYTE        @Z40WPYC*/ 02487000
         MVI   ACAFLG1(PVTPTR),X'00'                               0736 02488000
*   IF POINPROC='1'B THEN           /* IF PAGEOUT IN PROCESS @Z40WPYC*/ 02489000
         TM    POINPROC(PVTPTR),B'00000010'                        0737 02490000
         BNO   @RF00737                                            0737 02491000
*     DO;                           /* SET AIA ADDR, NOT LSID IN ACA    02492000
*                                                            @Z40WPYC*/ 02493000
*       ACAAIAP=IOSVAIA;            /* SET AIA IN ACA        @Z40WPYC*/ 02494000
         L     @12,IOSVAIA-1(,PVTPTR)                              0739 02495000
         LA    @12,0(,@12)                                         0739 02496000
         ST    @12,ACAAIAP(,PVTPTR)                                0739 02497000
*       ACAFLSID=0;                 /* ZERO FROM LSID FIELD AS I/O IS   02498000
*                                      NOT COMPLETE AND LSID HAS NOT    02499000
*                                      YET BEEN ASSIGNED     @Z40WPYC*/ 02500000
         SLR   @12,@12                                             0740 02501000
         ST    @12,ACAFLSID(,PVTPTR)                               0740 02502000
*     END;                          /* END PAGEOUT-IN-PROCESS      0741 02503000
*                                      PROCESSNG             @Z40WPYC*/ 02504000
*   ELSE                            /* NOT PAGEOUT I/O IN PROCESS  0742 02505000
*                                                            @Z40WPYC*/ 02506000
*     DO;                           /* SETUP FOR NO I/O      @Z40WPYC*/ 02507000
         B     @RC00737                                            0742 02508000
@RF00737 DS    0H                                                  0743 02509000
*       ACAAIAP=0;                  /* INDICATE NOT PAGEOUT IN     0743 02510000
*                                      PROCESS               @Z40WPYC*/ 02511000
         SLR   @12,@12                                             0743 02512000
         ST    @12,ACAAIAP(,PVTPTR)                                0743 02513000
*       ACAFLSID=XPTLSID;           /* SET FROM LSID         @Z40WPYC*/ 02514000
         L     @12,XPTLSID(,XPTEPTR)                               0744 02515000
         ST    @12,ACAFLSID(,PVTPTR)                               0744 02516000
*     END;                          /* END NO PAGEOUT PROCESSING   0745 02517000
*                                                            @Z40WPYC*/ 02518000
*   ACAASID=RSMASCB->ASCBASID;      /* SET ASID OF REQUESTING A.S. 0746 02519000
*                                                            @Z40WPYC*/ 02520000
@RC00737 L     @12,RSMASCB(,RSMHDPTR)                              0746 02521000
         LH    @12,ASCBASID(,@12)                                  0746 02522000
         N     @12,@CF02294                                        0746 02523000
         STH   @12,ACAASID(,PVTPTR)                                0746 02524000
*   ACATOLP=VCBLPID;                /* SET LPID TO TRANSFER LSID TO     02525000
*                                                            @Z40WPYC*/ 02526000
         MVC   ACATOLP(8,PVTPTR),VCBLPID(VCBPTR)                   0747 02527000
*   DO;                             /* ILRCALL                     0748 02528000
*                                      ACA(ADDR(PVTACA))OP(TRPAGE)   */ 02529000
*     RESPECIFY                                                    0749 02530000
*       R1 RSTD;                                                   0749 02531000
*     DECLARE                       /* ILRGOS ENTRY POINT    @Z40WPWG*/ 02532000
*       ILRGOS ENTRY BASED(ASMGOS) VALRG(0);                       0750 02533000
*     DECLARE                       /* ILRTRPAG ENTRY POINT  @Z40WPWG*/ 02534000
*       ILRTRPAG ENTRY BASED(ASMTRPAG) VALRG(0);                   0751 02535000
*     DCL                                                          0752 02536000
*       ILRACAOP FIXED(8) BASED(R1);                               0752 02537000
*     R1=ADDR(PVTACA);              /* SET OPCODE            @Z40WPWG*/ 02538000
         LA    R1,PVTACA(,PVTPTR)                                  0753 02539000
*     ILRACAOP=4;                                                  0754 02540000
         MVI   ILRACAOP(R1),X'04'                                  0754 02541000
*     CALL ILRTRPAG;                /*                       @Z40WPWG*/ 02542000
         L     @12,CVTPTR                                          0755 02543000
         L     @12,ASMVTPTR(,@12)                                  0755 02544000
         L     @15,ASMTRPAG(,@12)                                  0755 02545000
         BALR  @14,@15                                             0755 02546000
*     RESPECIFY                                                    0756 02547000
*       R1 UNRSTD;                                                 0756 02548000
*   END;                            /* ILRCALL                     0757 02549000
*                                      ACA(ADDR(PVTACA))OP(TRPAGE) 0757 02550000
*                                      TRANS PAGE            @Z40WPYC*/ 02551000
*   R14=SUBSUBWD;                   /* RESTORE THE RETURN REG        */ 02552000
         L     R14,SUBSUBWD(,PVTPTR)                               0758 02553000
*   RFY                                                            0759 02554000
*    (PTEPTR,                                                      0759 02555000
*     PFTEPTR,                                                     0759 02556000
*     XPTEPTR,                                                     0759 02557000
*     PCBPTR,                                                      0759 02558000
*     R13,                                                         0759 02559000
*     R14) UNRSTD;                                                 0759 02560000
*   END XFERP;                      /* END OF SUB-PROC               */ 02561000
@EL00006 DS    0H                                                  0760 02562000
@EF00006 DS    0H                                                  0760 02563000
@ER00006 BR    @14                                                 0760 02564000
*                                                                  0761 02565000
*   /*****************************************************************/ 02566000
*   /*                                                               */ 02567000
*   /* IOREQ - THIS IS THE I/O REQUEST SUB-ROUTINE OF VAMSI. PAGES   */ 02568000
*   /* THAT REQUIRE PAGEOUTS (AS EVIDENCED BY HAVING AN AIA ON THE   */ 02569000
*   /* INTERNAL QUEUE) ARE PROCESSED HERE. ILRPAGIO IS INVOKED TO    */ 02570000
*   /* INITIATE THE ACTUAL PAGEOUT. IF THE RETURN CODE FROM ASM IS   */ 02571000
*   /* ZERO, RETURN IS MADE TO VAMSI. FOR NON-ZERO RETURN CODES, THE */ 02572000
*   /* FAILING PAGE'S VCB IS LOCATED AND MARKED AS BEING IN ERROR.   */ 02573000
*   /* THE PFTE AND PGTE ARE REBUILT AND THE PCB RETURNED TO THE FREE*/ 02574000
*   /* QUEUE. ANY AIA'S REMAINING ON THE QUEUE ARE THEN PROCESSED AS */ 02575000
*   /* ABOVE, BUT REG15 SAVE AREA WILL BE LOADED IF THERE WERE NO    */ 02576000
*   /* PREVIOUS ERRORS. IF THE PROPER VCB CANNOT BE FOUND FOR        */ 02577000
*   /* BACKOUT, THE RETURN CODE WILL BE SET TO 16 TO FORCE AN ABEND, */ 02578000
*   /* AND RETURN IS MADE TO THE MAINLINE.                           */ 02579000
*   /*                                                               */ 02580000
*   /*****************************************************************/ 02581000
*                                                                  0761 02582000
*IOREQ:                                                            0761 02583000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0761 02584000
IOREQ    DS    0H                                                  0762 02585000
*   RFY                                                            0762 02586000
*    (R13,                                                         0762 02587000
*     R14) RSTD;                                                   0762 02588000
*   SUBSUBWD=R14;                   /* SAVE RETURN ADDRESS           */ 02589000
         ST    R14,SUBSUBWD(,PVTPTR)                               0763 02590000
*CALLIO:                                                           0764 02591000
*   RFY                                                            0764 02592000
*    (R1) RSTD;                                                    0764 02593000
*                                                                  0764 02594000
CALLIO   DS    0H                                                  0765 02595000
*   /*****************************************************************/ 02596000
*   /*                                                               */ 02597000
*   /* CALL ASM FOR PAGEOUT                                          */ 02598000
*   /*                                                               */ 02599000
*   /*****************************************************************/ 02600000
*                                                                  0765 02601000
*   R1=VAMAIAQ;                     /* SET PTR TO FIRST PAGEOUT AIA     02602000
*                                                            @Z40WPYC*/ 02603000
         L     R1,VAMAIAQ(,PVTPTR)                                 0765 02604000
*   CALL ILRPAGIO;                  /* CALL ASM TO INITIATE PAGEOUT     02605000
*                                                            @Z40WPYC*/ 02606000
         L     @15,PVTPAGIO(,PVTPTR)                               0766 02607000
         BALR  @14,@15                                             0766 02608000
*   RFY                                                            0767 02609000
*    (R13,                                                         0767 02610000
*     R14) UNRSTD;                                                 0767 02611000
*                                                                  0767 02612000
*   /*****************************************************************/ 02613000
*   /*                                                               */ 02614000
*   /* CHECK RETURN CODE FROM ASM                                    */ 02615000
*   /*                                                               */ 02616000
*   /*****************************************************************/ 02617000
*                                                                  0768 02618000
*   IF RETCODR=0 THEN               /* IF THE I/O WAS SUCCESSFUL     */ 02619000
         SLR   @14,@14                                             0768 02620000
         CR    RETCODR,@14                                         0768 02621000
         BNE   @RF00768                                            0768 02622000
*     VAMAIAQ=0;                    /* RESET THE AIA QUEUE FOR NEXT     02623000
*                                      GROUP                         */ 02624000
         ST    @14,VAMAIAQ(,PVTPTR)                                0769 02625000
*   ELSE                            /* IF THE I/O WAS UNSUCCESSFUL   */ 02626000
*     DO;                                                          0770 02627000
         B     @RC00768                                            0770 02628000
@RF00768 DS    0H                                                  0771 02629000
*       VCBPTR=REG1WD;              /* LOAD VCB POINTER TO SEARCH FOR   02630000
*                                      FAILING VCB                   */ 02631000
*                                                                  0771 02632000
         L     @14,RSTADDWD(,PVTPTR)                               0771 02633000
         L     VCBPTR,REG1WD(,@14)                                 0771 02634000
*       /*************************************************************/ 02635000
*       /*                                                           */ 02636000
*       /* LOCATE PROPER VCB                                         */ 02637000
*       /*                                                           */ 02638000
*       /*************************************************************/ 02639000
*                                                                  0772 02640000
*       RFY                                                        0772 02641000
*        (PCBPTR) RSTD;                                            0772 02642000
*       PCBPTR=R1-LENGTH(PCB)+LENGTH(PCBAIA);/* LOAD THE PCB POINTER */ 02643000
         LR    PCBPTR,R1                                           0773 02644000
         AL    PCBPTR,@CF02275                                     0773 02645000
*VCBCK:                                                            0774 02646000
*       IF VCBMVOUT='0'B|MYVCBLGN^=AIALGID|MYVCBLPN^=AIARPN THEN/* IF   02647000
*                                      NOT THE CORRECT VCB           */ 02648000
VCBCK    TM    VCBMVOUT(VCBPTR),B'10000000'                        0774 02649000
         BZ    @RT00774                                            0774 02650000
         CLC   MYVCBLGN(4,VCBPTR),AIALGID(PCBPTR)                  0774 02651000
         BNE   @RT00774                                            0774 02652000
         CLC   MYVCBLPN(4,VCBPTR),AIARPN(PCBPTR)                   0774 02653000
         BE    @RF00774                                            0774 02654000
@RT00774 DS    0H                                                  0775 02655000
*         DO;                                                      0775 02656000
*           VCBPTR=VCBLINK;         /* UPDATE THE VCB POINTER        */ 02657000
         L     VCBPTR,VCBLINK(,VCBPTR)                             0776 02658000
*           IF VCBPTR^=0 THEN       /* IF THERE IS ANOTHER VCB       */ 02659000
         LTR   VCBPTR,VCBPTR                                       0777 02660000
         BNZ   @RT00777                                            0777 02661000
*             GOTO VCBCK;           /* SEE IF THIS IS THE CORRECT VCB*/ 02662000
*           REG15WD=16;             /* SET RETURN CODE TO CAUSE ABEND*/ 02663000
         L     @14,RSTADDWD(,PVTPTR)                               0779 02664000
         MVC   REG15WD(4,@14),@CF00713                             0779 02665000
*           VAMAIAQ=R1;             /* STORE ADDRESS OF FAILING AIA     02666000
*                                      IN QUEUE                      */ 02667000
         ST    R1,VAMAIAQ(,PVTPTR)                                 0780 02668000
*           RETURN TO RC16ADJ;      /* RETURN TO MAINLINE FOR ABEND  */ 02669000
         LA    @14,RC16ADJ                                         0781 02670000
@EL00007 DS    0H                                                  0781 02671000
@EF00007 DS    0H                                                  0781 02672000
@ER00007 BR    @14                                                 0781 02673000
*         END;                                                     0782 02674000
*       RFY                                                        0783 02675000
*        (R1) UNRSTD;                                              0783 02676000
@RF00774 DS    0H                                                  0784 02677000
*       VCBELPID='1'B;              /* SET THE ERROR LPID FLAG       */ 02678000
         OI    VCBELPID(VCBPTR),B'00100000'                        0784 02679000
*       PVTVAMO=PVTVAMO-1;          /* DECREMENT THE VIO OUT COUNT   */ 02680000
         L     @14,PVTVAMO(,PVTPTR)                                0785 02681000
         BCTR  @14,0                                               0785 02682000
         ST    @14,PVTVAMO(,PVTPTR)                                0785 02683000
*       OUXBVAMO=OUXBVAMO-1;        /* DECREMENT THE LOCAL COUNT     */ 02684000
         L     @14,ASCBPTR                                         0786 02685000
         L     @14,OUXBPTR(,@14)                                   0786 02686000
         L     @12,OUXBVAMO(,@14)                                  0786 02687000
         BCTR  @12,0                                               0786 02688000
         ST    @12,OUXBVAMO(,@14)                                  0786 02689000
*       IF VCBNDISC='1'B THEN       /* IF THE NON-DISCONNECT OPTION     02690000
*                                      WAS SPECIFIED                 */ 02691000
         TM    VCBNDISC(VCBPTR),B'00001000'                        0787 02692000
         BNO   @RF00787                                            0787 02693000
*         DO;                                                      0788 02694000
*           PCBXPTA->XPTXAV='0'B;   /* RESET THE AUX ASSIGNED FLAG   */ 02695000
*           PCBXPTA->XPTVIOLP='0'B; /* RESET THE SAVE AUX FLAG     0790 02696000
*                                                            @Z40WPYC*/ 02697000
         L     @14,PCBXPTA-1(,PCBPTR)                              0790 02698000
         LA    @14,0(,@14)                                         0790 02699000
         NI    XPTXAV(@14),B'00111111'                             0790 02700000
*           PCBXPTA->XPTPOINP='0'B; /* CLEAR PAGEOUT IN PROC @Z40WPYC*/ 02701000
*           PCBXPTA->XPTIOERR='0'B; /* CLEAR I/O ERROR FLAG  @Z40WPYC*/ 02702000
*           PCBXPTA->XPTVALID='0'B; /* CLEAR LSID VALID FLAG @Z40WPYC*/ 02703000
         NI    XPTPOINP(@14),B'01001111'                           0793 02704000
*           PCBXPTA->XPTLPID=''B;   /* CLEAR THE LPID        @Z40WPYC*/ 02705000
         XC    XPTLPID(8,@14),XPTLPID(@14)                         0794 02706000
*         END;                                                     0795 02707000
*                                                                  0795 02708000
*       /*************************************************************/ 02709000
*       /*                                                           */ 02710000
*       /* RE-ADJUST PGTE AND PFTE                                   */ 02711000
*       /*                                                           */ 02712000
*       /*************************************************************/ 02713000
*                                                                  0796 02714000
*ADJPGTE:                                                          0796 02715000
*       PCBPGTA->PGTRSA=PCBRBN;     /* LOAD THE PAGE TABLE           */ 02716000
@RF00787 DS    0H                                                  0796 02717000
ADJPGTE  L     @14,PCBPGTA-1(,PCBPTR)                              0796 02718000
         LA    @14,0(,@14)                                         0796 02719000
         MVC   PGTRSA(2,@14),PCBRBN(PCBPTR)                        0796 02720000
*       PCBPGTA->PGTPAM='1'B;       /* SET THE GETMAINED BIT IN PGTE */ 02721000
         OI    PGTPAM(@14),B'00000001'                             0797 02722000
*       RFY                                                        0798 02723000
*        (PFTEPTR) RSTD;                                           0798 02724000
*       PFTEPTR=PVTPFTP+PCBRBN;     /* LOAD THE PFTE POINTER         */ 02725000
         LH    PFTEPTR,PCBRBN(,PCBPTR)                             0799 02726000
         N     PFTEPTR,@CF02294                                    0799 02727000
         AL    PFTEPTR,PVTPFTP(,PVTPTR)                            0799 02728000
*       PFTIRRG='0'B;               /* RESET THE IRREGULAR BIT       */ 02729000
         NI    PFTIRRG(PFTEPTR),B'11101111'                        0800 02730000
*       PFTPCBSI='0'B;              /* RESET THE PCB DEFINED FLAG    */ 02731000
         NI    PFTPCBSI(PFTEPTR),B'11110111'                       0801 02732000
*       PFTASID=ASCBASID;           /* LOAD THE ASID                 */ 02733000
         L     @14,ASCBPTR                                         0802 02734000
         LH    @14,ASCBASID(,@14)                                  0802 02735000
         N     @14,@CF02294                                        0802 02736000
         STH   @14,PFTASID(,PFTEPTR)                               0802 02737000
*       PFTVBN=CURVSN;              /* LOAD THE PROPER VSN           */ 02738000
         MVC   PFTVBN(2,PFTEPTR),CURVSN(VCBPTR)                    0803 02739000
*       RFY                                                        0804 02740000
*        (PFTEPTR) UNRSTD;                                         0804 02741000
*       RFY                                                        0805 02742000
*        (R0,                                                      0805 02743000
*         R1,                                                      0805 02744000
*         TESTPTR) RSTD;                                           0805 02745000
*                                                                  0805 02746000
*       /*************************************************************/ 02747000
*       /*                                                           */ 02748000
*       /* FORCE THE CHANGE BIT ON FOR PAGE RECOVERY. IF THE COMPARE */ 02749000
*       /* AND SWAP IS SUCESSFUL THE CHANGE BIT WILL BE SET ON. IF IT*/ 02750000
*       /* IS NOT (CC NOT ZERO) SOMEONE ELSE HAS FORCED CHANGE BIT ON*/ 02751000
*       /* AND THE OBJECTIVE IS STILL MET                            */ 02752000
*       /*                                                           */ 02753000
*       /*************************************************************/ 02754000
*                                                                  0806 02755000
*       TESTPTR=VCBVSA;             /* LOAD ADDRESS OF WORD IN STG   */ 02756000
         L     TESTPTR,VCBVSA(,VCBPTR)                             0806 02757000
*       GENERATE REFS(TESTPTR) SETS(R1);                           0807 02758000
      L  R1,0(TESTPTR)             LOAD FIRST WORD                      02759000
      CS R1,R1,0(TESTPTR)          COMPARE AND SWAP TO SET CHANGE BIT   02760000
*       VAMAIAQ=AIANXAIA;           /* LOAD HEADER TO POINT TO NEXT     02761000
*                                      AIA                           */ 02762000
*                                                                  0808 02763000
         L     @14,AIANXAIA(,PCBPTR)                               0808 02764000
         ST    @14,VAMAIAQ(,PVTPTR)                                0808 02765000
*       /*************************************************************/ 02766000
*       /*                                                           */ 02767000
*       /* SEND PCB TO FREE QUEUE                                    */ 02768000
*       /*                                                           */ 02769000
*       /*************************************************************/ 02770000
*                                                                  0809 02771000
*       R0=PCBFREQN;                /* SEND PCB TO FREE QUEUE        */ 02772000
         LA    R0,16                                               0809 02773000
*       R1=PCBPTR;                  /* LOAD PARM FOR PCB MANAGER     */ 02774000
         LR    R1,PCBPTR                                           0810 02775000
*       CALL IEAVPCB;               /* CALL PCB MANAGER              */ 02776000
         L     @15,PVTPCB(,PVTPTR)                                 0811 02777000
         BALR  @14,@15                                             0811 02778000
*       RFY                                                        0812 02779000
*        (R0,                                                      0812 02780000
*         R1,                                                      0812 02781000
*         TESTPTR,                                                 0812 02782000
*         PCBPTR) UNRSTD;                                          0812 02783000
*       IF REG15WD=0 THEN           /* IF THIS IS THE FIRST ERROR  0813 02784000
*                                      ENCOUNTERED                   */ 02785000
         L     @14,RSTADDWD(,PVTPTR)                               0813 02786000
         L     @12,REG15WD(,@14)                                   0813 02787000
         LTR   @12,@12                                             0813 02788000
         BNZ   @RF00813                                            0813 02789000
*         DO;                                                      0814 02790000
*           REG15WD=4;              /* LOAD A RETURN CODE            */ 02791000
         MVC   REG15WD(4,@14),@CF00053                             0815 02792000
*           REG2WD=LASTVCB;         /* LOAD ADDRESS OF LAST VCB    0816 02793000
*                                      PROCESSED                     */ 02794000
         L     @12,LASTVCB(,PVTPTR)                                0816 02795000
         ST    @12,REG2WD(,@14)                                    0816 02796000
*         END;                                                     0817 02797000
*                                                                  0817 02798000
*       /*************************************************************/ 02799000
*       /*                                                           */ 02800000
*       /* CHECK FOR MORE AIA'S ON QUEUE                             */ 02801000
*       /*                                                           */ 02802000
*       /*************************************************************/ 02803000
*                                                                  0818 02804000
*       IF VAMAIAQ^=0 THEN          /* IF THERE ARE MORE AIA'S       */ 02805000
@RF00813 L     @14,VAMAIAQ(,PVTPTR)                                0818 02806000
         LTR   @14,@14                                             0818 02807000
         BNZ   @RT00818                                            0818 02808000
*         GOTO CALLIO;              /* PROCESS REST OF AIA GROUP     */ 02809000
*     END;                                                         0820 02810000
*   RFY                                                            0821 02811000
*    (R14) RSTD;                                                   0821 02812000
@RC00768 DS    0H                                                  0822 02813000
*   R14=SUBSUBWD;                   /* RESTORE RETURN ADDRESS        */ 02814000
         L     R14,SUBSUBWD(,PVTPTR)                               0822 02815000
*   RFY                                                            0823 02816000
*    (R14) UNRSTD;                                                 0823 02817000
*   END IOREQ;                      /* END OF SUB-PROC, RETURNING TO    02818000
*                                      CALLER                        */ 02819000
         B     @EL00007                                            0824 02820000
*   END IEAVAMSI                    /* END OF IEAVAMSI               */ 02821000
*                                                                  0825 02822000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 02823000
*/*%INCLUDE SYSLIB  (CVT     )                                       */ 02824000
