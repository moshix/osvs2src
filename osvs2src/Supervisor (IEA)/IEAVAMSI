         BNZ   @RF00262                                            0262 00633000
@RT00262 DS    0H                                                  0263 00634000
*           PCBPTR=PCBFQPA;         /* GET NEXT ON CHAIN             */ 00635000
         L     @11,PCBFQPA-1(,PCBPTR)                              0263 00636000
         LA    @11,0(,@11)                                         0263 00637000
         LR    PCBPTR,@11                                          0263 00638000
*         ELSE                      /* IF THE CORRECT PCB WAS FOUND  */ 00639000
*           DO;                     /* IF PCB WAS FOUND              */ 00640000
*                                                                  0264 00641000
         B     @RC00262                                            0264 00642000
@RF00262 DS    0H                                                  0265 00643000
*             /*******************************************************/ 00644000
*             /*                                                     */ 00645000
*             /* THIS PCB CAN ONLY BE A VIO MOVEOUT PCB BECAUSE THE  */ 00646000
*             /* PCBVIO FLAG IS ON. IT IS CONVERTED INTO WHAT APPEARS*/ 00647000
*             /* TO BE A PGOUT WITH KEEPREAL OPTION. THE PCBVIO FLAG */ 00648000
*             /* REMAINS ON. THE PAGE WILL BE VALIDATED WHEN THE I/O */ 00649000
*             /* COMPLETES.                                          */ 00650000
*             /*                                                     */ 00651000
*             /*******************************************************/ 00652000
*                                                                  0265 00653000
*             PCBVBN=CURVSNR;       /* STORE VBN OF PAGE IN PCB      */ 00654000
         STH   CURVSNR,PCBVBN(,PCBPTR)                             0265 00655000
*             PCBPGTA=PTEPTR;       /* LOAD ADDRESS OF PGTE          */ 00656000
         STCM  PTEPTR,7,PCBPGTA(PCBPTR)                            0266 00657000
*             PCBXPTA=XPTEPTR;      /* LOAD ADDRESS OF XPGT          */ 00658000
         STCM  XPTEPTR,7,PCBXPTA(PCBPTR)                           0267 00659000
*             PCBFREAL='0'B;        /* RESET THE FREE REAL BIT       */ 00660000
         NI    PCBFREAL(PCBPTR),B'01111111'                        0268 00661000
*             PGTRSA=PGTRSA|VCBRSN; /* LOAD RSN, RETAIN FLAGS        */ 00662000
         LH    @11,VCBRSN(,VCBPTR)                                 0269 00663000
         N     @11,@CF02294                                        0269 00664000
         SLR   @01,@01                                             0269 00665000
         ICM   @01,3,PGTRSA(PTEPTR)                                0269 00666000
         OR    @11,@01                                             0269 00667000
         STCM  @11,3,PGTRSA(PTEPTR)                                0269 00668000
*             PFTASID=ASCBASID;     /* SET CURRENT ASID IN PFTE      */ 00669000
         L     @11,ASCBPTR                                         0270 00670000
         LH    @11,ASCBASID(,@11)                                  0270 00671000
         N     @11,@CF02294                                        0270 00672000
         STH   @11,PFTASID(,PFTEPTR)                               0270 00673000
*             PFTVBN=CURVSNR;       /* SET VBN OF PAGE IN PFTE       */ 00674000
         STH   CURVSNR,PFTVBN(,PFTEPTR)                            0271 00675000
*             PFTIRRG='0'B;         /* RESET THE VIO FRAME FLAG      */ 00676000
         NI    PFTIRRG(PFTEPTR),B'11101111'                        0272 00677000
*             PFTUIC=0;             /* RESET THE INTERVAL COUNT      */ 00678000
         MVI   PFTUIC(PFTEPTR),X'00'                               0273 00679000
*             GOTO RCLMCNTS;        /* KEEPING THE PAGE              */ 00680000
         B     RCLMCNTS                                            0274 00681000
*           END;                                                   0275 00682000
*       END;                                                       0276 00683000
@RC00262 DS    0H                                                  0276 00684000
@DE00261 LTR   PCBPTR,PCBPTR                                       0276 00685000
         BNZ   @DL00261                                            0276 00686000
*       RCARCRD='07'X;              /* SET RECOVERY CODE , NO PCB    */ 00687000
         L     @11,RCAPTR(,PVTPTR)                                 0277 00688000
         MVI   RCARCRD(@11),X'07'                                  0277 00689000
*       GENERATE SETS(R1);                                         0278 00690000
      ABEND X'C0D',,,SYSTEM        RECORD THE ERROR                     00691000
*       PFTIRRG='0'B;               /* PREVENT A RECLAIM ATTEMPT     */ 00692000
         NI    PFTIRRG(PFTEPTR),B'11101111'                        0279 00693000
*       VCBRSN=0;                   /* CLEAR THE REAL STORAGE NUMBER */ 00694000
         SLR   @11,@11                                             0280 00695000
         STH   @11,VCBRSN(,VCBPTR)                                 0280 00696000
*       GOTO REGASIGN;              /* PCB DEFINED, NOT FOUND        */ 00697000
         B     REGASIGN                                            0281 00698000
*       RFY                                                        0282 00699000
*        (PCBPTR) UNRSTD;                                          0282 00700000
*     END;                                                         0283 00701000
*                                                                  0284 00702000
*   /*****************************************************************/ 00703000
*   /*                                                               */ 00704000
*   /* ELSE - IF NO PAGEOUT IS IN PROGRESS, THE PFTE IS UPDATED WITH */ 00705000
*   /* THE ASID AND VBN. THE VIO FRAME FLAG IS RESET. IF THE PFTE IS */ 00706000
*   /* ON THE AVAILABLE FRAME QUEUE IT IS MOVED TO THE APPROPRIATE   */ 00707000
*   /* I/O QUEUE AND THE PAGE TABLE ENTRY VALIDATED.                 */ 00708000
*   /*                                                               */ 00709000
*   /*****************************************************************/ 00710000
*                                                                  0284 00711000
*   ELSE                            /* NO PCB EXISTS FOR THIS FRAME  */ 00712000
*     DO;                                                          0284 00713000
@RF00257 DS    0H                                                  0285 00714000
*       IF PFTQNDX=PFTNQN THEN      /* IF PFTE IS UNQUEUED, NO     0285 00715000
*                                      RECLAIM                       */ 00716000
         CLI   PFTQNDX(PFTEPTR),X'FF'                              0285 00717000
         BNE   @RF00285                                            0285 00718000
*         DO;                       /* PREVENT RECLAIM               */ 00719000
*           PFTIRRG='0'B;           /* TURN OFF IRREGULAR FLAG       */ 00720000
         NI    PFTIRRG(PFTEPTR),B'11101111'                        0287 00721000
*           VCBRSN=0;               /* ZERO RSN IN VCB               */ 00722000
         SLR   @11,@11                                             0288 00723000
         STH   @11,VCBRSN(,VCBPTR)                                 0288 00724000
*           GOTO REGASIGN;          /* GO TO NORMAL ASSIGN PROCESSING*/ 00725000
         B     REGASIGN                                            0289 00726000
*         END;                                                     0290 00727000
*       ELSE                                                       0291 00728000
*         ;                         /* CONTINUE RECLAIM PROCESSING   */ 00729000
@RF00285 DS    0H                                                  0292 00730000
*       PFTASID=ASCBASID;           /* SET CURRENT ASID IN PFTE      */ 00731000
         L     @11,ASCBPTR                                         0292 00732000
         LH    @10,ASCBASID(,@11)                                  0292 00733000
         N     @10,@CF02294                                        0292 00734000
         STH   @10,PFTASID(,PFTEPTR)                               0292 00735000
*       PFTVBN=CURVSNR;             /* SET VBN OF PAGE IN PFTE       */ 00736000
         STH   CURVSNR,PFTVBN(,PFTEPTR)                            0293 00737000
*       PFTUIC=0;                   /* RESET THE UNREFERANCED      0294 00738000
*                                      INTERVAL COUNT                */ 00739000
         MVI   PFTUIC(PFTEPTR),X'00'                               0294 00740000
*       PFTIRRG='0'B;               /* RESET THE VIO FRAME FLAG      */ 00741000
*                                                                  0295 00742000
         NI    PFTIRRG(PFTEPTR),B'11101111'                        0295 00743000
*       /*************************************************************/ 00744000
*       /*                                                           */ 00745000
*       /* CHECK FOR PFTE BEING ON AVAILABLE FRAME QUEUE             */ 00746000
*       /*                                                           */ 00747000
*       /*************************************************************/ 00748000
*                                                                  0296 00749000
*       IF PFTQNDX=PFTAFQN THEN     /* IF THE PFTE IS ON AVAILABLE 0296 00750000
*                                      FRAME QUE                     */ 00751000
         CLI   PFTQNDX(PFTEPTR),X'00'                              0296 00752000
         BNE   @RF00296                                            0296 00753000
*         DO;                                                      0297 00754000
*           QINDEX=PFTLFQN;         /* SEND PFTE TO LOCAL FRAME QUE  */ 00755000
         MVI   QINDEX(PVTPTR),X'80'                                0298 00756000
*           RFY                                                    0299 00757000
*            (R0,                                                  0299 00758000
*             R1) RSTD;                                            0299 00759000
*           QRBN=VCBRSN;            /* STORE THE RSN                 */ 00760000
         LH    @10,VCBRSN(,VCBPTR)                                 0300 00761000
         N     @10,@CF02294                                        0300 00762000
         STH   @10,QRBN(,PVTPTR)                                   0300 00763000
*           QHEADFLG=0;             /* SEND PFTE TO REAR OF QUEUE    */ 00764000
         MVI   QHEADFLG(PVTPTR),X'00'                              0301 00765000
