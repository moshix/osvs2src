         TITLE 'IEAVAR00 - RCT INITIALIZATION/TERMINATION              *00001000
                        '                                               00002000
IEAVAR00 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'IEAVAR00  74.306'                                 0001 00009000
IEAVAER0 DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00219-IEAVAER0)                                   00013000
         ENTRY IEAVAER0                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  BALR  @06,0                                               0001 00016000
@PSTART  DS    0H                                                  0001 00017000
         USING @PSTART,@06                                         0001 00018000
         AH    @15,4(,@15)                                         0001 00019000
         BR    @15                                                 0001 00020000
@EP00001 DS    0H                                                  0002 00021000
RBPRFXLN EQU 64                                                         00022000
RBPRFLNA EQU 32                                                         00023000
PRBLEN   EQU 136                                                        00024000
SIRBLEN  EQU 200                                                        00025000
TIRBLEN  EQU 136                                                        00026000
IRBLEN   EQU 128                                                        00027000
SVRBLEN  EQU 200                                                        00028000
*                                      UNTIL THE RETURN ADDRESS IS 0088 00029000
*                                      SAVED                         */ 00030000
*BEGIN:                             /* LABEL USED TO IDENTIFY      0089 00031000
*                                      BEGINNING OF EXECUTABLE CODE  */ 00032000
*/********************************************************************/ 00033000
*/*                                                                  */ 00034000
*/*      TOP SEGMENT                                                 */ 00035000
*/*                          - ESTABLISH RECOVERY ENVIRONMENT        */ 00036000
*/*                          - INVOKE INITIALIZATION SUBROUTINE TO   */ 00037000
*/*                            PERFORM REMAINDER OF INITIALIZATION   */ 00038000
*/*                          - INVOKE RCT COMMON PROCESSING          */ 00039000
*/*                          - INVOKE TERMINATION SUBROUTINE         */ 00040000
*/*                          - CANCEL RECOVERY ENVIRONMENT           */ 00041000
*/*                          - RETURN VIA RETURN ADDRESS SUPPLIED    */ 00042000
*/*                                                                  */ 00043000
*/*                  RETURN CODES ARE NOT PASSED                     */ 00044000
*/*                                                                  */ 00045000
*/********************************************************************/ 00046000
*/*                       AFTER INITIALIZING THE NEW BASE VALUES,  0089 00047000
*                         RESPECIFY THE BASE FOR THE ASCB AND THE RCTD  00048000
*                         TO SAVE CODE GENERATED BY THE PL/S COMPILER*/ 00049000
*                                                                  0089 00050000
*   ASCBREG7=PSAAOLD;                                              0089 00051000
BEGIN    L     ASCBREG7,PSAAOLD                                    0089 00052000
*   RESPECIFY                                                      0090 00053000
*     ASCB BASED(ASCBREG7);                                        0090 00054000
*   RCTDR13=ASXBRCTD;                                              0091 00055000
         L     @01,ASXBPTR(,ASCBREG7)                              0091 00056000
         L     RCTDR13,ASXBRCTD(,@01)                              0091 00057000
*   RESPECIFY                                                      0092 00058000
*     RCTD BASED(RCTDR13);                                         0092 00059000
*                                                                  0092 00060000
*   /*****************************************************************/ 00061000
*   /*                                                               */ 00062000
*   /* SAVE RETURN ADDRESS AND ISSUE ESTAE MACRO                     */ 00063000
*   /*                                                               */ 00064000
*   /*****************************************************************/ 00065000
*                                                                  0093 00066000
*   RCTDRET@=RETPTR14;              /* SAVE RETURN ADDRESS           */ 00067000
         ST    RETPTR14,RCTDRET@(,RCTDR13)                         0093 00068000
*   RESPECIFY                                                      0094 00069000
*     RETPTR14 UNRSTD;              /* RELEASE REGISTER - CONTENTS 0094 00070000
*                                      HAVE BEEN SAVED               */ 00071000
*   RCTDCLRL=''B;                   /* ZERO AREA USED TO CONTAIN LIST   00072000
*                                      FORMS OF MACROS               */ 00073000
         XC    RCTDCLRL(60,RCTDR13),RCTDCLRL(RCTDR13)              0095 00074000
*   RCTDLMAC(1:ESTAEL)=ESTAEL1(1:ESTAEL);/* INITIALIZE THAT AREA TO     00075000
*                                      CONTAIN THE LIST FORM OF THE     00076000
*                                      ESTAE MACRO (ENTIRE AREA NOT     00077000
*                                      NEEDED)                       */ 00078000
         LH    @12,ESTAEL                                          0096 00079000
         BCTR  @12,0                                               0096 00080000
         EX    @12,@SM02886                                        0096 00081000
*   RESPECIFY                                                      0097 00082000
*    (ERXTPTR3,                                                    0097 00083000
*     PARMREG1) RSTD;               /* RESTRICT REGISTERS NOT      0097 00084000
*                                      PREVIOUSLY RESTRICTED AND USED   00085000
*                                      AS INPUT REGISTERS            */ 00086000
*   ERXTPTR3=ERXTADDR;              /* PUT ADDRESS OF RECOVERY EXIT     00087000
*                                      IN A REGISTER                 */ 00088000
         L     ERXTPTR3,ERXTADDR                                   0098 00089000
*   PARMREG1=ADDR(RCTDLMAC);        /* INITIALIZE REGISTER 1 TO POINT   00090000
*                                      TO THE ESTAE PARAMETER LIST   */ 00091000
         LA    PARMREG1,RCTDLMAC(,RCTDR13)                         0099 00092000
*/*                                  THE ADDRESS OF THE RCTD IS    0100 00093000
*                                    SPECIFIED AS THE EXIT PARAMETER    00094000
*                                    LIST ADDRESS                    */ 00095000
*                                                                  0100 00096000
*   GENERATE CODE NODEFS REFS(RCTDLMAC) SETS(RCTDLMAC) NOFLOWS;    0100 00097000
         ESTAE (ERXTPTR3),PARAM=(RCTDR13),MF=(E,(1))                    00098000
*   RESPECIFY                                                      0101 00099000
*    (ERXTPTR3,                                                    0101 00100000
*     PARMREG1) UNRSTD;             /* RELEASE RESTRICTION ON ESTAE     00101000
*                                      INPUT REGISTERS               */ 00102000
*   RESPECIFY                                                      0102 00103000
*     RCREG15 RSTD;                 /* RESTRICT USE OF RETURN CODE 0102 00104000
*                                      REGISTER                      */ 00105000
*ESTAERC:                           /* THIS LABEL IS ONLY USED FOR 0103 00106000
*                                      TESTING PURPOSES              */ 00107000
*   IF RCREG15=0 THEN                                              0103 00108000
ESTAERC  LTR   RCREG15,RCREG15                                     0103 00109000
         BNZ   @RF00103                                            0103 00110000
*     DO;                           /* CHECK FOR SUCCESSFUL RET. CODE   00111000
*                                      FROM THE ESTAE MACRO AND WHEN    00112000
*                                      SUCCESSFUL PERFORM THE      0104 00113000
*                                      FOLLOWING                     */ 00114000
*       RFY                                                        0105 00115000
*         RCREG15 UNRSTD;           /* UNRESTRICT RETURN CODE      0105 00116000
*                                      REGISTER - CONTENTS HAVE BEEN    00117000
*                                      TESTED                        */ 00118000
*                                                                  0105 00119000
*       /*************************************************************/ 00120000
*       /*                                                           */ 00121000
*       /* INVOKE INITIALIZATION SUBROUTINE                          */ 00122000
*       /*                                                           */ 00123000
*       /*************************************************************/ 00124000
*                                                                  0106 00125000
*ESTAESUC:                          /* THIS LABEL IS ONLY USED FOR 0106 00126000
*                                      TESTING PURPOSES              */ 00127000
