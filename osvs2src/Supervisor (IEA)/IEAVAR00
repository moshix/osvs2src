*       RCTDSLST(1:SDUMPLEN)=SDUMPLST(1:SDUMPLEN);/* PLACE LIST FORM    01311000
*                                      OF SDUMP MACRO IN THAT AREA   */ 01312000
         LH    @12,SDUMPLEN                                        0408 01313000
         BCTR  @12,0                                               0408 01314000
         EX    @12,@SM02901                                        0408 01315000
*       SDUMPL@=ADDR(RCTDSLST);     /* POINT TO LIST FORM OF MACRO   */ 01316000
         LA    SDUMPL@,RCTDSLST(,RCTDPTR2)                         0409 01317000
*       GENERATE CODE REFS(DTITLE);                                0410 01318000
          SDUMP HDRAD=(TADDR8),MF=(E,(1))                               01319000
*       RFY                                                        0411 01320000
*        (TADDR8,                                                  0411 01321000
*         SDUMPL@) UNRSTD;          /* POINTER TO TITLE AND PARAMETER   01322000
*                                      LIST NO LONGER NEEDED         */ 01323000
*       IF SDWAVRAL-SDWAURAL>=LENGTH(RVRDUMP) THEN/*         @YM02727   01324000
*                                      WILL RETURN CODE      @YM02727   01325000
*                                      AND TITLE FIT INTO    @YM02727   01326000
*                                      RECORDING AREA?       @YM02727*/ 01327000
         LA    @12,20                                              0412 01328000
         SLR   @11,@11                                             0412 01329000
         IC    @11,SDWAURAL(,SDWAPTR)                              0412 01330000
         LH    @10,SDWAVRAL(,SDWAPTR)                              0412 01331000
         N     @10,@CF02903                                        0412 01332000
