*         ECB@,                     /* RESTRICT ATACHEE ECB ADDR REG */ 00356000
*         PRMREG15) RSTD;           /* RESTRICT CONTROL PROGRAM REG  */ 00357000
*       RCTDCLRL=''B;               /* ZERO AREA FOR LIST FORM OF  0154 00358000
*                                      ATTACH MACRO                  */ 00359000
         XC    RCTDCLRL(60,RCTDR13),RCTDCLRL(RCTDR13)              0154 00360000
*       PRMREG15=ADDR(RCTDLMAC);    /* INIT REG TO POINT TO LIST FORM   00361000
*                                      OF ATTACH MACRO               */ 00362000
         LA    PRMREG15,RCTDLMAC(,RCTDR13)                         0155 00363000
*       PARMREG1=ASCBREG7;          /* INIT PARM REG TO ASCB ADDR    */ 00364000
