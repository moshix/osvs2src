               KEY=ZERO,SM=SUPV,SVAREA=NO,SZERO=NO,SF=(E,(15))          00326000
*   RESPECIFY                                                      0147 00327000
*     ECB@ UNRSTD;                  /* RESET RESTRICTION ON ECB REG  */ 00328000
*ATCHDRC:                           /* LABEL IS USED FOR TESTING ONLY*/ 00329000
*   IF RCREG15=0 THEN               /* DETERMINE WHETHER THE MACRO 0148 00330000
*                                      WAS SUCCESSFUL                */ 00331000
ATCHDRC  LTR   RCREG15,RCREG15                                     0148 00332000
         BNZ   @RF00148                                            0148 00333000
*     DO;                           /* SUCCESSFUL - DO THE FOLLOWING */ 00334000
*ATCHDSUC:                          /* LABEL USED FOR TESTING ONLY   */ 00335000
*       RCTDTCBD=PARMREG1;          /* SAVE ADDR OF DUMP TCB         */ 00336000
ATCHDSUC ST    PARMREG1,RCTDTCBD(,RCTDR13)                         0150 00337000
*       RFY                                                        0151 00338000
*         PRMREG15 UNRSTD;          /* RELEASE RESTRICTION ON RETURN    00339000
*                                      CODE REGISTER                 */ 00340000
*                                                                  0151 00341000
*       /*************************************************************/ 00342000
*       /*                                                           */ 00343000
*       /* ATTACH STC TASK - REGISTER 1 CONTAINS THE ASCB ADDRESS AND*/ 00344000
*       /* THE ECB ADDRESS SPECIFIED IS THE RCT TERMINATION ECB      */ 00345000
*       /* ADDRESS (RCTDTECB ADDRESS)                                */ 00346000
*       /*                                                           */ 00347000
*       /*************************************************************/ 00348000
*                                                                  0152 00349000
*       RCTDATTS='1'B;              /* SET FUNCTION FOOTPRINT      0152 00350000
*                                      INDICATING STC IS BEING     0152 00351000
*                                      ATTACHED                      */ 00352000
         OI    RCTDATTS(RCTDR13),B'00100000'                       0152 00353000
*       RFY                                                        0153 00354000
