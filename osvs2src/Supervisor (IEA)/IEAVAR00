         L     RBREG5,TCBRBP(,@12)                                 0115 00173000
*R01CALL:                           /* LABEL USED FOR TESTING ONLY   */ 00174000
*           CALL IEAVAR01;          /* INVOKE COMMON PROCESSING      */ 00175000
R01CALL  L     @15,@CV02670                                        0116 00176000
         BALR  @14,@15                                             0116 00177000
*           RFY                                                    0117 00178000
*            (ASXBREG2,                                            0117 00179000
*             CVTREG3,                                             0117 00180000
*             TCBREG4,                                             0117 00181000
*             RBREG5) UNRSTD;       /* RELEASE INTERFACE REGISTERS   */ 00182000
*                                                                  0117 00183000
*           /*********************************************************/ 00184000
*           /*                                                       */ 00185000
*           /* INVOKE TERMINATION SUBROUTINE                         */ 00186000
*           /*                                                       */ 00187000
*           /*********************************************************/ 00188000
*                                                                  0118 00189000
*IEAVAR0A:                          /* RECOVERY RETRY LABEL          */ 00190000
*           CALL TERMCODE;          /* INVOKE TERMINATION SUBROUTINE */ 00191000
IEAVAR0A LA    @15,TERMCODE                                        0118 00192000
         BALR  @14,@15                                             0118 00193000
*TERMRET@:                          /* LABEL USED BY RECOVERY TO   0119 00194000
*                                      ESTABLISH RETURN ADDRESS FOR     00195000
*                                      RETRYS IN TERMCODE SUBROUTINE */ 00196000
*           RCTDCLRL=''B;           /* ZERO AREA USED FOR LIST FORMS    00197000
*                                      OF MACROS                     */ 00198000
TERMRET@ XC    RCTDCLRL(60,RCTDR13),RCTDCLRL(RCTDR13)              0119 00199000
*           RCTDLMAC(1:ESTAEL)=ESTAEL2(1:ESTAEL);/* INITIALIZE LIST     00200000
*                                      FORM AREA TO REFLECT ESTAE  0120 00201000
*                                      CANCEL MACRO - ENTIRE AREA IS    00202000
*                                      NOT USED                      */ 00203000
         LH    @12,ESTAEL                                          0120 00204000
         BCTR  @12,0                                               0120 00205000
         EX    @12,@SM02888                                        0120 00206000
*           RFY                                                    0121 00207000
*             PARMREG1 RSTD;        /* RESTRICT PARAMETER REGS       */ 00208000
*           PARMREG1=ADDR(RCTDLMAC);/* INITIALIZE REG 1 TO POINT TO     00209000
*                                      LIST FORM OF ESTAE MACRO      */ 00210000
         LA    PARMREG1,RCTDLMAC(,RCTDR13)                         0122 00211000
*           GENERATE CODE(ESTAE 0,MF=(E,(1)) )                     0123 00212000
*           NODEFS REFS(RCTDLMAC) SETS(RCTDLMAC) NOFLOWS;/* CANCEL 0123 00213000
*                                      RECOVERY ENVIRONMENT          */ 00214000
         ESTAE 0,MF=(E,(1))                                             00215000
*           RFY                                                    0124 00216000
*             PARMREG1 UNRSTD;      /* RELEASE PARAMETER REGISTER    */ 00217000
*           GOTO ENDR00;            /* BRANCH TO MODULE EXIT CODE    */ 00218000
         B     ENDR00                                              0125 00219000
*         END;                                                     0126 00220000
*                                                                  0126 00221000
*       /*************************************************************/ 00222000
*       /*                                                           */ 00223000
*       /* PROCESS ERROR INDICATED BY INITIALIZATION SUBROUTINE      */ 00224000
