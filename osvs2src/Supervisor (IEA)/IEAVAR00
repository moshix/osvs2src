*         DO;                       /* UNSUCCESSFUL INITIALIZATION,     00229000
*                                      PERFORM THE FOLLOWING         */ 00230000
@RF00108 DS    0H                                                  0128 00231000
*INITUN:                            /* LABEL USED FOR TESTING ONLY   */ 00232000
*           CALL PUTMSG;            /* INVOKE SUBROUTINE TO PRINT  0128 00233000
*                                      ERROR MESSAGE                 */ 00234000
INITUN   LA    @15,PUTMSG                                          0128 00235000
         BALR  @14,@15                                             0128 00236000
*/*                                ABEND -- RECOVERY ROUTINE WILL  0129 00237000
*                                  RECEIVE CONTROL AND RECORD ERROR  */ 00238000
*                                                                  0129 00239000
*           GENERATE CODE(ABEND X'078',,,SYSTEM )                  0129 00240000
*           NODEFS NOREFS NOSETS NOFLOWS;                          0129 00241000
         ABEND X'078',,,SYSTEM                                          00242000
*         END;                                                     0130 00243000
*     END;                                                         0131 00244000
*   ELSE                                                           0132 00245000
*     ;                             /* THIS ELSE CORRESPONDS TO THE     00246000
*                                      CHECKING OF THE ESTAE RETURN     00247000
*                                      CODE AND IT BEING UNSUCCESSFUL*/ 00248000
*                                                                  0132 00249000
@RF00103 DS    0H                                                  0133 00250000
*   /*****************************************************************/ 00251000
*   /*                                                               */ 00252000
*   /* PROCESS ERROR INDICATED BY ESTAE RETURN CODE                  */ 00253000
*   /*                                                               */ 00254000
*   /*****************************************************************/ 00255000
*                                                                  0133 00256000
*ESTAEUN:                           /* THIS LABEL IS ONLY USED FOR 0133 00257000
*                                      TESTING PURPOSES              */ 00258000
*   CALL PUTMSG;                    /* INVOKE SUBROUTINE TO PRINT  0133 00259000
*                                      ERROR MESSAGE                 */ 00260000
ESTAEUN  LA    @15,PUTMSG                                          0133 00261000
         BALR  @14,@15                                             0133 00262000
*   GOTO ENDR00;                    /* BRANCH MODULE TO EXIT CODE    */ 00263000
         B     ENDR00                                              0134 00264000
*                                                                  0135 00265000
*/********************************************************************/ 00266000
*/*                                                                  */ 00267000
*/*      INITIALIZATION SUBROUTINE       - ATTACHES DUMP TASK        */ 00268000
*/*                                      - ATTACHES STC              */ 00269000
*/*                                                                  */ 00270000
*/*      CODE RETURNED TO CALLER = 0 - ALL FUNCTIONS WERE SUCCESSFUL */ 00271000
*/*                              = 4 - ERROR ENCOUNTERED,            */ 00272000
*/*                                    INITIALIZATION ABORTED        */ 00273000
*/*                                                                  */ 00274000
*/********************************************************************/ 00275000
*                                                                  0135 00276000
*INITCODE:                                                         0135 00277000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,ENTREG,RETREG,NOPARMREG);       0135 00278000
INITCODE DS    0H                                                  0136 00279000
*   RESPECIFY                                                      0136 00280000
*    (RETPTR5,                      /* REGISTER 5 WILL BE RESTRICTED    00281000
*                                      FOR THIS ENTIRE SUBROUTINE -     00282000
*                                      USED TO SAVE RETURN ADDRESS   */ 00283000
*     RETPTR14) RSTD;               /* RESTRICT UNTIL RETURN ADDRESS    00284000
*                                      CAN BE SAVED                  */ 00285000
*   RCTDINIT='1'B;                  /* SET MODULE FOOTPRINT        0137 00286000
*                                      INDICATING INITIALIZATION   0137 00287000
*                                      SUBROUTINE IS PROCESSING      */ 00288000
         OI    RCTDINIT(RCTDR13),B'10000000'                       0137 00289000
*   RETPTR5=RETPTR14;               /* SAVE RETURN ADDRESS           */ 00290000
         LR    RETPTR5,RETPTR14                                    0138 00291000
*   RFY                                                            0139 00292000
*     RETPTR14 UNRSTD;              /* CONTENTS SAVED - UNRESTRICT 0139 00293000
*                                      REG                           */ 00294000
*                                                                  0139 00295000
*   /*****************************************************************/ 00296000
*   /*                                                               */ 00297000
*   /* ATTACH DUMP TASK - REGISTER 1 MUST CONTAIN 0 AND ECB EQUALS   */ 00298000
*   /* WORK ECB ADDRESS (ASCBECB ADDRESS)                            */ 00299000
*   /*                                                               */ 00300000
*   /*****************************************************************/ 00301000
*                                                                  0140 00302000
*   RCTDATTD='1'B;                  /* SET FUNCTION FOOTPRINT      0140 00303000
*                                      INDICATING ATTACHING DUMP     */ 00304000
         OI    RCTDATTD(RCTDR13),B'10000000'                       0140 00305000
*   RESPECIFY                                                      0141 00306000
*    (PARMREG1,                     /* RESTRICT ATACHEE PARAMETER REG*/ 00307000
*     ECB@,                         /* RESTRICT ATACHEE ECB REGISTER */ 00308000
*     PRMREG15) RSTD;               /* RESTRICT CONTROL PROGRAM    0141 00309000
*                                      PARAMETER REGISTER            */ 00310000
*   PARMREG1=0;                     /* ZERO ATACHEE PARAMETER REG    */ 00311000
         SLR   PARMREG1,PARMREG1                                   0142 00312000
*   ECB@=ADDR(ASCBECB);             /* INITIALIZE REGISTER WITH    0143 00313000
*                                      ADDRESS OF ECB TO BE POSTED 0143 00314000
*                                      WHEN TASK TERMINATES          */ 00315000
         LA    ECB@,ASCBECB(,ASCBREG7)                             0143 00316000
*   RCTDCLRL=''B;                   /* ZERO AREA FOR LIST FORM OF  0144 00317000
*                                      ATTACH MACRO                  */ 00318000
         XC    RCTDCLRL(60,RCTDR13),RCTDCLRL(RCTDR13)              0144 00319000
*   PRMREG15=ADDR(RCTDLMAC);        /* INITIALIZE REGISTER TO POINT     00320000
*                                      TO LIST FORM AREA USED AS   0145 00321000
*                                      CONTROL PROGRAM PARAMETER LIST*/ 00322000
         LA    PRMREG15,RCTDLMAC(,RCTDR13)                         0145 00323000
*   GENERATE CODE NODEFS REFS(DUMPNAME) SETS(ASCBECB,RCTDLMAC) NOFLOWS; 00324000
