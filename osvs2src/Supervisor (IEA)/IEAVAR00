               SF=(E,(15))                                              00372000
*       RFY                                                        0159 00373000
*         ECB@ UNRSTD;              /* ECB REGISTERS NO LONGER NEEDED*/ 00374000
*ATCHSRC:                           /* LABEL USED FOR TESTING ONLY   */ 00375000
*       IF RCREG15=0 THEN           /* DETERMINE - ATTACH SUCCESSFUL */ 00376000
ATCHSRC  LTR   RCREG15,RCREG15                                     0160 00377000
         BNZ   @RF00160                                            0160 00378000
*         DO;                       /* SUCCESSFUL - PERFORM FOLLOWING*/ 00379000
*ATCHSSUC:                          /* LABEL USED FOR TESTING ONLY   */ 00380000
*           RCTDTCBS=PARMREG1;      /* SAVE ADDRESS OF THE STC TCB   */ 00381000
ATCHSSUC ST    PARMREG1,RCTDTCBS(,RCTDR13)                         0162 00382000
*           RFY                                                    0163 00383000
*             PARMREG1 UNRSTD;      /* CONTENTS SAVED - RELEASE REG. */ 00384000
*           RCTDBRCP='1'B;          /* SET MODULE FOOTPRINT        0164 00385000
*                                      INDICATING BRANCHING TO COMMON   00386000
*                                      PROCESSING                    */ 00387000
*                                                                  0164 00388000
         OI    RCTDBRCP(RCTDR13),B'00010000'                       0164 00389000
*           /*********************************************************/ 00390000
*           /*                                                       */ 00391000
*           /* INITIALIZATION WAS SUCCESSFUL - RETURN CODE = 0       */ 00392000
*           /*                                                       */ 00393000
*           /*********************************************************/ 00394000
*                                                                  0165 00395000
*           RCREG15=0;              /* SET RETURN CODE FOR CALLER    */ 00396000
         SLR   RCREG15,RCREG15                                     0165 00397000
*           GOTO INITEND;           /* BRANCH TO SUBROUTINE EXIT CODE*/ 00398000
         B     INITEND                                             0166 00399000
*         END;                                                     0167 00400000
*       ELSE                                                       0168 00401000
*         ;                         /* THIS ELSE CORRESPONDS TO AN 0168 00402000
*                                      UNSUCCESSFUL ATTACH OF STC    */ 00403000
@RF00160 DS    0H                                                  0169 00404000
*     END;                                                         0169 00405000
*   ELSE                                                           0170 00406000
*     ;                             /* THIS ELSE CORRESPONDS TO AN 0170 00407000
*                                      UNSUCCESSFUL ATTACH OF DUMP   */ 00408000
*                                                                  0170 00409000
@RF00148 DS    0H                                                  0171 00410000
*   /*****************************************************************/ 00411000
*   /*                                                               */ 00412000
*   /* INITIALIZATION WAS UNSUCCESSFUL - RETURN CODE = 4             */ 00413000
*   /*                                                               */ 00414000
*   /*****************************************************************/ 00415000
*                                                                  0171 00416000
*ATCHUN:                            /* LABEL USED FOR TESTING ONLY   */ 00417000
