         TITLE 'IEAVAR02, REGION CONTROL TASK QUIESCE ROUTINE          *00001000
                        '                                               00002000
IEAVAR02 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVAR02  77.159'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @10,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@10                                         0001 00013000
RBPRFXLN EQU 64                                                         00014000
RBPRFLNA EQU 32                                                         00015000
PRBLEN   EQU 136                                                        00016000
SIRBLEN  EQU 200                                                        00017000
TIRBLEN  EQU 136                                                        00018000
IRBLEN   EQU 128                                                        00019000
SVRBLEN  EQU 224                                                        00020000
*                                                                  0116 00021000
*/********************************************************************/ 00022000
*/*                                                                  */ 00023000
*/*    EXECUTABLE CODE BEGINS HERE.                                  */ 00024000
*/*                                                                  */ 00025000
*/*    THIS SECTION OF CODE WILL DO THE FOLLOWING PROCESSING         */ 00026000
*/*    TO SET UP AND WAIT FOR A WORK OR TERMINATION REQUEST:         */ 00027000
*/*      - ESTABLISH ADDRESSABILITY TO THE RCTD FOR                  */ 00028000
*/*        BETTER OPTIMIZATION.                                      */ 00029000
*/*      - SET THE RECOVERY FOOTPRINT TO INDICATE WAIT               */ 00030000
*/*        STATE IS ABOUT TO BE ENTERED.                             */ 00031000
*/*      - INITIALIZE THE POINTER TO THE ECB LIST.                   */ 00032000
*/*      - ISSUE THE WAIT MACRO TO WAIT FOR A FUNCTIONAL             */ 00033000
*/*        REQUEST (ECB TO BE POSTED).                               */ 00034000
*/*                                                                  */ 00035000
*/********************************************************************/ 00036000
*                                                                  0116 00037000
*   R2=R13;                                                        0116 00038000
         LR    R2,R13                                              0116 00039000
*   RFY                                                            0117 00040000
*     RCTD BASED(R2);                                              0117 00041000
*   RCTDWAIT='1'B;                  /*                       @Z30JQVB*/ 00042000
         OI    RCTDWAIT(R2),B'10000000'                            0118 00043000
*   RFY                                                            0119 00044000
*     R11 RSTD;                     /*                       @Z30JQVB*/ 00045000
*   R11=ADDR(RCTDECBS);             /*                       @Z30JQVB*/ 00046000
         LA    R11,RCTDECBS(,R2)                                   0120 00047000
*   GENERATE CODE(WAIT 1,ECBLIST=(R11))                            0121 00048000
*   NODEFS NOFLOWS SETS(RCTDTECB,ASCBECB) REFS(RCTDWPTR,RCTDTPTR);/*    00049000
*                                                            @Z30JQVB*/ 00050000
         WAIT 1,ECBLIST=(R11)                                           00051000
*   RFY                                                            0122 00052000
*     R11 UNRSTD;                   /*                       @Z30JQVB*/ 00053000
*                                                                  0123 00054000
*/********************************************************************/ 00055000
*/*                                                                  */ 00056000
*/*    THIS SECTION OF CODE WILL DO THE FOLLOWING PROCESSING         */ 00057000
*/*    TO HANDLE THE FUNCTION REQUESTED AFTER THE ECB GETS           */ 00058000
*/*    POSTED:                                                       */ 00059000
*/*      - SET THE RECOVERY FOOTPRINT TO INDICATE LOOKING            */ 00060000
*/*        FOR FUNCTION REQUESTS.                                    */ 00061000
*/*      - CLEAR WORK ECB (NO MP PROBLEMS HERE BECAUSE               */ 00062000
*/*        BOTH FLAGS AND ECB ARE USED).                             */ 00063000
*/*      - DETERMINE WHAT FUNCTION WAS REQUESTED.                    */ 00064000
*/*          - IF THE TERMINATION ECB HAS NOT BEEN POSTED            */ 00065000
*/*            AND THE REQUEST IS FOR ADDRESS SPACE QUIESCING,       */ 00066000
*/*            THEN CONTINUE PROCESSING IN THE QUIESCE               */ 00067000
*/*            MODULE.                                               */ 00068000
*/*          - FOR A TERMINATION REQUEST OR ANY OTHER                */ 00069000
*/*            WORK REQUEST, RETURN TO COMMON PROCESSING.            */ 00070000
*/*                                                                  */ 00071000
*/********************************************************************/ 00072000
*                                                                  0123 00073000
*   RCTDLOOP='1'B;                  /*                       @Z30JQVB*/ 00074000
         OI    RCTDLOOP(R2),B'01000000'                            0123 00075000
*   ASCBECB=0;                      /*                       @Z30JQVB*/ 00076000
         SLR   @15,@15                                             0124 00077000
         ST    @15,ASCBECB(,R7)                                    0124 00078000
*   IF RCTDPOST='0'B&ASCBFQU='1'B THEN/*                     @Z30JQVB*/ 00079000
         TM    RCTDPOST(R2),B'01000000'                            0125 00080000
         BNZ   @RF00125                                            0125 00081000
         TM    ASCBFQU(R7),B'00100000'                             0125 00082000
         BNO   @RF00125                                            0125 00083000
*     DO;                           /*                       @Z30JQVB*/ 00084000
*                                                                  0127 00085000
*/********************************************************************/ 00086000
*/*                                                                  */ 00087000
*/*    THIS SECTION OF CODE WILL DO THE FOLLOWING TO INITIATE        */ 00088000
*/*    THE PROCESSING WHEN THE WORK REQUEST WAS FOR QUIESCING        */ 00089000
*/*    THE ADDRESS SPACE:                                            */ 00090000
*/*      - SET THE QUIESCE PROCESSING RECOVERY                       */ 00091000
*/*        FOOTPRINT AND CLEAR OTHER MODULE/FUNCTION                 */ 00092000
*/*        FOOTPRINTS.                                               */ 00093000
*/*      - RESET THE QUIESCE FLAG TO INDICATE REQUEST HONORED.       */ 00094000
*/*        IT IS NECESSARY TO USE COMPARE AND SWAP TO INSURE         */ 00095000
*/*        SERIALIZATION OF THIS FIELD.                              */ 00096000
*/*      - INITIALIZE THE ANCHOR OF THE PURGED                       */ 00097000
*/*        I/O REQUESTS TO INDICATE NO I/O PURGED.                   */ 00098000
*/*                                                                  */ 00099000
*/********************************************************************/ 00100000
*                                                                  0127 00101000
*       RCTDRCTR=INITMASK;                                         0127 00102000
         MVC   RCTDRCTR(4,R2),@CB00038                             0127 00103000
*       RFY                                                        0128 00104000
*        (R6,                                                      0128 00105000
