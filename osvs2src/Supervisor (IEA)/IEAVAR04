*           DO;                     /* YES                           */ 00907000
*             PTAXETBS=TAXEPTR;     /* SAVE ACTIVE TAXE PTR          */ 00908000
         LR    PTAXETBS,TAXEPTR                                    0390 00909000
*           END;                                                   0391 00910000
*         TAXEPTR=TAXELNK;          /* UPDATE SEARCH ARGUMENT        */ 00911000
@RF00388 L     TAXEPTR,TAXELNK(,TAXEPTR)                           0392 00912000
*       END;                                                       0393 00913000
@DE00387 SLR   @12,@12                                             0393 00914000
         CR    TAXEPTR,@12                                         0393 00915000
         BNE   @DL00387                                            0393 00916000
*                                                                  0394 00917000
*/********************************************************************/ 00918000
*/*                                                                  */ 00919000
*/* THE TAXES FOLLOWING THE HIGHEST ACTIVE TAXE MUST BE MARKED       */ 00920000
*/* AVAILABLE FOR USE AND THEY MUST BE REFLECTED IN THE TSBSTAX      */ 00921000
*/* COUNT. THIS SEGMENT OF CODE WILL SEARCH THE ENTIRE TAXE QUEUE    */ 00922000
*/* STARTING WITH THE HIGHEST SCHEDULED TAXE AND PERFORM THE         */ 00923000
*/* OPERATION                                                        */ 00924000
*/*                                                                  */ 00925000
*/********************************************************************/ 00926000
*                                                                  0394 00927000
*       RESPECIFY                                                  0394 00928000
*         STAXCNT RESTRICTED;                                      0394 00929000
*       STAXCNT=STAXCNT-STAXCNT;    /* CLEAR NEW STAX COUNT          */ 00930000
