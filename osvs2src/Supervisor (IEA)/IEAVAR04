         TITLE 'IEAVAR04 - ATTENTION EXIT SCHEDULER ROUTINE            *00001000
                        '                                               00002000
IEAVAR04 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVAR04  76.030'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @08,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@08                                         0001 00013000
RBPRFXLN EQU 64                                                         00014000
RBPRFLNA EQU 32                                                         00015000
PRBLEN   EQU 136                                                        00016000
SIRBLEN  EQU 200                                                        00017000
TIRBLEN  EQU 136                                                        00018000
IRBLEN   EQU 128                                                        00019000
SVRBLEN  EQU 200                                                        00020000
DECREG   DS    0H                                                  0051 00021000
*                                                                  0097 00022000
*/********************************************************************/ 00023000
*/*                                                                  */ 00024000
*/*  START  OF EXECUTABLE CODE                                       */ 00025000
*/*                                                                  */ 00026000
*/********************************************************************/ 00027000
*                                                                  0097 00028000
*   RCTDRCTR='08000000'X;           /* SET MODULE FLAG(RCTDATTN)   0097 00029000
*                                      WHILE RESETTING ALL OTHER   0097 00030000
*                                      FLAGS IN THE RCTDRCTR         */ 00031000
         L     @15,ASXBRCTD(,ASXBPTR)                              0097 00032000
         MVC   RCTDRCTR(4,@15),@CB02427                            0097 00033000
*/********************************************************************/ 00034000
*/*                                                                  */ 00035000
*/*  THE FOLLOWING SEGMENT WILL OBTAIN THE LOCAL LOCK                */ 00036000
*/*  UNCONDITIONALLY. IF A NON-ZERO RETURN CODE IS RETURNED          */ 00037000
*/*  AN ABEND WILL BE ISSUED. THE ABEND WILL BE PROCESSED            */ 00038000
*/*  BY RCT'S ESTAE ROUTINE.                                         */ 00039000
*/*                                                                  */ 00040000
*/********************************************************************/ 00041000
*                                                                  0098 00042000
*   RESPECIFY                                                      0098 00043000
*     SETLK11 RESTRICTED;           /* REGISTERS 11,12,13            */ 00044000
*   RESPECIFY                                                      0099 00045000
*     SETLK12 RESTRICTED;           /* AND 14 ARE USED BY            */ 00046000
*   RESPECIFY                                                      0100 00047000
*     SETLK13 RESTRICTED;           /* THE SETLOCK MACRO.            */ 00048000
*   RESPECIFY                                                      0101 00049000
*     SETLK14 RESTRICTED;           /* SETLOCK DOES NOT SAVE THE   0101 00050000
*                                      REGISTERS                     */ 00051000
*SETLKO1:                                                          0102 00052000
*   GENERATE CODE NOSETS NOFLOWS NODEFS REFS(PSALITA);             0102 00053000
SETLKO1  DS    0H                                                  0102 00054000
        SETLOCK  OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        X00055000
               RELATED=(TAXE-TCB-QUEUE,IEAVAR04(SETLKR1))               00056000
*   IF SETLK13^=ZERO THEN           /* WAS SETLOCK SUCCESSFUL        */ 00057000
         LTR   SETLK13,SETLK13                                     0103 00058000
         BZ    @RF00103                                            0103 00059000
*     DO;                                                          0104 00060000
*R04ABEND:                                                         0105 00061000
*       RCTDRC=SETLK13;             /* SAVE THE RETURN CODE FROM   0105 00062000
*                                      SETLOCK FOR LOGREC            */ 00063000
R04ABEND L     @15,ASXBRCTD(,ASXBPTR)                              0105 00064000
         ST    SETLK13,RCTDRC(,@15)                                0105 00065000
*       GENERATE CODE NOFLOWS NOREFS NODEFS NOSETS;                0106 00066000
                 ABEND  X'079',,,SYSTEM                                 00067000
*     END;                                                         0107 00068000
*   RESPECIFY                                                      0108 00069000
*     SETLK11 UNRESTRICTED;                                        0108 00070000
@RF00103 DS    0H                                                  0109 00071000
*   RESPECIFY                                                      0109 00072000
*     SETLK12 UNRESTRICTED;                                        0109 00073000
*   RESPECIFY                                                      0110 00074000
*     SETLK13 UNRESTRICTED;                                        0110 00075000
*   RESPECIFY                                                      0111 00076000
*     SETLK14 UNRESTRICTED;                                        0111 00077000
*                                                                  0112 00078000
*/********************************************************************/ 00079000
*/*                                                                  */ 00080000
*/*  THE FOLLOWING SEGMENT WILL ESTABLISH A RECOVERY                 */ 00081000
*/*  ENVIRONMENT FOR THE LOCKED CODE OF THIS ROUTINE                 */ 00082000
*/*                                                                  */ 00083000
*/********************************************************************/ 00084000
*                                                                  0112 00085000
*   RESPECIFY                                                      0112 00086000
*     FRRSPTR RESTRICTED;                                          0112 00087000
*   RESPECIFY                                                      0113 00088000
*     FRREPTR RESTRICTED;                                          0113 00089000
*   FRRSPTR=ADDR(IEAVAFR4);         /* ENTRY POINT TO FRR            */ 00090000
         LA    FRRSPTR,IEAVAFR4                                    0114 00091000
*   GENERATE CODE NODEFS NOSETS NOFLOWS REFS(FRRWKAP,FRRSPTR,FRREPTR,   00092000
*       PSACSTK,FRRSCURR,FRRSLAST,FRRSELEN,FRRSFRRA,FRRSPARM,PSALSFCC); 00093000
        SETFRR  A,FRRAD=(FRRSPTR),PARMAD=(FRRWKAP),                    X00094000
               WRKREGS=(FRRWKAP,FRREPTR)                                00095000
*   RESPECIFY                                                      0116 00096000
*     FRRSPTR UNRESTRICTED;                                        0116 00097000
*   RESPECIFY                                                      0117 00098000
*     FRREPTR UNRESTRICTED;                                        0117 00099000
*   RCTDAFPE='1'B;                  /* SET FOOTPRINT INDICATING THAT    00100000
*                                      THE FRR HAS BEEN ESTABLISHED  */ 00101000
         L     @15,ASXBRCTD(,ASXBPTR)                              0118 00102000
         OI    RCTDAFPE(@15),B'10000000'                           0118 00103000
*                                                                  0119 00104000
*/********************************************************************/ 00105000
*/*                                                                  */ 00106000
*/* THE FOLLOWING SEGMENT WILL SEARCH FOR ANY PREVIOUSLY             */ 00107000
*/* SCHEDULED ATTENTION EXITS THAT HAVE NOT YET ENTERED THE          */ 00108000
*/* ATTENTION EXIT. IF ANY ARE FOUND THE ATTENTION EXIT              */ 00109000
*/* WILL BE CANCELLED.                                               */ 00110000
*/*                                                                  */ 00111000
*/********************************************************************/ 00112000
*                                                                  0119 00113000
*   RESPECIFY                                                      0119 00114000
*     TSBPTR RESTRICTED;                                           0119 00115000
*   RESPECIFY                                                      0120 00116000
*     PTAXETBS RESTRICTED;                                         0120 00117000
*   RESPECIFY                                                      0121 00118000
*     TAXEPTR RESTRICTED;                                          0121 00119000
*   RESPECIFY                                                      0122 00120000
*     SWAPVAL RESTRICTED;                                          0122 00121000
*   RESPECIFY                                                      0123 00122000
*     ATTNCNT RESTRICTED;                                          0123 00123000
*   RESPECIFY                                                      0124 00124000
*     STAXCNT RESTRICTED;                                          0124 00125000
*   TSBPTR=ASCBTSB;                 /* LOAD BASE REG OF TSB          */ 00126000
         L     TSBPTR,ASCBTSB(,ASCBPTR)                            0125 00127000
*   GENERATE CODE NOSETS NOFLOWS NODEFS REFS(TSBPTR);              0126 00128000
        USING  TSB,TSBPTR                                               00129000
*   PTAXETBS=PTAXETBS-PTAXETBS;     /* INITIALIZE PTR OF TAXE TO BE     00130000
*                                      SCHED                         */ 00131000
         SLR   PTAXETBS,PTAXETBS                                   0127 00132000
*   SWAPVAL=TSBATTNC;               /* VALUE IN TSB AT START OF    0128 00133000
*                                      COMPARE AND SWAP LOOP         */ 00134000
         L     SWAPVAL,TSBATTNC                                    0128 00135000
*   STAXCNT=SWAPVAL&'00FF0000'X;    /* AVAILABLE TAXE CNT            */ 00136000
         LR    STAXCNT,SWAPVAL                                     0129 00137000
         N     STAXCNT,@CF02435                                    0129 00138000
*   ATTNCNT=SWAPVAL&'FF000000'X;    /* REQUESTED ATTN LEVEL          */ 00139000
         LR    ATTNCNT,SWAPVAL                                     0130 00140000
         N     ATTNCNT,@CF02436                                    0130 00141000
*   IF RCTDCLST>OFF THEN                                           0131 00142000
         L     @15,ASXBRCTD(,ASXBPTR)                              0131 00143000
         CLC   RCTDCLST(2,@15),@CH00036                            0131 00144000
         BNH   @RF00131                                            0131 00145000
*     DO;                           /* IF > 0 THEN SET ATTNCNT=    0132 00146000
*                                      AVAILABLE TAXE COUNT FOR THE     00147000
*                                      TMP EXIT              @ZA08100*/ 00148000
*       ATTNCNT=STAXCNT;            /* WILL FORCE THE TMP ATTN EXIT     00149000
*                                                            @ZA08100*/ 00150000
         LR    ATTNCNT,STAXCNT                                     0133 00151000
*       SLL(ATTNCNT,8);             /* SHIFT COUNT TO CORRECT BYTE 0134 00152000
*                                                            @ZA08100*/ 00153000
         SLL   ATTNCNT,8                                           0134 00154000
*     END;                          /*                       @ZA08100*/ 00155000
*   TAXEPTR=RCTDTAXE;               /* INITIALIZE TAXE SEARCH      0136 00156000
*                                      ARGUMENT                      */ 00157000
@RF00131 L     @15,ASXBRCTD(,ASXBPTR)                              0136 00158000
         L     TAXEPTR,RCTDTAXE(,@15)                              0136 00159000
*LOOP:                                                             0137 00160000
*   DO WHILE TAXEPTR^=ZERO;         /* ANY MORE TAXES?               */ 00161000
LOOP     B     @DE00137                                            0137 00162000
@DL00137 DS    0H                                                  0138 00163000
*     IF ATTNCNT=ZERO THEN          /* HIGHER LEVEL REQ?             */ 00164000
         LTR   ATTNCNT,ATTNCNT                                     0138 00165000
         BNZ   @RF00138                                            0138 00166000
*       DO;                         /* NO                            */ 00167000
*         GO TO UPDCNT;             /* UPDATE THE COUNTS             */ 00168000
         B     UPDCNT                                              0140 00169000
*       END;                                                       0141 00170000
*     IF TAXEFREQ='0'B THEN         /* IS TAXE AVAILABLE             */ 00171000
@RF00138 TM    TAXEFREQ(TAXEPTR),B'00100000'                       0142 00172000
         BNZ   @RF00142                                            0142 00173000
*       DO;                                                        0143 00174000
*         TAXEFREQ='1'B;            /* MARK TAXE UNAVAIL             */ 00175000
         OI    TAXEFREQ(TAXEPTR),B'00100000'                       0144 00176000
*         ATTNCNT=ATTNCNT-'01000000'X;/* DECREMENT THE ATTENTION CNT */ 00177000
         SL    ATTNCNT,@CF02442                                    0145 00178000
*         STAXCNT=STAXCNT-'00010000'X;/* DECREMENT THE AVAIL TAXE CNT*/ 00179000
         SL    STAXCNT,@CF02443                                    0146 00180000
*         PTAXETBS=TAXEPTR;         /* SAVE TAXE ADDRESS             */ 00181000
         LR    PTAXETBS,TAXEPTR                                    0147 00182000
*       END;                                                       0148 00183000
*     ELSE                          /* NO                            */ 00184000
*       IF TAXELNK=ZERO THEN        /* LAST TAXE                     */ 00185000
         B     @RC00142                                            0149 00186000
@RF00142 L     @15,TAXELNK(,TAXEPTR)                               0149 00187000
         LTR   @15,@15                                             0149 00188000
         BNZ   @RF00149                                            0149 00189000
*         DO;                                                      0150 00190000
*           PTAXETBS=PTAXETBS-PTAXETBS;/* NO TAXE TO BE SCHED.       */ 00191000
         SLR   PTAXETBS,PTAXETBS                                   0151 00192000
*           ATTNCNT=ATTNCNT-ATTNCNT;/* CLEAR ATTN COUNT              */ 00193000
         SLR   ATTNCNT,ATTNCNT                                     0152 00194000
*           STAXCNT=STAXCNT-STAXCNT;/* CLEAR STAX COUNT              */ 00195000
         SLR   STAXCNT,STAXCNT                                     0153 00196000
*         END;                                                     0154 00197000
*       ELSE                        /* NO                            */ 00198000
*         IF TAXESCHD='1'B THEN     /* TAXE SCHEDULED                */ 00199000
         B     @RC00149                                            0155 00200000
@RF00149 TM    TAXESCHD(TAXEPTR),B'00001000'                       0155 00201000
         BNO   @RF00155                                            0155 00202000
*           DO;                                                    0156 00203000
*             IF TAXERESM='0'B THEN /* WAS TAXE CANCELLED            */ 00204000
         TM    TAXERESM(TAXEPTR),B'00010000'                       0157 00205000
         BNZ   @RF00157                                            0157 00206000
*               DO;                                                0158 00207000
*                 FRRSTRF='1'B;     /* SET CANCEL PRINT              */ 00208000
         OI    FRRSTRF(FRRWKAP),B'10000000'                        0159 00209000
*                 FRRTRESM=TAXEPTR; /* PTR TO CANCELLED TAXE         */ 00210000
         ST    TAXEPTR,FRRTRESM(,FRRWKAP)                          0160 00211000
*                 TAXERESM='1'B;    /* CANCEL THE TAXE               */ 00212000
         OI    TAXERESM(TAXEPTR),B'00010000'                       0161 00213000
*               END;                                               0162 00214000
*           END;                                                   0163 00215000
@RF00157 DS    0H                                                  0164 00216000
*     TAXEPTR=TAXELNK;              /* UPDATE SEARCH ARGU            */ 00217000
@RF00155 DS    0H                                                  0164 00218000
@RC00149 DS    0H                                                  0164 00219000
@RC00142 L     TAXEPTR,TAXELNK(,TAXEPTR)                           0164 00220000
*   END;                                                           0165 00221000
@DE00137 LTR   TAXEPTR,TAXEPTR                                     0165 00222000
         BNZ   @DL00137                                            0165 00223000
*   STAXCNT=STAXCNT-STAXCNT;        /* CLEAR THE STAX CNT            */ 00224000
         SLR   STAXCNT,STAXCNT                                     0166 00225000
*   ATTNCNT=ATTNCNT-ATTNCNT;        /* CLEAR THE ATTN CNT            */ 00226000
         SLR   ATTNCNT,ATTNCNT                                     0167 00227000
*/********************************************************************/ 00228000
*/*                                                                  */ 00229000
*/* SET UP THE COMPARE AND SWAP LOOP TO UPDATE THE TSB WITH          */ 00230000
*/* THE NEW VALUES FOR THE TSBSTAX AND THE TSBATTNC FIELDS           */ 00231000
*/*                                                                  */ 00232000
*/********************************************************************/ 00233000
*                                                                  0168 00234000
*   RESPECIFY                                                      0168 00235000
*     NSWAPVAL RESTRICTED;                                         0168 00236000
*UPDCNT:                                                           0169 00237000
*   NSWAPVAL=SWAPVAL&'0000FFFF'X;   /* CLEAR ORIGINAL COUNTS         */ 00238000
UPDCNT   LR    NSWAPVAL,SWAPVAL                                    0169 00239000
         N     NSWAPVAL,@CF02444                                   0169 00240000
*   NSWAPVAL=NSWAPVAL|STAXCNT;      /* INSERT NEW STAX COUNT         */ 00241000
         OR    NSWAPVAL,STAXCNT                                    0170 00242000
*   NSWAPVAL=NSWAPVAL|ATTNCNT;      /* INSERT NEW ATTN COUNT         */ 00243000
         OR    NSWAPVAL,ATTNCNT                                    0171 00244000
*   ATTNCNT=SWAPVAL;                /* SAVE ORIGINAL VALUE FROM THE     00245000
*                                      TSB                           */ 00246000
         LR    ATTNCNT,SWAPVAL                                     0172 00247000
*   CS(SWAPVAL,NSWAPVAL,TSBATTNC);                                 0173 00248000
*                                                                  0173 00249000
         CS    SWAPVAL,@09,TSBATTNC                                0173 00250000
*   /*****************************************************************/ 00251000
*   /*                                                               */ 00252000
*   /* PERFORM ACTUAL UPDATE OF THE VALUES                           */ 00253000
*   /*                                                               */ 00254000
*   /*****************************************************************/ 00255000
*                                                                  0174 00256000
*   BC(8,STSTOP);                   /* DID THE UPDATE WORK? IF IT  0174 00257000
*                                      DID,BRANCH                    */ 00258000
         BC    8,STSTOP                                            0174 00259000
*/* THE UPDATE WAS NOT SUCCESSFUL. THE COUNTS MUST BE SET            */ 00260000
*/* UP SO THAT THE LOOP CAN BE RE-ENTERED SO THAT THE MORE           */ 00261000
*/* CURRENT TAXE CAN BE SCHEDULED.                                   */ 00262000
*                                                                  0175 00263000
*   NSWAPVAL=ATTNCNT;               /* MOVE THE ORIGINAL TSB VALUE 0175 00264000
*                                      FOR FURTHER USE               */ 00265000
         LR    NSWAPVAL,ATTNCNT                                    0175 00266000
*   STAXCNT=SWAPVAL&'00FF0000'X;    /* NEW AVAILABLE TAXE CNT FROM 0176 00267000
*                                      THE TSB                       */ 00268000
         L     @15,@CF02435                                        0176 00269000
         LR    STAXCNT,SWAPVAL                                     0176 00270000
         NR    STAXCNT,@15                                         0176 00271000
*   STAXCNT=STAXCNT-(NSWAPVAL&'00FF0000'X);                        0177 00272000
*                                                                  0177 00273000
         NR    @15,NSWAPVAL                                        0177 00274000
         SLR   STAXCNT,@15                                         0177 00275000
*   /*****************************************************************/ 00276000
*   /*                                                               */ 00277000
*   /* DECREMENT THE NEW AVAIL TAXE COUNT BY THE VALUE FROM THE      */ 00278000
*   /* PREVIOUS TIME THROUGH THE LOOP                                */ 00279000
*   /*                                                               */ 00280000
*   /*****************************************************************/ 00281000
*                                                                  0178 00282000
*   ATTNCNT=SWAPVAL&'FF000000'X;    /* NEW ATTN COUNT FROM TSB       */ 00283000
         L     @15,@CF02436                                        0178 00284000
         LR    ATTNCNT,SWAPVAL                                     0178 00285000
         NR    ATTNCNT,@15                                         0178 00286000
*   ATTNCNT=ATTNCNT-(NSWAPVAL&'FF000000'X);                        0179 00287000
*                                                                  0179 00288000
         NR    @15,NSWAPVAL                                        0179 00289000
         SLR   ATTNCNT,@15                                         0179 00290000
*   /*****************************************************************/ 00291000
*   /*                                                               */ 00292000
*   /* DECREMENT NEW ATTN CNT BY THE NUMBER OF ATTNS FROM THE        */ 00293000
*   /* PREVIOUS TIME THROUGH THE LOOP                                */ 00294000
*   /*                                                               */ 00295000
*   /*****************************************************************/ 00296000
*                                                                  0180 00297000
*   GO TO LOOP;                     /* CONTINUE SEARCH               */ 00298000
         B     LOOP                                                0180 00299000
*STSTOP:                            /* COMPLETED                     */ 00300000
*   GENERATE CODE NOSETS NODEFS NOFLOWS REFS(TSBPTR);              0181 00301000
STSTOP   DS    0H                                                  0181 00302000
        DROP  TSBPTR                                                    00303000
*   RESPECIFY                                                      0182 00304000
*     TSBPTR UNRESTRICTED;                                         0182 00305000
*   RESPECIFY                                                      0183 00306000
*     ATTNCNT UNRESTRICTED;                                        0183 00307000
*   RESPECIFY                                                      0184 00308000
*     STAXCNT UNRESTRICTED;                                        0184 00309000
*   RESPECIFY                                                      0185 00310000
*     SWAPVAL UNRESTRICTED;                                        0185 00311000
*   RESPECIFY                                                      0186 00312000
*     NSWAPVAL UNRESTRICTED;                                       0186 00313000
*                                                                  0187 00314000
*/********************************************************************/ 00315000
*/*                                                                  */ 00316000
*/* THE FOLLOWING SEGMENT OF CODE WILL STATUS STOP THE SUB-          */ 00317000
*/* TASKS OF THE TCB THAT IS HAVING THE ATTENTION EXIT SCHD.         */ 00318000
*/* IF THERE IS NO TAXE TO BE SCHEDULED A BRANCH AROUND THIS         */ 00319000
*/* SEGMENT WILL OCCUR. THE FIRST TEST WILL DETERMINE THE            */ 00320000
*/* ACTION THAT WILL BE TAKEN                                        */ 00321000
*/*                                                                  */ 00322000
*/********************************************************************/ 00323000
*                                                                  0187 00324000
*   IF PTAXETBS^=ZERO THEN          /* IF THERE IS AN ATTN EXIT TO BE   00325000
*                                      SCHEDULED THE SUBTASKS OF THE    00326000
*                                      TCB THAT IS HAVING THE ATTN 0187 00327000
*                                      EXIT SCHEDULED WILL BE STATUS    00328000
*                                      STOPPED                       */ 00329000
         LTR   PTAXETBS,PTAXETBS                                   0187 00330000
         BZ    @RF00187                                            0187 00331000
*     DO;                                                          0188 00332000
*       FRRATTS='1'B;               /* SET FOOTPRINT THAT AN ATTN  0189 00333000
*                                      EXIT WILL BE SCHD             */ 00334000
         OI    FRRATTS(FRRWKAP),B'01000000'                        0189 00335000
*       FRRSTAXE=PTAXETBS;          /* SAVE THE ADDRESS OF THE TAXE     00336000
*                                      TO BE SCHEDULED               */ 00337000
         ST    PTAXETBS,FRRSTAXE(,FRRWKAP)                         0190 00338000
*       TAXEPTR=PTAXETBS;           /* INITIALIZE BASE REG FOR THE 0191 00339000
*                                      TAXE                          */ 00340000
         LR    TAXEPTR,PTAXETBS                                    0191 00341000
*       IF RCTDCLST>OFF THEN                                       0192 00342000
         L     @15,ASXBRCTD(,ASXBPTR)                              0192 00343000
         CLC   RCTDCLST(2,@15),@CH00036                            0192 00344000
         BNH   @RF00192                                            0192 00345000
*         TAXEATTN='1'B;            /* IF >0 THEN SET FLAG FOR ATTN     00346000
*                                      PROLOGUE(IEAVAR05)    @ZA08100*/ 00347000
         OI    TAXEATTN(TAXEPTR),B'00000100'                       0193 00348000
*       TCBPTR=TAXETCB;             /* INITIALIZE THE TCB BASE     0194 00349000
*                                      REGISTER                      */ 00350000
@RF00192 L     TCBPTR,TAXETCB(,TAXEPTR)                            0194 00351000
*       IF TCBLTC^=ZERO THEN        /* IF A DAUGHER EXITS SEARCH   0195 00352000
*                                      SUBTASK QUEUE                 */ 00353000
         L     @15,TCBLTC(,TCBPTR)                                 0195 00354000
         LTR   @15,@15                                             0195 00355000
         BZ    @RF00195                                            0195 00356000
*         DO;                                                      0196 00357000
*           RESPECIFY                                              0197 00358000
*             PTCBPTR RESTRICTED;                                  0197 00359000
*           RESPECIFY                                              0198 00360000
*             STATST0 RESTRICTED;                                  0198 00361000
*           RESPECIFY                                              0199 00362000
*             STATST1 RESTRICTED;                                  0199 00363000
*           RESPECIFY                                              0200 00364000
*             STATST13 RESTRICTED;                                 0200 00365000
*           TCBPTR=TCBLTC;          /* ESTABLISH TCB BASE            */ 00366000
         LR    TCBPTR,@15                                          0201 00367000
*STSTOP1:                                                          0202 00368000
*           DO WHILE TCBPTR^=ZERO;  /* TCB BASE ZERO                 */ 00369000
STSTOP1  B     @DE00202                                            0202 00370000
@DL00202 DS    0H                                                  0203 00371000
*/* SET UP THE INTERFACE FOR STATUS STOP TCB                         */ 00372000
*                                                                  0203 00373000
*             FRRGTSS='1'B;         /* SET FOOTPRINT INDICATING GOING   00374000
*                                      TO STATUS STOP                */ 00375000
         OI    FRRGTSS(FRRWKAP),B'00100000'                        0203 00376000
*             FRRSSTCB=TCBPTR;      /* SAVE THE ADDRESS OF THE TCB TO   00377000
*                                      BE STOPPED                    */ 00378000
         ST    TCBPTR,FRRSSTCB(,FRRWKAP)                           0204 00379000
*             STATST0='00000006'X;  /* PLACE A SIX IN PARM REG       */ 00380000
         LA    STATST0,6                                           0205 00381000
*             STATST1=TCBPTR;       /* PLACE ADDRESS OF TCB TO BE  0206 00382000
*                                      STOPPED IN PARM REG           */ 00383000
         LR    STATST1,TCBPTR                                      0206 00384000
*             STATST13=ZERO;        /* CLEAR PARM REG FOR MASK       */ 00385000
         SLR   STATST13,STATST13                                   0207 00386000
*             CALL STATENT;         /* GO TO STATUS STOP TCB         */ 00387000
         L     @10,SCVTSPTR(,CVTPTR)                               0208 00388000
         L     @15,SCVTSTAT(,@10)                                  0208 00389000
         BALR  @14,@15                                             0208 00390000
*             FRRGTSS='0'B;         /* RESET THE FOOTPRINT IND THAT     00391000
*                                      STATUS HAS BEEN ENTERED       */ 00392000
         NI    FRRGTSS(FRRWKAP),B'11011111'                        0209 00393000
*             PTCBPTR=TCBPTR;       /* SAVE PREV TCB PTR             */ 00394000
         LR    PTCBPTR,TCBPTR                                      0210 00395000
*             TCBPTR=TCBLTC;        /* UPDATE TCB SEARCH ARG         */ 00396000
         L     TCBPTR,TCBLTC(,TCBPTR)                              0211 00397000
*           END;                                                   0212 00398000
@DE00202 LTR   TCBPTR,TCBPTR                                       0212 00399000
         BNZ   @DL00202                                            0212 00400000
*           TCBPTR=PTCBPTR;         /* REESTABLISH TCB BASE          */ 00401000
         LR    TCBPTR,PTCBPTR                                      0213 00402000
*           DO WHILE TCBPTR^=TAXETCB;/* CONTINUE SEARCHING TCB CHAIN    00403000
*                                      UNTIL THE ORIGINAL TCB        */ 00404000
         B     @DE00214                                            0214 00405000
@DL00214 DS    0H                                                  0215 00406000
*             IF TCBNTC^=ZERO THEN  /* IF THERE IS A SISTER TCB USE     00407000
*                                      IT AS NEW SEARCH ARGUMENT TCB */ 00408000
         L     @15,TCBNTC(,TCBPTR)                                 0215 00409000
         LTR   @15,@15                                             0215 00410000
         BZ    @RF00215                                            0215 00411000
*               DO;                                                0216 00412000
