*                 CALL STATENT;     /* GO TO STATUS START TCB        */ 00826000
         L     @14,SCVTSPTR(,CVTPTR)                               0360 00827000
         L     @15,SCVTSTAT(,@14)                                  0360 00828000
         BALR  @14,@15                                             0360 00829000
*                 PTCBPTR=TCBPTR;   /* SAVE PREVIOUS TCB PTR         */ 00830000
         LR    PTCBPTR,TCBPTR                                      0361 00831000
*                 TCBPTR=TCBLTC;    /* INDEX TCB BASE TO NEXT        */ 00832000
         L     TCBPTR,TCBLTC(,TCBPTR)                              0362 00833000
*                 IF PTCBPTR=FRRSSTCB THEN/* HAVE ALL STOPPED TCBS 0363 00834000
*                                      BEEN STARTED                  */ 00835000
         C     PTCBPTR,FRRSSTCB(,FRRWKAP)                          0363 00836000
         BNE   @RF00363                                            0363 00837000
*                   DO;             /* YES                           */ 00838000
*                     GO TO FR4ATTN1;/* STOP STARTING TCBS           */ 00839000
         B     FR4ATTN1                                            0365 00840000
*                   END;                                           0366 00841000
*               END;                                               0367 00842000
@RF00363 DS    0H                                                  0367 00843000
@DE00356 LTR   TCBPTR,TCBPTR                                       0367 00844000
         BNZ   @DL00356                                            0367 00845000
*               TCBPTR=PTCBPTR;     /* RE-ESTABLISH TCB BASE         */ 00846000
