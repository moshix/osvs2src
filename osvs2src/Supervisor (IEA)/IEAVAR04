@DE00406 LTR   TAXEPTR,TAXEPTR                                     0410 00965000
         BNZ   @DL00406                                            0410 00966000
*         END;                                                     0411 00967000
*       RESPECIFY                                                  0412 00968000
*         PTAXETBS UNRESTRICTED;                                   0412 00969000
@RF00396 DS    0H                                                  0413 00970000
*                                                                  0413 00971000
*/********************************************************************/ 00972000
*/*                                                                  */ 00973000
*/* UPDATE THE STAX COUNT IN THE TSB AND CLEAR THE ATTN              */ 00974000
*/* COUNT. THE COUNT IS UPDATED USING COMPARE AND SWAP               */ 00975000
*/* THE COUNT WILL NOT BE ALLOWED TO BE GREATER THAN 255             */ 00976000
*/* EVEN IF MORE TAXES EXIST.                                        */ 00977000
*/*                                                                  */ 00978000
*/********************************************************************/ 00979000
*                                                                  0413 00980000
*       RESPECIFY                                                  0413 00981000
*         TEMPR142 RESTRICTED;                                     0413 00982000
*       RESPECIFY                                                  0414 00983000
*         TSBPTR RESTRICTED;                                       0414 00984000
*       RESPECIFY                                                  0415 00985000
*         NSWAPVAL RESTRICTED;                                     0415 00986000
*       RESPECIFY                                                  0416 00987000
*         SWAPVAL RESTRICTED;                                      0416 00988000
*       TEMPR142=TEMPPR14;          /* SAVE R14 SOMEWHERE ELSE       */ 00989000
         LR    TEMPR142,TEMPPR14                                   0417 00990000
*       SDWAPTR=TEMPSDWA;           /* RELOAD SDWA BASE REGISTER     */ 00991000
         LR    SDWAPTR,TEMPSDWA                                    0418 00992000
*       TSBPTR=ASCBTSB;             /* SET BASE REG FOR TSB          */ 00993000
         L     TSBPTR,ASCBTSB(,ASCBPTR)                            0419 00994000
*       GENERATE CODE NODEFS NOSETS NOFLOWS REFS(TSBPTR);          0420 00995000
