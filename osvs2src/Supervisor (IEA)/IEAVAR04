         MVC   SDWAVRA(24,SDWAPTR),FRRWKA(FRRWKAP)                 0481 01184000
*           SDWAURAL=LENGTH(FRRWKA);/* MOVE LENGTH OF RECORD INTO  0482 01185000
*                                      SDWA                          */ 01186000
         MVI   SDWAURAL(SDWAPTR),X'18'                             0482 01187000
*         END;                                                     0483 01188000
*       DO;                         /* SETRP RECORD(YES)WKAREA(SDWAPT   01189000
*                                      R)RECPARM(FRRMODID)RC(0)      */ 01190000
@RC00475 DS    0H                                                  0485 01191000
*         RESPECIFY                                                0485 01192000
*          (GPR00P,                                                0485 01193000
*           GPR01P,                                                0485 01194000
*           GPR14P,                                                0485 01195000
*           GPR15P) RSTD;                                          0485 01196000
*         GPR01P=SDWAPTR;                                          0486 01197000
*         GPR01P->SDWARCDE=0;       /* SAVE RC VALUE                 */ 01198000
         MVI   SDWARCDE(GPR01P),X'00'                              0487 01199000
*         GPR01P->SDWARCRD='1'B;    /* TURN ON RECORD INDICATOR      */ 01200000
