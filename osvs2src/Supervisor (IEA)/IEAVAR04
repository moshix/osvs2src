*       TCBPTR=ASXBFTCB;            /* INITIALIZE THE SEARCH ARGUMENT   01060000
*                                      TCB BASE REG                  */ 01061000
         L     TCBPTR,ASXBFTCB(,ASXBPTR)                           0440 01062000
*       DO WHILE TCBPTR^=ZERO;      /* SCAN THE ENTIRE TCB Q         */ 01063000
         B     @DE00441                                            0441 01064000
@DL00441 DS    0H                                                  0442 01065000
*         RBSECPTR=TCBRBP;          /* INITIALIZE THE SEARCH ARGUMENT   01066000
*                                      RB BASE REG.                  */ 01067000
         L     RBSECPTR,TCBRBP(,TCBPTR)                            0442 01068000
*         IF RBSECPTR^=ZERO THEN    /* SCAN THE ENTIRE RB Q BUT CHECK   01069000
*                                      TO SEE IF AN RB EXISTS        */ 01070000
         LTR   RBSECPTR,RBSECPTR                                   0443 01071000
         BZ    @RF00443                                            0443 01072000
*           DO;                                                    0444 01073000
*             DO WHILE RBSECPTR^=TCBPTR;/* SCAN WHOLE RB QUEUE       */ 01074000
         B     @DE00445                                            0445 01075000
@DL00445 DS    0H                                                  0446 01076000
*               IF RBFTP='110'B THEN/* IF THIS RB IS AN SVRB CHECK 0446 01077000
*                                      FOR TGET/TPUT RB              */ 01078000
         TM    RBFTP(RBSECPTR),B'11000000'                         0446 01079000
         BNO   @RF00446                                            0446 01080000
         TM    RBFTP(RBSECPTR),B'00100000'                         0446 01081000
         BNZ   @RF00446                                            0446 01082000
*                 DO;                                              0447 01083000
*                   RBSECPTR=RBLINK;/* UPDATE RB SEARCH ARGU         */ 01084000
