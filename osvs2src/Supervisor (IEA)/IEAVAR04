*               END;                                               0371 00853000
@DE00369 L     @15,TCBNTC(,TCBPTR)                                 0371 00854000
         LTR   @15,@15                                             0371 00855000
         BZ    @DL00369                                            0371 00856000
*               TCBPTR=TCBNTC;      /* LOAD SISTER TCB               */ 00857000
         LR    TCBPTR,@15                                          0372 00858000
*               GO TO FR4ATTN2;     /* CONTINUE STARTING TCBS        */ 00859000
         B     FR4ATTN2                                            0373 00860000
*             END;                                                 0374 00861000
*           RESPECIFY                                              0375 00862000
*             PTCBPTR UNRESTRICTED;                                0375 00863000
@RF00345 DS    0H                                                  0376 00864000
*           RESPECIFY                                              0376 00865000
*             STATST0 UNRESTRICTED;                                0376 00866000
*           RESPECIFY                                              0377 00867000
*             STATST1 UNRESTRICTED;                                0377 00868000
*           RESPECIFY                                              0378 00869000
*             STATST13 UNRESTRICTED;                               0378 00870000
*FR4ATTN1:                                                         0379 00871000
*           IF FRRSTRF='1'B THEN    /* HAS A TAXE BEEN CANCELLED     */ 00872000
FR4ATTN1 TM    FRRSTRF(FRRWKAP),B'10000000'                        0379 00873000
         BNO   @RF00379                                            0379 00874000
*             DO;                   /* YES                           */ 00875000
*               TAXEPTR=FRRTRESM;   /* ESTABLISH NEW TAXE BASE       */ 00876000
         L     TAXEPTR,FRRTRESM(,FRRWKAP)                          0381 00877000
*               TAXERESM='0'B;      /* RESET CANCELLATION FLAG       */ 00878000
         NI    TAXERESM(TAXEPTR),B'11101111'                       0382 00879000
*             END;                                                 0383 00880000
*         END;                                                     0384 00881000
@RF00379 DS    0H                                                  0385 00882000
*                                                                  0385 00883000
*/********************************************************************/ 00884000
*/*                                                                  */ 00885000
*/* THE FOLLOWING SEGMENT OF CODE WILL CHECK TO SEE IF THERE         */ 00886000
*/* ARE ANY TAXES ON THE QUEUE. IF THERE ARE THE ENTIRE TAXE         */ 00887000
*/* QUEUE WILL BE SEARCHED LOCKING FOR THE HIGHEST LEVEL TAXE        */ 00888000
*/* THAT HAS BEEN SCHEDULED.                                         */ 00889000
*/*                                                                  */ 00890000
*/********************************************************************/ 00891000
*                                                                  0385 00892000
*       TAXEPTR=RCTDTAXE;           /* INITIALIZE SEARCH ARGU        */ 00893000
@RF00343 L     @12,ASXBRCTD(,ASXBPTR)                              0385 00894000
         L     TAXEPTR,RCTDTAXE(,@12)                              0385 00895000
*       PTAXETBS=PTAXETBS-PTAXETBS; /* CLEAR PREVIOUS ACTIVE TAXE PTR*/ 00896000
