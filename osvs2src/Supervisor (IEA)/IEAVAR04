*                                      TGET/TPUT SVC                 */ 01097000
         OI    TCBTIOTG(TCBPTR),B'00010000'                        0451 01098000
*                       RESPECIFY                                  0452 01099000
*                         STRSET0 RESTRICTED;                      0452 01100000
*                       RESPECIFY                                  0453 01101000
*                         STRSET1 RESTRICTED;                      0453 01102000
*                       RESPECIFY                                  0454 01103000
*                         STRSET13 RESTRICTED;                     0454 01104000
*                       STRSET0='0000000B'X;/* SET ENTRY CODE FOR THE   01105000
*                                      STATUS SET ROUTINE TO A HEX B */ 01106000
         LA    STRSET0,11                                          0455 01107000
*                       STRSET1=TCBPTR;/* ADDRESS OF THE TCB TO BE 0456 01108000
*                                      STATUS RESET                  */ 01109000
         LR    STRSET1,TCBPTR                                      0456 01110000
*                       STRSET1=STRSET1|'80000000'X;/* OR ON THE HIGH   01111000
*                                      ORDER BIT                     */ 01112000
         O     STRSET1,@CF02457                                    0457 01113000
*                       STRSET13='00000C00'X;/* SET MASK FOR THE NON    01114000
*                                      DISPATCHABILITY BIT           */ 01115000
         LA    STRSET13,3072                                       0458 01116000
*                       CALL STATENT;/* GO TO STATUS RESET           */ 01117000
         L     @12,SCVTSPTR(,CVTPTR)                               0459 01118000
         L     @15,SCVTSTAT(,@12)                                  0459 01119000
         BALR  @14,@15                                             0459 01120000
*                       RESPECIFY                                  0460 01121000
*                         STRSET0 UNRESTRICTED;                    0460 01122000
*                       RESPECIFY                                  0461 01123000
*                         STRSET1 UNRESTRICTED;                    0461 01124000
*                       RESPECIFY                                  0462 01125000
*                         STRSET13 UNRESTRICTED;                   0462 01126000
*                     END;                                         0463 01127000
*                 END;                                             0464 01128000
*               ELSE                /* NO                            */ 01129000
*                 DO;                                              0465 01130000
         B     @RC00446                                            0465 01131000
@RF00446 DS    0H                                                  0466 01132000
*                   RBSECPTR=RBLINKB;/* UPDATE RB SEARCH ARGUMENT    */ 01133000
         L     @12,RBLINKB-1(,RBSECPTR)                            0466 01134000
         LA    @12,0(,@12)                                         0466 01135000
         LR    RBSECPTR,@12                                        0466 01136000
*                 END;                                             0467 01137000
*             END;                                                 0468 01138000
@RC00446 DS    0H                                                  0468 01139000
@DE00445 CR    RBSECPTR,TCBPTR                                     0468 01140000
         BNE   @DL00445                                            0468 01141000
*           END;                                                   0469 01142000
*         TCBPTR=TCBTCB;            /* UPDATE TCB SEARCH ARGUMENT    */ 01143000
@RF00443 L     TCBPTR,TCBTCB(,TCBPTR)                              0470 01144000
*       END;                                                       0471 01145000
@DE00441 LTR   TCBPTR,TCBPTR                                       0471 01146000
         BNZ   @DL00441                                            0471 01147000
*       RESPECIFY                                                  0472 01148000
*         RBSECPTR UNRESTRICTED;                                   0472 01149000
*                                                                  0473 01150000
*/********************************************************************/ 01151000
*/*                                                                  */ 01152000
*/* THIS SEGMENT OF CODE WILL MOVE THE NECESSARY INFORMATION         */ 01153000
*/* INTO THE SDWA AND WILL COMMUNICATE WITH RTM                      */ 01154000
*/*                                                                  */ 01155000
*/********************************************************************/ 01156000
*                                                                  0473 01157000
*FR4RTN:                                                           0473 01158000
*       SDWAPTR=TEMPSDWA;           /* RELOAD SDWA PTR               */ 01159000
FR4RTN   LR    SDWAPTR,TEMPSDWA                                    0473 01160000
*       RESPECIFY                                                  0474 01161000
*         TEMPSDWA UNRESTRICTED;                                   0474 01162000
*       IF SDWAVRAL<LENGTH(FRRWKA) THEN/* CHECK IF VARIABLE AREA WILL   01163000
