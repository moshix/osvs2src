*               TCBPTR=TCBLTC;      /* LOAD DAUGHTER OF ATTN TCB     */ 00807000
         L     TCBPTR,TCBLTC(,TCBPTR)                              0352 00808000
*               RESPECIFY                                          0353 00809000
*                 STATST0 RESTRICTED;                              0353 00810000
*               RESPECIFY                                          0354 00811000
*                 STATST1 RESTRICTED;                              0354 00812000
*               RESPECIFY                                          0355 00813000
*                 STATST13 RESTRICTED;                             0355 00814000
*               DO WHILE TCBPTR^=ZERO;/* LAST TCB ON SUBTASK QUEUE   */ 00815000
         B     @DE00356                                            0356 00816000
@DL00356 DS    0H                                                  0357 00817000
*FR4ATTN2:                                                         0357 00818000
*                 STATST0=SEVEN;    /* INDICATE STATUS START         */ 00819000
FR4ATTN2 LA    STATST0,7                                           0357 00820000
*                 STATST1=TCBPTR;   /* PLACE ADDRESS OF TCB TO BE  0358 00821000
*                                      STARTED IN PARM REG           */ 00822000
         LR    STATST1,TCBPTR                                      0358 00823000
*                 STATST13=ZERO;    /* CLEAR PARM REG FOR MASK       */ 00824000
