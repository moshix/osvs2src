         TITLE 'IEAVAR06 - ATTENTION EXIT EPILOGUE ROUTINE             *00001000
                        '                                               00002000
IEAVAR06 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVAR06  77.133'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @08,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@08                                         0001 00013000
RBPRFXLN EQU 64                                                         00014000
RBPRFLNA EQU 32                                                         00015000
PRBLEN   EQU 136                                                        00016000
SIRBLEN  EQU 200                                                        00017000
TIRBLEN  EQU 136                                                        00018000
IRBLEN   EQU 128                                                        00019000
SVRBLEN  EQU 200                                                        00020000
DECREG   DS    0H                                                  0049 00021000
*                                                                  0087 00022000
*/********************************************************************/ 00023000
*/*                                                                  */ 00024000
*/*  START OF EXECUTABLE CODE                                        */ 00025000
*/*                                                                  */ 00026000
*/*  THE LOCAL LOCK IS HELD AT ENTRY FROM EXIT.                      */ 00027000
*/*                                                                  */ 00028000
*/*  THIS SEGMENT OF CODE WILL ESTABLISH A RECOVERY                  */ 00029000
*/*  ENVIRONMENT FOR THE LOCKED CODE OF THIS ROUTINE.                */ 00030000
*/*                                                                  */ 00031000
*/********************************************************************/ 00032000
*                                                                  0087 00033000
*   ENTREG13=RTNREG13;              /* SAVE ADDR OF SAVE AREA        */ 00034000
         LR    ENTREG13,RTNREG13                                   0087 00035000
*   RESPECIFY                                                      0088 00036000
*     RTNREG13 UNRESTRICTED;                                       0088 00037000
*   RESPECIFY                                                      0089 00038000
*     FRRSPTR RESTRICTED;                                          0089 00039000
*   RESPECIFY                                                      0090 00040000
*     FRREPTR RESTRICTED;                                          0090 00041000
*   FRRSPTR=ADDR(IEAVAFR6);         /* ENTRY POINT TO FRR            */ 00042000
         LA    FRRSPTR,IEAVAFR6                                    0091 00043000
*   GENERATE CODE NODEFS NOSETS NOFLOWS REFS(FRRWKAP,FRRSPTR,FRREPTR,   00044000
*       PSACSTK,FRRSCURR,FRRSLAST,FRRSELEN,FRRSFRRA,FRRSPARM,PSALSFCC); 00045000
        SETFRR A,FRRAD=(FRRSPTR),PARMAD=(FRRWKAP),                     X00046000
               WRKREGS=(FRRWKAP,FRREPTR)                                00047000
*   RESPECIFY                                                      0093 00048000
*     FRRSPTR UNRESTRICTED;                                        0093 00049000
*   RESPECIFY                                                      0094 00050000
*     FRREPTR UNRESTRICTED;                                        0094 00051000
*                                                                  0095 00052000
*/********************************************************************/ 00053000
*/*                                                                  */ 00054000
*/* THE FOLLOWING SEGMENT OF CODE WILL RESET THE FLAGS IN            */ 00055000
*/* THE TAXE AND WILL SAVE THE ADDRESS OF THE TAXE IN THE            */ 00056000
*/* FRR WORK AREA.                                                   */ 00057000
*/*                                                                  */ 00058000
*/********************************************************************/ 00059000
*                                                                  0095 00060000
*   TAXEPTR=RBSECPTR;               /* ESTABLISH BASE REG FOR THE  0095 00061000
*                                      TAXE                          */ 00062000
         LR    TAXEPTR,RBSECPTR                                    0095 00063000
*   FRRTAXE=TAXEPTR;                /* SAVE THE TAXE POINTER         */ 00064000
         ST    TAXEPTR,FRRTAXE(,FRRWKAP)                           0096 00065000
*   TAXESCHD='0'B;                  /* RESET SCHEDULED FLAG          */ 00066000
*   TAXERESM='0'B;                  /* RESET CANCELLED FLAG          */ 00067000
         NI    TAXESCHD(TAXEPTR),B'11100111'                       0098 00068000
*                                                                  0099 00069000
*/********************************************************************/ 00070000
*/*                                                                  */ 00071000
*/* THE FOLLOWING SEGMENT OF CODE WILL FREE THE TAIE IF              */ 00072000
*/* ONE WAS GOTTEN BY THE ATTENTION PROLOGUE ROUTINE                 */ 00073000
*/*                                                                  */ 00074000
*/********************************************************************/ 00075000
*                                                                  0099 00076000
*   IF TAXETAIE^='00000000'X THEN   /* IF THERE IS A TAIE FREE IT    */ 00077000
         L     @12,TAXETAIE(,TAXEPTR)                              0099 00078000
         LTR   @12,@12                                             0099 00079000
         BZ    @RF00099                                            0099 00080000
*     DO;                                                          0100 00081000
*       RESPECIFY                                                  0101 00082000
*         FREMPR0 RESTRICTED;                                      0101 00083000
*       RESPECIFY                                                  0102 00084000
*         FREMPR1 RESTRICTED;                                      0102 00085000
*       RESPECIFY                                                  0103 00086000
*         FREMPR3 RESTRICTED;                                      0103 00087000
*       RESPECIFY                                                  0104 00088000
*         FREMPR15 RESTRICTED;                                     0104 00089000
*       FRRGTFTI='1'B;              /* SET FOOTPRINT FOR FRR         */ 00090000
         OI    FRRGTFTI(FRRWKAP),B'10000000'                       0105 00091000
*       FREMPR0=LENGTH(TAIE);       /* LENGTH OF TAIE                */ 00092000
         LA    FREMPR0,72                                          0106 00093000
*       FREMPR1=TAXETAIE;           /* ADDR OF TAIE                  */ 00094000
         LR    FREMPR1,@12                                         0107 00095000
*       GENERATE CODE NOSETS NOFLOWS NODEFS REFS(CVTCRMN,CVTMAP);  0108 00096000
*                                                                  0108 00097000
         FREEMAIN RC,SP=250,BRANCH=YES,LV=(0),A=(1)            @ZA07753 00098000
*/* THE RETURN CODE FROM FREEMAIN WILL NOT BE CHECKED                */ 00099000
*/* BECAUSE THIS WAS A CONDITIONAL FREEMAIN                          */ 00100000
*                                                                  0109 00101000
*       RESPECIFY                                                  0109 00102000
*         FREMPR0 UNRESTRICTED;                                    0109 00103000
*       RESPECIFY                                                  0110 00104000
*         FREMPR1 UNRESTRICTED;                                    0110 00105000
*       RESPECIFY                                                  0111 00106000
*         FREMPR3 UNRESTRICTED;                                    0111 00107000
*       RESPECIFY                                                  0112 00108000
*         FREMPR15 UNRESTRICTED;                                   0112 00109000
*       TAXETAIE='00000000'X;       /* CLEAR POINTER TO TAIE         */ 00110000
         SLR   @12,@12                                             0113 00111000
         ST    @12,TAXETAIE(,TAXEPTR)                              0113 00112000
*       FRRCFFTI='1'B;              /* SET TRACKING FOOTPRINT        */ 00113000
         OI    FRRCFFTI(FRRWKAP),B'01000000'                       0114 00114000
*     END;                                                         0115 00115000
*                                                                  0116 00116000
*/********************************************************************/ 00117000
*/*                                                                  */ 00118000
*/* THE FOLLOWING SEGMENT OF CODE WILL CHECK TO SEE IF THE           */ 00119000
*/* TAXE IS TO BE FREED. IF IT IS THE TAXE WILL BE FREED             */ 00120000
*/* BY EXIT. THE TAXE MUST, HOWEVER BE DEQUEUED FROM THE             */ 00121000
*/* TAXE QUEUE                                                       */ 00122000
*/*                                                                  */ 00123000
*/********************************************************************/ 00124000
*                                                                  0116 00125000
*   IF RBFDYN='1'B THEN             /* IS TAXE TO BE FREED           */ 00126000
@RF00099 TM    RBFDYN(RBSECPTR),B'00000010'                        0116 00127000
         BNO   @RF00116                                            0116 00128000
*     DO;                           /* YES                           */ 00129000
*       RESPECIFY                                                  0118 00130000
*         PTAXEPTR RESTRICTED;                                     0118 00131000
*       RESPECIFY                                                  0119 00132000
*         NTAXEPTR RESTRICTED;                                     0119 00133000
*       PTAXEPTR=PTAXEPTR-PTAXEPTR; /* CLEAR PREVIOUS PTR            */ 00134000
         SLR   PTAXEPTR,PTAXEPTR                                   0120 00135000
*       TAXEPTR=RCTDTAXE;           /* LOAD TAXE SEARCH ARGU         */ 00136000
         L     @01,ASXBPTR(,ASCBPTR)                               0121 00137000
         L     @01,ASXBRCTD(,@01)                                  0121 00138000
         L     TAXEPTR,RCTDTAXE(,@01)                              0121 00139000
*       DO WHILE TAXEPTR^='00000000'X;/* SEARCH WHOLE QUEUE          */ 00140000
         B     @DE00122                                            0122 00141000
@DL00122 DS    0H                                                  0123 00142000
*         IF TAXEPTR=RBSECPTR THEN  /* CURRENT TAXE FOUND            */ 00143000
         CR    TAXEPTR,RBSECPTR                                    0123 00144000
         BNE   @RF00123                                            0123 00145000
*           DO;                     /* YES                           */ 00146000
*             FRRTTBDQ='1'B;        /* SET A FOOTPRINT THAT A TAXE IS   00147000
*                                      TO BE DEQUEUED                */ 00148000
         OI    FRRTTBDQ(FRRWKAP),B'00100000'                       0125 00149000
*             IF PTAXEPTR='00000000'X THEN/* FIRST TAXE ON QUEUE     */ 00150000
         LTR   PTAXEPTR,PTAXEPTR                                   0126 00151000
         BNZ   @RF00126                                            0126 00152000
*               DO;                 /* YES                           */ 00153000
*                 RCTDTAXE=TAXELNK; /* DEQUEUE FIRST TAXE            */ 00154000
         L     @01,ASXBPTR(,ASCBPTR)                               0128 00155000
         L     @01,ASXBRCTD(,@01)                                  0128 00156000
         L     @00,TAXELNK-1(,TAXEPTR)                             0128 00157000
         ICM   @00,8,@CB02095                                      0128 00158000
         ST    @00,RCTDTAXE(,@01)                                  0128 00159000
*               END;                                               0129 00160000
*             ELSE                  /* NO                            */ 00161000
*               DO;                                                0130 00162000
         B     @RC00126                                            0130 00163000
@RF00126 DS    0H                                                  0131 00164000
*                 NTAXEPTR=TAXELNK; /* SAVE NEXT TAXE PTR            */ 00165000
         L     NTAXEPTR,TAXELNK-1(,TAXEPTR)                        0131 00166000
         LA    NTAXEPTR,0(,NTAXEPTR)                               0131 00167000
*                 TAXEPTR=PTAXEPTR; /* UPDATE BASE REG               */ 00168000
         LR    TAXEPTR,PTAXEPTR                                    0132 00169000
*                 FRRPTAXE=PTAXEPTR;/* SAVE PREVIOUS TAXE            */ 00170000
         ST    PTAXEPTR,FRRPTAXE(,FRRWKAP)                         0133 00171000
*                 FRRNTAXE=NTAXEPTR;/* SAVE NEXT TAXE                */ 00172000
         ST    NTAXEPTR,FRRNTAXE(,FRRWKAP)                         0134 00173000
*                 TAXELNK=NTAXEPTR; /* DEQUEUE THE TAXE              */ 00174000
         STCM  NTAXEPTR,7,TAXELNK(TAXEPTR)                         0135 00175000
*               END;                                               0136 00176000
*             FRRTHBDQ='1'B;        /* SET FOOTPRINT FOR FRR         */ 00177000
@RC00126 OI    FRRTHBDQ(FRRWKAP),B'00010000'                       0137 00178000
*             GO TO STATST;         /* GO START SUBTASKS             */ 00179000
         B     STATST                                              0138 00180000
*           END;                                                   0139 00181000
*         PTAXEPTR=TAXEPTR;         /* SAVE PREVIOUS TAXE PTR        */ 00182000
@RF00123 LR    PTAXEPTR,TAXEPTR                                    0140 00183000
*         TAXEPTR=TAXELNK;          /* UPDATE BASE FOR TAXE          */ 00184000
         L     @15,TAXELNK-1(,TAXEPTR)                             0141 00185000
         LA    @15,0(,@15)                                         0141 00186000
         LR    TAXEPTR,@15                                         0141 00187000
*       END;                                                       0142 00188000
@DE00122 LTR   TAXEPTR,TAXEPTR                                     0142 00189000
         BNZ   @DL00122                                            0142 00190000
*       RESPECIFY                                                  0143 00191000
*         NTAXEPTR UNRESTRICTED;                                   0143 00192000
*       RESPECIFY                                                  0144 00193000
*         PTAXEPTR UNRESTRICTED;                                   0144 00194000
*     END;                                                         0145 00195000
*                                                                  0146 00196000
*/********************************************************************/ 00197000
*/*                                                                  */ 00198000
*/* THE SUBTASKS OF THE TCB THAT HAS THE EXITING TAXE ARE            */ 00199000
*/* RESTARTED USING THE BRANCH ENTRY TO STATUS START TCB             */ 00200000
*/*                                                                  */ 00201000
*/********************************************************************/ 00202000
*                                                                  0146 00203000
*STATST:                                                           0146 00204000
*   FRRGTSS='1'B;                   /* SET FOOTPRINT IND. THAT GOING    00205000
*                                      TO STATUS START               */ 00206000
@RF00116 DS    0H                                                  0146 00207000
STATST   OI    FRRGTSS(FRRWKAP),B'00001000'                        0146 00208000
*/* SET UP INTERFACE FOR STATUS START ALL SUBTASKS                   */ 00209000
*                                                                  0147 00210000
*   RESPECIFY                                                      0147 00211000
*     STATPR0 RESTRICTED;                                          0147 00212000
*   RESPECIFY                                                      0148 00213000
*     STATPR1 RESTRICTED;                                          0148 00214000
*   RESPECIFY                                                      0149 00215000
*     STATPR13 RESTRICTED;                                         0149 00216000
*   RESPECIFY                                                      0150 00217000
*     STATPR15 RESTRICTED;                                         0150 00218000
*   STATPR0='00000007'X;            /* INDICATE START TCBS           */ 00219000
         LA    STATPR0,7                                           0151 00220000
*   STATPR1=STATPR1-STATPR1;        /* INDICATE ALL SUBTASKS         */ 00221000
         SLR   STATPR1,STATPR1                                     0152 00222000
*   STATPR13=STATPR13-STATPR13;     /* INDICATE NO MASK              */ 00223000
         SLR   STATPR13,STATPR13                                   0153 00224000
*   CALL STATENT;                   /* GO TO STATUS START            */ 00225000
         L     @12,CVTPTR                                          0154 00226000
         L     @12,SCVTSPTR(,@12)                                  0154 00227000
         L     @15,SCVTSTAT(,@12)                                  0154 00228000
         BALR  @14,@15                                             0154 00229000
*   FRRCFSS='1'B;                   /* SET FOOTPRINT FOR FRR         */ 00230000
*   RESPECIFY                                                      0156 00231000
*     STATPR0 UNRESTRICTED;                                        0156 00232000
*   RESPECIFY                                                      0157 00233000
*     STATPR1 UNRESTRICTED;                                        0157 00234000
*   RESPECIFY                                                      0158 00235000
*     STATPR13 UNRESTRICTED;                                       0158 00236000
*   RESPECIFY                                                      0159 00237000
*     STATPR15 UNRESTRICTED;                                       0159 00238000
*                                                                  0160 00239000
*/********************************************************************/ 00240000
*/*                                                                  */ 00241000
*/* THE FOLLOWING SEGMENT OF CODE WILL CHECK TO SEE IF               */ 00242000
*/* THERE ARE ANY TAXES ON THE QUEUE. IF THERE ARE THE               */ 00243000
*/* ENTIRE TAXE QUEUE WILL BE SEARCHED LOOKING FOR THE               */ 00244000
*/* HIGHEST LEVEL TAXE THAT HAS BEEN SCHEDULED.                      */ 00245000
*/*                                                                  */ 00246000
*/********************************************************************/ 00247000
*                                                                  0160 00248000
*   RESPECIFY                                                      0160 00249000
*     PTAXEPTR RESTRICTED;                                         0160 00250000
*   RESPECIFY                                                      0161 00251000
*     ORIGTAXE RESTRICTED;                                         0161 00252000
*   FRRSTS='1'B;                    /* SET FOOTPRINT FOR FRR         */ 00253000
         OI    FRRCFSS(FRRWKAP),B'00000110'                        0162 00254000
*   PTAXEPTR=PTAXEPTR-PTAXEPTR;     /* CLEAR PREV PTR                */ 00255000
         SLR   PTAXEPTR,PTAXEPTR                                   0163 00256000
*   TAXEPTR=RCTDTAXE;               /* INITIALIZE SEARCH ARGU        */ 00257000
         L     @12,ASXBPTR(,ASCBPTR)                               0164 00258000
         L     @12,ASXBRCTD(,@12)                                  0164 00259000
         L     TAXEPTR,RCTDTAXE(,@12)                              0164 00260000
*   ORIGTAXE=RBSECPTR;              /* SAVE ORIGINAL TAXE PTR        */ 00261000
         LR    ORIGTAXE,RBSECPTR                                   0165 00262000
*   DO WHILE TAXEPTR^='00000000'X;  /* ANY MORE TAXES                */ 00263000
         B     @DE00166                                            0166 00264000
@DL00166 DS    0H                                                  0167 00265000
*     IF TAXEPTR^=ORIGTAXE THEN     /* IS THIS EXITING TAXE          */ 00266000
         CR    TAXEPTR,ORIGTAXE                                    0167 00267000
         BE    @RF00167                                            0167 00268000
*       DO;                         /* NO                            */ 00269000
*         IF RBFACTV='1'B|TAXESCHD='1'B THEN/* IS THE TAXE ACTIVE    */ 00270000
         TM    RBFACTV(RBSECPTR),B'01000000'                       0169 00271000
         BO    @RT00169                                            0169 00272000
         TM    TAXESCHD(TAXEPTR),B'00001000'                       0169 00273000
         BNO   @RF00169                                            0169 00274000
@RT00169 DS    0H                                                  0170 00275000
*           DO;                     /* YES                           */ 00276000
*             PTAXEPTR=TAXEPTR;     /* SAVE ACTIVE TAXE PTR          */ 00277000
         LR    PTAXEPTR,TAXEPTR                                    0171 00278000
*           END;                                                   0172 00279000
*       END;                                                       0173 00280000
@RF00169 DS    0H                                                  0174 00281000
*     TAXEPTR=TAXELNK;              /* CONTINUE SEARCH               */ 00282000
@RF00167 L     @12,TAXELNK-1(,TAXEPTR)                             0174 00283000
         LA    @12,0(,@12)                                         0174 00284000
         LR    TAXEPTR,@12                                         0174 00285000
*     RBSECPTR=TAXEPTR;             /* RESTORE RB BASE REG           */ 00286000
         LR    RBSECPTR,TAXEPTR                                    0175 00287000
*   END;                                                           0176 00288000
@DE00166 SLR   @12,@12                                             0176 00289000
         CR    TAXEPTR,@12                                         0176 00290000
         BNE   @DL00166                                            0176 00291000
*   RBSECPTR=ORIGTAXE;              /* RESTORE ORIG RB BASE          */ 00292000
         LR    RBSECPTR,ORIGTAXE                                   0177 00293000
*   RESPECIFY                                                      0178 00294000
*     ORIGTAXE UNRESTRICTED;                                       0178 00295000
*                                                                  0179 00296000
*/********************************************************************/ 00297000
*/*                                                                  */ 00298000
*/* THE TAXES FOLLOWING THE HIGHEST ACTIVE TAXE MUST BE              */ 00299000
*/* MARKED AVAILABLE FOR USE AND THEY MUST BE REFLECTED              */ 00300000
*/* IN THE TSBSTAX COUNT. THIS SEGMENT OF CODE WILL                  */ 00301000
*/* SEARCH THE ENTIRE TAXE QUEUE STARTING WITH THE HIGHEST           */ 00302000
*/* SCHEDULED TAXE AND PERFORM THE OPERATION.                        */ 00303000
*/*                                                                  */ 00304000
*/********************************************************************/ 00305000
*                                                                  0179 00306000
*   RESPECIFY                                                      0179 00307000
*     STAXCNT RESTRICTED;                                          0179 00308000
*   STAXCNT=STAXCNT-STAXCNT;        /* INITIALIZE STAX COUNT         */ 00309000
         SLR   STAXCNT,STAXCNT                                     0180 00310000
*   IF RCTDTAXE^='00000000'X THEN   /* ARE THERE ANY TAXES           */ 00311000
         SLR   @15,@15                                             0181 00312000
         L     @01,ASXBPTR(,ASCBPTR)                               0181 00313000
         L     @01,ASXBRCTD(,@01)                                  0181 00314000
         C     @15,RCTDTAXE(,@01)                                  0181 00315000
         BE    @RF00181                                            0181 00316000
*     DO;                           /* YES                           */ 00317000
*       IF PTAXEPTR^='00000000'X THEN/* IS THIS HEAD OF QUEUE        */ 00318000
         CR    PTAXEPTR,@15                                        0183 00319000
         BE    @RF00183                                            0183 00320000
*         DO;                       /* NO                            */ 00321000
*           TAXEPTR=PTAXEPTR;       /* ESTABLISH BASE                */ 00322000
         LR    TAXEPTR,PTAXEPTR                                    0185 00323000
*           TAXEPTR=TAXELNK;        /* SET BASE TO FIRST TAXE TO BE     00324000
*                                      MADE AVAILABLE                */ 00325000
         L     @15,TAXELNK-1(,TAXEPTR)                             0186 00326000
         LA    @15,0(,@15)                                         0186 00327000
         LR    TAXEPTR,@15                                         0186 00328000
*         END;                                                     0187 00329000
*       ELSE                        /* YES                           */ 00330000
*         DO;                                                      0188 00331000
         B     @RC00183                                            0188 00332000
@RF00183 DS    0H                                                  0189 00333000
*           TAXEPTR=RCTDTAXE;       /* SET BASE TO LOWEST POSSIBLE 0189 00334000
*                                      TAXE                          */ 00335000
         L     @01,ASXBPTR(,ASCBPTR)                               0189 00336000
         L     @01,ASXBRCTD(,@01)                                  0189 00337000
         L     TAXEPTR,RCTDTAXE(,@01)                              0189 00338000
*         END;                                                     0190 00339000
*       DO WHILE TAXEPTR^='00000000'X;/* SEARCH THE REMAINDER OF THE    00340000
*                                      QUEUE                         */ 00341000
@RC00183 B     @DE00191                                            0191 00342000
@DL00191 DS    0H                                                  0192 00343000
*         TAXEFREQ='0'B;            /* MARK TAXE AVAILABLE           */ 00344000
         NI    TAXEFREQ(TAXEPTR),B'11011111'                       0192 00345000
*         STAXCNT=STAXCNT+'00010000'X;/* INCREMENT COUNT BY ONE      */ 00346000
         AL    STAXCNT,@CF02063                                    0193 00347000
*         TAXEPTR=TAXELNK;          /* UPDATE TAXE PTR TO NEXT       */ 00348000
         L     @15,TAXELNK-1(,TAXEPTR)                             0194 00349000
         LA    @15,0(,@15)                                         0194 00350000
         LR    TAXEPTR,@15                                         0194 00351000
*       END;                                                       0195 00352000
@DE00191 LTR   TAXEPTR,TAXEPTR                                     0195 00353000
         BNZ   @DL00191                                            0195 00354000
*     END;                                                         0196 00355000
*   RESPECIFY                                                      0197 00356000
*     PTAXEPTR UNRESTRICTED;                                       0197 00357000
@RF00181 DS    0H                                                  0198 00358000
*                                                                  0198 00359000
*/********************************************************************/ 00360000
*/*                                                                  */ 00361000
*/* UPDATE THE STAX COUNT IN THE TSB                                 */ 00362000
*/*                                                                  */ 00363000
*/* THE COUNT IS UPDATED USING COMPARE AND SWAP.                     */ 00364000
*/*                                                                  */ 00365000
*/* THE COUNT WILL NOT BE ALLOWED TO BE GREATER THAN 256             */ 00366000
*/* EVEN IF MORE TAXES EXIST.                                        */ 00367000
*/*                                                                  */ 00368000
*/********************************************************************/ 00369000
*                                                                  0198 00370000
*   RESPECIFY                                                      0198 00371000
*     TSBPTR RESTRICTED;                                           0198 00372000
*   RESPECIFY                                                      0199 00373000
*     PSTAXCNT RESTRICTED;                                         0199 00374000
*   RESPECIFY                                                      0200 00375000
*     NSTAXCNT RESTRICTED;                                         0200 00376000
*   TSBPTR=ASCBTSB;                 /* LOAD THE TSB BASE REG         */ 00377000
         L     TSBPTR,ASCBTSB(,ASCBPTR)                            0201 00378000
*   GENERATE CODE NODEFS NOSETS NOFLOWS REFS(TSBPTR);              0202 00379000
         USING TSB,TSBPTR                                               00380000
*   IF STAXCNT>'00FF0000'X THEN     /* IS STAX COUNT > 255           */ 00381000
         L     @15,@CF02064                                        0203 00382000
         CLR   STAXCNT,@15                                         0203 00383000
         BNH   @RF00203                                            0203 00384000
*     DO;                           /* YES                           */ 00385000
*       STAXCNT='00FF0000'X;        /* SET TO 255                    */ 00386000
         LR    STAXCNT,@15                                         0205 00387000
*     END;                                                         0206 00388000
*   NSTAXCNT=STAXCNT;               /* SAVE NEW STAX COUNT           */ 00389000
@RF00203 LR    NSTAXCNT,STAXCNT                                    0207 00390000
*   PSTAXCNT=TSBATTNC;              /* LOAD ORIG SWAP VALUE          */ 00391000
         L     PSTAXCNT,TSBATTNC                                   0208 00392000
*LOOP:                                                             0209 00393000
*   STAXCNT=PSTAXCNT;               /* LOAD PREVIOUS STAX CNT        */ 00394000
LOOP     LR    STAXCNT,PSTAXCNT                                    0209 00395000
*   STAXCNT=STAXCNT&'FF00FFFF'X;    /* CLEAR OLD STAX COUNT          */ 00396000
         N     STAXCNT,@CF02067                                    0210 00397000
*   STAXCNT=STAXCNT|NSTAXCNT;       /* INSERT NEW STAX COUNT LOAD  0211 00398000
*                                      ORIGINAL STAX CNT             */ 00399000
         OR    STAXCNT,NSTAXCNT                                    0211 00400000
*   CS(PSTAXCNT,STAXCNT,TSBATTNC);  /* PERFORM UPDATE                */ 00401000
         CS    PSTAXCNT,@12,TSBATTNC                               0212 00402000
*   BC(7,LOOP);                     /* DID THE UPDATE WORK? IF IT DID   00403000
*                                      CONTINUE                      */ 00404000
         BC    7,LOOP                                              0213 00405000
*   GENERATE CODE NODEFS NOSETS NOFLOWS REFS(TSBPTR);              0214 00406000
         DROP TSBPTR                                                    00407000
*   RESPECIFY                                                      0215 00408000
*     PSTAXCNT UNRESTRICTED;                                       0215 00409000
*   RESPECIFY                                                      0216 00410000
*     NSTAXCNT UNRESTRICTED;                                       0216 00411000
