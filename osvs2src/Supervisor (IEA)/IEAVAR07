*       DO;                         /* SETRP RECORD(YES)WKAREA(SDWAPT   00533000
*                                      R)RECPARM(FRRMODID)RC(0)      */ 00534000
*         RESPECIFY                                                0235 00535000
*          (GPR00P,                                                0235 00536000
*           GPR01P,                                                0235 00537000
*           GPR14P,                                                0235 00538000
*           GPR15P) RSTD;                                          0235 00539000
*         GPR01P=SDWAPTR;                                          0236 00540000
*         GPR01P->SDWARCDE=0;       /* SAVE RC VALUE                 */ 00541000
