*       RESPECIFY                                                  0210 00467000
*         TSBPTR RESTRICTED;                                       0210 00468000
*       RESPECIFY                                                  0211 00469000
*         SWAPVAL RESTRICTED;                                      0211 00470000
*       RESPECIFY                                                  0212 00471000
*         NSWAPVAL RESTRICTED;                                     0212 00472000
*       TSBPTR=ASCBTSB;             /* INITIALIZE TSB BASE REG       */ 00473000
         L     TSBPTR,ASCBTSB(,ASCBPTR)                            0213 00474000
*       GENERATE CODE NOSETS NODEFS NOFLOWS REFS(TSBPTR);          0214 00475000
        USING  TSB,TSBPTR                                               00476000
*       SWAPVAL=TSBATTNC;           /* LOAD CURRENT TSB VALUE        */ 00477000
         L     SWAPVAL,TSBATTNC                                    0215 00478000
*LOOP1:                                                            0216 00479000
*       NSWAPVAL=SWAPVAL;           /* LOAD CURRENT TSB VALUE        */ 00480000
LOOP1    LR    NSWAPVAL,SWAPVAL                                    0216 00481000
*       NSWAPVAL=NSWAPVAL&'0000FFFF'X;/* CLEAR COUNTS                */ 00482000
         N     NSWAPVAL,@CF01377                                   0217 00483000
*       CS(SWAPVAL,NSWAPVAL,TSBATTNC);/* UPDATE TSB                  */ 00484000
         CS    SWAPVAL,@12,TSBATTNC                                0218 00485000
