         TITLE 'IEAVAR07 - ATTENTION EXIT PURGE ROUTINE                *00001000
                        '                                               00002000
IEAVAR07 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVAR07  77.133'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @08,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@08                                         0001 00013000
RBPRFXLN EQU 64                                                         00014000
RBPRFLNA EQU 32                                                         00015000
PRBLEN   EQU 136                                                        00016000
SIRBLEN  EQU 200                                                        00017000
TIRBLEN  EQU 136                                                        00018000
IRBLEN   EQU 128                                                        00019000
SVRBLEN  EQU 200                                                        00020000
DECREG   DS    0H                                                  0043 00021000
*   ENTREG13=RTNREG13;              /* SAVE REG SAVE PTR             */ 00022000
         LR    ENTREG13,RTNREG13                                   0075 00023000
*   RESPECIFY                                                      0076 00024000
*     RTNREG13 UNRESTRICTED;                                       0076 00025000
*   RESPECIFY                                                      0077 00026000
*     ASCBPTR RESTRICTED;                                          0077 00027000
*   ASCBPTR=PSAAOLD;                /* INITIALIZE ASCB BASE          */ 00028000
         L     ASCBPTR,PSAAOLD                                     0078 00029000
*   IF ASCBTSB^='00000000'X THEN    /* IS THIS A TSO USER            */ 00030000
         L     @12,ASCBTSB(,ASCBPTR)                               0079 00031000
         LTR   @12,@12                                             0079 00032000
         BZ    @RF00079                                            0079 00033000
*     DO;                                                          0080 00034000
*                                                                  0080 00035000
*/* THE SETLOCK MACRO USES AND DOES NOT SAVE REGISTERS               */ 00036000
*/* 11,12,13 AND 14.                                                 */ 00037000
*                                                                  0081 00038000
*       RESPECIFY                                                  0081 00039000
*         SETLK11 RESTRICTED;                                      0081 00040000
*       RESPECIFY                                                  0082 00041000
*         SETLK12 RESTRICTED;                                      0082 00042000
*       RESPECIFY                                                  0083 00043000
*         SETLK13 RESTRICTED;                                      0083 00044000
*       RESPECIFY                                                  0084 00045000
*         SETLK14 RESTRICTED;                                      0084 00046000
*SETLKO1:                           /* GET THE LOCAL LOCK            */ 00047000
*       GENERATE CODE NODEFS NOSETS NOFLOWS REFS(PSALITA);         0085 00048000
SETLKO1  DS    0H                                                  0085 00049000
       SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                          X00050000
               RELATED=(TAXE-QUEUE,IEAVAR07(SETLKR1))                   00051000
*       RESPECIFY                                                  0086 00052000
*         SETLK11 UNRESTRICTED;                                    0086 00053000
*       RESPECIFY                                                  0087 00054000
*         SETLK12 UNRESTRICTED;                                    0087 00055000
*       RESPECIFY                                                  0088 00056000
*         SETLK13 UNRESTRICTED;                                    0088 00057000
*       RESPECIFY                                                  0089 00058000
*         SETLK14 UNRESTRICTED;                                    0089 00059000
*                                                                  0090 00060000
*/********************************************************************/ 00061000
*/*                                                                  */ 00062000
*/* THE FOLLOWING SEGMENT OF CODE WILL ESTABLISH A RECOVERY          */ 00063000
*/* ENVIRONMENT FOR THE LOCKED CODE OF THIS ROUTINE                  */ 00064000
*/*                                                                  */ 00065000
*/********************************************************************/ 00066000
*                                                                  0090 00067000
*       RESPECIFY                                                  0090 00068000
*         FRREPTR RESTRICTED;                                      0090 00069000
*       RESPECIFY                                                  0091 00070000
*         FRRSPTR RESTRICTED;                                      0091 00071000
*       FRRSPTR=ADDR(IEAVAFR7);     /* ENTRY POINT TO FRR            */ 00072000
         LA    FRRSPTR,IEAVAFR7                                    0092 00073000
*       GENERATE CODE NOFLOWS NODEFS NOSETS REFS(FRRSPTR,FRREPTR,  0093 00074000
*           FRRWKAP,PSACSTK,FRRSCURR,PSALSFCC,FRRSLAST,FRRSELEN,   0093 00075000
*           FRRSFRRA,FRRSPARM);                                    0093 00076000
       SETFRR A,FRRAD=(FRRSPTR),PARMAD=(FRRWKAP),                      X00077000
               WRKREGS=(FRRWKAP,FRREPTR)                                00078000
*       RESPECIFY                                                  0094 00079000
*         FRRSPTR UNRESTRICTED;                                    0094 00080000
*       RESPECIFY                                                  0095 00081000
*         FRREPTR UNRESTRICTED;                                    0095 00082000
*                                                                  0096 00083000
*/********************************************************************/ 00084000
*/*                                                                  */ 00085000
*/* THE FOLLOWING SEGMENT OF CODE WILL SEARCH THE TAXE               */ 00086000
*/* QUEUE LOOKING FOR TAXES THAT ARE FOR THE TERMINATING             */ 00087000
*/* TASK. IF A TAXE IS FOUND FOR THE TCB THE FOLLOWING               */ 00088000
*/* ACTIONS ARE TAKEN.                                               */ 00089000
*/*                                                                  */ 00090000
*/* IF THE TAXE IS AN ACTIVE RB THE RBFDYN FLAG WILL BE              */ 00091000
*/* SET SO THAT THE RB WILL BE FREED BY EXIT. ALL OTHER              */ 00092000
*/* PROCESSING THAT IS NECESSARY TO HOUSEKEEP THE TAXE               */ 00093000
*/* RESOURCE IS PERFORMED AT EXIT TIME BY IEAVAR06.                  */ 00094000
*/*                                                                  */ 00095000
*/* IF THE TAXE IS SCHEDULED BUT IS NOT AN ACTIVE RB                 */ 00096000
*/* THE TAXE IS DEQUEUED AND THE RBFDYN FLAG IS SET.                 */ 00097000
*/* THE TAXE IS DEQUEUED BECAUSE THE TAXE IS SCHEDULED               */ 00098000
*/* AS AN IQE THAT WILL BE DEQUEUED FROM THE IQE QUEUE               */ 00099000
*/* AND NEVER SCHEDULED. THE RBFDYN FLAG IS SET SO THAT              */ 00100000
*/* IF FOR ANY REASON THE TAXE(IQE) DOES BECOME AN ACTIVE            */ 00101000
*/* RB IT WILL BE FREED BY EXIT.                                     */ 00102000
*/*                                                                  */ 00103000
*/* IF THE TAXE IS DORMANT THE TAXE IS DEQUEUED AND THE              */ 00104000
*/* RBFDYN FLAG IS SET ONLY FOR CONVIENCE.                           */ 00105000
*/*                                                                  */ 00106000
*/* THE TAXES ARE NOT FREED BY THIS ROUTINE. THE TAXES               */ 00107000
*/* THAT ARE ACTIVE RB'S WILL BE FREED BY EXIT, THE IQES             */ 00108000
*/* AND DORMANT TAXES ARE FREED WHEN THE SUBPOOL STORAGE             */ 00109000
*/* FOR THIS TCB IS FREED. MOREOVER THE TAXES THAT ARE               */ 00110000
*/* ON THE IQE QUEUE CANNOT BE FREED BECAUSE THEY HAVE               */ 00111000
*/* NOT YET BEEN DEQUEUED FROM THE STAGE TWO EXIT                    */ 00112000
*/* EFFECTORS IQE QUEUE.                                             */ 00113000
*/*                                                                  */ 00114000
*/********************************************************************/ 00115000
*                                                                  0096 00116000
*       RESPECIFY                                                  0096 00117000
*         TAXEPTR RESTRICTED;                                      0096 00118000
*       RESPECIFY                                                  0097 00119000
*         PTAXEPTR RESTRICTED;                                     0097 00120000
*       RESPECIFY                                                  0098 00121000
*         NTAXEPTR RESTRICTED;                                     0098 00122000
*       TAXEPTR=RCTDTAXE;           /* INIT TAXE BASE REG            */ 00123000
         L     @12,ASXBPTR(,ASCBPTR)                               0099 00124000
         L     @12,ASXBRCTD(,@12)                                  0099 00125000
         L     TAXEPTR,RCTDTAXE(,@12)                              0099 00126000
*       PTAXEPTR=PTAXEPTR-PTAXEPTR; /* CLEAR PREV TAXE PTR           */ 00127000
         SLR   PTAXEPTR,PTAXEPTR                                   0100 00128000
*       NTAXEPTR=NTAXEPTR-NTAXEPTR; /* CLEAR NEXT TAXE PTR           */ 00129000
         SLR   NTAXEPTR,NTAXEPTR                                   0101 00130000
*       DO WHILE TAXEPTR^='00000000'X;/* SCAN ENTIRE TAXE QUEUE      */ 00131000
         B     @DE00102                                            0102 00132000
@DL00102 DS    0H                                                  0103 00133000
*         IF TAXETCB=RMPLTCBA THEN  /* IS TAXE FOR TERM TCB          */ 00134000
         L     @12,RMPLPTR(,PTRRMPL)                               0103 00135000
         CLC   TAXETCB(4,TAXEPTR),RMPLTCBA(@12)                    0103 00136000
         BNE   @RF00103                                            0103 00137000
*           DO;                     /* YES                           */ 00138000
*             RBFDYN='1'B;          /* SET FREE AT EXIT FLAG         */ 00139000
         OI    RBFDYN(RBSECPTR),B'00000010'                        0105 00140000
*             IF RBFACTV='0'B THEN  /* IS TAXE AN ACTIVE RB          */ 00141000
         TM    RBFACTV(RBSECPTR),B'01000000'                       0106 00142000
         BNZ   @RF00106                                            0106 00143000
*               DO;                 /* NO                            */ 00144000
*                 FRRTTBDQ='1'B;    /* SET FOOTPRINT FOR FRR         */ 00145000
         OI    FRRTTBDQ(FRRWKAP),B'10000000'                       0108 00146000
*                 FRRTAXE=TAXEPTR;  /* SAVE TAXE ADDR                */ 00147000
         ST    TAXEPTR,FRRTAXE(,FRRWKAP)                           0109 00148000
*                 IF PTAXEPTR='00000000'X THEN/* FIRST TAXE ON QUEUE */ 00149000
         LTR   PTAXEPTR,PTAXEPTR                                   0110 00150000
         BNZ   @RF00110                                            0110 00151000
*                   DO;             /* YES                           */ 00152000
*                     RCTDTAXE=TAXELNK;/* DEQUEUE FIRST TAXE         */ 00153000
         L     @12,ASXBPTR(,ASCBPTR)                               0112 00154000
         L     @12,ASXBRCTD(,@12)                                  0112 00155000
         L     @11,TAXELNK-1(,TAXEPTR)                             0112 00156000
         LA    @11,0(,@11)                                         0112 00157000
         ST    @11,RCTDTAXE(,@12)                                  0112 00158000
*                   END;                                           0113 00159000
*                 ELSE              /* NO                            */ 00160000
*                   DO;             /* NO                            */ 00161000
         B     @RC00110                                            0114 00162000
@RF00110 DS    0H                                                  0115 00163000
*                     NTAXEPTR=TAXELNK;/* POINTER TO NEXT TAXE       */ 00164000
         L     NTAXEPTR,TAXELNK-1(,TAXEPTR)                        0115 00165000
         LA    NTAXEPTR,0(,NTAXEPTR)                               0115 00166000
*                     TAXEPTR=PTAXEPTR;/* UPDATE BASE PTR            */ 00167000
         LR    TAXEPTR,PTAXEPTR                                    0116 00168000
*                     TAXELNK=NTAXEPTR;/* DEQUEUE THE TAXE           */ 00169000
         STCM  NTAXEPTR,7,TAXELNK(TAXEPTR)                         0117 00170000
*                     PTAXEPTR=TAXEPTR;/* SAVE PREVIOUS TAXE PTR     */ 00171000
         LR    PTAXEPTR,TAXEPTR                                    0118 00172000
*                   END;                                           0119 00173000
*                 FRRTTBDQ='0'B;    /* RESET FOOTPRINT               */ 00174000
@RC00110 NI    FRRTTBDQ(FRRWKAP),B'01111111'                       0120 00175000
*                 FRRTAXE='00000000'X;/* RESET TAXE PTR              */ 00176000
         SLR   @12,@12                                             0121 00177000
         ST    @12,FRRTAXE(,FRRWKAP)                               0121 00178000
*               END;                                               0122 00179000
*             ELSE                  /* YES                           */ 00180000
*               DO;                                                0123 00181000
         B     @RC00106                                            0123 00182000
@RF00106 DS    0H                                                  0124 00183000
*                 PTAXEPTR=TAXEPTR; /* UPDATE PREVIOUS PTR           */ 00184000
         LR    PTAXEPTR,TAXEPTR                                    0124 00185000
*               END;                                               0125 00186000
*           END;                                                   0126 00187000
*         ELSE                      /* NO                            */ 00188000
*           DO;                                                    0127 00189000
         B     @RC00103                                            0127 00190000
@RF00103 DS    0H                                                  0128 00191000
*             PTAXEPTR=TAXEPTR;     /* UPDATE PREVIOUS PTR           */ 00192000
         LR    PTAXEPTR,TAXEPTR                                    0128 00193000
*           END;                                                   0129 00194000
*         TAXEPTR=TAXELNK;          /* UPDATE TAXE BASE              */ 00195000
@RC00103 L     @12,TAXELNK-1(,TAXEPTR)                             0130 00196000
         LA    @12,0(,@12)                                         0130 00197000
         LR    TAXEPTR,@12                                         0130 00198000
*       END;                                                       0131 00199000
@DE00102 LTR   TAXEPTR,TAXEPTR                                     0131 00200000
         BNZ   @DL00102                                            0131 00201000
*       RESPECIFY                                                  0132 00202000
*         NTAXEPTR UNRESTRICTED;                                   0132 00203000
*                                                                  0133 00204000
*/********************************************************************/ 00205000
*/*                                                                  */ 00206000
*/* THE FOLLOWING SEGMENT OF CODE WILL CHECK TO SEE IF               */ 00207000
*/* THERE ARE ANY TAXES ON THE QUEUE. IF THERE ARE THE               */ 00208000
*/* ENTIRE TAXE QUEUE WILL BE SEARCHED LOOKING FOR THE               */ 00209000
*/* HIGHEST LEVEL TAXE THAT HAS BEEN SCHEDULED.                      */ 00210000
*/*                                                                  */ 00211000
*/********************************************************************/ 00212000
*                                                                  0133 00213000
*       TAXEPTR=RCTDTAXE;           /* INITIALIZE SEARCH ARG         */ 00214000
         L     @12,ASXBPTR(,ASCBPTR)                               0133 00215000
         L     @12,ASXBRCTD(,@12)                                  0133 00216000
         L     TAXEPTR,RCTDTAXE(,@12)                              0133 00217000
*       PTAXEPTR=PTAXEPTR-PTAXEPTR; /* CLEAR ACTIVE TAXE PTR         */ 00218000
         SLR   PTAXEPTR,PTAXEPTR                                   0134 00219000
*       DO WHILE TAXEPTR^='00000000'X;/* ANY MORE TAXES              */ 00220000
         B     @DE00135                                            0135 00221000
@DL00135 DS    0H                                                  0136 00222000
*         IF RBFACTV='1'B|TAXESCHD='1'B THEN/* IS THE TAXE ACT       */ 00223000
         TM    RBFACTV(RBSECPTR),B'01000000'                       0136 00224000
         BO    @RT00136                                            0136 00225000
         TM    TAXESCHD(TAXEPTR),B'00001000'                       0136 00226000
         BNO   @RF00136                                            0136 00227000
@RT00136 DS    0H                                                  0137 00228000
*           DO;                     /* YES                           */ 00229000
*             PTAXEPTR=TAXEPTR;     /* SAVE ACTIVE TAXE PTR          */ 00230000
         LR    PTAXEPTR,TAXEPTR                                    0138 00231000
*           END;                                                   0139 00232000
*         TAXEPTR=TAXELNK;          /* UPDATE SEARCH ARGUMENT        */ 00233000
@RF00136 L     @12,TAXELNK-1(,TAXEPTR)                             0140 00234000
         LA    @12,0(,@12)                                         0140 00235000
         LR    TAXEPTR,@12                                         0140 00236000
*       END;                                                       0141 00237000
@DE00135 SLR   @12,@12                                             0141 00238000
         CR    TAXEPTR,@12                                         0141 00239000
         BNE   @DL00135                                            0141 00240000
*                                                                  0142 00241000
*/********************************************************************/ 00242000
*/*                                                                  */ 00243000
*/* THE TAXES FOLLOWING THE HIGHEST ACTIVE TAXE MUST BE              */ 00244000
*/* MARKED AVAILABLE FOR USE AND THEY MUST BE REFLECTED              */ 00245000
*/* IN THE TSBSTAX COUNT. THIS SEGMENT OF CODE WILL                  */ 00246000
*/* SEARCH THE ENTIRE TAXE QUEUE STARTING WITH THE HIGHEST           */ 00247000
*/* SCHEDULED TAXE AND PERFORM THE OPERATION.                        */ 00248000
*/*                                                                  */ 00249000
*/********************************************************************/ 00250000
*                                                                  0142 00251000
*       RESPECIFY                                                  0142 00252000
*         STAXCNT RESTRICTED;                                      0142 00253000
*       STAXCNT=STAXCNT-STAXCNT;    /* CLEAR NEW STAX CNT            */ 00254000
         SLR   STAXCNT,STAXCNT                                     0143 00255000
*       IF RCTDTAXE^='00000000'X THEN/* ARE THERE ANY TAXES          */ 00256000
         L     @11,ASXBPTR(,ASCBPTR)                               0144 00257000
         L     @11,ASXBRCTD(,@11)                                  0144 00258000
         C     @12,RCTDTAXE(,@11)                                  0144 00259000
         BE    @RF00144                                            0144 00260000
*         DO;                                                      0145 00261000
*           IF PTAXEPTR^='00000000'X THEN/* IS THIS HEAD OF QUEUE    */ 00262000
         CR    PTAXEPTR,@12                                        0146 00263000
         BE    @RF00146                                            0146 00264000
*             DO;                   /* NO                            */ 00265000
*               TAXEPTR=PTAXEPTR;   /* ESTABLISH BASE FOR TAXE       */ 00266000
         LR    TAXEPTR,PTAXEPTR                                    0148 00267000
*               TAXEPTR=TAXELNK;    /* START WITH NEXT TAXE          */ 00268000
         L     @12,TAXELNK-1(,TAXEPTR)                             0149 00269000
         LA    @12,0(,@12)                                         0149 00270000
         LR    TAXEPTR,@12                                         0149 00271000
*             END;                                                 0150 00272000
*           ELSE                    /* YES                           */ 00273000
*             DO;                                                  0151 00274000
         B     @RC00146                                            0151 00275000
@RF00146 DS    0H                                                  0152 00276000
*               TAXEPTR=RCTDTAXE;   /* SET BASE TO LOWEST            */ 00277000
         L     @12,ASXBPTR(,ASCBPTR)                               0152 00278000
         L     @12,ASXBRCTD(,@12)                                  0152 00279000
         L     TAXEPTR,RCTDTAXE(,@12)                              0152 00280000
*             END;                                                 0153 00281000
*           DO WHILE TAXEPTR^='00000000'X;/* SEARCH THE REMAINDER OF    00282000
*                                      THE TAXE QUEUE                */ 00283000
@RC00146 B     @DE00154                                            0154 00284000
@DL00154 DS    0H                                                  0155 00285000
*             TAXEFREQ='0'B;        /* MARK TAXE AVAILABLE           */ 00286000
         NI    TAXEFREQ(TAXEPTR),B'11011111'                       0155 00287000
*             STAXCNT=STAXCNT+'00010000'X;/* INCREMENT COUNT BY ONE  */ 00288000
         AL    STAXCNT,@CF01363                                    0156 00289000
*             TAXEPTR=TAXELNK;      /* UPDATE SEARCH ARGU            */ 00290000
         L     @12,TAXELNK-1(,TAXEPTR)                             0157 00291000
         LA    @12,0(,@12)                                         0157 00292000
         LR    TAXEPTR,@12                                         0157 00293000
*           END;                                                   0158 00294000
@DE00154 LTR   TAXEPTR,TAXEPTR                                     0158 00295000
         BNZ   @DL00154                                            0158 00296000
*         END;                                                     0159 00297000
*       RESPECIFY                                                  0160 00298000
*         PTAXEPTR UNRESTRICTED;                                   0160 00299000
@RF00144 DS    0H                                                  0161 00300000
*       RESPECIFY                                                  0161 00301000
*         TAXEPTR UNRESTRICTED;                                    0161 00302000
*                                                                  0162 00303000
*/********************************************************************/ 00304000
*/*                                                                  */ 00305000
*/* UPDATE THE STAX COUNT IN THE TSB                                 */ 00306000
*/*                                                                  */ 00307000
*/* THE COUNT IS UPDATED USING COMPARE AND SWAP                      */ 00308000
*/*                                                                  */ 00309000
*/* THE COUNT WILL NOT BE ALLOWED TO BE GREATER THAN 255             */ 00310000
*/* EVEN IF MORE TAXES EXIST.                                        */ 00311000
*/*                                                                  */ 00312000
*/********************************************************************/ 00313000
*                                                                  0162 00314000
*       RESPECIFY                                                  0162 00315000
*         TSBPTR RESTRICTED;                                       0162 00316000
*       RESPECIFY                                                  0163 00317000
*         SWAPVAL RESTRICTED;                                      0163 00318000
*       RESPECIFY                                                  0164 00319000
*         NSWAPVAL RESTRICTED;                                     0164 00320000
*       TSBPTR=ASCBTSB;             /* SET BASE REG FOR TSB          */ 00321000
         L     TSBPTR,ASCBTSB(,ASCBPTR)                            0165 00322000
*       GENERATE CODE NODEFS NOSETS NOFLOWS REFS(TSBPTR);          0166 00323000
         USING  TSB,TSBPTR                                              00324000
*       IF STAXCNT>'00FF0000'X THEN /* IS STAX COUNT >255            */ 00325000
         L     @05,@CF01364                                        0167 00326000
         CR    STAXCNT,@05                                         0167 00327000
         BNH   @RF00167                                            0167 00328000
*         DO;                       /* YES                           */ 00329000
*           STAXCNT='00FF0000'X;    /* SET TO 255                    */ 00330000
         LR    STAXCNT,@05                                         0169 00331000
*         END;                                                     0170 00332000
*       SWAPVAL=TSBATTNC;           /* LOAD ORIG COUNT               */ 00333000
@RF00167 L     SWAPVAL,TSBATTNC                                    0171 00334000
*LOOP:                                                             0172 00335000
*       NSWAPVAL=SWAPVAL;           /* LOAD ORIG COUNT               */ 00336000
LOOP     LR    NSWAPVAL,SWAPVAL                                    0172 00337000
*       NSWAPVAL=NSWAPVAL&'FF00FFFF'X;/* CLEAR STAX CNT              */ 00338000
         N     NSWAPVAL,@CF01367                                   0173 00339000
*       NSWAPVAL=NSWAPVAL|STAXCNT;  /* INSERT NEW COUNT              */ 00340000
         OR    NSWAPVAL,STAXCNT                                    0174 00341000
*       CS(SWAPVAL,NSWAPVAL,TSBATTNC);/* UPDATE THE COUNT            */ 00342000
         CS    SWAPVAL,@12,TSBATTNC                                0175 00343000
*       BC(7,LOOP);                 /* DID THE UPDATE WORK CONTINUE     00344000
*                                      IF IT DID                     */ 00345000
         BC    7,LOOP                                              0176 00346000
*       GENERATE CODE NODEFS NOSETS NOFLOWS REFS(TSBPTR);          0177 00347000
         DROP  TSBPTR                                                   00348000
*       RESPECIFY                                                  0178 00349000
*         SWAPVAL UNRESTRICTED;                                    0178 00350000
*       RESPECIFY                                                  0179 00351000
*         NSWAPVAL UNRESTRICTED;                                   0179 00352000
*       RESPECIFY                                                  0180 00353000
*         STAXCNT UNRESTRICTED;                                    0180 00354000
*       RESPECIFY                                                  0181 00355000
*         TSBPTR UNRESTRICTED;                                     0181 00356000
*                                                                  0182 00357000
*/********************************************************************/ 00358000
*/*                                                                  */ 00359000
*/* THE FOLLOWING SEGMENT OF CODE WILL DELETE THE RECOVERY           */ 00360000
*/*                                                                  */ 00361000
*/********************************************************************/ 00362000
*                                                                  0182 00363000
*       RESPECIFY                                                  0182 00364000
*         FRRSPTR RESTRICTED;                                      0182 00365000
*       RESPECIFY                                                  0183 00366000
*         FRREPTR RESTRICTED;                                      0183 00367000
*       GENERATE CODE NOSETS NODEFS NOFLOWS REFS(FRRSPTR,FRREPTR,  0184 00368000
*           PSACSTK,FRRSCURR,FRRSEMP,FRRSELEN);                    0184 00369000
        SETFRR D,WRKREGS=(FRRSPTR,FRREPTR)                              00370000
*       RESPECIFY                                                  0185 00371000
*         FRRSPTR RESTRICTED;                                      0185 00372000
*       RESPECIFY                                                  0186 00373000
*         FRREPTR RESTRICTED;                                      0186 00374000
*                                                                  0187 00375000
*/********************************************************************/ 00376000
*/*                                                                  */ 00377000
*/* THIS ROUTINE WILL RELEASE THE LOCAL LOCK. THE RETURN             */ 00378000
*/* CODE FROM SETLOCK IS NOT CHECKED TO SEE IF THE                   */ 00379000
*/* RELEASE WAS SUCCESSFUL                                           */ 00380000
*/*                                                                  */ 00381000
*/********************************************************************/ 00382000
*                                                                  0187 00383000
*       RESPECIFY                                                  0187 00384000
*         SETLK11 RESTRICTED;                                      0187 00385000
*       RESPECIFY                                                  0188 00386000
*         SETLK12 RESTRICTED;                                      0188 00387000
*       RESPECIFY                                                  0189 00388000
*         SETLK13 RESTRICTED;                                      0189 00389000
*       RESPECIFY                                                  0190 00390000
*         SETLK14 RESTRICTED;                                      0190 00391000
*SETLKR1:                           /* RELEASE THE LOCAL LOCK        */ 00392000
*       GENERATE CODE NOFLOWS NOSETS NODEFS REFS(PSALITA);         0191 00393000
SETLKR1  DS    0H                                                  0191 00394000
       SETLOCK  RELEASE,TYPE=LOCAL,                                    X00395000
               RELATED=(TAXE-QUEUE,IEAVAR07(SETLKO1))                   00396000
*       RESPECIFY                                                  0192 00397000
*         SETLK11 UNRESTRICTED;                                    0192 00398000
*       RESPECIFY                                                  0193 00399000
*         SETLK12 UNRESTRICTED;                                    0193 00400000
*       RESPECIFY                                                  0194 00401000
*         SETLK13 UNRESTRICTED;                                    0194 00402000
*       RESPECIFY                                                  0195 00403000
*         SETLK14 UNRESTRICTED;                                    0195 00404000
*     END;                                                         0196 00405000
*                                                                  0197 00406000
*/********************************************************************/ 00407000
*/*                                                                  */ 00408000
*/* THIS CODE WILL RE-ESTABLISH THE SAVE AREA POINTER                */ 00409000
*/* AND WILL SET THE RETURN CODE IN REGISTER 15                      */ 00410000
*/*                                                                  */ 00411000
*/********************************************************************/ 00412000
*                                                                  0197 00413000
*   RESPECIFY                                                      0197 00414000
*     RTNREG13 RESTRICTED;                                         0197 00415000
@RF00079 DS    0H                                                  0198 00416000
*   RTNREG13=ENTREG13;              /* RELOAD SAVE AREA ADDR         */ 00417000
         LR    RTNREG13,ENTREG13                                   0198 00418000
*   RESPECIFY                                                      0199 00419000
*     ENTREG13 UNRESTRICTED;                                       0199 00420000
*   RETURN CODE(0);                 /* RETURN TO END OF TASK         */ 00421000
         SLR   @15,@15                                             0200 00422000
         L     @14,12(,@13)                                        0200 00423000
         LM    @00,@12,20(@13)                                     0200 00424000
         BR    @14                                                 0200 00425000
*                                                                  0201 00426000
*/********************************************************************/ 00427000
*/*                                                                  */ 00428000
*/* THIS ROUTINE WILL RECEIVE CONTROL IN THE EVENT OF AN ERROR IN    */ 00429000
*/* THE ATTENTION EXIT PURGE ROUTINE. CONTROL WILL BE RECEIVED       */ 00430000
*/* FROM RTM VIA A LOAD PSW. REGISTERS UPON ENTRY ARE AS FOLLOWS:    */ 00431000
*/*             REGISTER  0  UNPREDICTABLE                           */ 00432000
*/*                       1  ADDRESS OF SDWA                         */ 00433000
*/*                    2-13  UNPREDICTABLE                           */ 00434000
*/*                      14  RETURN ADDRESS                          */ 00435000
*/*                      15  ENTRY POINT ADDRESS                     */ 00436000
*/*                                                                  */ 00437000
*/********************************************************************/ 00438000
*                                                                  0201 00439000
*IEAVAFR7:                                                         0201 00440000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG,RETREG);         0201 00441000
@EL00001 DS    0H                                                  0201 00442000
@EF00001 DS    0H                                                  0201 00443000
@ER00001 LM    @14,@12,12(@13)                                     0201 00444000
         BR    @14                                                 0201 00445000
IEAVAFR7 DS    0H                                                  0202 00446000
*   GENERATE CODE NOSETS NODEFS NOFLOWS REFS(IEAVAR07);            0202 00447000
       USING  IEAVAFR7,8                                                00448000
       LR     8,15                                                      00449000
*   RESPECIFY                                                      0203 00450000
*     SDWAPTR RESTRICTED;                                          0203 00451000
*   RESPECIFY                                                      0204 00452000
*     RTNREG14 RESTRICTED;                                         0204 00453000
*   FRRWKAP=SDWAPARM;               /* INITIALIZE FRR WORK AREA BASE */ 00454000
         L     FRRWKAP,SDWAPARM(,SDWAPTR)                          0205 00455000
*   IF SDWAFMID='00000000'X THEN    /* ERROR IN THIS ADDRESS SPACE   */ 00456000
         CLC   SDWAFMID(2,SDWAPTR),@CB01356                        0206 00457000
         BNE   @RF00206                                            0206 00458000
*     DO;                           /* YES                           */ 00459000
*       ASCBPTR=PSAAOLD;            /* INITIALIZE ASCB BASE REGISTER */ 00460000
         L     ASCBPTR,PSAAOLD                                     0208 00461000
*       RCTDTAXE='00000000'X;       /* CLEAR TAXE QUEUE POINTER      */ 00462000
         L     @15,ASXBPTR(,ASCBPTR)                               0209 00463000
         L     @15,ASXBRCTD(,@15)                                  0209 00464000
         SLR   @09,@09                                             0209 00465000
