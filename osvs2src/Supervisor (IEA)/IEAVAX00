*           SDWAVRA(1:SDWAVRAL)=FRRWKAR1;/* MOVE FRR WORK AREA INTO     01217000
*                                      RECORDING AREA                */ 01218000
         BCTR  @12,0                                               0468 01219000
         EX    @12,@SM02540                                        0468 01220000
*           SDWAURAL=SDWAVRAL(2:2); /* MOVE LENGTH OF RECORD INTO  0469 01221000
*                                      SDWA                          */ 01222000
         MVC   SDWAURAL(1,SDWAPTR),SDWAVRAL+1(SDWAPTR)             0469 01223000
*         END;                                                     0470 01224000
*       ELSE                        /* AREA TO SMALL                 */ 01225000
*         DO;                                                      0471 01226000
