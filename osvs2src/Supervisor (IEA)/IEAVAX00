         TM    TAXEFREQ(TAXEPTR),B'00100000'                       0389 00985000
         BO    @RF00389                                            0389 00986000
@RT00389 DS    0H                                                  0390 00987000
*     DO;                                                          0390 00988000
*       TAXEPTR=NEWTAXE;            /* POINT TO NEW TAXE             */ 00989000
         L     TAXEPTR,NEWTAXE(,FRRWKAR)                           0391 00990000
*       TAXEFREQ=OFF;               /* MARK TAXE AVAILABLE           */ 00991000
*                                                                  0392 00992000
         NI    TAXEFREQ(TAXEPTR),B'11011111'                       0392 00993000
*       /*************************************************************/ 00994000
*       /*                                                           */ 00995000
*       /* COMPARE AND SWAP TSBSTAX + 1                              */ 00996000
*       /*                                                           */ 00997000
*       /*************************************************************/ 00998000
*                                                                  0393 00999000
*       RFY                                                        0393 01000000
*        (R13,                                                     0393 01001000
*         CSWORD1,                                                 0393 01002000
*         CSWORD2) RSTD;                                           0393 01003000
*       R13=ASCBTSB;                /* ESTABLISH ADDRESSIBILITY TO 0394 01004000
*                                      TSB                           */ 01005000
         L     R13,ASCBTSB(,ASCBPTR)                               0394 01006000
*       CSWORD1=CSWORD3;            /* LOAD TSBSTAX COUNT IN REG     */ 01007000
         L     CSWORD1,CSWORD3                                     0395 01008000
*LOOP2:                                                            0396 01009000
*       CSWORD2=CSWORD1;            /* LOAD TSBSTAX COUNT IN REG     */ 01010000
LOOP2    LR    CSWORD2,CSWORD1                                     0396 01011000
*       IF(CSWORD2&CSMASK)^=CSMASK THEN/* CHECK IF TSBSTAX COUNT CAN    01012000
*                                      BE INCRIMENTED                */ 01013000
         L     @12,@CF02411                                        0397 01014000
         LR    @10,CSWORD2                                         0397 01015000
         NR    @10,@12                                             0397 01016000
         CLR   @10,@12                                             0397 01017000
         BE    @RF00397                                            0397 01018000
*         CSWORD2=CSWORD2+ONE;      /* ADD ONE TO TSBSTAX COUNT      */ 01019000
         AL    CSWORD2,@CF02415                                    0398 01020000
*       CS(CSWORD1,CSWORD2,CSWORD3);/* CS TSBSTAX COUNT              */ 01021000
@RF00397 CS    CSWORD1,@09,CSWORD3                                 0399 01022000
*       BC(7,LOOP2);                /* CS FAILED TRY AGAIN           */ 01023000
         BC    7,LOOP2                                             0400 01024000
*       RFY                                                        0401 01025000
*        (R13,                                                     0401 01026000
*         CSWORD1,                                                 0401 01027000
*         CSWORD2) UNRSTD;                                         0401 01028000
*     END;                                                         0402 01029000
*   ELSE                            /* NEXT TAXE IS UNAVAILABLE      */ 01030000
*     DO;                                                          0403 01031000
         B     @RC00389                                            0403 01032000
@RF00389 DS    0H                                                  0404 01033000
*       TAXEPTR=NEWTAXE;            /* POINT TO NEW TAXE             */ 01034000
         L     TAXEPTR,NEWTAXE(,FRRWKAR)                           0404 01035000
*       TAXEFREQ=ON;                /* MARK TAXE UNAVAILABLE         */ 01036000
         OI    TAXEFREQ(TAXEPTR),B'00100000'                       0405 01037000
*     END;                                                         0406 01038000
*   FRRTTBQ=OFF;                    /* INDICATE QUEUING COMPLETE     */ 01039000
@RC00389 NI    FRRTTBQ(FRRWKAR),B'11111101'                        0407 01040000
*   END QUEUING;                                                   0408 01041000
@EL00005 DS    0H                                                  0408 01042000
@EF00005 DS    0H                                                  0408 01043000
@ER00005 BR    @14                                                 0408 01044000
*                                                                  0409 01045000
*/********************************************************************/ 01046000
*/*                                                                  */ 01047000
*/*                             FRR ROUTINE                          */ 01048000
*/*                                                                  */ 01049000
*/*         THIS ROUTINE WILL GET CONTROL IN THE EVENT OF ANY        */ 01050000
*/*         ERROR ENCOUNTERED DURING STAX SVC PROCESSING FROM        */ 01051000
*/*         RTM VIA A LOAD PSW. REGISTERS AT INPUT ARE AS FOLLOWS    */ 01052000
*/*             REGISTER 0 INVALID                                   */ 01053000
*/*             REGISTER 1 ADDRESS OF SDWA                           */ 01054000
*/*             REGISTERS 2 - 13 INVALID                             */ 01055000
*/*             REGISTER 14 RETURN ADDRESS                           */ 01056000
*/*             REGISTER 15 ENTRY POINT ADDRESS                      */ 01057000
*/*                                                                  */ 01058000
*/********************************************************************/ 01059000
*                                                                  0409 01060000
*STXFRR:                                                           0409 01061000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG,NOENTREG,RETREG);     0409 01062000
STXFRR   DS    0H                                                  0410 01063000
*   GENERATE CODE NOSETS NODEFS NOFLOWS NOREFS;                    0410 01064000
           USING  STXFRR,6             ESTABLISH ADDRESSIBILITY         01065000
*                                      FOR THIS ROUTINE                 01066000
           LR     6,15                 LOAD BASE REGISTER WITH THE      01067000
*                                      ENTRY POINT ADDRESS              01068000
*   FRRWKAR=SDWAPARM;                                              0411 01069000
         L     FRRWKAR,SDWAPARM(,SDWAPTR)                          0411 01070000
*   IF SDWAFMID=ZERO THEN           /* CHECK IF ERROR IN THIS ADDRESS   01071000
*                                      SPACE                         */ 01072000
         ICM   @12,3,SDWAFMID(SDWAPTR)                             0412 01073000
         BNZ   @RF00412                                            0412 01074000
*     DO;                                                          0413 01075000
*       IF FRRDYSST=ON THEN         /* CHECK IF DEFER YES PROCESSING    01076000
*                                      STARTED                       */ 01077000
         TM    FRRDYSST(FRRWKAR),B'10000000'                       0414 01078000
         BNO   @RF00414                                            0414 01079000
*         DO;                                                      0415 01080000
*           RBSECPTR=FRRRBPTR;      /* POINT TO USERS RB             */ 01081000
         L     RBSECPTR,FRRRBPTR(,FRRWKAR)                         0416 01082000
*           RBATTN=OFF;             /* TURN DEFER BIT OFF            */ 01083000
         NI    RBATTN(RBSECPTR),B'11011111'                        0417 01084000
*         END;                                                     0418 01085000
*       IF FRRDNST=ON THEN          /* WAS DEFER NO PROCESSING     0419 01086000
*                                      STARTED                       */ 01087000
@RF00414 TM    FRRDNST(FRRWKAR),B'01000000'                        0419 01088000
         BNO   @RF00419                                            0419 01089000
*         DO;                                                      0420 01090000
*           RBSECPTR=FRRRBPTR;      /* POINT TO USERS RB             */ 01091000
         L     RBSECPTR,FRRRBPTR(,FRRWKAR)                         0421 01092000
*           RBATTN=ON;              /* TURN DEFER BIT ON             */ 01093000
         OI    RBATTN(RBSECPTR),B'00100000'                        0422 01094000
*         END;                                                     0423 01095000
*       IF FRRDYNST=ON THEN         /* CHECK IF TAXE WAS TO BE FREED    01096000
*                                      BY EXIT                       */ 01097000
@RF00419 TM    FRRDYNST(FRRWKAR),B'00100000'                       0424 01098000
         BNO   @RF00424                                            0424 01099000
*         DO;                                                      0425 01100000
*           RBSECPTR=FONDTAXE;      /* POINT TO RB IN TAXE           */ 01101000
         L     RBSECPTR,FONDTAXE(,FRRWKAR)                         0426 01102000
*           RBFDYN=OFF;             /* RESET FREE TAXE FLAG          */ 01103000
         NI    RBFDYN(RBSECPTR),B'11111101'                        0427 01104000
*         END;                                                     0428 01105000
*       IF FRRTRESM=ON THEN         /* CHECK IF TAXE WAS MARKED FOR     01106000
*                                      CANCELLATION                  */ 01107000
@RF00424 TM    FRRTRESM(FRRWKAR),B'00010000'                       0429 01108000
         BNO   @RF00429                                            0429 01109000
*         DO;                                                      0430 01110000
*           TAXEPTR=FONDTAXE;       /* POINT TO AFFECTED TAXE        */ 01111000
         L     TAXEPTR,FONDTAXE(,FRRWKAR)                          0431 01112000
*           TAXERESM=OFF;           /* TURN OFF TAXE RESUME BIT      */ 01113000
         NI    TAXERESM(TAXEPTR),B'11101111'                       0432 01114000
*         END;                                                     0433 01115000
*       ASCBPTR=PSAAOLD;            /* ESTABLISH ADDRESSIBILITY TO 0434 01116000
*                                      CURRENT ASCB                  */ 01117000
@RF00429 L     ASCBPTR,PSAAOLD                                     0434 01118000
*       IF FRRTTBRP=ON|FRRTTBDQ=ON|FRRTTBQ=ON THEN/* CHECK IF TAXE 0435 01119000
*                                      WAS BEING QUEUED DEQUEUED OR     01120000
*                                      BEING REPLACED                */ 01121000
         TM    FRRTTBRP(FRRWKAR),B'00001110'                       0435 01122000
         BZ    @RF00435                                            0435 01123000
*         DO;                                                      0436 01124000
*           IF TAXEPREV=ZERO THEN   /* CHECK IF TAXE WAS AT TOP OF 0437 01125000
*                                      QUEUE                         */ 01126000
         ICM   @12,15,TAXEPREV(FRRWKAR)                            0437 01127000
         BNZ   @RF00437                                            0437 01128000
*             DO;                                                  0438 01129000
*               RCTDTAXE=NEXTTAXE;  /* INSURE TAXE IS DEQUEUED       */ 01130000
         L     @12,ASXBPTR(,ASCBPTR)                               0439 01131000
         L     @12,ASXBRCTD(,@12)                                  0439 01132000
         MVC   RCTDTAXE(4,@12),NEXTTAXE(FRRWKAR)                   0439 01133000
*             END;                                                 0440 01134000
*           ELSE                    /* TAXE NOT TOP OF QUEUE         */ 01135000
*             DO;                                                  0441 01136000
         B     @RC00437                                            0441 01137000
@RF00437 DS    0H                                                  0442 01138000
*               TAXEPTR=TAXEPREV;   /* SET BASE TO PREVIOUS TAXE     */ 01139000
         L     TAXEPTR,TAXEPREV(,FRRWKAR)                          0442 01140000
*               TAXELNK=NEXTTAXE;   /* INSURE TAXE IS DEQUEUED       */ 01141000
         MVC   TAXELNK(3,TAXEPTR),NEXTTAXE+1(FRRWKAR)              0443 01142000
*             END;                                                 0444 01143000
*         END;                                                     0445 01144000
*                                                                  0445 01145000
@RC00437 DS    0H                                                  0446 01146000
*       /*************************************************************/ 01147000
*       /*                                                           */ 01148000
*       /* THE FOLLOWING SEGMENT OF CODE WILL RESTABLISH THE STAX    */ 01149000
*       /* COUNT. THE FIRST AVAILABLE TAXE WILL BE FOUND AND THE     */ 01150000
*       /* SUBSEQUENT TAXES WILL BE COUNTED                          */ 01151000
*       /*                                                           */ 01152000
*       /*************************************************************/ 01153000
*                                                                  0446 01154000
*       TAXEPTR=RCTDTAXE;           /* SET TAXE BASE REGISTER        */ 01155000
@RF00435 L     @12,ASXBPTR(,ASCBPTR)                               0446 01156000
         L     @12,ASXBRCTD(,@12)                                  0446 01157000
         L     TAXEPTR,RCTDTAXE(,@12)                              0446 01158000
*       RFY                                                        0447 01159000
*        (R13,                                                     0447 01160000
*         CSWORD1,                                                 0447 01161000
*         CSWORD2) RSTD;                                           0447 01162000
*       R13=ASCBTSB;                /* ESTABLISH ADDRESSIBILITY TO 0448 01163000
*                                      TSB                           */ 01164000
         L     R13,ASCBTSB(,ASCBPTR)                               0448 01165000
*       CSWORD2=ZERO;               /* SET NEW COUNT TO ZERO         */ 01166000
         SLR   CSWORD2,CSWORD2                                     0449 01167000
*       DO WHILE TAXEPTR^=ZERO;     /* SEARCH ENTIRE TAXE QUEUE      */ 01168000
         B     @DE00450                                            0450 01169000
@DL00450 DS    0H                                                  0451 01170000
*         IF TAXEFREQ=OFF THEN      /* CHECK FOR AVAILABLE TAXES     */ 01171000
         TM    TAXEFREQ(TAXEPTR),B'00100000'                       0451 01172000
         BNZ   @RF00451                                            0451 01173000
*           DO;                                                    0452 01174000
*             CSWORD2=CSWORD2+ONE;  /* IF AVAILABLE ADD ONE TO COUNT */ 01175000
         AL    CSWORD2,@CF02415                                    0453 01176000
*           END;                                                   0454 01177000
*         TAXEPTR=TAXELNK;          /* POINT TO NEXT TAXE            */ 01178000
@RF00451 SLR   @12,@12                                             0455 01179000
         ICM   @12,7,TAXELNK(TAXEPTR)                              0455 01180000
         LR    TAXEPTR,@12                                         0455 01181000
*       END;                                                       0456 01182000
@DE00450 LTR   TAXEPTR,TAXEPTR                                     0456 01183000
