*       IF CSWORD2>CSMASK THEN      /* CHECK FOR COUNT GREATER THAN     01185000
*                                      255                           */ 01186000
         L     @12,@CF02411                                        0457 01187000
         CLR   CSWORD2,@12                                         0457 01188000
         BNH   @RF00457                                            0457 01189000
*         DO;                                                      0458 01190000
*           CSWORD2=CSMASK;         /* LOAD STAX COUNT OF 255        */ 01191000
         LR    CSWORD2,@12                                         0459 01192000
*         END;                                                     0460 01193000
*       CSWORD1=CSWORD3;            /* LOAD INITIAL VALUE            */ 01194000
@RF00457 L     CSWORD1,CSWORD3                                     0461 01195000
*LOOP3:                                                            0462 01196000
*       CSWORD2=(CSWORD1&CSMASKCL)|CSWORD2;/* AND IN NEW COUNT       */ 01197000
LOOP3    LR    @12,CSWORD1                                         0462 01198000
         N     @12,@CF02413                                        0462 01199000
         OR    CSWORD2,@12                                         0462 01200000
*       CS(CSWORD1,CSWORD2,CSWORD3);/* CS TSBSTAX COUNT              */ 01201000
