*       BC(7,LOOP3);                /* CS FAILED TRY AGAIN           */ 01203000
         BC    7,LOOP3                                             0464 01204000
*       RFY                                                        0465 01205000
*        (R13,                                                     0465 01206000
*         CSWORD1,                                                 0465 01207000
*         CSWORD2) UNRSTD;                                         0465 01208000
*       IF SDWAVRAL<LENGTH(FRRWKAR1) THEN/* CHECK IF VARIABLE AREA 0466 01209000
*                                      WILL HOLD THE ENTIRE FRR WORK    01210000
*                                      AREA                          */ 01211000
         SLR   @12,@12                                             0466 01212000
         ICM   @12,3,SDWAVRAL(SDWAPTR)                             0466 01213000
         CH    @12,@CH00166                                        0466 01214000
         BNL   @RF00466                                            0466 01215000
