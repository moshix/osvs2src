         ST    TAXEPTR,TIQEIRB(,TAXEPTR)                           0330 00779000
*   TAXEUSER=STXUSER;               /* STORE USER PARAMETER LIST   0331 00780000
*                                      ADDRESS IN TAXE               */ 00781000
         SLR   @12,@12                                             0331 00782000
         ICM   @12,7,STXUSER(STXPARM)                              0331 00783000
         ST    @12,TAXEUSER(,TAXEPTR)                              0331 00784000
*   TAXEEXIT=STXEXIT;               /* STORE USER EXIT ADDRESS IN  0332 00785000
*                                      TAXE                          */ 00786000
         MVC   TAXEEXIT(4,TAXEPTR),STXEXIT(STXPARM)                0332 00787000
*   RBSECPTR=RBLINKB;               /* POINT TO USERS RB             */ 00788000
         SLR   @12,@12                                             0333 00789000
         ICM   @12,7,RBLINKB(RBSECPTR)                             0333 00790000
         LR    RBSECPTR,@12                                        0333 00791000
*   IF UKEY^=('0000'B) THEN         /* CHECK FOR KEY OF ZERO         */ 00792000
         TM    UKEY(RBSECPTR),B'11110000'                          0334 00793000
         BZ    @RF00334                                            0334 00794000
*     TAXEFKEY=ON;                  /* SET TAXEFKEY SW TO ONE        */ 00795000
         OI    TAXEFKEY(TAXEPTR),B'10000000'                       0335 00796000
*   ELSE                            /* USER KEY ZERO                 */ 00797000
*     TAXEFKEY=OFF;                 /* SET TAXEFKEY SW TO ZERO       */ 00798000
         B     @RC00334                                            0336 00799000
@RF00334 NI    TAXEFKEY(TAXEPTR),B'01111111'                       0336 00800000
*   IF USTATE=ON THEN               /* CHECK FOR PROBLEM PROGRAM   0337 00801000
*                                      STATE                         */ 00802000
@RC00334 TM    USTATE(RBSECPTR),B'00000001'                        0337 00803000
         BNO   @RF00337                                            0337 00804000
*     TAXEFMOD=ON;                  /* SET TAXEFMOD SW TO ONE        */ 00805000
         OI    TAXEFMOD(TAXEPTR),B'01000000'                       0338 00806000
*   ELSE                            /* USER SUPV STATE               */ 00807000
*     TAXEFMOD=OFF;                 /* SET TAXEFMOD SW TO ZERO       */ 00808000
         B     @RC00337                                            0339 00809000
@RF00337 NI    TAXEFMOD(TAXEPTR),B'10111111'                       0339 00810000
*   R0=LENGTH(STXLIST);             /* GET LENGTH OF LIST    @ZA31889*/ 00811000
@RC00337 LA    R0,24                                               0340 00812000
*   FRRGTGM5=OFF;                   /* INDICATE LIST FREED   @ZA31889*/ 00813000
         NI    FRRGTGM5(FRRWKAR),B'11111110'                       0341 00814000
*   RFY                                                            0342 00815000
*    (R12,                                                         0342 00816000
*     RETADD) RSTD;                 /*                       @ZA31889*/ 00817000
*   R12=RETADD;                     /* SAVE RETURN ADDRESS   @ZA31889*/ 00818000
         LR    R12,RETADD                                          0343 00819000
*   GEN( FREEMAIN RU,SP=255,BRANCH=YES,LV=(0),A=(1))               0344 00820000
*   REFS(R3,RETADD);                /* FREE SYSTEM LIST      @ZA31889*/ 00821000
          FREEMAIN RU,SP=255,BRANCH=YES,LV=(0),A=(1)                    00822000
*   RETADD=R12;                     /* RESTORE RETURN ADDRESS@ZA31889*/ 00823000
         LR    RETADD,R12                                          0345 00824000
*   RFY                                                            0346 00825000
*    (R12,                                                         0346 00826000
*     RETADD) UNRSTD;               /*                       @ZA31889*/ 00827000
*   END INIT;                                                      0347 00828000
@EL00004 DS    0H                                                  0347 00829000
@EF00004 DS    0H                                                  0347 00830000
@ER00004 BR    @14                                                 0347 00831000
*                                                                  0348 00832000
*/********************************************************************/ 00833000
*/*                                                                  */ 00834000
*/*                   QUEUING PROCESSING                             */ 00835000
*/*                                                                  */ 00836000
*/*         THIS ROUTINE QUEUES THE NEW TAXE TO ITS PROPER           */ 00837000
*/*         PLACE ON THE TAXE QUEUE. THE TAXE SHOULD BE PLACED       */ 00838000
*/*         ON THE QUEUE AS INDICATED BY THE FOLLOWING               */ 00839000
*/*         TWO RULES                                                */ 00840000
*/*           1. AN ATTENTION EXIT ROUTINE FOR A TASK WILL ALWAYS    */ 00841000
*/*              OCCUPY A HIGHER LEVEL THAN THE ATTENTION EXIT       */ 00842000
*/*              ROUTINE OF ANY OF ITS SUBTASKS                      */ 00843000
*/*           2. THE ATTENTION EXIT ROUTINE IS PLACED AT THE LOWEST  */ 00844000
*/*              POSSIBLE ATTENTION LEVEL, WITHOUT VIOLATING RULE 1  */ 00845000
*/*         THESE TWO RULES ARE DESIGNED TO ELIMINATE INTERLOCKS     */ 00846000
*/*         WHEN A SUBSTASK IS SET NON-DISPATCHABLE ACCROSS AN       */ 00847000
*/*         ATTENTION EXIT AND ANOTHER ATTENTION IS FIELDED          */ 00848000
*/*                                                                  */ 00849000
*/*                                                                  */ 00850000
*/********************************************************************/ 00851000
*                                                                  0348 00852000
*QUEUING:                                                          0348 00853000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG,NOENTREG,RETREG);     0348 00854000
QUEUING  DS    0H                                                  0349 00855000
*   NEXTTAXE=RCTDTAXE;              /* INDICATE TAXE QUEUED AT HEAD     00856000
*                                      OF QUEUE                      */ 00857000
         L     @12,ASXBPTR(,ASCBPTR)                               0349 00858000
         L     @12,ASXBRCTD(,@12)                                  0349 00859000
         L     @12,RCTDTAXE(,@12)                                  0349 00860000
         ST    @12,NEXTTAXE(,FRRWKAR)                              0349 00861000
*   TAXEPREV=ZERO;                  /* INDICATE NO PREVIOUS TAXE     */ 00862000
         SLR   @10,@10                                             0350 00863000
         ST    @10,TAXEPREV(,FRRWKAR)                              0350 00864000
*   TAXEPTR=RCTDTAXE;                                              0351 00865000
         LR    TAXEPTR,@12                                         0351 00866000
*   IF TCBLTC^=ZERO THEN            /* CHECK IF THIS TCB HAS ANY SUB    00867000
*                                      TASKS                         */ 00868000
         C     @10,TCBLTC(,TCBPTR)                                 0352 00869000
         BE    @RF00352                                            0352 00870000
*     DO WHILE TAXEPTR^=ZERO;       /* SEARCH UNTIL END OF TAXE QUEUE   00871000
*                                      REACHED                       */ 00872000
         B     @DE00353                                            0353 00873000
@DL00353 DS    0H                                                  0354 00874000
*       TCBPTR=TCBLTC;                                             0354 00875000
         L     TCBPTR,TCBLTC(,TCBPTR)                              0354 00876000
*       DO WHILE TAXETCB^=TCBPTR;   /* SEARCH UNTIL A SUB TASK IS  0355 00877000
*                                      FOUND FOR THIS TAXE           */ 00878000
         B     @DE00355                                            0355 00879000
@DL00355 DS    0H                                                  0356 00880000
*         IF TCBLTC^=ZERO THEN      /* CHECK IF NO DAUGHTERS FOR THIS   00881000
*                                      TCB                           */ 00882000
         L     @12,TCBLTC(,TCBPTR)                                 0356 00883000
         LTR   @12,@12                                             0356 00884000
         BZ    @RF00356                                            0356 00885000
*           TCBPTR=TCBLTC;          /* POINT TO YOUNGEST DAUGHTER    */ 00886000
         LR    TCBPTR,@12                                          0357 00887000
*         ELSE                      /* CHECK FOR SISTERS             */ 00888000
*TCBLOOP:                                                          0358 00889000
*           IF TCBNTC^=ZERO THEN    /* CHECK IF ANY SISTERS          */ 00890000
         B     @RC00356                                            0358 00891000
@RF00356 DS    0H                                                  0358 00892000
TCBLOOP  L     @12,TCBNTC(,TCBPTR)                                 0358 00893000
         LTR   @12,@12                                             0358 00894000
         BZ    @RF00358                                            0358 00895000
*             TCBPTR=TCBNTC;        /* POINT TO NEXT SISTER          */ 00896000
         LR    TCBPTR,@12                                          0359 00897000
*           ELSE                    /* CHECK MOTHER                  */ 00898000
*             DO;                                                  0360 00899000
         B     @RC00358                                            0360 00900000
@RF00358 DS    0H                                                  0361 00901000
*               TCBPTR=TCBOTC;      /* POINT TO MOTHER TCB           */ 00902000
         L     TCBPTR,TCBOTC(,TCBPTR)                              0361 00903000
*               IF TCBPTR=PSATOLD THEN/* CHECK IF THIS IS THE TCB FOR   00904000
*                                      THIS TASK                     */ 00905000
         C     TCBPTR,PSATOLD                                      0362 00906000
         BNE   @RF00362                                            0362 00907000
*                 DO;                                              0363 00908000
*                   TAXEPTR=TAXELNK;/* POINT TO NEXT TAXE IN QUEUE   */ 00909000
         SLR   @12,@12                                             0364 00910000
         ICM   @12,7,TAXELNK(TAXEPTR)                              0364 00911000
         LR    TAXEPTR,@12                                         0364 00912000
*                   GOTO ENDTCB;    /* GO TO END OF TCB SEARCH       */ 00913000
         B     ENDTCB                                              0365 00914000
*                 END;                                             0366 00915000
*               ELSE                /* MOTHER NOT CURRENT            */ 00916000
*                 GOTO TCBLOOP;     /* GO CHECK FOR SISTER   @YM21940*/ 00917000
*             END;                                                 0368 00918000
*       END;                                                       0369 00919000
@RC00358 DS    0H                                                  0369 00920000
@RC00356 DS    0H                                                  0369 00921000
@DE00355 C     TCBPTR,TAXETCB(,TAXEPTR)                            0369 00922000
         BNE   @DL00355                                            0369 00923000
*       TAXEPREV=TAXEPTR;           /* STORE ADDRESS OF FIRST TAXE 0370 00924000
*                                      FOUND FOR THIS SUB TASK       */ 00925000
         ST    TAXEPTR,TAXEPREV(,FRRWKAR)                          0370 00926000
*       TAXEPTR=TAXELNK;            /* POINT TO NEXT TAXE            */ 00927000
         SLR   @12,@12                                             0371 00928000
         ICM   @12,7,TAXELNK(TAXEPTR)                              0371 00929000
         LR    TAXEPTR,@12                                         0371 00930000
*       TCBPTR=PSATOLD;             /* POINT TO TCB FOR THIS TASK    */ 00931000
         L     TCBPTR,PSATOLD                                      0372 00932000
*ENDTCB:                                                           0373 00933000
*     END;                          /* END OF TCB CHECK LOOP         */ 00934000
ENDTCB   DS    0H                                                  0373 00935000
@DE00353 LTR   TAXEPTR,TAXEPTR                                     0373 00936000
         BNZ   @DL00353                                            0373 00937000
*   FRRTTBQ=ON;                                                    0374 00938000
@RF00352 OI    FRRTTBQ(FRRWKAR),B'00000010'                        0374 00939000
*   IF TAXEPREV=ZERO THEN           /* CHECK IF NO TAXES FOUND FOR 0375 00940000
*                                      ANY SUB TASKS                 */ 00941000
         ICM   @12,15,TAXEPREV(FRRWKAR)                            0375 00942000
         BNZ   @RF00375                                            0375 00943000
*     DO;                           /* QUEUE TAXE AT HEAD OF TAXE  0376 00944000
*                                      QUEUE                         */ 00945000
*       TAXEPTR=NEWTAXE;            /* POINT TO NEW TAXE             */ 00946000
         L     TAXEPTR,NEWTAXE(,FRRWKAR)                           0377 00947000
*       TAXELNK=RCTDTAXE;           /* STORE RCTDTAXE IN TAXELNK     */ 00948000
         L     @12,ASXBPTR(,ASCBPTR)                               0378 00949000
         L     @12,ASXBRCTD(,@12)                                  0378 00950000
         MVC   TAXELNK(3,TAXEPTR),RCTDTAXE+1(@12)                  0378 00951000
*       RCTDTAXE=TAXEPTR;           /* STORE ADDRESS OF NEW TAXE IN     00952000
*                                      RCTD                          */ 00953000
         ST    TAXEPTR,RCTDTAXE(,@12)                              0379 00954000
*     END;                                                         0380 00955000
*   ELSE                            /* QUEUE TAXE AFTER LAST TAXE  0381 00956000
*                                      FOUND FOR ANY SUB TASKS       */ 00957000
*     DO;                                                          0381 00958000
         B     @RC00375                                            0381 00959000
@RF00375 DS    0H                                                  0382 00960000
*       TAXEPTR=TAXEPREV;           /* POINT TO LAST TAXE FOUND      */ 00961000
         L     TAXEPTR,TAXEPREV(,FRRWKAR)                          0382 00962000
*       NEXTTAXE=TAXELNK;           /* SAVE ADDRESS OF NEXT TAXE     */ 00963000
         SLR   @12,@12                                             0383 00964000
         ICM   @12,7,TAXELNK(TAXEPTR)                              0383 00965000
         ST    @12,NEXTTAXE(,FRRWKAR)                              0383 00966000
*       TAXELNK=NEWTAXE;            /* STORE ADDRESS OF NEW TAXE IN     00967000
*                                      PREVIOUS TAXE                 */ 00968000
         L     @10,NEWTAXE(,FRRWKAR)                               0384 00969000
         STCM  @10,7,TAXELNK(TAXEPTR)                              0384 00970000
*       TAXEPTR=TAXELNK;            /* POINT TO NEW TAXE             */ 00971000
         LR    TAXEPTR,@10                                         0385 00972000
*       TAXELNK=NEXTTAXE;           /* STORE ADDRESS OF NEXT TAXE IN    00973000
*                                      NEW TAXE                      */ 00974000
         STCM  @12,7,TAXELNK(TAXEPTR)                              0386 00975000
*     END;                                                         0387 00976000
*   TAXEPTR=TAXELNK;                /* POINT TO NEXT TAXE            */ 00977000
@RC00375 SLR   @12,@12                                             0388 00978000
         ICM   @12,7,TAXELNK(TAXEPTR)                              0388 00979000
         LR    TAXEPTR,@12                                         0388 00980000
*   IF TAXEPTR=ZERO|TAXEFREQ^=ON THEN/* CHECK IF NO TAXES ON QUEUE OR   00981000
*                                      NEXT TAXE AVAILABLE           */ 00982000
         LTR   TAXEPTR,TAXEPTR                                     0389 00983000
