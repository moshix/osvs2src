         TITLE 'IEAVCSEG: CREATE SEGMENT                               *00001000
                        '                                               00002000
IEAVCSEG CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'IEAVCSEG  76.087'                                 0001 00009000
IEAVCSE2 DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00078-IEAVCSE2)                                   00013000
         ENTRY IEAVCSE2                                                 00014000
IEAVCSGB DS    0H                                                  0001 00015000
         USING *,@15                                               0001 00016000
         B     @PROLOG                                             0001 00017000
         DC    AL2(@EP00087-IEAVCSGB)                                   00018000
         ENTRY IEAVCSGB                                                 00019000
         DROP  @15                                                      00020000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00021000
         BALR  @09,0                                               0001 00022000
@PSTART  DS    0H                                                  0001 00023000
         USING @PSTART,@09                                         0001 00024000
         AH    @15,4(,@15)                                         0001 00025000
         BR    @15                                                 0001 00026000
@EP00001 DS    0H                                                  0002 00027000
*                                                                  0078 00028000
*/********************************************************************/ 00029000
*/* THIS ENTRY POINT (IEAVCSEG) IS TO BE USED BY CALLERS OF CREATE   */ 00030000
*/* WHO ARE EXTERNAL TO REAL STORAGE MANAGEMENT. FIRST THE RSM       */ 00031000
*/* INTERNAL REGISTER CONVENTIONS ARE ESTABLISHED, AN THE RSM        */ 00032000
*/* FUNCTIONAL RECOVERY ROUTINE IS ESTABLISHED. NEXT THE RECOVERY    */ 00033000
*/* COMMUNICATIONS AREA IS INITIALIZED FOR THIS MODULE. THE ADDRESS  */ 00034000
*/* OF THE RCA CANNOT BE PLACED IN THE PVT BECAUSE CREATE DOES NOT   */ 00035000
*/* HOLD THE SALLOC LOCK.                                            */ 00036000
*/********************************************************************/ 00037000
*                                                                  0078 00038000
*IEAVCSE2:                                                         0078 00039000
*   ENTRY;                                                         0078 00040000
@EP00078 DS    0H                                                  0079 00041000
*   PVTPTR=CVTPVTP;                 /* GET ADDRESS OF PVT            */ 00042000
         L     @12,CVTPTR                                          0079 00043000
         L     PVTPTR,CVTPVTP(,@12)                                0079 00044000
*   RSMHDPTR=PSAAOLD->ASCBRSM;      /* GET RSMHEADER ADDRESS         */ 00045000
         L     @12,PSAAOLD                                         0080 00046000
         L     RSMHDPTR,ASCBRSM(,@12)                              0080 00047000
*   RESPECIFY                                                      0081 00048000
*    (R4,                                                          0081 00049000
*     R5) RESTRICTED;                                              0081 00050000
*                                                                  0081 00051000
*   /*****************************************************************/ 00052000
*   /*                                                               */ 00053000
*   /* SET THE FRR                                                   */ 00054000
*   /*                                                               */ 00055000
*   /*****************************************************************/ 00056000
*                                                                  0082 00057000
*GETFRR:                                                           0082 00058000
*   GENERATE NODEFS REFS(R3,R13,PVTPRCV,PSACSTK,FRRSCURR,FRRSLAST, 0082 00059000
*       FRRSELEN,FRRSENTR,FRRSFRRA,FRRSPARM) SETS(CHAINBK,R4,R5)   0082 00060000
*       NOFLOWS;                                                   0082 00061000
GETFRR   DS    0H                                                  0082 00062000
    SETFRR A,FRRAD=PVTPRCV(R3),PARMAD=CHAINBK(,R13),WRKREGS=(R4,R5),  xx00063000
               RELATED=(RSM,IEAVCSEG(FREEFRR))                          00064000
*   RESPECIFY                                                      0083 00065000
*    (R4,                                                          0083 00066000
*     R5) UNRESTRICTED;                                            0083 00067000
*   RCACSEG='1'B;                   /* SET CSEG ENTERED FLAG         */ 00068000
         L     @12,RCAPTR(,R13)                                    0084 00069000
         OI    RCACSEG(@12),B'01000000'                            0084 00070000
*   ENTCSEG='1'B;                   /* SET HOW ENTERED FLAG          */ 00071000
         OI    ENTCSEG(R13),B'00100000'                            0085 00072000
*   GO TO CKGMAREA;                 /* GO CHECK REQUEST              */ 00073000
*                                                                  0086 00074000
         B     CKGMAREA                                            0086 00075000
*   /*****************************************************************/ 00076000
*   /*                                                               */ 00077000
*   /* THIS ENTRY POINT (IEAVCSGB) IS TO BE USED BY CALLERS OF CREATE*/ 00078000
*   /* WHO ARE INTERNAL TO REAL STORAGE MANAGEMENT. THE RSM          */ 00079000
*   /* FUNCTIONAL RECOVERY ROUTINE HAS ALREADY BEEN ESTABLISHED, SO  */ 00080000
*   /* CREATE NEED ONLY SET ITS CSECT ID AND MODULE ENTERED BIT IN   */ 00081000
*   /* THE RECOVERY COMMUNICATIONS AREA.                             */ 00082000
*   /*                                                               */ 00083000
*   /*****************************************************************/ 00084000
*                                                                  0087 00085000
*IEAVCSGB:                                                         0087 00086000
*   ENTRY;                                                         0087 00087000
@EP00087 DS    0H                                                  0088 00088000
*   CHAINBK=PVTPRCA;                /* PUT ADDRESS OF RCA WHERE    0088 00089000
*                                      CREATE EXPECTS TO FIND IT     */ 00090000
         MVC   CHAINBK(4,R13),PVTPRCA(PVTPTR)                      0088 00091000
*   RCACSGB='1'B;                   /* SET CSGB ENTERED BIT          */ 00092000
         L     @12,RCAPTR(,R13)                                    0089 00093000
         OI    RCACSGB(@12),B'00100000'                            0089 00094000
*   ENTCSEG='0'B;                   /* SET HOW ENTERED FLAG          */ 00095000
         NI    ENTCSEG(R13),B'11011111'                            0090 00096000
*                                                                  0091 00097000
*/********************************************************************/ 00098000
*/* ALL PARAMATERS OF THE REQUEST ARE CHECKED FOR VALIDITY PRIOR TO  */ 00099000
*/* ALTERING ANY CONTROL BLOCKS. IF THE GETMAINED AREA PASSED AS AN  */ 00100000
*/* INPUT PARAMETER IS NOT ON A DOUBLE WORD BOUNDARY, OR IF THE      */ 00101000
*/* NUMBER OF SEGMENTS TO BE CREATED IS NULL, THEN RETURN IS MADE    */ 00102000
*/* TO THE CALLER WITH A RETURN CODE OF 4. IF A SEGMENT TO           */ 00103000
*/* BE CREATED HAS ALREADY BEEN CREATED (IS ALREADY VALID) A         */ 00104000
*/* RETURN CODE OF 8 IS RETURNED TO THE CALLER. IT IS THEN           */ 00105000
*/* DETERMINED IF SPCT PROCESSING IS REQUIRED. SPCT PROCESSING WILL  */ 00106000
*/* NOT BE DONE IF XPT PROCESSING IS NOT REQUESTED. SPCT PROCESSING  */ 00107000
*/* WILL BE DONE FOR PRIVATE AREA SEGMENTS OF SWAPPABLE ADDRESS      */ 00108000
*/* SPACES                                                           */ 00109000
*/********************************************************************/ 00110000
*                                                                  0091 00111000
*CKGMAREA:                                                         0091 00112000
*   RCACSECT=RCACREAT;              /* SET CSECT ID                  */ 00113000
CKGMAREA L     @12,RCAPTR(,R13)                                    0091 00114000
         MVI   RCACSECT(@12),X'0A'                                 0091 00115000
*   IF DWBBITS^='000'B THEN                                        0092 00116000
         TM    DWBBITS(R13),B'00000111'                            0092 00117000
         BNZ   @RT00092                                            0092 00118000
*     GO TO RETURN4;                /* GETMAINED AREA FOR PAGE TABLE    00119000
*                                      MUST BE ON A DOUBLE WORD    0093 00120000
*                                      BOUNDARY                      */ 00121000
*   IF NSEGS=0 THEN                 /* TEST IF NULL CREATE           */ 00122000
         CLI   NSEGS(R13),0                                        0094 00123000
         BE    @RT00094                                            0094 00124000
*     GO TO RETURN4;                /* GIVE RETURN CODE OF 4 AND   0095 00125000
*                                      PREPARE TO RETURN TO CALLER   */ 00126000
*   IF(FIRSTSEG+NSEGS)>256 THEN     /* IS LAST SEGMENT TO BE CREATED    00127000
*                                      BEYOND END OF SEG TABLE       */ 00128000
         SLR   @12,@12                                             0096 00129000
         IC    @12,FIRSTSEG(,R13)                                  0096 00130000
         SLR   @08,@08                                             0096 00131000
         IC    @08,NSEGS(,R13)                                     0096 00132000
         ALR   @08,@12                                             0096 00133000
         CH    @08,@CH01633                                        0096 00134000
         BH    @RT00096                                            0096 00135000
*     GO TO RETURN4;                /* GIVE RETURN CODE OF 4 AND   0097 00136000
*                                      PREAPRE TO RETURN TO CALLER   */ 00137000
*                                                                  0097 00138000
*   /*****************************************************************/ 00139000
*   /*                                                               */ 00140000
*   /* CHECK THE VALIDITY BIT OF EACH SEGMENT TABLE ENTRY OF EACH    */ 00141000
*   /* SEGMENT TO BE CREATED. IF A SEGMENT TO BE CREATED IS VALID, DO*/ 00142000
*   /* NOT CREATE ANY SEGMENTS BUT RETURN TO THE CALLER WITH A RETURN*/ 00143000
*   /* CODE OF 8 AND THE VALID SEGMENT SEGID IN REGISTER 0.          */ 00144000
*   /*                                                               */ 00145000
*   /*****************************************************************/ 00146000
*                                                                  0098 00147000
*   RESPECIFY                                                      0098 00148000
*    (STEPTR,                                                      0098 00149000
*     R6) RESTRICTED;                                              0098 00150000
*   SEGID=FIRSTSEG;                 /* SEGMENT ID OF SEGMENT BEING 0099 00151000
*                                      TESTED FOR VALIDITY           */ 00152000
         STC   @12,SEGID(,R13)                                     0099 00153000
*   R6=RSMVSTO+LENGTH(SGTSTE)*(FIRSTSEG+NSEGS-1);/* TO VALUE FOR DO     00154000
*                                      LOOP                          */ 00155000
         L     @10,RSMVSTO(,RSMHDPTR)                              0100 00156000
         BCTR  @08,0                                               0100 00157000
         SLA   @08,2                                               0100 00158000
         ALR   @08,@10                                             0100 00159000
         LR    R6,@08                                              0100 00160000
*   DO STEPTR=RSMVSTO+LENGTH(SGTSTE)*FIRSTSEG BY LENGTH(SGTSTE) TO R6;  00161000
         SLA   @12,2                                               0101 00162000
         ALR   @10,@12                                             0101 00163000
         LR    STEPTR,@10                                          0101 00164000
         B     @DE00101                                            0101 00165000
@DL00101 DS    0H                                                  0102 00166000
*     IF SGTPAM='0'B THEN           /* CHECK VALIDY BIT              */ 00167000
         TM    SGTPAM(STEPTR),B'00000001'                          0102 00168000
         BNZ   @RF00102                                            0102 00169000
*       DO;                         /* SEGMENT IS VALID              */ 00170000
*         INPUTR0=SEGID;            /* SEGID OF VALID SEGMENT        */ 00171000
         SLR   @12,@12                                             0104 00172000
         IC    @12,SEGID(,R13)                                     0104 00173000
         ST    @12,INPUTR0(,R13)                                   0104 00174000
*RTN8:                                                             0105 00175000
*         OUTPUT15=8;               /* SET A RETURN CODE OF 8        */ 00176000
RTN8     MVC   OUTPUT15(4,R13),@CF00035                            0105 00177000
*         GO TO FREEFRR;            /* DELETE FRR & RETURN TO CALLER */ 00178000
         B     FREEFRR                                             0106 00179000
*       END;                                                       0107 00180000
*     ELSE                          /* SEGMENT IS NOT VALID          */ 00181000
*       SEGID=SEGID+1;              /* INCREMENT SEGID IN STEP WITH     00182000
*                                      SEGMENT POINTER               */ 00183000
@RF00102 LA    @12,1                                               0108 00184000
         SLR   @10,@10                                             0108 00185000
         IC    @10,SEGID(,R13)                                     0108 00186000
         ALR   @12,@10                                             0108 00187000
         STC   @12,SEGID(,R13)                                     0108 00188000
*   END;                                                           0109 00189000
         AH    STEPTR,@CH00043                                     0109 00190000
@DE00101 CR    STEPTR,R6                                           0109 00191000
         BNH   @DL00101                                            0109 00192000
*   RESPECIFY                                                      0110 00193000
*    (STEPTR,                                                      0110 00194000
*     R6) UNRESTRICTED;                                            0110 00195000
*                                                                  0110 00196000
*   /*****************************************************************/ 00197000
*   /*                                                               */ 00198000
*   /* IF BYPASS XPT OPTION REQUESTED SPCT PROCESSING WILL NOT BE    */ 00199000
*   /* DONE. THEREFORE ONLY SGTE'S AND PAGE TABLES WILL BE CREATED.  */ 00200000
*   /*                                                               */ 00201000
*   /*****************************************************************/ 00202000
*                                                                  0111 00203000
*   DOSPCT='0'B;                    /* INITIALIZE FLAG INDICATING  0111 00204000
*                                      SPCT PROCESSING IS NOT TO BE     00205000
*                                      DONE                          */ 00206000
         NI    DOSPCT(R13),B'10111111'                             0111 00207000
*   IF PASSXPT='1'B THEN            /* TEST BY-PASS XPT OPTION       */ 00208000
         TM    PASSXPT(R13),B'01000000'                            0112 00209000
         BO    @RT00112                                            0112 00210000
*     GO TO INITPGT;                /* GO INITIALIZE PAGE TABLES, NO    00211000
*                                      SPCT PROCESSING IS TO BE DONE */ 00212000
*                                                                  0113 00213000
*   /*****************************************************************/ 00214000
*   /*                                                               */ 00215000
*   /* IF SPCT ADDRESS IS ZERO, THE ADDRESS SPACE IS NOT SWAPABLE AND*/ 00216000
*   /* SPCT PROCESSING CAN BE SKIPED. IF THE SPCT ADDRESS IS NOT ZERO*/ 00217000
*   /* AND THE SEGMENT IS NOT VALID IT IS ASSUMED TO BE A PRIVATE    */ 00218000
*   /* AREA SEGMENT FOR WHICH AN ENTRY IS TO BE MADE IN THE SPCT.    */ 00219000
*   /*                                                               */ 00220000
*   /*****************************************************************/ 00221000
*                                                                  0114 00222000
*CKRSMHD:                                                          0114 00223000
*   IF RSMSPCT=0 THEN               /* PREPARE TO INITIALIZE       0114 00224000
*                                      SGTE,PGT,ETC                  */ 00225000
CKRSMHD  L     @12,RSMSPCT(,RSMHDPTR)                              0114 00226000
         LTR   @12,@12                                             0114 00227000
         BZ    @RT00114                                            0114 00228000
*     GO TO INITPGT;                /* BRANCH TO INITIALIZATION    0115 00229000
*                                      PROCESS                       */ 00230000
*                                                                  0116 00231000
*/********************************************************************/ 00232000
*/* PRIVATE AREA SEGMENTS ARE TO BE CREATED FOR A SWAPABLE ADDRESS   */ 00233000
*/* SPACE. THE VSA OF EACH PAGE TABLE CREATED IS TO BE STORED ALONG  */ 00234000
*/* WITH THE SEGMENT ID IN THE SPCT.  FIRST IT IS DETERMINED IF      */ 00235000
*/* THERE IS ENOUGH SPACE IN THE SPCT FOR THESE ENTRIES.  IF NOT,    */ 00236000
*/* A LARGER SPCT IS ACQUIRED AND ALL THE OLD SPCT IS MOVED TO THE   */ 00237000
*/* NEW SPCT AND THE OLD SPCT IS FREEDED. TO GET A LARGER SPCT       */ 00238000
*/* GETMAIN MUST BE CALLED. TO CALL GETMAIN THE SALLOC LOCK MUST BE  */ 00239000
*/* HELD.                                                            */ 00240000
*/********************************************************************/ 00241000
*                                                                  0116 00242000
*   DOSPCT='1'B;                    /* SET FLAG TO INDICATE SPCT   0116 00243000
*                                      PROCESSING IS TO BE DONE      */ 00244000
         OI    DOSPCT(R13),B'01000000'                             0116 00245000
*   IF SPCTNSEG>=(SPCTSSEG+NSEGS) THEN/* IS THERE ENOUGH SPACE IN THE   00246000
*                                      SPCT FOR THE SEGMENT ENTRIES     00247000
*                                      TO BE CREATED                 */ 00248000
         SLR   @08,@08                                             0117 00249000
         IC    @08,SPCTSSEG(,@12)                                  0117 00250000
         SLR   @15,@15                                             0117 00251000
         IC    @15,NSEGS(,R13)                                     0117 00252000
         ALR   @08,@15                                             0117 00253000
         SLR   @15,@15                                             0117 00254000
         IC    @15,SPCTNSEG(,@12)                                  0117 00255000
         CR    @08,@15                                             0117 00256000
         BNH   @RT00117                                            0117 00257000
*     GO TO INITPGT;                /* SPACE OK                      */ 00258000
*                                                                  0118 00259000
*   /*****************************************************************/ 00260000
*   /*                                                               */ 00261000
*   /* REQUEST SALLOC LOCK SO YOU CAN CALL GETMAIN AT GLBRANCH ENTRY */ 00262000
*   /* POINT                                                         */ 00263000
*   /*                                                               */ 00264000
*   /*****************************************************************/ 00265000
*                                                                  0119 00266000
*   LOCKHELD='0'B;                  /* INITIALIZE LOCK HELD FLAG     */ 00267000
         NI    LOCKHELD(R13),B'01111111'                           0119 00268000
*   RESPECIFY                                                      0120 00269000
*    (R10,                                                         0120 00270000
*     R11,                                                         0120 00271000
*     R12,                                                         0120 00272000
*     R14) RESTRICTED;                                             0120 00273000
*   R10=R13;                        /* SAVE R13 VALUE OVER SETLOCK   */ 00274000
*                                                                  0121 00275000
         LR    R10,R13                                             0121 00276000
*   /*****************************************************************/ 00277000
*   /*                                                               */ 00278000
*   /* SETLOCK ROUTINE, OBTAIN SALLOC LOCK                           */ 00279000
*   /*                                                               */ 00280000
*   /*****************************************************************/ 00281000
*                                                                  0122 00282000
*GETLOCK:                                                          0122 00283000
*   GENERATE NODEFS REFS(PSALITA,FLC) SETS(R11,R12,R13,R14) NOFLOWS;    00284000
GETLOCK  DS    0H                                                  0122 00285000
    SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                           **00286000
               RELATED=(RES,IEAVCSEG(FREELOK1),IEAVCSEG(FREELOK2))      00287000
*   RESPECIFY                                                      0123 00288000
*    (R11,                                                         0123 00289000
*     R12,                                                         0123 00290000
*     R14) UNRESTRICTED;                                           0123 00291000
*   IF R13^=0 THEN                  /* IF RETURN CODE IS NOT ZERO  0124 00292000
*                                      ASSUME IT IS 4 AND LOCK IS  0124 00293000
*                                      HELD BY MY CPU AND WAS      0124 00294000
*                                      OBTAINED BY MY CALLER         */ 00295000
         LTR   R13,R13                                             0124 00296000
         BZ    @RF00124                                            0124 00297000
*     DO;                                                          0125 00298000
*       R13=R10;                    /* RESTORE R13                   */ 00299000
         LR    R13,R10                                             0126 00300000
*       GO TO CALCSIZE;             /* LOCK IS HELD BY MY CPU,     0127 00301000
*                                      THEREFORE GETMAIN CAN BE    0127 00302000
*                                      CALLED                        */ 00303000
         B     CALCSIZE                                            0127 00304000
*     END;                                                         0128 00305000
*   R13=R10;                        /* RESOTRE R13                   */ 00306000
@RF00124 LR    R13,R10                                             0129 00307000
*   RESPECIFY                                                      0130 00308000
*    (R10) UNRESTRICTED;                                           0130 00309000
*   RCASAL='1'B;                    /* SET SALLOC HELD IND IN RCA    */ 00310000
         L     @12,RCAPTR(,R13)                                    0131 00311000
         OI    RCASAL(@12),B'10000000'                             0131 00312000
*   LOCKHELD='1'B;                  /* SET LOCK HELD FLAG SO LOCK  0132 00313000
*                                      WILL BE FREEDED LATER         */ 00314000
         OI    LOCKHELD(R13),B'10000000'                           0132 00315000
*                                                                  0133 00316000
*/********************************************************************/ 00317000
*/* CALCULATE SIZE OF SPCT NEEDED TO HOLD ALL OF THE OLD SPCT PLUS   */ 00318000
*/* THE SEGMENT ENTRIES FOR THE SEGMENTS ABOUT TO BE CREATED.        */ 00319000
*/* THE SPCT IS EXPANDED IN INCREMENTS OF NUMEXTEN SEGMENT ENTRIES   */ 00320000
*/* CALCULATE THE NUMBER OF BYTES NEEDED AND CALL GETMAIN. IF        */ 00321000
*/* GETMAIN FAILS, NO SEGMENTS CAN BE CREATED. THE SALLOC LOCK       */ 00322000
*/* MUST BE FREEDED BY CREATE IF CREATE ACQUIRED IT.                 */ 00323000
*/********************************************************************/ 00324000
*                                                                  0133 00325000
*   RESPECIFY                                                      0133 00326000
*    (R0,                                                          0133 00327000
*     R1,                                                          0133 00328000
*     R4,                                                          0133 00329000
*     R5,                                                          0133 00330000
*     R6,                                                          0133 00331000
*     NEXPAND,                                                     0133 00332000
*     R14,                                                         0133 00333000
*     R15) RESTRICTED;                                             0133 00334000
*CALCSIZE:                                                         0134 00335000
*   NEXPAND=(NSEGS-1-(SPCTNSEG-SPCTSSEG))/NUMEXTEN+1;/* CALCULATE  0134 00336000
*                                      NUMBER OF FIXED SIZE ADDITIONS   00337000
*                                      NEEDED                        */ 00338000
CALCSIZE SLR   @10,@10                                             0134 00339000
         IC    @10,NSEGS(,R13)                                     0134 00340000
         BCTR  @10,0                                               0134 00341000
         L     @12,RSMSPCT(,RSMHDPTR)                              0134 00342000
         SLR   NEXPAND,NEXPAND                                     0134 00343000
         IC    NEXPAND,SPCTNSEG(,@12)                              0134 00344000
         SLR   @08,@08                                             0134 00345000
         IC    @08,SPCTSSEG(,@12)                                  0134 00346000
         SLR   NEXPAND,@08                                         0134 00347000
         SLR   @10,NEXPAND                                         0134 00348000
         SRDA  @10,32                                              0134 00349000
         D     @10,@CF00126                                        0134 00350000
         AH    @11,@CH00066                                        0134 00351000
         LR    NEXPAND,@11                                         0134 00352000
*   R0=SPCTSIZE+(NEXPAND*NUMEXTEN*LENGTH(SPCTSEGE));/* GET SIZE OF 0135 00353000
*                                      NEW SPCT IN BYTES FOR GETMAIN */ 00354000
         LR    R0,NEXPAND                                          0135 00355000
         MH    R0,@CH00122                                         0135 00356000
         AH    R0,SPCTSIZE(,@12)                                   0135 00357000
*   R1=0;                           /* ADDRESS OF GETMAINED AREA IS     00358000
*                                      RETURNED IN R1                */ 00359000
         SLR   R1,R1                                               0136 00360000
*   R5=R3;                          /* SAVE R3 OVER GETMAIN          */ 00361000
         LR    R5,R3                                               0137 00362000
*   R6=R0;                          /* SAVE R0 OVER GETMAIN          */ 00363000
*                                                                  0138 00364000
         LR    R6,R0                                               0138 00365000
*   /*****************************************************************/ 00366000
*   /*                                                               */ 00367000
*   /* GET SQA FOR A NEW SPCT. THE RECOVERY COMMUNICATIONS AREA      */ 00368000
*   /* ADDRESS MUST BE SAVED BY CREATE BECAUSE IT IS POSSIBLE GETMAIN*/ 00369000
*   /* MAY CALL LSQA ALLOCATION WHO WILL SETFRR AND WIPE OUT THE     */ 00370000
*   /* ADDRESS IN PVTPRCA.                                           */ 00371000
*   /*                                                               */ 00372000
*   /*****************************************************************/ 00373000
*                                                                  0139 00374000
*GETM:                                                             0139 00375000
*   GENERATE NODEFS REFS(CVTPTR,CVTSPSA,CVTMAP,CVTGLMN,WSAGGMFM,WSAG,R0 00376000
*       ) SETS(R1,R3,R4,R14,R15) NOFLOWS;                          0139 00377000
GETM     DS    0H                                                  0139 00378000
   GETMAIN RC,LV=(0),SP=245,BRANCH=(YES,GLOBAL),KEY=0                   00379000
*   R3=R5;                          /* RESTORE R3                    */ 00380000
         LR    R3,R5                                               0140 00381000
*   R0=R6;                          /* RESTORE R6                    */ 00382000
         LR    R0,R6                                               0141 00383000
*   RESPECIFY                                                      0142 00384000
*    (R0,                                                          0142 00385000
*     R4,                                                          0142 00386000
*     R5,                                                          0142 00387000
*     R6,                                                          0142 00388000
*     R14) UNRESTRICTED;                                           0142 00389000
*   IF R15=0                        /* WAS GETMAIN SUCCESSFUL        */ 00390000
*     THEN                          /* PREPARE TO MOVE OLD SPCT      */ 00391000
         LTR   R15,R15                                             0143 00392000
         BZ    @RT00143                                            0143 00393000
*     GO TO MOVESPCT;               /* GO MOVE SPCT                  */ 00394000
*                                                                  0144 00395000
*   /*****************************************************************/ 00396000
*   /*                                                               */ 00397000
*   /* RECORD ERROR THAT GETMAIN FAILED NO SEGMENTS WILL BE CREATED. */ 00398000
*   /* GIVE CALLER CODE OF 4.                                        */ 00399000
*   /*                                                               */ 00400000
*   /*****************************************************************/ 00401000
*                                                                  0145 00402000
*   RCARCRD='05'X;                  /* RECORD GETMAIN FAILURE        */ 00403000
         L     @14,RCAPTR(,R13)                                    0145 00404000
         MVI   RCARCRD(@14),X'05'                                  0145 00405000
*   GENERATE NODEFS REFS(R1) SETS(R1) NOFLOWS;                     0146 00406000
    ABEND X'C0D',,,SYSTEM                                               00407000
*   RESPECIFY                                                      0147 00408000
*    (R1,                                                          0147 00409000
*     R15) UNRESTRICTED;                                           0147 00410000
*                                                                  0147 00411000
*   /*****************************************************************/ 00412000
*   /*                                                               */ 00413000
*   /* GETMAIN WAS NOT SUCCESSFUL, CANNOT OBTAIN LAGER SPCT SO CANNOT*/ 00414000
