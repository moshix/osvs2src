         TITLE ' IEAVDSEG: DESTROY SEGMENT                             *00001000
                        '                                               00002000
*                                                                  0051 00003000
IEAVDSEG CSECT                                                          00004000
        USING *,R15                   ADDRESSIBILITY FOR BRANCH         00005000
        MODID BRANCH=NO                                                 00006000
        DROP  R15                                                       00007000
        ENTRY IEAVDSE2        @Z40WPYD                                  00008000
IEAVDSE2 DS 0H                               @Z40WPYD                   00009000
        STM   R0,R15,PVTWSA1(R3)      STORE CALLERS REGISTERS           00010000
        BALR  R12,R0                  ADDRESSIBILITY FOR MODULE         00011000
        DS    0H                                                        00012000
        USING *,R12                   SETTING UP BASE REGISTER          00013000
*   /*****************************************************************/ 00014000
*   /*                                                               */ 00015000
*   /* THE FOLLOWING SECTION ESTABLISHES THE RECOVERY INFORMATION IN */ 00016000
*   /* THE RCA FOR THIS MODULE                                       */ 00017000
*   /*                                                               */ 00018000
*   /*****************************************************************/ 00019000
*                                                                  0052 00020000
*   R10=PVTPRCA;                    /* GET RCA ADDRESS               */ 00021000
         L     R10,PVTPRCA(,PVTPTR)                                0052 00022000
*   RCACSECT=RCACDEST;              /* SET DSEG CSECT ID             */ 00023000
         MVI   RCACSECT(RCAPTR),X'0B'                              0053 00024000
*   RCADSEG='1'B;                   /* SET DSEG MODULE ENTERED BIT   */ 00025000
         OI    RCADSEG(RCAPTR),B'00010000'                         0054 00026000
*   SEG0='0'B;                                                     0055 00027000
         MVI   SEG0(PVTPTR),B'00000000'                            0055 00028000
*   SEGID2=SEGID;                                                  0056 00029000
*                                                                  0056 00030000
         SLR   @14,@14                                             0056 00031000
         IC    @14,SEGID(,PVTPTR)                                  0056 00032000
         STC   @14,SEGID2(,PVTPTR)                                 0056 00033000
*   /*****************************************************************/ 00034000
*   /*                                                               */ 00035000
*   /* IS THE SEGMENT TO BE DESTROYED IN THE PRIVATE AREA            */ 00036000
*   /*                                                               */ 00037000
*   /*****************************************************************/ 00038000
*                                                                  0057 00039000
*   RFY                                                            0057 00040000
*     STEPTR RSTD;                  /* RESTRICT POINTER TO SGTE      */ 00041000
*   IF SEGNUM>=PVTLPRIV&SEGNUM<PVTLCSA THEN/* TEST IF SEGID IS IN  0058 00042000
*                                      PRIVATE AREA.                 */ 00043000
         CLC   SEGNUM(2,PVTPTR),PVTLPRIV(PVTPTR)                   0058 00044000
         BL    @RF00058                                            0058 00045000
         CLC   SEGNUM(2,PVTPTR),PVTLCSA(PVTPTR)                    0058 00046000
         BNL   @RF00058                                            0058 00047000
*     DO;                           /* IF YES GET ADR OF SEG TABLE   */ 00048000
*       STEPTR=RSMVSTO+LENGTH(SGTSTE)*SEGID;/* ESTABLISH SEG TABLE 0060 00049000
*                                      ENTRY                         */ 00050000
         SLA   @14,2                                               0060 00051000
         AL    @14,RSMVSTO(,RSMHDPTR)                              0060 00052000
         LR    STEPTR,@14                                          0060 00053000
*       IF SGTPAM='1'B THEN         /* TEST SEG TABLE ENTRY        0061 00054000
*                                      VALIDITY--IF YES BRANCH TO SET   00055000
*                                      R1=0                          */ 00056000
         TM    SGTPAM(STEPTR),B'00000001'                          0061 00057000
         BNO   @RF00061                                            0061 00058000
*         DO;                                                      0062 00059000
*           RCARCRD='0D'X;          /* SEGMENT WAS ALREADY INVALID   */ 00060000
         MVI   RCARCRD(RCAPTR),X'0D'                               0063 00061000
*           GO TO REG1IS0;          /* GO RECORD ERROR AND EXIT      */ 00062000
         B     REG1IS0                                             0064 00063000
*         END;                                                     0065 00064000
*       ELSE                                                       0066 00065000
*         ;                         /* IF NO FALL THROUGH            */ 00066000
@RF00061 DS    0H                                                  0067 00067000
*     END;                                                         0067 00068000
*   ELSE                            /* SEGMENT IS IN COMMON AREA     */ 00069000
*     DO;                                                          0068 00070000
         B     @RC00058                                            0068 00071000
@RF00058 DS    0H                                                  0069 00072000
*       RCARCRD='0D'X;              /* RECORD ATTEMPT TO DESTROY   0069 00073000
*                                      COMMON AREA SEGMENT           */ 00074000
         MVI   RCARCRD(RCAPTR),X'0D'                               0069 00075000
*       GO TO REG1IS0;              /* RECORD ERROR AND EXIT         */ 00076000
         B     REG1IS0                                             0070 00077000
*     END;                                                         0071 00078000
*   RSAPGT=SGTPTO;                  /* PUT RSA OF PAGE TABLE IN R1   */ 00079000
@RC00058 SLR   RSAPGT,RSAPGT                                       0072 00080000
         ICM   RSAPGT,7,SGTPTO(STEPTR)                             0072 00081000
*/********************************************************************/ 00082000
*/* TRANSLATE THE REAL PAGE TABLE ADDRESS TO A VIRTUAL               */ 00083000
*/* ADDRESS.                                                         */ 00084000
*/*   INPUT TO THIS MODULE IS:                                       */ 00085000
*/*      R0:         -IGNORED                                        */ 00086000
*/*      R1:         -24 BIT REAL STORAGE ADDRESS TO BE TRANSLATED   */ 00087000
*/*      R2-13:      -IGNORED                                        */ 00088000
*/*      R14:        -RETURN ADDRESS                                 */ 00089000
*/*      R15:        -ENTRY POINT ADDRESS                            */ 00090000
*/*   OUTPUT FROM THIS MODULE IS:                                    */ 00091000
*/*      R0:  BYTES 0,1    -ZERO                                     */ 00092000
*/*           BYTES 2,3    -ASID MAPPED                              */ 00093000
*/*      R1:  BYTE 0       -ZERO                                     */ 00094000
*/*           BYTES 1-3    -VIRTUAL ADDRESS MAPPED IF R15=0          */ 00095000
*/*      R2-14:            -UNCHANGED                                */ 00096000
*/*      R15:              - 0 = SUCCESSFUL                          */ 00097000
*/*                        - 4 = FUNCTION NOT PERFORMED              */ 00098000
*/********************************************************************/ 00099000
*                                                                  0073 00100000
*   CALL TRV;                       /* CALL TRANS REAL TO VIRTUAL    */ 00101000
         L     @11,CVTPTR                                          0073 00102000
         L     @15,CVTPTRV(,@11)                                   0073 00103000
         BALR  @14,@15                                             0073 00104000
*   IF RTNCODE^=0 THEN              /* IF FCTN NOT PERFORMED RECORD     00105000
*                                      FAILURE                       */ 00106000
         LTR   RTNCODE,RTNCODE                                     0074 00107000
         BZ    @RF00074                                            0074 00108000
*     DO;                                                          0075 00109000
*       RCARCRD='01'X;              /* RECORD TRV FAILURE            */ 00110000
         MVI   RCARCRD(RCAPTR),X'01'                               0076 00111000
*       GO TO REG1IS0;              /* RECORD FAILURE AND EXIT       */ 00112000
         B     REG1IS0                                             0077 00113000
*     END;                                                         0078 00114000
*   ELSE                                                           0079 00115000
*     ;                             /* ELSE IF R15=0, CONTINUE       */ 00116000
@RF00074 DS    0H                                                  0080 00117000
*   SGTSTE=IVALSGT;                 /* ZERO SEG TABLE ENTRY IN 1 INST   00118000
*                                      AND SET INVALID BIT ON        */ 00119000
         MVC   SGTSTE(4,STEPTR),@CB01373                           0080 00120000
*/********************************************************************/ 00121000
*/*   PAGE INVALIDATE IS CALLED BY IEAVDSEG TO DO PTLB               */ 00122000
*/*   INPUT TO MODULE:                                               */ 00123000
*/*     R0:     -UNUSED                                              */ 00124000
*/*     R1:     -PGTE ADDRESS OR ZERO                                */ 00125000
*/*     R2:     -RSMHD ADDRESS                                       */ 00126000
*/*     R3:     -PVT ADDRESS                                         */ 00127000
*/*     R4-13:  -IGNORED                                             */ 00128000
*/*     R14:    -RETURN ADDRESS                                      */ 00129000
*/*     R15:    -ENTRY POINT ADDRESS                                 */ 00130000
*/*   OUTPUT FROM MODULE:                                            */ 00131000
*/*     R0-R15:  -RESTORED                                           */ 00132000
*/********************************************************************/ 00133000
*                                                                  0081 00134000
*   TEMPSTO=R1;                     /* TEMP.SAVE VSA OF PGT          */ 00135000
         ST    R1,TEMPSTO(,PVTPTR)                                 0081 00136000
*   R1=0;                           /* SET DUMMY PGTE ADR TO PASS    */ 00137000
         SLR   R1,R1                                               0082 00138000
*   CALL PGI;                       /* TO PAGE-INVALID. ROUTINE WILL    00139000
*                                      DO PTLB                       */ 00140000
         L     @15,PVTPINV(,PVTPTR)                                0083 00141000
         BALR  @14,@15                                             0083 00142000
*   IF XPTFLG='1'B THEN             /* IF FLAG ON, NO XPT EXISTS     */ 00143000
         TM    XPTFLG(PVTPTR),B'01000000'                          0084 00144000
         BO    @RT00084                                            0084 00145000
*     GOTO RETN;                    /* GO EXIT                       */ 00146000
*   IF RSMSPCT=0 THEN               /* TEST SWAP CONTROL TABLE ADR IF   00147000
*                                      =0 THEN PREPARE TO RETURN     */ 00148000
         L     @14,RSMSPCT(,RSMHDPTR)                              0086 00149000
         LTR   @14,@14                                             0086 00150000
         BZ    @RT00086                                            0086 00151000
*     GOTO RETN;                    /* GO RETURN                     */ 00152000
*   SPCTLMT=ADDR(SPCTSEGS)+LENGTH(SPCTSEGE)*(SPCTSSEG-1);/* ELSE   0088 00153000
*                                      ESTABLISH LIMIT OF SPCT IN DO    00154000
*                                      LOOP                          */ 00155000
         LA    @11,SPCTSEGS(,@14)                                  0088 00156000
         SLR   SPCTLMT,SPCTLMT                                     0088 00157000
         IC    SPCTLMT,SPCTSSEG(,@14)                              0088 00158000
         BCTR  SPCTLMT,0                                           0088 00159000
         MH    SPCTLMT,@CH00223                                    0088 00160000
         ALR   SPCTLMT,@11                                         0088 00161000
*   RFY                                                            0089 00162000
*    (STEPTR,                                                      0089 00163000
*     R13) UNRSTD;                  /* FREE R4 AND R13               */ 00164000
*                                                                  0090 00165000
*   /*****************************************************************/ 00166000
*   /*                                                               */ 00167000
*   /* SCAN SPCT FOR SEGMENT ENTRY FOR SEGMENT JUST DESTROYED        */ 00168000
*   /*                                                               */ 00169000
*   /*****************************************************************/ 00170000
*                                                                  0090 00171000
*   DO SEGSPTR=ADDR(SPCTSEGS) BY LENGTH(SPCTSEGE) TO SPCTLMT;/* SCAN    00172000
*                                      SWAP CONTROL TABLE            */ 00173000
         LR    SEGSPTR,@11                                         0090 00174000
         B     @DE00090                                            0090 00175000
@DL00090 DS    0H                                                  0091 00176000
*     IF SEGID=SPCTSEGX THEN        /* IF SEGID MATCH SPCT SEG INDX  */ 00177000
         CLC   SEGID(1,PVTPTR),SPCTSEGX(SEGSPTR)                   0091 00178000
         BNE   @RF00091                                            0091 00179000
*       DO;                         /* THEN 0 SPCT SEG ENTRY,MOVE    */ 00180000
*         SPCTSEGE=SPCTSEGE&&SPCTSEGE;/* LAST ENTRY TO ENTRY JUST 0     00181000
*                                      AND 0 LAST ENTRY SLOT         */ 00182000
         XC    SPCTSEGE(6,SEGSPTR),SPCTSEGE(SEGSPTR)               0093 00183000
*         SEGSPTR2=ADDR(SPCTSEGS)+SPCTSSEG*LENGTH(SPCTSEGE)-LENGTH(     00184000
*             SPCTSEGE);            /* SET UP PTR FOR LAST SPCT ENTRY*/ 00185000
         LA    @14,6                                               0094 00186000
         L     @11,RSMSPCT(,RSMHDPTR)                              0094 00187000
         LA    SEGSPTR2,SPCTSEGS(,@11)                             0094 00188000
         SLR   @04,@04                                             0094 00189000
         IC    @04,SPCTSSEG(,@11)                                  0094 00190000
         LR    @09,@04                                             0094 00191000
         MR    @08,@14                                             0094 00192000
         ALR   SEGSPTR2,@09                                        0094 00193000
         SLR   SEGSPTR2,@14                                        0094 00194000
*         SEGSPTR->SPCTSEGE=SEGSPTR2->SPCTSEGE;/* MOVE LAST ENTRY TO    00195000
*                                      ENTRY JUST ZEROED             */ 00196000
         MVC   SPCTSEGE(6,SEGSPTR),SPCTSEGE(SEGSPTR2)              0095 00197000
*         SEGSPTR2->SPCTSEGE=SEGSPTR2->SPCTSEGE&&SEGSPTR2->SPCTSEGE;    00198000
         XC    SPCTSEGE(6,SEGSPTR2),SPCTSEGE(SEGSPTR2)             0096 00199000
*                                   /* ZERO LAST ENTRY SLOT          */ 00200000
*         SPCTSSEG=SPCTSSEG-1;      /* DECRMENT NO. ACTIVE SEGS      */ 00201000
         BCTR  @04,0                                               0097 00202000
         STC   @04,SPCTSSEG(,@11)                                  0097 00203000
*         GO TO RETN;               /* BR DOWN-SUCCESSFUL DESTROY    */ 00204000
         B     RETN                                                0098 00205000
*       END;                                                       0099 00206000
*   END;                                                           0100 00207000
@RF00091 AH    SEGSPTR,@CH00223                                    0100 00208000
@DE00090 CR    SEGSPTR,SPCTLMT                                     0100 00209000
         BNH   @DL00090                                            0100 00210000
*   RCARCRD='18'X;                  /* SEG ENTRY NOT FOUND           */ 00211000
         MVI   RCARCRD(RCAPTR),X'18'                               0101 00212000
*   GO TO RECDERR;                  /* BRANCH DOWN                   */ 00213000
         B     RECDERR                                             0102 00214000
*REG1IS0:                                                          0103 00215000
*   TEMPSTO=0;                      /* NO VSA OF PAGE TABLE          */ 00216000
*                                                                  0103 00217000
REG1IS0  SLR   @14,@14                                             0103 00218000
         ST    @14,TEMPSTO(,PVTPTR)                                0103 00219000
*   /*****************************************************************/ 00220000
*   /*                                                               */ 00221000
*   /* RECORD ERROR                                                  */ 00222000
*   /*                                                               */ 00223000
*   /*****************************************************************/ 00224000
*                                                                  0104 00225000
*RECDERR:                                                          0104 00226000
*   GENERATE CODE;                                                 0104 00227000
*                                                                  0104 00228000
RECDERR  DS    0H                                                  0104 00229000
       ABEND X'C0D',,,SYSTEM          C0D RECORD INTERNAL ERROR         00230000
*   /*****************************************************************/ 00231000
*   /*                                                               */ 00232000
*   /* CLEAR MODULE ENTERED FLAG, LOAD PGT ADDRESS OR ZERO, RESTORE  */ 00233000
*   /* REGISTERS AND RETURN TO CALLER                                */ 00234000
*   /*                                                               */ 00235000
*   /*****************************************************************/ 00236000
*                                                                  0105 00237000
*RETN:                                                             0105 00238000
*   RCADSEG='0'B;                   /* RESET,DSEG MOD. LEAVING BIT   */ 00239000
RETN     NI    RCADSEG(RCAPTR),B'11101111'                         0105 00240000
*   R1=TEMPSTO;                     /* RESTORE R1 WITH 0 OR VSA      */ 00241000
         L     R1,TEMPSTO(,PVTPTR)                                 0106 00242000
*   GENERATE CODE NODEFS REFS(RSMHDADR,R3) NOSETS NOFLOWS;         0107 00243000
        LM R2,R15,RSMHDADR(R3)       RESTORE CALLERS R2-R15             00244000
        BR R14                       RETURN TO CALLER                   00245000
*   END IEAVDSEG                                                   0108 00246000
*                                                                  0108 00247000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 00248000
*/*%INCLUDE SYSLIB  (IHAPVT  )                                       */ 00249000
