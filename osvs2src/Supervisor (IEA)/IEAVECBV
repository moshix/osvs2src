*   /*                                                               */ 00435000
*   /* TCB VERIFICATION ROUTINE                                      */ 00436000
*   /*                                                               */ 00437000
*   /*****************************************************************/ 00438000
*                                                                  0159 00439000
*IEAVETCB:                                                         0159 00440000
*   ENTRY;                                                         0159 00441000
*                                                                  0159 00442000
@EP00159 DS    0H                                                  0160 00443000
*   /*****************************************************************/ 00444000
*   /*                                                               */ 00445000
*   /* SAVE RETURN ADDRESS AND INVOKE ADDRESS VERIFICATION ROUTINE TO*/ 00446000
*   /* DETERMINE WHETHER POTENTIAL TCB STORAGE IS REFERENCEABLE      */ 00447000
*   /*                                                               */ 00448000
*   /*****************************************************************/ 00449000
*                                                                  0160 00450000
*   RETSAVE4=RETREG14;              /* SAVE RETURN ADDRESS           */ 00451000
         LR    RETSAVE4,RETREG14                                   0160 00452000
*   TCBPTR=CBADDR0;                 /* ESTABLISH ADDRESSABILITY TO 0161 00453000
*                                      TCB AND PREPARE INTERFACE FOR    00454000
*                                      ADDRESS VERIFICATION ROUTINE  */ 00455000
         LR    TCBPTR,CBADDR0                                      0161 00456000
*   CBSIZER0=LENGTH(TCB);           /* OBTAIN LENGTH OF TCB          */ 00457000
         LA    CBSIZER0,296                                        0162 00458000
*   CALL IEAVEADV;                  /* ADDRESS VERIFICATION ROUTINE  */ 00459000
         L     @15,@CV01750                                        0163 00460000
         BALR  @14,@15                                             0163 00461000
*   IF RCREG15=RC0                  /* PASSED ALL TESTS?             */ 00462000
*       &TCBZERO=ZERO4BIT THEN      /* UNUSED PROTECTION KEY BITS  0164 00463000
*                                      ZERO?                         */ 00464000
*                                                                  0164 00465000
         LTR   RCREG15,RCREG15                                     0164 00466000
         BNZ   @RF00164                                            0164 00467000
         TM    TCBZERO(TCBPTR),B'00001111'                         0164 00468000
         BNZ   @RF00164                                            0164 00469000
*     /***************************************************************/ 00470000
*     /*                                                             */ 00471000
*     /* CHECK FOR BAD CONTROL BLOCK INFORMATION                     */ 00472000
*     /*                                                             */ 00473000
*     /***************************************************************/ 00474000
*                                                                  0165 00475000
*     DO;                           /* YES TO BOTH - PERFORM       0165 00476000
*                                      FOLLOWING                     */ 00477000
*       RCREG15=RC0;                /* SET RETURN CODE ACCUMULATOR TO   00478000
*                                      ZERO                          */ 00479000
         SLR   RCREG15,RCREG15                                     0166 00480000
*       IF TCBPTR^=ADDR(IEAWTCB) THEN/* NOT ADDRESS OF "WAIT TCB"?   */ 00481000
         L     @03,@CA01753                                        0167 00482000
         CR    TCBPTR,@03                                          0167 00483000
         BE    @RF00167                                            0167 00484000
*         DO;                       /* NOT "WAIT TCB" ADDRESS        */ 00485000
*           IF TCBTCBID^=NTCB THEN  /* TCB ACRONYM INVALID?          */ 00486000
         CLC   TCBTCBID(4,TCBPTR),NTCB                             0169 00487000
         BE    @RF00169                                            0169 00488000
*             RCREG15=RCREG15+RC4;  /* BUMP RETURN CODE ACCUMULATOR     00489000
*                                      BY 4                          */ 00490000
         LA    RCREG15,4(,RCREG15)                                 0170 00491000
*           IF TCBEXT2^=TCBPTR+LENGTH(TCB) THEN/* AOS COMMON       0171 00492000
*                                      EXTENSION ADDRESS INVALID?    */ 00493000
@RF00169 LR    @03,TCBPTR                                          0171 00494000
         AL    @03,@CF01795                                        0171 00495000
         C     @03,TCBEXT2(,TCBPTR)                                0171 00496000
         BE    @RF00171                                            0171 00497000
*             RCREG15=RCREG15+RC4;  /* BUMP RETURN CODE ACCUMULATOR     00498000
*                                      BY 4                          */ 00499000
         LA    RCREG15,4(,RCREG15)                                 0172 00500000
*           GENERATE CODE REFS(TCBRBP,RC4A) DEFS(LRAOK) SETS(RCREG15,   00501000
*               LRAREG14);                                         0173 00502000
@RF00171 DS    0H                                                  0173 00503000
         L     LRAREG14,TCBRBP(,TCBPTR) OBTAIN RB ADDRESS    @YA03540   00504000
         LRA   LRAREG14,0(,LRAREG14) ATTEMPT TO OBTAIN REAL ADDRESS     00505000
*                                OF RB                       @YA03540   00506000
         BZ   LRAOK              BRANCH IF REAL ADDRESS OF RB           00507000
*                                WAS OBTAINED                           00508000
         LA   RCREG15,RC4A(,RCREG15)     BUMP RETURN CODE ACCUMULATOR   00509000
*                                BY 4                                   00510000
LRAOK     EQU  *        LABEL BRANCHED TO WHEN READ                     00511000
*                                ADDRESS OF RB IS OBTAINABLE            00512000
*           IF RCREG15>RC8 THEN     /* HAS RETURN CODE ACCUMULATOR 0174 00513000
*                                      EXCEEDED 8                    */ 00514000
         LA    @03,8                                               0174 00515000
         CR    RCREG15,@03                                         0174 00516000
         BNH   @RF00174                                            0174 00517000
*             RCREG15=RC8;          /* SET RETURN CODE TO 8 - BLOCK     00518000
*                                      IS NOT A TCB                  */ 00519000
         LR    RCREG15,@03                                         0175 00520000
*         END;                                                     0176 00521000
*     END;                                                         0177 00522000
*   ELSE                            /* FAILED INITIAL TESTS          */ 00523000
*     RCREG15=RC8;                  /* WHEN POTENTIAL TCB STORAGE IS    00524000
*                                      NOT "REFERENCEABLE" AND/OR  0178 00525000
*                                      PROTECTION KEY ERROR EXISTS -    00526000
*                                      RETURN CODE SET INDICATING  0178 00527000
*                                      BLOCK IS NOT A TCB            */ 00528000
*                                                                  0178 00529000
         B     @RC00164                                            0178 00530000
@RF00164 LA    RCREG15,8                                           0178 00531000
*   /*****************************************************************/ 00532000
*   /*                                                               */ 00533000
*   /* RESET INTERFACE REGISTERS PRIOR TO EXITING - REGISTER 15      */ 00534000
*   /* CURRENTLY CONTAINS RETURN CODE FOR CALLER                     */ 00535000
*   /*                                                               */ 00536000
*   /*****************************************************************/ 00537000
*                                                                  0179 00538000
*ENDTCB:                            /* LABEL NOT REFERENCED - USED 0179 00539000
*                                      ONLY TO IDENTIFY EXITING CODE */ 00540000
*   CBADDR0=TCBPTR;                 /* RESET CONTROL BLOCK POINTER   */ 00541000
@RC00164 DS    0H                                                  0179 00542000
ENDTCB   LR    CBADDR0,TCBPTR                                      0179 00543000
*   RETREG14=RETSAVE4;              /* RESET RETURN ADDRESS          */ 00544000
         LR    RETREG14,RETSAVE4                                   0180 00545000
*   RETURN;                         /* RETURN TO CALLER              */ 00546000
         B     @EL00001                                            0181 00547000
*                                                                  0182 00548000
*/********************************************************************/ 00549000
*/*                                                                  */ 00550000
*/*      CHECK-ASCB SUBROUTINE                                       */ 00551000
*/*                                                                  */ 00552000
*/*      THIS SUBROUTINE IS INVOKED BY THE ASCB(GENERAL AND CURRENT) */ 00553000
*/*      AND SRB VERIFICATION ROUTINES. ITS FUNCTION IS TO DETERMINE */ 00554000
*/*      WHETHER THE INPUT ADDRESS IS A VALID ASCB                   */ 00555000
*/*                                                                  */ 00556000
*/*           REGISTER CONTENT AT ENTRY                              */ 00557000
*/*                REGISTER 0 - UNPREDICTABLE                        */ 00558000
*/*                         1 - ADDRESS OF SDWA                      */ 00559000
*/*                         2 - ADDRESS OF POTENTIAL ASCB            */ 00560000
*/*                         3 - UNPREDICTABLE                        */ 00561000
*/*                         4 - MAINLINE CALLER'S RETURN ADDRESS     */ 00562000
*/*                         5 - BASE                                 */ 00563000
*/*                         6 - UNPREDICTABLE                        */ 00564000
*/*                         7 - PERHAPS CONTROL BLOCK SAVE POINTER   */ 00565000
*/*                      8-13 - UNPREDICTABLE                        */ 00566000
*/*                        14 - RETURN ADDRESS                       */ 00567000
*/*                        15 - ENTRY POINT ADDRESS                  */ 00568000
*/*                                                                  */ 00569000
*/*           REGISTER USAGE (REGISTERS CHANGED PRIOR TO EXITING)    */ 00570000
*/*                REGISTER 0 - ASCB LENGTH                          */ 00571000
*/*                         3 - WORK REGISTER                        */ 00572000
*/*                         6 - SAVES RETURN ADDRESS                 */ 00573000
*/*                        14 - MODIFIED BY ADDRESS VERIFICATION     */ 00574000
*/*                             AND COMPILER WORK REGISTER AND       */ 00575000
*/*                             THEN RESET PRIOR TO EXITING          */ 00576000
*/*                        15 - RETURN CODES                         */ 00577000
*/*                               0 - PASSED ALL TESTS               */ 00578000
*/*                               8 - INPUT ADDR NOT AN ASCB         */ 00579000
*/*                                                                  */ 00580000
*/********************************************************************/ 00581000
*                                                                  0182 00582000
*CKASCB48:                                                         0182 00583000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,ENTREG,RETREG,NOPARMREG);       0182 00584000
*                                                                  0182 00585000
CKASCB48 DS    0H                                                  0183 00586000
*   /*****************************************************************/ 00587000
*   /*                                                               */ 00588000
*   /* SAVE RETURN ADDRESS AND INVOKE ADDRESS VERIFICATION           */ 00589000
*   /*                                                               */ 00590000
*   /*****************************************************************/ 00591000
*                                                                  0183 00592000
*   RFY                                                            0183 00593000
*     CBSAVER7 RSTD;                                               0183 00594000
*   RFY                                                            0184 00595000
*     RETPTR6 RSTD;                                                0184 00596000
*   RETPTR6=RETREG14;               /* SAVE RETURN ADDRESS           */ 00597000
         LR    RETPTR6,RETREG14                                    0185 00598000
*   RFY                                                            0186 00599000
*     RETREG14 UNRSTD;              /* COMPILER NEEDS WORK REGISTER  */ 00600000
*   CBSIZER0=LENGTH(ASCB);          /* OBTAIN ASCB LENGTH            */ 00601000
         LA    CBSIZER0,208                                        0187 00602000
*   CALL IEAVEADV;                  /* ADDRESS VERIFICATION          */ 00603000
         L     @15,@CV01750                                        0188 00604000
         BALR  @14,@15                                             0188 00605000
*   RFY                                                            0189 00606000
*     ASVTPTR RSTD;                                                0189 00607000
*   CVTPTR3=CVTPTR;                 /* OBTAIN ADDRESS OF CVT         */ 00608000
         L     CVTPTR3,CVTPTR                                      0190 00609000
*   ASVTPTR=CVTPTR3->CVTASVT;       /* OBTAIN ADDRESS OF ASVT        */ 00610000
         L     ASVTPTR,CVTASVT(,CVTPTR3)                           0191 00611000
*   IF RCREG15=RC0                  /* PASSED TESTS?                 */ 00612000
*       &ASCBASID<=ASVTMAXU THEN    /* POTENTIAL ASID LESS THAN    0192 00613000
*                                      MAXIMUM POSSIBLE?             */ 00614000
         LTR   RCREG15,RCREG15                                     0192 00615000
         BNZ   @RF00192                                            0192 00616000
         LH    @14,ASCBASID(,ASCBPTR)                              0192 00617000
         N     @14,@CF01796                                        0192 00618000
         C     @14,ASVTMAXU(,ASVTPTR)                              0192 00619000
         BH    @RF00192                                            0192 00620000
*     IF ASCBASID^=ZEROASID THEN    /* NOT WAIT ASCB?                */ 00621000
*                                                                  0193 00622000
         CLC   ASCBASID(2,ASCBPTR),ZEROASID                        0193 00623000
         BE    @RF00193                                            0193 00624000
*       /*************************************************************/ 00625000
*       /*                                                           */ 00626000
*       /* VERIFY USING ASVT                                         */ 00627000
*       /*                                                           */ 00628000
*       /*************************************************************/ 00629000
*                                                                  0194 00630000
*       DO;                                                        0194 00631000
*         IF ASVTAVAL(ASCBASID)='0'B/* ASCB ASSIGNED?                */ 00632000
*             &ASCBPTR=ASVTENTY(ASCBASID) THEN/* POTENTIAL AND ACTUAL   00633000
*                                      ASCB ADDRESSES MATCH?         */ 00634000
         SLA   @14,2                                               0195 00635000
         ALR   @14,ASVTPTR                                         0195 00636000
         TM    ASVTAVAL-4(@14),B'10000000'                         0195 00637000
         BNZ   @RF00195                                            0195 00638000
         LH    @14,ASCBASID(,ASCBPTR)                              0195 00639000
         N     @14,@CF01796                                        0195 00640000
         SLA   @14,2                                               0195 00641000
         C     ASCBPTR,ASVTENTY-4(@14,ASVTPTR)                     0195 00642000
         BNE   @RF00195                                            0195 00643000
*           RCREG15=RC0;            /* INDICATE THAT ALL TESTS WERE     00644000
*                                      PASSED                        */ 00645000
         SLR   RCREG15,RCREG15                                     0196 00646000
*         ELSE                      /* NOT FOUND IN ASVT             */ 00647000
*           RCREG15=RC8;            /* INDICATE INPUT ADDRESS IS NOT    00648000
*                                      AN ELEMENT                    */ 00649000
         B     @RC00195                                            0197 00650000
@RF00195 LA    RCREG15,8                                           0197 00651000
*       END;                                                       0198 00652000
*                                                                  0198 00653000
*   /*****************************************************************/ 00654000
*   /*                                                               */ 00655000
*   /* DETERMINE IF WAIT ASCB                                        */ 00656000
*   /*                                                               */ 00657000
*   /*****************************************************************/ 00658000
*                                                                  0199 00659000
*     ELSE                          /* POTENTIAL ASID WAS ZERO       */ 00660000
*       IF ASCBPTR=ADDR(IEAWASCB) THEN/* INPUT ASCB ADDRESS EQUALS 0199 00661000
*                                      WAIT ASCB?                    */ 00662000
         B     @RC00193                                            0199 00663000
@RF00193 L     @14,@CA01752                                        0199 00664000
         CR    ASCBPTR,@14                                         0199 00665000
         BNE   @RF00199                                            0199 00666000
*         RCREG15=RC0;              /* YES, INDICATE THAT ALL TESTS     00667000
*                                      WERE PASSED                   */ 00668000
         SLR   RCREG15,RCREG15                                     0200 00669000
*       ELSE                        /* NOT WAIT ASCB                 */ 00670000
*         RCREG15=RC8;              /* INDICATE INPUT ADDRESS IS NOT    00671000
*                                      AN ELEMENT                    */ 00672000
         B     @RC00199                                            0201 00673000
@RF00199 LA    RCREG15,8                                           0201 00674000
*   ELSE                            /* NON-ZERO FROM AV ROUTINE OR 0202 00675000
*                                      ASID GREATER THAN MAXIMUM   0202 00676000
*                                      POSSIBLE                      */ 00677000
*     RCREG15=RC8;                  /* INDICATE INPUT ADDRESS IS NOT    00678000
*                                      AN ELEMENT                    */ 00679000
         B     @RC00192                                            0202 00680000
@RF00192 LA    RCREG15,8                                           0202 00681000
*                                                                  0203 00682000
*   /*****************************************************************/ 00683000
*   /*                                                               */ 00684000
*   /* PREPARE TO RETURN TO CALLER                                   */ 00685000
*   /*                                                               */ 00686000
*   /*****************************************************************/ 00687000
*                                                                  0203 00688000
*   RFY                                                            0203 00689000
*     ASVTPTR UNRSTD;                                              0203 00690000
@RC00192 DS    0H                                                  0204 00691000
*   RFY                                                            0204 00692000
*     RETREG14 RSTD;                                               0204 00693000
*   RETREG14=RETPTR6;               /* RESET RETURN ADDRESS          */ 00694000
         LR    RETREG14,RETPTR6                                    0205 00695000
*   RFY                                                            0206 00696000
*    (RETPTR6,                                                     0206 00697000
*     RETREG14) UNRSTD;                                            0206 00698000
*   RFY                                                            0207 00699000
*     CBSAVER7 UNRSTD;                                             0207 00700000
*   END CKASCB48;                   /* RETURN                        */ 00701000
@EL00002 DS    0H                                                  0208 00702000
@EF00002 DS    0H                                                  0208 00703000
@ER00002 BR    @14                                                 0208 00704000
*   END IEAVECBV                                                   0209 00705000
*                                                                  0209 00706000
