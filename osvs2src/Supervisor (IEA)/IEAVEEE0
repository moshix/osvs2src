         TITLE '    IEAVEEE0 - STAGE 3 EXIT EFFECTOR                   *00001000
                        '                                               00002000
IEA0EF03 CSECT ,                                                01S0001 00003000
@PROLOG  BALR  @12,0                                            01S0001 00004000
@PSTART  DS    0H                                               01S0001 00005000
         USING @PSTART,@12                                      01S0001 00006000
RBPRFXLN EQU 64                                                         00007000
RBPRFLNA EQU 32                                                         00008000
PRBLEN   EQU 136                                                        00009000
SIRBLEN  EQU 200                                                        00010000
TIRBLEN  EQU 136                                                        00011000
IRBLEN   EQU 128                                                        00012000
SVRBLEN  EQU 224                                                        00013000
*/*                    END OF CVT MACRO                              */ 00014000
*                                                               03S0075 00015000
*                                                               01S0075 00016000
*/*   MAINLINE CODE FOR STAGE 3 EXIT EFFECTOR                        */ 00017000
*                                                               01S0075 00018000
*                                                               02S0075 00019000
*/********************************************************************/ 00020000
*/*  STAGE3-                                                         */ 00021000
*/*      THIS IS THE MAINLINE CODE FOR THE STAGE 3 EXIT EFFECTOR     */ 00022000
*/*      INPUT:                                                      */ 00023000
*/*        SEE PROLOGUE                                              */ 00024000
*/********************************************************************/ 00025000
*/********************************************************************/ 00026000
*/*  INITIALIZATION                                                  */ 00027000
*/********************************************************************/ 00028000
*                                                               02S0075 00029000
*                                                               03S0075 00030000
*/********************************************************************/ 00031000
*/*  INIT3 -                                                         */ 00032000
*/*     THIS ROUTINE PERFORMS SPECIAL INITIALIZATION .               */ 00033000
*/*      INPUT:                                                      */ 00034000
*/*        ASCBPTR  (R8) - CONTAINS ADDR OF THE CURRENT ASCB.        */ 00035000
*/*        R14 - RETURN ADDR TO THE DISPATCHER.                      */ 00036000
*/********************************************************************/ 00037000
*                                                               03S0075 00038000
*   ASXBPTR=ASCBASXB;               /* POINT TO ASXB                 */ 00039000
         L     ASXBPTR,ASCBASXB(,ASCBPTR)                       03S0075 00040000
*   SAVE1=R14;                      /* SAVE RETURN ADDRESS           */ 00041000
         L     @15,ASXBSPSA(,ASXBPTR)                           03S0076 00042000
         L     @15,WSALS2EE(,@15)                               03S0076 00043000
         STCM  R14,15,SAVE1(@15)                                03S0076 00044000
*/********************************************************************/ 00045000
*/*  END OF INIT3 -                                                  */ 00046000
*/*      OUTPUT:                                                     */ 00047000
*/*        ASXBPTR (R6) - CONTAINS ADDR OF THE CURRENT ASXB.         */ 00048000
*/*        SAVE1  - CONTAINS SAVED RETURN ADDR TO THE DISPATCHER.    */ 00049000
*/********************************************************************/ 00050000
*                                                               03S0077 00051000
*                                                               02S0077 00052000
*/********************************************************************/ 00053000
*/*    IQE PROCESSING                                                */ 00054000
*/********************************************************************/ 00055000
*                                                               02S0077 00056000
*                                                               03S0077 00057000
*/********************************************************************/ 00058000
*/*  IQEPROC-                                                        */ 00059000
*/*     THIS SEGMENT PROCESSES IQES.                                 */ 00060000
*/*      IT SCANS THE ASYNCHRONOUS EXIT QUEUES, LOOKING FOR          */ 00061000
*/*      IQES THAT CAN BE DEQUEUED.  IT WILL THEN DEQUEUE THE        */ 00062000
*/*      ELIGIBLE IQES, AND SCHEDULE THE CORRESPONDING ASYNCHRONOUS  */ 00063000
*/*      EXIT.                                                       */ 00064000
*/*      INPUT:                                                      */ 00065000
*/*        ASXBPTR (R6) - CONTAINS ADDR OF CURRENT ASXB.             */ 00066000
*/*        ASCBPTR (R8) - CONTAINS ADDR OF CURRENT ASCB.             */ 00067000
*/********************************************************************/ 00068000
*                                                               03S0077 00069000
*   PREVIQE=0;                      /* SET POINTER TO PREVIOUS       */ 00070000
         SLR   PREVIQE,PREVIQE                                  03S0077 00071000
*   IQEPTR=ASXBFIQE;                /* SET PTR TO FIRST IQE          */ 00072000
         L     IQEPTR,ASXBFIQE(,ASXBPTR)                        03S0078 00073000
*   DO WHILE IQEPTR^=0;             /* DO WHILE THERE ARE ANY IQES ON   00074000
*                                      THE QUEUE                     */ 00075000
         B     @DE00079                                         03S0079 00076000
@DL00079 DS    0H                                               03S0080 00077000
*     RESPECIFY                                                 03S0080 00078000
*       DQ RSTD;                    /* RESTRICT THE REGISTER         */ 00079000
*                                                               03S0080 00080000
*     /***************************************************************/ 00081000
*     /*                                                             */ 00082000
*     /* TEST THE IQE                                                */ 00083000
*     /*                                                             */ 00084000
*     /***************************************************************/ 00085000
*                                                               03S0081 00086000
*                                                               04S0081 00087000
*/********************************************************************/ 00088000
*/*  TESTIQE  -                                                      */ 00089000
*/*    THIS SEGMENTS TESTS TO SEE IF THE IQE SHOULD BE DEQUEUED      */ 00090000
*/*      IF IT CAN BE DEQUEUED, THE DQ SWITCH WILL BE SET TO 0.      */ 00091000
*/*      OTHERWISE IT WILL BE SET TO 4   .                           */ 00092000
*/*      INPUT:                                                      */ 00093000
*/*        IQEPTR (R2) - CONTAINS ADDR OF THE IQE BEING TESTED.      */ 00094000
*/*        ASXBPTR (R6) - CONTAINS ADDR OF THE CURRENT ASXB.         */ 00095000
*/********************************************************************/ 00096000
*                                                               04S0081 00097000
*     TCBPTR=IQETCBA;               /* GET THE TCB ADDRESS           */ 00098000
         L     TCBPTR,IQETCBA-1(,IQEPTR)                        04S0081 00099000
         LA    TCBPTR,0(,TCBPTR)                                04S0081 00100000
*     RBSECPTR=IQEIRBA;             /* GET THE IRB ADDRESS           */ 00101000
         L     RBSECPTR,IQEIRBA-1(,IQEPTR)                      04S0082 00102000
         LA    RBSECPTR,0(,RBSECPTR)                            04S0082 00103000
*     DQ=YES;                       /* INITIALIZE THE SWITCH         */ 00104000
         SLR   DQ,DQ                                            04S0083 00105000
*     IF(TCBPTR=ASXBETSK&           /* IF THIS IS THE ERROR TASK     */ 00106000
*         ASXBSIRB->RBFACTV=ON)     /* AND THE SIRB IS ACTIVE        */ 00107000
*         |TCBFX=ON                 /* OR ASYNCH EXITS ARE SUPPRESSED*/ 00108000
*         |RBFACTV=ON               /* OR THE RB IS ALREADY ACTIVE   */ 00109000
*         |(TCBACTIV=ON&TCBCCPVI^=PSACPULA)/* OR TCB ACTIVE ON  04S0084 00110000
*                                      ANOTHER CPU                   */ 00111000
*         |IQEPURGE=ON              /* OR THE IQE HAS BEEN PURGED    */ 00112000
*       THEN                                                    04S0084 00113000
         C     TCBPTR,ASXBETSK(,ASXBPTR)                        04S0084 00114000
         BNE   @GL00004                                         04S0084 00115000
         L     @15,ASXBSIRB(,ASXBPTR)                           04S0084 00116000
         TM    RBFACTV(@15),B'01000000'                         04S0084 00117000
         BO    @RT00084                                         04S0084 00118000
@GL00004 TM    TCBFX(TCBPTR),B'00000001'                        04S0084 00119000
         BO    @RT00084                                         04S0084 00120000
         TM    RBFACTV(RBSECPTR),B'01000000'                    04S0084 00121000
         BO    @RT00084                                         04S0084 00122000
         TM    TCBACTIV(TCBPTR),B'10000000'                     04S0084 00123000
         BNO   @GL00001                                         04S0084 00124000
         CLC   TCBCCPVI(2,TCBPTR),PSACPULA                      04S0084 00125000
         BNE   @RT00084                                         04S0084 00126000
@GL00001 TM    IQEPURGE(IQEPTR),B'10000000'                     04S0084 00127000
         BNO   @RF00084                                         04S0084 00128000
@RT00084 DS    0H                                               04S0085 00129000
*       DQ=NO;                      /* THEN DONT DEQUEUE IT          */ 00130000
         LA    DQ,4                                             04S0085 00131000
*     ELSE                                                      04S0086 00132000
*       IF RBATNXIT=ON              /* IF THIS IS AN ATTN IRB        */ 00133000
*         THEN                                                  04S0086 00134000
         B     @RC00084                                         04S0086 00135000
@RF00084 TM    RBATNXIT(RBSECPTR),B'00000010'                   04S0086 00136000
         BNO   @RF00086                                         04S0086 00137000
*         IF TCBATT=ON              /* IF ATTN EXITS ARE SUPPR       */ 00138000
*           THEN                                                04S0087 00139000
         TM    TCBATT(TCBPTR),B'00100000'                       04S0087 00140000
         BNO   @RF00087                                         04S0087 00141000
*           DQ=NO;                  /* DONT DEQUEUE IT               */ 00142000
         LA    DQ,4                                             04S0088 00143000
*         ELSE                                                  04S0089 00144000
*                                                               04S0089 00145000
*           /*********************************************************/ 00146000
*           /*                                                       */ 00147000
*           /* CHECK THE SUBTASKS FOR TCBATT OR TCBFX                */ 00148000
*           /*                                                       */ 00149000
*           /*********************************************************/ 00150000
*                                                               04S0089 00151000
*                                                               05S0089 00152000
*/********************************************************************/ 00153000
*/*  CHECKSUB -                                                      */ 00154000
*/*      THIS ROUTINE SCANS THE SUBTASK TREE TO SEE IF EITHER        */ 00155000
*/*      ALL ASYNCHRONOUS EXITS OR ATTENTION EXITS ARE SUPPRESSED    */ 00156000
*/*      FOR ANY OF THE SUBTASKS.   IF THEY ARE SUPPRESSED,A DQ SWITCH  00157000
*                                                                    */ 00158000
*/*      WILL BE SET TO 'NO'.  THIS IS ENTERED ONLY                  */ 00159000
*/*      IF  THE ASYNCHRONOUS EXIT BEING SCHEDULED IS AN ATTENTION   */ 00160000
*/*      EXIT.  THIS IS DONE TO AVOID SPECIFIC PROBLEMS IN THE       */ 00161000
*/*      TSO ATTENTION PROCESSING.                                   */ 00162000
*/*      INPUT:                                                      */ 00163000
*/*        TCBPTR (R4) - CONTAINS ADDR OF TCB TO WHICH IRB IS BEING  */ 00164000
*/*                 SCHEDULED.                                       */ 00165000
*/*        ASXBPTR (R6) - CONTAINS ADDR OF CURRENT ASXB              */ 00166000
*/********************************************************************/ 00167000
*                                                               05S0089 00168000
*           DO;                                                 05S0089 00169000
         B     @RC00087                                         05S0089 00170000
@RF00087 DS    0H                                               05S0090 00171000
*             RESPECIFY                                         05S0090 00172000
*              (R10,                                            05S0090 00173000
*               R11) RSTD;          /* RESTRICT THE REGS             */ 00174000
*/* SAVE REGISTERS ACROSS TREE SCAN INTERFACE                        */ 00175000
*                                                               05S0091 00176000
*             SAVE2=R7;                                         05S0091 00177000
         L     @15,ASXBSPSA(,ASXBPTR)                           05S0091 00178000
         L     @15,WSALS2EE(,@15)                               05S0091 00179000
         STCM  R7,15,SAVE2(@15)                                 05S0091 00180000
*             SAVE3=R8;                                         05S0092 00181000
         STCM  R8,15,SAVE3(@15)                                 05S0092 00182000
*             SAVE4=R9;                                         05S0093 00183000
         STCM  R9,15,SAVE4(@15)                                 05S0093 00184000
*             R10=TCBPTR;           /* SET TO START OF SEARCH        */ 00185000
         LR    R10,TCBPTR                                       05S0094 00186000
*             R8=R10;               /* SET TO TOP TCB IN TREE        */ 00187000
         LR    R8,R10                                           05S0095 00188000
*             R11=ADDR(NOTCB);      /* SET RET ADDR IF NOT FOUND     */ 00189000
         LA    R11,NOTCB                                        05S0096 00190000
*CALLTR:                                                        05S0097 00191000
*             CALL IEATRSCN;        /* CALL THE TREE SCAN RTN        */ 00192000
CALLTR   L     @15,@CV00060                                     05S0097 00193000
         BALR  @14,@15                                          05S0097 00194000
*             IF R10->TCBFC=ON THEN /* IF DESCENDENT TASK IS COMPLETE   00195000
*                                                            @ZA11369*/ 00196000
         TM    TCBFC(R10),B'10000000'                           05S0098 00197000
         BO    @RT00098                                         05S0098 00198000
*               GO TO CALLTR;       /* THEN IGNORE IT        @ZA11369*/ 00199000
*             IF R10->TCBFX=ON      /* IF ASYNCH EXITS ARE SUPPRESSED*/ 00200000
*                 |R10->TCBATT=ON   /* OR ATTN EXITS ARE SUPPRESSED  */ 00201000
*               THEN                                            05S0100 00202000
         TM    TCBFX(R10),B'00000001'                           05S0100 00203000
         BO    @RT00100                                         05S0100 00204000
         TM    TCBATT(R10),B'00100000'                          05S0100 00205000
         BNO   @RF00100                                         05S0100 00206000
@RT00100 DS    0H                                               05S0101 00207000
*               DQ=NO;              /* DONT DEQUEUE IT               */ 00208000
         LA    DQ,4                                             05S0101 00209000
*             ELSE                                              05S0102 00210000
*               GO TO CALLTR;       /* GET NEXT TCB                  */ 00211000
*NOTCB:                                                         05S0103 00212000
*             ;                                                 05S0103 00213000
*                                                               05S0103 00214000
NOTCB    DS    0H                                               05S0104 00215000
*/*  RESTORE REGISTERS AFTER TREE SCAN INTERFACE                     */ 00216000
*                                                               05S0104 00217000
*             R7=SAVE2;                                         05S0104 00218000
         L     @15,ASXBSPSA(,ASXBPTR)                           05S0104 00219000
         L     @15,WSALS2EE(,@15)                               05S0104 00220000
         ICM   R7,15,SAVE2(@15)                                 05S0104 00221000
*             R8=SAVE3;                                         05S0105 00222000
         ICM   R8,15,SAVE3(@15)                                 05S0105 00223000
*             R9=SAVE4;                                         05S0106 00224000
         ICM   R9,15,SAVE4(@15)                                 05S0106 00225000
*             RESPECIFY                                         05S0107 00226000
*              (R10,                                            05S0107 00227000
*               R11) UNRSTD;        /* FREE THE REGS                 */ 00228000
*           END;                                                05S0108 00229000
*                                                               05S0108 00230000
*/********************************************************************/ 00231000
*/*   END OF CHECKSUB -                                              */ 00232000
*/*      OUTPUT -                                                    */ 00233000
*/*        DQ (R13) - SET TO NO IF THE  IRB IS NOT TO BE ENQUEUED    */ 00234000
*/*        R10,R11,R14,R15,SAVE2,SAVE3,SAVE4 - VOLATILE              */ 00235000
*/********************************************************************/ 00236000
*                                                               05S0109 00237000
*                                                               04S0109 00238000
*/********************************************************************/ 00239000
*/*  END OF TESTIQE -                                                */ 00240000
*/*      OUTPUT -                                                    */ 00241000
*/*       R10,R11,R14,R15 - VOLATILE                                 */ 00242000
*/*       DQ (R13) = 0 IF IQE SHOULD BE DEQUEUED                     */ 00243000
*/*                = 4 IF IQE SHOULD NOT BE DEQUEUED                 */ 00244000
*/*       TCBPTR (R4) = ADDRESS OF TCB                               */ 00245000
*/*       RBSECPTR (R5) = ADDRESS OF THE IRB                         */ 00246000
*/********************************************************************/ 00247000
*                                                               04S0109 00248000
*     IF DQ=YES                     /* IF IT SHOULD BE DEQUEUED      */ 00249000
*       THEN                                                    03S0109 00250000
@RC00087 DS    0H                                               03S0109 00251000
@RF00086 DS    0H                                               03S0109 00252000
@RC00084 LTR   DQ,DQ                                            03S0109 00253000
         BNZ   @RF00109                                         03S0109 00254000
*CSLOOP1:                                                       03S0110 00255000
*       IF TCBS3A=OFF THEN          /* CHECK FOR INTERSECT WITH 03S0110 00256000
*                                      SUSPEND/RESUME        @Z40FPYJ*/ 00257000
CSLOOP1  TM    TCBS3A(TCBPTR),B'01000000'                       03S0110 00258000
         BNZ   @RF00110                                         03S0110 00259000
*         DO;                                                   03S0111 00260000
*           RFY                                                 03S0112 00261000
*            (WRKREG1,                                          03S0112 00262000
*             WRKREG2) RSTD;                                    03S0112 00263000
*           WRKREG1=TCBXSCT&S3AOFMSK;/* TURN OFF S3A IN REGISTER        00264000
*                                                            @Z40FPYJ*/ 00265000
         L     @15,TCBXSCT(,TCBPTR)                             03S0113 00266000
         LR    WRKREG1,@15                                      03S0113 00267000
         N     WRKREG1,@CF00090                                 03S0113 00268000
*           WRKREG2=TCBXSCT|S3AONMSK;/* TURN ON S3A IN REGISTER 03S0114 00269000
*                                                            @Z40FPYJ*/ 00270000
         O     @15,@CF00088                                     03S0114 00271000
         LR    WRKREG2,@15                                      03S0114 00272000
*           GENERATE;                                           03S0115 00273000
*                                      /*                   @Z40FPYJ*/  00274000
          CS    WRKREG1,WRKREG2,TCBXSCT(TCBPTR) TURN ON S3A   @Z40FPYJ  00275000
          BNE   CSLOOP1                   LOOP ON FAILURE     @Z40FPYJ  00276000
*           RESPECIFY                                           03S0116 00277000
*             DQ UNRSTD;            /* FREE THE REGISTER             */ 00278000
*           RESPECIFY                                           03S0117 00279000
*            (WRKREG1,                                          03S0117 00280000
*             WRKREG2) UNRSTD;      /* FREE THE REGISTERS            */ 00281000
*                                                               03S0117 00282000
*           /*********************************************************/ 00283000
*           /*                                                       */ 00284000
*           /* DEQUEUE THE IQE                                       */ 00285000
*           /*                                                       */ 00286000
*           /*********************************************************/ 00287000
*                                                               03S0118 00288000
*                                                               04S0118 00289000
*/********************************************************************/ 00290000
*/*   DQIQE -                                                        */ 00291000
*/*      THIS SEGMENT DEQUEUES THE IQE FROM THE ASYNCHRONOUS EXIT  Q.*/ 00292000
*/*      INPUT:                                                      */ 00293000
*/*        IQEPTR  (R2) - POINTS TO THE IQE TO BE DEQUEUED           */ 00294000
*/*        PREVIQE (R3) - CONTAINS ADDR OF THE IQE  PRECEDING THE ONE*/ 00295000
*/*                TO BE DEQUEUED.  IF THERE IS NO PREVIOUS , THEN IT*/ 00296000
*/*                CONTAINS ZERORES.                                 */ 00297000
*/*        ASXBPTR (R6) - CONTAINS ADDR OF THE CURRENT ASXB.         */ 00298000
*/********************************************************************/ 00299000
*                                                               04S0118 00300000
*           IF PREVIQE=0            /* IF THIS IS THE TOP IQE        */ 00301000
*             THEN                                              04S0118 00302000
         LTR   PREVIQE,PREVIQE                                  04S0118 00303000
         BNZ   @RF00118                                         04S0118 00304000
*             ASXBFIQE=IQEPTR->IQELNKA;/* UPDATE PTR TO FIRST        */ 00305000
         L     @15,IQELNKA-1(,IQEPTR)                           04S0119 00306000
         LA    @15,0(,@15)                                      04S0119 00307000
         ST    @15,ASXBFIQE(,ASXBPTR)                           04S0119 00308000
*           ELSE                                                04S0120 00309000
*             PREVIQE->IQELNKA=IQEPTR->IQELNKA;/* UPDATE CHAIN IN       00310000
*                                      PREVIOUS IQE                  */ 00311000
         B     @RC00118                                         04S0120 00312000
@RF00118 MVC   IQELNKA(3,PREVIQE),IQELNKA(IQEPTR)               04S0120 00313000
*           IF ASXBLIQE=IQEPTR      /* IF THIS WAS LAST IQE          */ 00314000
*             THEN                                              04S0121 00315000
@RC00118 C     IQEPTR,ASXBLIQE(,ASXBPTR)                        04S0121 00316000
         BNE   @RF00121                                         04S0121 00317000
*             ASXBLIQE=PREVIQE;     /* UPDATE PTR TO LAST IQE        */ 00318000
         ST    PREVIQE,ASXBLIQE(,ASXBPTR)                       04S0122 00319000
*/********************************************************************/ 00320000
*/*  END OF DQIQE -                                                  */ 00321000
*/********************************************************************/ 00322000
*                                                               04S0123 00323000
*           BLOCK=-IQEPTR;          /* INDICATE AN IQE TO SUBRTN     */ 00324000
*                                                               03S0123 00325000
@RF00121 LCR   @15,IQEPTR                                       03S0123 00326000
         LR    BLOCK,@15                                        03S0123 00327000
*           /*********************************************************/ 00328000
*           /*                                                       */ 00329000
*           /* INITIALIZE THE IRB AND THE TCB                        */ 00330000
*           /*                                                       */ 00331000
*           /*********************************************************/ 00332000
*                                                               03S0124 00333000
*           CALL IRBTCBIN;          /* INIT THE IRB AND TCB          */ 00334000
         BAL   @14,IRBTCBIN                                     03S0124 00335000
*           TCBS3A=OFF;             /* TURN OFF RESUME/TCTL INTERSECT   00336000
*                                      FLAG                  @Z40FPYJ*/ 00337000
         NI    TCBS3A(TCBPTR),B'10111111'                       03S0125 00338000
*         END;                                                  03S0126 00339000
*       ELSE                        /* TCBS3A IS ON          @ZA32738*/ 00340000
*         PREVIQE=IQEPTR;           /* SAVE CURRENT IN PREV  @ZA32738*/ 00341000
         B     @RC00110                                         03S0127 00342000
@RF00110 LR    PREVIQE,IQEPTR                                   03S0127 00343000
*     ELSE                                                      03S0128 00344000
*       PREVIQE=IQEPTR;             /* UPDATE PREVIOUS PTR           */ 00345000
         B     @RC00109                                         03S0128 00346000
@RF00109 LR    PREVIQE,IQEPTR                                   03S0128 00347000
*     IQEPTR=IQEPTR->IQELNKA;       /* UPDATE TO NEXT IQE            */ 00348000
@RC00109 L     @15,IQELNKA-1(,IQEPTR)                           03S0129 00349000
         LA    @15,0(,@15)                                      03S0129 00350000
         LR    IQEPTR,@15                                       03S0129 00351000
*   END;                                                        03S0130 00352000
*                                                               03S0130 00353000
@DE00079 LTR   IQEPTR,IQEPTR                                    03S0130 00354000
         BNZ   @DL00079                                         03S0130 00355000
*/********************************************************************/ 00356000
*/*  END OF  IQEPROC -                                               */ 00357000
*/*        OUTPUT:                                                   */ 00358000
*/*        PREVIQE (R3) - VOLATILE                                   */ 00359000
*/*        IQEPTR (R2) - VOLATILE                                    */ 00360000
*/*        R4,R5,R10,R11,R13,R14,R15 - VOLATILE                      */ 00361000
*/*        SAVE2,SAVE3,SAVE4 - VOLATILE                              */ 00362000
*/********************************************************************/ 00363000
*                                                               03S0131 00364000
*                                                               02S0131 00365000
*/********************************************************************/ 00366000
*/*    RQE PROCESSING                                                */ 00367000
*/********************************************************************/ 00368000
*                                                               02S0131 00369000
*                                                               03S0131 00370000
*/********************************************************************/ 00371000
*/*   RQEPROC  -                                                     */ 00372000
*/*   THIS SEGMENT PROCESSES RQES ON THE QUEUES.  IT SCANS THE       */ 00373000
*/*    ASYNCHRONOUS EXIT QUEUES LOOKING FOR RQES THAT CAN BE         */ 00374000
*/*    DEQUEUED AT THIS TIME.  IT DEQUEUES THE ONES THAT CAN BE      */ 00375000
*/*    AND SCHEDULES THE CORRESPONDING ASYNCHRONOUS EXITS.           */ 00376000
*/*      INPUT:                                                      */ 00377000
*/*        ASXBPTR (R6) - CONTAINS ADDR OF CURRENT ASXB.             */ 00378000
*/*        ASCBPTR (R8) - CONTAINS ADDR OF CURRENT ASCB.             */ 00379000
*/********************************************************************/ 00380000
*                                                               03S0131 00381000
*   PREVRQE=0;                      /* CLEAR PTR TO PREVIOUS         */ 00382000
         SLR   PREVRQE,PREVRQE                                  03S0131 00383000
*   RQEPTR=ASXBFRQE;                /* POINT TO TOP RQE              */ 00384000
         L     RQEPTR,ASXBFRQE(,ASXBPTR)                        03S0132 00385000
*   DO WHILE RQEPTR^=0;             /* WHILE THERE ARE ANY RQES      */ 00386000
         B     @DE00133                                         03S0133 00387000
@DL00133 DS    0H                                               03S0134 00388000
*     RESPECIFY                                                 03S0134 00389000
*       DQ RSTD;                    /* RESTRICT REGISTER             */ 00390000
*                                                               03S0134 00391000
*     /***************************************************************/ 00392000
*     /*                                                             */ 00393000
*     /* TEST THE RQE                                                */ 00394000
*     /*                                                             */ 00395000
*     /***************************************************************/ 00396000
*                                                               03S0135 00397000
*                                                               04S0135 00398000
*/********************************************************************/ 00399000
*/*  TESTRQE -                                                       */ 00400000
*/*      THIS SEGMENT DETERMINES IF AN RQE CAN BE DEQUEUED FROM      */ 00401000
*/*      THE ASYNCHRONOUS EXIT QUEUE.  IF IT CAN, DQ IS SET TO 0    .*/ 00402000
*/*      OTHERWISE, DQ IS SET TO 4   .                               */ 00403000
*/*      INPUT:                                                      */ 00404000
*/*        RQEPTR   (R2) - CONTAINS ADDR OF THE RQE TO BE TESTED.    */ 00405000
*/*        ASXBPTR (R6) - CONTAINS ADDR OF CURRENT ASXB.             */ 00406000
*/********************************************************************/ 00407000
*                                                               04S0135 00408000
*     TCBPTR=RQETCB;                /* GET TCB ADDRESS       @YM01424*/ 00409000
         L     TCBPTR,RQETCB(,RQEPTR)                           04S0135 00410000
*     RBSECPTR=RQERRQ;              /* GET IRB ADDRESS       @YM01424*/ 00411000
         L     RBSECPTR,RQERRQ(,RQEPTR)                         04S0136 00412000
*     DQ=YES;                       /* TURN OFF THE SWITCH           */ 00413000
         SLR   DQ,DQ                                            04S0137 00414000
*     IF(TCBPTR=ASXBETSK            /* IF THIS IS THE ERROR TASK     */ 00415000
*         &ASXBSIRB->RBFACTV=ON)    /* AND THE SIRB IS ACTIVE        */ 00416000
*         |TCBFX=ON                 /* OR ASYNCH EXITS ARE SUPPRESSED*/ 00417000
*         |RBFACTV=ON               /* OR THE IRB IS ACTIVE          */ 00418000
*         |(TCBACTIV=ON             /* OR THE TASK IS ACTIVE         */ 00419000
*         &TCBCCPVI^=PSACPULA)      /* ON ANOTHER CPU                */ 00420000
*       THEN                                                    04S0138 00421000
         C     TCBPTR,ASXBETSK(,ASXBPTR)                        04S0138 00422000
         BNE   @GL00010                                         04S0138 00423000
         L     @15,ASXBSIRB(,ASXBPTR)                           04S0138 00424000
         TM    RBFACTV(@15),B'01000000'                         04S0138 00425000
         BO    @RT00138                                         04S0138 00426000
@GL00010 TM    TCBFX(TCBPTR),B'00000001'                        04S0138 00427000
         BO    @RT00138                                         04S0138 00428000
         TM    RBFACTV(RBSECPTR),B'01000000'                    04S0138 00429000
         BO    @RT00138                                         04S0138 00430000
         TM    TCBACTIV(TCBPTR),B'10000000'                     04S0138 00431000
         BNO   @RF00138                                         04S0138 00432000
         CLC   TCBCCPVI(2,TCBPTR),PSACPULA                      04S0138 00433000
         BE    @RF00138                                         04S0138 00434000
@RT00138 DS    0H                                               04S0139 00435000
*       DQ=NO;                      /* THEN DONT DEQUEUE             */ 00436000
         LA    DQ,4                                             04S0139 00437000
*/********************************************************************/ 00438000
*/*  END OF TESTRQE -                                                */ 00439000
*/*      OUTPUT:                                                     */ 00440000
*/*        DQ (R13) = 0  , IF RQE IS TO BE DEQUEUED.                 */ 00441000
*/*                 =4  , IF IT IS NOT.                              */ 00442000
*/*        TCBPTR (R4)  - CONTAINS ADDR OF TCB TO WHICH THE RQE      */ 00443000
*/*                   IS BEING SCHEDULED.                            */ 00444000
*/*        RBSECPTR (R5) - CONTAINS ADDR OF THE IRB WHICH IS BEING   */ 00445000
*/*              SCHEDULED.                                          */ 00446000
*/********************************************************************/ 00447000
*                                                               04S0140 00448000
*                                   /* DEQUEUED                      */ 00449000
*     IF DQ=YES                     /* IF SO                         */ 00450000
*       THEN                                                    03S0140 00451000
@RF00138 LTR   DQ,DQ                                            03S0140 00452000
         BNZ   @RF00140                                         03S0140 00453000
*CSLOOP2:                                                       03S0141 00454000
*       IF TCBS3A=OFF THEN          /* CHECK FOR INTERSECT WITH 03S0141 00455000
*                                      SUSPEND/RESUME        @Z40FPYJ*/ 00456000
CSLOOP2  TM    TCBS3A(TCBPTR),B'01000000'                       03S0141 00457000
         BNZ   @RF00141                                         03S0141 00458000
*         DO;                                                   03S0142 00459000
*           RFY                                                 03S0143 00460000
*            (WRKREG1,                                          03S0143 00461000
*             WRKREG2) RSTD;                                    03S0143 00462000
*           WRKREG1=TCBXSCT&S3AOFMSK;/* TURN OFF S3A IN REGISTER        00463000
*                                                            @Z40FPYJ*/ 00464000
         L     @15,TCBXSCT(,TCBPTR)                             03S0144 00465000
         LR    WRKREG1,@15                                      03S0144 00466000
         N     WRKREG1,@CF00090                                 03S0144 00467000
*           WRKREG2=TCBXSCT|S3AONMSK;/* TURN ON S3A IN REGISTER 03S0145 00468000
*                                                            @Z40FPYJ*/ 00469000
         O     @15,@CF00088                                     03S0145 00470000
         LR    WRKREG2,@15                                      03S0145 00471000
*           GENERATE;                                           03S0146 00472000
*                                      /*                   @Z40FPYJ*/  00473000
          CS    WRKREG1,WRKREG2,TCBXSCT(TCBPTR) TURN ON S3A   @Z40FPYJ  00474000
          BNE   CSLOOP2                   LOOP ON FAILURE     @Z40FPYJ  00475000
*           RESPECIFY                                           03S0147 00476000
*             DQ UNRSTD;            /* FREE THE REGISTER             */ 00477000
*           RESPECIFY                                           03S0148 00478000
*            (WRKREG1,                                          03S0148 00479000
*             WRKREG2) UNRSTD;      /* FREE THE REGISTERS            */ 00480000
*                                                               03S0148 00481000
*           /*********************************************************/ 00482000
*           /*                                                       */ 00483000
*           /* DEQUEUE THE RQE                                       */ 00484000
*           /*                                                       */ 00485000
*           /*********************************************************/ 00486000
*                                                               03S0149 00487000
*                                                               04S0149 00488000
*/********************************************************************/ 00489000
*/*  DQRQE -                                                         */ 00490000
*/*      THIS SEGMENT DEQUEUES AN RQE FROM THE ASYNCHRONOUS EXIT QUEUE  00491000
*                                                                    */ 00492000
*/*      INPUT:                                                      */ 00493000
*/*        RQEPTR   (R2)  - CONTAINS ADDR OF THE RQE TO BE DEQUEUED. */ 00494000
*/*        PREVRQE (R3) - CONTAINS ADDR OF PREVIOUS RQE IF THERE IS  */ 00495000
*/*                   ONE, OR ZEROES IF THERE IS NO PREVIOUS RQE.    */ 00496000
*/*        ASXBPTR (R6) - CONTAINS ADDR OF THE CURRENT ASXB.         */ 00497000
*/********************************************************************/ 00498000
*                                                               04S0149 00499000
*           IF PREVRQE=0            /* IF THIS IS THE TOP RQE        */ 00500000
*             THEN                                              04S0149 00501000
         LTR   PREVRQE,PREVRQE                                  04S0149 00502000
         BNZ   @RF00149                                         04S0149 00503000
*             ASXBFRQE=RQEPTR->RQENRQE;/* UPDATE PTR TO FIRST RQE    */ 00504000
         L     @15,RQENRQE(,RQEPTR)                             04S0150 00505000
         ST    @15,ASXBFRQE(,ASXBPTR)                           04S0150 00506000
*           ELSE                                                04S0151 00507000
*             PREVRQE->RQENRQE=RQEPTR->RQENRQE;                 04S0151 00508000
         B     @RC00149                                         04S0151 00509000
@RF00149 L     @15,RQENRQE(,RQEPTR)                             04S0151 00510000
         ST    @15,RQENRQE(,PREVRQE)                            04S0151 00511000
*           IF ASXBLRQE=RQEPTR      /* IF THIS WAS LAST RQE          */ 00512000
*             THEN                                              04S0152 00513000
@RC00149 C     RQEPTR,ASXBLRQE(,ASXBPTR)                        04S0152 00514000
         BNE   @RF00152                                         04S0152 00515000
*             ASXBLRQE=PREVRQE;     /* UPDATE PTR TO LAST            */ 00516000
         ST    PREVRQE,ASXBLRQE(,ASXBPTR)                       04S0153 00517000
*/********************************************************************/ 00518000
*/*  END OF DQRQE -                                                  */ 00519000
*/********************************************************************/ 00520000
*                                                               04S0154 00521000
*                                                               03S0154 00522000
*           /*********************************************************/ 00523000
*           /*                                                       */ 00524000
*           /* INITIALIZE THE IRB AND TCB                            */ 00525000
*           /*                                                       */ 00526000
*           /*********************************************************/ 00527000
*                                                               03S0154 00528000
*           CALL IRBTCBIN;          /* INITIALIZE THE IRB AND TCB    */ 00529000
@RF00152 BAL   @14,IRBTCBIN                                     03S0154 00530000
*           TCBS3A=OFF;             /* TURN OFF RESUME/TCTL INTERSECT   00531000
*                                      FLAG                  @Z40FPYJ*/ 00532000
         NI    TCBS3A(TCBPTR),B'10111111'                       03S0155 00533000
*         END;                                                  03S0156 00534000
*       ELSE                        /* TCBS3A IS ON          @ZA32738*/ 00535000
*         PREVRQE=RQEPTR;           /* SAVE CURRENT IN PREV  @ZA32738*/ 00536000
         B     @RC00141                                         03S0157 00537000
@RF00141 LR    PREVRQE,RQEPTR                                   03S0157 00538000
*     ELSE                                                      03S0158 00539000
*       PREVRQE=RQEPTR;             /* UPDATE PREVIOUS POINTER       */ 00540000
         B     @RC00140                                         03S0158 00541000
@RF00140 LR    PREVRQE,RQEPTR                                   03S0158 00542000
*     RQEPTR=RQEPTR->RQENRQE;       /* PICK UP THE NEXT RQE          */ 00543000
@RC00140 L     RQEPTR,RQENRQE(,RQEPTR)                          03S0159 00544000
*   END;                                                        03S0160 00545000
*                                                               03S0160 00546000
@DE00133 SLR   @15,@15                                          03S0160 00547000
         CR    RQEPTR,@15                                       03S0160 00548000
         BNE   @DL00133                                         03S0160 00549000
*/********************************************************************/ 00550000
*/*  END OF RQEPROC -                                                */ 00551000
*/*      OUTPUT:                                                     */ 00552000
*/*        R2-R5 - VOLATILE                                          */ 00553000
*/*        R10,R11 - VOLATILE                                        */ 00554000
*/*        R13,R14 - VOLATILE                                        */ 00555000
*/********************************************************************/ 00556000
*                                                               03S0161 00557000
*                                                               02S0161 00558000
*/********************************************************************/ 00559000
*/*   SRB PROCESSING                                                 */ 00560000
*/********************************************************************/ 00561000
*                                                               02S0161 00562000
*                                                               03S0161 00563000
*/********************************************************************/ 00564000
*/*  SRBPROC  -                                                      */ 00565000
*/*   THIS SEGMENT PROCESSES SRBS.  IT TAKES ONLY THE TOP SRB ON THE */ 00566000
*/*   QUEUE.  IF THE ADDRESS SPACE SIRB CAN BE SCHEDULED AT THIS TIME*/ 00567000
*/*   THEN THE TOP SRB WILL BE DEQUEUED, AND THE SIRB SCHEDULED      */ 00568000
*/*    TO THE TASK DESIGNATED AS THE ERROR TASK, I.E. THE ONE        */ 00569000
*/*   UNDER WHICH ERPS ARE TO EXECUTE.                               */ 00570000
*/*      INPUT:                                                      */ 00571000
*/*        ASXBPTR (R6) - CONTAINS ADDR OF CURRENT ASXB.             */ 00572000
*/*        ASCBPTR (R8) - CONTAINS ADDR OF CURRENT ASCB.             */ 00573000
*/********************************************************************/ 00574000
*                                                               03S0161 00575000
*   SRBPTR=ASXBFSRB;                /* POINT TO FIRST SRB            */ 00576000
         L     SRBPTR,ASXBFSRB(,ASXBPTR)                        03S0161 00577000
*   IF SRBPTR^=0                    /* IF THERE IS AN SRB            */ 00578000
*     THEN                                                      03S0162 00579000
         CR    SRBPTR,@15                                       03S0162 00580000
         BE    @RF00162                                         03S0162 00581000
*     DO;                                                       03S0163 00582000
*       RESPECIFY                                               03S0164 00583000
*         DQ RSTD;                  /* RESTRIC TTHE REGISTER         */ 00584000
*                                                               03S0164 00585000
*       /*************************************************************/ 00586000
*       /*                                                           */ 00587000
*       /* TEST THE SRB                                              */ 00588000
*       /*                                                           */ 00589000
*       /*************************************************************/ 00590000
*                                                               03S0165 00591000
*                                                               04S0165 00592000
*/********************************************************************/ 00593000
*/*  TESTSRB -                                                       */ 00594000
*/*      THIS SEGMENT DETERMINES WHETHER THE SRB MAY BE DEQUEUED     */ 00595000
*/*      NOW.  IF IT CAN BE DEQUEUED, A DQ SWITCH IS SET             */ 00596000
*/*      TO     0.  OTHERWISE , IT IS SET TO  4  .                   */ 00597000
*/*      INPUT:                                                      */ 00598000
*/*        ASXBPTR (R6) - CONTAINS ADDR OF THE CURRENT ASXB.         */ 00599000
*/********************************************************************/ 00600000
*                                                               04S0165 00601000
*       TCBPTR=ASXBETSK;            /* POINT TO THE ERROR TASK       */ 00602000
         L     TCBPTR,ASXBETSK(,ASXBPTR)                        04S0165 00603000
*       RBSECPTR=ASXBSIRB;          /* POINT TO THE SIRB             */ 00604000
         L     RBSECPTR,ASXBSIRB(,ASXBPTR)                      04S0166 00605000
*       IF TCBACTIV=ON              /* IF THE TASK IS ACTIVE         */ 00606000
*           &TCBCCPVI^=PSACPULA     /* ON ANOTHER CPU                */ 00607000
*           |RBFACTV=ON             /* OR THE SIRB IS ACTIVE         */ 00608000
*         THEN                                                  04S0167 00609000
         TM    TCBACTIV(TCBPTR),B'10000000'                     04S0167 00610000
         BNO   @GL00013                                         04S0167 00611000
         CLC   TCBCCPVI(2,TCBPTR),PSACPULA                      04S0167 00612000
         BNE   @RT00167                                         04S0167 00613000
@GL00013 TM    RBFACTV(RBSECPTR),B'01000000'                    04S0167 00614000
         BNO   @RF00167                                         04S0167 00615000
@RT00167 DS    0H                                               04S0168 00616000
*         DQ=NO;                    /* THEN DONT DEQUEUE IT          */ 00617000
         LA    DQ,4                                             04S0168 00618000
*       ELSE                                                    04S0169 00619000
*         DQ=YES;                   /* DO DEQUEUE IT                 */ 00620000
         B     @RC00167                                         04S0169 00621000
@RF00167 SLR   DQ,DQ                                            04S0169 00622000
*/********************************************************************/ 00623000
*/*  END OF TESTSRB-                                                 */ 00624000
*/*      OUTPUT:                                                     */ 00625000
*/*        DQ (R13) - EQUALS   0   IF THE SRB CAN BE DEQUEUED.       */ 00626000
*/*                   EQUALS  4   IF THE SRB CANNOT BE DEQUEUED.     */ 00627000
*/*        TCBPTR (R4) -CONTAINS ADDR OF THE  ERROR TASK.            */ 00628000
*/*        RBSECPTR (R5) - CONTAINS ADDR OF THE SIRB.                */ 00629000
*/********************************************************************/ 00630000
*                                                               04S0170 00631000
*       IF DQ=YES                   /* IF IT SHOULD BE DEQUEUED      */ 00632000
*         THEN                                                  03S0170 00633000
@RC00167 LTR   DQ,DQ                                            03S0170 00634000
         BNZ   @RF00170                                         03S0170 00635000
*CSLOOP3:                                                       03S0171 00636000
*         IF TCBS3A=OFF THEN        /* CHECK FOR INTERSECT WITH 03S0171 00637000
*                                      SUSPEND/RESUME        @Z40FPYJ*/ 00638000
CSLOOP3  TM    TCBS3A(TCBPTR),B'01000000'                       03S0171 00639000
         BNZ   @RF00171                                         03S0171 00640000
*           DO;                                                 03S0172 00641000
*             RFY                                               03S0173 00642000
*              (WRKREG1,                                        03S0173 00643000
*               WRKREG2) RSTD;                                  03S0173 00644000
*             WRKREG1=TCBXSCT&S3AOFMSK;/* TURN OFF S3A IN REGISTER      00645000
*                                                            @Z40FPYJ*/ 00646000
         L     @15,TCBXSCT(,TCBPTR)                             03S0174 00647000
         LR    WRKREG1,@15                                      03S0174 00648000
         N     WRKREG1,@CF00090                                 03S0174 00649000
*             WRKREG2=TCBXSCT|S3AONMSK;/* TURN ON S3A IN REGISTER       00650000
*                                                            @Z40FPYJ*/ 00651000
         O     @15,@CF00088                                     03S0175 00652000
         LR    WRKREG2,@15                                      03S0175 00653000
*             GENERATE;                                         03S0176 00654000
*                                                               03S0176 00655000
*                                      /*                   @Z40FPYJ*/  00656000
          CS    WRKREG1,WRKREG2,TCBXSCT(TCBPTR) TURN ON S3A   @Z40FPYJ  00657000
          BNE   CSLOOP3                   LOOP ON FAILURE     @Z40FPYJ  00658000
*             /*******************************************************/ 00659000
*             /*                                                     */ 00660000
*             /* DEQUEUE THE SRB                                     */ 00661000
*             /*                                                     */ 00662000
*             /*******************************************************/ 00663000
*                                                               03S0177 00664000
*             RESPECIFY                                         03S0177 00665000
*               DQ UNRSTD;          /* FREE THE REGISTER             */ 00666000
*             RESPECIFY                                         03S0178 00667000
*              (WRKREG1,                                        03S0178 00668000
*               WRKREG2) UNRSTD;    /* FREE THE REGISTERS            */ 00669000
*             ASXBFSRB=SRBFLNK;     /* UPDATE POINTER TO FIRST       */ 00670000
         L     @15,SRBFLNK(,SRBPTR)                             03S0179 00671000
         ST    @15,ASXBFSRB(,ASXBPTR)                           03S0179 00672000
*             IF ASXBFSRB=0         /* IF THIS WAS ONLY SRB          */ 00673000
*               THEN                                            03S0180 00674000
         SLR   @11,@11                                          03S0180 00675000
         CR    @15,@11                                          03S0180 00676000
         BNE   @RF00180                                         03S0180 00677000
*               ASXBLSRB=0;         /* UPDATE LAST SRB PTR           */ 00678000
*                                                               03S0181 00679000
         ST    @11,ASXBLSRB(,ASXBPTR)                           03S0181 00680000
*             /*******************************************************/ 00681000
*             /*                                                     */ 00682000
*             /* INITIALIZE THE SIRB                                 */ 00683000
*             /*                                                     */ 00684000
*             /*******************************************************/ 00685000
*                                                               03S0182 00686000
*                                                               04S0182 00687000
*/****************************************************************** */ 00688000
*/*  INITSIRB -                                                      */ 00689000
*/*      THIS SEGMENT INITIALIZES SPECIAL FIELDS FOR THE SIRB        */ 00690000
*/*      INPUT-                                                      */ 00691000
*/*        RBSECPTR - R5 - ADDRESS OF SIRB                           */ 00692000
*/****************************************************************** */ 00693000
*                                                               04S0182 00694000
*             RBSIZE=ADDR(SIRBLEN)/8;/* SET SIZE OF SIRB             */ 00695000
@RF00180 LA    @00,SIRBLEN                                      04S0182 00696000
         SRL   @00,3                                            04S0182 00697000
         STH   @00,RBSIZE(,RBSECPTR)                            04S0182 00698000
*             RBSTAB=SIRBSTAB;      /* SET THE STAB BYTES            */ 00699000
         MVC   RBSTAB(2,RBSECPTR),@CB00081                      04S0183 00700000
*             RBOPSW=SIRBPSW;       /* SET THE PSW FIELDS            */ 00701000
         MVC   RBOPSW(8,RBSECPTR),@CB00084                      04S0184 00702000
*             RBEP=ADDR(IECVERPL);  /* SET ENTRY POINT               */ 00703000
         L     @15,@CV00061                                     04S0185 00704000
         ST    @15,RBEP(,RBSECPTR)                              04S0185 00705000
*/****************************************************************** */ 00706000
*/* END OF INITSIRB                                                  */ 00707000
*/****************************************************************** */ 00708000
*                                                               04S0186 00709000
*                                                               03S0186 00710000
*             /*******************************************************/ 00711000
*             /*                                                     */ 00712000
*             /* INITIALIZE THE SIRB AND TCB                         */ 00713000
*             /*                                                     */ 00714000
*             /*******************************************************/ 00715000
*                                                               03S0186 00716000
*             CALL IRBTCBIN;        /* INITIALIZE IRB AND TCB        */ 00717000
         BAL   @14,IRBTCBIN                                     03S0186 00718000
*             TCBS3A=OFF;           /* TURN OFF RESUME/TCTL INTERSECT   00719000
*                                      FLAG                  @Z40FPYJ*/ 00720000
         NI    TCBS3A(TCBPTR),B'10111111'                       03S0187 00721000
*           END;                                                03S0188 00722000
*     END;                                                      03S0189 00723000
*                                                               03S0189 00724000
@RF00171 DS    0H                                               03S0189 00725000
@RF00170 DS    0H                                               03S0190 00726000
*/********************************************************************/ 00727000
*/*  END OF SRBPROC -                                                */ 00728000
*/*      OUTPUT:                                                     */ 00729000
*/*        R2,R4,R5 - VOLATILE                                       */ 00730000
*/*        R10,R11,R13,R14 - VOLATILE                                */ 00731000
*/********************************************************************/ 00732000
*                                                               03S0190 00733000
*   IF ASXBAEQ=NULL                 /* IF QUEUES EMPTY               */ 00734000
*     THEN                                                      02S0190 00735000
@RF00162 CLC   ASXBAEQ(24,ASXBPTR),NULL                         02S0190 00736000
         BNE   @RF00190                                         02S0190 00737000
*     ASCBS3S='0'B;                 /* TURN OFF STAGE 3 SWITCH       */ 00738000
         NI    ASCBS3S(ASCBPTR),B'11011111'                     02S0191 00739000
*   ASCBS2S='0'B;                   /* TURN OFF THE STAGE 2 SWITCH   */ 00740000
@RF00190 NI    ASCBS2S(ASCBPTR),B'11101111'                     02S0192 00741000
*/*  RESTORE STATUS AND RETURN                                       */ 00742000
*                                                               02S0193 00743000
*                                                               03S0193 00744000
*/********************************************************************/ 00745000
*/*   RESREG  -                                                      */ 00746000
*/*        THIS SEGMENT RESTORES THE NECESSARY REGISTERS BEFORE      */ 00747000
*/*      RETURNING TO THE DISPATCHER.                                */ 00748000
*/*      INPUT:                                                      */ 00749000
*/*        ASXBPTR (R6) - CONTAINS ADDR OF THE CURRENT ASXB.         */ 00750000
*/*                 THE ASXB THEN POINTS TO THE LOCAL WSA VECTOR     */ 00751000
*/*                  TABLE,WHICH POINTS TO THE SAVE AREA.            */ 00752000
*/*        SAVE1 - CONTAINS THE SAVED RETURN ADDR.                   */ 00753000
*/********************************************************************/ 00754000
*                                                               03S0193 00755000
*   R14=SAVE1;                      /* RESTORE RETURN REGISTER       */ 00756000
         L     @15,ASXBSPSA(,ASXBPTR)                           03S0193 00757000
         L     @15,WSALS2EE(,@15)                               03S0193 00758000
         ICM   R14,15,SAVE1(@15)                                03S0193 00759000
*/********************************************************************/ 00760000
*/*  END OF RESREG -                                                 */ 00761000
*/*      OUTPUT:                                                     */ 00762000
*/*        R14 - CONTAINS RETURN ADDR TO THE DISPATCHER.             */ 00763000
*/********************************************************************/ 00764000
*                                                               03S0194 00765000
*                                                               02S0194 00766000
*/********************************************************************/ 00767000
*                                                               02S0194 00768000
*                                                               01S0194 00769000
*/*  INTERNAL PROCEDURES                                             */ 00770000
*                                                               01S0194 00771000
*                                                               02S0194 00772000
*/********************************************************************/ 00773000
*/*  IRBTCBIN -                                                      */ 00774000
*/*      THIS SEGMENT INITIALIZES A TCB AND IRB.  IT HANDLES SAVING  */ 00775000
*/*      STATUS OF THE PREVIOUS ROUTINE, AND SETTING UP  FOR DISPATCH*/ 00776000
*/*      OF THE ASYNCHRONOUS EXIT.                                   */ 00777000
*/*      INPUT:                                                      */ 00778000
*/*        TCBPTR  (R4) - CONTAINS ADDR OF TCB TO WHICH THE IRB      */ 00779000
*/*                  IS BEING SCHEDULED.                             */ 00780000
*/*        RBSECPTR - (R5) CONTAINS ADDR OF THE IRB WHICH IS         */ 00781000
*/*            BEING SCHEDULED.                                      */ 00782000
*/*        R2 - CONTAINS ADDR OF THE CONTROL BLOCK WHICH HAS         */ 00783000
*/*            SCHEDULED THE IRB.  IF COMPLEMENTED, IT IS THE        */ 00784000
*/*            NEGATIVE ADDR OF AN IQE.  IF POSITIVE, IT IS THE      */ 00785000
*/*            ADDR OF AN  SRB OR RQE.                               */ 00786000
*/*        ASCBPTR (R8) - CONTAINS CURRENT ASCB ADDR                 */ 00787000
*/*        R14 - CONTAINS RETURN ADDR.                               */ 00788000
*/********************************************************************/ 00789000
*                                                               02S0194 00790000
*IRBTCBIN:                                                      02S0194 00791000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                            02S0194 00792000
@EL00001 DS    0H                                               02S0194 00793000
@EF00001 DS    0H                                               02S0194 00794000
@ER00001 BR    @14                                              02S0194 00795000
IRBTCBIN DS    0H                                               02S0195 00796000
*   RBFACTV=ON;                     /* SET THE RB ACTIVE     @ZA09499*/ 00797000
         OI    RBFACTV(RBSECPTR),B'01000000'                    02S0195 00798000
*   RBGRSAVE=TCBGRS;                /* SAVE TASK'S REGISTERS         */ 00799000
         MVC   RBGRSAVE(64,RBSECPTR),TCBGRS(TCBPTR)             02S0196 00800000
*   IF BLOCK<0                      /* IF THIS IS AN IQE             */ 00801000
*     THEN                                                      02S0197 00802000
         LTR   BLOCK,BLOCK                                      02S0197 00803000
         BNM   @RF00197                                         02S0197 00804000
*     DO;                                                       02S0198 00805000
*       IQEPTR=-BLOCK;              /* SET ADDRESS POSITIVE          */ 00806000
         LCR   @15,BLOCK                                        02S0199 00807000
         LR    IQEPTR,@15                                       02S0199 00808000
*       TCBGRS0=IQEPTR;             /* SET R0 TO ADDR OF IQE         */ 00809000
         ST    IQEPTR,TCBGRS0(,TCBPTR)                          02S0200 00810000
*       TCBGRS1=IQEPARAM;           /* SET R1 TO PARAMETER           */ 00811000
         L     @15,IQEPARAM(,IQEPTR)                            02S0201 00812000
         ST    @15,TCBGRS1(,TCBPTR)                             02S0201 00813000
*       IF RBIQETP=ANRQE1|          /* IF THE IRB INDICATES RQES     */ 00814000
*           RBIQETP=ANRQE2          /* SCHEDULE THE IRB              */ 00815000
*         THEN                                                  02S0202 00816000
         TM    RBIQETP(RBSECPTR),B'00000100'                    02S0202 00817000
         BNO   @GL00015                                         02S0202 00818000
         TM    RBIQETP(RBSECPTR),B'00001000'                    02S0202 00819000
         BZ    @RT00202                                         02S0202 00820000
@GL00015 TM    RBIQETP(RBSECPTR),B'00001100'                    02S0202 00821000
         BNZ   @RF00202                                         02S0202 00822000
@RT00202 DS    0H                                               02S0203 00823000
*         RBIQETP='11'B;            /* CORRECT TO INDICATE IQES      */ 00824000
         OI    RBIQETP(RBSECPTR),B'00001100'                    02S0203 00825000
*     END;                                                      02S0204 00826000
*   ELSE                                                        02S0205 00827000
*     TCBGRS1=BLOCK;                /* SET R1 TO ADDR OF RQE OR SRB  */ 00828000
         B     @RC00197                                         02S0205 00829000
@RF00197 ST    BLOCK,TCBGRS1(,TCBPTR)                           02S0205 00830000
*   TCBGRS13=RBPPSAV1;              /* SET R13 TO ADDR OF PROBLEM       00831000
*                                      PROGRAM SAVE AREA             */ 00832000
@RC00197 L     @15,RBPPSAV1-1(,RBSECPTR)                        02S0206 00833000
         LA    @15,0(,@15)                                      02S0206 00834000
         ST    @15,TCBGRS13(,TCBPTR)                            02S0206 00835000
*   TCBGRS14=ADDR(CVTEXIT);         /* R14 POINTS TO SVC 3           */ 00836000
         L     @15,CVTPTR                                       02S0207 00837000
         LA    @15,CVTEXIT(,@15)                                02S0207 00838000
         ST    @15,TCBGRS14(,TCBPTR)                            02S0207 00839000
*   TCBGRS15=RBEP;                  /* R15 CONTAINS ENTRY POINT      */ 00840000
         L     @15,RBEP(,RBSECPTR)                              02S0208 00841000
         ST    @15,TCBGRS15(,TCBPTR)                            02S0208 00842000
*   RBIQE1=BLOCK;                   /* POINT TO IQE/RQE/SRB          */ 00843000
         STCM  BLOCK,7,RBIQE1(RBSECPTR)                         02S0209 00844000
*   RBOPSW(6:8)=RBEP;               /* PSW SET TO ENTRY POINT        */ 00845000
*                                                               02S0210 00846000
         STCM  @15,7,RBOPSW+5(RBSECPTR)                         02S0210 00847000
*   /*****************************************************************/ 00848000
*   /*                                                               */ 00849000
*   /* QUEUE THE RB TO THE TCB                                       */ 00850000
*   /*                                                               */ 00851000
*   /*****************************************************************/ 00852000
*                                                               02S0211 00853000
*                                                               03S0211 00854000
*/********************************************************************/ 00855000
*/*  QRB -                                                           */ 00856000
*/*  THIS SEGMENT QUEUES THE IRB TO THE TCB.  IT ALSO BUMPS THE COUNT*/ 00857000
*/*   OF READY TCBS IF THE TASK WAS NOT PREVIOUSLY READY,BUT NOW IS. */ 00858000
*/*      INPUT:                                                      */ 00859000
*/*        TCBPTR (R4) - CONTAINS ADDR OF THE TCB TO WHICH IRB       */ 00860000
*/*                 IS BEING QUEUED.                                 */ 00861000
*/*        RBSECPTR (R5) - CONTAINS ADDRESS OF THE IRB.              */ 00862000
*/*        ASCBPTR (R8) - CONTAINS ADDR OF THE CURRENT ASCB.         */ 00863000
*/********************************************************************/ 00864000
*                                                               03S0211 00865000
*   RBLINKB=TCBRBP;                 /* POINT TO PREVIOUS RB          */ 00866000
         L     @15,TCBRBP(,TCBPTR)                              03S0211 00867000
         STCM  @15,7,RBLINKB(RBSECPTR)                          03S0211 00868000
*   TCBRBP=RBSECPTR;                /* POINT TO NEW RB               */ 00869000
         ST    RBSECPTR,TCBRBP(,TCBPTR)                         03S0212 00870000
*   IF RBLINKB->RBWCF>0             /* IF OLD RB WAS WAITING         */ 00871000
*     THEN                                                      03S0213 00872000
         CLI   RBWCF(@15),0                                     03S0213 00873000
         BNH   @RF00213                                         03S0213 00874000
*     IF TCBFLGS4=0&TCBFLGS5=0      /* AND TASK WAS DISPATCHABLE     */ 00875000
*       THEN                                                    03S0214 00876000
         CLI   TCBFLGS4(TCBPTR),0                               03S0214 00877000
         BNE   @RF00214                                         03S0214 00878000
         CLI   TCBFLGS5(TCBPTR),0                               03S0214 00879000
         BNE   @RF00214                                         03S0214 00880000
*       DO;                                                     03S0215 00881000
*                                                               03S0215 00882000
*         /***********************************************************/ 00883000
*         /*                                                         */ 00884000
*         /* INCREMENT THE COUNT OF READY TCBS                       */ 00885000
*         /*                                                         */ 00886000
*         /***********************************************************/ 00887000
*                                                               03S0216 00888000
*         RESPECIFY                                             03S0216 00889000
*          (WRKREG1,                                            03S0216 00890000
*           WRKREG2) RSTD;          /* RESTRICT REGS                 */ 00891000
*         WRKREG1=ASCBTCBS;         /* GET COUNT OF TCBS             */ 00892000
         L     WRKREG1,ASCBTCBS(,ASCBPTR)                       03S0217 00893000
*CSLOOP:                                                        03S0218 00894000
*         WRKREG2=WRKREG1+1;        /* BUMP COUNT                    */ 00895000
CSLOOP   LA    WRKREG2,1                                        03S0218 00896000
         ALR   WRKREG2,WRKREG1                                  03S0218 00897000
*         GEN CODE;                                             03S0219 00898000
         CS    WRKREG1,WRKREG2,ASCBTCBS(ASCBPTR)                        00899000
        BNE    CSLOOP                                                   00900000
*         RESPECIFY                                             03S0220 00901000
*          (WRKREG1,                                            03S0220 00902000
*           WRKREG2) UNRSTD;        /* FREE THE REGS                 */ 00903000
*       END;                                                    03S0221 00904000
*                                                               03S0221 00905000
*/********************************************************************/ 00906000
*/*  END OF QRB -                                                    */ 00907000
*/*      OUTPUT:                                                     */ 00908000
*/*        WRKREG1 (R10) - VOLATILE                                  */ 00909000
*/*        WRKREG2 (R11) - VOLATILE.                                 */ 00910000
*/********************************************************************/ 00911000
*                                                               03S0222 00912000
*   END IRBTCBIN;                                               02S0222 00913000
*                                                               02S0222 00914000
@EL00002 DS    0H                                               02S0222 00915000
@EF00002 DS    0H                                               02S0222 00916000
@ER00002 BR    @14                                              02S0222 00917000
*/********************************************************************/ 00918000
*/*  END OF IRBTCBIN -                                               */ 00919000
*/*      OUTPUT:                                                     */ 00920000
*/*        R2 - CONTAINS TRUE ADDR OF IQE,RQE,OR SRB.                */ 00921000
*/*        R10,R11 - VOLATILE.                                       */ 00922000
*/********************************************************************/ 00923000
*                                                               02S0223 00924000
*   END IEA0EF03                                                01S0223 00925000
*                                                               01S0223 00926000
*/********************************************************************/ 00927000
*                                                               01S0223 00928000
*                                                               01S0223 00929000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 00930000
