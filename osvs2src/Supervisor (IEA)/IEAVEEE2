*   /*                                                               */ 03420000
*   /* EXECUTABLE CODE                                               */ 03421000
*   /*                                                               */ 03422000
*   /*****************************************************************/ 03423000
*                                                               01S0059 03424000
*                                                               02S0059 03425000
*   /*****************************************************************/ 03426000
*   /*                                                               */ 03427000
*   /* STAGE2 - THIS SEGMENT IS THE MAINLINE EXECUTABLE CODE. SEE THE*/ 03428000
*   /* MODULE PROLOG FOR DESCRIPTION OF INPUTS. PERFORM              */ 03429000
*   /* INITIALIZATION                                                */ 03430000
*   /*                                                               */ 03431000
*   /*****************************************************************/ 03432000
*                                                               02S0059 03433000
*                                                               03S0059 03434000
*/********************************************************************  03435000
*                                                                    */ 03436000
*/********************************************************************  03437000
*                                                                    */ 03438000
*/*  INIT2-                                                          */ 03439000
*/*  THIS SEGMENT SAVES STATUS AND ESTABLISHES AN FRR.               */ 03440000
*/*      INPUT:                                                      */ 03441000
*/*        R0-R9,R11-R15  - AS PASSED FROM USER. THESE WILL          */ 03442000
*/*                 BE SAVED.                                        */ 03443000
*/********************************************************************  03444000
*                                                                    */ 03445000
*                                                               03S0059 03446000
*   GEN CODE REFS(PSAAOLD,ASXBSPSA,ASCBASXB,WSALS2EE,RECADDR,R12,R13,   03447000
*       R10,ASXB,ASCB,WSAL,PSACSTK);                            03S0059 03448000
        L   R10,PSAAOLD                GET CURRENT ASCB ADDR            03449000
        L   R10,ASCBASXB-ASCB(R10)     GET CURRENT ASXB                 03450000
        L   R10,ASXBSPSA-ASXB(R10)     GET ADDR OF WSA VECTOR TBL       03451000
        L   R10,WSALS2EE-WSAL(R10)     GET WORK AREA ADDR               03452000
        STM 0,15,0(R10)                SAVE REGISTERS                   03453000
        LR   WAPTR,R10                 SAVE WORKAREA ADDR               03454000
        BALR  10,0                     ESTABLISH BASE                   03455000
        USING *,10                                                      03456000
IEAVEEE2  MODID  BR=YES                                                 03457000
*   ASCBPTR=PSAAOLD;                /* GET CURRENT ASCB ADDR         */ 03458000
         L     ASCBPTR,PSAAOLD                                  03S0060 03459000
*   ASXBPTR=ASCBASXB;               /* GET CURRENT ASXB ADDR         */ 03460000
         L     ASXBPTR,ASCBASXB(,ASCBPTR)                       03S0061 03461000
*/********************************************************************  03462000
*                                                                    */ 03463000
*/*  END OF INIT2 -                                                  */ 03464000
*/*      OUTPUT:                                                     */ 03465000
*/*        R10 - CONTAINS BASE ADDRESS                               */ 03466000
*/*        WAPTR (R3) - CONTAINS ADDR OF WORK SAVE AREA.             */ 03467000
*/*        ASCBPTR (R8) - CONTAINS ADDR OF CURRENT ASCB.             */ 03468000
*/*        ASXBPTR (R7) - CONTAINS ADDR OF CURRENT ASXB.             */ 03469000
*/*        THE SAVE AREA POINTED TO BY WAPTR CONTAINS THE            */ 03470000
*/*             CALLERS REGISTERS.                                   */ 03471000
*/********************************************************************  03472000
*                                                                    */ 03473000
*                                                               03S0062 03474000
*   IF R1=0                         /* IF SRBS ARE PASSED            */ 03475000
*     THEN                                                      02S0062 03476000
         SLR   @12,@12                                          02S0062 03477000
         CR    R1,@12                                           02S0062 03478000
         BNE   @RF00062                                         02S0062 03479000
*     DO;                                                       02S0063 03480000
*                                                               02S0063 03481000
*       /*************************************************************/ 03482000
*       /*                                                           */ 03483000
*       /* QUEUE THE SRB TO THE ASYNCHRONOUS EXIT QUEUE              */ 03484000
*       /*                                                           */ 03485000
*       /*************************************************************/ 03486000
*                                                               02S0064 03487000
*                                                               03S0064 03488000
*/********************************************************************  03489000
*                                                                    */ 03490000
*/*  QSRB -                                                          */ 03491000
*/*  THIS SEGMENT QUEUES THE SRB TO THE ASYNCHRONOUS EXIT QUEUE      */ 03492000
*/*      INPUT:                                                      */ 03493000
*/*        R0 - CONTAINS ADDR OF SRB TO BE ENQUEUED.                 */ 03494000
*/*        ASXBPTR (R7) -CONTAINS ADDR OF CURRENT ASXB.              */ 03495000
*/********************************************************************  03496000
*                                                                    */ 03497000
*                                                               03S0064 03498000
*       SRBPTR=R0;                  /* GET ADDRESS OF SRB            */ 03499000
         LR    SRBPTR,R0                                        03S0064 03500000
*       IF ASXBLSRB=0               /* IF Q IS EMPTY                 */ 03501000
*         THEN                                                  03S0065 03502000
         C     @12,ASXBLSRB(,ASXBPTR)                           03S0065 03503000
         BNE   @RF00065                                         03S0065 03504000
*         DO;                                                   03S0066 03505000
*           ASXBFSRB=SRBPTR;        /* POINT TO FIRST                */ 03506000
         ST    SRBPTR,ASXBFSRB(,ASXBPTR)                        03S0067 03507000
*           ASXBLSRB=SRBPTR;        /* AND LAST                      */ 03508000
         ST    SRBPTR,ASXBLSRB(,ASXBPTR)                        03S0068 03509000
*         END;                                                  03S0069 03510000
*       ELSE                                                    03S0070 03511000
*         DO;                                                   03S0070 03512000
         B     @RC00065                                         03S0070 03513000
@RF00065 DS    0H                                               03S0071 03514000
*           ASXBLSRB->SRBFLNK=SRBPTR;/* CHAIN OFF LAST SRB           */ 03515000
         L     @12,ASXBLSRB(,ASXBPTR)                           03S0071 03516000
         ST    SRBPTR,SRBFLNK(,@12)                             03S0071 03517000
*           ASXBLSRB=SRBPTR;        /* POINT TO LAST                 */ 03518000
         ST    SRBPTR,ASXBLSRB(,ASXBPTR)                        03S0072 03519000
*         END;                                                  03S0073 03520000
*       SRBFLNK=0;                  /* ZERO CHAIN FIELD              */ 03521000
@RC00065 SLR   @12,@12                                          03S0074 03522000
         ST    @12,SRBFLNK(,SRBPTR)                             03S0074 03523000
*/********************************************************************  03524000
*                                                                    */ 03525000
*/*  END OF QSRB -                                                   */ 03526000
*/*      OUTPUT:                                                     */ 03527000
*/*        SRBPTR (R1) -CONTAINS ADDR OF SRB.                        */ 03528000
*/********************************************************************  03529000
*                                                                    */ 03530000
*                                                               03S0075 03531000
*     END;                                                      02S0075 03532000
*   ELSE                                                        02S0076 03533000
*     IF R1A<0                      /* IF AN IQE IS PASSED           */ 03534000
*       THEN                                                    02S0076 03535000
         B     @RC00062                                         02S0076 03536000
@RF00062 SLR   @12,@12                                          02S0076 03537000
         CR    R1A,@12                                          02S0076 03538000
         BNL   @RF00076                                         02S0076 03539000
*       DO;                                                     02S0077 03540000
*                                                               02S0077 03541000
*         /***********************************************************/ 03542000
*         /*                                                         */ 03543000
*         /* QUEUE THE IQE TO THE ASYNCHRONOUS EXIT QUEUE            */ 03544000
*         /*                                                         */ 03545000
*         /***********************************************************/ 03546000
*                                                               02S0078 03547000
*                                                               03S0078 03548000
*/********************************************************************  03549000
*                                                                    */ 03550000
*/*  QIQE -                                                          */ 03551000
*/* THIS SEGMENT QUEUES THE IQE TO THE ASYNCHRONOUS EXIT QUEUE       */ 03552000
*/*      INPUT:                                                      */ 03553000
*/*        IQEPTR (R1) - CONTAINS THE COMPLEMENTED ADDR OF THE       */ 03554000
*/*                   IQE TO BE ENQUEUED.                            */ 03555000
*/*        ASXBPTR (R7) - CONTAINS ADDR OF THE CURRENT ASXB.         */ 03556000
*/********************************************************************  03557000
*                                                                    */ 03558000
*                                                               03S0078 03559000
*         IQEPTR=-IQEPTR;           /* GET TRUE ADDRESS OF IQE       */ 03560000
         LCR   IQEPTR,IQEPTR                                    03S0078 03561000
*         IF ASXBLIQE=0             /* IF THE QUEUE IS EMPTY         */ 03562000
*           THEN                                                03S0079 03563000
         C     @12,ASXBLIQE(,ASXBPTR)                           03S0079 03564000
         BNE   @RF00079                                         03S0079 03565000
*           DO;                                                 03S0080 03566000
*             ASXBFIQE=IQEPTR;      /* SET PTR TO FIRST IQE          */ 03567000
         ST    IQEPTR,ASXBFIQE(,ASXBPTR)                        03S0081 03568000
*             ASXBLIQE=IQEPTR;      /* SET PTR TO LAST IQE           */ 03569000
         ST    IQEPTR,ASXBLIQE(,ASXBPTR)                        03S0082 03570000
*           END;                                                03S0083 03571000
*         ELSE                                                  03S0084 03572000
*           DO;                                                 03S0084 03573000
         B     @RC00079                                         03S0084 03574000
@RF00079 DS    0H                                               03S0085 03575000
*             ASXBLIQE->IQELNKA=IQEPTR;/* SET PTR IN LAST IQE        */ 03576000
         L     @12,ASXBLIQE(,ASXBPTR)                           03S0085 03577000
         STCM  IQEPTR,7,IQELNKA(@12)                            03S0085 03578000
*             ASXBLIQE=IQEPTR;      /* SET PTR TO LAST               */ 03579000
         ST    IQEPTR,ASXBLIQE(,ASXBPTR)                        03S0086 03580000
*           END;                                                03S0087 03581000
*         IQELNKA=0;                /* CLEAR FORWARD CHAIN IN LAST   */ 03582000
@RC00079 SLR   @12,@12                                          03S0088 03583000
         STCM  @12,7,IQELNKA(IQEPTR)                            03S0088 03584000
*/********************************************************************  03585000
*                                                                    */ 03586000
*/*  END OF QIQE -                                                   */ 03587000
*/*      OUTPUT:                                                     */ 03588000
*/*        IQEPTR (R1) - CONTAINS TRUE ADDR OF THE IQE.              */ 03589000
*/********************************************************************  03590000
*                                                                    */ 03591000
*                                                               03S0089 03592000
*       END;                                                    02S0089 03593000
*     ELSE                                                      02S0090 03594000
*       DO;                                                     02S0090 03595000
*                                                               02S0090 03596000
         B     @RC00076                                         02S0090 03597000
@RF00076 DS    0H                                               02S0091 03598000
*         /***********************************************************/ 03599000
*         /*                                                         */ 03600000
*         /* QUEUE THE RQE TO THE ASYNCHRONOUS EXIT QUEUE            */ 03601000
*         /*                                                         */ 03602000
*         /***********************************************************/ 03603000
*                                                               02S0091 03604000
*                                                               03S0091 03605000
*/********************************************************************  03606000
*                                                                    */ 03607000
*/*  QRQE -                                                          */ 03608000
*/* THIS SEGMENT QUEUES THE RQE TO THE ASYNCHRONOUS EXIT QUEUE       */ 03609000
*/*      INPUT:                                                      */ 03610000
*/*        RQEPTR    (R1) -CONTAINS ADDR OF RQE TO BE ENQUEUED.      */ 03611000
*/*        ASXBPTR (R7) - CONTAINS ADDR OF CURRENT ASXB.             */ 03612000
*/********************************************************************  03613000
*                                                                    */ 03614000
*                                                               03S0091 03615000
*         IF ASXBLRQE=0             /* IF THERE ARE NO RQES ON Q     */ 03616000
*           THEN                                                03S0091 03617000
         ICM   @12,15,ASXBLRQE(ASXBPTR)                         03S0091 03618000
         BNZ   @RF00091                                         03S0091 03619000
*           DO;                                                 03S0092 03620000
*             ASXBFRQE=RQEPTR;      /* POINT TO FIRST                */ 03621000
         ST    RQEPTR,ASXBFRQE(,ASXBPTR)                        03S0093 03622000
*             ASXBLRQE=RQEPTR;      /* POINT TO LAST                 */ 03623000
         ST    RQEPTR,ASXBLRQE(,ASXBPTR)                        03S0094 03624000
*           END;                                                03S0095 03625000
*         ELSE                                                  03S0096 03626000
*           DO;                                                 03S0096 03627000
         B     @RC00091                                         03S0096 03628000
@RF00091 DS    0H                                               03S0097 03629000
*             ASXBLRQE->RQENRQE=RQEPTR;/* CHAIN OFF LAST RQE         */ 03630000
         L     @12,ASXBLRQE(,ASXBPTR)                           03S0097 03631000
         ST    RQEPTR,RQENRQE(,@12)                             03S0097 03632000
*             ASXBLRQE=RQEPTR;      /* POINT TO LAST                 */ 03633000
         ST    RQEPTR,ASXBLRQE(,ASXBPTR)                        03S0098 03634000
*           END;                                                03S0099 03635000
*         RQENRQE=0;                /* ZERO CHAIN FIELD              */ 03636000
@RC00091 SLR   @12,@12                                          03S0100 03637000
         ST    @12,RQENRQE(,RQEPTR)                             03S0100 03638000
*         RQETCB=DEBTCBB;           /* COPY TCB ADDR INTO A FIXED       03639000
*                                      AREA                  @YM05064*/ 03640000
         L     @12,DEBPTR(,RQEPTR)                              03S0101 03641000
         SLR   @11,@11                                          03S0101 03642000
         ICM   @11,7,DEBTCBB(@12)                               03S0101 03643000
         ST    @11,RQETCB(,RQEPTR)                              03S0101 03644000
*         RQERRQ=DEBIRBB;           /* COPY IRB ADDR INTO A FIXED       03645000
*                                      AREA                  @YM05064*/ 03646000
         SLR   @11,@11                                          03S0102 03647000
         ICM   @11,7,DEBIRBB(@12)                               03S0102 03648000
         ST    @11,RQERRQ(,RQEPTR)                              03S0102 03649000
*/********************************************************************  03650000
*                                                                    */ 03651000
*/*  END OF QRQE -                                                   */ 03652000
*/*      OUTPUT:  NONE.                                              */ 03653000
*/********************************************************************  03654000
*                                                                    */ 03655000
*                                                               03S0103 03656000
*       END;                                                    02S0103 03657000
*   ASCBS3S=ON;                     /* TURN ON THE STAGE 3 SWITCH    */ 03658000
@RC00076 DS    0H                                               02S0104 03659000
@RC00062 OI    ASCBS3S(ASCBPTR),B'00100000'                     02S0104 03660000
*   ASCBS2S=ON;                     /* TURN ON THE STAGE 2 SWITCH    */ 03661000
*                                                               02S0105 03662000
         OI    ASCBS2S(ASCBPTR),B'00010000'                     02S0105 03663000
*   /*****************************************************************/ 03664000
*   /*                                                               */ 03665000
*   /* RESTORE REGISTERS                                             */ 03666000
*   /*                                                               */ 03667000
*   /*****************************************************************/ 03668000
*                                                               02S0106 03669000
*                                                               03S0106 03670000
*/********************************************************************  03671000
*                                                                    */ 03672000
*/*   RESREG  -                                                      */ 03673000
*/*   THIS SEGMENT RESTORES THE CALLERS REGISTERS,                   */ 03674000
*/*    AND RETURNS TO THE CALLER VIA A BR14.                         */ 03675000
*/*      INPUT:                                                      */ 03676000
*/*        R1   -CONTAINS TRUE ADDR OF IQE ,RQE,OR SRB               */ 03677000
*/*        WAPTR (R3) -CONTAINS ADDR OF WORK SAVE AREA.              */ 03678000
*/*              THIS WORK SAVE AREA CONTAINS THE CALLERS            */ 03679000
*/*               REGISTERS.                                         */ 03680000
*/********************************************************************  03681000
*                                                                    */ 03682000
*                                                               03S0106 03683000
*   GEN CODE REFS(PSACSTK,R11,R12);                             03S0106 03684000
*                                                               03S0106 03685000
        L     0,0(WAPTR)           RESTORE REGISTER 0                   03686000
        LM    2,15,8(WAPTR)        RESTORE REGISTERS 2-15               03687000
        BR     14                                                       03688000
*/********************************************************************  03689000
*                                                                    */ 03690000
*/*  END OF RESREG -                                                 */ 03691000
*/*      OUTPUT:                                                     */ 03692000
*/*        R1 - CONTAINS TRUE ADDR OF IQE,RQE,OR SRB                 */ 03693000
*/*        R0,R2-R9,R11-R15 - ARE RESTORED TO THE CONTENTS AT THE    */ 03694000
*/*        CALL TO STAGE 2 EXIT EFFECTOR.                            */ 03695000
*/*        THIS SEGMENT RETURNS TO THE CALLER OF STAGE 2 EXIT        */ 03696000
*/*         EFFECTOR.                                                */ 03697000
*/********************************************************************  03698000
*                                                                    */ 03699000
*                                                               03S0107 03700000
*                                                               02S0107 03701000
*/********************************************************************  03702000
*                                                                    */ 03703000
*                                                               02S0107 03704000
*   END IEA0EF00                                                01S0107 03705000
*                                                               01S0107 03706000
*/********************************************************************  03707000
*                                                                    */ 03708000
*                                                               01S0107 03709000
*                                                               01S0107 03710000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 03711000
