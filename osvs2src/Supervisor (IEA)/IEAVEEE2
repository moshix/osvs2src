*                                      BLOCK.                        */ 00282000
*      2 RQEPRT BIT(8),             /* PROTECTION KEY USED IN THE    */ 00283000
*       3 RQEPKEY BIT(4),           /* PROTECTION KEY                */ 00284000
*       3 * BIT(4),                 /* RESERVED - MUST BE ZERO       */ 00285000
*      2 RQETYPE BIT(8),            /* RQE TYPE FLAGS                */ 00286000
*       3 RQE114 BIT(1),            /* EXCPVR REQUEST                */ 00287000
*       3 RQEVIRT BIT(1),           /* VIRTUAL EXCP REQUEST          */ 00288000
*       3 RQE1TO1 BIT(1),           /* VIRTUAL = REAL EXCP REQUEST   */ 00289000
*       3 RQEVAM BIT(1),            /* VIO REQUEST                   */ 00290000
*       3 * BIT(2),                 /* RESERVED - MUST BE ZERO       */ 00291000
*       3 RQETYP3 BIT(2),           /* TYPE 3 RELATED REQUEST        */ 00292000
*        4 RQETYP2 BIT(1),          /* TYPE 2 RELATED REQUEST        */ 00293000
*        4 RQETYP1 BIT(1),          /* TYPE 1 RELATED REQUEST        */ 00294000
*      2 RQEFLAG BIT(8),            /* FLAGS INDICATING WORK TO DO      00295000
*                                      AND POSITION IN EXCP          */ 00296000
*       3 RQERETRY BIT(1),          /* RETRY REQUEST FROM APPENDAGE  */ 00297000
*       3 RQENOPST BIT(1),          /* NO POST REQUEST FROM APPEND   */ 00298000
*       3 RQENOFRE BIT(1),          /* NO FREE REQUEST FROM APPEND   */ 00299000
*       3 RQEFIXST BIT(1),          /* FIXING IS STARTED FOR REQUEST */ 00300000
*       3 RQESTBL BIT(1),           /* REQUEST IS STARTABLE          */ 00301000
*       3 RQESRBS BIT(1),           /* REQUEST SENT TO IOS           */ 00302000
*       3 RQEPURGE BIT(1),          /* PURGE IN CONTROL              */ 00303000
