*   RESPECIFY                                                      0116 00051000
*     WRKRG1 RSTD;                                                 0116 00052000
*   WRKRG1=ADDR(ESTAPRM);           /* ADDRESS OF PARM AREA          */ 00053000
         LR    WRKRG1,@12                                          0117 00054000
*   GENERATE REFS(ESTAPTR,WRKRG1);                                 0118 00055000
*                                 /* ISSUE ESTAE MACRO               */ 00056000
