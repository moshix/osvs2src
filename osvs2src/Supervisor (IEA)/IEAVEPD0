*                                                                    */ 00538000
*/*  END OF RESREG                                                   */ 00539000
*/*      OUTPUT-                                                     */ 00540000
*/*        WAPTR - (R6) -CONTAINS ADDR OF THE WORK AREA              */ 00541000
*/*        BASERG - (R9) RESTORED TO BASE VALUE                      */ 00542000
*/********************************************************************  00543000
*                                                                    */ 00544000
*                                                               04S0170 00545000
*     JRNSRB=0;                     /* CLEAR JOURNAL WORD            */ 00546000
         SR    @14,@14                                          03S0170 00547000
         ST    @14,JRNSRB(,WAPTR)                               03S0170 00548000
*   END;                                                        03S0171 00549000
@DE00158 ICM   @14,15,PURGHDR(WAPTR)                            03S0171 00550000
         BNZ   @DL00158                                         03S0171 00551000
*   RESPECIFY                                                   03S0172 00552000
*    (SRBPTR,                                                   03S0172 00553000
*     R0,                                                       03S0172 00554000
*     R1,                                                       03S0172 00555000
*     R2,                                                       03S0172 00556000
*     R15)UNRSTD;                   /* FREE THE REGS                 */ 00557000
*                                                               03S0172 00558000
*/********************************************************************  00559000
*                                                                    */ 00560000
*/* END OF GORMTR                                                    */ 00561000
*/*      OUTPUT                                                      */ 00562000
*/*        R0-R5,R7-R8,R10-R15 - VOLATILE                            */ 00563000
*/*        WAPTR - (R6) - CONTAINS ADDRESS OF WORK AREA              */ 00564000
*/*        BASERG - (R9) - CONTAINS BASE ADDRESS                     */ 00565000
*/*        PURGHDR - NOW CONTAINS ZEROES                             */ 00566000
*/********************************************************************  00567000
*                                                                    */ 00568000
*                                                               03S0173 00569000
*   RESPECIFY                                                   02S0173 00570000
*     R14 RSTD;                     /* RESTRICT THE REGISTER         */ 00571000
*   R14=RETADDR1;                                               02S0174 00572000
         L     R14,RETADDR1(,WAPTR)                             02S0174 00573000
*   RETURN;                         /* RETURN TO CALLER VIA R14      */ 00574000
@EL00001 DS    0H                                               02S0175 00575000
@EF00001 DS    0H                                               02S0175 00576000
@ER00001 BR    @14                                              02S0175 00577000
*   RESPECIFY                                                   02S0176 00578000
*     R14 UNRSTD;                   /* FREE THE REGISTER             */ 00579000
*                                                               02S0176 00580000
*/*  INCLUDE THE INTERNAL PROCEDURES                                 */ 00581000
*                                                               02S0177 00582000
*                                                               03S0177 00583000
*/********************************************************************  00584000
*                                                                    */ 00585000
*/*  DQS1Q-                                                          */ 00586000
*/*    THIS ROUTINE PURGES SRBS FROM THE GLOBAL OR LOCAL SERVICE     */ 00587000
*/*     MANAGER QUEUES                                               */ 00588000
*/*      INPUT-                                                      */ 00589000
*/*        R14 - CONTAINS THE RETURN ADDRESS                         */ 00590000
*/*        WAPTR  (R6) - CONTAINS ADDR OF WORK AREA .                */ 00591000
*/*        QPTR   (R7) - CONTAINS ADDR OF QUEUE TO BE PURGED         */ 00592000
*/*             IT WILL CONTAIN THE ADDRESS OF EITHER IEAGSMQ        */ 00593000
*/*             OR IEALSMQ .                                         */ 00594000
*/*        PURGHDR  -   POINTS TO A QUEUED LIST OF PURGED SRBS .     */ 00595000
*/********************************************************************  00596000
*                                                                    */ 00597000
*                                                               03S0177 00598000
*   RESPECIFY                                                   03S0177 00599000
*    (R14,                                                      03S0177 00600000
*     QPTR)RSTD;                    /* RESTRICT REGISTERS            */ 00601000
*DQS1Q:                                                         03S0178 00602000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                            03S0178 00603000
DQS1Q    DS    0H                                               03S0179 00604000
*   RESPECIFY                                                   03S0179 00605000
*    (SRBPTR,                                                   03S0179 00606000
*     WRKREG)RSTD;                  /* RESTRICT REGISTERS            */ 00607000
*   RETADDR2=R14;                   /* SAVE RETURN ADDR              */ 00608000
         ST    R14,RETADDR2(,WAPTR)                             03S0180 00609000
*   SRBPTR=QPTR->SMQ;               /* GET ADDR OF TOP SRB           */ 00610000
         L     SRBPTR,SMQ(,QPTR)                                03S0181 00611000
*   WRKREG=0;                       /* CLEAR THE REGISTER            */ 00612000
*                                                               03S0182 00613000
         SR    WRKREG,WRKREG                                    03S0182 00614000
*/*  DEQUEUE THE ENTIRE QUEUE                                        */ 00615000
*                                                               03S0183 00616000
*   GEN CODE REFS(SRBPTR,WRKREG,QPTR);                          03S0183 00617000
CSLOOP   CS    SRBPTR,WRKREG,0(QPTR)    IF SERVICE MGR QUEUE IS         00618000
*                                       UNCHANGED, STORE ZEROES         00619000
         BNE   CSLOOP                   OTHERWISE, TRY AGAIN            00620000
*   LCCASMQJ=SRBPTR;                /* SET JOURNAL FOR QUEUE         */ 00621000
         L     @15,LCCAPTR                                      03S0184 00622000
         ST    SRBPTR,LCCASMQJ(,@15)                            03S0184 00623000
*   DO WHILE SRBPTR^=0;             /* WHILE THERE ARE ANY MORE SRBS */ 00624000
*                                                               03S0185 00625000
         B     @DE00185                                         03S0185 00626000
@DL00185 DS    0H                                               03S0186 00627000
*     /***************************************************************/ 00628000
*     /*                                                             */ 00629000
*     /* MATCH THE SRB AGAINST INPUT PARAMETERS                      */ 00630000
*     /*                                                             */ 00631000
*     /***************************************************************/ 00632000
*                                                               03S0186 00633000
*                                                               04S0186 00634000
*/********************************************************************  00635000
*                                                                    */ 00636000
*/*  MATCHSRB-                                                       */ 00637000
*/*    THIS ROUTINE DETERMINES WHETHER  A SPECIFIED SRB MATCHES THE  */ 00638000
*/*      THE PARAMETER INPUT.                                        */ 00639000
*/*      INPUT-                                                      */ 00640000
*/*        WAPTR (R6) -CONTAINS ADDR OF WORK AREA                    */ 00641000
*/*        SRBPTR (R5)- CONTAINS ADDR OF SRB TO BE TESTED            */ 00642000
*/*                                                                  */ 00643000
*/*      FIELDS REFERENCED-                                          */ 00644000
*/*        PDQ2RMTR,PDQ2AS,PDQ2TCB,PDQ2ASCB                          */ 00645000
*/*        SRBRMTR,SRBASCB,SRBPASID,SRBPTCB                          */ 00646000
*/********************************************************************  00647000
*                                                                    */ 00648000
*                                                               04S0186 00649000
*     MATCH=NO;                     /* TURN OFF SWITCH               */ 00650000
         NI    MATCH(WAPTR),B'01111111'                         04S0186 00651000
*     IF(PDQ2RMTR=0|PDQ2RMTR=SRBRMTR)/* RMTR=0 OR A MATCH            */ 00652000
*       THEN                                                    04S0187 00653000
         L     @15,PDQ2RMTR(,WAPTR)                             04S0187 00654000
         LTR   @15,@15                                          04S0187 00655000
         BZ    @RT00187                                         04S0187 00656000
         C     @15,SRBRMTR(,SRBPTR)                             04S0187 00657000
         BNE   @RF00187                                         04S0187 00658000
@RT00187 DS    0H                                               04S0188 00659000
*       IF(PDQ2AS=0|PDQ2AS=SRBPASID)/* ASID IN ASIDTCB=0 OR A MATCH  */ 00660000
*         THEN                                                  04S0188 00661000
         SR    @15,@15                                          04S0188 00662000
         ICM   @15,3,PDQ2AS(WAPTR)                              04S0188 00663000
         LTR   @15,@15                                          04S0188 00664000
         BZ    @RT00188                                         04S0188 00665000
         CH    @15,SRBPASID(,SRBPTR)                            04S0188 00666000
         BNE   @RF00188                                         04S0188 00667000
@RT00188 DS    0H                                               04S0189 00668000
*         IF(PDQ2TCB=0|PDQ2TCB=SRBPTCB)/* TCB=0 OR TCB MATCH         */ 00669000
*           THEN                                                04S0189 00670000
         L     @15,PDQ2TCB(,WAPTR)                              04S0189 00671000
         LTR   @15,@15                                          04S0189 00672000
         BZ    @RT00189                                         04S0189 00673000
         C     @15,SRBPTCB(,SRBPTR)                             04S0189 00674000
         BNE   @RF00189                                         04S0189 00675000
@RT00189 DS    0H                                               04S0190 00676000
*           IF PDQ2ASCB=SRBASCB     /* ASID MATCH                    */ 00677000
*             THEN                                              04S0190 00678000
         CLC   PDQ2ASCB(4,WAPTR),SRBASCB(SRBPTR)                04S0190 00679000
         BNE   @RF00190                                         04S0190 00680000
*             MATCH=YES;            /* INDICATE A MATCH TO CALLER    */ 00681000
*                                                               04S0191 00682000
         OI    MATCH(WAPTR),B'10000000'                         04S0191 00683000
*/********************************************************************  00684000
*                                                                    */ 00685000
*/*  END OF MATCHSRB-                                                */ 00686000
*/*      OUTPUT-                                                     */ 00687000
*/*        MATCH = YES   IF SRB MATCHES INPUT                        */ 00688000
*/*              = NO    IF SRB DOES NOT MATCH INPUT                 */ 00689000
*/********************************************************************  00690000
*                                                                    */ 00691000
*                                                               04S0192 00692000
*     WRKREG=SRBPTR;                /* COPY SRB ADDR                 */ 00693000
@RF00190 DS    0H                                               03S0192 00694000
@RF00189 DS    0H                                               03S0192 00695000
@RF00188 DS    0H                                               03S0192 00696000
@RF00187 LR    WRKREG,SRBPTR                                    03S0192 00697000
*     SRBPTR=SRBFLNK;               /* GET NEXT SRB                  */ 00698000
         L     SRBPTR,SRBFLNK(,SRBPTR)                          03S0193 00699000
*     LCCASMQJ=SRBPTR;              /* UPDATE JOURNAL WORD           */ 00700000
         L     @15,LCCAPTR                                      03S0194 00701000
         ST    SRBPTR,LCCASMQJ(,@15)                            03S0194 00702000
*     RESPECIFY                                                 03S0195 00703000
*      (R0,                                                     03S0195 00704000
*       R1,                                                     03S0195 00705000
*       R15)RSTD;                   /* RESTRICT REGISTERS            */ 00706000
*     IF MATCH=YES                  /* IF THE SRB MATCHES THE   03S0196 00707000
*                                      PARAMETER INPUTS              */ 00708000
*       THEN                                                    03S0196 00709000
         TM    MATCH(WAPTR),B'10000000'                         03S0196 00710000
         BNO   @RF00196                                         03S0196 00711000
*       DO;                                                     03S0197 00712000
*         WRKREG->SRBFLNK=PURGHDR;  /* PLACE SRB ON A                */ 00713000
