@EF00002 DS    0H                                               03S0209 00754000
@ER00002 BR    @14                                              03S0209 00755000
*/********************************************************************  00756000
*                                                                    */ 00757000
*/*  END OF DQS1Q-                                                   */ 00758000
*/*      OUTPUT-                                                     */ 00759000
*/*        R0,R1,R15 - VOLATILE                                      */ 00760000
*/*        SRBPTR - (R5) - VOLATILE                                  */ 00761000
*/*        WRKREG - (R3) - VOLATILE                                  */ 00762000
*/*        RETADDR2,MATCH - VOLATILE                                 */ 00763000
*/*        WAPTR(R6), BASERG(R9),QPTR(R7) - UNCHANGED                */ 00764000
*/*        R2,R4,RR8,R10,R11,R12,R13 - ARE UNRESTRICTED              */ 00765000
*/*                           AND THEREFORE VOLATILE                 */ 00766000
*/********************************************************************  00767000
*                                                                    */ 00768000
*                                                               03S0210 00769000
*                                                               03S0210 00770000
*/********************************************************************  00771000
*                                                                    */ 00772000
*/*  DQS2Q -                                                         */ 00773000
*/*    THIS ROUTINE PURGES A SPECIFIED LEVEL OF AN SPL.              */ 00774000
*/*         IT SCANS THE SRBS QUEUED OFF THE PRIORITY LEVEL          */ 00775000
*/*          SPECIFIED BY THE CALLER.  THOSE THAT MATCH THE PARAMETER*/ 00776000
*/*         INPUTS ARE PLACED ON THE QUEUE ANCHORED OUT OF PURGHDR.  */ 00777000
*/*      INPUT-                                                      */ 00778000
*/*        R14 - RETURN ADDRESS                                      */ 00779000
*/*        WAPTR (R6) -CONTAINS ADDRESS OF WORK AREA                 */ 00780000
*/*        QPTR  (R7) - ADDRESS OF ENTRY IN SPL                      */ 00781000
*/*        PURGHDR -  POINTS TO A QUEUE OF PURGED SRBS               */ 00782000
*/********************************************************************  00783000
*                                                                    */ 00784000
*                                                               03S0210 00785000
*   RESPECIFY                                                   03S0210 00786000
*    (QPTR,                                                     03S0210 00787000
*     R14,                                                      03S0210 00788000
*     WRKREG,                                                   03S0210 00789000
*     SRBPTR)RSTD;                                              03S0210 00790000
*DQS2Q:                                                         03S0211 00791000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                            03S0211 00792000
DQS2Q    DS    0H                                               03S0212 00793000
*   SRBPTR=QPTR->SPLFSRB;           /* GET FIRST SRB                 */ 00794000
         L     SRBPTR,SPLFSRB(,QPTR)                            03S0212 00795000
*   LASTSRB=0;                      /* SET PTR TO PREVIOUS SRB       */ 00796000
         SR    @15,@15                                          03S0213 00797000
         ST    @15,LASTSRB(,WAPTR)                              03S0213 00798000
*   DO WHILE SRBPTR^=0;             /* WHILE THERE ARE UNSEARCHED       00799000
*                                      SRBS                          */ 00800000
*                                                               03S0214 00801000
         B     @DE00214                                         03S0214 00802000
@DL00214 DS    0H                                               03S0215 00803000
*     /***************************************************************/ 00804000
*     /*                                                             */ 00805000
*     /* MATCH THE SRB AGAINST INPUT PARAMETERS                      */ 00806000
*     /*                                                             */ 00807000
*     /***************************************************************/ 00808000
*                                                               03S0215 00809000
*                                                               04S0215 00810000
*/********************************************************************  00811000
*                                                                    */ 00812000
*/*  MATCHSRB-                                                       */ 00813000
*/*    THIS ROUTINE DETERMINES WHETHER  A SPECIFIED SRB MATCHES THE  */ 00814000
*/*      THE PARAMETER INPUT.                                        */ 00815000
*/*      INPUT-                                                      */ 00816000
*/*        WAPTR (R6) -CONTAINS ADDR OF WORK AREA                    */ 00817000
*/*        SRBPTR (R5)- CONTAINS ADDR OF SRB TO BE TESTED            */ 00818000
*/*                                                                  */ 00819000
*/*      FIELDS REFERENCED-                                          */ 00820000
*/*        PDQ2RMTR,PDQ2AS,PDQ2TCB,PDQ2ASCB                          */ 00821000
*/*        SRBRMTR,SRBASCB,SRBPASID,SRBPTCB                          */ 00822000
*/********************************************************************  00823000
*                                                                    */ 00824000
*                                                               04S0215 00825000
*     MATCH=NO;                     /* TURN OFF SWITCH               */ 00826000
         NI    MATCH(WAPTR),B'01111111'                         04S0215 00827000
*     IF(PDQ2RMTR=0|PDQ2RMTR=SRBRMTR)/* RMTR=0 OR A MATCH            */ 00828000
*       THEN                                                    04S0216 00829000
         L     @15,PDQ2RMTR(,WAPTR)                             04S0216 00830000
         LTR   @15,@15                                          04S0216 00831000
         BZ    @RT00216                                         04S0216 00832000
         C     @15,SRBRMTR(,SRBPTR)                             04S0216 00833000
         BNE   @RF00216                                         04S0216 00834000
@RT00216 DS    0H                                               04S0217 00835000
*       IF(PDQ2AS=0|PDQ2AS=SRBPASID)/* ASID IN ASIDTCB=0 OR A MATCH  */ 00836000
*         THEN                                                  04S0217 00837000
         SR    @15,@15                                          04S0217 00838000
