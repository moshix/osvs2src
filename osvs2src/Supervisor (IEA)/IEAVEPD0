         BZ    @RT00217                                         04S0217 00841000
         CH    @15,SRBPASID(,SRBPTR)                            04S0217 00842000
         BNE   @RF00217                                         04S0217 00843000
@RT00217 DS    0H                                               04S0218 00844000
*         IF(PDQ2TCB=0|PDQ2TCB=SRBPTCB)/* TCB=0 OR TCB MATCH         */ 00845000
*           THEN                                                04S0218 00846000
         L     @15,PDQ2TCB(,WAPTR)                              04S0218 00847000
         LTR   @15,@15                                          04S0218 00848000
         BZ    @RT00218                                         04S0218 00849000
         C     @15,SRBPTCB(,SRBPTR)                             04S0218 00850000
         BNE   @RF00218                                         04S0218 00851000
@RT00218 DS    0H                                               04S0219 00852000
*           IF PDQ2ASCB=SRBASCB     /* ASID MATCH                    */ 00853000
*             THEN                                              04S0219 00854000
         CLC   PDQ2ASCB(4,WAPTR),SRBASCB(SRBPTR)                04S0219 00855000
         BNE   @RF00219                                         04S0219 00856000
*             MATCH=YES;            /* INDICATE A MATCH TO CALLER    */ 00857000
*                                                               04S0220 00858000
         OI    MATCH(WAPTR),B'10000000'                         04S0220 00859000
*/********************************************************************  00860000
*                                                                    */ 00861000
*/*  END OF MATCHSRB-                                                */ 00862000
*/*      OUTPUT-                                                     */ 00863000
*/*        MATCH = YES   IF SRB MATCHES INPUT                        */ 00864000
*/*              = NO    IF SRB DOES NOT MATCH INPUT                 */ 00865000
*/********************************************************************  00866000
*                                                                    */ 00867000
*                                                               04S0221 00868000
*     IF MATCH=YES                  /* IF IT MATCHES                 */ 00869000
*       THEN                                                    03S0221 00870000
@RF00219 DS    0H                                               03S0221 00871000
@RF00218 DS    0H                                               03S0221 00872000
@RF00217 DS    0H                                               03S0221 00873000
@RF00216 TM    MATCH(WAPTR),B'10000000'                         03S0221 00874000
         BNO   @RF00221                                         03S0221 00875000
*       DO;                                                     03S0222 00876000
*                                                               04S0223 00877000
*/********************************************************************  00878000
*                                                                    */ 00879000
*/*  DEQSRB -                                                        */ 00880000
*/*      THIS ROUTINE DEQUEUES A SPECIFIED SRB FROM THE SPL          */ 00881000
*/*      INPUT -                                                     */ 00882000
*/*        WAPTR - (R6) - CONTAINS ADDRESS OF WORK AREA              */ 00883000
*/*        SRBPTR - (R5) - CONTAINS ADDRESS OF SRB TO BE DEQUEUED    */ 00884000
*/*        QPTR - (R7)  CONTAINS ADDRESS OF ENTRY IN SPL ON WHICH    */ 00885000
*/*               THE SRB IS ENQUEUED.                               */ 00886000
*/*        LASTSRB - CONTAINS ADDRESS OF PREVIOUS SRB,OR IF          */ 00887000
*/*             THIS IS THE TOP SRB, THEN ZERO                       */ 00888000
*/*      FIELDS REFERENCED-                                          */ 00889000
*/*        SRBFLNK                                                   */ 00890000
*/*        SPLLSRB                                                   */ 00891000
*/********************************************************************  00892000
*                                                                    */ 00893000
*                                                               04S0223 00894000
*         IF LASTSRB=0              /* IF NO PREVIOUS SRB            */ 00895000
*           THEN                                                04S0223 00896000
         ICM   @15,15,LASTSRB(WAPTR)                            04S0223 00897000
         BNZ   @RF00223                                         04S0223 00898000
*           QPTR->SPLFSRB=SRBFLNK;  /* CHANGE FIRST SRB ADDR         */ 00899000
         MVC   SPLFSRB(4,QPTR),SRBFLNK(SRBPTR)                  04S0224 00900000
*         ELSE                                                  04S0225 00901000
*           LASTSRB->SRBFLNK=SRBPTR->/* SET FORWARD CHAIN IN         */ 00902000
*               SRBFLNK;            /* PREVIOUS SRB                  */ 00903000
         B     @RC00223                                         04S0225 00904000
@RF00223 L     @15,LASTSRB(,WAPTR)                              04S0225 00905000
         MVC   SRBFLNK(4,@15),SRBFLNK(SRBPTR)                   04S0225 00906000
*         IF QPTR->SPLLSRB=SRBPTR   /* IF THIS WAS THE LAST SRB      */ 00907000
*           THEN                                                04S0226 00908000
@RC00223 C     SRBPTR,SPLLSRB(,QPTR)                            04S0226 00909000
         BNE   @RF00226                                         04S0226 00910000
*           QPTR->SPLLSRB=LASTSRB;  /* ZERO THE POINTER TO THE LAST     00911000
*                                      SRB                           */ 00912000
*                                                               04S0227 00913000
         MVC   SPLLSRB(4,QPTR),LASTSRB(WAPTR)                   04S0227 00914000
*/********************************************************************  00915000
*                                                                    */ 00916000
*/*  END OF DEQSRB-                                                  */ 00917000
*/*      FIELDS CHANGED-                                             */ 00918000
*/*        SRBFLNK                                                   */ 00919000
*/*        SPLLSRB,SPLFSRB                                           */ 00920000
*/********************************************************************  00921000
*                                                                    */ 00922000
*                                                               04S0228 00923000
*         WRKREG=SRBPTR;            /* SAVE SRB ADDR                 */ 00924000
@RF00226 LR    WRKREG,SRBPTR                                    03S0228 00925000
*         SRBPTR=SRBFLNK;           /* GET NEXT SRB                  */ 00926000
         L     SRBPTR,SRBFLNK(,SRBPTR)                          03S0229 00927000
*         WRKREG->SRBFLNK=PURGHDR;  /* PUT SRB ON                    */ 00928000
         MVC   SRBFLNK(4,WRKREG),PURGHDR(WAPTR)                 03S0230 00929000
*         PURGHDR=WRKREG;           /* THE PURGE QUEUE               */ 00930000
         ST    WRKREG,PURGHDR(,WAPTR)                           03S0231 00931000
*       END;                                                    03S0232 00932000
*     ELSE                                                      03S0233 00933000
*       DO;                                                     03S0233 00934000
         B     @RC00221                                         03S0233 00935000
@RF00221 DS    0H                                               03S0234 00936000
*         LASTSRB=SRBPTR;           /* UPDATE PREVIOUS SRB PTR       */ 00937000
         ST    SRBPTR,LASTSRB(,WAPTR)                           03S0234 00938000
*         SRBPTR=SRBFLNK;           /* UPDATE TO NEXT SRB            */ 00939000
         L     SRBPTR,SRBFLNK(,SRBPTR)                          03S0235 00940000
*       END;                                                    03S0236 00941000
*   END;                                                        03S0237 00942000
@RC00221 DS    0H                                               03S0237 00943000
@DE00214 LTR   SRBPTR,SRBPTR                                    03S0237 00944000
         BNZ   @DL00214                                         03S0237 00945000
*   END DQS2Q;                                                  03S0238 00946000
@EL00003 DS    0H                                               03S0238 00947000
@EF00003 DS    0H                                               03S0238 00948000
@ER00003 BR    @14                                              03S0238 00949000
@PB00003 DS    0H                                               03S0239 00950000
*   RESPECIFY                                                   03S0239 00951000
*    (QPTR,                                                     03S0239 00952000
*     R14,                                                      03S0239 00953000
*     WRKREG,                                                   03S0239 00954000
*     SRBPTR)UNRSTD;                                            03S0239 00955000
*                                                               03S0239 00956000
*/********************************************************************  00957000
*                                                                    */ 00958000
*/*  END OF DQS2Q                                                    */ 00959000
*/*     OUTPUT-                                                      */ 00960000
*/*      R3,R5- VOLATILE                                             */ 00961000
*/********************************************************************  00962000
*                                                                    */ 00963000
*                                                               03S0240 00964000
*                                                               02S0240 00965000
*/* INCLUDE THE SECTION OF CODE THAT EXITS TO ABEND                  */ 00966000
*                                                               02S0240 00967000
*                                                               03S0240 00968000
*/********************************************************************  00969000
*                                                                    */ 00970000
*/*  ABND -                                                          */ 00971000
*/*  THIS ROUTINE HANDLES THE ABEND INTERFACE                        */ 00972000
*/*      INPUT-                                                      */ 00973000
*/*        WAPTR - (R6) - CONTAINS ADDRESS OF WORK AREA              */ 00974000
*/*        ABCODER (R3) - CONTAINS ABEND CODE,RIGHT JUSTIFIED        */ 00975000
*/********************************************************************  00976000
*                                                                    */ 00977000
*                                                               03S0240 00978000
*   RESPECIFY                                                   03S0240 00979000
*    (ABCODER,                                                  03S0240 00980000
*     R0,                                                       03S0240 00981000
*     R1,                                                       03S0240 00982000
*     R15)RSTD;                     /* RESTRICT REGS TO BE USED      */ 00983000
*ABND1:                                                         03S0241 00984000
*   ;                                                           03S0241 00985000
ABND1    DS    0H                                               03S0242 00986000
*   GEN CODE;                                                   03S0242 00987000
*                                                               03S0242 00988000
         ESTAE  0                                                       00989000
         ABEND (ABCODER),,,SYSTEM                                       00990000
*/********************************************************************  00991000
*                                                                    */ 00992000
*/* END OF ABND                                                      */ 00993000
*/*      OUTPUT-                                                     */ 00994000
*/*        THERE IS NO RETURN FROM THIS SEGMENT                      */ 00995000
*/********************************************************************  00996000
*                                                                    */ 00997000
*                                                               03S0243 00998000
*   END IGC123                                                  02S0243 00999000
*                                                               02S0243 01000000
*/********************************************************************  01001000
*                                                                    */ 01002000
*                                                               02S0243 01003000
*                                                               02S0243 01004000
