*                                                               03S0156 00434000
*     END;                                                      02S0156 00435000
*                                                               02S0156 00436000
*/* ROUTE CONTROL TO ALL OF THE RMTR ROUTINES FOR EACH DEQUEUED SRB  */ 00437000
*                                                               02S0157 00438000
*                                                               03S0157 00439000
*/********************************************************************  00440000
*                                                                    */ 00441000
*/*  GORMTR -                                                        */ 00442000
*/*    THIS ROUTINE ROUTES CONTROL TO THE RMTR ROUTINES FOR THE      */ 00443000
*/*     PURGED SRBS  .  IT REMOVES ALL SRBS QUEUED OUT OF PURGHDR AND*/ 00444000
*/*      ROUTES CONTROL TO THE ROUTINE SPECIFIED IN SRBRMTR.         */ 00445000
*/*      INPUT-                                                      */ 00446000
*/*        WAPTR (R6) - CONTAINS ADDRESS OF THE WORK AREA            */ 00447000
*/*        PURGHDR - POINTS TO A LIFO CHAIN OF PURGED SRBS           */ 00448000
*/********************************************************************  00449000
*                                                                    */ 00450000
*                                                               03S0157 00451000
*   RESPECIFY                                                   03S0157 00452000
*    (SRBPTR,                                                   03S0157 00453000
*     R0,                                                       03S0157 00454000
*     R1,                                                       03S0157 00455000
*     R2,                                                       03S0157 00456000
*     R15)RSTD;                     /* RESTRICT THE REGS             */ 00457000
@RF00150 DS    0H                                               03S0158 00458000
*   DO WHILE PURGHDR^=0;            /* WHILE THERE ARE ANY MORE 03S0158 00459000
*                                      PURGED SRBS AROUND            */ 00460000
         B     @DE00158                                         03S0158 00461000
@DL00158 DS    0H                                               03S0159 00462000
*     SRBPTR=PURGHDR;               /* DEQUEUE THE TOP SRB FROM      */ 00463000
         L     SRBPTR,PURGHDR(,WAPTR)                           03S0159 00464000
*     PURGHDR=SRBFLNK;              /* THE PURGE QUEUE               */ 00465000
*                                                               03S0160 00466000
         MVC   PURGHDR(4,WAPTR),SRBFLNK(SRBPTR)                 03S0160 00467000
*     /***************************************************************/ 00468000
*     /*                                                             */ 00469000
*     /* SAVE REGISTERS                                              */ 00470000
*     /*                                                             */ 00471000
*     /***************************************************************/ 00472000
*                                                               03S0161 00473000
*                                                               04S0161 00474000
*/********************************************************************  00475000
*                                                                    */ 00476000
*/*   SAVEREG  -                                                     */ 00477000
*/*       THIS ROUTINE SAVES REGISTERS BEFORE GOING TO THE RMTR      */ 00478000
*/********************************************************************  00479000
*                                                                    */ 00480000
*                                                               04S0161 00481000
*     /***************************************************************/ 00482000
*     /*                                                             */ 00483000
*     /* CURRENTLY NO REGISTERS NEED BE SAVED                        */ 00484000
*     /*                                                             */ 00485000
*     /***************************************************************/ 00486000
*                                                               04S0161 00487000
*     ;                                                         04S0161 00488000
*     R0=PSATOLD->TCBRBP->RBGRS0;   /* PASS ALONG REG 0              */ 00489000
         L     @14,PSATOLD                                      03S0162 00490000
         L     @14,TCBRBP(,@14)                                 03S0162 00491000
         L     R0,RBGRS0(,@14)                                  03S0162 00492000
*     R1=SRBPTR;                    /* SET R1 TO ADDRESS OF SRB      */ 00493000
         LR    R1,SRBPTR                                        03S0163 00494000
*     JRNSRB=R1;                    /* JOURNAL SRB ADDRESS           */ 00495000
         ST    R1,JRNSRB(,WAPTR)                                03S0164 00496000
*     R2=SRBPARM;                   /* SET R2 TO PARAMETER           */ 00497000
         L     R2,SRBPARM(,SRBPTR)                              03S0165 00498000
*     R15=SRBRMTR;                  /* GET EP OF CLEANUP RTN         */ 00499000
*                                                               03S0166 00500000
         L     R15,SRBRMTR(,SRBPTR)                             03S0166 00501000
*     /***************************************************************/ 00502000
*     /*                                                             */ 00503000
*     /* INVOKE THE RMTR ROUTINE                                     */ 00504000
*     /*                                                             */ 00505000
*     /***************************************************************/ 00506000
*                                                               03S0167 00507000
*     GEN(    BALR  14,15) ;        /* CALL THE RMTR                 */ 00508000
*                                                               03S0167 00509000
             BALR  14,15                                                00510000
*     /***************************************************************/ 00511000
*     /*                                                             */ 00512000
*     /* RESTORE REGISTERS                                           */ 00513000
*     /*                                                             */ 00514000
*     /***************************************************************/ 00515000
*                                                               03S0168 00516000
*                                                               04S0168 00517000
*/********************************************************************  00518000
*                                                                    */ 00519000
*/*  RESREG                                                          */ 00520000
*/*    THIS ROUTINE RESTORES REGISTERS AFTER RETURNING FROM THE RMTR */ 00521000
*/********************************************************************  00522000
*                                                                    */ 00523000
*/* RESTORE BASE REG HERE                                            */ 00524000
*                                                               04S0168 00525000
*     GEN CODE;                                                 04S0168 00526000
           BALR   9,0                                                   00527000
           USING  *,9                                                   00528000
           L      9,BASE                                                00529000
           USING  @PSTART,9                                             00530000
*     WAPTR=ADDR(PSATOLD->TCBRBP->RBEXSAVE);/* RESTORE PTR TO WORK      00531000
*                                      AREA                          */ 00532000
*                                                               04S0169 00533000
