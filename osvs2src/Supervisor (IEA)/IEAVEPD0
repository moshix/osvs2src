@DL00137 DS    0H                                               03S0138 00312000
*     CALL DQS2Q;                   /* PURGE THAT LEVEL              */ 00313000
         BAL   @14,DQS2Q                                        03S0138 00314000
*     QPTR=QPTR+LENGTH(SPLENTRY);   /* GET NEXT LEVEL                */ 00315000
         AH    QPTR,@CH00059                                    03S0139 00316000
*   END;                                                        03S0140 00317000
@DE00137 L     @08,@CV02127                                     03S0140 00318000
         AH    @08,@CH00375                                     03S0140 00319000
         CR    QPTR,@08                                         03S0140 00320000
         BL    @DL00137                                         03S0140 00321000
*   QPTR=PDQ2ASCB->ASCBSPL;         /* GET SPL ADDR                  */ 00322000
*                                                               03S0141 00323000
