         TITLE 'IEAVEPDR -- PURGEDQ RECOVERY ROUTINE                   *00001000
                        '                                               00002000
IEAVEPDR CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                          01S0001 00008000
         DC    C'IEAVEPDR  76.341'                              01S0001 00009000
IEAVEPDF DS    0H                                               01S0001 00010000
         USING *,@15                                            01S0001 00011000
         B     @PROLOG                                          01S0001 00012000
         DC    AL2(@EP00050-IEAVEPDF)                                   00013000
         ENTRY IEAVEPDF                                                 00014000
IEAVEPDE DS    0H                                               01S0001 00015000
         USING *,@15                                            01S0001 00016000
         B     @PROLOG                                          01S0001 00017000
         DC    AL2(@EP00064-IEAVEPDE)                                   00018000
         ENTRY IEAVEPDE                                                 00019000
IEAVEPDS DS    0H                                               01S0001 00020000
         USING *,@15                                            01S0001 00021000
         B     @PROLOG                                          01S0001 00022000
         DC    AL2(@EP00109-IEAVEPDS)                                   00023000
         ENTRY IEAVEPDS                                                 00024000
         DROP  @15                                                      00025000
@PROLOG  BALR  @10,0                                            01S0001 00026000
@PSTART  DS    0H                                               01S0001 00027000
         USING @PSTART,@10                                      01S0001 00028000
         AH    @15,4(,@15)                                      01S0001 00029000
         BR    @15                                              01S0001 00030000
@EP00001 DS    0H                                               01S0002 00031000
RBPRFXLN EQU 64                                                         00032000
RBPRFLNA EQU 32                                                         00033000
PRBLEN   EQU 136                                                        00034000
SIRBLEN  EQU 200                                                        00035000
TIRBLEN  EQU 136                                                        00036000
IRBLEN   EQU 128                                                        00037000
SVRBLEN  EQU 200                                                        00038000
STAEWASZ EQU   16                                                       00039000
*                                                               01S0050 00040000
*/********************************************************************/ 00041000
*/*                                                                  */ 00042000
*/*     PURPOSE = THIS IS THE ENTRY POINT OF THE PURGE DQ FRR.  IT   */ 00043000
*/*           IS ENTERED IF AN ERROR OCCURRED DURING THE QUEUE-      */ 00044000
*/*           SCANNING PORTION OF THE PURGE DQ MAINLINE.  IT WILL    */ 00045000
*/*           ATTEMPT TO VERIFY AND CORRECT THE SRB QUEUES BY        */ 00046000
*/*           INVOKING A SECONDARY ENTRY POINT OF THE SCHEDULE       */ 00047000
*/*           RECOVERY ROUTINE.  UPON RECIEVING CONTROL BACK         */ 00048000
*/*           FROM THAT ROUTINE, IT WILL ISSUE THE SETRP MACRO       */ 00049000
*/*           TO CAUSE RECORDING TO BE DONE, AND TO PERCOLATE        */ 00050000
*/*           TO THE PURGEDQ ESTAE ROUTINE                           */ 00051000
*/*                                                                  */ 00052000
*/*     ENTRY POINT = IEAVEPDF                                       */ 00053000
*/*                                                                  */ 00054000
*/*     INPUT =                                                      */ 00055000
*/*          R0 = ADDRESS 200-BYTE WORK AREA                         */ 00056000
*/*          R1 = ADDRESS SDWA                                       */ 00057000
*/*          R14= RETURN ADDRESS TO RTM                              */ 00058000
*/*          R15= ENTRY POINT ADDRESS                                */ 00059000
*/*                                                                  */ 00060000
*/*     OUTPUT:                                                      */ 00061000
*/*           R7, R15 = VOLITILE                                     */ 00062000
*/*           R10 = BASE REGISTER                                    */ 00063000
*/*           R14 = RETURN ADDRESS TO RTM                            */ 00064000
*/*           ALL OTHER REGISTERS UNCHANGED                          */ 00065000
*/*                                                                  */ 00066000
*/********************************************************************/ 00067000
*                                                               01S0050 00068000
*IEAVEPDF:                                                      01S0050 00069000
*   ENTRY;                          /* FRR ROUTINE FOR PURGEDQ       */ 00070000
@EP00050 DS    0H                                               01S0051 00071000
*   RFY                                                         01S0051 00072000
*    (R0,                                                       01S0051 00073000
*     R1,                                                       01S0051 00074000
*     R14) RSTD;                                                01S0051 00075000
*   R14SAVE=R14;                    /* SAVE RETURN ADDRESS           */ 00076000
         LR    R14SAVE,R14                                      01S0052 00077000
*   CALL IEAVESQV;                  /* VERIFY THE SRB QUEUE ISSUE       00078000
*                                      SETRP TO INDICATE RECORDING      00079000
*                                      AND PERCOLATION               */ 00080000
         L     @15,@CV01407                                     01S0053 00081000
         BALR  @14,@15                                          01S0053 00082000
*   DO;                             /* SETRP RECORD(YES)RECPARM(MODNA   00083000
*                                      ME)RC(0)                      */ 00084000
*     RESPECIFY                                                 01S0055 00085000
*      (GPR00P,                                                 01S0055 00086000
*       GPR01P,                                                 01S0055 00087000
*       GPR14P,                                                 01S0055 00088000
*       GPR15P) RSTD;                                           01S0055 00089000
*     GPR01P->SDWARCDE=0;           /* SAVE RC VALUE                 */ 00090000
         MVI   SDWARCDE(GPR01P),X'00'                           01S0056 00091000
*     GPR01P->SDWARCRD='1'B;        /* TURN ON RECORD INDICATOR      */ 00092000
         OI    SDWARCRD(GPR01P),B'10000000'                     01S0057 00093000
*     GPR15P=ADDR(MODNAME);         /* ACCESS RECORD PARAMETER LIST  */ 00094000
         LA    GPR15P,MODNAME                                   01S0058 00095000
*     GPR01P->SDWARECP=GPR15P->I256C(1:24);/* COPY RECORD PARAMETERS */ 00096000
         MVC   SDWARECP(24,GPR01P),I256C(GPR15P)                01S0059 00097000
*     RESPECIFY                                                 01S0060 00098000
*      (GPR00P,                                                 01S0060 00099000
*       GPR01P,                                                 01S0060 00100000
*       GPR14P,                                                 01S0060 00101000
*       GPR15P) UNRSTD;                                         01S0060 00102000
*   END;                                                        01S0061 00103000
*   R14=R14SAVE;                    /* RESTORE RETURN ADDRESS        */ 00104000
         LR    R14,R14SAVE                                      01S0062 00105000
*   RETURN;                         /* RETURN TO CALLER              */ 00106000
@EL00001 DS    0H                                               01S0063 00107000
@EF00001 DS    0H                                               01S0063 00108000
@ER00001 BR    @14                                              01S0063 00109000
*                                                               01S0064 00110000
*/********************************************************************/ 00111000
*/*                                                                  */ 00112000
*/*     PURPOSE = THIS SEGMENT CONTAINS THE ENTRY POINT TO THE       */ 00113000
*/*           ESTAE ROUTINE FOR PURQE DQ.  IT WILL SET UP A          */ 00114000
*/*           PARAMETER AREA, AND INDICATE RECORDING IS TO BE        */ 00115000
*/*           DONE IN THE SDWA.                                      */ 00116000
*/*                                                                  */ 00117000
*/*     ENTRY POINT = IEAVEPDE                                       */ 00118000
*/*                                                                  */ 00119000
*/*     INPUT =                                                      */ 00120000
*/*           R0 = 12, IF NO SDWA                                    */ 00121000
*/*           R1 = ADDRESS SDWA                                      */ 00122000
*/*           R2 = ADDRESS PURGE DQ WORK AREA                        */ 00123000
*/*           R10= BASE REGISTER                                     */ 00124000
*/*           R14= RETURN ADDRESS TO RTM                             */ 00125000
*/*           R15= ENTRY POINT ADDRESS                               */ 00126000
*/*                                                                  */ 00127000
*/*     OUTPUT =                                                     */ 00128000
*/*           R2 = ADDRESS PURGE DQ WORK AREA                        */ 00129000
*/*           R7 = RETURN ADDRESS TO RTM                             */ 00130000
*/*           R8 = CONTENTS INPUT REG 0                              */ 00131000
*/*           R9 = CONTENTS INPUT REG 1                              */ 00132000
*/*           R11, R12, R14, R15 = VOLITILE                          */ 00133000
*/*           ALL OTHER REGISTERS UNCHANGED                          */ 00134000
*/*                                                                  */ 00135000
*/********************************************************************/ 00136000
*                                                               01S0064 00137000
*IEAVEPDE:                                                      01S0064 00138000
*   ENTRY;                          /* PURGEDQ ESTEA ROUTINE         */ 00139000
@EP00064 DS    0H                                               01S0065 00140000
*   R0SAVE=PARMREG0;                /* SAVE REGS 0, 1 AND 14         */ 00141000
         LR    R0SAVE,PARMREG0                                  01S0065 00142000
*   R1SAVE=PARMREG1;                /* THEY ARE DESTROYED ACROSS     */ 00143000
         LR    R1SAVE,PARMREG1                                  01S0066 00144000
*   R14SAVE=R14;                    /* INNERFACES                    */ 00145000
         LR    R14SAVE,R14                                      01S0067 00146000
*   RFY                                                         01S0068 00147000
*     R14 UNRSTD;                                               01S0068 00148000
*   IF PARMREG0^=12                 /* CHECK THAT THE SDWA EXISTS    */ 00149000
*     THEN                          /* AN SDWA HAS BEEN PASSED       */ 00150000
         C     PARMREG0,@CF01429                                01S0069 00151000
         BE    @RF00069                                         01S0069 00152000
*     DO;                                                       01S0070 00153000
*       WAPTR=SDWAPARM;             /* R2->WORK AREA IN SVRB         */ 00154000
         L     WAPTR,SDWAPARM(,SDWAPTR)                         01S0071 00155000
*       DO;                         /* SETRP                    01S0072 00156000
*                                      RECORD(YES)RECPARM(MODNAME)   */ 00157000
*         RESPECIFY                                             01S0073 00158000
*          (GPR00P,                                             01S0073 00159000
*           GPR01P,                                             01S0073 00160000
*           GPR14P,                                             01S0073 00161000
*           GPR15P) RSTD;                                       01S0073 00162000
*         GPR01P->SDWARCDE=0;       /* SAVE RC VALUE                 */ 00163000
         MVI   SDWARCDE(GPR01P),X'00'                           01S0074 00164000
*         GPR01P->SDWARCRD='1'B;    /* TURN ON RECORD INDICATOR      */ 00165000
         OI    SDWARCRD(GPR01P),B'10000000'                     01S0075 00166000
*         GPR15P=ADDR(MODNAME);     /* ACCESS RECORD PARAMETER LIST  */ 00167000
         LA    GPR15P,MODNAME                                   01S0076 00168000
*         GPR01P->SDWARECP=GPR15P->I256C(1:24);/* COPY RECORD   01S0077 00169000
*                                      PARAMETERS                    */ 00170000
         MVC   SDWARECP(24,GPR01P),I256C(GPR15P)                01S0077 00171000
*         RESPECIFY                                             01S0078 00172000
*          (GPR00P,                                             01S0078 00173000
*           GPR01P,                                             01S0078 00174000
*           GPR14P,                                             01S0078 00175000
*           GPR15P) UNRSTD;                                     01S0078 00176000
*       END;                                                    01S0079 00177000
*     END;                                                      01S0080 00178000
*   ELSE                                                        01S0081 00179000
*     ;                             /* NO SDWA EXISTS                */ 00180000
@RF00069 DS    0H                                               01S0082 00181000
*   NUMERR=NUMERR+1;                /* INCREMENT THE ERROR COUNT     */ 00182000
         LA    @12,1                                            01S0082 00183000
         SLR   @11,@11                                          01S0082 00184000
         IC    @11,NUMERR(,WAPTR)                               01S0082 00185000
         ALR   @12,@11                                          01S0082 00186000
         STC   @12,NUMERR(,WAPTR)                               01S0082 00187000
*                                                               01S0083 00188000
*/********************************************************************/ 00189000
*/*                                                                  */ 00190000
*/*     PURPOSE = TO ISSUE STATUS TO START SRB'S IF THEY HAVE BEEN   */ 00191000
*/*           STOPPED IN THE MAINLINE ROUTINE, AND RESTORE REGS      */ 00192000
*/*           0 AND 1 TO ORIGINAL CONTENTS ON ENTRY.                 */ 00193000
*/*                                                                  */ 00194000
*/*     INPUT =                                                      */ 00195000
*/*           R2 = ADDRESS PURGE DQ WORK AREA                        */ 00196000
*/*           R8 = ORIGINAL CONTENTS R0                              */ 00197000
*/*           R9 = ORIGINAL CONTENTS R1                              */ 00198000
*/*           R10= BASE REGISTER                                     */ 00199000
*/*                                                                  */ 00200000
*/*     OUTPUT =                                                     */ 00201000
*/*           R0 = RESTORED                                          */ 00202000
*/*           R1 = RESTORED                                          */ 00203000
*/*           ALL OTHER REGISTERS UNCHANGED                          */ 00204000
*/*                                                                  */ 00205000
*/********************************************************************/ 00206000
*                                                               01S0083 00207000
*   IF STPSRB='1'B                  /* CHECK STATUS OF SRB'S         */ 00208000
*     THEN                          /* SRB'S HAVE BEEN STOPPED       */ 00209000
         TM    STPSRB(WAPTR),B'01000000'                        01S0083 00210000
         BNO   @RF00083                                         01S0083 00211000
*     DO;                                                       01S0084 00212000
*       GEN( STATUS START,SRB);     /* START UP THE SRBS             */ 00213000
          STATUS START,SRB                                              00214000
*       STPSRB='0'B;                /* TURN OFF STOPPED FLAG         */ 00215000
         NI    STPSRB(WAPTR),B'10111111'                        01S0086 00216000
*     END;                                                      01S0087 00217000
*   SDWAPTR=R1SAVE;                 /* RESTORE SDWA POINTER          */ 00218000
@RF00083 LR    SDWAPTR,R1SAVE                                   01S0088 00219000
*   PARMREG0=R0SAVE;                /* RESTORE REG 0                 */ 00220000
         LR    PARMREG0,R0SAVE                                  01S0089 00221000
*                                                               01S0090 00222000
*/********************************************************************/ 00223000
*/*                                                                  */ 00224000
*/*     PURPOSE = IF AN ERROR OCCURRED DURING THE PROCESSING OF AN   */ 00225000
*/*           RMTR ROUTINE INVOKED BY PURGEDQ, THEN THE ADDRESS OF   */ 00226000
*/*           THE ASSOCIATED SRB WILL BE RECORDED IN THE SDWAM AND   */ 00227000
*/*           A RETRY WILL BE ATTEMPTED.  IF THIS IS THE FIRST ERROR */ 00228000
*/*           OCCURRENCE, A RETRY WILL ALSO BE ATTEMPTED.  OTHERWISE */ 00229000
*/*           THE ERROR WILL BE PERCOLATED TO THE CALLER.            */ 00230000
*/*                                                                  */ 00231000
*/*     INPUT =                                                      */ 00232000
*/*           R0 = 12, IF NO SDWA                                    */ 00233000
*/*           R1 = ADDRESS SDWA                                      */ 00234000
*/*           R2 = ADDRESS PURGE DQ WORK AREA                        */ 00235000
*/*           R10= BASE REGISTER                                     */ 00236000
*/*                                                                  */ 00237000
*/*     OUTPUT =                                                     */ 00238000
*/*           R8, R9, R14 AND R15 = VOLITILE                         */ 00239000
*/*           ALL OTHER REGISTERS UNCHANGED                          */ 00240000
*/*                                                                  */ 00241000
*/********************************************************************/ 00242000
*                                                               01S0090 00243000
*   RFY                                                         01S0090 00244000
*     SRBADR RSTD;                                              01S0090 00245000
*   SRBADR=JRNSRB;                  /* GET THE JOURNALLED SRB ADDR   */ 00246000
         L     SRBADR,JRNSRB(,WAPTR)                            01S0091 00247000
*   IF SRBADR^=0                    /* CHECK IF RMTR IN PROCESS      */ 00248000
*     THEN                          /* MAINLINE IN RMTR              */ 00249000
         LTR   SRBADR,SRBADR                                    01S0092 00250000
         BZ    @RF00092                                         01S0092 00251000
*     DO;                                                       01S0093 00252000
*       IF PARMREG0^=12             /* AND THE SDWA EXISTS           */ 00253000
*         THEN                      /* RECORD THE SRB IN THE SDWA    */ 00254000
         C     PARMREG0,@CF01429                                01S0094 00255000
         BE    @RF00094                                         01S0094 00256000
*         DO;                                                   01S0095 00257000
*           RFY                                                 01S0096 00258000
*             TEMPREG RSTD;                                     01S0096 00259000
*           TEMPREG=SDWAURAL;       /* GET OFFSET INTO VAR REC AREA  */ 00260000
         SLR   TEMPREG,TEMPREG                                  01S0097 00261000
         IC    TEMPREG,SDWAURAL(,SDWAPTR)                       01S0097 00262000
*           SDWAVRA(TEMPREG+1:TEMPREG+4)=SRBADR;/* STORE JOURNALLED     00263000
*                                      SRB IN SDWA           @ZA15356*/ 00264000
         LR    @12,SDWAPTR                                      01S0098 00265000
         ALR   @12,TEMPREG                                      01S0098 00266000
         STCM  SRBADR,15,SDWAVRA(@12)                           01S0098 00267000
*           TEMPREG=TEMPREG+4;      /* UPDATE SDWA USER AREA INDEX   */ 00268000
         AL    TEMPREG,@CF00071                                 01S0099 00269000
*           SDWAURAL=TEMPREG;       /* INCREMENT USED BYTE COUNT     */ 00270000
         STC   TEMPREG,SDWAURAL(,SDWAPTR)                       01S0100 00271000
*           RFY                                                 01S0101 00272000
*            (SRBADR,                                           01S0101 00273000
*             TEMPREG) UNRSTD;                                  01S0101 00274000
*         END;                                                  01S0102 00275000
*       CALL RETRY;                 /* SET UP FOR RETRY              */ 00276000
@RF00094 BAL   @14,RETRY                                        01S0103 00277000
*     END;                                                      01S0104 00278000
*   ELSE                            /* IF NOT IN MIDDLE OF RMTR      */ 00279000
*     IF NUMERR^>1                  /* BUT NO RETRY ATTEMPTED PREV   */ 00280000
*       THEN                                                    01S0105 00281000
         B     @RC00092                                         01S0105 00282000
@RF00092 CLI   NUMERR(WAPTR),1                                  01S0105 00283000
         BH    @RF00105                                         01S0105 00284000
*       CALL RETRY;                 /* SET UP FOR RETRY              */ 00285000
         BAL   @14,RETRY                                        01S0106 00286000
*     ELSE                                                      01S0107 00287000
*       CALL PERC;                  /* OTHERWISE, PERCOLATE          */ 00288000
         B     @RC00105                                         01S0107 00289000
@RF00105 BAL   @14,PERC                                         01S0107 00290000
*   RFY                                                         01S0108 00291000
*     R14 UNRSTD;                                               01S0108 00292000
*                                                               01S0109 00293000
*/********************************************************************/ 00294000
*/*                                                                  */ 00295000
*/*     PURPOSE = THIS SEGMENT IS ENTERED ON A RETRY REQUEST.  IT    */ 00296000
*/*           WILL ESTABLISH THE CORRECT ENVIRONMENT AND REGISTERS   */ 00297000
*/*           BEFORE PROCEEDING TO THE MAIN PURGEDQ ENTRY POINT.     */ 00298000
*/*                                                                  */ 00299000
*/*     ENTRY POINT = IEAVEPDS                                       */ 00300000
*/*                                                                  */ 00301000
*/*     INPUT =                                                      */ 00302000
*/*           R10= BASE REGISTER                                     */ 00303000
*/*                                                                  */ 00304000
*/*     OUTPUT =                                                     */ 00305000
*/*           R0, R1, R14 AND R15 = ORIGINAL CONTENTS WHEN PURGE DQ  */ 00306000
*/*                                 ORIGINALLY ISSUED                */ 00307000
*/*           R2 = PURGE DQ WORK AREA ADDRESS                        */ 00308000
*/*           R5, R6, R11-R13 = VOLITILE                             */ 00309000
*/*           ALL OTHER REGISTERS UNCHANGED                          */ 00310000
*/*                                                                  */ 00311000
*/********************************************************************/ 00312000
*                                                               01S0109 00313000
*IEAVEPDS:                                                      01S0109 00314000
*   ENTRY;                                                      01S0109 00315000
@EP00109 DS    0H                                               01S0109 00316000
@EC00109 DS    0H                                               01S0110 00317000
*   RFY                                                         01S0110 00318000
*    (RBSECPTR,                                                 01S0110 00319000
*     R0,                                                       01S0110 00320000
*     R1,                                                       01S0110 00321000
*     R15,                                                      01S0110 00322000
*     WAPTR,                                                    01S0110 00323000
*     PURGEDQR) RSTD;                                           01S0110 00324000
*   GEN CODE REFS(PSALITA,FLC);                                 01S0111 00325000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=('OBTAIN LOCAL LO00326000
               CK BEFORE ENTERING PURGEDQ',IEAVEPDR(IEAVEPDS))          00327000
*   RBSECPTR=PSATOLD->TCBRBP;                                   01S0112 00328000
         L     @14,PSATOLD                                      01S0112 00329000
         L     RBSECPTR,TCBRBP(,@14)                            01S0112 00330000
*   WAPTR=ADDR(RBEXSAVE);           /* FIND PURGEDQ WORK AREA IN RB  */ 00331000
         LA    WAPTR,RBEXSAVE(,RBSECPTR)                        01S0113 00332000
*   RFY                                                         01S0114 00333000
*     R14 RSTD;                                                 01S0114 00334000
*   R14=RETADDR1;                   /* RET ADDR OF PURGEDQ CALLER    */ 00335000
         L     R14,RETADDR1(,WAPTR)                             01S0115 00336000
*   R0=RBGRS0;                      /* RESTORE REGS 0,1 AND 15       */ 00337000
         L     R0,RBGRS0(,RBSECPTR)                             01S0116 00338000
*   R1=RBGRS1;                      /* FROM THE CURRENT RB           */ 00339000
         L     R1,RBGRS1(,RBSECPTR)                             01S0117 00340000
*   R15=RBGRS15;                                                01S0118 00341000
         L     R15,RBGRS15(,RBSECPTR)                           01S0118 00342000
*   PURGEDQR=ADDR(IGC123);          /* GET PURGEDQ ENTRY POINT ADDR  */ 00343000
         L     PURGEDQR,@CA01408                                01S0119 00344000
*   GOTO PURGEDQR;                  /* GO TO PURGEDQ ENTRY POINT     */ 00345000
         BR    PURGEDQR                                         01S0120 00346000
*                                                               01S0121 00347000
*/********************************************************************/ 00348000
*/*                                                                  */ 00349000
*/*     PURPOSE = THIS SEGMENT COMMUNICATES TO RTM THAT PERCOLATION  */ 00350000
*/*           IS REQUESTED.                                          */ 00351000
*/*                                                                  */ 00352000
*/*     INPUT =                                                      */ 00353000
*/*           R0 = 12, IF NO SDWA                                    */ 00354000
*/*           R1 = ADDRESS SDWA                                      */ 00355000
*/*           R2 = ADDRESS 200 BYTE WORK AREA                        */ 00356000
*/*           R7 = RETURN ADDRESS TO RTM                             */ 00357000
*/*           R10= BASE REGISTER                                     */ 00358000
*/*                                                                  */ 00359000
*/*     OUTPUT =                                                     */ 00360000
*/*           R14 = RETURN ADDRESS TO RTM                            */ 00361000
*/*           R15 = RETURN CODE (=0)                                 */ 00362000
*/*           ALL OTHER REGISTERS UNCHANGED                          */ 00363000
*/*                                                                  */ 00364000
*/********************************************************************/ 00365000
*                                                               01S0121 00366000
*PERC:                                                          01S0121 00367000
*   PROC OPTIONS(NOSAVE);           /* PERCOLATION IS REQUESTED      */ 00368000
PERC     DS    0H                                               01S0122 00369000
*   RFY                                                         01S0122 00370000
*     R14 UNRSTD;                                               01S0122 00371000
*   IF PARMREG0^=12                 /* IF AN SDWA HAS BEEN PASSED    */ 00372000
*     THEN                          /* INDICATE PERCOLATION IN SDWA  */ 00373000
         C     PARMREG0,@CF01429                                01S0123 00374000
         BE    @RF00123                                         01S0123 00375000
*     DO;                           /* SETRP WKAREA(R1)RC(0)         */ 00376000
*       RESPECIFY                                               01S0125 00377000
*        (GPR00P,                                               01S0125 00378000
*         GPR01P,                                               01S0125 00379000
*         GPR14P,                                               01S0125 00380000
*         GPR15P) RSTD;                                         01S0125 00381000
*       GPR01P=R1;                                              01S0126 00382000
*       GPR01P->SDWARCDE=0;         /* SAVE RC VALUE                 */ 00383000
         MVI   SDWARCDE(GPR01P),X'00'                           01S0127 00384000
*       RESPECIFY                                               01S0128 00385000
*        (GPR00P,                                               01S0128 00386000
*         GPR01P,                                               01S0128 00387000
*         GPR14P,                                               01S0128 00388000
*         GPR15P) UNRSTD;                                       01S0128 00389000
*     END;                                                      01S0129 00390000
*   ELSE                                                        01S0130 00391000
*     RETCODE=0;                    /* SET R15 = 0 FOR PERCOLATION   */ 00392000
         B     @RC00123                                         01S0130 00393000
@RF00123 SLR   RETCODE,RETCODE                                  01S0130 00394000
*   RFY                                                         01S0131 00395000
*     R14 RSTD;                                                 01S0131 00396000
@RC00123 DS    0H                                               01S0132 00397000
*   R14=R14SAVE;                    /* RESTORE RETURN ADDRESS        */ 00398000
         LR    R14,R14SAVE                                      01S0132 00399000
*   END PERC;                       /* RETURN TO CALLER              */ 00400000
@EL00002 DS    0H                                               01S0133 00401000
@EF00002 DS    0H                                               01S0133 00402000
@ER00002 BR    @14                                              01S0133 00403000
*                                                               01S0134 00404000
*/********************************************************************/ 00405000
*/*                                                                  */ 00406000
*/*     PURPOSE = THIS SEGMENT COMMUNICATES TO RTM THAT A RETRY IS   */ 00407000
*/*           WANTED.  THIS MAY BE DONE BY ISSUING THE SETRP         */ 00408000
*/*           MACRO IF AN SDWA WAS PROVIDED, OR BY SETTING UP        */ 00409000
*/*           REGISTERS 0 AND 15 IF NONE WAS PROVIDED.               */ 00410000
*/*                                                                  */ 00411000
*/*     INPUT =                                                      */ 00412000
*/*           R0 = 12, IF NO SDWA                                    */ 00413000
*/*           R1 = ADDRESS SDWA                                      */ 00414000
*/*           R7 = RETURN ADDRESS TO RTM                             */ 00415000
*/*           R10 = BASE REGISTER                                    */ 00416000
*/*                                                                  */ 00417000
*/*     OUTPUT =                                                     */ 00418000
*/*           R0 = ADDRESS SETUP ROUTINE (=IEAVEPDS)                 */ 00419000
*/*           R12= VOLITILE                                          */ 00420000
*/*           R14= CALLER'S RETURN ADDRESS                           */ 00421000
*/*           R15= RETURN CODE (=4)                                  */ 00422000
*/*           ALL OTHER REGISTERS UNCHANGED                          */ 00423000
*/*                                                                  */ 00424000
*/********************************************************************/ 00425000
*                                                               01S0134 00426000
*RETRY:                                                         01S0134 00427000
*   PROC OPTIONS(NOSAVE);           /* RETRY IS WANTED               */ 00428000
RETRY    DS    0H                                               01S0135 00429000
*   RFY                                                         01S0135 00430000
*     R14 UNRSTD;                                               01S0135 00431000
*   IF PARMREG0^=12                 /* IF AN SDWA WAS PASSED         */ 00432000
*     THEN                          /* INDICATE RETRY VIA SETRP      */ 00433000
         C     PARMREG0,@CF01429                                01S0136 00434000
         BE    @RF00136                                         01S0136 00435000
*     DO;                           /* SETRP WKAREA(R1)FRESDWA(YES)RC   00436000
*                                      (4)RETADDR(IEAVEPDS)DUMP(NO)  */ 00437000
*       RESPECIFY                                               01S0138 00438000
*        (GPR00P,                                               01S0138 00439000
*         GPR01P,                                               01S0138 00440000
*         GPR14P,                                               01S0138 00441000
*         GPR15P) RSTD;                                         01S0138 00442000
*       GPR01P=R1;                                              01S0139 00443000
*       GPR01P->SDWARCDE=4;         /* STORE RC INTO SDWA            */ 00444000
         MVI   SDWARCDE(GPR01P),X'04'                           01S0140 00445000
*       GPR01P->SDWARTYA=ADDR(IEAVEPDS);/* SAVE RETRY ADDRESS        */ 00446000
         L     @12,@CA01433                                     01S0141 00447000
         ST    @12,SDWARTYA(,GPR01P)                            01S0141 00448000
*       GPR01P->SDWAFREE='1'B;      /* SET FLAG TO FREE SDWA         */ 00449000
         OI    SDWAFREE(GPR01P),B'00000100'                     01S0142 00450000
*       GPR01P->SDWAREQ='0'B;       /* TURN OFF DUMP INDICATOR       */ 00451000
         NI    SDWAREQ(GPR01P),B'01111111'                      01S0143 00452000
*       RESPECIFY                                               01S0144 00453000
*        (GPR00P,                                               01S0144 00454000
*         GPR01P,                                               01S0144 00455000
*         GPR14P,                                               01S0144 00456000
*         GPR15P) UNRSTD;                                       01S0144 00457000
*     END;                                                      01S0145 00458000
*   ELSE                                                        01S0146 00459000
*     DO;                           /* SET UP REG 0 FOR RETRY        */ 00460000
         B     @RC00136                                         01S0146 00461000
@RF00136 DS    0H                                               01S0147 00462000
*       R0=ADDR(IEAVEPDS);          /* PUT ADDR OF SETUP RTN IN R0   */ 00463000
         L     R0,@CA01433                                      01S0147 00464000
*       R15=4;                      /* SET RETURN CODE=4=RETRY       */ 00465000
         LA    R15,4                                            01S0148 00466000
*     END;                                                      01S0149 00467000
*   RFY                                                         01S0150 00468000
*     R14 RSTD;                                                 01S0150 00469000
@RC00136 DS    0H                                               01S0151 00470000
*   R14=R14SAVE;                    /* RESTORE RETURN ADDRESS        */ 00471000
         LR    R14,R14SAVE                                      01S0151 00472000
*   END RETRY;                                                  01S0152 00473000
@EL00003 DS    0H                                               01S0152 00474000
@EF00003 DS    0H                                               01S0152 00475000
@ER00003 BR    @14                                              01S0152 00476000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 00477000
*     GPR00P PTR(31) REG(0),                                    01S0153 00478000
*     GPR01P PTR(31) REG(1),                                    01S0153 00479000
