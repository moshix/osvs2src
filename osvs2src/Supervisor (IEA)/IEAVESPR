*       SDWASR15=REG15;             /*                       @ZA16400*/ 00210000
         L     @12,REG15(,@12)                                     0137 00211000
         ST    @12,SDWASR15(,SDWAPTR)                              0137 00212000
*       SDWASR08=PSAGPREG(1);       /*                       @ZA16400*/ 00213000
         L     @12,PSAGPREG                                        0138 00214000
