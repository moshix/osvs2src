         BNL   @RF00130                                            0130 00181000
@RT00130 DS    0H                                                  0131 00182000
*     DO;                           /*                       @ZA16400*/ 00183000
*       CVTTRACE=CCVTTOFF;          /* TURN OFF TRACE. TRACE IS    0132 00184000
*                                      INVOKED BY BRANCHING TO     0132 00185000
*                                      CVTTRACE. THIS STATEMENT TURNS   00186000
*                                      CVTTRACE INTO A NOPR. THE NEXT   00187000
*                                      HALFWORD IN THE CVT IS AN   0132 00188000
*                                      INSTRUCTION WHICH WILL RETURN    00189000
*                                      CONTROL TO THE CALLER OF    0132 00190000
*                                      TRACE.                @ZA16400*/ 00191000
         L     @12,CVTPTR                                          0132 00192000
         MVC   CVTTRACE(2,@12),@CB01729                            0132 00193000
*       SDWASR11=REG11;             /* RESTORE THE REGISTERS SAVED BY   00194000
*                                      TRACE                 @ZA16400*/ 00195000
