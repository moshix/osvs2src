*     GOTO ERREXIT;                 /* ERROR - CANT FIX WINDOW PAGES */ 00379000
*   PCBPTR=CIWWRK1;                 /* SET PCBPTR TO FIRST ON QUE    */ 00380000
*                                                                  0200 00381000
@RF00198 L     PCBPTR,CIWWRK1(,CIWAPTR)                            0200 00382000
*   /*****************************************************************/ 00383000
*   /*                                                               */ 00384000
*   /* TRY TO FIND THE ASSOCIATED PCB                                */ 00385000
*   /*                                                               */ 00386000
*   /*****************************************************************/ 00387000
*                                                                  0201 00388000
*   DO WHILE PCBPTR^=0;             /* LOOP AS LONG AS THERE IS A PCB*/ 00389000
