*   RCAFXL='1'B;                    /* SET ENTRY POINT FLAG          */ 01317000
         OI    RCAFXL(@14),B'01000000'                             0477 01318000
*   PCBRPTR=R1;                     /* PICK UP ROOT POINTER FROM PARM*/ 01319000
