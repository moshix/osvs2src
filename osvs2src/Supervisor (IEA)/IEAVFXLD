         TM    ASCB1LPU(@14),B'01000000'                           0276 00640000
         BNO   @RF00276                                            0276 00641000
         TM    CIWLONG(CIWAPTR),B'00000010'                        0276 00642000
         BO    @RT00276                                            0276 00643000
         TM    ASCB2LPU(@14),B'10000000'                           0276 00644000
         BNO   @RF00276                                            0276 00645000
@RT00276 DS    0H                                                  0277 00646000
*         PCBPREFR='1'B;            /* REQUEST A PREF FRAME  @ZA16816*/ 00647000
         OI    PCBPREFR(PCBPTR),B'00001000'                        0277 00648000
*       PCBFXC=1;                   /* SET FIX COUNT TO 1            */ 00649000
@RF00276 MVI   PCBFXC(PCBPTR),X'01'                                0278 00650000
*       RFY                                                        0279 00651000
*        (INPUTVBN) RSTD;                                          0279 00652000
*       INPUTVBN=CURVBN;            /* LOAD PARM FOR FOEADD          */ 00653000
         SLR   INPUTVBN,INPUTVBN                                   0280 00654000
         ICM   INPUTVBN,3,CURVBN(CIWAPTR)                          0280 00655000
*       CALL FOEADD;                /* CALL TO ADD FOE TO CHAIN      */ 00656000
         BAL   @14,FOEADD                                          0281 00657000
*       RFY                                                        0282 00658000
*        (INPUTVBN) UNRSTD;                                        0282 00659000
