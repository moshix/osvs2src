*           DO;                                                    0212 00421000
*             CIWRETC=20;           /* MARK ENVIRONMENTAL ERROR      */ 00422000
         MVI   CIWRETC(CIWAPTR),X'14'                              0213 00423000
*             GOTO ERREXITA;        /* ERROR-FOE GETMAIN FAILED      */ 00424000
         B     ERREXITA                                            0214 00425000
