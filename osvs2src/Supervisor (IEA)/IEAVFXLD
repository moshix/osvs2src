         MVC   PCBRTCB(4,PCBRPTR),CIWTCB(CIWAPTR)                  0248 00537000
*       PCBRECB=CIWECB;             /* SET ROOT PCB ECB FIELD        */ 00538000
         MVC   PCBRECB(4,PCBRPTR),CIWECB(CIWAPTR)                  0249 00539000
