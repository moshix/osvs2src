*           SPQEADDR=ADDR(TCBSWA);  /* SWA SPQE POINTER              */ 00817000
         LA    @11,TCBSWA(,@11)                                    0268 00818000
         ST    @11,SPQEADDR                                        0268 00819000
*           QBLCKLOC=ON;            /* BLOCKS IN LSQA                */ 00820000
