         BH    @RT00421                                            0421 01266000
*     GO TO DQECHK15;               /* EXCEEDS LIMIT                 */ 01267000
*                                                                  0422 01268000
*   /*****************************************************************/ 01269000
*   /*                                                               */ 01270000
*   /* CHECK OVERLAP WITH PREVIOUS DQE'S                             */ 01271000
*   /*                                                               */ 01272000
*   /*****************************************************************/ 01273000
*                                                                  0423 01274000
*   TPREVDQE=SPQEADDR;              /* FIRST PREVIOUS TEMPORARY DQE  */ 01275000
