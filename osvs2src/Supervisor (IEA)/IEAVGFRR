         C     CIRSPQE,SPQEPTR(,@11)                               0328 00984000
         BE    @RT00328                                            0328 00985000
*     GOTO B1ERR;                   /* RECORD THE ERROR      @ZA27378*/ 00986000
*   IF SPQEADDR^=CIRSPQE THEN       /* IF MORE TO CHECK      @ZA27378*/ 00987000
