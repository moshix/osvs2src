         OI    QBLCKLOC,B'10000010'                                0270 00822000
*           CALL SPQECHK;           /* CHECK SWA QUEUE               */ 00823000
         BAL   @14,SPQECHK                                         0271 00824000
*           SPQEADDR=ADDR(TCBUKYSP);/* AUK SPQE POINTER              */ 00825000
