*                                   /* RECOVERY ROUTINE.             */ 00040000
*                                                               01S0061 00041000
*   /*****************************************************************/ 00042000
*   /*                                                               */ 00043000
*   /* ISSUE EXECUTE FORM OF ESTAE - SEE ABOVE FOR MF=L ESTAE        */ 00044000
*   /*                                                               */ 00045000
*   /*****************************************************************/ 00046000
*                                                               01S0061 00047000
*   ESTAEPRM=ESTAELST;              /* COPY ESTAE LIST TO AUTO       */ 00048000
         MVC   ESTAEPRM(16),ESTAELST                            01S0061 00049000
*                                   /* STORAGE                       */ 00050000
*   GEN(ESTAE (REG2),CT,PARAM=RCVPARM,RECORD=YES,MF=(E,ESTAEPRM));      00051000
         ESTAE (REG2),CT,PARAM=RCVPARM,RECORD=YES,MF=(E,ESTAEPRM)       00052000
*   PARMPTR=ADDR(RCVPARM);          /* BASE FOR ESTAE PARM'S         */ 00053000
         LA    @12,RCVPARM                                      01S0063 00054000
         ST    @12,PARMPTR                                      01S0063 00055000
*   PARMID=MASV;                    /* IDENTIFY MODULE TO FRR        */ 00056000
         MVC   PARMID(4,@12),@CC00078                           01S0064 00057000
*   PARMRGAD=ADDR(RCVRSAVE);        /* RECOVERY REG SAVE AREA        */ 00058000
         LA    @11,RCVRSAVE                                     01S0065 00059000
         ST    @11,PARMRGAD(,@12)                               01S0065 00060000
*   PARMRTAD=ADDR(ESTRETRY);        /* RETRY ADDRESS IN IEAVMASV     */ 00061000
         LA    @11,ESTRETRY                                     01S0066 00062000
         ST    @11,PARMRTAD(,@12)                               01S0066 00063000
*   RCVRGMAP=ALLREGS;               /* RESTORE ALL REGS AT RETRY     */ 00064000
         MVC   RCVRGMAP(2),@CB00081                             01S0067 00065000
*   GENERATE SETS(RCVREGS);                                     01S0068 00066000
         STM  REG0,REG15,RCVREGS       SAVE REGS FOR RETRY              00067000
*   RESPECIFY                                                   01S0069 00068000
*    (REG2)UNRESTRICTED;                                        01S0069 00069000
*                                                               01S0069 00070000
*   /*****************************************************************/ 00071000
*   /*                                                               */ 00072000
*   /* START MODULE INITIALIZATION AND MAINLINE PROCESSING INITIALIZE*/ 00073000
*   /* THE ECB WAIT LIST                                             */ 00074000
*   /*                                                               */ 00075000
*   /*****************************************************************/ 00076000
*                                                               01S0070 00077000
*   ECB1=ADDR(UCMMNECB);            /* ADDR OF MONITOR ECB           */ 00078000
         L     @12,CVTPTR                                       01S0070 00079000
         L     @12,UCMBASPT(,@12)                               01S0070 00080000
         LA    @11,UCMMNECB(,@12)                               01S0070 00081000
         ST    @11,ECB1                                         01S0070 00082000
*   ECB2B=ADDR(UCMTRECB);           /* ADDR OF TERMINATION ECB       */ 00083000
         LA    @12,UCMTRECB(,@12)                               01S0071 00084000
         STCM  @12,7,ECB2B                                      01S0071 00085000
*   ECB2A='1'B;                     /* INDICATE LAST ECB ADDR IN THE    00086000
*                                      LIST                          */ 00087000
*                                                               01S0072 00088000
         OI    ECB2A,B'10000000'                                01S0072 00089000
*   /*****************************************************************/ 00090000
*   /*                                                               */ 00091000
*   /* WAIT FOR WORK TO DO                                           */ 00092000
*   /*                                                               */ 00093000
*   /*****************************************************************/ 00094000
*                                                               01S0073 00095000
*WORKWAIT:                                                      01S0073 00096000
*   DO;                             /* WAIT (ONE) ECBLIST(ECBADDRS)  */ 00097000
WORKWAIT DS    0H                                               01S0074 00098000
*     RESPECIFY                                                 01S0074 00099000
*      (GPR00P,                                                 01S0074 00100000
*       GPR01P)RESTRICTED;                                      01S0074 00101000
*     GPR00P=ONE;                   /* LOAD WAIT COUNT               */ 00102000
         LA    GPR00P,1                                         01S0075 00103000
*     GPR01P=-ADDR(ECBADDRS);       /* ADDRESS OF LIST OF ECB'S      */ 00104000
         LA    GPR01P,ECBADDRS                                  01S0076 00105000
         LCR   GPR01P,GPR01P                                    01S0076 00106000
*     SVC(1);                       /* WAIT SVC                      */ 00107000
         SVC   1                                                01S0077 00108000
*     RESPECIFY                                                 01S0078 00109000
*      (GPR00P,                                                 01S0078 00110000
*       GPR01P)UNRESTRICTED;                                    01S0078 00111000
*   END;                            /* WAIT (ONE) ECBLIST(ECBADDRS)  */ 00112000
*   CALL OBTLOCKS;                  /* OBTAIN LOCAL AND CMS LOCKS TO    00113000
*                                      SERIALIZE USE OF THE UCM, MQES   00114000
*                                      AND MMBS                      */ 00115000
*                                                               01S0080 00116000
         BAL   @14,OBTLOCKS                                     01S0080 00117000
*   /*****************************************************************/ 00118000
*   /*                                                               */ 00119000
*   /* DETERMINE WORK TO BE DONE                                     */ 00120000
*   /*                                                               */ 00121000
*   /*****************************************************************/ 00122000
*                                                               01S0081 00123000
*CHEKECBS:                                                      01S0081 00124000
*   ECBPTR=ADDR(UCMTRECB);          /* CHECK TERMINATION ECB         */ 00125000
CHEKECBS L     @12,CVTPTR                                       01S0081 00126000
         L     @12,UCMBASPT(,@12)                               01S0081 00127000
         LA    ECBPTR,UCMTRECB(,@12)                            01S0081 00128000
*   IF ECBPOSTB='0'B THEN           /* IS ECB POSTED?                */ 00129000
         TM    ECBPOSTB(ECBPTR),B'01000000'                     01S0082 00130000
         BNZ   @RF00082                                         01S0082 00131000
*     DO;                           /* NO                            */ 00132000
*       ECBPTR=ADDR(UCMMNECB);      /* CHECK MONITOR ECB             */ 00133000
         LA    ECBPTR,UCMMNECB(,@12)                            01S0084 00134000
*       IF ECBPOSTB='1'B THEN       /* IS ECB POSTED?                */ 00135000
         TM    ECBPOSTB(ECBPTR),B'01000000'                     01S0085 00136000
         BNO   @RF00085                                         01S0085 00137000
*         DO;                       /* YES                           */ 00138000
*                                                               02S0087 00139000
*/*** START OF SCANMMBQ SEGMENT **************************************/ 00140000
*/*                                                                  */ 00141000
*/* SCANMMBQ - SCAN FOR MATCH ON MSGTYPE WITH MQES                   */ 00142000
*/*                                                                  */ 00143000
*/* FUNCTION - A SCAN IS DONE OVER THE MMB QUEUE.  FOR EACH MMB, THE */ 00144000
*/*    ENTIRE MQE CHAIN IS SEARCHED.  IF THE MSGTYPE OF THE MMB      */ 00145000
*/*    MATCHES THAT OF A MQE, CONTROL IS PASSED TO THE TPUTMMB       */ 00146000
*/*    SEGMENT TO WRITE THE MESSAGE TO THE TERMINAL REPRESENTED      */ 00147000
*/*    BY THE MQE. WHEN THE END OF THE MQE QUEUE IS REACHED, THE     */ 00148000
*/*    DELETMMB SUBROUTINE IS CALLED TO DELETE THE MMB.  THE SCAN    */ 00149000
*/*    ENDS WHEN THE MMB QUEUE IS EMPTY.                             */ 00150000
*/*                                                                  */ 00151000
*/* INPUT - MMB QUEUE                                                */ 00152000
*/*        MQE QUEUE                                                 */ 00153000
*/*                                                                  */ 00154000
*/* OUTPUT - MMB QUEUE IS EMPTY                                      */ 00155000
*/*                                                                  */ 00156000
*/********************************************************************/ 00157000
*                                                               02S0087 00158000
*           UCMMNECB=ZERO;          /* CLEAR THE MONITOR ECB         */ 00159000
         SLR   @11,@11                                          02S0087 00160000
         ST    @11,UCMMNECB(,@12)                               02S0087 00161000
*           MMBPTR=UCMMBPTR;        /* INITIALIZE PTR TO FIRST MMB   */ 00162000
*                                                               02S0088 00163000
         L     MMBPTR,UCMMBPTR(,@12)                            02S0088 00164000
*           /*********************************************************/ 00165000
*           /*                                                       */ 00166000
*           /* PROCESS MMBS UNTIL ALL HAVE BEEN DELETED              */ 00167000
*           /*                                                       */ 00168000
*           /*********************************************************/ 00169000
*                                                               02S0089 00170000
*           DO WHILE(MMBPTR^=0);                                02S0089 00171000
         B     @DE00089                                         02S0089 00172000
@DL00089 DS    0H                                               02S0090 00173000
*             MQEPTR=UCMMQPTR;      /* INITIALIZE PTR TO FIRST MQE   */ 00174000
*                                                               02S0090 00175000
         L     @12,CVTPTR                                       02S0090 00176000
         L     @12,UCMBASPT(,@12)                               02S0090 00177000
         L     MQEPTR,UCMMQPTR(,@12)                            02S0090 00178000
*             /*******************************************************/ 00179000
*             /*                                                     */ 00180000
*             /* LOOP THROUGH CHAIN OF MQES                          */ 00181000
*             /*                                                     */ 00182000
*             /*******************************************************/ 00183000
*                                                               02S0091 00184000
*             DO WHILE(MQEPTR^=0);                              02S0091 00185000
         B     @DE00091                                         02S0091 00186000
@DL00091 DS    0H                                               02S0092 00187000
*               IF(MQETYPE&MMBTYPE)^=ZERO THEN/* DOES ANY MQE TYPE      00188000
*                                      MATCH MMBTYPE?                */ 00189000
         SLR   @12,@12                                          02S0092 00190000
         ICM   @12,3,MMBTYPE(MMBPTR)                            02S0092 00191000
         SLR   @11,@11                                          02S0092 00192000
         ICM   @11,3,MQETYPE(MQEPTR)                            02S0092 00193000
         NR    @12,@11                                          02S0092 00194000
         LTR   @12,@12                                          02S0092 00195000
         BZ    @RF00092                                         02S0092 00196000
*                 DO;               /* YES                           */ 00197000
*                                                               02S0093 00198000
*                   /*************************************************/ 00199000
*                   /*                                               */ 00200000
*                   /* SEND THE MESSAGE IN THE MMB TO THE TERMINAL   */ 00201000
*                   /* REPRESENTED BY THE MQE                        */ 00202000
*                   /*                                               */ 00203000
*                   /*************************************************/ 00204000
*                                                               02S0094 00205000
*                                                               03S0094 00206000
*/*** START OF TPUTMMB SEGMENT ***************************************/ 00207000
*/*                                                                  */ 00208000
*/* TPUTMMB - WRITE THE MESSAGE TO THE MONITORING TSO TERMINAL       */ 00209000
*/*                                                                  */ 00210000
*/* FUNCTION - SETS UP TPUT WITH ASID R-FORM PARAMETER LIST,         */ 00211000
*/*    SAVES PLACE IN THE MQE CHAIN, FREES THE LOCKS, ISSUES         */ 00212000
*/*    TPUT, OBTAINS THE LOCKS AND PICKS UP PLACE IN THE MQE         */ 00213000
*/*    CHAIN                                                         */ 00214000
*/*                                                                  */ 00215000
*/* INPUT - MMBPTR                                                   */ 00216000
*/*         MQEPTR                                                   */ 00217000
*/*                                                                  */ 00218000
*/* OUTPUT - MESSAGE WRITTEN TO THE TERMINAL                         */ 00219000
*/*          UCMMQNXT = ADDRESS OF NEXT MQE OR ZERO                  */ 00220000
*/*          MQEPTR = ADDRESS OF NEXT MQE OR ZERO                    */ 00221000
*/*                                                                  */ 00222000
*/********************************************************************/ 00223000
*                                                               03S0094 00224000
*                   RFY                                         03S0094 00225000
*                    (REG0,                                     03S0094 00226000
*                     REG1,                                     03S0094 00227000
*                     REG5)RSTD;    /* RESTRICT TPUT PARAMETER  03S0094 00228000
*                                      REGISTERS FROM CPMPILER USE   */ 00229000
*                   REG1=ADDR(MMBTEXT);/* ADDRESS OF THE MESSAGE TEXT*/ 00230000
         LA    REG1,MMBTEXT(,MMBPTR)                            03S0095 00231000
*                   REG1=REG1|TPUTFLGS;/* SET TPUT PROCESSING OPTIONS*/ 00232000
         O     REG1,@CF00046                                    03S0096 00233000
*                   REG0=MQEID;     /* GET ASID OF MESSAGE RECEIVER  */ 00234000
         SLR   REG0,REG0                                        03S0097 00235000
         ICM   REG0,3,MQEID(MQEPTR)                             03S0097 00236000
*                   GEN(SLL REG0,16);/* SHIFT ASID TO HIGH TWO BYTES */ 00237000
         SLL REG0,16                                                    00238000
*                   REG5=MMBTXLN;   /* GET MESSAGE LENGTH            */ 00239000
         LH    REG5,MMBTXLN(,MMBPTR)                            03S0099 00240000
*                   REG0=REG0|REG5; /* PUT MESSAGE LENGTH IN LOW TWO    00241000
*                                      BYTES OF REG 0                */ 00242000
         OR    REG0,REG5                                        03S0100 00243000
*                   UCMMQNXT=MQELINK;/* SAVE PLACE IN MQE CHAIN      */ 00244000
         L     @12,CVTPTR                                       03S0101 00245000
         L     @12,UCMBASPT(,@12)                               03S0101 00246000
         L     @11,MQELINK(,MQEPTR)                             03S0101 00247000
         ST    @11,UCMMQNXT(,@12)                               03S0101 00248000
*                   CALL FRLOCKS;   /* FREE CMS AND LOCAL LOCKS 03S0102 00249000
*                                      AROUND TPUT SVC               */ 00250000
*                                                               03S0102 00251000
         BAL   @14,FRLOCKS                                      03S0102 00252000
*                   /*************************************************/ 00253000
*                   /*                                               */ 00254000
*                   /* WRITE THE MESSAGE TO THE TERMINAL             */ 00255000
*                   /*                                               */ 00256000
*                   /*************************************************/ 00257000
*                                                               03S0103 00258000
*                   GEN(TPUT (1),(0),R);                        03S0103 00259000
         TPUT (1),(0),R                                                 00260000
*                   CALL OBTLOCKS;  /* REOBTAIN LOCAL AND CMS LOCKS  */ 00261000
         BAL   @14,OBTLOCKS                                     03S0104 00262000
*                   MQEPTR=UCMMQNXT;/* PICK UP PLACE IN MQE CHAIN    */ 00263000
         L     @12,CVTPTR                                       03S0105 00264000
         L     @12,UCMBASPT(,@12)                               03S0105 00265000
         L     MQEPTR,UCMMQNXT(,@12)                            03S0105 00266000
*                   RFY                                         03S0106 00267000
*                    (REG0,                                     03S0106 00268000
*                     REG1,                                     03S0106 00269000
*                     REG5)UNRSTD;  /* REGS NO LONGER NEEDED         */ 00270000
*                                                               03S0106 00271000
*/*** END OF TPUTMMB SEGMENT *****************************************/ 00272000
*                                                               03S0107 00273000
*                 END;                                          02S0107 00274000
*               ELSE                                            02S0108 00275000
*                 MQEPTR=MQELINK;   /* UPDATE PTR TO ADDRESS OF NEXT    00276000
*                                      MQE                           */ 00277000
         B     @RC00092                                         02S0108 00278000
@RF00092 L     MQEPTR,MQELINK(,MQEPTR)                          02S0108 00279000
*             END;                                              02S0109 00280000
*                                                               02S0109 00281000
@RC00092 DS    0H                                               02S0109 00282000
@DE00091 LTR   MQEPTR,MQEPTR                                    02S0109 00283000
         BNZ   @DL00091                                         02S0109 00284000
*             /*******************************************************/ 00285000
*             /*                                                     */ 00286000
*             /* DELETE THE NO-LONGER-NEEDED MMB FROM THE TOP OF THE */ 00287000
*             /* CHAIN                                               */ 00288000
*             /*                                                     */ 00289000
*             /*******************************************************/ 00290000
*                                                               02S0110 00291000
*             CALL DELETMMB;                                    02S0110 00292000
         BAL   @14,DELETMMB                                     02S0110 00293000
*             MMBPTR=UCMMBPTR;      /* GET PTR TO NEW FIRST MMB      */ 00294000
         L     @12,CVTPTR                                       02S0111 00295000
         L     @12,UCMBASPT(,@12)                               02S0111 00296000
         L     MMBPTR,UCMMBPTR(,@12)                            02S0111 00297000
*           END;                                                02S0112 00298000
*                                                               02S0112 00299000
@DE00089 LTR   MMBPTR,MMBPTR                                    02S0112 00300000
         BNZ   @DL00089                                         02S0112 00301000
*/*** END OF SCANMMBQ SEGMENT ****************************************/ 00302000
*                                                               02S0113 00303000
*           GO TO CHEKECBS;         /* RECHECK THE ECBS              */ 00304000
         B     CHEKECBS                                         01S0113 00305000
*         END;                                                  01S0114 00306000
*       ELSE                                                    01S0115 00307000
*         ;                         /* MONITOR ECB IS NOT POSTED     */ 00308000
@RF00085 DS    0H                                               01S0116 00309000
*       DO;                                                     01S0116 00310000
*         CALL FRLOCKS;             /* FREE THE CMS AND LOCAL LOCKS  */ 00311000
         BAL   @14,FRLOCKS                                      01S0117 00312000
*         GO TO WORKWAIT;           /* WAIT FOR WORK TO DO           */ 00313000
         B     WORKWAIT                                         01S0118 00314000
*       END;                                                    01S0119 00315000
*     END;                                                      01S0120 00316000
*   ELSE                                                        01S0121 00317000
*     ;                             /* TERMINATION ECB IS POSTED     */ 00318000
@RF00082 DS    0H                                               01S0122 00319000
*   DO;                                                         01S0122 00320000
*                                                               02S0123 00321000
*/*** START OF CLEANUP SEGMENT ***************************************/ 00322000
*/*                                                                  */ 00323000
*/* CLEANUP - PREPARES TO TERMINATE TPUT TASK                        */ 00324000
*/*                                                                  */ 00325000
*/* FUNCTION - CLEARS THE ECBS.  SCANS THE MMB QUEUE AND CALLS       */ 00326000
*/*    SUBROUTINE DELETMMB TO DECHAIN AND FREE ANY REMAINING MMBS.   */ 00327000
*/*    INDICATES TPUT ROUTINE IS NO LONGER ACTIVE.                   */ 00328000
*/*                                                                  */ 00329000
*/* INPUT - NONE                                                     */ 00330000
*/*                                                                  */ 00331000
*/* OUTPUT - UNCMMNECB = 0                                           */ 00332000
*/*          UCMTRECB = 0                                            */ 00333000
*/*         MMB QUEUE IS EMPTY                                       */ 00334000
*/*         UCMTPUTA = 0 (TPUT ROUTINE NO LONGER ACTIVE)             */ 00335000
*/*                                                                  */ 00336000
*/********************************************************************/ 00337000
*                                                               02S0123 00338000
*     /***************************************************************/ 00339000
*     /*                                                             */ 00340000
*     /* CLEAR ECBS                                                  */ 00341000
*     /*                                                             */ 00342000
*     /***************************************************************/ 00343000
*                                                               02S0123 00344000
*     UCMMNECB=ZERO;                /* CLEAR MONITOR ECB             */ 00345000
         SLR   @12,@12                                          02S0123 00346000
         L     @11,CVTPTR                                       02S0123 00347000
         L     @11,UCMBASPT(,@11)                               02S0123 00348000
         ST    @12,UCMMNECB(,@11)                               02S0123 00349000
*     UCMTRECB=ZERO;                /* CLEAR TERMINATION ECB         */ 00350000
*                                                               02S0124 00351000
         ST    @12,UCMTRECB(,@11)                               02S0124 00352000
*     /***************************************************************/ 00353000
*     /*                                                             */ 00354000
*     /* LOOP THROUGH THE MMB QUEUE AND DELETE EACH MMB              */ 00355000
*     /*                                                             */ 00356000
*     /***************************************************************/ 00357000
*                                                               02S0125 00358000
*     MMBPTR=UCMMBPTR;              /* GET PTR TO FIRST MMB          */ 00359000
         L     MMBPTR,UCMMBPTR(,@11)                            02S0125 00360000
*     DO WHILE(MMBPTR^=ZERO);                                   02S0126 00361000
         B     @DE00126                                         02S0126 00362000
@DL00126 DS    0H                                               02S0127 00363000
*       CALL DELETMMB;              /* DELETE THE MMB FROM THE TOP OF   00364000
*                                      THE QUEUE                     */ 00365000
         BAL   @14,DELETMMB                                     02S0127 00366000
*       MMBPTR=UCMMBPTR;            /* GET PTR TO NEW FIRST MMB      */ 00367000
         L     @12,CVTPTR                                       02S0128 00368000
         L     @12,UCMBASPT(,@12)                               02S0128 00369000
         L     MMBPTR,UCMMBPTR(,@12)                            02S0128 00370000
*     END;                                                      02S0129 00371000
@DE00126 LTR   MMBPTR,MMBPTR                                    02S0129 00372000
         BNZ   @DL00126                                         02S0129 00373000
*     UCMTPUTA='0'B;                /* INDICATE TPUT ROUTINE IS NO      00374000
*                                      LONGER ACTIVE                 */ 00375000
*                                                               02S0130 00376000
         L     @12,CVTPTR                                       02S0130 00377000
         L     @12,UCMBASPT(,@12)                               02S0130 00378000
         NI    UCMTPUTA(@12),B'11011111'                        02S0130 00379000
*/*** END OF CLEANUP SEGMENT *****************************************/ 00380000
*                                                               02S0131 00381000
*     CALL FRLOCKS;                 /* FREE THE CMS AND LOCAL LOCKS  */ 00382000
         BAL   @14,FRLOCKS                                      01S0131 00383000
*     RETURN;                       /* TERMINATE THE TASK            */ 00384000
@EL00001 L     @13,4(,@13)                                      01S0132 00385000
@EF00001 L     @00,@SIZDATD                                     01S0132 00386000
         LR    @01,@09                                          01S0132 00387000
         FREEMAIN R,LV=(0),A=(1)                                        00388000
@ER00001 LM    @14,@12,12(@13)                                  01S0132 00389000
         BR    @14                                              01S0132 00390000
*   END;                                                        01S0133 00391000
*                                                               01S0133 00392000
*   /*****************************************************************/ 00393000
*   /*                                                               */ 00394000
*   /* ENTER HERE FOR ESTAE AND FRR RETRY                            */ 00395000
*   /*                                                               */ 00396000
*   /*****************************************************************/ 00397000
*                                                               01S0134 00398000
*ESTRETRY:                          /* BRANCH ENTER HERE FOR ESTAE      00399000
*                                      RETRY                         */ 00400000
*   CALL OBTLOCKS;                  /* OBTAIN LOCAL AND CMS LOCKS    */ 00401000
ESTRETRY BAL   @14,OBTLOCKS                                     01S0134 00402000
*FRRRETRY:                          /* BRANCH ENTER HERE FOR FRR        00403000
*                                      RETRY                         */ 00404000
*   UCMMNECB=ZERO;                  /* CLEAR MONITOR ECB             */ 00405000
FRRRETRY SLR   @12,@12                                          01S0135 00406000
         L     @11,CVTPTR                                       01S0135 00407000
         L     @11,UCMBASPT(,@11)                               01S0135 00408000
         ST    @12,UCMMNECB(,@11)                               01S0135 00409000
*   UCMTRECB=ZERO;                  /* CLEAR TERMINATION ECB         */ 00410000
         ST    @12,UCMTRECB(,@11)                               01S0136 00411000
*   UCMTPUTA='0'B;                  /* INDICATE TPUT NO LONGER ACTVE */ 00412000
         NI    UCMTPUTA(@11),B'11011111'                        01S0137 00413000
*   CALL FRLOCKS;                   /* FREE LOCAL AND CMS LOCKS      */ 00414000
         BAL   @14,FRLOCKS                                      01S0138 00415000
*   RETURN;                         /* RETURN TO CALLER              */ 00416000
         B     @EL00001                                         01S0139 00417000
*                                                               02S0140 00418000
*/*** START OF FRLOCKS SEGMENT ***************************************/ 00419000
*/*                                                                  */ 00420000
*/* FRLOCKS - SUBROUTINE TO FREE CMS AND LOCAL LOCKS OBTAINED BY     */ 00421000
*/*    OBTLOCKS SUBROUTINE AND FREE THE FRR FROM THE STACK.          */ 00422000
*/*                                                                  */ 00423000
*/* INPUT - R13 = ADDR OF REGISTER SAVE AREA                         */ 00424000
*/*                                                                  */ 00425000
*/* OUTPUT - NONE                                                    */ 00426000
*/*                                                                  */ 00427000
*/********************************************************************/ 00428000
*                                                               02S0140 00429000
*FRLOCKS:                                                       02S0140 00430000
*   PROC OPTIONS(SAVE(8,9,10,11,12,13,14),NOSAVEAREA);          02S0140 00431000
*                                                               02S0140 00432000
FRLOCKS  ST    @14,12(,@13)                                     02S0140 00433000
         STM   @08,@12,52(@13)                                  02S0140 00434000
*   /*****************************************************************/ 00435000
*   /*                                                               */ 00436000
*   /* FREE THE FRR                                                  */ 00437000
*   /*                                                               */ 00438000
*   /*****************************************************************/ 00439000
*                                                               02S0141 00440000
*   GENERATE REFS(FRRSEMP);                                     02S0141 00441000
         SETFRR D,WRKREGS=(REG11,REG12)                                 00442000
*   PARMFRID='0'B;                  /* INDICATE MODULE IS            */ 00443000
         L     @12,PARMPTR                                      02S0142 00444000
         NI    PARMFRID(@12),B'11101111'                        02S0142 00445000
*                                   /* ONLY ESTAE PROTECTED.         */ 00446000
*                                                               02S0143 00447000
*   /*****************************************************************/ 00448000
*   /*                                                               */ 00449000
*   /* SETLOCK DESTROYS REGISTERS 11,12,13,14. REGISTER 13 IS SAVED  */ 00450000
*   /* IN REGISTER 8 AND RESTORED AFTER SETLOCK.                     */ 00451000
*   /*                                                               */ 00452000
*   /*****************************************************************/ 00453000
*                                                               02S0143 00454000
*   RFY                                                         02S0143 00455000
*    (REG8,                                                     02S0143 00456000
*     REG13)RSTD;                   /* RESTRICT REGS 8 AND 13 FROM      00457000
*                                      COMPILER USE                  */ 00458000
*   REG8=REG13;                     /* SAVE PTR TO CALLING PROCEDURES   00459000
*                                      SAVE AREA                     */ 00460000
         LR    REG8,REG13                                       02S0144 00461000
*   GENERATE REFS(PSALITA,FLC);                                 02S0145 00462000
         SETLOCK RELEASE,TYPE=CMS,                                     C00463000
               RELATED=(UCM,IEAVTPUT(OBTLOCKS))                         00464000
         SETLOCK RELEASE,TYPE=LOCAL,                                   C00465000
               RELATED=(UCM,IEAVTPUT(OBTLOCKS))                         00466000
*   REG13=REG8;                     /* RESTORE PTR TO CALLING   02S0146 00467000
*                                      PROCEDURES SAVE AREA          */ 00468000
         LR    REG13,REG8                                       02S0146 00469000
*   RFY                                                         02S0147 00470000
*    (REG8,                                                     02S0147 00471000
*     REG13)UNRSTD;                 /* RETURN REGS 8 AND 13 TO  02S0147 00472000
*                                      COMPILER USE                  */ 00473000
*   END FRLOCKS;                                                02S0148 00474000
*                                                               02S0148 00475000
@EL00002 DS    0H                                               02S0148 00476000
@EF00002 DS    0H                                               02S0148 00477000
@ER00002 L     @14,12(,@13)                                     02S0148 00478000
         LM    @08,@12,52(@13)                                  02S0148 00479000
         BR    @14                                              02S0148 00480000
*/*** END OF FRLOCKS SEGMENT *****************************************/ 00481000
*                                                               02S0149 00482000
*                                                               02S0149 00483000
*/*** START OF OBTLOCKS SEGMENT **************************************/ 00484000
*/*                                                                  */ 00485000
*/* OBTLOCKS - SUBROUTINE TO OBTAIN LOCAL AND CMS LOCKS TO SERIALIZE */ 00486000
*/*           USE OF THE UCM, MQE CHAIN AND MMB CHAIN.               */ 00487000
*/*           OBTLOCKS WILL ALSO SET AN FRR AND INITIALIZE THE       */ 00488000
*/*           USER PARAMETER LIST.                                   */ 00489000
*/*                                                                  */ 00490000
*/* INPUT - R13 = ADDR OF REGISTER SAVE AREA                         */ 00491000
*/*                                                                  */ 00492000
*/* OUTPUT - NONE                                                    */ 00493000
*/*                                                                  */ 00494000
*/********************************************************************/ 00495000
*                                                               02S0149 00496000
*OBTLOCKS:                                                      02S0149 00497000
*   PROC OPTIONS(SAVE(8,9,10,11,12,13,14),NOSAVEAREA);          02S0149 00498000
*                                                               02S0149 00499000
OBTLOCKS ST    @14,12(,@13)                                     02S0149 00500000
         STM   @08,@12,52(@13)                                  02S0149 00501000
*   /*****************************************************************/ 00502000
*   /*                                                               */ 00503000
*   /* SETLOCK DESTROYS REGISTERS 11,12,13,14. REGISTER 13 IS SAVED  */ 00504000
*   /* IN REGISTER 8 AND RESTORED AFTER SETLOCK.                     */ 00505000
*   /*                                                               */ 00506000
*   /*****************************************************************/ 00507000
*                                                               02S0150 00508000
*   RFY                                                         02S0150 00509000
*    (REG8,                                                     02S0150 00510000
*     REG13)RSTD;                   /* RESTRICT REGS 8 AND 13 FROM      00511000
*                                      COMPILER USE                  */ 00512000
*   REG8=REG13;                     /* SAVE PTR TO CALLING PROCEDURES   00513000
*                                      SAVE AREA                     */ 00514000
         LR    REG8,REG13                                       02S0151 00515000
*   GENERATE REFS(PSALITA,FLC);                                 02S0152 00516000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        C00517000
               RELATED=(UCM,IEAVTPUT(FRLOCKS))                          00518000
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,                          C00519000
               RELATED=(UCM,IEAVTPUT(FRLOCKS))                          00520000
*   REG13=REG8;                     /* RESTORE PTR TO CALLING   02S0153 00521000
*                                      PROCEDURES SAVE AREA          */ 00522000
         LR    REG13,REG8                                       02S0153 00523000
*   GENERATE;                                                   02S0154 00524000
*                                                               02S0154 00525000
*                                      /* RESTORE REGS               */ 00526000
         L   REG14,12(,REG13)          RESTORE REG14                    00527000
         LM  REG8,REG12,52(REG13)      RESTORE REG8 THRU REG12          00528000
*   /*****************************************************************/ 00529000
*   /*                                                               */ 00530000
*   /* SETTUP FOR FRR                                                */ 00531000
*   /*                                                               */ 00532000
*   /*****************************************************************/ 00533000
*                                                               02S0155 00534000
*   RESPECIFY                                                   02S0155 00535000
*    (REG1)RESTRICTED;                                          02S0155 00536000
*   REG1=UCMFRRAD;                  /* COMTASK RECOVERY ADDRESS      */ 00537000
*                                                               02S0156 00538000
         L     @12,CVTPTR                                       02S0156 00539000
         L     @12,UCMBASPT(,@12)                               02S0156 00540000
         L     REG1,UCMFRRAD(,@12)                              02S0156 00541000
*   /*****************************************************************/ 00542000
*   /*                                                               */ 00543000
*   /* ISSUE SETFRR                                                  */ 00544000
*   /*                                                               */ 00545000
*   /*****************************************************************/ 00546000
*                                                               02S0157 00547000
*   GEN REFS(PSACSTK,FRRSCURR,FRRSLAST,PSALSFCC,FRRSENTR,FRRSELEN,      00548000
*       FRRSFRRA,FRRSPARM)SETS(REG11,REG12);                    02S0157 00549000
           SETFRR A,FRRAD=(REG1),PARMAD=PARMFRR,WRKREGS=(REG11,REG12)   00550000
*   PARMFRR->PARMRGAD=ADDR(RCVRSAVE);/* ADDR OF REG SAVEAREA         */ 00551000
         LA    @12,RCVRSAVE                                     02S0158 00552000
         L     @11,PARMFRR                                      02S0158 00553000
         ST    @12,PARMRGAD(,@11)                               02S0158 00554000
*   PARMFRR->PARMID=MASV;           /* IDENTIFY MODULE TO FRR        */ 00555000
         L     @12,PARMFRR                                      02S0159 00556000
         MVC   PARMID(4,@12),@CC00078                           02S0159 00557000
*   PARMFRR->PARMRTAD=ADDR(FRRRETRY);/* RETRY ADDR IN IEAVMQR0       */ 00558000
         LA    @12,FRRRETRY                                     02S0160 00559000
         L     @11,PARMFRR                                      02S0160 00560000
         ST    @12,PARMRTAD(,@11)                               02S0160 00561000
*   PARMFRID='1'B;                  /* INDICATE BOTH ESTAE AND       */ 00562000
         L     @12,PARMPTR                                      02S0161 00563000
         OI    PARMFRID(@12),B'00010000'                        02S0161 00564000
*                                   /* FRR PROTECTION IN ESTAE       */ 00565000
*                                   /* USER PARMLIST.                */ 00566000
*   RFY                                                         02S0162 00567000
*    (REG8,                                                     02S0162 00568000
*     REG13,                                                    02S0162 00569000
*     REG1)UNRESTRICTED;                                        02S0162 00570000
*   END OBTLOCKS;                                               02S0163 00571000
*                                                               02S0163 00572000
@EL00003 DS    0H                                               02S0163 00573000
@EF00003 DS    0H                                               02S0163 00574000
@ER00003 L     @14,12(,@13)                                     02S0163 00575000
         LM    @08,@12,52(@13)                                  02S0163 00576000
         BR    @14                                              02S0163 00577000
*/*** END OF OBTLOCKS SEGMENT ****************************************/ 00578000
*                                                               02S0164 00579000
*                                                               02S0164 00580000
*/*** START OF DELETMMB SEGMENT **************************************/ 00581000
*/*                                                                  */ 00582000
*/* DELETMMB - DELETE AN MMB                                         */ 00583000
*/*                                                                  */ 00584000
*/* FUNCTION - DECHAIN THE MMB FROM THE HEAD OF THE QUEUE AND FREE   */ 00585000
*/*    IT VIA THE BRANCH ENTRY TO FREEMAIN.                          */ 00586000
*/*                                                                  */ 00587000
*/* INPUT - MMBPTR                                                   */ 00588000
*/*                                                                  */ 00589000
*/* OUTPUT - UCMMBPTR = ADDR OF NEXT MMB OR ZERO                     */ 00590000
*/*          UCMMBEND = 0 IF LAST MMB WAS DELETED                    */ 00591000
*/*                                                                  */ 00592000
*/********************************************************************/ 00593000
*                                                               02S0164 00594000
*DELETMMB:                                                      02S0164 00595000
*   PROC OPTIONS(SAVE(0,1,4,7,14,15),NOSAVEAREA);               02S0164 00596000
*                                                               02S0164 00597000
DELETMMB STM   @14,@01,12(@13)                                  02S0164 00598000
         ST    @04,36(,@13)                                     02S0164 00599000
         ST    @07,48(,@13)                                     02S0164 00600000
*   /*****************************************************************/ 00601000
*   /*                                                               */ 00602000
*   /* REGS 0,1,4,7,14,15 ARE SAVED BECAUSE THEY ARE USED FOR THE    */ 00603000
*   /* BRANCH ENTRY TO FREEMAIN. DECHAIN MMB FROM THE TOP OF THE     */ 00604000
*   /* QUEUE                                                         */ 00605000
*   /*                                                               */ 00606000
*   /*****************************************************************/ 00607000
*                                                               02S0165 00608000
*   UCMMBPTR=MMBLINK;               /* PTR TO TOP OF THE MMB QUEUE IS   00609000
*                                      PTR TO NEXT MMB               */ 00610000
         L     @12,CVTPTR                                       02S0165 00611000
         L     @12,UCMBASPT(,@12)                               02S0165 00612000
         L     @11,MMBLINK(,MMBPTR)                             02S0165 00613000
         ST    @11,UCMMBPTR(,@12)                               02S0165 00614000
*   IF UCMMBPTR=ZERO THEN           /* ARE THERE ANY MMBS LEFT?      */ 00615000
         SLR   @08,@08                                          02S0166 00616000
         CR    @11,@08                                          02S0166 00617000
         BNE   @RF00166                                         02S0166 00618000
*     UCMMBEND=ZERO;                /* NO, CLEAR PTR TO END OF MMB      00619000
*                                      QUEUE                         */ 00620000
         ST    @08,UCMMBEND(,@12)                               02S0167 00621000
*   ELSE                                                        02S0168 00622000
*     UCMMBPTR->MMBBKPTR=ZERO;      /* YES, CLEAR BACK PTR OF FIRST     00623000
*                                      MMB                           */ 00624000
*                                                               02S0168 00625000
         B     @RC00166                                         02S0168 00626000
@RF00166 L     @12,CVTPTR                                       02S0168 00627000
         L     @12,UCMBASPT(,@12)                               02S0168 00628000
         L     @12,UCMMBPTR(,@12)                               02S0168 00629000
         SLR   @11,@11                                          02S0168 00630000
         ST    @11,MMBBKPTR(,@12)                               02S0168 00631000
*   /*****************************************************************/ 00632000
*   /*                                                               */ 00633000
*   /* SET UP PARAMETERS FOR BRANCH ENTRY TO FREEMAIN                */ 00634000
*   /*                                                               */ 00635000
*   /*****************************************************************/ 00636000
*                                                               02S0169 00637000
*   RFY                                                         02S0169 00638000
*    (REG4,                                                     02S0169 00639000
*     REG7)RSTD;                    /* RESTRICT PARAMETER REGS FROM     00640000
*                                      COMPILER USE                  */ 00641000
@RC00166 DS    0H                                               02S0170 00642000
*   REG4=UCMPXA;                    /* COMMTASK TCB ADDR             */ 00643000
         L     @12,CVTPTR                                       02S0170 00644000
         L     @12,UCMBASPT(,@12)                               02S0170 00645000
         L     REG4,UCMPXA(,@12)                                02S0170 00646000
*   REG7=UCMASCB;                   /* COMMTASK ASCB ADDR            */ 00647000
*                                                               02S0171 00648000
         L     REG7,UCMASCB(,@12)                               02S0171 00649000
*   /*****************************************************************/ 00650000
*   /*                                                               */ 00651000
*   /* FREE THE NO-LONGER-NEEDED MMB                                 */ 00652000
*   /*                                                               */ 00653000
*   /*****************************************************************/ 00654000
*                                                               02S0172 00655000
*   DO;                             /* FREEMAIN(R) LV(LENGTH(MMB))SP(   00656000
*                                      MMBSUBPL)A(MMB) BRANCH        */ 00657000
*     RESPECIFY                                                 02S0173 00658000
*      (GPR01F,                                                 02S0173 00659000
*       GPR00F)RESTRICTED;                                      02S0173 00660000
*     GPR00F=LENGTH(MMB);           /* LENGTH REQUESTED              */ 00661000
         LA    GPR00F,144                                       02S0174 00662000
*     GPR01F=MMBSUBPL;              /* SP IN REG 1                   */ 00663000
         LA    GPR01F,250                                       02S0175 00664000
*     GEN(SLL   GPR01F,24)                                      02S0176 00665000
*     NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;              02S0176 00666000
         SLL   GPR01F,24                                                00667000
*     GPR00F=GPR00F|GPR01F;         /* SP IN HI BYTE                 */ 00668000
         OR    GPR00F,GPR01F                                    02S0177 00669000
*     GPR01F=ADDR(MMB);             /* SET REGISTER ONE              */ 00670000
         LR    GPR01F,MMBPTR                                    02S0178 00671000
*     CALL CVTRMBR;                 /* BRANCH ENTRY POINT            */ 00672000
         L     @12,CVTPTR                                       02S0179 00673000
         L     @15,CVTRMBR(,@12)                                02S0179 00674000
         BALR  @14,@15                                          02S0179 00675000
*     RESPECIFY                                                 02S0180 00676000
*      (GPR01F,                                                 02S0180 00677000
*       GPR00F)UNRESTRICTED;                                    02S0180 00678000
*   END;                            /* FREEMAIN(R) LV(LENGTH(MMB))SP(   00679000
*                                      MMBSUBPL)A(MMB) BRANCH        */ 00680000
*   RFY                                                         02S0182 00681000
*    (REG4,                                                     02S0182 00682000
*     REG7)UNRSTD;                  /* RETURN PARAMETER REGS TO 02S0182 00683000
*                                      COMPILER USE                  */ 00684000
*   END;                                                        02S0183 00685000
*                                                               02S0183 00686000
@EL00004 DS    0H                                               02S0183 00687000
@EF00004 DS    0H                                               02S0183 00688000
@ER00004 LM    @14,@01,12(@13)                                  02S0183 00689000
         L     @04,36(,@13)                                     02S0183 00690000
         L     @07,48(,@13)                                     02S0183 00691000
         BR    @14                                              02S0183 00692000
*/*** END OF DELETMMB SEGMENT ****************************************/ 00693000
*                                                               02S0184 00694000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 00695000
*     GPR00F FIXED(31)REG(0),                                   01S0184 00696000
*     GPR01F FIXED(31)REG(1),                                   01S0184 00697000
*     GPR00P PTR(31)REG(0),                                     01S0184 00698000
*     GPR01P PTR(31)REG(1);                                     01S0184 00699000
*   DECLARE                         /* COMMON VARIABLES              */ 00700000
*     I256C CHAR(256)BASED,                                     01S0185 00701000
*     I031F FIXED(31)BASED,                                     01S0185 00702000
*     I031P PTR(31)BASED,                                       01S0185 00703000
*     I015F FIXED(15)BASED,                                     01S0185 00704000
*     I015P PTR(15)BASED,                                       01S0185 00705000
*     I008P PTR(8)BASED,                                        01S0185 00706000
*     I001C CHAR(1)BASED;                                       01S0185 00707000
*   END                                                         01S0186 00708000
*                                                               01S0186 00709000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 00710000
