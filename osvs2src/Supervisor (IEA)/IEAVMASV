         TITLE 'IEAVMASV, COMMTASK ASYNCHRONOUS SERVICES               *00001000
                       '                                                00002000
IEAVTPUT CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(19)                                          01S0001 00007000
         DC    C'IEAVTPUTR0200073236'                           01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @10,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@10                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         GETMAIN R,LV=(0)                                               00015000
         LR    @09,@01                                          01S0001 00016000
         USING @DATD,@09                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @09,8(,@13)                                      01S0001 00020000
         LR    @13,@09                                          01S0001 00021000
*                                                               02S0058 00022000
*/*** END OF DATADECL SEGMENT ****************************************/ 00023000
*                                                               02S0058 00024000
*                                                               01S0058 00025000
*   /*****************************************************************/ 00026000
*   /*                                                               */ 00027000
*   /* ESTAE RECOVERY CODE                                           */ 00028000
*   /*                                                               */ 00029000
*   /*****************************************************************/ 00030000
*                                                               01S0058 00031000
*   RCVPARM=RCVPARM&&RCVPARM;       /* CLEAR ESTAE PARM LIST         */ 00032000
         XC    RCVPARM(24),RCVPARM                              01S0058 00033000
*   RESPECIFY                                                   01S0059 00034000
*    (REG2)RESTRICTED;                                          01S0059 00035000
*   REG2=UCMFRRAD;                  /* SET ADDRESS OF COMTASK        */ 00036000
         L     @12,CVTPTR                                       01S0060 00037000
         L     @12,UCMBASPT(,@12)                               01S0060 00038000
