         TITLE 'IEAVMDOM - COMM TASK DOM PROCESSOR                     *00001000
                        '                                               00002000
IEAVMDOM CSECT                                                          00003000
* A 079820                                                    @ZA00065  00004000
* C (POSTOECB) APPROX 134730                                   @ZA03242 00005000
* A (MSGPROC) APPROX. 183213, 183223                           @ZA08117 00006000
* C (MSGPROC) APPROX 184175                                    @ZA09205 00007000
         USING *,REG15            ENTRY POINT IS IN REG 15              00008000
         MODID BR=YES                                                   00009000
         USING UCMPRFX,REG4       REG4 POINTS AT THE UCMPRFX            00010000
         LA    REG13,UCMSAVE0     LOAD ADDR OF UCM SAVEAREA.            00011000
         DROP  REG4                                                     00012000
         STM   REG14,REG12,12(REG13)   SAVE CALLERS REGISTERS           00013000
         DROP  REG15                                                    00014000
         BALR  REG10,0                                                  00015000
@PSTART  DS    0H                                                       00016000
         USING @PSTART,REG10                                            00017000
         USING @DATD,REG9                                               00018000
         L     REG4,PSATOLD       PICK UP ADDR OF CURRENT TCB           00019000
         L     REG7,PSAAOLD       AND CURRENT ASCB FOR BRANCH ENTRY     00020000
         L     REG0,@SIZDATD      AUTOMATIC DATA AREA                   00021000
         GETMAIN  R,LV=(0),BRANCH=YES                                   00022000
         LR    REG9,REG1          LOAD DATAREG                          00023000
         ST    REG13,@SA00001+4   LINK UP CALLERS SAVEAREA              00024000
         ST    REG9,8(,REG13)     AND  OUR SAVEAREA                     00025000
         LR    REG13,REG9         SET OUR SAVEAREA POINTER              00026000
*                                      STACK                 @ZM02352*/ 00027000
*                                                               01S0098 00028000
*   /*****************************************************************/ 00029000
*   /*                                                               */ 00030000
*   /* INITIALIZE LOCAL POINTERS AND FLAGS                           */ 00031000
*   /*                                                               */ 00032000
*   /*****************************************************************/ 00033000
*                                                               01S0098 00034000
*   LUCMPTR=UCMBASPT;               /* INIT LOCAL ADDR OF THE UCMBASE*/ 00035000
         L     @12,CVTPTR                                       01S0098 00036000
         L     LUCMPTR,UCMBASPT(,@12)                           01S0098 00037000
*   MDOMFLGS=ZERO;                  /* NULL OUT FLAGS BYTE           */ 00038000
         MVI   MDOMFLGS,X'00'                                   01S0099 00039000
*   MSGPTR=ZERO;                    /* THERE ISN'T A MSG WPL YET        00040000
*                                      OBTAINED              @Y02668P*/ 00041000
         SLR   @12,@12                                          01S0100 00042000
         ST    @12,MSGPTR                                       01S0100 00043000
*/*  ********************************************************@Y02668P*/ 00044000
*/*   THE FOLLOWING SET OF INSTRUCTIONS WILL CREATE ANOTHER  @Y02668P*/ 00045000
*/*   FUNCTIONAL RECOVERY ROUTINE TO PROTECT THE DOM         @Y02668P*/ 00046000
*/*   PROCESSING. UPON ENTRY TO THIS MODULE, THE LOCAL AND   @Y02668P*/ 00047000
*/*   CMS LOCKS ARE HELD AND FRR IS CURRENTLY ON THE FRR     @Y02668P*/ 00048000
*/*   STACK.                                                 @Y02668P*/ 00049000
*/*  ********************************************************@Y02668P*/ 00050000
*                                                               01S0101 00051000
*   CALL SETFRRTN;                  /* SET RECOVERY ENVIRONMENT 01S0101 00052000
*                                                            @Y02668P*/ 00053000
         BAL   @14,SETFRRTN                                     01S0101 00054000
*   PARMFTPT=FTPTDMST;              /* IND. START OF DOM PROCESSING     00055000
*                                                            @Y02668P*/ 00056000
         L     @12,PARMPTR                                      01S0102 00057000
         MVI   PARMFTPT(@12),X'01'                              01S0102 00058000
*   STM(REG0,REG15,RECREGS);        /* SAVE ALL REGS IN PARMLIST FRR    00059000
*                                                            @Y02668P*/ 00060000
         STM   REG0,REG15,RECREGS                               01S0103 00061000
*   STM(REG0,REG15,RECOREGS);       /* SAVE ORIGINAL REGS    @Y02668P*/ 00062000
         STM   REG0,REG15,RECOREGS                              01S0104 00063000
*   PARMRTAD=ADDR(DOMCOMP);         /* SET RETRY ADDR        @Y02668P*/ 00064000
*                                                               01S0105 00065000
         LA    @11,DOMCOMP                                      01S0105 00066000
         ST    @11,PARMRTAD(,@12)                               01S0105 00067000
*   /*****************************************************************/ 00068000
*   /*                                                               */ 00069000
*   /* LOOP UNTIL THE DOMCB CHAIN IS EMPTY                           */ 00070000
*   /*                                                               */ 00071000
*   /*****************************************************************/ 00072000
*                                                               01S0106 00073000
*   DO WHILE(UCMDOME^=ZERO);                                    01S0106 00074000
*                                                               01S0106 00075000
         B     @DE00106                                         01S0106 00076000
@DL00106 DS    0H                                               01S0107 00077000
*     /***************************************************************/ 00078000
*     /*                                                             */ 00079000
*     /* NOW LOOP THRU THE DOMCBS ON THE UCMDOME CHAIN               */ 00080000
*     /*                                                             */ 00081000
*     /***************************************************************/ 00082000
*                                                               01S0107 00083000
*     DO DOMCPTR=UCMDOME BY ZERO WHILE(DOMCPTR^=ZERO);          01S0107 00084000
*                                                               01S0107 00085000
         L     @12,CVTPTR                                       01S0107 00086000
         L     @12,UCMBASPT(,@12)                               01S0107 00087000
         AL    @12,@CF02657                                     01S0107 00088000
         L     @12,UCMMCSPT(,@12)                               01S0107 00089000
         L     DOMCPTR,UCMDOME(,@12)                            01S0107 00090000
         B     @DE00107                                         01S0107 00091000
@DL00107 DS    0H                                               01S0108 00092000
*       /*************************************************************/ 00093000
*       /*                                                           */ 00094000
*       /* CHECK IF THIS DOMCB HAS A MESSAGE ID.                     */ 00095000
*       /*                                                           */ 00096000
*       /*************************************************************/ 00097000
*                                                               01S0108 00098000
*       IF DOMCDBAJ=NO&             /* IS DOM BY ASID AND JSTCB OFF  */ 00099000
*           DOMCDBL=NO THEN         /* AND IS DOM BY ASID ONLY OFF   */ 00100000
         TM    DOMCDBAJ(DOMCPTR),B'00001010'                    01S0108 00101000
         BNZ   @RF00108                                         01S0108 00102000
*         DO;                       /* YES, THEN THIS IS A DOM BY MSG   00103000
*                                      ID                            */ 00104000
*           ENDOMLP=NO;             /* TURN OFF END OF IDS FOUND FLAG*/ 00105000
*                                                               01S0110 00106000
         NI    ENDOMLP,B'11101111'                              01S0110 00107000
*           /*********************************************************/ 00108000
*           /*                                                       */ 00109000
*           /* LOOP THRU ALL THE IDS IN THE DOMCB                    */ 00110000
*           /*                                                       */ 00111000
*           /*********************************************************/ 00112000
*                                                               01S0111 00113000
*           DO IDPTR=DOMCPTR+LENGTH(DOMCLNK) BY LENGTH(DOMCFLAG)/EIGHT+ 00114000
*                 LENGTH(DOMCIDA) WHILE(ENDOMLP=NO);            01S0111 00115000
*                                                               01S0111 00116000
         LA    @12,4                                            01S0111 00117000
         ALR   @12,DOMCPTR                                      01S0111 00118000
         ST    @12,IDPTR                                        01S0111 00119000
         B     @DE00111                                         01S0111 00120000
@DL00111 DS    0H                                               01S0112 00121000
*/***********************************************************@Y02668P*/ 00122000
*/* TEST THE DOMCB TO DETERMINE IF THE SUBSYSTEM EXIT HAS    @Y02668P*/ 00123000
*/* BEEN TAKEN FOR THIS DOM CONTROL BLOCK. IF IT HAS NOT     @Y02668P*/ 00124000
*/* BEEN TAKEN, THE SUBSYSTEM WILL HAVE A CHANCE TO VIEW     @Y02668P*/ 00125000
*/* THE DOMCB OUT OF THIS MODULE. IF THE EXIT HAS BEEN TAKEN @Y02668P*/ 00126000
*/* IT WILL NOT BE TAKEN AGAIN.                              @Y02668P*/ 00127000
*/***********************************************************@Y02668P*/ 00128000
*                                                               01S0112 00129000
*             IF DOMCSEXT=YES THEN  /* EXIT TO BE TAKEN      @Y02668P*/ 00130000
         L     @12,IDPTR                                        01S0112 00131000
         TM    DOMCSEXT(@12),B'01000000'                        01S0112 00132000
         BNO   @RF00112                                         01S0112 00133000
*               DO;                 /* IF EXIT BIT IS ON     @Y02668P*/ 00134000
*                                                               02S0114 00135000
*/***********************************************************@Y02668P*/ 00136000
*/*                                                          @Y02668P*/ 00137000
*/*           SUBSYSTEM EXIT SEGMENT                         @Y02668P*/ 00138000
*/*                                                          @Y02668P*/ 00139000
*/* FUNCTION - THIS SEGMENT WILL BUILD THE SUBSYSTEM         @Y02668P*/ 00140000
*/*            INTERFACE CONTROL BLOCKS (SSOB AND SSDM) AND  @Y02668P*/ 00141000
*/*            WILL PASS THE SUBSYSTEM THE DOM CONTROL BLOCK @Y02668P*/ 00142000
*/*            PASSED.                                       @Y02668P*/ 00143000
*/*                                                          @Y02668P*/ 00144000
*/* INPUT - DOM CONTROL BLOCK PASSED UPON ENTRY              @Y02668P*/ 00145000
*/*                                                          @Y02668P*/ 00146000
*/* OUTPUT - THE SUBSYSTEM EXIT IS CALLED                    @Y02668P*/ 00147000
*/*                                                          @Y02668P*/ 00148000
*/***********************************************************@Y02668P*/ 00149000
*                                                               02S0114 00150000
*                 /***************************************************/ 00151000
*                 /*                                                 */ 00152000
*                 /* INITIALIZE THE SSOB                     @Y02668P*/ 00153000
*                 /*                                                 */ 00154000
*                 /***************************************************/ 00155000
*                                                               02S0114 00156000
*                 SSOBID=NAMESSOB;  /* SET SSOB IDENTIFIER   @Y02668P*/ 00157000
         MVC   SSOBID(4),@CC00118                               02S0114 00158000
*                 SSOBLEN=LENGTH(SSOB);/* SET LENGTH OF SSOB @Y02668P*/ 00159000
         MVC   SSOBLEN(2),@CH01406                              02S0115 00160000
*                 SSOBFUNC=SSOBDOM; /* INDICATE DOM EXIT     @Y02668P*/ 00161000
         MVC   SSOBFUNC(2),@CH00896                             02S0116 00162000
*                 SSOBSSIB=ZERO;    /* INDICATE NO SSIB      @Y02668P*/ 00163000
         SLR   @12,@12                                          02S0117 00164000
         ST    @12,SSOBSSIB                                     02S0117 00165000
*                 SSOBINDV=ADDR(SSDM);/* IND. DOM AS FUNCTIONAL AREA    00166000
*                                                            @Y02668P*/ 00167000
*                                                               02S0118 00168000
         LA    @12,SSDM                                         02S0118 00169000
         ST    @12,SSOBINDV                                     02S0118 00170000
*                 /***************************************************/ 00171000
*                 /*                                                 */ 00172000
*                 /* INITIALIZE THE SSDM                     @Y02668P*/ 00173000
*                 /*                                                 */ 00174000
*                 /***************************************************/ 00175000
*                                                               02S0119 00176000
*                 SSDMLEN=LENGTH(SSDM);/* SET LENGTH OF SSDM @Y02668P*/ 00177000
         MVC   SSDMLEN(2),@CH00070                              02S0119 00178000
*                 SSDMDMCB=ADDR(DOMC);/* SET ADDR OF DOM CONTROL BLK    00179000
*                                                            @Y02668P*/ 00180000
         ST    DOMCPTR,SSDMDMCB                                 02S0120 00181000
*                 CALL RELFRRTN;    /* RELEASE DOM FRR       @Y02668P*/ 00182000
         BAL   @14,RELFRRTN                                     02S0121 00183000
*                 CALL FRELCKS;     /* RELEASE ORIGINAL FRR  @Y02668P*/ 00184000
         BAL   @14,FRELCKS                                      02S0122 00185000
*                 CALL SETESTAE;    /* CREATE ESTAE ENVIRONMENT 02S0123 00186000
*                                                            @Y02668P*/ 00187000
         BAL   @14,SETESTAE                                     02S0123 00188000
*                 PARMFTPT=FTPTSECT;/* IND. SUBSYSTEM EXIT IN CONTROL   00189000
*                                                            @Y02668P*/ 00190000
         L     @12,PARMPTR                                      02S0124 00191000
         MVI   PARMFTPT(@12),X'05'                              02S0124 00192000
*                 PARMRTAD=ADDR(SUBRETN);/* SET RETRY ADDR   @Y02668P*/ 00193000
         LA    @11,SUBRETN                                      02S0125 00194000
         ST    @11,PARMRTAD(,@12)                               02S0125 00195000
*                 STM(REG0,REG15,RECREGS);/* SAVE ALL REGS   @Y02668P*/ 00196000
*                                                               02S0126 00197000
         STM   REG0,REG15,RECREGS                               02S0126 00198000
*                 /***************************************************/ 00199000
*                 /*                                                 */ 00200000
*                 /* CALL THE SUBSYSTEM - ? IEFSSREQ (SSOB); @Y02668P*/ 00201000
*                 /* ? IEFSSREQ (SSOB);                              */ 00202000
*                 /*                                                 */ 00203000
*                 /***************************************************/ 00204000
*                                                               02S0127 00205000
*/*                                                                  */ 00206000
*/*  PASS CONTROL TO JOB ENTRY SUBSYSTEM TO                          */ 00207000
*/*  PROCESS REQUEST -                                               */ 00208000
*/*                                                                  */ 00209000
*/*    INPUT - REGISTER 1 POINTS TO A ONE-                           */ 00210000
*/*            WORD PARAMETER LIST WHICH                             */ 00211000
*/*            POINTS TO THE SSOB                                    */ 00212000
*/*                                                                  */ 00213000
*                                                               02S0127 00214000
*                 CALL SSREQ(SSOB); /*                       @Y02668P*/ 00215000
         LA    @12,SSOB                                         02S0127 00216000
         ST    @12,@AL00001                                     02S0127 00217000
         MVI   @AL00001,X'80'                                   02S0127 00218000
         L     @12,CVTPTR                                       02S0127 00219000
         L     @12,CVTJESCT(,@12)                               02S0127 00220000
         L     @15,JESSSREQ(,@12)                               02S0127 00221000
         LA    @01,@AL00001                                     02S0127 00222000
         BALR  @14,@15                                          02S0127 00223000
*SUBRETN:                                                       02S0128 00224000
*                 GEN SETS(REG0)(ESTAE   0);/* FREE ESTAE    @ZA03871*/ 00225000
SUBRETN  ESTAE   0                                                      00226000
*                 CALL SETLCKS;     /* REGAIN LOCKS AND FRR  @Y02668P*/ 00227000
         BAL   @14,SETLCKS                                      02S0129 00228000
*                 CALL SETFRRTN;    /* REGAIN DOM FRR        @Y02668P*/ 00229000
         BAL   @14,SETFRRTN                                     02S0130 00230000
*                 PARMFTPT=FTPTDMST;/* IND. DOM PROCESSING   @Y02668P*/ 00231000
         L     @12,PARMPTR                                      02S0131 00232000
         MVI   PARMFTPT(@12),X'01'                              02S0131 00233000
*                 RECREGS=RECOREGS; /* RESTORE ORIGINAL REGS @Y02668P*/ 00234000
         MVC   RECREGS(64),RECOREGS                             02S0132 00235000
*                 PARMRTAD=ADDR(DOMCOMP);/* SET RETRY ADDR   @Y02668P*/ 00236000
         LA    @11,DOMCOMP                                      02S0133 00237000
         ST    @11,PARMRTAD(,@12)                               02S0133 00238000
*/***********      END  SUBEXIT SEGMENT     *****************@Y02668P*/ 00239000
*                                                               02S0134 00240000
*               END;                /* END EXIT TEST         @Y02668P*/ 00241000
*                                                               01S0134 00242000
*             /*******************************************************/ 00243000
*             /*                                                     */ 00244000
*             /* NOW LOOP THRU ALL THE WQES ON THE WQE CHAIN         */ 00245000
*             /*                                                     */ 00246000
*             /*******************************************************/ 00247000
*                                                               01S0135 00248000
*             DO CHAINPTR=UCMWTOQ BY ZERO WHILE(CHAINPTR^=ZERO);        00249000
*                                                               01S0135 00250000
@RF00112 L     CHAINPTR,UCMWTOQ(,LUCMPTR)                       01S0135 00251000
         B     @DE00135                                         01S0135 00252000
@DL00135 DS    0H                                               01S0136 00253000
*               /*****************************************************/ 00254000
*               /*                                                   */ 00255000
*               /* CHECK IF MESSAGE ID MATCHS THIS SEQUENCE NUMBER   */ 00256000
*               /*                                                   */ 00257000
*               /*****************************************************/ 00258000
*                                                               01S0136 00259000
*               IF DOMCIDA=WQESEQN THEN                         01S0136 00260000
         L     @12,IDPTR                                        01S0136 00261000
         CLC   DOMCIDA(3,@12),WQESEQN(CHAINPTR)                 01S0136 00262000
         BNE   @RF00136                                         01S0136 00263000
*                 DO;               /* YES, MESSAGE IDS MATCH. CHECK    00264000
*                                      FOR A MULTI-LINE WTO          */ 00265000
*                   IF WMJMMLW^=ZERO THEN/* IS THIS A MULTI-LINE WTO */ 00266000
         CLI   WMJMMLW(CHAINPTR),0                              01S0138 00267000
         BE    @RF00138                                         01S0138 00268000
*                     DO;           /* YES. INSURE MWTO IS ENDED     */ 00269000
*                                                               02S0140 00270000
*/********************************************************************/ 00271000
*/*   MWTOPROC SEGMENT                                               */ 00272000
*/*                                                                  */ 00273000
*/*   FUNCTION - PROCESS THE MULTI-LINE WTO-S. THIS SEGMENT WILL     */ 00274000
*/*   INSURE THAT THE MULTI-LINE WTO IS ENDED. IF THERE IS NO ROOM   */ 00275000
*/*   FOR AN ENDLINE IN ONE OF THE MINORS, IT USES AN AREA IN THE    */ 00276000
*/*   MAJOR. THIS 'FAKE' MINOR IS MARKED AS 'ABENDS' MINOR.          */ 00277000
*/*                                                                  */ 00278000
*/*   INPUT - CHAINPTR PTS AT THE WQE BEING PROCESSED.               */ 00279000
*/*                                                                  */ 00280000
*/*   OUTPUT- THE MLWTO IS ENDED IF IT WAS ISSUED BY THE TERMINATING */ 00281000
*/*   MEMORY OR TASK                                                 */ 00282000
*/*                                                                  */ 00283000
*/********************************************************************/ 00284000
*                                                               02S0140 00285000
*                       /*********************************************/ 00286000
*                       /*                                           */ 00287000
*                       /* INSURE THAT THIS MLWTO ISN'T WAITING FOR A*/ 00288000
*                       /* WQE TO BE RELEASED                        */ 00289000
*                       /*                                           */ 00290000
*                       /*********************************************/ 00291000
*                                                               02S0140 00292000
*                       WMJMWAIT=NO;/* TURN OFF WAITING FOR WQE BIT  */ 00293000
*                                                               02S0140 00294000
         NI    WMJMWAIT(CHAINPTR),B'01111111'                   02S0140 00295000
*                       /*********************************************/ 00296000
*                       /*                                           */ 00297000
*                       /* INSURE THE MAJOR IS ENDED                 */ 00298000
*                       /*                                           */ 00299000
*                       /*********************************************/ 00300000
*                                                               02S0141 00301000
*                       MINORPTR=WMJMMIN;/* POINT AT FIRST MINOR WQE */ 00302000
*                                                               02S0141 00303000
         L     MINORPTR,WMJMMIN(,CHAINPTR)                      02S0141 00304000
*                       /*********************************************/ 00305000
*                       /*                                           */ 00306000
*                       /* CHECK IF MINOR CAN BE USED FOR AN END     */ 00307000
*                       /* LINE. THERE IS ALWAYS A MINOR WQE HUNG OFF*/ 00308000
*                       /* THE MAJOR                                 */ 00309000
*                       /*                                           */ 00310000
*                       /*********************************************/ 00311000
*                                                               02S0142 00312000
*                       IF WMJMMLWH=YES THEN/* 'MINOR HAS NO TEXT'      00313000
*                                      SET ON?                       */ 00314000
         TM    WMJMMLWH(CHAINPTR),B'00000001'                   02S0142 00315000
         BNO   @RF00142                                         02S0142 00316000
*                         DO;       /* YES. PUT END LINE IN MINOR    */ 00317000
*                           WMJMMLWH=NO;/* MINOR NOW HAS TEXT        */ 00318000
*                           WMJMMLWF=YES;/* INDICATE START AT TOP OF    00319000
*                                      CHAIN                         */ 00320000
         OI    WMJMMLWF(CHAINPTR),B'00000100'                   02S0145 00321000
         NI    WMJMMLWH(CHAINPTR),B'11111110'                   02S0145 00322000
*                           CALL BUILDEND;/* BUILD END LINE IN MINOR */ 00323000
         BAL   @14,BUILDEND                                     02S0146 00324000
*                         END;                                  02S0147 00325000
*                                                               02S0147 00326000
*                       /*********************************************/ 00327000
*                       /*                                           */ 00328000
*                       /* NO. FIRST MINOR HAS TEXT.                 */ 00329000
*                       /*                                           */ 00330000
*                       /*********************************************/ 00331000
*                                                               02S0148 00332000
*                       ELSE                                    02S0148 00333000
*                         DO;       /* SCAN FOR END OF MINOR CHAIN.  */ 00334000
         B     @RC00142                                         02S0148 00335000
@RF00142 DS    0H                                               02S0149 00336000
*                           ENDFND=NO;/* TURN OFF LOOP BIT           */ 00337000
*                                                               02S0149 00338000
         NI    ENDFND,B'11011111'                               02S0149 00339000
*                           /*****************************************/ 00340000
*                           /*                                       */ 00341000
*                           /* CONTINUE TO SCAN THE MINOR CHAIN UNTIL*/ 00342000
*                           /* THE END IS FOUND                      */ 00343000
*                           /*                                       */ 00344000
*                           /*****************************************/ 00345000
*                                                               02S0150 00346000
*                           DO WHILE(ENDFND=NO);                02S0150 00347000
*                                                               02S0150 00348000
         B     @DE00150                                         02S0150 00349000
@DL00150 DS    0H                                               02S0151 00350000
*                             /***************************************/ 00351000
*                             /*                                     */ 00352000
*                             /* CHECK IF 1ST LINE IN MINOR IS LAST  */ 00353000
*                             /* LINE                                */ 00354000
*                             /*                                     */ 00355000
*                             /***************************************/ 00356000
*                                                               02S0151 00357000
*                             IF WMNMNX1=ZERO THEN/* IS SECOND MINOR    00358000
*                                      NOT USED                      */ 00359000
*                                                               02S0151 00360000
         L     @12,WMNMNX1-1(,MINORPTR)                         02S0151 00361000
         LA    @12,0(,@12)                                      02S0151 00362000
         LTR   @12,@12                                          02S0151 00363000
         BNZ   @RF00151                                         02S0151 00364000
*                               /*************************************/ 00365000
*                               /*                                   */ 00366000
*                               /* YES. CHECK IF LINE TYPE CONTAINS  */ 00367000
*                               /* AN END                            */ 00368000
*                               /*                                   */ 00369000
*                               /*************************************/ 00370000
*                                                               02S0152 00371000
*                               IF WMNMLT1D=ENDLINE THEN        02S0152 00372000
         TM    WMNMLT1D(MINORPTR),B'00010000'                   02S0152 00373000
         BNO   @RF00152                                         02S0152 00374000
*                                 DO;/* YES. END OF LOOP AND NO MINOR   00375000
*                                      NEEDED                        */ 00376000
*                                   ENDFND=YES;/* GET OUT OF LOOP    */ 00377000
         OI    ENDFND,B'00100000'                               02S0154 00378000
*                                   MINORPTR=ZERO;/* DON'T BUILD A      00379000
*                                      MINOR                         */ 00380000
         SLR   MINORPTR,MINORPTR                                02S0155 00381000
*                                 END;                          02S0156 00382000
*                                                               02S0156 00383000
*                             /***************************************/ 00384000
*                             /*                                     */ 00385000
*                             /* NO. THE LAST LINE DOESN'T CONTAIN AN*/ 00386000
*                             /* END.                                */ 00387000
*                             /*                                     */ 00388000
*                             /***************************************/ 00389000
*                                                               02S0157 00390000
*                               ELSE                            02S0157 00391000
*                                 DO;/* PT AT SECOND HALF AS LOCATION   00392000
*                                      OF WHERE TO BUILD END LINE.   */ 00393000
         B     @RC00152                                         02S0157 00394000
@RF00152 DS    0H                                               02S0158 00395000
*                                   ENDFND=YES;/* GET OUT OF LOOP PT    00396000
*                                      1ST HALF AT 2ND HALF          */ 00397000
         OI    ENDFND,B'00100000'                               02S0158 00398000
*                                   WMNMNX1=ADDR(WMNMUC2);      02S0159 00399000
         LA    @12,WMNMUC2(,MINORPTR)                           02S0159 00400000
         STCM  @12,7,WMNMNX1(MINORPTR)                          02S0159 00401000
*                                   MINORPTR=WMNMNX1;/* PT AT LOC FOR   00402000
*                                      END LINE                      */ 00403000
         LR    MINORPTR,@12                                     02S0160 00404000
*                                 END;                          02S0161 00405000
*                                                               02S0161 00406000
*                             /***************************************/ 00407000
*                             /*                                     */ 00408000
*                             /* NO, THE SECOND HALF IS USED         */ 00409000
*                             /*                                     */ 00410000
*                             /***************************************/ 00411000
*                                                               02S0162 00412000
*                             ELSE                              02S0162 00413000
*                               DO; /* CHECK IF THERE IS ANOTHER        00414000
*                                      MINOR                         */ 00415000
         B     @RC00151                                         02S0162 00416000
@RF00151 DS    0H                                               02S0163 00417000
*                                 IF WMNMNX2^=ZERO THEN         02S0163 00418000
         L     @12,WMNMNX2-1(,MINORPTR)                         02S0163 00419000
         LA    @12,0(,@12)                                      02S0163 00420000
         LTR   @12,@12                                          02S0163 00421000
         BZ    @RF00163                                         02S0163 00422000
*                                   MINORPTR=WMNMNX2;/* LOOP TO CHECK   00423000
*                                      NEXT MINOR                    */ 00424000
*                                                               02S0164 00425000
         LR    MINORPTR,@12                                     02S0164 00426000
*                                 /***********************************/ 00427000
*                                 /*                                 */ 00428000
*                                 /* THE SECOND HALF IS THE LAST     */ 00429000
*                                 /* LINE. CHECK IF IT IS AN END LINE*/ 00430000
*                                 /*                                 */ 00431000
*                                 /***********************************/ 00432000
*                                                               02S0165 00433000
*                                 ELSE                          02S0165 00434000
*                                   IF WMNMLT2D=ENDLINE THEN    02S0165 00435000
         B     @RC00163                                         02S0165 00436000
@RF00163 TM    WMNMLT2D(MINORPTR),B'00010000'                   02S0165 00437000
         BNO   @RF00165                                         02S0165 00438000
*                                    DO;/* YES THE MLWTO IS ENDED    */ 00439000
*                                     ENDFND=YES;/* END OF CHAIN IS     00440000
*                                      FOUND                         */ 00441000
         OI    ENDFND,B'00100000'                               02S0167 00442000
*                                     MINORPTR=ZERO;/* NO END NEEDED    00443000
*                                      TO BUILD                      */ 00444000
         SLR   MINORPTR,MINORPTR                                02S0168 00445000
*                                    END;                       02S0169 00446000
*                                                               02S0169 00447000
*                                 /***********************************/ 00448000
*                                 /*                                 */ 00449000
*                                 /* THE LAST LINE ISN'T AN END.     */ 00450000
*                                 /* BUILD AN END LINE IN THE MAJOR  */ 00451000
*                                 /* AS WE CAN'T GET ANOTHER WQE. PT */ 00452000
*                                 /* MINOR AT FAKE MINOR IN THE MAJOR*/ 00453000
*                                 /*                                 */ 00454000
*                                 /***********************************/ 00455000
*                                                               02S0170 00456000
*                                   ELSE                        02S0170 00457000
*                                    DO;                        02S0170 00458000
         B     @RC00165                                         02S0170 00459000
@RF00165 DS    0H                                               02S0171 00460000
*                                     ENDFND=YES;/* GET OUT OF LOOP  */ 00461000
         OI    ENDFND,B'00100000'                               02S0171 00462000
*                                     WMNMNX2=ADDR(WMJMRESA);/* PT AT   00463000
*                                      FAKE AREA                     */ 00464000
         LA    @12,WMJMRESA(,CHAINPTR)                          02S0172 00465000
         STCM  @12,7,WMNMNX2(MINORPTR)                          02S0172 00466000
*                                     MINORPTR=ADDR(WMJMRESA);/* PT     00467000
*                                      AT LOCATION FOR END LINE      */ 00468000
*                                                               02S0173 00469000
         LR    MINORPTR,@12                                     02S0173 00470000
*                                     /*******************************/ 00471000
*                                     /*                             */ 00472000
*                                     /* ADDRESSING FOR FAKE MINOR   */ 00473000
*                                     /* NOW SET. SHOW MINOR AS FAKE */ 00474000
*                                     /* ONE FOR ABEND               */ 00475000
*                                     /*                             */ 00476000
*                                     /*******************************/ 00477000
*                                                               02S0174 00478000
*                                     WMNMML1=ABNDMIN;/* TURN ON        00479000
*                                      ABEND MINOR BIT               */ 00480000
         MVI   WMNMML1(MINORPTR),X'04'                          02S0174 00481000
*                                    END;                       02S0175 00482000
*                               END;/* OF 2ND HALF PROCESSING        */ 00483000
@RC00165 DS    0H                                               02S0176 00484000
@RC00163 DS    0H                                               02S0177 00485000
*                           END;    /* OF DO WHILE(ENDFOUND = NO)       00486000
*                                      LOOP                          */ 00487000
*                                                               02S0177 00488000
@RC00151 DS    0H                                               02S0177 00489000
@DE00150 TM    ENDFND,B'00100000'                               02S0177 00490000
         BZ    @DL00150                                         02S0177 00491000
*                           /*****************************************/ 00492000
*                           /*                                       */ 00493000
*                           /* CHECK IF MINOR NEEDS TO BE BUILT      */ 00494000
*                           /*                                       */ 00495000
*                           /*****************************************/ 00496000
*                                                               02S0178 00497000
*                           IF MINORPTR^=ZERO THEN/* IS MINORPTR        00498000
*                                      POINTING AT LOC FOR END LINE  */ 00499000
         LTR   MINORPTR,MINORPTR                                02S0178 00500000
         BZ    @RF00178                                         02S0178 00501000
*                             CALL BUILDEND;/* YES. BUILD END LINE   */ 00502000
         BAL   @14,BUILDEND                                     02S0179 00503000
*                         END;      /* OF NOT DUMMY MINOR CLAUSE     */ 00504000
*                     END;                                      01S0181 00505000
*                   ELSE            /* THIS IS A SINGLE LINE WQE     */ 00506000
*                     IF WQEORE=YES&/* IS THERE AN ORE AND           */ 00507000
*                         DOMCWR=YES THEN/* IS THIS A DOM FOR WTORS  */ 00508000
         B     @RC00138                                         01S0182 00509000
@RF00138 TM    WQEORE(CHAINPTR),B'00100000'                     01S0182 00510000
         BNO   @RF00182                                         01S0182 00511000
         TM    DOMCWR(DOMCPTR),B'00000100'                      01S0182 00512000
         BNO   @RF00182                                         01S0182 00513000
*                       DO;         /* PROCESS THE ORE               */ 00514000
*                                                               02S0184 00515000
*/********************************************************************/ 00516000
*/*   PROCESS THE ORE SEGMENT                                        */ 00517000
*/*                                                                  */ 00518000
*/*   THE SEGMENT WILL DECHAIN AND FREE THE ORE FOR THE DOMED WTOR.  */ 00519000
*/*   THE REPLY ID WILL BE RETURNED FOR USE BY OTHER WTORS. THE COUNT*/ 00520000
*/*   OF ORES WILL BE DECREASED AND WWB (IF ANY) WAITING ON THE      */ 00521000
*/*   UCMOECBH CHAIN WILL BE POSTED. IF THE ORE POINTS TO A TEMPORARY*/ 00522000
*/*   REPLY BUFFER, THE BUFFER IS FREED.                             */ 00523000
*/*                                                                  */ 00524000
*/*   INPUT - CHAINPTR PTS AT A WQE FOR A WTOR                       */ 00525000
*/*                                                                  */ 00526000
*/*   OUTPUT - THE ORE IS FREED AND THE WQE IS MARKED SO THAT IT IS  */ 00527000
*/*            NOT A WTOR                                            */ 00528000
*/*                                                                  */ 00529000
*/********************************************************************/ 00530000
*                                                               02S0184 00531000
*                         LSTBLK=ADDR(UCMRPYQ);/* PREPARE PTR TO        00532000
*                                      PREVIOUS ORE                  */ 00533000
         LA    @12,UCMRPYQ(,LUCMPTR)                            02S0184 00534000
         ST    @12,LSTBLK                                       02S0184 00535000
*                         DO OREPTR=UCMRPYQ BY ZERO WHILE(OREPTR^=ZERO) 00536000
*                               ;                               02S0185 00537000
*                                                               02S0185 00538000
         L     OREPTR,UCMRPYQ(,LUCMPTR)                         02S0185 00539000
         B     @DE00185                                         02S0185 00540000
@DL00185 DS    0H                                               02S0186 00541000
*                           /*****************************************/ 00542000
*                           /*                                       */ 00543000
*                           /* LOOP THRU THE ORES AND FIND THE ONE   */ 00544000
*                           /* FOR THIS WTOR                         */ 00545000
*                           /*                                       */ 00546000
*                           /*****************************************/ 00547000
*                                                               02S0186 00548000
*                           IF OREID^=WQERPYID THEN/* DO THE REPLY      00549000
*                                      IDS MATCH                     */ 00550000
         CLC   OREID(2,OREPTR),WQERPYID(CHAINPTR)               02S0186 00551000
         BE    @RF00186                                         02S0186 00552000
*                             DO;   /* NO, LOOP TO NEXT ORE          */ 00553000
*                               LSTBLK=OREPTR;/* THIS ORE BECOMES       00554000
*                                      PREVIOUS ORE                  */ 00555000
         ST    OREPTR,LSTBLK                                    02S0188 00556000
*                               OREPTR=ORELKP;/* PT AT NEXT ORE      */ 00557000
         L     OREPTR,ORELKP(,OREPTR)                           02S0189 00558000
*                             END;                              02S0190 00559000
*                           ELSE    /* YES THE IDS DO MATCH          */ 00560000
*                             DO;   /* PROCESS THIS ORE              */ 00561000
         B     @RC00186                                         02S0191 00562000
@RF00186 DS    0H                                               02S0192 00563000
*                               OREFOUND=YES;/* INDICATE WE FOUND THE   00564000
*                                      ORE                           */ 00565000
         OI    OREFOUND,B'00001000'                             02S0192 00566000
*                               CALL MSGPROC(OREID);/* PUT ID OF        00567000
*                                      FREED ORE IN MESSAGE TO THE      00568000
*                                      OPERATOR                      */ 00569000
         LA    @12,OREID(,OREPTR)                               02S0193 00570000
         ST    @12,@AL00001                                     02S0193 00571000
         LA    @01,@AL00001                                     02S0193 00572000
         BAL   @14,MSGPROC                                      02S0193 00573000
*                                                               03S0194 00574000
*/********************************************************************/ 00575000
*/*                AVAILID       SEGMENT                             */ 00576000
*/*                                                                  */ 00577000
*/* THE FOLLOWING SEGMENT MARKS THE REPLY ID ASSIGNMENT PATTERN IN   */ 00578000
*/* THE UCM TO INDICATE THAT THE REPLY ID WHICH IS BEING PROCESSED   */ 00579000
*/* CAN BE REASSIGNED.                                               */ 00580000
*/*                                                                  */ 00581000
*/* INPUT:                                                           */ 00582000
*/*      OREPTR - PTS AT ORE BEING PROCESSED.                        */ 00583000
*/*      OREID - REPLY ID                                            */ 00584000
*/*      UCMRPYI - REPLY ID ASSIGNMENT BIT PATTERN                   */ 00585000
*/*                                                                  */ 00586000
*/* OUTPUT:                                                          */ 00587000
*/*      UCM ASSIGNMENT BIT PATTERN MASKED                           */ 00588000
*/*                                                                  */ 00589000
*/********************************************************************/ 00590000
*                                                               03S0194 00591000
*                               RFY                             03S0194 00592000
*                                 REG1 RSTD;                    03S0194 00593000
*                               PACK(PAREA,OREID);/* CONVERT REPLY ID*/ 00594000
         PACK  PAREA(8),OREID(2,OREPTR)                         03S0195 00595000
*                               CVB(REG1,PAREA);/* CONVERT ID TO        00596000
*                                      BINARY FORM                   */ 00597000
         CVB   REG1,PAREA                                       03S0196 00598000
*                               UCMRPYI(REG1/EIGHT+ONE)=/* FIND 03S0197 00599000
*                                      MATCHING BYTE PATTERN         */ 00600000
*                                   UCMRPYI(REG1/EIGHT+ONE)&/* AND      00601000
*                                      (OP) MATCHING BYTE            */ 00602000
*                                   BITMAP(REG1//EIGHT+ONE);/* WITH     00603000
*                                      MATCHING BIT MAP              */ 00604000
         LR    @12,REG1                                         03S0197 00605000
         SRL   @12,3                                            03S0197 00606000
         ALR   @12,LUCMPTR                                      03S0197 00607000
         LR    @14,REG1                                         03S0197 00608000
         SRDA  @14,32                                           03S0197 00609000
         D     @14,@CF00070                                     03S0197 00610000
         LA    @11,BITMAP(@14)                                  03S0197 00611000
         NC    UCMRPYI(1,@12),0(@11)                            03S0197 00612000
*                               RFY                             03S0198 00613000
*                                 REG1 UNRSTD;                  03S0198 00614000
*                                                               03S0199 00615000
*/********************************************************************/ 00616000
*/*                REMOVE     ORE     SEGMENT                        */ 00617000
*/*                                                                  */ 00618000
*/* THE FOLLOWING SEGMENT DECHAINS THE ORE FROM THE ORE CHAIN, FREES */ 00619000
*/* THE ORE CORE, DECREASES THE ORE COUNT AND MARKS THE WQE AS       */ 00620000
*/* NOT A WTOR.                                                      */ 00621000
*/*                                                                  */ 00622000
*/* INPUT:                                                           */ 00623000
*/*      UCMRPYQ - ORE CHAIN POINTER                                 */ 00624000
*/*      UCMRQNR - ORE COUNT                                         */ 00625000
*/*      OREPTR  - POINTER TO CURRENT ORE                            */ 00626000
*/*      LSTBLK  - POINTER TO PREVIOUS ORE                           */ 00627000
*/*                                                                  */ 00628000
*/* OUTPUT:                                                          */ 00629000
*/*      1) ORE REMOVED FROM THE CHAIN                               */ 00630000
*/*      2) ORE CORE FREED                                           */ 00631000
*/*      3) ORE COUNT DECREASED                                      */ 00632000
*/*      4) TEMPORARY BUFFER FREED                                   */ 00633000
*/*                                                                  */ 00634000
*/********************************************************************/ 00635000
*                                                               03S0199 00636000
*                               IF OREOPBUF^=ZERO THEN/* IS THERE A     00637000
*                                      REPLY BUFFER                  */ 00638000
         SLR   @12,@12                                          03S0199 00639000
         L     @11,OREOPBUF(,OREPTR)                            03S0199 00640000
         CR    @11,@12                                          03S0199 00641000
         BE    @RF00199                                         03S0199 00642000
*                                 DO;/* YES, THEN FREE IT.           */ 00643000
*                                                               04S0201 00644000
*/********************************************************************/ 00645000
*/*                FREE  BUFFER  SEGMENT                             */ 00646000
*/*                                                                  */ 00647000
*/* THE FOLLOWING SEGMENT FREES THE TEMPORARY BUFFER ASSOCIATED WITH */ 00648000
*/* THE REPLY WHICH WAS CREATED BY THE FIRST STAGE OF REPLY          */ 00649000
*/* PROCESSING. THE TEMPORARY BUFFER WHICH IS POINTED TO OUT OF THE  */ 00650000
*/* ORE CONTAINS THE ACTUAL REPLY TEXT AS SPECIFIED BY THE USER WHO  */ 00651000
*/* IS RESPONDING TO THE ORIGINAL WTOR MESSAGE.                      */ 00652000
*/*                                                                  */ 00653000
*/* INPUT:                                                           */ 00654000
*/*      OREOPBUF - POINTER TO THE TEMPORARY BUFFER                  */ 00655000
*/*                                                                  */ 00656000
*/********************************************************************/ 00657000
*                                                               04S0201 00658000
*                                   RFY                         04S0201 00659000
*                                    (REG0,                     04S0201 00660000
*                                     REG1) RSTD;/*          @ZA03871*/ 00661000
*                                   REG0=TEMPLNT+LENGTH(TEMPLNT);/*     00662000
*                                      LENGTH OF FREEMAIN            */ 00663000
         LA    REG0,2                                           04S0202 00664000
         AH    REG0,TEMPLNT(,@11)                               04S0202 00665000
*                                   REG1=OREOPBUF;/* ADDR OF    04S0203 00666000
*                                      TEMPORARY BUFFER              */ 00667000
         LR    REG1,@11                                         04S0203 00668000
*                                   OREOPBUF=ZERO;/* ZERO POINTER TO    00669000
*                                      TEMP BUFFER                   */ 00670000
         ST    @12,OREOPBUF(,OREPTR)                            04S0204 00671000
*                                   GEN REFS(CVTCRMN);          04S0205 00672000
*                                                               04S0205 00673000
*                                      /*ISSUE FREEMAIN              */ 00674000
         FREEMAIN  RU,LV=(0),A=(1),SP=231,BRANCH=YES                    00675000
*                                   /*********************************/ 00676000
*                                   /*                               */ 00677000
*                                   /* RESET THE LOCAL UCM PTR IN    */ 00678000
*                                   /* REG3.                         */ 00679000
*                                   /*                               */ 00680000
*                                   /*********************************/ 00681000
*                                                               04S0206 00682000
*                                   LUCMPTR=UCMBASPT;/* ALSO RESTORE    00683000
*                                      LUCMPTR (REG 3)               */ 00684000
         L     @12,CVTPTR                                       04S0206 00685000
         L     LUCMPTR,UCMBASPT(,@12)                           04S0206 00686000
*                                   RFY                         04S0207 00687000
*                                    (REG0,                     04S0207 00688000
*                                     REG1) UNRSTD;/*        @ZA03871*/ 00689000
*                                 END;/* END OF FREE REPLY BUFFER.   */ 00690000
*                                                               03S0208 00691000
*                               /*************************************/ 00692000
*                               /*                                   */ 00693000
*                               /* DECHAIN THE ORE                   */ 00694000
*                               /*                                   */ 00695000
*                               /*************************************/ 00696000
*                                                               03S0209 00697000
*                               RFY                             03S0209 00698000
*                                (REG0,                         03S0209 00699000
*                                 REG1,                         03S0209 00700000
*                                 REG15) RSTD;/*             @ZA03871*/ 00701000
@RF00199 DS    0H                                               03S0210 00702000
*                               LSTBLK->ORELKP=ORELKP;/* LINK LAST      00703000
*                                      BLOCK TO NEXT BLOCK           */ 00704000
         L     @14,ORELKP(,OREPTR)                              03S0210 00705000
         L     @12,LSTBLK                                       03S0210 00706000
         ST    @14,ORELKP(,@12)                                 03S0210 00707000
*                               UCMRQNR=UCMRQNR-ONE;/* DECREASE ORE     00708000
*                                      COUNT BY ONE                  */ 00709000
         LH    @14,UCMRQNR(,LUCMPTR)                            03S0211 00710000
         BCTR  @14,0                                            03S0211 00711000
         STH   @14,UCMRQNR(,LUCMPTR)                            03S0211 00712000
*                               WQEORE=NO;/* INDICATE NO ORE EXIST   */ 00713000
         NI    WQEORE(CHAINPTR),B'11011111'                     03S0212 00714000
*                               REG0=UCMORECP;/* SET CELL POOL ID    */ 00715000
         L     REG0,UCMORECP(,LUCMPTR)                          03S0213 00716000
*                               REG1=OREPTR;/* SET ADDR OF ORE       */ 00717000
         LR    REG1,OREPTR                                      03S0214 00718000
*                               GEN REFS(CVTMAP,CVTFRECL,REG0,REG15)    00719000
*                                   SETS(REG0,REG1);            03S0215 00720000
*                                                               03S0215 00721000
*                                      /*ISSUE FREECELL              */ 00722000
         FREECELL  CPID=(0),CELL=(1),BRANCH=YES,SAVE=NO                 00723000
*                               /*************************************/ 00724000
*                               /*                                   */ 00725000
*                               /* IF REGISTER 15 IS SET TO HEX '20' */ 00726000
*                               /* ISSUE NORMAL FREEMAIN TO FREE ORE */ 00727000
*                               /* CELL EXTENSION.REGISTERS 0 AND 1  */ 00728000
*                               /* ARE SET BY FREECELL.              */ 00729000
*                               /*                                   */ 00730000
*                               /*************************************/ 00731000
*                                                               03S0216 00732000
*                               IF REG15=FREEXT THEN/* REGISTER 15      00733000
*                                      SET ?                         */ 00734000
         C     REG15,@CF00082                                   03S0216 00735000
         BNE   @RF00216                                         03S0216 00736000
*                                 DO;/* ISSUE FREEMAIN               */ 00737000
*                                   GEN REFS(REG0,REG1,CVTCRMN);        00738000
*                                              /*FREEMAIN SETUP      */ 00739000
           FREEMAIN  RU,LV=(0),A=(1),BRANCH=YES                         00740000
*                                 END;/* END NORMAL FREEMAIN         */ 00741000
*                               LUCMPTR=UCMBASPT;/* RESTORE LUCMPTR     00742000
*                                      (REG3)                        */ 00743000
@RF00216 L     @14,CVTPTR                                       03S0220 00744000
         L     LUCMPTR,UCMBASPT(,@14)                           03S0220 00745000
*                               RFY                             03S0221 00746000
*                                (REG0,                         03S0221 00747000
*                                 REG1,                         03S0221 00748000
*                                 REG15) UNRSTD;/*           @ZA03871*/ 00749000
*                                                               02S0222 00750000
*                               /*************************************/ 00751000
*                               /*                                   */ 00752000
*                               /* CHECK IF THE NUMBER OF ORES IS NOW*/ 00753000
*                               /* UNDER THE LIMIT. IF SO POST ANY   */ 00754000
*                               /* WAITING WWB ON THE UCMOECBH CHAIN.*/ 00755000
*                               /*                                   */ 00756000
*                               /*************************************/ 00757000
*                                                               02S0222 00758000
*                               IF UCMRQNR<UCMRQLM THEN         02S0222 00759000
         LH    @12,UCMRQNR(,LUCMPTR)                            02S0222 00760000
         SLR   @11,@11                                          02S0222 00761000
         IC    @11,UCMRQLM(,LUCMPTR)                            02S0222 00762000
         CR    @12,@11                                          02S0222 00763000
         BNL   @RF00222                                         02S0222 00764000
*                                 DO WWBPTR=UCMOECBH BY ZERO WHILE(     00765000
*                                       WWBPTR^=ADDR(UCMOECBH));        00766000
*                                                               02S0223 00767000
         L     WWBPTR,UCMOECBH(,LUCMPTR)                        02S0223 00768000
         B     @DE00223                                         02S0223 00769000
@DL00223 DS    0H                                               02S0224 00770000
*                                   /*********************************/ 00771000
*                                   /*                               */ 00772000
*                                   /* HAS THIS ECB ELEMENT BEEN     */ 00773000
*                                   /* POSTED ?                      */ 00774000
*                                   /*                               */ 00775000
*                                   /*********************************/ 00776000
*                                                               02S0224 00777000
*                                   IF WWBPOSTD=NO THEN/* ECB POSTED    00778000
*                                      ?                             */ 00779000
         TM    WWBPOSTD(WWBPTR),B'10000000'                     02S0224 00780000
         BNZ   @RF00224                                         02S0224 00781000
*                                    DO;/* IF NO,                    */ 00782000
*                                     WWBPOSTD=YES;/* TURN POST BIT     00783000
*                                      ON                            */ 00784000
         OI    WWBPOSTD(WWBPTR),B'10000000'                     02S0226 00785000
*                                     CALL POSTOECB;/* POST ANY TASK    00786000
*                                      WAITING FOR AN ORE            */ 00787000
         BAL   @14,POSTOECB                                     02S0227 00788000
*                                     WWBPTR=ADDR(UCMOECBH);/* GET      00789000
*                                      OUT OF LOOP                   */ 00790000
         LA    WWBPTR,UCMOECBH(,LUCMPTR)                        02S0228 00791000
*                                    END;/* IF POST BIT IS ON        */ 00792000
*                                   ELSE/* IF ELEMENT IS NOT POSTED  */ 00793000
*                                    WWBPTR=WWBFWDPT;/* GET NEXT ECB    00794000
*                                      ELEMENT                       */ 00795000
         B     @RC00224                                         02S0230 00796000
@RF00224 L     WWBPTR,WWBFWDPT(,WWBPTR)                         02S0230 00797000
*                                 END;/* END DO LOOP                 */ 00798000
@RC00224 DS    0H                                               02S0231 00799000
@DE00223 LA    @12,UCMOECBH(,LUCMPTR)                           02S0231 00800000
         CR    WWBPTR,@12                                       02S0231 00801000
         BNE   @DL00223                                         02S0231 00802000
*                               OREPTR=ZERO;/* GET OUT OF LOOP       */ 00803000
@RF00222 SLR   OREPTR,OREPTR                                    02S0232 00804000
*                             END;                              02S0233 00805000
*                         END;                                  02S0234 00806000
@RC00186 DS    0H                                               02S0234 00807000
@DE00185 LTR   OREPTR,OREPTR                                    02S0234 00808000
         BNZ   @DL00185                                         02S0234 00809000
*                       END;                                    01S0235 00810000
*                   WQEDOM=YES;     /* INDICATE THAT THIS MSG HAS       00811000
*                                      BEEN DOMED                    */ 00812000
@RF00182 DS    0H                                               01S0236 00813000
@RC00138 DS    0H                                               01S0237 00814000
*                   WQEPURGE=YES;   /* DONT PRINT THIS MESSAGE IF IT    00815000
*                                      HASN'T GONE OUT YET           */ 00816000
         OI    WQEDOM(CHAINPTR),B'10000100'                     01S0237 00817000
*                   CHAINPTR=ZERO;  /* GET OUT OF WQE SCAN LOOP      */ 00818000
         SLR   CHAINPTR,CHAINPTR                                01S0238 00819000
*                 END;                                          01S0239 00820000
*               ELSE                /* POINT TO NEXT WQE ON THE CHAIN*/ 00821000
*                 CHAINPTR=WQELKPA; /* PICK UP LINK PART ONLY        */ 00822000
         B     @RC00136                                         01S0240 00823000
@RF00136 L     @12,WQELKPA-1(,CHAINPTR)                         01S0240 00824000
         LA    @12,0(,@12)                                      01S0240 00825000
         LR    CHAINPTR,@12                                     01S0240 00826000
*             END;                  /* OF LOOP THRU WTOQ             */ 00827000
*                                                               01S0241 00828000
@RC00136 DS    0H                                               01S0241 00829000
@DE00135 LTR   CHAINPTR,CHAINPTR                                01S0241 00830000
         BNZ   @DL00135                                         01S0241 00831000
*             /*******************************************************/ 00832000
*             /*                                                     */ 00833000
*             /* CHECK IF LAST ID. IF SO SET ENDOMLP BIT ON TO END   */ 00834000
*             /* LOOP                                                */ 00835000
*             /*                                                     */ 00836000
*             /*******************************************************/ 00837000
*                                                               01S0242 00838000
*             IF DOMCEND=YES THEN                               01S0242 00839000
         L     @12,IDPTR                                        01S0242 00840000
         TM    DOMCEND(@12),B'10000000'                         01S0242 00841000
         BNO   @RF00242                                         01S0242 00842000
*               ENDOMLP=YES;        /* GET OUT OF THE LOOP           */ 00843000
         OI    ENDOMLP,B'00010000'                              01S0243 00844000
*           END;                    /* OF DOMCB ID LOOP              */ 00845000
@RF00242 LA    @12,4                                            01S0244 00846000
         AL    @12,IDPTR                                        01S0244 00847000
         ST    @12,IDPTR                                        01S0244 00848000
@DE00111 TM    ENDOMLP,B'00010000'                              01S0244 00849000
         BZ    @DL00111                                         01S0244 00850000
*         END;                      /* OF TEST FOR DOM WITH MESSAGE     00851000
*                                      ID                            */ 00852000
*                                                               01S0245 00853000
*       /*************************************************************/ 00854000
*       /*                                                           */ 00855000
*       /* MARK THE DOMCB AS HAVING BEEN PROCESSED                   */ 00856000
*       /*                                                           */ 00857000
*       /*************************************************************/ 00858000
*                                                               01S0246 00859000
*       DOMCPROC=YES;                                           01S0246 00860000
@RF00108 OI    DOMCPROC(DOMCPTR),B'00000001'                    01S0246 00861000
*       DOMCPTR=DOMCLNKA;           /* POINT TO NEXT DOMCB           */ 00862000
         L     @12,DOMCLNKA-1(,DOMCPTR)                         01S0247 00863000
         LA    @12,0(,@12)                                      01S0247 00864000
         LR    DOMCPTR,@12                                      01S0247 00865000
*     END;                          /* DOMCB PROCESS                 */ 00866000
*                                                               01S0248 00867000
@DE00107 LTR   DOMCPTR,DOMCPTR                                  01S0248 00868000
         BNZ   @DL00107                                         01S0248 00869000
*     /***************************************************************/ 00870000
*     /*                                                             */ 00871000
*     /* NOW FREE THE LOCKS AND PASS CONTROL TO GRAPHICS PROCESSOR   */ 00872000
*     /*                                                             */ 00873000
*     /***************************************************************/ 00874000
*                                                               01S0249 00875000
*     CALL RELFRRTN;                /* RELEASE DOM FRR       @Y02668P*/ 00876000
         BAL   @14,RELFRRTN                                     01S0249 00877000
*     CALL FRELCKS;                 /* FREE THE CMS AND LOCAL LOCKS  */ 00878000
         BAL   @14,FRELCKS                                      01S0250 00879000
*                                                               02S0251 00880000
*/********************************************************************/ 00881000
*/*                                                                  */ 00882000
*/*  GRAPHICS SEGMENT                                                */ 00883000
*/*                                                                  */ 00884000
*/*  FUNCTION - THIS ROUTINE GIVES CONTROL TO THE GRAPHIC DEVICE     */ 00885000
*/*  PROCESSOR FOR EVERY ACTIVE GRAPHIC DEVICE IN THE SYSTEM.        */ 00886000
*/*                                                                  */ 00887000
*/*  INPUT - UCMVEA IS THE ADOR OF THE FIRST UCM ENTRY               */ 00888000
*/*          UCMVEZ IS THE NUMBER OF BYTES IN EACH UCM.              */ 00889000
*/*          UCMVEL IS THE ADDR OF THE LAST UCM ENTRY.               */ 00890000
*/*                                                                  */ 00891000
*/*  OUTPUT - THE DEVICE PROCESSOR PARM LIST WILL BE BUILT IN        */ 00892000
*/*           UCMXSA.                                                */ 00893000
*/*                                                                  */ 00894000
*/********************************************************************/ 00895000
*                                                               02S0251 00896000
*     DO CHAINPTR=UCMVEA            /* LOOP THRU THE UCM ENTRIES,       00897000
*                                      LOOKING                       */ 00898000
*           BY UCMVEZ               /* FOR AN ACTIVE GRAPHIC DEVICE  */ 00899000
*           TO UCMVEL;                                          02S0251 00900000
*                                                               02S0251 00901000
         L     CHAINPTR,UCMVEA(,LUCMPTR)                        02S0251 00902000
         B     @DE00251                                         02S0251 00903000
@DL00251 DS    0H                                               02S0252 00904000
*       /*************************************************************/ 00905000
*       /*                                                           */ 00906000
*       /* CHECK IF THIS DEVICE IS ACTIVE AND GRAPHICS               */ 00907000
*       /*                                                           */ 00908000
*       /*************************************************************/ 00909000
*                                                               02S0252 00910000
*       IF UCMDISPC=YES&            /* IS GRAPHIC DEVICE BIT ON AND  */ 00911000
*           UCMUF=YES               /* IS THE DEVICE ACTIVE          */ 00912000
*           &UCMTA=NO               /* AND OPEN NOT PENDING  @ZA00065*/ 00913000
*           &UCMDISPG=NO THEN       /* AND NOT STATUS ONLY    YM03456*/ 00914000
         TM    UCMDISPC(CHAINPTR),B'00100000'                   02S0252 00915000
         BNO   @RF00252                                         02S0252 00916000
         TM    UCMUF(CHAINPTR),B'00010000'                      02S0252 00917000
         BNO   @RF00252                                         02S0252 00918000
         TM    UCMTA(CHAINPTR),B'00001000'                      02S0252 00919000
         BNZ   @RF00252                                         02S0252 00920000
         TM    UCMDISPG(CHAINPTR),B'00000010'                   02S0252 00921000
         BNZ   @RF00252                                         02S0252 00922000
*         DO;                       /* YES. GIVE CONTROL TO SVC 72      00923000
*                                      PROCESS.                      */ 00924000
*                                                               02S0253 00925000
*           /*********************************************************/ 00926000
*           /*                                                       */ 00927000
*           /* SET UP SVC 72 PARAMETER LIST                          */ 00928000
*           /*                                                       */ 00929000
*           /*********************************************************/ 00930000
*                                                               02S0254 00931000
*           RFY                                                 02S0254 00932000
*             REG1 RSTD;            /* RESTRICT PARM PTR REGISTER    */ 00933000
*           REG1=ADDR(UCMXSA);      /* ADDR OF PARAMETER AREA        */ 00934000
         L     @12,CVTPTR                                       02S0255 00935000
         L     @12,UCMBASPT(,@12)                               02S0255 00936000
         AL    @12,@CF02657                                     02S0255 00937000
         L     @12,UCMMCSPT(,@12)                               02S0255 00938000
         LA    REG1,UCMXSA(,@12)                                02S0255 00939000
*           UCMXSA=UCMNAME;         /* MOVE IN NAME OF PROCESSING       00940000
*                                      MODULE                        */ 00941000
         MVI   UCMXSA+8(@12),C' '                               02S0256 00942000
         MVC   UCMXSA+9(15,@12),UCMXSA+8(@12)                   02S0256 00943000
         MVC   UCMXSA(8,@12),UCMNAME(CHAINPTR)                  02S0256 00944000
*           UCM3WD=CHAINPTR;        /* MOVE IN ADDRESS OF UCME FOIS     00945000
*                                      ACTIVE GRAPHIC DEVICE         */ 00946000
         ST    CHAINPTR,UCM3WD(,@12)                            02S0257 00947000
*           UCM4WD=LUCMPTR;         /* MOVE IN ADDR OF THE UCM       */ 00948000
         ST    LUCMPTR,UCM4WD(,@12)                             02S0258 00949000
*           UCM5WD=ZERO;            /* ZERO OUT THE FIFTH AND SIXTH     00950000
*                                      WORDS                         */ 00951000
         SLR   @11,@11                                          02S0259 00952000
         ST    @11,UCM5WD(,@12)                                 02S0259 00953000
*           UCM6WD=ZERO;            /* IN THE PARAMETER LIST         */ 00954000
         ST    @11,UCM6WD(,@12)                                 02S0260 00955000
*           UCMDEVD=YES;            /* TURN ON 'DOM ISSUED' BIT      */ 00956000
         OI    UCMDEVD(CHAINPTR),B'00010000'                    02S0261 00957000
*           GEN( SVC 72);                                       02S0262 00958000
          SVC 72                                                        00959000
*           RFY                                                 02S0263 00960000
*             REG1 UNRSTD;                                      02S0263 00961000
*         END;                                                  02S0264 00962000
*     END;                                                      02S0265 00963000
@RF00252 AL    CHAINPTR,UCMVEZ(,LUCMPTR)                        02S0265 00964000
@DE00251 C     CHAINPTR,UCMVEL(,LUCMPTR)                        02S0265 00965000
         BNH   @DL00251                                         02S0265 00966000
*     CALL SETLCKS;                 /* SET THE LOCAL AND CMS LOCKS   */ 00967000
         BAL   @14,SETLCKS                                      01S0266 00968000
*     CALL SETFRRTN;                /* REGAIN DOM FRR        @Y02668P*/ 00969000
         BAL   @14,SETFRRTN                                     01S0267 00970000
*                                                               02S0268 00971000
*/********************************************************************/ 00972000
*/*                                                                  */ 00973000
*/*   FREE THE PROCESSED DOMCBS.                                     */ 00974000
*/*                                                                  */ 00975000
*/*   FUNCTION - THIS SEGMENT GOES DOWN THE CHAIN OF DOMCBS AND FREES*/ 00976000
*/*   ANY THAT HAVE BEEN PROCESSED. THE DOMCBS ARE NOT FREED WHEN    */ 00977000
*/*   PROCESSED SO THAT THE GRAPHIC DEVICE PROCESSOR CAN USE THE     */ 00978000
*/*   DOMCB.                                                         */ 00979000
*/*                                                                  */ 00980000
*/*   INPUT - CHAIN OF DOMCBS POINTED AT BY UCMDOME.                 */ 00981000
*/*                                                                  */ 00982000
*/*   OUTPUT- PROCESSED DOMCBS ARE DECHAINED AND FREED.              */ 00983000
*/*                                                                  */ 00984000
*/********************************************************************/ 00985000
*                                                               02S0268 00986000
*     LSTBLK=ADDR(UCMDOME);         /* PREPARE PTR TO PREVIOUS DOMCB */ 00987000
*                                                               02S0268 00988000
         L     @12,CVTPTR                                       02S0268 00989000
         L     @12,UCMBASPT(,@12)                               02S0268 00990000
         AL    @12,@CF02657                                     02S0268 00991000
         L     @12,UCMMCSPT(,@12)                               02S0268 00992000
         LA    @11,UCMDOME(,@12)                                02S0268 00993000
         ST    @11,LSTBLK                                       02S0268 00994000
*     /***************************************************************/ 00995000
*     /*                                                             */ 00996000
*     /* LOOP THRU THE DUMCBS                                        */ 00997000
*     /*                                                             */ 00998000
*     /***************************************************************/ 00999000
*                                                               02S0269 01000000
*     DO DOMCPTR=UCMDOME BY ZERO WHILE(DOMCPTR^=ZERO);          02S0269 01001000
*                                                               02S0269 01002000
         L     DOMCPTR,UCMDOME(,@12)                            02S0269 01003000
         B     @DE00269                                         02S0269 01004000
@DL00269 DS    0H                                               02S0270 01005000
*       /*************************************************************/ 01006000
*       /*                                                           */ 01007000
*       /* CHECK IF THIS DOMCB IS PROCESSED. IF SO DECHAIN IT AND    */ 01008000
*       /* FREE IT                                                   */ 01009000
*       /*                                                           */ 01010000
*       /*************************************************************/ 01011000
*                                                               02S0270 01012000
*       IF DOMCPROC=YES THEN        /* HAS DOMCB BEEN PROCESSED      */ 01013000
         TM    DOMCPROC(DOMCPTR),B'00000001'                    02S0270 01014000
         BNO   @RF00270                                         02S0270 01015000
*         DO;                       /* YES. DECHAIN IT               */ 01016000
*           LSTBLK->DOMCLNKA=DOMCLNKA;/* PUT PTR TO NEXT DOMCB IN       01017000
*                                      LINK FIELD OF PREVIOUS DOMCB  */ 01018000
*                                                               02S0272 01019000
         L     @12,LSTBLK                                       02S0272 01020000
         MVC   DOMCLNKA(3,@12),DOMCLNKA(DOMCPTR)                02S0272 01021000
*           /*********************************************************/ 01022000
*           /*                                                       */ 01023000
*           /* NOW PICK UP THE SIZE OF THE DOMCB TO BE FREED         */ 01024000
*           /*                                                       */ 01025000
*           /*********************************************************/ 01026000
*                                                               02S0273 01027000
*           IF DOMCDBAJ=YES         /* IS THIS A DOM BY ASID & JSTCB    01028000
*                                      OR                            */ 01029000
*               |DOMCDBL=YES THEN   /* DOM BY ASID                   */ 01030000
*                                                               02S0273 01031000
         TM    DOMCDBAJ(DOMCPTR),B'00001010'                    02S0273 01032000
         BZ    @RF00273                                         02S0273 01033000
*             /*******************************************************/ 01034000
*             /*                                                     */ 01035000
*             /* YES. THEN THERE ARE NO MSG IDS IN THE DOMCB. SET    */ 01036000
*             /* ADDRESSIBILITY TO THE SIZE FIELD                    */ 01037000
*             /*                                                     */ 01038000
*             /*******************************************************/ 01039000
*                                                               02S0274 01040000
*             IDPTR=DOMCPTR;        /* LET THE 1ST FOUR BYTES OF THE    01041000
*                                      DOMCID OCCUPY SAME SPACE AS      01042000
*                                      DOMCLNK                       */ 01043000
         ST    DOMCPTR,IDPTR                                    02S0274 01044000
*           ELSE                                                02S0275 01045000
*                                                               02S0275 01046000
*             /*******************************************************/ 01047000
*             /*                                                     */ 01048000
*             /* YES THERE ARE MSGIDS IN THIS DOMCB. FIND THE END OF */ 01049000
*             /* THE LIST OF MSG IDS                                 */ 01050000
*             /*                                                     */ 01051000
*             /*******************************************************/ 01052000
*                                                               02S0275 01053000
*             DO IDPTR=DOMCPTR+LENGTH(DOMCLNK) BY LENGTH(DOMCFLAG)/     01054000
*                   EIGHT+LENGTH(DOMCIDA) WHILE(DOMCEND=NO);/* LOOP     01055000
*                                      UNTIL HIGH ORER BIT IS ON     */ 01056000
         B     @RC00273                                         02S0275 01057000
@RF00273 LA    @12,4                                            02S0275 01058000
         ALR   @12,DOMCPTR                                      02S0275 01059000
         ST    @12,IDPTR                                        02S0275 01060000
         B     @DE00275                                         02S0275 01061000
@DL00275 DS    0H                                               02S0276 01062000
*             END;                  /* IDPTR NOW PTS AT LAST MSG ID. */ 01063000
*                                                               02S0276 01064000
         LA    @12,4                                            02S0276 01065000
         AL    @12,IDPTR                                        02S0276 01066000
         ST    @12,IDPTR                                        02S0276 01067000
@DE00275 L     @12,IDPTR                                        02S0276 01068000
         TM    DOMCEND(@12),B'10000000'                         02S0276 01069000
         BZ    @DL00275                                         02S0276 01070000
*           /*********************************************************/ 01071000
*           /*                                                       */ 01072000
*           /* IDPTR NOW SET SO THAT DOMCSIZE CAN BE ACCESSED. FREE  */ 01073000
*           /* THE DOM CONTROL BLOCK                                 */ 01074000
*           /*                                                       */ 01075000
*           /*********************************************************/ 01076000
*                                                               02S0277 01077000
*           DO;                     /* FREEMAIN(R) LV(DOMCSIZE)SP(SUB   01078000
*                                      PL231)A(DOMC) BRANCH          */ 01079000
@RC00273 DS    0H                                               02S0278 01080000
*             RESPECIFY                                         02S0278 01081000
*              (GPR01F,                                         02S0278 01082000
*               GPR00F) RESTRICTED;                             02S0278 01083000
*             GPR00F=DOMCSIZE;      /* LENGTH REQUESTED              */ 01084000
         L     @12,IDPTR                                        02S0279 01085000
         SLR   GPR00F,GPR00F                                    02S0279 01086000
         IC    GPR00F,DOMCSIZE(,@12)                            02S0279 01087000
*             GPR01F=SUBPL231;      /* SP IN REG 1                   */ 01088000
         LA    GPR01F,231                                       02S0280 01089000
*             GEN(SLL   GPR01F,24)                              02S0281 01090000
*             NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;      02S0281 01091000
         SLL   GPR01F,24                                                01092000
*             GPR00F=GPR00F|GPR01F; /* SP IN HI BYTE                 */ 01093000
         OR    GPR00F,GPR01F                                    02S0282 01094000
*             GPR01F=ADDR(DOMC);    /* SET REGISTER ONE              */ 01095000
         LR    GPR01F,DOMCPTR                                   02S0283 01096000
*             CALL CVTRMBR;         /* BRANCH ENTRY POINT            */ 01097000
         L     @12,CVTPTR                                       02S0284 01098000
         L     @15,CVTRMBR(,@12)                                02S0284 01099000
         BALR  @14,@15                                          02S0284 01100000
*             RESPECIFY                                         02S0285 01101000
*              (GPR01F,                                         02S0285 01102000
*               GPR00F) UNRESTRICTED;                           02S0285 01103000
*           END;                    /* FREEMAIN(R) LV(DOMCSIZE)SP(SUB   01104000
*                                      PL231)A(DOMC) BRANCH          */ 01105000
*                                                               02S0286 01106000
*           /*********************************************************/ 01107000
*           /*                                                       */ 01108000
*           /* NOW SET DOMCPTR TO NEXT DOMCB ON THE CHAIN            */ 01109000
*           /*                                                       */ 01110000
*           /*********************************************************/ 01111000
*                                                               02S0287 01112000
*           DOMCPTR=LSTBLK->DOMCLNKA;                           02S0287 01113000
         L     @12,LSTBLK                                       02S0287 01114000
         L     DOMCPTR,DOMCLNKA-1(,@12)                         02S0287 01115000
         LA    DOMCPTR,0(,DOMCPTR)                              02S0287 01116000
*         END;                                                  02S0288 01117000
*       ELSE                                                    02S0289 01118000
*                                                               02S0289 01119000
*         /***********************************************************/ 01120000
*         /*                                                         */ 01121000
*         /* THIS DOMCB HASN'T BEEN PROCESSED. SKIP IT               */ 01122000
*         /*                                                         */ 01123000
*         /***********************************************************/ 01124000
*                                                               02S0289 01125000
*         DO;                                                   02S0289 01126000
         B     @RC00270                                         02S0289 01127000
@RF00270 DS    0H                                               02S0290 01128000
*           LSTBLK=DOMCPTR;         /* THIS DOMCB BECOMES PREVIOUS      01129000
*                                      DOMCB FOR THE NEXT DOMCB.     */ 01130000
         ST    DOMCPTR,LSTBLK                                   02S0290 01131000
*           DOMCPTR=DOMCLNKA;       /* PT AT NEXT DOMCB ON THE CHAIN */ 01132000
         L     @12,DOMCLNKA-1(,DOMCPTR)                         02S0291 01133000
         LA    @12,0(,@12)                                      02S0291 01134000
         LR    DOMCPTR,@12                                      02S0291 01135000
*         END;                                                  02S0292 01136000
*     END;                          /* OF LOOP OVER DOMCBS           */ 01137000
@RC00270 DS    0H                                               02S0293 01138000
@DE00269 LTR   DOMCPTR,DOMCPTR                                  02S0293 01139000
         BNZ   @DL00269                                         02S0293 01140000
*   END;                            /* UCMDOME CHAIN PROCESSING      */ 01141000
*                                                               01S0294 01142000
@DE00106 L     @12,CVTPTR                                       01S0294 01143000
         L     @12,UCMBASPT(,@12)                               01S0294 01144000
         AL    @12,@CF02657                                     01S0294 01145000
         L     @12,UCMMCSPT(,@12)                               01S0294 01146000
         L     @12,UCMDOME(,@12)                                01S0294 01147000
         LTR   @12,@12                                          01S0294 01148000
         BNZ   @DL00106                                         01S0294 01149000
*   /*****************************************************************/ 01150000
*   /*                                                               */ 01151000
*   /* NOW CLEAR THE UCM'S DOM ECB AND PUT OUT THE REPLY OUTSTANDING */ 01152000
*   /* MESSAGE IF ANY CONSTRUCTED                                    */ 01153000
*   /*                                                               */ 01154000
*   /*****************************************************************/ 01155000
*                                                               01S0295 01156000
*DOMCOMP:                           /* RETRTY LOC FOR DOM FAILURES      01157000
*                                                            @Y02668P*/ 01158000
*   UCMDECB=ZERO;                   /* CLEAR THE DOMECB              */ 01159000
DOMCOMP  SLR   @12,@12                                          01S0295 01160000
         ST    @12,UCMDECB(,LUCMPTR)                            01S0295 01161000
*   CALL MSGPROC(LAST);                                         01S0296 01162000
         LA    @01,@AL00296                                     01S0296 01163000
         BAL   @14,MSGPROC                                      01S0296 01164000
*   CALL RELFRRTN;                  /* FREE DOM FRR          @Y02668P*/ 01165000
         BAL   @14,RELFRRTN                                     01S0297 01166000
*   GENERATE;                                                   01S0298 01167000
         L     REG13,4(,REG9)     PICK UP ADDR OF CALLERS SAVEAREA      01168000
         L     REG0,@SIZDATD      SET UP FREEMAIN PARAMETERS AND        01169000
         L     REG4,PSATOLD       ADDR OF CURRENT TCB                   01170000
         L     REG7,PSAAOLD       AND ADDR OF CURRENT ASCB              01171000
         LR    REG1,REG9          FOR BRANCH ENTRY TO FREEMAIN          01172000
         FREEMAIN  R,LV=(0),A=(1),BRANCH=YES                            01173000
         LM    REG14,REG12,12(REG13)                                    01174000
         BR    REG14              RETURN TO CALLER                      01175000
*                                                               02S0299 01176000
*/********************************************************************/ 01177000
*/*                POST  REPLY  ECB  SEGMENT                         */ 01178000
*/*                                                                  */ 01179000
*/* THE FOLLOWING SEGMENT WILL BE CALLED DURING A SCAN OF THE ORE ECB*/ 01180000
*/* CHAIN POINTED TO OUT OF THE UCM(UCMOECBH). THE FIRST ECB ELEMENT */ 01181000
*/* ON THE CHAIN WHOSE POST BIT IS OFF WILL BE POINTED AT BY WWBPTR. */ 01182000
*/* THE WWB WILL BE POSTED VIA XMPOST. THE WWB POSTED BIT WILL BE SET*/ 01183000
*/* ON INDICATING THAT THIS ECB ELEMENT HAS BEEN POSTED.             */ 01184000
*/*                                                                  */ 01185000
*/* INPUT:                                                           */ 01186000
*/*      WWBPTR - ADDRESS OF THE WWB TO BE POSTED.                   */ 01187000
*/*      CVT0PT01 - POST ENTRY POINT                                 */ 01188000
*/* OUTPUT:                                                          */ 01189000
*/*      REPLY ECB POSTED                                            */ 01190000
*/*                                                                  */ 01191000
*/*  DEPENDENCY - THE SAVEAREA FOR THIS ROUTINE'S CALLER WILL BE     */ 01192000
*/*  BASED ON REG13. THIS ROUTINE ALSO DEPENDS ON  THE BRANCH ENTRY  */ 01193000
*/*  TO XMPOST NOT DESTROYING REGISTER 9. THE CODE ALSO DEPENDS      */ 01194000
*/*  ON THE CODE BEING IN THE ORDER OF: LOAD REG10, BALR,            */ 01195000
*/*  LR REG13,REG9, LM TO RESTORE THE REGISTERS.  THE LOAD REG10     */ 01196000
*/*  DESTROYS THE PROGRAMS BASE REG AND IT IS WITHOUT A BASE UNTIL   */ 01197000
*/* THE LOAD MULTIPLE INSTRUCTION.                                   */ 01198000
*/*                                                                  */ 01199000
*/********************************************************************/ 01200000
*                                                               02S0299 01201000
*POSTOECB:                                                      02S0299 01202000
*   PROC;                                                       02S0299 01203000
POSTOECB STM   @14,@12,@SA00002                                 02S0299 01204000
*   RFY                                                         02S0300 01205000
*    (REG10,                                                    02S0300 01206000
*     REG11,                                                    02S0300 01207000
*     REG12,                                                    02S0300 01208000
*     REG13,                                                    02S0300 01209000
*     REG14,                                                    02S0300 01210000
*     REG15) RSTD;                  /*                       @ZA03871*/ 01211000
*   REG13=WWBASCB;                  /* ADDRESS OF USERS ASCB         */ 01212000
         L     REG13,WWBASCB(,WWBPTR)                           02S0301 01213000
*   REG15=CVT0PT01;                 /* POST ENTRY POINT              */ 01214000
         L     @02,CVTPTR                                       02S0302 01215000
         L     REG15,CVT0PT01(,@02)                             02S0302 01216000
*   REG11=ADDR(WWBECB);             /* ADDRESS OF ECB                */ 01217000
         LA    REG11,WWBECB(,WWBPTR)                            02S0303 01218000
*   REG11=REG11|CROSSMEM;           /* INDICATE XMPOST               */ 01219000
         L     @08,@CF00085                                     02S0304 01220000
         OR    REG11,@08                                        02S0304 01221000
*   REG12=UCMWAKUP;                 /* ADDR OF IEAVMEST              */ 01222000
         L     REG12,UCMWAKUP(,LUCMPTR)                         02S0305 01223000
*   REG12=REG12|CROSSMEM;           /* INDICATE ERRET TO RUN IN 02S0306 01224000
*                                      MASTER MEMORY         @ZA03242*/ 01225000
         OR    REG12,@08                                        02S0306 01226000
*   REG10=ZERO;                     /* COMPLETION CODE               */ 01227000
         SLR   REG10,REG10                                      02S0307 01228000
*   GEN;                                                        02S0308 01229000
*                                      /*ISSUE POST                  */ 01230000
            BALR  14,15                BRANCH TO POST                   01231000
            LR    REG13,REG9           RESTORE SAVE AREA BASE REG       01232000
*   RFY                                                         02S0309 01233000
*    (REG10,                                                    02S0309 01234000
*     REG11,                                                    02S0309 01235000
*     REG12,                                                    02S0309 01236000
*     REG13,                                                    02S0309 01237000
*     REG14,                                                    02S0309 01238000
*     REG15) UNRSTD;                /*                       @ZA03871*/ 01239000
*   END POSTOECB;                                               02S0310 01240000
@EL00002 DS    0H                                               02S0310 01241000
@EF00002 DS    0H                                               02S0310 01242000
@ER00002 LM    @14,@12,@SA00002                                 02S0310 01243000
         BR    @14                                              02S0310 01244000
*                                                               02S0311 01245000
*/********************************************************************/ 01246000
*/*                                                                  */ 01247000
*/*   BUILDEND SUBROUTINE                                            */ 01248000
*/*                                                                  */ 01249000
*/*   FUNCTION - THIS ROUTINE WILL BUILD A MINOR WQE END LINE AT     */ 01250000
*/*   THE LOCATION POINTED AT BY MINORPTR. THIS ROUTINE DEPENDS ON   */ 01251000
*/*   THE FORMAT FOR THE 1ST AND 2ND HALFS OF THE MINOR WQE BEING    */ 01252000
*/*   IDENTICAL. THE MAPPING FOR THE FIRST HALF IS USED EVEN THOUGH  */ 01253000
*/*   THE MINORPTR PTS AT THE SECOND HALF OR THE RESA (FAKE)         */ 01254000
*/*   AREA IN THE MAJOR WQE.                                         */ 01255000
*/*                                                                  */ 01256000
*/*   INPUT - MINORPTR PTS AT THE END LINE AND CHAINPTR PTS AT MAJOR.*/ 01257000
*/*                                                                  */ 01258000
*/*   OUTPUT - THE MLWTO IS ENDED                                    */ 01259000
*/*                                                                  */ 01260000
*/********************************************************************/ 01261000
*                                                               02S0311 01262000
*BUILDEND:                                                      02S0311 01263000
*   PROC OPTIONS(NOSAVEAREA);                                   02S0311 01264000
BUILDEND STM   @14,@12,12(@13)                                  02S0311 01265000
*   WMNMUC1=WMJMUC;                 /* PICK UP USE COUNT FROM MAJOR  */ 01266000
         IC    @12,WMJMUC(,CHAINPTR)                            02S0312 01267000
         STC   @12,WMNMUC1(,MINORPTR)                           02S0312 01268000
*   WMNMNX1=ZERO;                   /* END OF THE MINOR CHAIN        */ 01269000
         SLR   @12,@12                                          02S0313 01270000
         STCM  @12,7,WMNMNX1(MINORPTR)                          02S0313 01271000
*   WMNMLT1=ENDTYPE;                /* MARK LINE TYPE BYTE AS END       01272000
*                                      LINE                          */ 01273000
         MVI   WMNMLT1(MINORPTR),X'10'                          02S0314 01274000
*   WMNMTL1=ZERO;                   /* ZERO TEXT                     */ 01275000
         MVI   WMNMTL1(MINORPTR),X'00'                          02S0315 01276000
*   WMNMML1C=YES;                   /* INDICATE MINOR WQE            */ 01277000
         OI    WMNMML1C(MINORPTR),B'00100000'                   02S0316 01278000
*   WMJMMLWD=YES;                   /* INDICATE CHAIN WAS ALTERED    */ 01279000
         OI    WMJMMLWD(CHAINPTR),B'00010000'                   02S0317 01280000
*   END BUILDEND;                                               02S0318 01281000
@EL00003 DS    0H                                               02S0318 01282000
@EF00003 DS    0H                                               02S0318 01283000
@ER00003 LM    @14,@12,12(@13)                                  02S0318 01284000
         BR    @14                                              02S0318 01285000
*                                                               02S0319 01286000
*/********************************************************************/ 01287000
*/*                                                                  */ 01288000
*/*   MESSAGE OUTPUT SUBROUTINE                                      */ 01289000
*/*                                                                  */ 01290000
*/*   FUNCTION - THIS ROUTINE PREPARES A WPL WHICH CONTAINS  @Y02668P*/ 01291000
*/*   THE 'IEE400I THESE MESSAGES CANCELLED' MESSAGE. THE    @Y02668P*/ 01292000
*/*   ROUTINE WILL GET CORE FOR A WPL IF NEEDED AND ADD THE  @Y02668P*/ 01293000
*/*   REPLY ID OR IDS PASSED. WHEN THE WPL IS FULL, ALL      @Y02668P*/ 01294000
*/*   LOCKS WILL BE FREED AND A WTO SVC WILL BE ISSUED.      @Y02668P*/ 01295000
*/*                                                                  */ 01296000
*/*   INPUT - THIS ROUTINE HAS AN INPUT PARAMETER OF THE REPLY ID    */ 01297000
*/*   AS TWO PRINTABLE CHARACTERS FROM '00' TO '99'. IF              */ 01298000
*/*   PARAMETER IS A NUMERIC ZERO (NOT A CHARACTER ZERO WHICH IS     */ 01299000
*/*   'F0F0' IN HEX) THEN THIS IS THE LAST CALL TO MSGPROC AND       */ 01300000
*/*   ANY WPL CURRENTLY IN USE IS TO BE PROCESSED.           @Y02668P*/ 01301000
*/*                                                                  */ 01302000
*/*   OUTPUT - THE 'IEE400I' MESSAGE.                                */ 01303000
*/*                                                                  */ 01304000
*/********************************************************************/ 01305000
*                                                               02S0319 01306000
*MSGPROC:                                                       02S0319 01307000
*   PROC(REPLYID);                                              02S0319 01308000
MSGPROC  STM   @14,@12,@SA00004                                 02S0319 01309000
         MVC   @PC00004(4),0(@01)                               02S0319 01310000
*   DCL                                                         02S0320 01311000
*     REPLYID CHAR(2);              /* THE ORE ID TO BE INSERTED INTO   01312000
*                                      THE MESSAGE OR ZERO FOR CLEAN    01313000
*                                      UP. IF AN ORE ID THE ID IS IN    01314000
*                                      PRINTABLE FORM                */ 01315000
*                                                               02S0320 01316000
*   /*****************************************************************/ 01317000
*   /*                                                               */ 01318000
*   /* A RECOVERY FOOTPRINT WILL BE SET IN THE FUNCTIONAL    @Y02668P*/ 01319000
*   /* RECOVERY PARAMETER LIST FOR DOM PROCESSING TO         @Y02668P*/ 01320000
*   /* INDICATE START OF MESSAGE PROCESSING. IF AN ERROR     @Y02668P*/ 01321000
*   /* OCCURS WITHIN THIS SEGMENT BEFORE THE NEXT FOOTPRINT  @Y02668P*/ 01322000
*   /* IS SET, RTM WILL TRANSFER CONTROL VIA RETRY TO THE    @Y02668P*/ 01323000
*   /* END OF THIS SEGMENT THUS SUPPRESSING MESSAGE          @Y02668P*/ 01324000
*   /* PROCESSING.                                           @Y02668P*/ 01325000
*   /*                                                               */ 01326000
*   /*****************************************************************/ 01327000
*                                                               02S0321 01328000
*   PARMFTPT=FTPTMSST;              /* IND. START MSG PROCESSING        01329000
*                                                            @Y02668P*/ 01330000
         L     @12,PARMPTR                                      02S0321 01331000
         MVI   PARMFTPT(@12),X'02'                              02S0321 01332000
*   PARMRTAD=ADDR(MSGLABEL);        /* RETRY AT END OF SEGMENT  02S0322 01333000
*                                                            @Y02668P*/ 01334000
         LA    @11,MSGLABEL                                     02S0322 01335000
         ST    @11,PARMRTAD(,@12)                               02S0322 01336000
*   STM(REG0,REG15,RECREGS);        /* SAVE ALL REGS         @Y02668P*/ 01337000
*                                                               02S0323 01338000
         STM   REG0,REG15,RECREGS                               02S0323 01339000
*   /*****************************************************************/ 01340000
*   /*                                                               */ 01341000
*   /* CHECK IF A REPLY ID WAS PASSED OR IS THIS THE LAST CALL       */ 01342000
*   /*                                                               */ 01343000
*   /*****************************************************************/ 01344000
*                                                               02S0324 01345000
*   IF REPLYID^=LAST THEN           /* WAS A VALID REPLY ID PASSED   */ 01346000
         SLR   @11,@11                                          02S0324 01347000
         L     @08,@PC00004                                     02S0324 01348000
         SLR   @02,@02                                          02S0324 01349000
         ICM   @02,3,REPLYID(@08)                               02S0324 01350000
         CR    @11,@02                                          02S0324 01351000
         BE    @RF00324                                         02S0324 01352000
*     DO;                           /* YES. CHECK IF WE NEED TO GET A   01353000
*                                      WQE                           */ 01354000
*       IF MSGPTR=ZERO              /* WE NEED A WPL IF NONE IS 02S0326 01355000
*                                      CURRENTLY                     */ 01356000
*           &ERRINWQE=NO THEN       /* AVAILABLE AND NO WPL ERROR WAS   01357000
*                                      FOUND                 @Y02668P*/ 01358000
         C     @11,MSGPTR                                       02S0326 01359000
         BNE   @RF00326                                         02S0326 01360000
         TM    ERRINWQE,B'01000000'                             02S0326 01361000
         BNZ   @RF00326                                         02S0326 01362000
*         DO;                       /* YES WE NEED ONE       @Y02668P*/ 01363000
*                                                               03S0328 01364000
*/*      GETWPL   SEGMENT                                    @Y02668P*/ 01365000
*/*                                                          @Y02668P*/ 01366000
*/*  FUNCTION - THE FOLLOWING SEGMENT WILL GET CORE OUT OF   @Y02668P*/ 01367000
*/*             SUBPOOL 229 FOR A WPL (WRITE PARAMETER LIST).@Y02668P*/ 01368000
*/*             THIS CORE WILL BE ZEROED OUT AND USED TO     @Y02668P*/ 01369000
*/*             CONTAIN MESSAGE 'IEE400I THESE MESSAGES      @Y02668P*/ 01370000
*/*             CANCELLED'. IF NO CORE CAN BE OBTAINED,      @Y02668P*/ 01371000
*/*             GETMAIN FAILS, AN ERROR INDICATOR WILL BE    @Y02668P*/ 01372000
*/*             SET INDICATING THAT NORMAL DOM PROCESSING    @Y02668P*/ 01373000
*/*             SHOULD CONTINUE BUT PROCESSING OF MESSAGE    @Y02668P*/ 01374000
*/*             IEE400I SHOULD BE TERMINATED.                @Y02668P*/ 01375000
*/*                                                          @Y02668P*/ 01376000
*/*  INPUT - NONE                                            @Y02668P*/ 01377000
*/*                                                          @Y02668P*/ 01378000
*/*  OUTPUT - A BLOCK OF CORE (WPL) IS ZEROED AND RETURNED TO@Y02668P*/ 01379000
*/*           THE CALLER IN MSGPTR.                          @Y02668P*/ 01380000
*/*           IF A WPL COULD NOT BE GOTTEN THEN THE ADDR     @Y02668P*/ 01381000
*/*           POINTER MSGPTR IS SET TO ONE INSTEAD OF BEING  @Y02668P*/ 01382000
*/*           THE ADDR OF A WPL.                             @Y02668P*/ 01383000
*/***********************************************************@Y02668P*/ 01384000
*                                                               03S0328 01385000
*           RFY                                                 03S0328 01386000
*            (REG0,                                             03S0328 01387000
*             REG1,                                             03S0328 01388000
*             REG15) RSTD;          /*                       @ZA03871*/ 01389000
*           PARMFTPT=FTPTWPLP;      /* IND. START OF WPL PROC   03S0329 01390000
*                                                            @Y02668P*/ 01391000
         MVI   PARMFTPT(@12),X'03'                              03S0329 01392000
*           PARMRTAD=ADDR(GETWPLRT);/* SET RETRY ADDR        @Y02668P*/ 01393000
         LA    @14,GETWPLRT                                     03S0330 01394000
         ST    @14,PARMRTAD(,@12)                               03S0330 01395000
*           REG15=ONE;              /* IND. CORE ERROR FOR RETRY        01396000
*                                                            @Y02668P*/ 01397000
         LA    REG15,1                                          03S0331 01398000
*           STM(REG0,REG15,RECREGS);/* SAVE ALL REGISTERS    @Y02668P*/ 01399000
         STM   REG0,REG15,RECREGS                               03S0332 01400000
*           MSGPTR=ZERO;            /* ZERO MSG ADDR INDICATOR  03S0333 01401000
*                                                            @Y02668P*/ 01402000
         ST    @11,MSGPTR                                       03S0333 01403000
*           REG0=LENGTH(WQE);       /* SET LENGTH OF GETMAIN @Y02668P*/ 01404000
         LA    REG0,192                                         03S0334 01405000
*           REG4=UCMPXA;            /* ADDR OF TCB           @Y02668P*/ 01406000
         L     REG4,UCMPXA(,LUCMPTR)                            03S0335 01407000
*           REG7=UCMASCB;           /* ADDR OF ASCB          @Y02668P*/ 01408000
         L     REG7,UCMASCB(,LUCMPTR)                           03S0336 01409000
*           GEN(GETMAIN RC,LV=(REG0),SP=231,BRANCH=YES);/* GETMAIN      01410000
*                                                            @Y02668P*/ 01411000
*                                                               03S0337 01412000
         GETMAIN RC,LV=(REG0),SP=231,BRANCH=YES                         01413000
*           /*********************************************************/ 01414000
*           /*                                                       */ 01415000
*           /* CHECK TO SEE IF GETMAIN WAS SUCCESSFUL. IF NOT        */ 01416000
*           /*                                               @Y02668P*/ 01417000
*           /* INDICATE NO CORE OBTAINED.                    @Y02668P*/ 01418000
*           /*                                                       */ 01419000
*           /*********************************************************/ 01420000
*                                                               03S0338 01421000
*GETWPLRT:                          /* GETWPL RECOVERY RETRY LOCATION   01422000
*                                                            @Y02668P*/ 01423000
*           IF REG15^=ZERO THEN     /* WAS GETMAIN SUCCESSFUL?  03S0338 01424000
*                                                            @Y02668P*/ 01425000
GETWPLRT LTR   REG15,REG15                                      03S0338 01426000
         BZ    @RF00338                                         03S0338 01427000
*             DO;                   /* SET NO WPL            @Y026689*/ 01428000
*               MSGPTR=ONE;         /* INDICATE NO WPL GOTTEN   03S0340 01429000
*                                                            @Y02668P*/ 01430000
         MVC   MSGPTR(4),@CF00054                               03S0340 01431000
*             END;                  /* END IF NO WPL         @Y02668P*/ 01432000
*           ELSE                                                03S0342 01433000
*             DO;                   /* IF WPL WAS OBTAINED   @Y02668P*/ 01434000
         B     @RC00338                                         03S0342 01435000
@RF00338 DS    0H                                               03S0343 01436000
*               MSGPTR=REG1;        /* SAVE ADDRESS OF WQE   @Y02668P*/ 01437000
         ST    REG1,MSGPTR                                      03S0343 01438000
*               REG1->BITZERO(1:LENGTH(WQE)*EIGHT)=''B;/* ZERO AREA     01439000
*                                                            @Y02668P*/ 01440000
         XC    BITZERO(192,REG1),BITZERO(REG1)                  03S0344 01441000
*             END;                  /* END IF WPL GOTTEN     @Y02668P*/ 01442000
*           PARMFTPT=FTPTMSST;      /* RESET FOOTPRINT TO MSG   03S0346 01443000
*                                      PROCESSING            @Y02668P*/ 01444000
@RC00338 L     @14,PARMPTR                                      03S0346 01445000
         MVI   PARMFTPT(@14),X'02'                              03S0346 01446000
*           PARMRTAD=ADDR(MSGLABEL);/* SET NEW RETRY ADDRESS @Y02668P*/ 01447000
         LA    @12,MSGLABEL                                     03S0347 01448000
         ST    @12,PARMRTAD(,@14)                               03S0347 01449000
*           STM(REG0,REG15,RECREGS);/* RESET ALL REGS        @Y02668P*/ 01450000
         STM   REG0,REG15,RECREGS                               03S0348 01451000
*           RFY                                                 03S0349 01452000
*            (REG0,                                             03S0349 01453000
*             REG1,                                             03S0349 01454000
*             REG15) UNRSTD;        /*                       @ZA03871*/ 01455000
*                                                               02S0350 01456000
*           /*********************************************************/ 01457000
*           /*                                                       */ 01458000
*           /* CHECK IF WE DID GET A WPL, IF MSGPTR IS NOT SET TO ONE*/ 01459000
*           /*                                               @Y02668P*/ 01460000
*           /*                                                       */ 01461000
*           /*********************************************************/ 01462000
*                                                               02S0350 01463000
*           IF MSGPTR^=ONE THEN     /* WE GOT A WPL, FILL IT IN 02S0350 01464000
*                                                            @Y02668P*/ 01465000
         L     @12,MSGPTR                                       02S0350 01466000
         C     @12,@CF00054                                     02S0350 01467000
         BE    @RF00350                                         02S0350 01468000
*             DO;                                               02S0351 01469000
*               RFY                                             02S0352 01470000
*                 WPL BASED(MSGPTR);/* SET ADDRESSING FOR OUR WPL    */ 01471000
*               RFY                                             02S0353 01472000
*                 WPLFLGS BASED(MSGPTR+MSGROUTE);/* R/C FLD  @ZM30405*/ 01473000
*               WPLMCSFA=YES;       /* ROUT/DESC CODES EXIST @Y02668P*/ 01474000
*               WPLMCSFE=YES;       /* MSG IS RESULT OF A WTOR  02S0355 01475000
*                                                            @Y02668P*/ 01476000
*                                                               02S0355 01477000
         OI    WPLMCSFA(@12),B'10001000'                        02S0355 01478000
*               /*****************************************************/ 01479000
*               /*                                                   */ 01480000
*               /* NOW MOVE IN THE MSG TEXT AND SET UP FOR INSERTING */ 01481000
*               /* THE REPLY IDS.                                    */ 01482000
*               /*                                                   */ 01483000
*               /*****************************************************/ 01484000
*                                                               02S0356 01485000
*               WPLTXT=MSGTEXT;     /* MOVE IN TEXT OF THE MESSAGE      01486000
*                                                            @Y02668P*/ 01487000
         MVI   WPLTXT+35(@12),C' '                              02S0356 01488000
         MVC   WPLTXT+36(90,@12),WPLTXT+35(@12)                 02S0356 01489000
         MVC   WPLTXT(35,@12),@CC00121                          02S0356 01490000
*               MSGLOC=ADDR(WPLTXT)+LENGTH(MSGTEXT);/* PT AT WHERE TO   01491000
*                                      INSERT REPLY ID       @Y02668P*/ 01492000
         LA    @12,WPLTXT(,@12)                                 02S0357 01493000
         AL    @12,@CF00120                                     02S0357 01494000
         ST    @12,MSGLOC                                       02S0357 01495000
*             END;                                              02S0358 01496000
*           ELSE                    /* NO WE DIDNT GET A WPL @Y02668P*/ 01497000
*             DO;                                               02S0359 01498000
         B     @RC00350                                         02S0359 01499000
@RF00350 DS    0H                                               02S0360 01500000
*               ERRINWQE=YES;       /* TURN ON ERROR INDICATOR       */ 01501000
         OI    ERRINWQE,B'01000000'                             02S0360 01502000
*               MSGPTR=ZERO;        /* INDICATE NO WPL IN USE   02S0361 01503000
*                                                            @Y02668P*/ 01504000
         SLR   @12,@12                                          02S0361 01505000
         ST    @12,MSGPTR                                       02S0361 01506000
*             END;                                              02S0362 01507000
*         END;                      /* OF GET AND SET UP A WPL PART     01508000
*                                                            @Y02668P*/ 01509000
*                                                               02S0363 01510000
@RC00350 DS    0H                                               02S0364 01511000
*       /*************************************************************/ 01512000
*       /*                                                           */ 01513000
*       /* NOW MOVE IN REPLY ID IF NO ERROR WAS ENCOUNTERED          */ 01514000
*       /*                                                           */ 01515000
*       /*************************************************************/ 01516000
*                                                               02S0364 01517000
*       IF ERRINWQE=NO THEN         /* DO WE HAVE A WPL?     @Y02668P*/ 01518000
@RF00326 TM    ERRINWQE,B'01000000'                             02S0364 01519000
         BNZ   @RF00364                                         02S0364 01520000
*         DO;                       /* YES, MOVE IN THE REPLY ID     */ 01521000
*           REPID=REPLYID;          /* MOVE INPUT PARM INTO MSG      */ 01522000
         L     @12,MSGLOC                                       02S0366 01523000
         L     @11,@PC00004                                     02S0366 01524000
         MVC   REPID(2,@12),REPLYID(@11)                        02S0366 01525000
*           REPCOMMA=COMMA;         /* FOLLOW IT WITH A COMMA        */ 01526000
         MVI   REPCOMMA(@12),C','                               02S0367 01527000
*           MSGLOC=MSGLOC+LENGTH(REPMOVE);/* INCR PTR TO NEXT ID     */ 01528000
*                                                               02S0368 01529000
         AL    @12,@CF00112                                     02S0368 01530000
         ST    @12,MSGLOC                                       02S0368 01531000
*           /*********************************************************/ 01532000
*           /*                                                       */ 01533000
*           /* IF THIS WTOR HAS ROUTE CODES, ADD THEN TO THIS WPLS   */ 01534000
*           /* ROUTE CODES                                   @Y02668P*/ 01535000
*           /*                                                       */ 01536000
*           /*********************************************************/ 01537000
*                                                               02S0369 01538000
*           IF CHAINPTR->WQEMCSA=YES THEN/* DOES WTOR HAVE ROUTE CODS*/ 01539000
*                                                               02S0369 01540000
         TM    WQEMCSA(CHAINPTR),B'10000000'                    02S0369 01541000
         BNO   @RF00369                                         02S0369 01542000
*             /*******************************************************/ 01543000
*             /*                                                     */ 01544000
*             /* OR IN WTOR'S ROUTE CODES                            */ 01545000
*             /*                                                     */ 01546000
*             /*******************************************************/ 01547000
*                                                               02S0370 01548000
*             WPLROUT=WPLROUT|CHAINPTR->WQEROUT;/* SET ROUTS @Y02668P*/ 01549000
*                                                               02S0370 01550000
         L     @12,MSGPTR                                       02S0370 01551000
         OC    WPLROUT+180(2,@12),WQEROUT(CHAINPTR)             02S0370 01552000
*           /*********************************************************/ 01553000
*           /*                                                       */ 01554000
*           /* CHECK IF WE REACHED THE END OF THE MSG WITH THIS ID   */ 01555000
*           /*                                                       */ 01556000
*           /*********************************************************/ 01557000
*                                                               02S0371 01558000
*           IF MSGLOC>=ADDR(WPLTXT) /* HAVE WE REACHED THE END       */ 01559000
*               +MAXLNGTH THEN      /* OF THE TEXT PART OF THE MSG      01560000
*                                                            @Y02668P*/ 01561000
@RF00369 L     @12,MSGPTR                                       02S0371 01562000
         LA    @12,WPLTXT(,@12)                                 02S0371 01563000
         AL    @12,@CF00093                                     02S0371 01564000
         C     @12,MSGLOC                                       02S0371 01565000
         BH    @RF00371                                         02S0371 01566000
*             PUTOUTMS=YES;         /* TURN ON PUT OUT MSG FLAG      */ 01567000
         OI    PUTOUTMS,B'10000000'                             02S0372 01568000
*         END;                                                  02S0373 01569000
*     END;                          /* OF NOT LAST CALL PROCESSING   */ 01570000
*                                                               02S0374 01571000
*   /*****************************************************************/ 01572000
*   /*                                                               */ 01573000
*   /* THIS IS A LAST CALL TO MSGPROC. SEE IF THERE IS AN OUTSTANDING*/ 01574000
*   /* WPL TO BE PROCESSED                                   @Y02668P*/ 01575000
*   /*                                                               */ 01576000
*   /*****************************************************************/ 01577000
*                                                               02S0375 01578000
*   ELSE                                                        02S0375 01579000
*     IF MSGPTR^=ZERO THEN          /* IS THERE AN UNENDED WPL LEFT     01580000
*                                                            @Y02668P*/ 01581000
         B     @RC00324                                         02S0375 01582000
@RF00324 L     @12,MSGPTR                                       02S0375 01583000
         LTR   @12,@12                                          02S0375 01584000
         BZ    @RF00375                                         02S0375 01585000
*       IF MSGLOC>ADDR(WPLTXT)+LENGTH(MSGTEXT) THEN/*        @ZA08117*/ 01586000
*                                                               02S0376 01587000
         LA    @12,WPLTXT(,@12)                                 02S0376 01588000
         AL    @12,@CF00120                                     02S0376 01589000
         C     @12,MSGLOC                                       02S0376 01590000
         BNL   @RF00376                                         02S0376 01591000
*         /***********************************************************/ 01592000
*         /*                                                         */ 01593000
*         /* CONTAINING MSG TEXT AND AT LEAST 1 MSG ID.      @ZA08117*/ 01594000
*         /*                                                         */ 01595000
*         /***********************************************************/ 01596000
*                                                               02S0377 01597000
*         PUTOUTMS=YES;             /* TURN ON PUT OUT WPL FLAG 02S0377 01598000
*                                                            @Y02668P*/ 01599000
         OI    PUTOUTMS,B'10000000'                             02S0377 01600000
*       ELSE                        /* NO, ALL DONE DON'T    @ZA08117*/ 01601000
*         DO;                       /* WRITE NULL BUFFER     @ZA08117*/ 01602000
         B     @RC00376                                         02S0378 01603000
@RF00376 DS    0H                                               02S0379 01604000
*           RFY                                                 02S0379 01605000
*            (REG0,                                             02S0379 01606000
*             REG1) RSTD;           /* JUST CLEANUP AND EXIT @ZA08117*/ 01607000
*           REG0=LENGTH(WQE);       /* LENGTH TO FREE        @ZA08117*/ 01608000
         LA    REG0,192                                         02S0380 01609000
*           REG1=MSGPTR;            /* ADDR OF CORE          @ZA08117*/ 01610000
         L     REG1,MSGPTR                                      02S0381 01611000
*           REG4=UCMPXA;            /* TCB ADDRESS           @ZA08117*/ 01612000
         L     REG4,UCMPXA(,LUCMPTR)                            02S0382 01613000
*           REG7=UCMASCB;           /* ASCB ADDRESS          @ZA08117*/ 01614000
         L     REG7,UCMASCB(,LUCMPTR)                           02S0383 01615000
*           GEN REFS(CVTCRMN);                                  02S0384 01616000
*                                                         /* @ZA08117*/ 01617000
*                                  /* ISSUE FREEMAIN         @ZA08117*/ 01618000
             FREEMAIN RU,LV=(0),A=(1),SP=231,BRANCH=YES        @ZA08117 01619000
*           RFY                                                 02S0385 01620000
*            (REG0,                                             02S0385 01621000
*             REG1) UNRSTD;         /*                       @ZA08117*/ 01622000
*           RECREGS=RECOREGS;       /* RESTORE ORIGINAL REGS @ZA08117*/ 01623000
         MVC   RECREGS(64),RECOREGS                             02S0386 01624000
*         END;                      /* END OF CLEANUP NOW EXIT  02S0387 01625000
*                                                            @ZA08117*/ 01626000
*                                                               02S0387 01627000
*   /*****************************************************************/ 01628000
*   /*                                                               */ 01629000
*   /* NOW CHECK IF WE NEED TO PUT OUT THE WPL MESSAGE.      @Y02668P*/ 01630000
*   /*                                                               */ 01631000
*   /*****************************************************************/ 01632000
*                                                               02S0388 01633000
*   IF PUTOUTMS=YES THEN            /* IS PUT OUT MSG FLAG SET ON    */ 01634000
