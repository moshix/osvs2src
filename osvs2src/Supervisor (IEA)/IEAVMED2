*                                                          /*  Y02751*/ 01725000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        X01726000
               RELATED=(UCM,IEAVMED2(FRELCKS))                          01727000
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,                          X01728000
               RELATED=(UCM,IEAVMED2(FRELCKS))                          01729000
*   /*****************************************************************/ 01730000
*   /*                                                               */ 01731000
*   /* RESTORE THE REGISTERS FROM THE SETLOCK                        */ 01732000
*   /*                                                               */ 01733000
*   /*****************************************************************/ 01734000
*                                                               02S0398 01735000
*   REG13=REG0;                     /* RESTORE SAVEAREA PTR    Y02751*/ 01736000
         LR    REG13,REG0                                       02S0398 01737000
*   GEN(  LM REG14,REG12,12(REG13));/* RESET REGS FROM STORAGE  02S0399 01738000
*                                      LOCATION USED BY PROC PROLOG.    01739000
*                                      THIS IS DONE SO THAT REGS USED   01740000
*                                      BY OPTIMIZER WILL BE RESET.   */ 01741000
*                                                               02S0399 01742000
