*           NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;        02S0368 01603000
         SLL   GPR01F,24                                                01604000
*           GPR00F=GPR00F|GPR01F;   /* SP IN HIGH BYTE               */ 01605000
         OR    GPR00F,GPR01F                                    02S0369 01606000
