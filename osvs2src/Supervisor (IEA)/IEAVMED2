         TITLE 'IEAVMED2 - WQE/ORE PURGE ROUTINE                       *00001000
                        '                                               00002000
IEAVMED2 CSECT ,                                                01S0001 00003000
IEECVPRG DS    0H                                               01S0001 00004000
         ENTRY IEECVPRG                                                 00005000
@MAINENT DS    0H                                               01S0001 00006000
         USING *,@15                                            01S0001 00007000
         B     @PROLOG                                          01S0001 00008000
         DC    AL2(@EP00001-@MAINENT)                                   00009000
         DC    AL1(19)                                          01S0001 00010000
         DC    C'IEAVMED2R0300076049'                           01S0001 00011000
         DROP  @15                                                      00012000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00013000
         BALR  @10,0                                            01S0001 00014000
@PSTART  DS    0H                                               01S0001 00015000
         USING @PSTART,@10                                      01S0001 00016000
         L     @00,@SIZDATD                                     01S0001 00017000
         GETMAIN  R,LV=(0)                                              00018000
         LR    @09,@01                                          01S0001 00019000
         USING @DATD,@09                                        01S0001 00020000
         ST    @13,@SA00001+4                                   01S0001 00021000
         LM    @15,@01,16(@13)                                  01S0001 00022000
         ST    @09,8(,@13)                                      01S0001 00023000
         LR    @13,@09                                          01S0001 00024000
         AH    @15,4(,@15)                                      01S0001 00025000
         BR    @15                                              01S0001 00026000
@EP00001 MVC   @PC00001(4),0(@01)                               01S0001 00027000
*                                                               03S0076 00028000
*/*                    END OF TCB                                    */ 00029000
*                                                               03S0077 00030000
*   LRMPLPTR=ADDR(PARMRMPL);        /* INITIALIZE LOCAL COPY OF RMPL    00031000
*                                      ADDR                          */ 00032000
         L     LRMPLPTR,@PC00001                                01S0077 00033000
*   LWKAPTR=RMPLRMWA;               /* INIT LOCAL COPY OF WORKAREA      00034000
*                                      ADDR                          */ 00035000
         L     LWKAPTR,RMPLRMWA(,LRMPLPTR)                      01S0078 00036000
*   LUCMPTR=UCMBASPT;               /* INIT LOCAL ADDR OF THE UCM       00037000
*                                      BASE                          */ 00038000
*                                                               01S0079 00039000
         L     @12,CVTPTR                                       01S0079 00040000
         L     LUCMPTR,UCMBASPT(,@12)                           01S0079 00041000
*   /*****************************************************************/ 00042000
*   /*                                                               */ 00043000
*   /* THIS ROUTINE IS A NOOP FOR NORMAL MEMORY TERMINATION          */ 00044000
*   /*                                                               */ 00045000
*   /*****************************************************************/ 00046000
*                                                               01S0080 00047000
*   IF^(RMPLTYPE=NORMAL&RMPLTERM=MEMORY) THEN                   01S0080 00048000
         TM    RMPLTYPE(LRMPLPTR),B'10000000'                   01S0080 00049000
         BNZ   @RT00080                                         01S0080 00050000
         TM    RMPLTERM(LRMPLPTR),B'01000000'                   01S0080 00051000
         BO    @RF00080                                         01S0080 00052000
@RT00080 DS    0H                                               01S0081 00053000
*     DO;                           /* THIS IS NOT A NORMAL MEMORY      00054000
*                                      TERM.                         */ 00055000
*                                                               01S0081 00056000
*       /*************************************************************/ 00057000
*       /*                                                           */ 00058000
*       /* SET THE LOCAL AND CMS LOCKS                               */ 00059000
*       /*                                                           */ 00060000
*       /*************************************************************/ 00061000
*                                                               01S0082 00062000
*       CALL SETLCKS;                                           01S0082 00063000
*                                                               01S0082 00064000
         BAL   @14,SETLCKS                                      01S0082 00065000
*       /*************************************************************/ 00066000
*       /*                                                           */ 00067000
*       /* INIT FRR FOOTPRINT AND SAVE REGS FOR RETRY                */ 00068000
*       /*                                                           */ 00069000
*       /*************************************************************/ 00070000
*                                                               01S0083 00071000
*       STM(REG0,REG15,RECREGS);    /* STORE REGS IN ORDER 0-15      */ 00072000
         STM   REG0,REG15,RECREGS                               01S0083 00073000
*       RECRGMAP=ALLREGS;           /* INDICATE ALL REGS ARE TO BE      00074000
*                                      RESTORED AT RETRY             */ 00075000
         MVC   RECRGMAP(2),@CB00163                             01S0084 00076000
*       PARMFTPT=FTPTWWB;           /* INDICATE WWB PROCESSING  01S0085 00077000
*                                      STARTED                       */ 00078000
         L     @12,PARMPTR                                      01S0085 00079000
         MVI   PARMFTPT(@12),X'01'                              01S0085 00080000
*       PARMRTAD=ADDR(IEAVORE);     /* IF ERROR, SKIP TO ORE    01S0086 00081000
*                                      PROCESSING                    */ 00082000
*                                                               01S0086 00083000
         LA    @08,IEAVORE                                      01S0086 00084000
         ST    @08,PARMRTAD(,@12)                               01S0086 00085000
*       /*************************************************************/ 00086000
*       /*                                                           */ 00087000
*       /* INITIALIZE M2SUBCHN AND M2FLAGS                           */ 00088000
*       /*                                                           */ 00089000
*       /*************************************************************/ 00090000
*                                                               01S0087 00091000
*       M2SUBCHN=ZERO4;             /* INITIALIZE PTR TO DOMCBS FOR     00092000
*                                      THE SUBSYSTEM EXIT            */ 00093000
         SLR   @12,@12                                          01S0087 00094000
         STCM  @12,15,M2SUBCHN(LWKAPTR)                         01S0087 00095000
*       M2FLAGS=ZERO1;              /* ZERO OUT THE FLAGS BYTE       */ 00096000
*                                                               01S0088 00097000
         MVI   M2FLAGS(LWKAPTR),X'00'                           01S0088 00098000
*       /*************************************************************/ 00099000
*       /*                                                           */ 00100000
*       /* SCAN THE WQE ECB CHAIN AND REMOVE WWB-S FOR THIS TASK OR  */ 00101000
*       /* MEMORY.                                                   */ 00102000
*       /*                                                           */ 00103000
*       /*************************************************************/ 00104000
*                                                               01S0089 00105000
*       DO CHAINPTR=UCMWECBH BY ZERO WHILE(CHAINPTR^=ADDR(UCMWECBH));   00106000
         L     CHAINPTR,UCMWECBH(,LUCMPTR)                      01S0089 00107000
         B     @DE00089                                         01S0089 00108000
@DL00089 DS    0H                                               01S0090 00109000
*         CALL WWBRMOVR;            /* CHECK WWB AND REMOVE IT IF IT    00110000
*                                      BELONGS TO THIS TASK/MEMORY   */ 00111000
         BAL   @14,WWBRMOVR                                     01S0090 00112000
*         CHAINPTR=M2WWBSAV;        /* PT TO NEXT WWB ON THE CHAIN   */ 00113000
         ICM   CHAINPTR,15,M2WWBSAV(LWKAPTR)                    01S0091 00114000
*       END;                                                    01S0092 00115000
*                                                               01S0092 00116000
@DE00089 LA    @08,UCMWECBH(,LUCMPTR)                           01S0092 00117000
         CR    CHAINPTR,@08                                     01S0092 00118000
         BNE   @DL00089                                         01S0092 00119000
*       /*************************************************************/ 00120000
*       /*                                                           */ 00121000
*       /* SCAN THE ORE ECB CHAIN AND REMOVE WWB-S FOR THIS TASK OR  */ 00122000
*       /* MEMORY.                                                   */ 00123000
*       /*                                                           */ 00124000
*       /*************************************************************/ 00125000
*                                                               01S0093 00126000
*       DO CHAINPTR=UCMOECBH BY ZERO WHILE(CHAINPTR^=ADDR(UCMOECBH));   00127000
         L     CHAINPTR,UCMOECBH(,LUCMPTR)                      01S0093 00128000
         B     @DE00093                                         01S0093 00129000
@DL00093 DS    0H                                               01S0094 00130000
*         CALL WWBRMOVR;            /* CHECK WWB AND REMOVE IT IF IT    00131000
*                                      BELONGS TO THIS TASK/MEMORY.  */ 00132000
         BAL   @14,WWBRMOVR                                     01S0094 00133000
*         CHAINPTR=M2WWBSAV;        /* PT TO NEXT WWB ON THE CHAIN   */ 00134000
         ICM   CHAINPTR,15,M2WWBSAV(LWKAPTR)                    01S0095 00135000
*       END;                                                    01S0096 00136000
*                                                               01S0096 00137000
@DE00093 LA    @08,UCMOECBH(,LUCMPTR)                           01S0096 00138000
         CR    CHAINPTR,@08                                     01S0096 00139000
         BNE   @DL00093                                         01S0096 00140000
*       /*************************************************************/ 00141000
*       /*                                                           */ 00142000
*       /* SET UP THE ORE FOOTPRINT                                  */ 00143000
*       /*                                                           */ 00144000
*       /*************************************************************/ 00145000
*                                                               01S0097 00146000
*IEAVORE:                                                       01S0097 00147000
*       PARMFTPT=FTPTORE;           /* SHOW ORE BEING PROCESSED NOW  */ 00148000
IEAVORE  L     @12,PARMPTR                                      01S0097 00149000
         MVI   PARMFTPT(@12),X'02'                              01S0097 00150000
*       PARMRTAD=ADDR(IEAVWQE);     /* IF ERROR THEN SKIP TO WQE        00151000
*                                      ROUTINE                       */ 00152000
*                                                               01S0098 00153000
         LA    @08,IEAVWQE                                      01S0098 00154000
         ST    @08,PARMRTAD(,@12)                               01S0098 00155000
*       /*************************************************************/ 00156000
*       /*                                                           */ 00157000
*       /* SCAN THE ORE CHAIN AND MARK ORE-S FOR THIS TASK/MEMORY AS */ 00158000
*       /* 'DELETED'.                                                */ 00159000
*       /*                                                           */ 00160000
*       /*************************************************************/ 00161000
*                                                               01S0099 00162000
*       DO CHAINPTR=UCMRPYQ BY ZERO WHILE(CHAINPTR^=ZERO);      01S0099 00163000
         L     CHAINPTR,UCMRPYQ(,LUCMPTR)                       01S0099 00164000
         B     @DE00099                                         01S0099 00165000
@DL00099 DS    0H                                               01S0100 00166000
*                                                               02S0100 00167000
*/********************************************************************/ 00168000
*/*                                                                  */ 00169000
*/*   OREPROC SEGMENT                                                */ 00170000
*/*                                                                  */ 00171000
*/*   FUNCTION - THIS SEGMENT PROCESSES AND PURGES ORES BELONGING TO */ 00172000
*/*   THE TERMINATING TASK/MEMORY. THE ORE WILL BE MARKED AS         */ 00173000
*/*   'DELETED' AND A DOMCB WILL BE BUILT WITH THE WQE-S MESSAGE ID. */ 00174000
*/*                                                                  */ 00175000
*/*   INPUT - CHAINPTR IS POINTING AT THE ORE TO PROCESS.            */ 00176000
*/*                                                                  */ 00177000
*/*   OUTPUT- THE OREBUFC BIT WILL BE SET MARKING THIS ORE FOR       */ 00178000
*/*   DELETION. A DOMCB WILL BE BUILT AND ADDED TO THE DOM CHAIN.    */ 00179000
*/*                                                                  */ 00180000
*/*   ROUTINES - THIS SEGMENT USES THE DOMCBLD ROUTINE               */ 00181000
*/*                                                                  */ 00182000
*/********************************************************************/ 00183000
*                                                               02S0100 00184000
*         /***********************************************************/ 00185000
*         /*                                                         */ 00186000
*         /* CHECK IF MEMORY IDENTIFIERS MATCH UP.                   */ 00187000
*         /*                                                         */ 00188000
*         /***********************************************************/ 00189000
*                                                               02S0100 00190000
*         IF OREASID=RMPLASID THEN  /* DOES ORE BELONG TO THIS MEMORY*/ 00191000
         LH    @08,RMPLASID(,LRMPLPTR)                          02S0100 00192000
         N     @08,@CF02556                                     02S0100 00193000
         CH    @08,OREASID(,CHAINPTR)                           02S0100 00194000
         BNE   @RF00100                                         02S0100 00195000
*           IF RMPLTERM=MEMORY      /* YES, IS THIS A MEMORY    02S0101 00196000
*                                      TERMINATION                   */ 00197000
*               |ORETCBA=RMPLTCBA   /* OR DO TCB ADDRESSES MATCH     */ 00198000
*             THEN                  /* YES, PROCESS THIS ORE         */ 00199000
         TM    RMPLTERM(LRMPLPTR),B'01000000'                   02S0101 00200000
         BO    @RT00101                                         02S0101 00201000
         L     @08,ORETCBA-1(,CHAINPTR)                         02S0101 00202000
         LA    @08,0(,@08)                                      02S0101 00203000
         C     @08,RMPLTCBA(,LRMPLPTR)                          02S0101 00204000
         BNE   @RF00101                                         02S0101 00205000
@RT00101 DS    0H                                               02S0102 00206000
*             DO;                                               02S0102 00207000
*                                                               02S0102 00208000
*               /*****************************************************/ 00209000
*               /*                                                   */ 00210000
*               /* MARK THE ORE AS 'TO BE DELETED' SO THAT REPLY AND */ 00211000
*               /* D R WONT PROCESS THIS ORE.                        */ 00212000
*               /*                                                   */ 00213000
*               /*****************************************************/ 00214000
*                                                               02S0103 00215000
*               OREBUFC=YES;        /* TURN ON 'DELETED' BIT         */ 00216000
         OI    OREBUFC(CHAINPTR),B'00100000'                    02S0103 00217000
*               OREWQE->WQEDOM=YES; /* MARK THE MSG TO BE DOMED      */ 00218000
*               OREWQE->WQEPURGE=YES;/* MARK THE MSG PURGABLE        */ 00219000
*                                                               02S0105 00220000
         L     @08,OREWQE(,CHAINPTR)                            02S0105 00221000
         OI    WQEDOM(@08),B'10000100'                          02S0105 00222000
*               /*****************************************************/ 00223000
*               /*                                                   */ 00224000
*               /* BUILD A DOMCB WITH THE MESSAGE ID FROM THE RELATED*/ 00225000
*               /* WQE.                                      @ZA06808*/ 00226000
*               /*                                                   */ 00227000
*               /*****************************************************/ 00228000
*                                                               02S0106 00229000
*               M2DOMTYP=TWOMSGID;  /* ONE FOR DOM CHAIN AND ONE FOR    00230000
*                                      THE SUBSYSTEM EXIT    @ZA06808*/ 00231000
         MVC   M2DOMTYP(2,LWKAPTR),@CH00078                     02S0106 00232000
*               CALL DOMCBLD(M2DOMTYP,OREWQE->WQESEQN);         02S0107 00233000
         LA    @07,M2DOMTYP(,LWKAPTR)                           02S0107 00234000
         ST    @07,@AL00001                                     02S0107 00235000
         LA    @08,WQESEQN(,@08)                                02S0107 00236000
         ST    @08,@AL00001+4                                   02S0107 00237000
         LA    @01,@AL00001                                     02S0107 00238000
         BAL   @14,DOMCBLD                                      02S0107 00239000
*               DOMCWR=YES;         /* MSG ID IS FOR A WTOR          */ 00240000
*                                                               02S0108 00241000
         OI    DOMCWR(DOMCPTR),B'00000100'                      02S0108 00242000
*               /*****************************************************/ 00243000
*               /*                                                   */ 00244000
*               /* ADD DOMCB TO START OF CHAIN                       */ 00245000
*               /*                                                   */ 00246000
*               /*****************************************************/ 00247000
*                                                               02S0109 00248000
*               DOMCLNKA=UCMDOME;   /* PT THIS DOMCB TO FIRST ON        00249000
*                                      CHAIN                         */ 00250000
         L     @08,CVTPTR                                       02S0109 00251000
         L     @08,UCMBASPT(,@08)                               02S0109 00252000
         AL    @08,@CF02559                                     02S0109 00253000
         L     @08,UCMMCSPT(,@08)                               02S0109 00254000
         MVC   DOMCLNKA(3,DOMCPTR),UCMDOME+1(@08)               02S0109 00255000
*               UCMDOME=DOMCPTR;    /* PT DOME CHAIN HEAD AT THIS       00256000
*                                      DOMCB                         */ 00257000
         ST    DOMCPTR,UCMDOME(,@08)                            02S0110 00258000
*               M2DOMBLT=YES;       /* TURN ON DOM CHAIN ADDED TO BIT*/ 00259000
*                                                               02S0111 00260000
         OI    M2DOMBLT(LWKAPTR),B'00100000'                    02S0111 00261000
*               /*****************************************************/ 00262000
*               /*                                                   */ 00263000
*               /* LINK SECOND DOMCB TO THE SUBSYSTEM CHAIN  @ZA06808*/ 00264000
*               /*                                                   */ 00265000
*               /*****************************************************/ 00266000
*                                                               02S0112 00267000
*               DOMCPTR=DOMCPTR+IDSIZE;                         02S0112 00268000
*                                                               02S0112 00269000
         AL    DOMCPTR,@CF00097                                 02S0112 00270000
*               /*****************************************************/ 00271000
*               /*                                                   */ 00272000
*               /* THE SECOND DOMCB IS RIGHT AFTER THE FIRST         */ 00273000
*               /*                                                   */ 00274000
*               /*****************************************************/ 00275000
*                                                               02S0113 00276000
*               DOMCWR=YES;         /* MSG ID IS FOR A WTOR          */ 00277000
         OI    DOMCWR(DOMCPTR),B'00000100'                      02S0113 00278000
*               DOMCLNKA=M2SUBCHN;  /* PUT NEW DOMCB ON THE CHAIN FOR*/ 00279000
         MVC   DOMCLNKA(3,DOMCPTR),M2SUBCHN+1(LWKAPTR)          02S0114 00280000
*               M2SUBCHN=DOMCPTR;   /* SUBSYSTEM EXIT                */ 00281000
         STCM  DOMCPTR,15,M2SUBCHN(LWKAPTR)                     02S0115 00282000
*             END;                  /* OF ORE PROCESSING             */ 00283000
*         CHAINPTR=ORELKP;          /* PROCESS NEXT ORE ON THE CHAIN */ 00284000
@RF00101 DS    0H                                               01S0117 00285000
@RF00100 L     CHAINPTR,ORELKP(,CHAINPTR)                       01S0117 00286000
*       END;                                                    01S0118 00287000
*                                                               01S0118 00288000
@DE00099 LTR   CHAINPTR,CHAINPTR                                01S0118 00289000
         BNZ   @DL00099                                         01S0118 00290000
*       /*************************************************************/ 00291000
*       /*                                                           */ 00292000
*       /* SET UP THE WQE FOOTPRINT                                  */ 00293000
*       /*                                                           */ 00294000
*       /*************************************************************/ 00295000
*                                                               01S0119 00296000
*IEAVWQE:                                                       01S0119 00297000
*       PARMFTPT=FTPTWQE;           /* SHOW WQES BEING PROCESSED NOW */ 00298000
IEAVWQE  L     @12,PARMPTR                                      01S0119 00299000
         MVI   PARMFTPT(@12),X'03'                              01S0119 00300000
*       PARMRTAD=ADDR(IEAVCALL);    /* IF ERROR, SKIP TO CALL OF        00301000
*                                      IEAVMNTR                      */ 00302000
*                                                               01S0120 00303000
         LA    @08,IEAVCALL                                     01S0120 00304000
         ST    @08,PARMRTAD(,@12)                               01S0120 00305000
*       /*************************************************************/ 00306000
*       /*                                                           */ 00307000
*       /* SCAN THE WQE CHAIN FOR MULTI-LINE MESSAGES AND DESCRIPTOR */ 00308000
*       /* CODE 7 MESSAGES. END THE MULTI-LINE AND 'DOM' THE         */ 00309000
*       /* DESCRIPTOR CODE 7-S.                                      */ 00310000
*       /*                                                           */ 00311000
*       /*************************************************************/ 00312000
*                                                               01S0121 00313000
*       DO CHAINPTR=UCMWTOQ BY ZERO WHILE(CHAINPTR^=ZERO);      01S0121 00314000
*                                                               01S0121 00315000
         L     CHAINPTR,UCMWTOQ(,LUCMPTR)                       01S0121 00316000
         B     @DE00121                                         01S0121 00317000
@DL00121 DS    0H                                               01S0122 00318000
*         /***********************************************************/ 00319000
*         /*                                                         */ 00320000
*         /* CHECK IF THIS MESSAGE IS A MULTI-LINE WTO               */ 00321000
*         /*                                                         */ 00322000
*         /***********************************************************/ 00323000
*                                                               01S0122 00324000
*         IF WMJMMLW^=ZERO1 THEN    /* IS THIS WQE FOR AN MLWTO      */ 00325000
         CLI   WMJMMLW(CHAINPTR),0                              01S0122 00326000
         BE    @RF00122                                         01S0122 00327000
*           DO;                     /* YES, INSURE MESSAGE IS ENDED  */ 00328000
*                                                               02S0124 00329000
*/********************************************************************/ 00330000
*/*   MWTOPROC SEGMENT                                               */ 00331000
*/*                                                                  */ 00332000
*/*   FUNCTION - PROCESS THE MULTI-LINE WTO-S. THIS SEGMENT WILL     */ 00333000
*/*   CHECK THAT THIS MWTO BELONGS TO THE TERMINATING TASK/MEMORY.   */ 00334000
*/*   IT THEN INSURES THAT THE MWTO IS ENDED. IF THERE IS NO ROOM    */ 00335000
*/*   FOR AN ENDLINE IN ONE OF THE MINORS, IT USES AN AREA IN THE    */ 00336000
*/*   MAJOR.                                                         */ 00337000
*/*                                                                  */ 00338000
*/*   INPUT - CHAINPTR PTS AT THE WQE BEING PROCESSED.               */ 00339000
*/*                                                                  */ 00340000
*/*   OUTPUT- THE MLWTO IS ENDED IF IT WAS ISSUED BY THE TERMINATING */ 00341000
*/*   MEMORY OR TASK                                                 */ 00342000
*/*                                                                  */ 00343000
*/********************************************************************/ 00344000
*                                                               02S0124 00345000
*             /*******************************************************/ 00346000
*             /*                                                     */ 00347000
*             /* CHECK IF MLWTO WAS ISSUED BY THIS TASK/MEMORY       */ 00348000
*             /*                                                     */ 00349000
*             /*******************************************************/ 00350000
*                                                               02S0124 00351000
*             IF WMJMASID=RMPLASID THEN/* DO MEMORY IDS MATCH        */ 00352000
         CLC   WMJMASID(2,CHAINPTR),RMPLASID(LRMPLPTR)          02S0124 00353000
         BNE   @RF00124                                         02S0124 00354000
*               IF RMPLTERM=MEMORY  /* YES. IS THIS MEMORY ABEND OR     00355000
*                                      DO                            */ 00356000
*                   |WMJMTCB=RMPLTCBA THEN/* TCBS MATCH              */ 00357000
         TM    RMPLTERM(LRMPLPTR),B'01000000'                   02S0125 00358000
         BO    @RT00125                                         02S0125 00359000
         CLC   WMJMTCB(4,CHAINPTR),RMPLTCBA(LRMPLPTR)           02S0125 00360000
         BNE   @RF00125                                         02S0125 00361000
@RT00125 DS    0H                                               02S0126 00362000
*                 DO;               /* YES. END THE MLWTO            */ 00363000
*                                                               02S0126 00364000
*                   /*************************************************/ 00365000
*                   /*                                               */ 00366000
*                   /* INSURE THAT THIS MLWTO ISN'T WAITING FOR A WQE*/ 00367000
*                   /* TO BE RELEASED                                */ 00368000
*                   /*                                               */ 00369000
*                   /*************************************************/ 00370000
*                                                               02S0127 00371000
*                   WMJMWAIT=NO;    /* TURN OFF WAITING FOR WQE BIT  */ 00372000
*                                                               02S0127 00373000
         NI    WMJMWAIT(CHAINPTR),B'01111111'                   02S0127 00374000
*                   /*************************************************/ 00375000
*                   /*                                               */ 00376000
*                   /* INSURE THE MAJOR IS ENDED                     */ 00377000
*                   /*                                               */ 00378000
*                   /*************************************************/ 00379000
*                                                               02S0128 00380000
*                   MINORPTR=WMJMMIN;/* POINT AT FIRST MINOR WQE     */ 00381000
*                                                               02S0128 00382000
         L     MINORPTR,WMJMMIN(,CHAINPTR)                      02S0128 00383000
*                   /*************************************************/ 00384000
*                   /*                                               */ 00385000
*                   /* CHECK IF MINOR CAN BE USED FOR AN END LINE.   */ 00386000
*                   /* THERE IS ALWAYS A MINOR WQE HUNG OFF THE MAJOR*/ 00387000
*                   /*                                               */ 00388000
*                   /*************************************************/ 00389000
*                                                               02S0129 00390000
*                   IF WMJMMLWH=YES THEN/* 'MINOR HAS NO TEXT' SET      00391000
*                                      ON?                           */ 00392000
         TM    WMJMMLWH(CHAINPTR),B'00000001'                   02S0129 00393000
         BNO   @RF00129                                         02S0129 00394000
*                     DO;           /* YES. PUT END LINE IN MINOR    */ 00395000
*                       WMJMMLWH=NO;/* MINOR NOW HAS TEXT            */ 00396000
*                       WMJMMLWF=YES;/* INDICATE START AT TOP OF        00397000
*                                      CHAIN                         */ 00398000
         OI    WMJMMLWF(CHAINPTR),B'00000100'                   02S0132 00399000
         NI    WMJMMLWH(CHAINPTR),B'11111110'                   02S0132 00400000
*                       CALL BUILDEND;/* BUILD END LINE IN MINOR     */ 00401000
         BAL   @14,BUILDEND                                     02S0133 00402000
*                     END;                                      02S0134 00403000
*                                                               02S0134 00404000
*                   /*************************************************/ 00405000
*                   /*                                               */ 00406000
*                   /* NO. FIRST MINOR HAS TEXT. SCAN FOR END OF     */ 00407000
*                   /* MINOR CHAIN                                   */ 00408000
*                   /*                                               */ 00409000
*                   /*************************************************/ 00410000
*                                                               02S0135 00411000
*                   ELSE                                        02S0135 00412000
*                     DO;                                       02S0135 00413000
         B     @RC00129                                         02S0135 00414000
@RF00129 DS    0H                                               02S0136 00415000
*                       M2ENDFND=NO;/* TURN OFF LOOP BIT             */ 00416000
         NI    M2ENDFND(LWKAPTR),B'01111111'                    02S0136 00417000
*                       DO WHILE(M2ENDFND=NO);                  02S0137 00418000
*                                                               02S0137 00419000
         B     @DE00137                                         02S0137 00420000
@DL00137 DS    0H                                               02S0138 00421000
*                         /*******************************************/ 00422000
*                         /*                                         */ 00423000
*                         /* CONTINUE TO SCAN THE MINOR CHAIN UNTIL  */ 00424000
*                         /* THE END IS FOUND CHECK IF 1ST LINE IN   */ 00425000
*                         /* MINOR IS LAST LINE                      */ 00426000
