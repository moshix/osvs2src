*            (GPR01F,                                           02S0372 01615000
*             GPR00F) UNRESTRICTED;                             02S0372 01616000
*         END;                      /* GETMAIN (R)              02S0373 01617000
*                                      LV(M2DOMSIZ)SP(SUBPL231) 02S0373 01618000
*                                      BRANCH                @ZA06808*/ 01619000
*         DOMCPTR=REG1;             /* SAVE LOCATION                 */ 01620000
         LR    DOMCPTR,REG1                                     02S0374 01621000
