         CLC   WWBTCBAD(4,CHAINPTR),RMPLTCBA(LRMPLPTR)          02S0285 01226000
         BNE   @RF00285                                         02S0285 01227000
@RT00285 DS    0H                                               02S0286 01228000
*       DO;                         /* YES, REMOVE AND FREE THE WWB  */ 01229000
*                                                               02S0286 01230000
*         /***********************************************************/ 01231000
*         /*                                                         */ 01232000
*         /* TAKE THE WWB POINTED AT BY CHAINPTR OFF THE CHAIN. POINT*/ 01233000
*         /* NEXT WWB AT BACK WWB                                    */ 01234000
*         /*                                                         */ 01235000
*         /***********************************************************/ 01236000
*                                                               02S0287 01237000
*         WWBFWDPT->WWBBCKPT=WWBBCKPT;                          02S0287 01238000
*                                                               02S0287 01239000
         L     @08,WWBFWDPT(,CHAINPTR)                          02S0287 01240000
         L     @04,WWBBCKPT(,CHAINPTR)                          02S0287 01241000
         ST    @04,WWBBCKPT(,@08)                               02S0287 01242000
*         /***********************************************************/ 01243000
*         /*                                                         */ 01244000
*         /* NOW PT BACK WWB AT NEXT WWB                             */ 01245000
*         /*                                                         */ 01246000
*         /***********************************************************/ 01247000
*                                                               02S0288 01248000
*         WWBBCKPT->WWBFWDPT=WWBFWDPT;                          02S0288 01249000
*                                                               02S0288 01250000
         ST    @08,WWBFWDPT(,@04)                               02S0288 01251000
*         /***********************************************************/ 01252000
*         /*                                                         */ 01253000
*         /* THE WWB HAS NOW BEEN REMOVED FROM THE CHAIN. FREE THE   */ 01254000
*         /* WWB. LOAD REG4 AND REG7 WITH THE ADDRESSES OF THE       */ 01255000
*         /* CURRENT TCB AND ASCB.                                   */ 01256000
*         /*                                                         */ 01257000
*         /***********************************************************/ 01258000
*                                                               02S0289 01259000
*         RFY                                                   02S0289 01260000
*          (REG4,                                               02S0289 01261000
*           REG7) RSTD;                                         02S0289 01262000
*         REG4=PSATOLD;             /* GET ADDR OF OLD TCB FROM PSA  */ 01263000
         L     REG4,PSATOLD                                     02S0290 01264000
*         REG7=PSAAOLD;             /* GET ADDR OF OLD ASCB FROM PSA */ 01265000
         L     REG7,PSAAOLD                                     02S0291 01266000
*         DO;                       /* FREEMAIN(R) LV(LENGTH(WWB))SP(   01267000
*                                      SUBPL231)A(WWB) BRANCH        */ 01268000
*           RESPECIFY                                           02S0293 01269000
*            (GPR01F,                                           02S0293 01270000
*             GPR00F) RESTRICTED;                               02S0293 01271000
*           GPR00F=LENGTH(WWB);     /* LENGTH REQUESTED              */ 01272000
         LA    GPR00F,24                                        02S0294 01273000
*           GPR01F=SUBPL231;        /* SP IN REG 1                   */ 01274000
         LA    GPR01F,231                                       02S0295 01275000
*           GEN(SLL   GPR01F,24)                                02S0296 01276000
*           NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;        02S0296 01277000
         SLL   GPR01F,24                                                01278000
*           GPR00F=GPR00F|GPR01F;   /* SP IN HI BYTE                 */ 01279000
         OR    GPR00F,GPR01F                                    02S0297 01280000
*           GPR01F=ADDR(WWB);       /* SET REGISTER ONE              */ 01281000
         LR    GPR01F,CHAINPTR                                  02S0298 01282000
*           CALL CVTRMBR;           /* BRANCH ENTRY POINT            */ 01283000
         L     @12,CVTPTR                                       02S0299 01284000
         L     @15,CVTRMBR(,@12)                                02S0299 01285000
         BALR  @14,@15                                          02S0299 01286000
*           RESPECIFY                                           02S0300 01287000
*            (GPR01F,                                           02S0300 01288000
*             GPR00F) UNRESTRICTED;                             02S0300 01289000
*         END;                      /* FREEMAIN(R) LV(LENGTH(WWB))SP(   01290000
*                                      SUBPL231)A(WWB) BRANCH        */ 01291000
*         RFY                                                   02S0302 01292000
*          (REG4,                                               02S0302 01293000
*           REG7) UNRSTD;           /* REG4 & REG7 NO LONGER NEEDED  */ 01294000
*       END;                                                    02S0303 01295000
*   END WWBRMOVR;                                               02S0304 01296000
@EL00002 DS    0H                                               02S0304 01297000
@EF00002 DS    0H                                               02S0304 01298000
@ER00002 LM    @14,@12,12(@13)                                  02S0304 01299000
         BR    @14                                              02S0304 01300000
*                                                               02S0305 01301000
*/********************************************************************/ 01302000
*/*        DOMCBLD ROUTINE                                           */ 01303000
*/*                                                                  */ 01304000
*/*        FUNCTION - THIS ROUTINE WILL BUILD EITHER ONE OR TWO      */ 01305000
*/*        DOM CONTROL BLOCKS. IF TWO ARE BUILT, THE SECOND WILL BE  */ 01306000
*/*        A DUPLICATE OF THE FIRST.                                 */ 01307000
*/*                                                                  */ 01308000
*/*        INPUT - THIS ROUTINE HAS TWO INPUT PARAMETERS 'BLDTYPE'   */ 01309000
*/*        AND 'BLDID'. THE VALUES FOR THESE PARAMETERS ARE AS       */ 01310000
*/*        FOLLOWS:                                                  */ 01311000
*/*          PARAMETER      CONSTANT  VALUE    MEANING               */ 01312000
*/*                         NAME                                     */ 01313000
*/*                                                                  */ 01314000
*/*          BLDTYPE        ONEMSGID   1   BUILD A DOMCB THAT IS 16  */ 01315000
*/*                                        BYTES LONG WITH A MSG ID  */ 01316000
*/*                         TWOMSGID   2   BUILD TWO IDENTICAL DOMCBS*/ 01317000
*/*                                       THAT ARE EACH 16 BYTES LONG*/ 01318000
*/*                         ASIDONLY   3   BUILD TWO 8 BYTE DOMCBS   */ 01319000
*/*                                        WITH ASID                 */ 01320000
*/*                         ONEASID    4   BUILD ONE DOMCB OF 8 BYTES*/ 01321000
*/*                                        WITH ASID ONLY            */ 01322000
*/*                         ASIDJTCB   5   BUILD TWO DOMCBS EACH 16  */ 01323000
*/*                                        BYTES LONG WITH ASID &    */ 01324000
*/*                                        JSTCB ADDRESS             */ 01325000
*/*                         ONEAJ      6   BUILD ONE DOM BY ASID AND */ 01326000
*/*                                        JOBSTEP TCB.              */ 01327000
*/*                                                          @ZA06808*/ 01328000
*/*          BLDID        CONTAINS THE MESSAGE ID OF THE MESSAGE TO  */ 01329000
*/*                       BE DOMED BY THE DOMCB. IT WILL BE ZERO FOR */ 01330000
*/*                       TYPES 3,4,5&6                      @ZA06808*/ 01331000
*/*        OUTPUT - THIS ROUTINE WILL BUILD THE REQUIRED CONTROL     */ 01332000
*/*        BLOCK(S) AND RETURN THE ADDRESS IN DOMCPTR.               */ 01333000
*/*                                                          @ZA06808*/ 01334000
*/********************************************************************/ 01335000
*                                                               02S0305 01336000
*DOMCBLD:                                                       02S0305 01337000
*   PROC(BLDTYPE,BLDID) OPTIONS(NOSAVEAREA);                    02S0305 01338000
DOMCBLD  STM   @14,@05,12(@13)                                  02S0305 01339000
         STM   @07,@12,48(@13)                                  02S0305 01340000
         MVC   @PC00003(8),0(@01)                               02S0305 01341000
*   DCL                                                         02S0306 01342000
*     BLDTYPE FIXED(15),            /* TYPE OF DOMCB TO BUILD. ALSO     01343000
*                                      USED TO DETERMINE SIZE        */ 01344000
*     BLDID PTR(24);                /* MESSAGE ID OF MESSAGE TO BE      01345000
*                                      DELETED                       */ 01346000
*   RFY                                                         02S0307 01347000
*    (REG4,                                                     02S0307 01348000
*     REG7) RSTD;                   /* REG 4 AND 7 ARE NEEDED FOR       01349000
*                                      GETMAIN                       */ 01350000
*   REG4=PSATOLD;                   /* PUT IN ADDR OF OUR TCB        */ 01351000
         L     REG4,PSATOLD                                     02S0308 01352000
*   REG7=PSAAOLD;                   /* PUT IN ADDR OF OUR ASCB       */ 01353000
         L     REG7,PSAAOLD                                     02S0309 01354000
*                                                               02S0310 01355000
*   /*****************************************************************/ 01356000
*   /*                                                               */ 01357000
*   /* CHECK IF THIS REQUEST IS TO BUILD A DOMCB WITH A MSGID. IF SO */ 01358000
*   /* BUILD IT                                                      */ 01359000
*   /*                                                               */ 01360000
*   /*****************************************************************/ 01361000
*                                                               02S0310 01362000
*   IF BLDTYPE=ONEMSGID|            /* IS REQUEST FOR ONE OR @ZA06808*/ 01363000
*       BLDTYPE=TWOMSGID THEN       /* TWO MSGID DOMCBS ?    @ZA06808*/ 01364000
         L     @12,@PC00003                                     02S0310 01365000
         LH    @12,BLDTYPE(,@12)                                02S0310 01366000
         C     @12,@CF00060                                     02S0310 01367000
         BE    @RT00310                                         02S0310 01368000
         C     @12,@CF00078                                     02S0310 01369000
         BNE   @RF00310                                         02S0310 01370000
@RT00310 DS    0H                                               02S0311 01371000
*     DO;                           /* YES. GET CORE FOR THE DOMCBS  */ 01372000
*       IF BLDTYPE=ONEMSGID THEN    /* ONE DOMCB TO BE BUILT ?  02S0312 01373000
*                                                            @ZA06808*/ 01374000
         L     @12,@PC00003                                     02S0312 01375000
         CLC   BLDTYPE(2,@12),@CH00060                          02S0312 01376000
         BNE   @RF00312                                         02S0312 01377000
*         M2DOMSIZ=IDSIZE*ONE;      /* YES. GET 16 BYTES     @ZA06808*/ 01378000
         MVI   M2DOMSIZ(LWKAPTR),X'10'                          02S0313 01379000
*       ELSE                        /* TWO DOMCBS TO BE BUILT   02S0314 01380000
*                                                            @ZA06808*/ 01381000
*         M2DOMSIZ=IDSIZE*TWO;      /* GET 32 BYTES          @ZA06808*/ 01382000
         B     @RC00312                                         02S0314 01383000
@RF00312 MVI   M2DOMSIZ(LWKAPTR),X'20'                          02S0314 01384000
*       DO;                         /* GETMAIN (R)              02S0315 01385000
*                                      LV(M2DOMSIZ)SP(SUBPL231) 02S0315 01386000
*                                      BRANCH                        */ 01387000
@RC00312 DS    0H                                               02S0316 01388000
*         RESPECIFY                                             02S0316 01389000
*          (GPR01F,                                             02S0316 01390000
*           GPR00F) RESTRICTED;                                 02S0316 01391000
*         GPR00F=M2DOMSIZ;          /* LENGTH REQUESTED              */ 01392000
         SLR   GPR00F,GPR00F                                    02S0317 01393000
         IC    GPR00F,M2DOMSIZ(,LWKAPTR)                        02S0317 01394000
*         GPR01F=SUBPL231;          /* SP IN REG 1                   */ 01395000
         LA    GPR01F,231                                       02S0318 01396000
*         GEN(SLL   GPR01F,24)                                  02S0319 01397000
*         NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;          02S0319 01398000
         SLL   GPR01F,24                                                01399000
*         GPR00F=GPR00F|GPR01F;     /* SP IN HIGH BYTE               */ 01400000
         OR    GPR00F,GPR01F                                    02S0320 01401000
*         GPR01F=-1;                /* INDICATE A GETMAIN REQUEST    */ 01402000
         SLR   GPR01F,GPR01F                                    02S0321 01403000
         BCTR  GPR01F,0                                         02S0321 01404000
*         CALL CVTRMBR;             /* BRANCH ENTRY POINT            */ 01405000
         L     @12,CVTPTR                                       02S0322 01406000
         L     @15,CVTRMBR(,@12)                                02S0322 01407000
         BALR  @14,@15                                          02S0322 01408000
*         RESPECIFY                                             02S0323 01409000
*          (GPR01F,                                             02S0323 01410000
*           GPR00F) UNRESTRICTED;                               02S0323 01411000
*       END;                        /* GETMAIN (R)              02S0324 01412000
*                                      LV(M2DOMSIZ)SP(SUBPL231) 02S0324 01413000
*                                      BRANCH                        */ 01414000
*                                                               02S0324 01415000
*       /*************************************************************/ 01416000
*       /*                                                           */ 01417000
*       /* ZERO THE AREA OF ONE DOMCB                                */ 01418000
*       /*                                                           */ 01419000
*       /*************************************************************/ 01420000
*                                                               02S0325 01421000
*       DOMCPTR=REG1;               /* SET UP PTR TO THE AREA        */ 01422000
         LR    DOMCPTR,REG1                                     02S0325 01423000
*       DOM4WORD=NULLB;             /* INSURE BLOCK IS ZERO          */ 01424000
         XC    DOM4WORD+1(15,DOMCPTR),DOM4WORD+1(DOMCPTR)       02S0326 01425000
         MVI   DOM4WORD(DOMCPTR),B'00000000'                    02S0326 01426000
*       DOMCEND=YES;                /* INDICATE ONLY ONE MSGID IN       01427000
*                                      DOMCB                         */ 01428000
         LA    @12,4                                            02S0327 01429000
         LR    @08,DOMCPTR                                      02S0327 01430000
         ALR   @08,@12                                          02S0327 01431000
         OI    DOMCEND(@08),B'10000000'                         02S0327 01432000
*       DOMCIDA=BLDID;              /* MOVE IN MESSAGE ID OF MSG TO     01433000
*                                      DELETE                        */ 01434000
         LR    @08,DOMCPTR                                      02S0328 01435000
         ALR   @08,@12                                          02S0328 01436000
         L     @15,@PC00003+4                                   02S0328 01437000
         MVC   DOMCIDA(3,@08),BLDID(@15)                        02S0328 01438000
*       DOMCSIZE=IDSIZE;            /* FILL IN LENGTH OF THE DOMCB   */ 01439000
         LR    @08,DOMCPTR                                      02S0329 01440000
         ALR   @08,@12                                          02S0329 01441000
         MVI   DOMCSIZE(@08),X'10'                              02S0329 01442000
*       DOMCASID=RMPLASID;          /* FILL IN THE ASID FROM THE RMPL*/ 01443000
*                                                               02S0330 01444000
         LR    @08,DOMCPTR                                      02S0330 01445000
         ALR   @08,@12                                          02S0330 01446000
         LH    @15,RMPLASID(,LRMPLPTR)                          02S0330 01447000
         N     @15,@CF02556                                     02S0330 01448000
         STH   @15,DOMCASID(,@08)                               02S0330 01449000
*       /*************************************************************/ 01450000
*       /*                                                           */ 01451000
*       /* AND ALSO THE JOBSTEP TCB ADDR                             */ 01452000
*       /*                                                           */ 01453000
*       /*************************************************************/ 01454000
*                                                               02S0331 01455000
*       IF RMPLTERM^=MEMORY THEN    /* DO NOT MOVE IN JSTCB ADDR IF     01456000
*                                      THIS IS MEMORY TERMINATION       01457000
*                                                            @ZA06808*/ 01458000
         TM    RMPLTERM(LRMPLPTR),B'01000000'                   02S0331 01459000
         BO    @RF00331                                         02S0331 01460000
*         DOMCJTCB=RMPLTCBA->TCBJSTCB;                          02S0332 01461000
*                                                               02S0332 01462000
         ALR   @12,DOMCPTR                                      02S0332 01463000
         L     @08,RMPLTCBA(,LRMPLPTR)                          02S0332 01464000
         L     @08,TCBJSTCB(,@08)                               02S0332 01465000
         ST    @08,DOMCJTCB(,@12)                               02S0332 01466000
*       /*************************************************************/ 01467000
*       /*                                                           */ 01468000
*       /* CHECK IF EXTRA DOMCB TO BE BUILT FOR SUBSYSTEM            */ 01469000
*       /*                                                           */ 01470000
*       /*************************************************************/ 01471000
*                                                               02S0333 01472000
*       IF BLDTYPE=TWOMSGID THEN    /* DO WE BUILD THE SECOND DOMCB  */ 01473000
*                                                               02S0333 01474000
@RF00331 L     @12,@PC00003                                     02S0333 01475000
         CLC   BLDTYPE(2,@12),@CH00078                          02S0333 01476000
         BNE   @RF00333                                         02S0333 01477000
*         /***********************************************************/ 01478000
*         /*                                                         */ 01479000
*         /* YES. COPY 1ST DOMCB INTO 2ND.                           */ 01480000
*         /*                                                         */ 01481000
*         /***********************************************************/ 01482000
*                                                               02S0334 01483000
*         EVAL(DOMCPTR+IDSIZE)->DOM4WORD=DOM4WORD;              02S0334 01484000
         LA    @12,16                                           02S0334 01485000
         ALR   @12,DOMCPTR                                      02S0334 01486000
         MVC   DOM4WORD(16,@12),DOM4WORD(DOMCPTR)               02S0334 01487000
*     END;                                                      02S0335 01488000
*   ELSE                            /* THIS REQUEST IS FOR A DOM BY     01489000
*                                      ASID OR AISD AND JSTCB. CHECK    01490000
*                                      WHICH                         */ 01491000
*     IF BLDTYPE=ASIDONLY|          /* IS THIS DOM BY ASID REQUEST      01492000
*                                                            @ZA06808*/ 01493000
*         BLDTYPE=ONEASID THEN      /* FOR ONE OR TWO DOMCBS ?  02S0336 01494000
*                                                            @ZA06808*/ 01495000
         B     @RC00310                                         02S0336 01496000
@RF00310 L     @12,@PC00003                                     02S0336 01497000
         LH    @12,BLDTYPE(,@12)                                02S0336 01498000
         C     @12,@CF00080                                     02S0336 01499000
         BE    @RT00336                                         02S0336 01500000
         C     @12,@CF00082                                     02S0336 01501000
         BNE   @RF00336                                         02S0336 01502000
@RT00336 DS    0H                                               02S0337 01503000
*       DO;                         /* YES. BUILD IT                 */ 01504000
*         RFY                                                   02S0338 01505000
*           DOMCID BASED(DOMCPTR);  /* BASE ID PART SO THAT THE ASID    01506000
*                                      FOLLOWS THE LINK FIELD IN THE    01507000
*                                      DOMCB                         */ 01508000
*         IF BLDTYPE=ONEASID THEN   /* ONLY ON DOMCB TO BUILD ? 02S0339 01509000
*                                                            @ZA06808*/ 01510000
