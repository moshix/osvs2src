*   PARMSYSR='01'X;                 /* INDICATE XMPOST RECOVERY      */ 00964000
         MVI   PARMSYSR(R5),X'01'                                  0358 00965000
*   PARMRTAD=BASEREG;               /* STORE BASE REG                */ 00966000
*                                                                  0359 00967000
         ST    BASEREG,PARMRTAD(,R5)                               0359 00968000
*   /*****************************************************************/ 00969000
*   /*                                                               */ 00970000
*   /* COMPARE ECB ADDRESSES TO DETERMINE IF COMM TASK ECB WAS BEING */ 00971000
*   /* POSTED. IF SO, A LOCAL POST WILL BE                   @ZA05368*/ 00972000
*   /*                                                               */ 00973000
*   /*****************************************************************/ 00974000
*                                                                  0360 00975000
*/*         GENERATED TO GIVE THE COMMTASK CONTROL. IF A     @ZA05368*/ 00976000
*/*         COMMTASK ECB WAS NOT BEING POSTED, AN A23 ABEND  @ZA05368*/ 00977000
*/*         WILL BE ISSUED.                                  @ZA05368*/ 00978000
*                                                                  0360 00979000
*   IF R0=ADDR(UCMOECB)|R0=ADDR(UCMDECB) THEN/* COMM TASK ECB        */ 00980000
         L     @08,CVTPTR                                          0360 00981000
         L     @08,UCMBASPT(,@08)                                  0360 00982000
