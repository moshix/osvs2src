*   R9=ADDR(RECOVEX);               /* SET ADDR OF RECOVERY EXIT     */ 00956000
         LA    R9,RECOVEX                                          0355 00957000
*   GEN REFS(PSACSTK,FRRSCURR,FRRSLAST,PSALSFCC,FRRSENTR,FRRSELEN, 0356 00958000
