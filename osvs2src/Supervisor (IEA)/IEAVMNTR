         LA    REG11,1                                          01S0254 01039000
*   REG5=RCAREA;                    /* SAVE RETURN CODE              */ 01040000
@RF00253 L     REG5,RCAREA                                      01S0255 01041000
*   GENERATE REFS(PSA,PSALITA,FLC,PSACSTK,FRRSCURR,FRRS,PSALSFCC,       01042000
