         TITLE 'IEAVMQR0 - PROCESS MSG TO INACTIVE TERMINAL            *00001000
                        '                                               00002000
IEAVMQR0 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(19)                                             0001 00007000
         DC    C'IEAVMQR0R0300076163'                              0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @10,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@10                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @09,@01                                             0001 00016000
         USING @DATD,@09                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @09,8(,@13)                                         0001 00020000
         LR    @13,@09                                             0001 00021000
         MVC   @PC00001(4),0(@01)                                  0001 00022000
*                                      SAVED BY RESTART FLIH MDC078  */ 00023000
*   QR0PPTR=ADDR(QR0INPUT);         /* ESTABLISH BASE FOR INPUT PARM    00024000
*                                      LIST                          */ 00025000
*                                                                  0087 00026000
         L     QR0PPTR,@PC00001                                    0087 00027000
*   /*****************************************************************/ 00028000
*   /*                                                               */ 00029000
*   /* ESTAE RECOVERY CODE                                           */ 00030000
*   /*                                                               */ 00031000
*   /*****************************************************************/ 00032000
*                                                                  0088 00033000
*   RCVPARM=RCVPARM&&RCVPARM;       /* CLEAR ESTAE PARM LIST         */ 00034000
         XC    RCVPARM(24),RCVPARM                                 0088 00035000
*   RESPECIFY                                                      0089 00036000
*    (REG2) RESTRICTED;                                            0089 00037000
*   REG2=UCMFRRAD;                  /* SET ADDRESS OF COMTASK      0090 00038000
*                                      RECOVERY ROUTINE.             */ 00039000
*                                                                  0090 00040000
         L     @12,UCMBASPT(,QR0PPTR)                              0090 00041000
         L     REG2,UCMFRRAD(,@12)                                 0090 00042000
*   /*****************************************************************/ 00043000
*   /*                                                               */ 00044000
*   /* ISSUE EXECUTE FORM OF ESTAE - SEE ABOVE FOR MF=L ESTAE        */ 00045000
*   /*                                                               */ 00046000
*   /*****************************************************************/ 00047000
*                                                                  0091 00048000
*   ESTAEPRM=ESTAELST;              /* COPY ESTAE LIST TO AUTO     0091 00049000
*                                      STORAGE                       */ 00050000
         MVC   ESTAEPRM(16),ESTAELST                               0091 00051000
*   GEN(ESTAE (REG2),CT,PARAM=RCVPARM,RECORD=YES,MF=(E,ESTAEPRM)); 0092 00052000
         ESTAE (REG2),CT,PARAM=RCVPARM,RECORD=YES,MF=(E,ESTAEPRM)       00053000
*   PARMPTR=ADDR(RCVPARM);          /* BASE FOR ESTAE PARM'S         */ 00054000
         LA    PARMPTR,RCVPARM                                     0093 00055000
*   PARMID=MQR0;                    /* IDENTIFY MODULE TO FRR        */ 00056000
         MVC   PARMID(4,PARMPTR),@CC00160                          0094 00057000
*   PARMRGAD=ADDR(RCVRSAVE);        /* RECOVERY REG SAVE AREA        */ 00058000
         LA    @12,RCVRSAVE                                        0095 00059000
         ST    @12,PARMRGAD(,PARMPTR)                              0095 00060000
*   PARMRTAD=ADDR(ESTRETRY);        /* RETRY ADDRESS IN IEAVMQR0     */ 00061000
         LA    @12,ESTRETRY                                        0096 00062000
         ST    @12,PARMRTAD(,PARMPTR)                              0096 00063000
*   RCVRGMAP=ALLREGS;               /* RESTORE ALL REGS AT RETRY     */ 00064000
         MVC   RCVRGMAP(2),@CB00162                                0097 00065000
*   GENERATE SETS(RCVREGS);                                        0098 00066000
         STM  REG0,REG15,RCVREGS       SAVE REGS FOR RETRY              00067000
*   RESPECIFY                                                      0099 00068000
*    (REG2) UNRESTRICTED;                                          0099 00069000
*                                                                  0099 00070000
*   /*****************************************************************/ 00071000
*   /*                                                               */ 00072000
*   /* START MODULE INITIALIZATION AND MAINLINE PROCESSING           */ 00073000
*   /*                                                               */ 00074000
*   /*****************************************************************/ 00075000
*                                                                  0100 00076000
*   MSGECB=ZEROES;                  /* CLEAR WTOR ECB                */ 00077000
         SLR   @12,@12                                             0100 00078000
         ST    @12,MSGECB                                          0100 00079000
*   WPL=WPL&&WPL;                   /* CLEAR WTOR PARAMETER LIST     */ 00080000
         XC    WPL(84),WPL                                         0101 00081000
*   QR0FLAGS=QR0FLAGS&&QR0FLAGS;    /* CLEAR WORK FLAGS              */ 00082000
         XC    QR0FLAGS(1),QR0FLAGS                                0102 00083000
*   CALL OBTLOCKS;                  /* GET LOCAL AND CMS LOCKS       */ 00084000
         BAL   @14,OBTLOCKS                                        0103 00085000
*   CALL VERIFY;                    /* VERIFY CONSOLE - WQE    STATUS*/ 00086000
         BAL   @14,VERIFY                                          0104 00087000
*   IF QR0VERFY=ZERO THEN           /* IS WTO STILL PENDING          */ 00088000
         TM    QR0VERFY,B'10000000'                                0105 00089000
         BNZ   @RF00105                                            0105 00090000
*     DO;                           /* YES                           */ 00091000
*       IF WQEPURGE=ONE|(WQEDOM=ONE&UCMDISPC=ONE) THEN             0107 00092000
         L     @12,WQEPTR(,QR0PPTR)                                0107 00093000
         TM    WQEPURGE(@12),B'10000000'                           0107 00094000
         BO    @RT00107                                            0107 00095000
         TM    WQEDOM(@12),B'00000100'                             0107 00096000
         BNO   @RF00107                                            0107 00097000
         L     @12,UCMDVEPT(,QR0PPTR)                              0107 00098000
         TM    UCMDISPC(@12),B'00100000'                           0107 00099000
         BNO   @RF00107                                            0107 00100000
@RT00107 DS    0H                                                  0108 00101000
*         DO;                       /* HAS MSG ISSUER TERMINATED OR     00102000
*                                      DOM'D THE MESSAGE TO A      0108 00103000
*                                      GRAPHICS DEVICE ?             */ 00104000
*           CALL DELEPROC;          /* YES- DELETE CQE ENTRY         */ 00105000
         BAL   @14,DELEPROC                                        0109 00106000
*         END;                                                     0110 00107000
*       ELSE                        /* NO- CHECK TYPE OF CQE,        */ 00108000
*         DO;                       /* I.E., MAJ, OR NORMAL          */ 00109000
         B     @RC00107                                            0111 00110000
@RF00107 DS    0H                                                  0112 00111000
*           IF CQEMAJOR=ZERO THEN   /* IS THIS NORMAL CQE            */ 00112000
         TM    CQEMAJOR(CQEPTR),B'00000100'                        0112 00113000
         BNZ   @RF00112                                            0112 00114000
*             QR0MAJOR=ZERO;        /* YES- INDICATE WTOR MSG IEA962A   00115000
*                                      SHOULD INCLUDE THE 'SEND,   0113 00116000
*                                      DELETE OR OK' OPTION          */ 00117000
         NI    QR0MAJOR,B'10111111'                                0113 00118000
*           ELSE                    /* NO- THIS IS ML MSG            */ 00119000
*             QR0MAJOR=ONE;         /* INDICATE WTOR WITHOUT SEND  0114 00120000
*                                      SHOULD BE ISSUED              */ 00121000
         B     @RC00112                                            0114 00122000
@RF00112 OI    QR0MAJOR,B'01000000'                                0114 00123000
*           DO WHILE QR0RETRY=ZERO; /* LOOP TILL MSG SUCCESSFL       */ 00124000
@RC00112 B     @DE00115                                            0115 00125000
@DL00115 DS    0H                                                  0116 00126000
*             QR0RETRY=ONE;         /* IF NO ERRORS SHOULD BE ONE  0116 00127000
*                                      PASS                          */ 00128000
         OI    QR0RETRY,B'00100000'                                0116 00129000
*             CALL FRLOCKS;         /* FREE CMS AND LOCAL LOCKS      */ 00130000
         BAL   @14,FRLOCKS                                         0117 00131000
*             CALL WTORPROC;        /* ISSUE MSG IEA962A (QR0ERMSG 0118 00132000
*                                      =0) OR IEA963A (QR0ERMSG=1) TO   00133000
*                                      MASTER CONSOLE AND WAIT FOR 0118 00134000
*                                      REPLY                         */ 00135000
         BAL   @14,WTORPROC                                        0118 00136000
*             CALL OBTLOCKS;        /* GET LOCAL AND CMS LOCKS       */ 00137000
         BAL   @14,OBTLOCKS                                        0119 00138000
*             CALL VERIFY;          /* VERIFY CONSOLE-WQE STATUS     */ 00139000
         BAL   @14,VERIFY                                          0120 00140000
*             IF QR0VERFY=ZERO THEN /* WTO STILL PENDING TO INACTIVE    00141000
*                                      TERMINAL?                     */ 00142000
         TM    QR0VERFY,B'10000000'                                0121 00143000
         BNZ   @RF00121                                            0121 00144000
*               DO;                 /* YES- HAS MSG ISSUER           */ 00145000
*                 IF WQEPURGE=ONE|  /* TERMINATED OR                 */ 00146000
*                     (WQEDOM=ONE&UCMDISPC=ONE) THEN               0123 00147000
         L     @12,WQEPTR(,QR0PPTR)                                0123 00148000
         TM    WQEPURGE(@12),B'10000000'                           0123 00149000
         BO    @RT00123                                            0123 00150000
         TM    WQEDOM(@12),B'00000100'                             0123 00151000
         BNO   @RF00123                                            0123 00152000
         L     @12,UCMDVEPT(,QR0PPTR)                              0123 00153000
         TM    UCMDISPC(@12),B'00100000'                           0123 00154000
         BNO   @RF00123                                            0123 00155000
@RT00123 DS    0H                                                  0124 00156000
*                   DO;             /* DOM'D THE MSG -YES            */ 00157000
*                     CALL DELEPROC;/* DELETE CQE ENTRY              */ 00158000
         BAL   @14,DELEPROC                                        0125 00159000
*                   END;                                           0126 00160000
*                 ELSE              /* NO- DETERMINE IF NORMAL       */ 00161000
*                   DO;             /* CQE AND REPLY WAS 'SEND'      */ 00162000
         B     @RC00123                                            0127 00163000
@RF00123 DS    0H                                                  0128 00164000
*                     IF CQEMAJOR=ZERO&REPLYBUF=SEND THEN          0128 00165000
         TM    CQEMAJOR(CQEPTR),B'00000100'                        0128 00166000
         BNZ   @RF00128                                            0128 00167000
         CLC   REPLYBUF(6),SEND                                    0128 00168000
         BNE   @RF00128                                            0128 00169000
*                       DO;         /* YES- SEND MSG TO MSTR CNSLE   */ 00170000
*/********************************************************************/ 00171000
*/*                                                                  */ 00172000
*/* SENDPROC SEGMENT                                                 */ 00173000
*/*                                                                  */ 00174000
*/* FUNCTION: CALL ROUTINES TO CHAIN WQE TO MASTER CONSOLE.          */ 00175000
*/*                                                                  */ 00176000
*/* INPUT:    POINTER TO UCM PREFIX, AND UCME.                       */ 00177000
*/*                                                                  */ 00178000
*/* OUTPUT:   WQEQDFHC - QUEUE FOR HC                                */ 00179000
*/*                                                                  */ 00180000
*/********************************************************************/ 00181000
*                                                                  0130 00182000
*                         IF UCMMCENT->UCMDISPB=ONE THEN/* IS MASTER    00183000
*                                      CONSLE ALSO HC                */ 00184000
         L     @12,UCMBASPT(,QR0PPTR)                              0130 00185000
         AL    @12,@CF01235                                        0130 00186000
         L     @12,UCMMCSPT(,@12)                                  0130 00187000
         L     @12,UCMMCENT(,@12)                                  0130 00188000
         TM    UCMDISPB(@12),B'01000000'                           0130 00189000
         BNO   @RF00130                                            0130 00190000
*                           WQEQDFHC=ONE;/* YES- INDICATE QUEUE FOR     00191000
*                                      HC                            */ 00192000
         L     @12,WQEPTR(,QR0PPTR)                                0131 00193000
         OI    WQEQDFHC(@12),B'00010000'                           0131 00194000
*                         ELSE                                     0132 00195000
*                           ;       /* NO                            */ 00196000
@RF00130 DS    0H                                                  0133 00197000
*                         CALL ENQWQE;/* ENQUEUE WQE TO MASTER CNSLE */ 00198000
         BAL   @14,ENQWQE                                          0133 00199000
*                         CALL DELEPROC;/* DELTE CQE FRM INACTVE   0134 00200000
*                                      CNSLE                         */ 00201000
         BAL   @14,DELEPROC                                        0134 00202000
*                       END;                                       0135 00203000
*                     ELSE                                         0136 00204000
*                       IF REPLYBUF=DELETE THEN/* REPLY = DELETE     */ 00205000
         B     @RC00128                                            0136 00206000
@RF00128 CLC   REPLYBUF(6),DELETE                                  0136 00207000
         BNE   @RF00136                                            0136 00208000
*                         CALL DELEPROC;/* YES- DELETE CQE ENTRY     */ 00209000
         BAL   @14,DELEPROC                                        0137 00210000
*                       ELSE        /* NO - OK REPLY                 */ 00211000
*                         IF REPLYBUF^=OK THEN/* IF REPLY WAS NOT  0138 00212000
*                                      SEND-DELETE-OK THEN WAS     0138 00213000
*                                      INCORRECT                     */ 00214000
         B     @RC00136                                            0138 00215000
@RF00136 CLC   REPLYBUF(6),OK                                      0138 00216000
         BE    @RF00138                                            0138 00217000
*                           DO;     /* REPLY ERROR                   */ 00218000
*                             QR0ERMSG=ONE;/* INDICATE ERR MSG     0140 00219000
*                                      REQUEST                       */ 00220000
*                             QR0RETRY=ZERO;/* LOOP ONCE MORE        */ 00221000
         OI    QR0ERMSG,B'00010000'                                0141 00222000
         NI    QR0RETRY,B'11011111'                                0141 00223000
*                           END;                                   0142 00224000
*                         ELSE                                     0143 00225000
*                           ;       /* REPLY =OK. LEAVE AS IS        */ 00226000
@RF00138 DS    0H                                                  0144 00227000
*                   END;            /* END OF NOT 'DELETE' PROCESS   */ 00228000
@RC00136 DS    0H                                                  0144 00229000
@RC00128 DS    0H                                                  0145 00230000
*               END;                /* END VERFY=ZERO DO             */ 00231000
@RC00123 DS    0H                                                  0146 00232000
*           END;                    /* END OF RETRY=ZERO DO          */ 00233000
@RF00121 DS    0H                                                  0146 00234000
@DE00115 TM    QR0RETRY,B'00100000'                                0146 00235000
         BZ    @DL00115                                            0146 00236000
*         END;                      /* END ELSE CHECK TYPE CQE       */ 00237000
*     END;                          /* END FIRST VERFY=ZERO DO       */ 00238000
*                                                                  0148 00239000
@RC00107 DS    0H                                                  0149 00240000
*   /*****************************************************************/ 00241000
*   /*                                                               */ 00242000
*   /* BRANCH ENTER HERE FOR FRR RETRY                               */ 00243000
*   /*                                                               */ 00244000
*   /*****************************************************************/ 00245000
*                                                                  0149 00246000
*FRRRETRY:                                                         0149 00247000
*   CALL FRLOCKS;                   /* FREE CMS-LOCAL LOCKS          */ 00248000
*                                                                  0149 00249000
@RF00105 DS    0H                                                  0149 00250000
FRRRETRY BAL   @14,FRLOCKS                                         0149 00251000
*   /*****************************************************************/ 00252000
*   /*                                                               */ 00253000
*   /* BRANCH ENTER HERE FOR ESTAE RETRY                             */ 00254000
*   /*                                                               */ 00255000
*   /*****************************************************************/ 00256000
*                                                                  0150 00257000
*ESTRETRY:                                                         0150 00258000
*   RESPECIFY                                                      0150 00259000
*    (REG0,                                                        0150 00260000
*     REG1) RESTRICTED;                                            0150 00261000
ESTRETRY DS    0H                                                  0151 00262000
*   REG0=QR0SPLEN;                  /* SET SP AND LENGTH FOR FRMN    */ 00263000
         L     REG0,QR0SPLEN                                       0151 00264000
*   REG1=QR0PPTR-FOUR;              /* SET ADDRESS TO BE FREED 4   0152 00265000
*                                      BYTES PRECEDING QR0PARMS IS 0152 00266000
*                                      QR0PARMS INPUT POINTER        */ 00267000
*                                                                  0152 00268000
         LR    REG1,QR0PPTR                                        0152 00269000
         SL    REG1,@CF00059                                       0152 00270000
*   /*****************************************************************/ 00271000
*   /*                                                               */ 00272000
*   /* FREEMAIN INPUT PARMLIST (QR0PARMS) AND POINTER                */ 00273000
*   /*                                                               */ 00274000
*   /*****************************************************************/ 00275000
*                                                                  0153 00276000
*   GENERATE REFS(REG0,REG1) SETS(REG1);                           0153 00277000
           FREEMAIN R,LV=(0),A=(REG1)                                   00278000
*   RESPECIFY                                                      0154 00279000
*    (REG0,                                                        0154 00280000
*     REG1) UNRESTRICTED;                                          0154 00281000
*   RETURN;                         /* RETURN TO CALLER              */ 00282000
@EL00001 L     @13,4(,@13)                                         0155 00283000
@EF00001 L     @00,@SIZDATD                                        0155 00284000
         LR    @01,@09                                             0155 00285000
         FREEMAIN R,LV=(0),A=(1)                                        00286000
@ER00001 LM    @14,@12,12(@13)                                     0155 00287000
         BR    @14                                                 0155 00288000
*/********************************************************************/ 00289000
*/*                                                                  */ 00290000
*/* OBTLOCKS SEGMENT                                                 */ 00291000
*/*                                                                  */ 00292000
*/* FUNCTION: THIS SEGMENT WILL OBTAIN THE LOCAL AND CMS LOCKS.      */ 00293000
*/*           THE SETLOCK ROUTINE DESTROYS REGISTERS 11 TO 14. REG 13*/ 00294000
*/*           WILL BE SAVED AND RESTORED. REGS 11 AND 12 SHOULD NOT  */ 00295000
*/*           BE USED AS GLOBAL POINTERS.                            */ 00296000
*/*           OBTLOCKS WILL ALSO SET AN FRR AND INITIALIZE THE       */ 00297000
*/*           USER PARAMETER LIST.                                   */ 00298000
*/*                                                                  */ 00299000
*/* INPUT:    NONE                                                   */ 00300000
*/*                                                                  */ 00301000
*/* OUTPUT:   NONE                                                   */ 00302000
*/*                                                                  */ 00303000
*/********************************************************************/ 00304000
*                                                                  0156 00305000
*OBTLOCKS:                                                         0156 00306000
*   PROC OPTIONS(NOSAVEAREA);                                      0156 00307000
OBTLOCKS STM   @14,@12,12(@13)                                     0156 00308000
*   RFY                                                            0157 00309000
*    (REG13) RSTD;                                                 0157 00310000
*   SAVER13=REG13;                  /* SAVE PTR TO SAVE AREA         */ 00311000
         LR    SAVER13,REG13                                       0158 00312000
*   GENERATE REFS(PSALITA,FLC) SETS(REG11,REG12,REG13,REG14);      0159 00313000
           SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                      X00314000
               RELATED=(CQE,IEAVMQR0(FRLOCKS))                          00315000
           SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,                        X00316000
               RELATED=(CQE,IEAVMQR0(FRLOCKS))                          00317000
*   REG13=SAVER13;                  /* RESTORE SAVEAREA PTR          */ 00318000
         LR    REG13,SAVER13                                       0160 00319000
*   GEN(   LM REG14,REG12,12(REG13));/* RESTORE REG'S                */ 00320000
*                                                                  0161 00321000
            LM REG14,REG12,12(REG13)                                    00322000
*   /*****************************************************************/ 00323000
*   /*                                                               */ 00324000
*   /* SETTUP FOR FRR                                                */ 00325000
*   /*                                                               */ 00326000
*   /*****************************************************************/ 00327000
*                                                                  0162 00328000
*   RESPECIFY                                                      0162 00329000
*    (REG1) RESTRICTED;                                            0162 00330000
*   REG1=UCMFRRAD;                  /* COMTASK RECOVERY ADDRESS      */ 00331000
*                                                                  0163 00332000
         L     @12,UCMBASPT(,QR0PPTR)                              0163 00333000
         L     REG1,UCMFRRAD(,@12)                                 0163 00334000
*   /*****************************************************************/ 00335000
*   /*                                                               */ 00336000
*   /* ISSUE SETFRR                                                  */ 00337000
*   /*                                                               */ 00338000
*   /*****************************************************************/ 00339000
*                                                                  0164 00340000
*   GEN REFS(PSACSTK,FRRSCURR,FRRSLAST,PSALSFCC,FRRSENTR,FRRSELEN, 0164 00341000
*       FRRSFRRA,FRRSPARM) SETS(REG11,REG12);                      0164 00342000
           SETFRR A,FRRAD=(REG1),PARMAD=PARMFRR,WRKREGS=(REG11,REG12)   00343000
*   PARMFRR->PARMRGAD=ADDR(RCVRSAVE);/* ADDR OF REG SAVEAREA         */ 00344000
         LA    @12,RCVRSAVE                                        0165 00345000
         L     @11,PARMFRR                                         0165 00346000
         ST    @12,PARMRGAD(,@11)                                  0165 00347000
*   PARMFRR->PARMID=MQR0;           /* IDENTIFY MODULE TO FRR        */ 00348000
         L     @12,PARMFRR                                         0166 00349000
         MVC   PARMID(4,@12),@CC00160                              0166 00350000
*   PARMFRR->PARMRTAD=ADDR(FRRRETRY);/* RETRY ADDR IN IEAVMQR0       */ 00351000
         LA    @12,FRRRETRY                                        0167 00352000
         L     @11,PARMFRR                                         0167 00353000
         ST    @12,PARMRTAD(,@11)                                  0167 00354000
*   PARMFRID=ONE;                   /* INDICATE BOTH ESTAE AND FRR 0168 00355000
*                                      PROTECTION IN ESTAE USER    0168 00356000
*                                      PARMLIST.                     */ 00357000
         OI    PARMFRID(PARMPTR),B'00010000'                       0168 00358000
*   RFY                                                            0169 00359000
*    (REG13,                                                       0169 00360000
*     REG1) UNRESTRICTED;                                          0169 00361000
*   END OBTLOCKS;                                                  0170 00362000
*                                                                  0170 00363000
@EL00002 DS    0H                                                  0170 00364000
@EF00002 DS    0H                                                  0170 00365000
@ER00002 LM    @14,@12,12(@13)                                     0170 00366000
         BR    @14                                                 0170 00367000
*/********************************************************************/ 00368000
*/*                                                                  */ 00369000
*/* VERIFY SEGMENT                                                   */ 00370000
*/*                                                                  */ 00371000
*/* FUNCTION: VERIFY 'INACTIVE' CONSOLE STATUS, I.E., DETERMINE IF   */ 00372000
*/*          ITS STILL INACTIVE, OR IF THERE IS AN OUTPUT QUEUE OR   */ 00373000
*/*          INPUT WQE NOT IN CQE.                                   */ 00374000
*/*                                                                  */ 00375000
*/* INPUT:   PTR TO UCME OF INACTIVE CONSOLE.                        */ 00376000
*/*                                                                  */ 00377000
*/* OUTPUT:  QR0VERFY=0  SUCCESSFUL VERIFY                           */ 00378000
*/*          QR0VERFY=1  UNSUCCESSFUL VERIFY                         */ 00379000
*/*                                                                  */ 00380000
*/********************************************************************/ 00381000
*                                                                  0171 00382000
*VERIFY:                                                           0171 00383000
*   PROC OPTIONS(NOSAVEAREA);                                      0171 00384000
VERIFY   STM   @14,@01,12(@13)                                     0171 00385000
         STM   @04,@12,36(@13)                                     0171 00386000
*   QR0VERFY=ONE;                   /* INDICATES CONSOLE ACTIVATED OR   00387000
*                                      NO OUTPUT Q OR INPUT WQE NOT     00388000
*                                      IN CQE                        */ 00389000
         OI    QR0VERFY,B'10000000'                                0172 00390000
*   IF UCMUF=ONE|UCMOUTQ=ZEROES THEN/* IF CONSOLE HAS BEEN ACTIVATED    00391000
*                                      OR NO OUTPUT QUEUE THEN SET 0173 00392000
*                                      INDICATOR.                    */ 00393000
         L     @12,UCMDVEPT(,QR0PPTR)                              0173 00394000
         TM    UCMUF(@12),B'00010000'                              0173 00395000
         BO    @RT00173                                            0173 00396000
         L     @12,UCMOUTQ(,@12)                                   0173 00397000
         LTR   @12,@12                                             0173 00398000
         BNZ   @RF00173                                            0173 00399000
@RT00173 DS    0H                                                  0174 00400000
*     QR0END=ONE;                   /* BYPASS LOOP AND RETURN        */ 00401000
         OI    QR0END,B'00001000'                                  0174 00402000
*   ELSE                                                           0175 00403000
*     ;                             /* VERIFY ENTRY                  */ 00404000
@RF00173 DS    0H                                                  0176 00405000
*   CQEPTR=UCMOUTQ;                 /* EST. POINTER TO CQE           */ 00406000
         L     @12,UCMDVEPT(,QR0PPTR)                              0176 00407000
         L     CQEPTR,UCMOUTQ(,@12)                                0176 00408000
*   DO WHILE QR0END=ZERO;           /* LOOP THRU CQE CHAIN           */ 00409000
         B     @DE00177                                            0177 00410000
@DL00177 DS    0H                                                  0178 00411000
*     IF CQEWQEA=QR0WQEA&CQEENTR=ONE THEN/* DOES VALID               */ 00412000
         CLC   CQEWQEA(3,CQEPTR),QR0WQEA(QR0PPTR)                  0178 00413000
         BNE   @RF00178                                            0178 00414000
         TM    CQEENTR(CQEPTR),B'00000001'                         0178 00415000
         BNO   @RF00178                                            0178 00416000
*       DO;                         /* CQE ENTRY = PARMLIST WQE      */ 00417000
