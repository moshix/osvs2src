*   /*****************************************************************/ 02296000
*   /*                                                               */ 02297000
*   /* SET FINAL FIELDS IN PVT NECESSARY TO GET PAGING GOING AND     */ 02298000
*   /* RELEASE THE LOCK.                                             */ 02299000
*   /*                                                               */ 02300000
*   /*****************************************************************/ 02301000
*                                                                  0911 02302000
*   PVTPOOL=NDYNAMCT;               /* DYNAMIC PAGE(ONLINE) COUNT    */ 02303000
