         MR    @00,@11                                             0640 01703000
         DR    @00,@12                                             0640 01704000
         STH   @01,PVTFIXOK(,RPVT)                                 0640 01705000
*   /*****************************************************************/ 01706000
*   /*                                                               */ 01707000
*   /* SET UP TOTAL FRAME COUNT AND AVAILABLE COUNT FOR CALL TO      */ 01708000
*   /* SYSEVENT                                                      */ 01709000
*   /*                                                               */ 01710000
*   /*****************************************************************/ 01711000
*                                                                  0641 01712000
*   POOLCNT=PVTPOOL;                /* SET UP TOTAL FRAME COUNT      */ 01713000
