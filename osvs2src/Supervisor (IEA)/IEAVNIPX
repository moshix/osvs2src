*     RPQE,                                                        0551 01170000
*     RPARM) UNRESTRICTED;                                         0551 01171000
*                                                                  0552 01172000
*/************************************************************       */ 01173000
*/* THIS ROUTINE ADDS PFTES WHICH ARE FOR VIRTUAL EQUAL REAL         */ 01174000
*/* CORE TO THE AVAILABLE PAGE FRAME QUEUE.  THE ROUTINE             */ 01175000
*/* STARTS WITH THE LAST PFTE AND WORKS DOWN TO THE NUCLEUS.         */ 01176000
*/************************************************************       */ 01177000
*                                                                  0552 01178000
*NPXPFTAQ:                                                         0552 01179000
*   ENTRY;                          /* INTERNAL ROUTINE              */ 01180000
NPXPFTAQ STM   @14,@12,@SA00002                                    0552 01181000
*   RESPECIFY                                                      0553 01182000
*    (RNVT,                                                        0553 01183000
*     RCVT,                                                        0553 01184000
*     RPVT,                                                        0553 01185000
*     RINEXT,                                                      0553 01186000
*     PTEPTR) RESTRICTED;                                          0553 01187000
*   RPVT=CVTPVTP;                   /* PAGE VECTOR TABLE BASE        */ 01188000
         L     RPVT,CVTPVTP(,RCVT)                                 0554 01189000
*   RINEXT=(((NVTNUCND+4095)|4095)&&4095);/* ROUND UP TOP OF NUCLEUS    01190000
*                                      TO PAGE BOUNDARY              */ 01191000
*                                                                  0555 01192000
         LA    @11,4095                                            0555 01193000
         L     @15,NVTNUCND(,RNVT)                                 0555 01194000
         LR    RINEXT,@15                                          0555 01195000
         ALR   RINEXT,@11                                          0555 01196000
         OR    RINEXT,@11                                          0555 01197000
         XR    RINEXT,@11                                          0555 01198000
*   /*****************************************************************/ 01199000
*   /*                                                               */ 01200000
*   /* DETERMINE AMOUNT OF FREE STORAGE ABOVE TOP OF NUC TO END OF   */ 01201000
*   /* PAGE                                                          */ 01202000
*   /*                                                               */ 01203000
*   /*****************************************************************/ 01204000
*                                                                  0556 01205000
*   FREESTOR=NVTNUCND-RINEXT;       /* AMOUNT OF FREE STORAGE      0556 01206000
*                                                            @Z30ENXA*/ 01207000
         LR    FREESTOR,@15                                        0556 01208000
         SLR   FREESTOR,RINEXT                                     0556 01209000
*   NUCTOP=RINEXT;                  /* TOP OF NUCLEUS AFTER ROUND  0557 01210000
*                                                            @Z30ENXA*/ 01211000
*                                                                  0557 01212000
         LR    NUCTOP,RINEXT                                       0557 01213000
*   /*****************************************************************/ 01214000
*   /*                                                               */ 01215000
*   /* FORMAT FREE STORAGE WITH SCB'S FOR MASTER MEMORY              */ 01216000
*   /*                                                               */ 01217000
*   /*****************************************************************/ 01218000
*                                                                  0558 01219000
*   SCBPTR=NVTNUCND;                /* FIRST AVAILABLE STORAGE LOC 0558 01220000
*                                                            @Z30ENXA*/ 01221000
         LR    SCBPTR,@15                                          0558 01222000
*   DO WHILE((SCBPTR+LENGTH(SCB))<NUCTOP);/* LOOP TO END     @Z30ENXA*/ 01223000
         B     @DE00559                                            0559 01224000
@DL00559 DS    0H                                                  0560 01225000
