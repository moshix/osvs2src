         LTR   @12,@12                                             0683 01919000
         BZ    @RT00683                                            0683 01920000
*     GO TO NPXRDPGX;               /* INVALID GO GET NEXT PAGE      */ 01921000
*   PTEPTR=ADDRPGT;                 /* BASE FOR IST PGTE             */ 01922000
         LR    PTEPTR,@12                                          0685 01923000
*NPXRDPGL:                          /* SCAN ALL PAGE TABLE ENTRIES 0686 01924000
*                                      FOR THIS SEGMENT              */ 01925000
*                                                                  0686 01926000
*   /*****************************************************************/ 01927000
*   /*                                                               */ 01928000
*   /* IF PAGE IS ASSIGNED OR VALID THE SEGMENT IS VALID             */ 01929000
*   /*                                                               */ 01930000
*   /*****************************************************************/ 01931000
*                                                                  0686 01932000
*   IF PGTPAM=PGTASN|PGTPVM=PGTVA THEN/* SEGMENT IS VALID            */ 01933000
