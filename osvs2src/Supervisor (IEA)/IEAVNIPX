*DATOFF:                                                           0704 02019000
*                                                                  0704 02020000
*   /*****************************************************************/ 02021000
*   /*                                                               */ 02022000
*   /* DISABLE ADDRESS TRANSLATION                                   */ 02023000
*   /*                                                               */ 02024000
*   /*****************************************************************/ 02025000
*                                                                  0704 02026000
*   GENERATE REFS(NPXSMASK) SETS(NPXSMASK)(STNSM  NPXSMASK,B'11111011') 02027000
*       ;                                                          0704 02028000
DATOFF   STNSM  NPXSMASK,B'11111011'                                    02029000
*   RESPECIFY                                                      0705 02030000
*    (R4,                                                          0705 02031000
*     R9) UNRESTRICTED;                                            0705 02032000
*NPXRDXRS:                                                         0706 02033000
*                                                                  0706 02034000
*   /*****************************************************************/ 02035000
*   /*                                                               */ 02036000
*   /* CALCULATE REAL ADDRESS OF SEGMENT TABLE SEGTAB ENTRY EQ       */ 02037000
*   /* VIRTUAL ADDRESS DIVIDED BY 64K TIMES THE LENGTH OF ONE SEGMENT*/ 02038000
*   /* TABLE ENTRY PLUS THE ADDRESS OF THE START OF THE SEGMENT TABLE*/ 02039000
*   /*                                                               */ 02040000
*   /*****************************************************************/ 02041000
*                                                                  0706 02042000
*   STEPTR=(LENGTH(SGTSTE)*VIRTA1)+SYSTSEGP;                       0706 02043000
NPXRDXRS SLR   @12,@12                                             0706 02044000
         IC    @12,VIRTA1                                          0706 02045000
         SLA   @12,2                                               0706 02046000
         AL    @12,SYSTSEGP                                        0706 02047000
         ST    @12,STEPTR                                          0706 02048000
*   RPGT=SGTSTE&MASKIPGA;           /* ADDR OF PAGE TABLE            */ 02049000
         L     RPGT,SGTSTE(,@12)                                   0707 02050000
         N     RPGT,MASKIPGA                                       0707 02051000
*NPXRDPGZ:                          /* LOOP THRU PGTE'S              */ 02052000
*   PTEPTR=((VIRTA2/NPG)*LENGTH(PGTPTE))+RPGT;/* PGTE POINTER        */ 02053000
NPXRDPGZ SLR   PTEPTR,PTEPTR                                       0708 02054000
         IC    PTEPTR,VIRTA2                                       0708 02055000
         SRL   PTEPTR,4                                            0708 02056000
         ALR   PTEPTR,PTEPTR                                       0708 02057000
         ALR   PTEPTR,RPGT                                         0708 02058000
*   PGTPAM=PGTNASN;                 /* PAGE NOT ASSIGNED             */ 02059000
*   PGTPVM=PGTNVA;                  /* SET PAGE NOT VALID            */ 02060000
         OI    PGTPVM(PTEPTR),B'00001000'                          0710 02061000
         NI    PGTPAM(PTEPTR),B'11111110'                          0710 02062000
*   VIRTADDR=VIRTADDR+PAGEL;        /* ADDR. OF START OF NEXT PAGE   */ 02063000
*                                                                  0711 02064000
         SLR   @12,@12                                             0711 02065000
         ICM   @12,3,PAGEL                                         0711 02066000
         AL    @12,VIRTADDR                                        0711 02067000
         ST    @12,VIRTADDR                                        0711 02068000
*   /*****************************************************************/ 02069000
*   /*                                                               */ 02070000
*   /* LOOP TILL LAST RESTRICTED ADDRESS PASSED OR A SEGMENT BOUNDARY*/ 02071000
*   /* IS REACHED                                                    */ 02072000
*   /*                                                               */ 02073000
*   /*****************************************************************/ 02074000
*                                                                  0712 02075000
*   IF VIRTADDR^>RESHI&VIRTA2^=B64K THEN/* STILL RESTRICTED          */ 02076000
         C     @12,RESHI                                           0712 02077000
         BH    @RF00712                                            0712 02078000
         CLI   VIRTA2,0                                            0712 02079000
         BNE   @RT00712                                            0712 02080000
*     GO TO NPXRDPGZ;               /* CONTINUE LOOP                 */ 02081000
*   PGCTR=NEWSEG;                   /* USE PAGE COUNTER AS AN INDEX  */ 02082000
*                                                                  0714 02083000
@RF00712 SLR   @12,@12                                             0714 02084000
         ST    @12,PGCTR                                           0714 02085000
*   /*****************************************************************/ 02086000
*   /*                                                               */ 02087000
*   /* CALCULATE ADDRESS OF SEGMENT START                            */ 02088000
*   /*                                                               */ 02089000
*   /*****************************************************************/ 02090000
*                                                                  0715 02091000
*   RESLOW=VIRTADDR&MASKISEG;                                      0715 02092000
         L     @12,VIRTADDR                                        0715 02093000
         N     @12,MASKISEG                                        0715 02094000
         ST    @12,RESLOW                                          0715 02095000
*NPXRDCKP:                          /* CHECK SEGMENT                 */ 02096000
*   PTEPTR=(LENGTH(PGTPTE)*PGCTR)+RPGT;/* PGTE BASE                  */ 02097000
*                                                                  0716 02098000
NPXRDCKP L     @12,PGCTR                                           0716 02099000
         LR    PTEPTR,@12                                          0716 02100000
         ALR   PTEPTR,PTEPTR                                       0716 02101000
         ALR   PTEPTR,RPGT                                         0716 02102000
*   /*****************************************************************/ 02103000
*   /*                                                               */ 02104000
*   /* IF PAGE IS ASSIGNED OR VALID THE SEGMENT IS VALID             */ 02105000
*   /*                                                               */ 02106000
*   /*****************************************************************/ 02107000
*                                                                  0717 02108000
*   IF PGTPAM=PGTASN|PGTPVM=PGTVA THEN/* SEGMENT IS VALID            */ 02109000
         TM    PGTPAM(PTEPTR),B'00000001'                          0717 02110000
         BNZ   @RT00717                                            0717 02111000
         TM    PGTPVM(PTEPTR),B'00001000'                          0717 02112000
         BNO   @RT00717                                            0717 02113000
*     GO TO NPXRDCL;                /* DO NEXT SEGMENT               */ 02114000
*                                                                  0718 02115000
*   /*****************************************************************/ 02116000
*   /*                                                               */ 02117000
*   /* IF ALL PAGES ARE INVALID AND UNASSIGNED SET SEGMENT IS        */ 02118000
*   /* UNAVAILABLE                                                   */ 02119000
*   /*                                                               */ 02120000
*   /*****************************************************************/ 02121000
*                                                                  0719 02122000
*   IF PGCTR=LSTPG THEN             /* ALL PAGES INVALID             */ 02123000
         CH    @12,@CH00089                                        0719 02124000
         BNE   @RF00719                                            0719 02125000
*     DO;                                                          0720 02126000
*       SGTPAM=SGTNASN;             /* SEGMENT UNAVAILABLE           */ 02127000
         L     @12,STEPTR                                          0721 02128000
         OI    SGTPAM(@12),B'00000001'                             0721 02129000
*       GO TO NPXRDCL;              /* CHECK FOR END OF OF RESTRICTED   02130000
*                                      ADDR                          */ 02131000
         B     NPXRDCL                                             0722 02132000
*     END;                                                         0723 02133000
*   ELSE                                                           0724 02134000
*     DO;                           /* CHECK NEXT SEGMENT            */ 02135000
@RF00719 DS    0H                                                  0725 02136000
*       PGCTR=PGCTR+NXTPG;          /* ADDR NEXT SEGMENT             */ 02137000
         LA    @12,1                                               0725 02138000
         AL    @12,PGCTR                                           0725 02139000
         ST    @12,PGCTR                                           0725 02140000
*       GO TO NPXRDCKP;             /* LOOP TO PROCESS               */ 02141000
         B     NPXRDCKP                                            0726 02142000
*     END;                                                         0727 02143000
*                                                                  0727 02144000
*   /*****************************************************************/ 02145000
*   /*                                                               */ 02146000
*   /* CHECK FOR LAST RESTRICTED ADDRESS                             */ 02147000
*   /*                                                               */ 02148000
*   /*****************************************************************/ 02149000
*                                                                  0728 02150000
*NPXRDCL:                                                          0728 02151000
*                                                                  0728 02152000
*   /*****************************************************************/ 02153000
*   /*                                                               */ 02154000
*   /* IF VARIABLE IS GREATER THAN RESHI THEN ALL RESTRICTED         */ 02155000
*   /* ADDRESSES HAVE BEEN PROCESSED                                 */ 02156000
*   /*                                                               */ 02157000
*   /*****************************************************************/ 02158000
*                                                                  0728 02159000
*   IF VIRTADDR<RESHI THEN          /* MORE RESTRICTED               */ 02160000
NPXRDCL  CLC   VIRTADDR(4),RESHI                                   0728 02161000
         BL    @RT00728                                            0728 02162000
*     GO TO NPXRDXRS;               /* CONTINUE LOOP                 */ 02163000
*   RESPECIFY                                                      0730 02164000
*    (RTCB,                                                        0730 02165000
*     RINEXT,                                                      0730 02166000
*     RPGT,                                                        0730 02167000
*     PTEPTR) UNRESTRICTED;                                        0730 02168000
*   RESPECIFY                                                      0731 02169000
*    (RINEXT,                                                      0731 02170000
*     RSCAN,                                                       0731 02171000
*     RCURR) RESTRICTED;                                           0731 02172000
*                                                                  0731 02173000
*   /*****************************************************************/ 02174000
*   /*                                                               */ 02175000
*   /* DETERMINE WHETHER TRACE WAS DYNAMICALLY TURNED OFF    @YM08212*/ 02176000
*   /*                                                               */ 02177000
*   /*****************************************************************/ 02178000
*                                                                  0732 02179000
*   IF CVTTRACE=TRCEDOFF THEN                                      0732 02180000
         CLC   CVTTRACE(2,RCVT),@CB00103                           0732 02181000
         BNE   @RF00732                                            0732 02182000
*     CVTTRACE=CVTTRACE|TRACEON;    /* ACTIVATE TRACE        @YM08212*/ 02183000
*                                                                  0733 02184000
         OC    CVTTRACE(2,RCVT),@CB00101                           0733 02185000
*   /*****************************************************************/ 02186000
*   /*                                                               */ 02187000
*   /* SET UP REGISTERS FOR RETURN VIA LINK SVC PARAMETER REGISTERS  */ 02188000
*   /* ARE FOR LINK TO IEEVIPL                                       */ 02189000
*   /*                                                               */ 02190000
*   /*****************************************************************/ 02191000
*                                                                  0734 02192000
*NPXMODST:                                                         0734 02193000
*   RESPECIFY                                                      0734 02194000
*    (RLINKA) RESTRICTED;                                          0734 02195000
@RF00732 DS    0H                                                  0734 02196000
NPXMODST DS    0H                                                  0735 02197000
*   RESPECIFY                                                      0735 02198000
*    (RENTRY,                                                      0735 02199000
*     REXIT,                                                       0735 02200000
*     R2,                                                          0735 02201000
*     R3,                                                          0735 02202000
*     R5,                                                          0735 02203000
*     R9) RESTRICTED;                                              0735 02204000
*   REXIT=NVTNXPTR;                                                0736 02205000
         L     REXIT,NVTNXPTR(,RNVT)                               0736 02206000
*   R9=LINKSAVE;                    /* ADDR OF LINK PARM LIST        */ 02207000
         L     R9,LINKSAVE                                         0737 02208000
*   RPARM=ZERO;                     /* SPECIFY PTLB                  */ 02209000
         SLR   RPARM,RPARM                                         0738 02210000
*   R2=ASCBRSM;                     /* PROVIDE RSMHEADER ADDRESS     */ 02211000
         L     @12,ASCBPTR                                         0739 02212000
         L     R2,ASCBRSM(,@12)                                    0739 02213000
*   R3=RPVT;                        /* PROVIDE PVT ADDRESS           */ 02214000
         LR    R3,RPVT                                             0740 02215000
*   RENTRY=PVTPINV;                 /* GET ADDR OF PTLB ROUTINE      */ 02216000
*                                                                  0741 02217000
         L     RENTRY,PVTPINV(,RPVT)                               0741 02218000
*   /*****************************************************************/ 02219000
*   /*                                                               */ 02220000
*   /* TURN DAT ON                                                   */ 02221000
*   /*                                                               */ 02222000
*   /*****************************************************************/ 02223000
*                                                                  0742 02224000
*   GENERATE REFS(RMODEBR,RMODEC,R5,R9,PSALITA,NVTNXPTR,RCODE) SETS(    02225000
*       RCODE,RLINKA);                                             0742 02226000
ZEROFF   EQU   0                           OFFSET FOR MOVE              02227000
LINK     EQU   6                           LINK SVC                     02228000
         MVC   ZEROFF(NPXEND-NPXSSM,REXIT),NPXSSM  RELOCATE INSTR       02229000
         USING NPXSSM,REXIT                                             02230000
         B     NPXSSM                      GO EXECUTE RELOCATED INSTR   02231000
NPXSSM   STOSM  NPXMASK,B'00000100'                                     02232000
         BALR  REXIT,RENTRY                GO TO PTLB ROUTINE           02233000
*    RELEASE LOCAL AND SALLOC LOCKS                                     02234000
         SETLOCK RELEASE,TYPE=SALLOC,RELATED=(PFTE,IEAVNIPX,GETLOCK)    02235000
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(PGT,IEAVNIPX,GETL)         02236000
         LR    RCODE,R9                  PICK UP LINK PARM LIST         02237000
*****************************************************************       02238000
*        LINK TO THE MASTER SCHEDULER ROUTINE -- IEEVIPL                02239000
         SVC  LINK                                                      02240000
NPXMASK  DS    H                        TEMP FOR STOSM INSTRUCTION      02241000
         DROP  REXIT                                                    02242000
NPXEND   EQU   *                        END OF NIPX PROCESSING          02243000
*****************************************************************       02244000
***************************************************************/        02245000
*********** GENERATE CONSTANTS AND                            */        02246000
***********  WORK AREAS WHICH MUST FALL IN THE RESTRICTED     */        02247000
*********** ADDRESS RANGE                                     */        02248000
***************************************************************/        02249000
PGCTR    DS    F                       PAGES PER SEGMENT                02250000
ADDRPGTE DS    F                       OUTPUT FROM NPXFPGT              02251000
ADDRPGT  DS    F                       OUTPUT FROM NPXFPGT              02252000
ADDRSEG  DS    F                       OUTPUT FROM NPXFPGT              02253000
USERSEGP DS    F                       ADDRESS OF USER                  02254000
*                                      SEGMENT TABLE                    02255000
SYSTSEGP DS    F                       ADDRESS OF SYSTEM                02256000
*                                      SEGMENT TABLE                    02257000
VIRTADDR DC    F'0'                    INPUT FOR NPXFPGT                02258000
         DS    0F                                                       02259000
MASKOPFN DC    X'00000FFF'             AND OUT PAGE FRAME NO.           02260000
         DS    0F                                                       02261000
MASKIPFN DC    X'00FFF000'             AND IN PAGE FRAME NO.            02262000
         DS    0F                                                       02263000
MASKIPGA DC    X'00FFFFF8'             AND IN PGA ADDRESS               02264000
         DS    0F                                                       02265000
MASKISEG DC    X'00FF0000'             AND IN SEGMENT NUMBER            02266000
LINKSAVE DS    F                       ADDR LINK PARM LIST              02267000
FBQESPTR DS    F                       ADDR OF FBQE                     02268000
PFTEPTR  DS    F                       ADDR OF PFT                      02269000
RFVV     DS    F                       ADDR OF FIRST V=V PAGE           02270000
XTLSTPTR DS    F                       ADDR OF EXTENT LIST              02271000
ASCBPTR  DS    F                       ADDR OF MS ASCB                  02272000
STEPTR   DS    F                       ADDR OF SEG TABLE ENTRY          02273000
REGSIZE  DS    F                       SIZE OF PRIVATE AREA             02274000
REGSTART DS    F                       START OF PRIVATE AREA            02275000
SAVER4   DS    F                       SAVE AREA FOR FREEMAIN           02276000
SUBP255  DC    X'FF000000'             SUBPOOL FOR FREEMAIN             02277000
PGTADR   DS    F                       POINTER TO PAGE TABLE            02278000
MSTRLPID DS    2F                      RESERVED                @Z40WPXA 02279000
SAVER7   DS    F                       SAVE AREA FOR FREEMAIN           02280000
SAVER14  DS    F                       SAVE AREA FOR R 14               02281000
SEGENTRY DC    F'256'                  ENTRIES IN SEG TABLE             02282000
NPXSMASK DS    CL1                     WORKBYTE FOR STNSM      @YM06985 02283000
PTELNGTH DC    H'2'                    LENGTH PAGE TABLE ENTRY @YM06985 02284000
PAGEL    DC    H'4096'                 PAGE SIZE               @YM06985 02285000
****************************************************************        02286000
*THIS LABEL MUST BE FOLLOWED BY ADCONS FOR NPXRDPFT AND NPXFPQE         02287000
         DS    0F                                              @YM06985 02288000
RESTABLE EQU   *                       RESTRICTED ADDRESSES             02289000
         DC    A(NPXRDPGT)             LOW BOUNDARY                     02290000
         DC    A(NPXFPQE)              HIGH BOUNDARY                    02291000
****************************************************************        02292000
*   RESPECIFY                                                      0743 02293000
*    (RPARM,                                                       0743 02294000
*     RNVT,                                                        0743 02295000
*     RCVT,                                                        0743 02296000
*     RTCB,                                                        0743 02297000
*     RPVT,                                                        0743 02298000
*     RINEXT,                                                      0743 02299000
