         FREEMAIN R,LV=LPGTXPT,A=ADDRPGT,SP=SP255,BRANCH=YES            01987000
*   R7=R9;                          /* GET ADDR OF NEXT PAGE BACK    */ 01988000
         LR    R7,R9                                               0698 01989000
*NPXRDPGX:                          /* LOOP CONTROL                  */ 01990000
*   VIRTADDR=RINEXT;                /* PARAM FOR NPXFPGT             */ 01991000
*                                                                  0699 01992000
NPXRDPGX ST    RINEXT,VIRTADDR                                     0699 01993000
*   /*****************************************************************/ 01994000
*   /*                                                               */ 01995000
*   /* END OF LOOP AFTER LAST PAGE                                   */ 01996000
*   /*                                                               */ 01997000
*   /*****************************************************************/ 01998000
*                                                                  0700 01999000
*   IF RINEXT<CVTGDA->CSAPQEP->PQEREGN/*                     @YM04951*/ 02000000
*     THEN                          /* MORE REAL PAGES               */ 02001000
         L     @12,CVTGDA(,RCVT)                                   0700 02002000
         L     @12,CSAPQEP(,@12)                                   0700 02003000
         C     RINEXT,PQEREGN(,@12)                                0700 02004000
