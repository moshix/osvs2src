*   R7=ASCBPTR;                     /* GET ADDR OF ASCB              */ 01981000
         L     R7,ASCBPTR                                          0696 01982000
*   GENERATE REFS(ADDRPGT,SCVTRMBR) SETS(RPARM0,RPARM)             0697 01983000
*       (FREEMAIN R,LV=LPGTXPT,A=ADDRPGT,SP=SP255,BRANCH=YES );/*  0697 01984000
*                                      FREE PGTE AND XPTE (CONTIGUOUS   01985000
