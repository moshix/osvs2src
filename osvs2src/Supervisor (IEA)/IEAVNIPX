*   END NPXOTEST;                   /* END OF INNER ROUTINES         */ 02310000
         B     @EL00002                                            0744 02311000
*/************************************************************       */ 02312000
*/************************************************************       */ 02313000
*/* INPUT   VIRTADDR   A VIRTUAL ADDRESS                             */ 02314000
*/*         RCVT       ADDRESS OF CVT                                */ 02315000
*/*         RPVT       ADDRESS OF PVT                                */ 02316000
*/* OUTPUT                                                           */ 02317000
*/*         OUTPUT VALUES ARE THE VIRTUAL ADDRESSES OF               */ 02318000
*/*         OF CONTROL BLOCKS FOR THE VIRTUAL ADDRESS                */ 02319000
*/*         SPECIFIED BY VIRTADDR                                    */ 02320000
*/*         ADDRSEG          SEGMENT TABLE ENTRY                     */ 02321000
*/*         ADDRPGT          PAGE TABLE                              */ 02322000
*/*         ADDRPGTE         PAGE TABLE ENTRY                        */ 02323000
*/* RETURN CODES                                                     */ 02324000
*/*         ZERO             NOT IN RESTRICTED RANGE                 */ 02325000
*/*         FOUR             IN RESTRICTED RANGE                     */ 02326000
*/*         EIGHT            SEGMENT NOT VALID                       */ 02327000
*/************************************************************       */ 02328000
*                                                                  0745 02329000
*NPXFPGT:                                                          0745 02330000
*   PROC OPTIONS(NOSAVEAREA);                                      0745 02331000
NPXFPGT  STM   @14,@12,12(@13)                                     0745 02332000
*   RESPECIFY                                                      0746 02333000
*    (RCVT,                                                        0746 02334000
*     RPVT,                                                        0746 02335000
*     PTEPTR,                                                      0746 02336000
*     RNVT,                                                        0746 02337000
*     RINEXT,                                                      0746 02338000
*     RPGT) RESTRICTED;                                            0746 02339000
*                                                                  0746 02340000
*   /*****************************************************************/ 02341000
*   /*                                                               */ 02342000
*   /* CALCULATE VIRTUAL ADDRESS OF SEGMENT TABLE SEGTAB ENTRY EQ    */ 02343000
*   /* VIRTUAL ADDRESS DIVIDED BY 64K TIMES THE LENGTH OF ONE SEGMENT*/ 02344000
*   /* TABLE ENTRY PLUS THE ADDRESS OF THE START OF THE SEGMENT TABLE*/ 02345000
*   /*                                                               */ 02346000
*   /*****************************************************************/ 02347000
*                                                                  0747 02348000
*   STEPTR=ASCBRSM->RSMVSTO;        /* GET VIRTUAL ADDRESS OF SEGMENT   02349000
*                                      TABLE                         */ 02350000
         L     @12,ASCBPTR                                         0747 02351000
         L     @12,ASCBRSM(,@12)                                   0747 02352000
         L     @12,RSMVSTO(,@12)                                   0747 02353000
         ST    @12,STEPTR                                          0747 02354000
*   STEPTR=LENGTH(SGTSTE)*VIRTA1+STEPTR;                           0748 02355000
         SLR   @11,@11                                             0748 02356000
         IC    @11,VIRTA1                                          0748 02357000
         SLA   @11,2                                               0748 02358000
         ALR   @11,@12                                             0748 02359000
         ST    @11,STEPTR                                          0748 02360000
*   ADDRSEG=STEPTR;                 /* SAVE ADDRESS                  */ 02361000
*                                                                  0749 02362000
         ST    @11,ADDRSEG                                         0749 02363000
*   /*****************************************************************/ 02364000
*   /*                                                               */ 02365000
*   /* IF THIS SEGMENT IS INVALID GIVE APPROPRIATE RETURN CODE       */ 02366000
*   /*                                                               */ 02367000
*   /*****************************************************************/ 02368000
*                                                                  0750 02369000
*   IF SGTPAM=SGTNASN THEN          /* INVALID SEGMENT               */ 02370000
         TM    SGTPAM(@11),B'00000001'                             0750 02371000
         BNO   @RF00750                                            0750 02372000
*     DO;                                                          0751 02373000
*       ADDRPGT=''B;                                               0752 02374000
         SLR   @12,@12                                             0752 02375000
         ST    @12,ADDRPGT                                         0752 02376000
*       ADDRPGTE=''B;                                              0753 02377000
         ST    @12,ADDRPGTE                                        0753 02378000
*       RETURN CODE(EIGHT);         /* RETURN CODE 8                 */ 02379000
         LA    @15,8                                               0754 02380000
         L     @14,12(,@13)                                        0754 02381000
         LM    @00,@12,20(@13)                                     0754 02382000
         BR    @14                                                 0754 02383000
*     END;                                                         0755 02384000
*                                                                  0755 02385000
*   /*****************************************************************/ 02386000
*   /*                                                               */ 02387000
*   /* CALCULATE VIRTUAL ADDRESS OF PAGE TABLE GET REAL ADDRESS OF   */ 02388000
*   /* THE PAGE TABLE FROM THE SEGMENT TABLE AND REPLACE THE PFN     */ 02389000
*   /* PORTION OF THIS ADDRESS WITH THE VIRTUAL BLOCK NUMBER         */ 02390000
*   /*                                                               */ 02391000
*   /*****************************************************************/ 02392000
*                                                                  0756 02393000
*   ADDRPGT=SGTSTE&MASKIPGA;                                       0756 02394000
*                                                                  0756 02395000
@RF00750 L     @12,STEPTR                                          0756 02396000
         L     @12,SGTSTE(,@12)                                    0756 02397000
         N     @12,MASKIPGA                                        0756 02398000
         ST    @12,ADDRPGT                                         0756 02399000
*   /*****************************************************************/ 02400000
*   /*                                                               */ 02401000
*   /* CALCULATE PFTE INDEX OF PAGE TABLE BEFORE MASKING OUT PFN OF  */ 02402000
*   /* ADDRESS                                                       */ 02403000
*   /*                                                               */ 02404000
*   /*****************************************************************/ 02405000
*                                                                  0757 02406000
*   PFTEPTR=((ADDRPGT/PAGEL)*LENGTH(PFTE))+PVTPFTP;                0757 02407000
         LR    @01,@12                                             0757 02408000
         SLR   @00,@00                                             0757 02409000
         SLR   @11,@11                                             0757 02410000
         ICM   @11,3,PAGEL                                         0757 02411000
         DR    @00,@11                                             0757 02412000
         SLA   @01,4                                               0757 02413000
         AL    @01,PVTPFTP(,RPVT)                                  0757 02414000
         ST    @01,PFTEPTR                                         0757 02415000
*   ADDRPGT=ADDRPGT&MASKOPFN;       /* MASK OUT PFN                  */ 02416000
         N     @12,MASKOPFN                                        0758 02417000
         ST    @12,ADDRPGT                                         0758 02418000
*   ADDRPGT=(VBNTV*PFTVBN)|ADDRPGT; /* OR IN PFN                     */ 02419000
*                                                                  0759 02420000
         SLR   @11,@11                                             0759 02421000
         ICM   @11,3,PFTVBN(@01)                                   0759 02422000
         SLA   @11,8                                               0759 02423000
         OR    @11,@12                                             0759 02424000
         ST    @11,ADDRPGT                                         0759 02425000
*   /*****************************************************************/ 02426000
*   /*                                                               */ 02427000
*   /* CALCULATE VIRTUAL ADDRESS OF PAGE TABLE ENTRY THE VIRTUAL     */ 02428000
*   /* ADDRESS OF THE PAGE TABLE ENTRY IS THE PAGE NUMBER TIMES THE  */ 02429000
*   /* LENGTH ON ONE PAGE TABLE ENTRY PLUS THE VIRTUAL ADDRESS OF THE*/ 02430000
*   /* PAGE TABLE.                                                   */ 02431000
*   /*                                                               */ 02432000
*   /*****************************************************************/ 02433000
*                                                                  0760 02434000
*   ADDRPGTE=((VIRTA2/NPG)*LENGTH(PGTPTE))+ADDRPGT;                0760 02435000
*                                                                  0760 02436000
         SLR   @12,@12                                             0760 02437000
         IC    @12,VIRTA2                                          0760 02438000
         SRL   @12,4                                               0760 02439000
         ALR   @12,@12                                             0760 02440000
         ALR   @12,@11                                             0760 02441000
         ST    @12,ADDRPGTE                                        0760 02442000
*   /*****************************************************************/ 02443000
*   /*                                                               */ 02444000
*   /* IF ANY ADDRESS ON THIS PAGE IS IN THE RANGE OF THE RESTRICTED */ 02445000
*   /* ADDRESSES (RESTABLE) THE RETURN CODE IS FOUR                  */ 02446000
*   /*                                                               */ 02447000
*   /*****************************************************************/ 02448000
*                                                                  0761 02449000
*   IF RINEXT<RESHI&RINEXT^<RESLOW THEN/* RESTRICTED ADDRESS         */ 02450000
         C     RINEXT,RESHI                                        0761 02451000
         BNL   @RF00761                                            0761 02452000
         C     RINEXT,RESLOW                                       0761 02453000
         BL    @RF00761                                            0761 02454000
*     RETURN CODE(FOURRC);          /* RETURN TO CALLER              */ 02455000
         LA    @15,4                                               0762 02456000
         L     @14,12(,@13)                                        0762 02457000
         LM    @00,@12,20(@13)                                     0762 02458000
         BR    @14                                                 0762 02459000
*   ELSE                            /* UNRESTRICTED ADDRESS          */ 02460000
*     RETURN CODE(ZERORC);          /* RETURN TO CALLER              */ 02461000
@RF00761 SLR   @15,@15                                             0763 02462000
         L     @14,12(,@13)                                        0763 02463000
         LM    @00,@12,20(@13)                                     0763 02464000
         BR    @14                                                 0763 02465000
*   RETURN;                         /* RETURN TO CALLER              */ 02466000
*   RESPECIFY                                                      0765 02467000
*    (RCVT,                                                        0765 02468000
*     RPVT,                                                        0765 02469000
*     PTEPTR,                                                      0765 02470000
*     RNVT,                                                        0765 02471000
*     RINEXT,                                                      0765 02472000
*     RPGT) UNRESTRICTED;                                          0765 02473000
*   END NPXFPGT;                    /* END SUBROUTINE                */ 02474000
*                                                                  0767 02475000
*/********************************************************************/ 02476000
*/*     SUBROUTINE FOR BRANCH ENTERED FREEMAINS OF SUBPOOL 255       */ 02477000
*/********************************************************************/ 02478000
*/*       INPUT                                                      */ 02479000
*/*         RPARM0 (REG 0) - SIZE TO BE FREED                        */ 02480000
*/*         RPARM  (REG 1) - ADDRESS OF AREA TO BE FREED             */ 02481000
*/********************************************************************/ 02482000
*                                                                  0767 02483000
*NPXFMAIN:                                                         0767 02484000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0767 02485000
NPXFMAIN DS    0H                                                  0768 02486000
*   RESPECIFY                                                      0768 02487000
*    (RNVT,                                                        0768 02488000
*     RCVT,                                                        0768 02489000
*     R4,                                                          0768 02490000
*     R7,                                                          0768 02491000
*     RPARM,                                                       0768 02492000
*     RPARM0) RESTRICTED;                                          0768 02493000
*   SAVER14=R14;                    /* SAVE RETURN ADDRESS           */ 02494000
         ST    R14,SAVER14                                         0769 02495000
*   SAVER4=R4;                      /* SAVE R4 FOR FREEMAIN INTERFACE*/ 02496000
         ST    R4,SAVER4                                           0770 02497000
*   SAVER7=R7;                      /* SAVE R7 FOR FREEMAIN INTERFACE*/ 02498000
*                                                                  0771 02499000
         ST    R7,SAVER7                                           0771 02500000
*   /*****************************************************************/ 02501000
*   /*                                                               */ 02502000
*   /* CHECK IF STORAGE BEING FREED IS A GETMAIN CELL. IF IT IS THE  */ 02503000
*   /* SIZE OF A CELL THEN RETURN IT TO GETMAINS CELLPOOL QUEUE      */ 02504000
*   /*                                                               */ 02505000
*   /*****************************************************************/ 02506000
*                                                                  0772 02507000
*   IF RPARM0=16 THEN               /* CELL SIZE?            @Z30ENXA*/ 02508000
         CH    RPARM0,@CH00057                                     0772 02509000
         BNE   @RF00772                                            0772 02510000
*     DO;                                                          0773 02511000
*       R7=NVTMASCB;                /* MASTERS ASCB ADDR     @Z30ENXA*/ 02512000
         L     R7,NVTMASCB(,RNVT)                                  0774 02513000
*       R15=CVTCBBR;                /* CELL RETURN ADDRESS   @Z30ENXA*/ 02514000
         L     R15,CVTCBBR(,RCVT)                                  0775 02515000
*       GEN(LA  1,0(1));            /* CLEAR HIGH BYTE       @Z30ENXA*/ 02516000
         LA  1,0(1)                                                     02517000
*       GEN(BALR  R14,R15);         /* GO FREE CELL          @Z30ENXA*/ 02518000
         BALR  R14,R15                                                  02519000
*     END;                                                         0778 02520000
*   ELSE                                                           0779 02521000
*                                                                  0779 02522000
*     /***************************************************************/ 02523000
*     /*                                                             */ 02524000
*     /* WAS NOT SIZE OF CELL. DO NORMAL FREEMAIN                    */ 02525000
*     /*                                                             */ 02526000
*     /***************************************************************/ 02527000
*                                                                  0779 02528000
*     DO;                                                          0779 02529000
         B     @RC00772                                            0779 02530000
@RF00772 DS    0H                                                  0780 02531000
*       RPARM0=RPARM0|SUBP255;      /* OR IN SUBPOOL NUMBER FOR    0780 02532000
*                                      FREEMAIN                      */ 02533000
         O     RPARM0,SUBP255                                      0780 02534000
*       R4=NVTMSTCB;                /* SET UP MASTER TCB ADDRESS     */ 02535000
         L     R4,NVTMSTCB(,RNVT)                                  0781 02536000
*       R7=NVTMASCB;                /* SET UP MASTER ASCB ADDRESS    */ 02537000
         L     R7,NVTMASCB(,RNVT)                                  0782 02538000
*       GENERATE REFS(RPARM,RPARM0,SCVTRMBR,CVTRMBR) SETS(RPARM0)  0783 02539000
*           (FREEMAIN R,LV=(0),A=(1),BRANCH=YES);/* FREE AREA        */ 02540000
         FREEMAIN R,LV=(0),A=(1),BRANCH=YES                             02541000
*     END;                                                         0784 02542000
*   R4=SAVER4;                      /* RESTORE R4                    */ 02543000
@RC00772 L     R4,SAVER4                                           0785 02544000
*   R7=SAVER7;                      /* RESTORE R7                    */ 02545000
         L     R7,SAVER7                                           0786 02546000
*   R14=SAVER14;                    /* RESTORE RETURN ADDRESS        */ 02547000
         L     R14,SAVER14                                         0787 02548000
*   RETURN;                         /* RETURN TO CALLER              */ 02549000
@EL00004 DS    0H                                                  0788 02550000
@EF00004 DS    0H                                                  0788 02551000
@ER00004 BR    @14                                                 0788 02552000
*   END NPXFMAIN;                                                  0789 02553000
*                                                                  0790 02554000
*/************************************************************       */ 02555000
*/*      SUBROUTINE TO FREE DUMMY PQE, PQE, FBQE                     */ 02556000
*/************************************************************       */ 02557000
*/*    INPUT                                                         */ 02558000
*/*       RPQE      ADDRESS OF DUMMY PQE                             */ 02559000
*/*    OUTPUT                                                        */ 02560000
*/*       ZERO PQE CHAIN                                             */ 02561000
*/************************************************************       */ 02562000
*                                                                  0790 02563000
*NPXFPQE:                                                          0790 02564000
*   PROC OPTIONS(NOSAVEAREA);                                      0790 02565000
NPXFPQE  STM   @14,@12,12(@13)                                     0790 02566000
*   RESPECIFY                                                      0791 02567000
*    (RPARM,                                                       0791 02568000
*     RPARM0,                                                      0791 02569000
*     RNVT,                                                        0791 02570000
*     RCVT,                                                        0791 02571000
*     RPQE,                                                        0791 02572000
*     RSIZE,                                                       0791 02573000
*     RTCB) RESTRICTED;                                            0791 02574000
*   IF RPQE=ZERO THEN               /* IF NO PQES DONT FREE          */ 02575000
         LTR   RPQE,RPQE                                           0792 02576000
         BZ    @RT00792                                            0792 02577000
*     RETURN;                                                      0793 02578000
*   NPXSNEXT=PQEFPQE;               /* SAVE REAL PQE ADDRESS         */ 02579000
         L     NPXSNEXT,PQEFPQE(,RPQE)                             0794 02580000
*   RPQE=RPQE+EIGHT;                /* GET TO BEGINNING OF D-PQE     */ 02581000
         LA    @12,8                                               0795 02582000
         ALR   RPQE,@12                                            0795 02583000
*   RPARM=RPQE;                     /* SET UP ADDRESS FOR FREEMAIN   */ 02584000
         LR    RPARM,RPQE                                          0796 02585000
*   RPARM0=DPQELEN;                 /* SET UP SIZE FOR FREEMAIN      */ 02586000
         LR    RPARM0,@12                                          0797 02587000
*   CALL NPXFMAIN;                  /* CALL FREEMAIN SUBROUTINE      */ 02588000
         BAL   @14,NPXFMAIN                                        0798 02589000
*   RPQE=NPXSNEXT;                  /* ADDRESS OF REAL PQE           */ 02590000
         LR    RPQE,NPXSNEXT                                       0799 02591000
*   FBQESPTR=PQEFFBQE;              /* ADDRESS OF FIRST FBQE         */ 02592000
         MVC   FBQESPTR(4),PQEFFBQE(RPQE)                          0800 02593000
*   RPARM=RPQE;                     /* SET UP ADDRESS FOR FREEMAIN   */ 02594000
         LR    RPARM,RPQE                                          0801 02595000
*   RPARM0=LENGTH(PQESECT);         /* SET UP SIZE FOR FREEMAIN      */ 02596000
         LA    RPARM0,32                                           0802 02597000
*   CALL NPXFMAIN;                  /* CALL FREEMAIN SUBROUTINE      */ 02598000
         BAL   @14,NPXFMAIN                                        0803 02599000
*   IF RPQE=FBQESPTR THEN                                          0804 02600000
         C     RPQE,FBQESPTR                                       0804 02601000
         BE    @RT00804                                            0804 02602000
*     RETURN;                       /* IF NO FBQE RETURN             */ 02603000
*NPXFFBQE:                                                         0806 02604000
*   NPXSNEXT=FWDPTR;                /* SAVE ADDRESS OF NEXT FBQE     */ 02605000
NPXFFBQE L     @12,FBQESPTR                                        0806 02606000
         L     NPXSNEXT,FWDPTR(,@12)                               0806 02607000
*   RPARM=FBQESPTR;                 /* SET UP ADDRESS FOR FREEMAIN   */ 02608000
         LR    RPARM,@12                                           0807 02609000
*   RPARM0=LENGTH(FBQESECT);        /* SET UP SIZE FOR FREEMAIN      */ 02610000
         LA    RPARM0,16                                           0808 02611000
*   CALL NPXFMAIN;                  /* CALL FREEMAIN SUBROUTINE      */ 02612000
         BAL   @14,NPXFMAIN                                        0809 02613000
*   FBQESPTR=NPXSNEXT;              /* GET ADDRESS OF NEXT FBQE      */ 02614000
         ST    NPXSNEXT,FBQESPTR                                   0810 02615000
*   IF RPQE^=FBQESPTR THEN          /* IF NEXT FBQE NOT = PQE ADDR   */ 02616000
         L     @12,FBQESPTR                                        0811 02617000
         CR    RPQE,@12                                            0811 02618000
         BNE   @RT00811                                            0811 02619000
*     GO TO NPXFFBQE;               /* GO PROCESS NEXT FBQE          */ 02620000
*   RETURN;                         /* ELSE ALL FBQE'S FREED         */ 02621000
@EL00005 DS    0H                                                  0813 02622000
@EF00005 DS    0H                                                  0813 02623000
@ER00005 LM    @14,@12,12(@13)                                     0813 02624000
         BR    @14                                                 0813 02625000
*   RESPECIFY                                                      0814 02626000
*    (RPARM,                                                       0814 02627000
*     RPARM0,                                                      0814 02628000
*     RNVT,                                                        0814 02629000
*     RCVT,                                                        0814 02630000
*     RPQE,                                                        0814 02631000
*     RSIZE,                                                       0814 02632000
*     RTCB) UNRESTRICTED;                                          0814 02633000
*   END NPXFPQE;                                                   0815 02634000
         B     @EL00005                                            0815 02635000
*                                                                  0816 02636000
*/************************************************************       */ 02637000
*/*      SUBROUTINE TO FREE SPQE, DQE, AND FQE                       */ 02638000
*/************************************************************       */ 02639000
*/*    INPUT                                                         */ 02640000
*/*       SPQESPTR  ADDRESS OF FIRST SPQE ON CHAIN                   */ 02641000
*/*    OUTPUT                                                        */ 02642000
*/*       ZERO SPQE CHAIN                                            */ 02643000
*/************************************************************       */ 02644000
*                                                                  0816 02645000
*NPXFSPQE:                                                         0816 02646000
*   PROC OPTIONS(NOSAVEAREA);                                      0816 02647000
NPXFSPQE STM   @14,@12,12(@13)                                     0816 02648000
*   RESPECIFY                                                      0817 02649000
*    (RNVT,                                                        0817 02650000
*     RCVT,                                                        0817 02651000
*     RTCB,                                                        0817 02652000
*     RSCAN,                                                       0817 02653000
*     SPQESPTR,                                                    0817 02654000
*     DQESEPTR,                                                    0817 02655000
*     FQESEPTR,                                                    0817 02656000
*     RPARM,                                                       0817 02657000
*     RPARM0) RESTRICTED;                                          0817 02658000
*   NPXSNEXT=SPQESPTR;              /* INITIALIZE NEXT SPQE          */ 02659000
         LR    NPXSNEXT,SPQESPTR                                   0818 02660000
*NPXNSPQE:                                                         0819 02661000
*   SPQESPTR=NPXSNEXT;              /* GET NEXT SPQE ADDRESS         */ 02662000
NPXNSPQE LR    SPQESPTR,NPXSNEXT                                   0819 02663000
*   IF SPQESPTR=ZERO THEN           /* IF THIS SPQE ADDR IS ZERO     */ 02664000
         LTR   SPQESPTR,SPQESPTR                                   0820 02665000
         BZ    @RT00820                                            0820 02666000
*     RETURN;                       /* ALL SPQE, DQE, FQE CHAIN FREED*/ 02667000
*   NPXSNEXT=SPQEAD;                /* SAVE NEXT SPQE ADDRESS THAT IS   02668000
*                                      TO BE PROCESSED               */ 02669000
         L     NPXSNEXT,SPQEAD(,SPQESPTR)                          0822 02670000
*   NPXSNXT1=SPDQEAD;               /* SAVE PTR TO NEXT DQE          */ 02671000
         MVC   NPXSNXT1(4),SPDQEAD(SPQESPTR)                       0823 02672000
*   RPARM=SPQESPTR;                 /* SET UP ADDRESS FOR FREEMAIN   */ 02673000
         LR    RPARM,SPQESPTR                                      0824 02674000
*   RPARM0=LENGTH(SPQESECT);        /* SET UP SIZE FOR FREEMAIN      */ 02675000
         LA    RPARM0,16                                           0825 02676000
*   CALL NPXFMAIN;                  /* CALL FREEMAIN SUBROUTINE      */ 02677000
         BAL   @14,NPXFMAIN                                        0826 02678000
*NPXDQE:                                                           0827 02679000
*   DQESEPTR=NPXSNXT1;              /* GET DQE ADDRESS               */ 02680000
NPXDQE   L     DQESEPTR,NPXSNXT1                                   0827 02681000
*   IF DQESEPTR=ZERO THEN           /* IF NO MORE DQE'S              */ 02682000
         LTR   DQESEPTR,DQESEPTR                                   0828 02683000
         BZ    @RT00828                                            0828 02684000
*     GO TO NPXNSPQE;               /* GO GET NEXT SPQE              */ 02685000
*   NPXSNXT1=DQEPTR;                /* SAVE NEXT DQE ADDRESS         */ 02686000
         MVC   NPXSNXT1(4),DQEPTR(DQESEPTR)                        0830 02687000
*   NPXSNXT2=DQFQEPTR;              /* GET PTR TO FQE                */ 02688000
         MVC   NPXSNXT2(4),DQFQEPTR(DQESEPTR)                      0831 02689000
*   RPARM=DQESEPTR;                 /* SET UP ADDRESS FOR FREEMAIN   */ 02690000
         LR    RPARM,DQESEPTR                                      0832 02691000
*   RPARM0=LENGTH(DQESECT);         /* SET UP SIZE FOR FREEMAIN      */ 02692000
         LA    RPARM0,16                                           0833 02693000
*   CALL NPXFMAIN;                  /* CALL FREEMAIN SUBROUTINE      */ 02694000
         BAL   @14,NPXFMAIN                                        0834 02695000
*NPXFQE:                                                           0835 02696000
*   FQESEPTR=NPXSNXT2;              /* GET NEXT FQE                  */ 02697000
NPXFQE   L     FQESEPTR,NPXSNXT2                                   0835 02698000
*   IF FQESEPTR=ZERO THEN           /* IF NO MORE FQE'S              */ 02699000
         LTR   FQESEPTR,FQESEPTR                                   0836 02700000
         BZ    @RT00836                                            0836 02701000
*     GO TO NPXDQE;                 /* GO GET NEXT DQE               */ 02702000
*   NPXSNXT2=FQEPTRA;               /* SAVE NEXT FQE                 */ 02703000
         SLR   @12,@12                                             0838 02704000
         ICM   @12,7,FQEPTRA(FQESEPTR)                             0838 02705000
         ST    @12,NPXSNXT2                                        0838 02706000
*   RPARM=FQESEPTR;                 /* SET UP ADDRESS FOR FREEMAIN   */ 02707000
         LR    RPARM,FQESEPTR                                      0839 02708000
*   RPARM0=LENGTH(FQESECT);         /* SET UP SIZE FOR FREEMAIN      */ 02709000
         LA    RPARM0,16                                           0840 02710000
*   CALL NPXFMAIN;                  /* CALL FREEMAIN SUBROUTINE      */ 02711000
         BAL   @14,NPXFMAIN                                        0841 02712000
*   GO TO NPXFQE;                   /* GO GET NEXT FQE               */ 02713000
         B     NPXFQE                                              0842 02714000
*   RESPECIFY                                                      0843 02715000
*    (RNVT,                                                        0843 02716000
*     RCVT,                                                        0843 02717000
*     RTCB,                                                        0843 02718000
*     RSCAN,                                                       0843 02719000
*     SPQESPTR,                                                    0843 02720000
*     DQESEPTR,                                                    0843 02721000
*     FQESEPTR,                                                    0843 02722000
*     RPARM,                                                       0843 02723000
*     RPARM0) UNRESTRICTED;                                        0843 02724000
*   END NPXFSPQE;                                                  0844 02725000
@EL00006 DS    0H                                                  0844 02726000
@EF00006 DS    0H                                                  0844 02727000
@ER00006 LM    @14,@12,12(@13)                                     0844 02728000
         BR    @14                                                 0844 02729000
*                                                                  0845 02730000
*/************************************************************       */ 02731000
*/*    SUBROUTINE NPXLPAFN                                           */ 02732000
*/*       SEARCHES THE ACTIVE AND INACTIVE LINK PACK AREA            */ 02733000
*/*       FOR A SPECIFIED MODULE                                     */ 02734000
*/************************************************************       */ 02735000
*/*    INPUT                                                         */ 02736000
*/*       MODNAME   MODULE NAME                                      */ 02737000
*/*    OUTPUT                                                        */ 02738000
*/*       MODADDR     ENTRY POINT ADDRESS IF MODULE IS FOUND         */ 02739000
*/*       MODADDR     ZERO IF MODULE IS NOT FOUND                    */ 02740000
*/*    EXTERNAL INTERFACES                                           */ 02741000
*/*       IEAQCDSR                                                   */ 02742000
*/*       IEAVVMSR                                                   */ 02743000
*/************************************************************       */ 02744000
*                                                                  0845 02745000
*NPXLPAFN:                                                         0845 02746000
*   PROC OPTIONS(NOSAVEAREA);                                      0845 02747000
NPXLPAFN STM   @14,@12,12(@13)                                     0845 02748000
*   RESPECIFY                                                      0846 02749000
*    (RPARM0,                                                      0846 02750000
*     RPARM,                                                       0846 02751000
*     RNVT,                                                        0846 02752000
*     RCVT,                                                        0846 02753000
*     REXIT,                                                       0846 02754000
*     RENTRY,                                                      0846 02755000
*     RTCB,                                                        0846 02756000
*     RQADR,                                                       0846 02757000
*     RNAME,                                                       0846 02758000
*     CDENTPTR,                                                    0846 02759000
*     LPDEPTR) RESTRICTED;                                         0846 02760000
*   RENTRY=CVTQCDSR;                /* PICK UP ADDRESS OF ROUTINE TO    02761000
*                                      SEARCH THE ACTIVE LPA QUEUE   */ 02762000
         L     RENTRY,CVTQCDSR(,RCVT)                              0847 02763000
*   RNAME=ADDR(MODNAME);            /* ADDRESS OF MODULE NAME TO   0848 02764000
*                                      SEARCH FOR                    */ 02765000
         LA    RNAME,MODNAME                                       0848 02766000
*   RTCB=NVTMSTCB;                  /* ADDR OF MS TCB                */ 02767000
         L     RTCB,NVTMSTCB(,RNVT)                                0849 02768000
*   RQADR=CVTQLPAQ;                 /* ADDR OF QUEUE TO BE SEARCHED  */ 02769000
*                                                                  0850 02770000
         L     RQADR,CVTQLPAQ(,RCVT)                               0850 02771000
*   /*****************************************************************/ 02772000
*   /*                                                               */ 02773000
*   /* CALL IEAQCDSR TO SEARCH THE ACTIVE LINK PACK AREA             */ 02774000
*   /*                                                               */ 02775000
*   /*****************************************************************/ 02776000
*                                                                  0851 02777000
*   GENERATE                        /* GEN CALL AND BR TAB           */ 02778000
*       REFS(RQADR,RNAME,RENTRY,RTCB) SETS(RPARM0,RPARM,CDENTPTR,REXIT, 02779000
*       RENTRY) FLOWS(NPXLPACQ);                                   0851 02780000
         BALR  REXIT,RENTRY            /* IEAQCDSR                   */ 02781000
         B     NPXLPACQ                /* CODE 0 CDE FOUND           */ 02782000
*NPXLPNAC:                          /* NOT ON ACTIVE Q               */ 02783000
*   RENTRY=CVTLPDSR;                /* ADDR OF LPDE SEARCH ROUTINE   */ 02784000
NPXLPNAC L     RENTRY,CVTLPDSR(,RCVT)                              0852 02785000
*   R7=B2;                          /* SAVE BASE REG OVER INTERFACE  */ 02786000
*                                                                  0853 02787000
         LR    R7,B2                                               0853 02788000
*   /*****************************************************************/ 02789000
*   /*                                                               */ 02790000
*   /* CALL IEAVVMSR TO SEARCH THE LINK PACK AREA                    */ 02791000
*   /*                                                               */ 02792000
*   /*****************************************************************/ 02793000
*                                                                  0854 02794000
*   GENERATE                        /* GEN CALL AND BR TAB           */ 02795000
*       REFS(MODNAME,RCVT,RENTRY) SETS(RPARM0,RPARM,R7,R8,R9,REXIT)     02796000
*       FLOWS(NPXLPDIR);                                           0854 02797000
         LM    RPARM0,RPARM,MODNAME    /* NAME IN PARM REGS          */ 02798000
         BALR  REXIT,RENTRY            /* IEAVVMSR                   */ 02799000
         B     NPXLPDIR                /* CODE 0 LPDE FOUND          */ 02800000
*NPXLPNFN:                          /* LPDE NOT FOUND                */ 02801000
*   B2=R7;                          /* RESTORE BASE REG              */ 02802000
NPXLPNFN LR    B2,R7                                               0855 02803000
*   MODADDR=ZERO;                   /* SET RETURN PARM               */ 02804000
         SLR   @12,@12                                             0856 02805000
         ST    @12,MODADDR                                         0856 02806000
*   RETURN;                         /* RETURN TO CALLER              */ 02807000
@EL00007 DS    0H                                                  0857 02808000
@EF00007 DS    0H                                                  0857 02809000
@ER00007 LM    @14,@12,12(@13)                                     0857 02810000
         BR    @14                                                 0857 02811000
*NPXLPACQ:                          /* CDE ON ACTIVE Q               */ 02812000
*   CDENTPTR=RETCDE;                /* SET UP CDE BASE               */ 02813000
NPXLPACQ LR    CDENTPTR,RETCDE                                     0858 02814000
*   MODADDR=CDENTPT;                /* ENTRY PT ADDR                 */ 02815000
         MVC   MODADDR(4),CDENTPT(CDENTPTR)                        0859 02816000
*   RETURN;                         /* RETURN TO CALLER              */ 02817000
         B     @EL00007                                            0860 02818000
*NPXLPDIR:                          /* CDE IN LPA DIRECTORY          */ 02819000
*   B2=R7;                          /* RESTORE BASE REG              */ 02820000
NPXLPDIR LR    B2,R7                                               0861 02821000
*   LPDEPTR=RPARM0;                 /* BASE FOR LPDE                 */ 02822000
         LR    LPDEPTR,RPARM0                                      0862 02823000
*   MODADDR=LPDENTP;                /* ENTRY PT ADDR                 */ 02824000
         MVC   MODADDR(4),LPDENTP(LPDEPTR)                         0863 02825000
*   RETURN;                         /* RETURN TO CALLER              */ 02826000
         B     @EL00007                                            0864 02827000
*   RESPECIFY                                                      0865 02828000
*    (RPARM0,                                                      0865 02829000
*     RPARM,                                                       0865 02830000
*     RNVT,                                                        0865 02831000
*     RCVT,                                                        0865 02832000
*     REXIT,                                                       0865 02833000
*     RENTRY,                                                      0865 02834000
*     RTCB,                                                        0865 02835000
*     RQADR,                                                       0865 02836000
*     RNAME,                                                       0865 02837000
*     CDENTPTR,                                                    0865 02838000
*     LPDEPTR,                                                     0865 02839000
*     R12) UNRESTRICTED;                                           0865 02840000
*   END NPXLPAFN;                                                  0866 02841000
*                                                                  0867 02842000
*/************************************************************       */ 02843000
*/*      SUBROUTINE TO ADD A PAGE TO THE AVAILABLE                   */ 02844000
*/*      PAGE FRAME QUEUE                                            */ 02845000
*/************************************************************       */ 02846000
*/*    INPUT                                                         */ 02847000
*/*       PREVPFTE                                                   */ 02848000
*/*       RINEXT                                                     */ 02849000
*/*    OUTPUT                                                        */ 02850000
*/*       PFTBQPTR                                                   */ 02851000
*/*       PFTFQPTR                                                   */ 02852000
*/*       PFTONAVQ                                                   */ 02853000
*/*       PFTQNDX                                                    */ 02854000
*/*       PFTVBN                                                     */ 02855000
*/*       PREVPFTE                                                   */ 02856000
*/*       PVTAFC                                                     */ 02857000
*/*       PVTAFQF                                                    */ 02858000
*/*       PVTAFQL                                                    */ 02859000
*/************************************************************       */ 02860000
*                                                                  0867 02861000
*NPXQPFTE:                                                         0867 02862000
*   PROC OPTIONS(NOSAVEAREA);                                      0867 02863000
NPXQPFTE STM   @14,@11,12(@13)                                     0867 02864000
*   RESPECIFY                                                      0868 02865000
*    (RNVT,                                                        0868 02866000
*     RCVT,                                                        0868 02867000
*     RPVT,                                                        0868 02868000
*     RINEXT) RESTRICTED;                                          0868 02869000
*                                                                  0868 02870000
*   /*****************************************************************/ 02871000
*   /*                                                               */ 02872000
*   /* IS THIS THE FIRST PAGE ON THE AVAILABLE QUEUE                 */ 02873000
*   /*                                                               */ 02874000
*   /*****************************************************************/ 02875000
*                                                                  0869 02876000
*   IF PREVPFTE^=NOPREV THEN        /* NOT FIRST PAGE                */ 02877000
         LTR   PREVPFTE,PREVPFTE                                   0869 02878000
         BZ    @RF00869                                            0869 02879000
*     DO;                                                          0870 02880000
*                                                                  0870 02881000
*       /*************************************************************/ 02882000
*       /*                                                           */ 02883000
*       /* SET UP PREVIOUS PFTE TO POINT TO NEW PFT                  */ 02884000
*       /*                                                           */ 02885000
*       /*************************************************************/ 02886000
*                                                                  0871 02887000
*       PFTEPTR=PVTPFTP+PREVPFTE;                                  0871 02888000
         LR    @11,PREVPFTE                                        0871 02889000
         AL    @11,PVTPFTP(,RPVT)                                  0871 02890000
         ST    @11,PFTEPTR                                         0871 02891000
*       PFTFQPTR=RINEXT;            /* FORWARD CHAIN                 */ 02892000
         STH   RINEXT,PFTFQPTR(,@11)                               0872 02893000
*     END;                                                         0873 02894000
*   ELSE                            /* FIRST PAGE                    */ 02895000
*     PVTAFQF=RINEXT;               /* FIRST PFTE                    */ 02896000
         B     @RC00869                                            0874 02897000
@RF00869 STH   RINEXT,PVTAFQF(,RPVT)                               0874 02898000
*   PFTEPTR=PVTPFTP+RINEXT;         /* ADDR. OF NEXT PFTE            */ 02899000
@RC00869 LR    @11,RINEXT                                          0875 02900000
         AL    @11,PVTPFTP(,RPVT)                                  0875 02901000
         ST    @11,PFTEPTR                                         0875 02902000
*   PFTONAVQ=AVLPG;                 /* FLAG PAGE AS AVAILABLE        */ 02903000
         OI    PFTONAVQ(@11),B'10000000'                           0876 02904000
*   PFTQNDX=PFTAFQN;                /* FLAG PAGE AS AVAILABLE        */ 02905000
         MVI   PFTQNDX(@11),X'00'                                  0877 02906000
*   PFTBQPTR=PREVPFTE;              /* CHAIN PREVIOUS                */ 02907000
         STH   PREVPFTE,PFTBQPTR(,@11)                             0878 02908000
*   PREVPFTE=RINEXT;                /* SAVE INDEX TO NEW PFTE        */ 02909000
         LR    PREVPFTE,RINEXT                                     0879 02910000
*   PFTFQPTR=NOBPFT;                /* ZERO FORWARD PTR              */ 02911000
         SLR   @09,@09                                             0880 02912000
         STH   @09,PFTFQPTR(,@11)                                  0880 02913000
*   PVTAFQL=RINEXT;                 /* PUT PFTE ON TOP OF AVAILABLE     02914000
*                                      QUEUE                         */ 02915000
         STH   RINEXT,PVTAFQL(,RPVT)                               0881 02916000
*   PVTAFC=PVTAFC+ONEAV;            /* NUMBER OF AVAILABLE PAGES IN     02917000
*                                      PVT                           */ 02918000
         LA    @11,1                                               0882 02919000
         AH    @11,PVTAFC(,RPVT)                                   0882 02920000
         STH   @11,PVTAFC(,RPVT)                                   0882 02921000
*   RETURN;                         /* TO CALLING ROUTINE            */ 02922000
@EL00008 DS    0H                                                  0883 02923000
@EF00008 DS    0H                                                  0883 02924000
@ER00008 LM    @14,@11,12(@13)                                     0883 02925000
         BR    @14                                                 0883 02926000
*   RESPECIFY                                                      0884 02927000
*    (RNVT,                                                        0884 02928000
*     RCVT,                                                        0884 02929000
*     RPVT,                                                        0884 02930000
*     RINEXT) UNRESTRICTED;                                        0884 02931000
*   END NPXQPFTE;                   /* END SUBROUTINE                */ 02932000
*RSUKEYWD:                                                         0886 02933000
*   PROC;                                                          0886 02934000
*                                                                  0886 02935000
RSUKEYWD STM   @14,@12,@SA00009                                    0886 02936000
*   /*****************************************************************/ 02937000
*   /*                                                               */ 02938000
*   /* ROUTINE WILL SCAN RSU SYSTEM PARAMETER AND SAVE VALUE FOR     */ 02939000
*   /* PROCESSING WHEN PFTE'S ARE MARKED. IF AN ERROR IS DETECTED THE*/ 02940000
*   /* OPERATOR WILL BE PROMPTED TO REENTER THE VALUE. IF RSU IS NOT */ 02941000
*   /* SPECIFIED, A DEFAULT VALUE OF ZERO WILL BE ASSUMED            */ 02942000
*   /*                                                               */ 02943000
*   /*****************************************************************/ 02944000
*                                                                  0887 02945000
*   RFY                                                            0887 02946000
*    (RCVT,                                                        0887 02947000
*     RNVT) RSTD;                                                  0887 02948000
*RSUSCAN:                                                          0888 02949000
*   RSUPTR=NVTPTAB->RSU;            /* INPUT VALUE ADDR FROM PARMTAB    02950000
*                                                            @YD03005*/ 02951000
*                                                                  0888 02952000
RSUSCAN  L     @12,NVTPTAB(,RNVT)                                  0888 02953000
         L     RSUPTR,RSU(,@12)                                    0888 02954000
*   /*****************************************************************/ 02955000
*   /*                                                               */ 02956000
*   /* DETERMINE IF RSU KEYWORD WAS SPECIFIED VALUE IS DEFAULTED TO  */ 02957000
*   /* ZERO IF MP FEATURE IS NOT PRESENT OR RSU KEYWORD NOT          */ 02958000
*   /* SPECIFIED.                                                    */ 02959000
*   /*                                                               */ 02960000
*   /*****************************************************************/ 02961000
*                                                                  0889 02962000
*   IF RSUPTR=0|NVTMP='0'B THEN     /* VALUE NOT SPEC OR UP  @YD03005*/ 02963000
         LTR   RSUPTR,RSUPTR                                       0889 02964000
         BZ    @RT00889                                            0889 02965000
         TM    NVTMP(RNVT),B'00100000'                             0889 02966000
         BNZ   @RF00889                                            0889 02967000
@RT00889 DS    0H                                                  0890 02968000
*     RSUVALUE=0;                   /* DEFAULT RSU VALUE TO ZERO   0890 02969000
*                                                            @YD03005*/ 02970000
         SLR   @12,@12                                             0890 02971000
         ST    @12,RSUVALUE                                        0890 02972000
*   ELSE                                                           0891 02973000
*     DO;                                                          0891 02974000
*                                                                  0891 02975000
         B     @RC00889                                            0891 02976000
@RF00889 DS    0H                                                  0892 02977000
*       /*************************************************************/ 02978000
*       /*                                                           */ 02979000
*       /* RSU VALUE WAS SPECIFIED, SCAN INPUT VALUE FOR PROPER      */ 02980000
*       /* SYNTAX                                                    */ 02981000
*       /*                                                           */ 02982000
*       /*************************************************************/ 02983000
*                                                                  0892 02984000
*       IF RSUPARM='(' THEN         /* VALUE IN PARENS?      @YD03005*/ 02985000
         CLI   RSUPARM(RSUPTR),C'('                                0892 02986000
         BNE   @RF00892                                            0892 02987000
*         RSUPTR=RSUPTR+1;          /* INCREMENT TO NEXT CHARACTER 0893 02988000
*                                                            @YD03005*/ 02989000
         AH    RSUPTR,@CH00041                                     0893 02990000
*       IF(RSUPARM<'0'|RSUPARM>'9') THEN/* VALID CHAR?       @YD03005*/ 02991000
@RF00892 CLI   RSUPARM(RSUPTR),C'0'                                0894 02992000
         BL    @RT00894                                            0894 02993000
         CLI   RSUPARM(RSUPTR),C'9'                                0894 02994000
         BH    @RT00894                                            0894 02995000
*         GOTO REPROMPT;            /* NO.. RESPECIFY.       @YD03005*/ 02996000
*       RSUVALUE=RSUPARM-240;       /* SAVE FIRST CHARACHTER @YD03005*/ 02997000
         SLR   @12,@12                                             0896 02998000
         IC    @12,RSUPARM(,RSUPTR)                                0896 02999000
         SH    @12,@CH03345                                        0896 03000000
         ST    @12,RSUVALUE                                        0896 03001000
*       RSUPTR=RSUPTR+1;            /* INCREMENT TO NEXT CHAR      0897 03002000
*                                                            @YD03005*/ 03003000
*                                                                  0897 03004000
         LA    @12,1                                               0897 03005000
         ALR   RSUPTR,@12                                          0897 03006000
*       /*************************************************************/ 03007000
*       /*                                                           */ 03008000
*       /* CHECK FOR NEXT CHARACTER                                  */ 03009000
*       /*                                                           */ 03010000
*       /*************************************************************/ 03011000
*                                                                  0898 03012000
*       IF RSUPARM^=')'&RSUPARM^='''' THEN/* LAST CHARACTER? @YD03005*/ 03013000
*                                                                  0898 03014000
         CLI   RSUPARM(RSUPTR),C')'                                0898 03015000
         BE    @RF00898                                            0898 03016000
         CLI   RSUPARM(RSUPTR),C''''                               0898 03017000
         BE    @RF00898                                            0898 03018000
*         /***********************************************************/ 03019000
*         /*                                                         */ 03020000
*         /* CHECK SECOND CHARACTER                                  */ 03021000
*         /*                                                         */ 03022000
*         /***********************************************************/ 03023000
*                                                                  0899 03024000
*         DO;                                                      0899 03025000
*           IF(RSUPARM<'0'|RSUPARM>'9')|/* VALID CHARACTER?  @YD03005*/ 03026000
*               (((RSUPTR+1)->RSUPARM^=')')&((RSUPTR+1)->RSUPARM^='''') 03027000
*               ) THEN                                             0900 03028000
         CLI   RSUPARM(RSUPTR),C'0'                                0900 03029000
         BL    @RT00900                                            0900 03030000
         CLI   RSUPARM(RSUPTR),C'9'                                0900 03031000
         BH    @RT00900                                            0900 03032000
         ALR   @12,RSUPTR                                          0900 03033000
         CLI   RSUPARM(@12),C')'                                   0900 03034000
         BE    @RF00900                                            0900 03035000
         CLI   RSUPARM(@12),C''''                                  0900 03036000
         BNE   @RT00900                                            0900 03037000
*             GOTO REPROMPT;        /* INVALID INPUT CHARACTERS    0901 03038000
*                                                            @YD03005*/ 03039000
*           RSUVALUE=(RSUVALUE*10)+(RSUPARM-240);/* ADD SECOND CHAR     03040000
*                                                            @YD03005*/ 03041000
@RF00900 L     @12,RSUVALUE                                        0902 03042000
         MH    @12,@CH03348                                        0902 03043000
         SLR   @11,@11                                             0902 03044000
         IC    @11,RSUPARM(,RSUPTR)                                0902 03045000
         SH    @11,@CH03345                                        0902 03046000
         ALR   @12,@11                                             0902 03047000
         ST    @12,RSUVALUE                                        0902 03048000
*         END;                                                     0903 03049000
*     END;                                                         0904 03050000
*   GOTO RSUEND;                    /*                       @YD03005*/ 03051000
         B     RSUEND                                              0905 03052000
*REPROMPT:                                                         0906 03053000
*                                                                  0906 03054000
*   /*****************************************************************/ 03055000
*   /*                                                               */ 03056000
*   /* REPROMPT THE OPERATOR FOR A VALID RSU VALUE                   */ 03057000
*   /*                                                               */ 03058000
*   /*****************************************************************/ 03059000
*                                                                  0906 03060000
*   GEN REFS(RSUCHAR,NVTPRMPT)                                     0906 03061000
*       (IEAPMNIP TYPE=PRMPT,PARAM=RSUCHAR,OPT=ERROR);/*     @YD03005*/ 03062000
REPROMPT IEAPMNIP TYPE=PRMPT,PARAM=RSUCHAR,OPT=ERROR                    03063000
*   GOTO RSUSCAN;                   /* GO RESCAN INPUT       @YD03005*/ 03064000
         B     RSUSCAN                                             0907 03065000
*RSUEND:                                                           0908 03066000
*   END;                                                           0908 03067000
@EL00009 DS    0H                                                  0908 03068000
@EF00009 DS    0H                                                  0908 03069000
@ER00009 LM    @14,@12,@SA00009                                    0908 03070000
         BR    @14                                                 0908 03071000
*   END IEAVNIPX                    /* END OF PROCEEDURE             */ 03072000
