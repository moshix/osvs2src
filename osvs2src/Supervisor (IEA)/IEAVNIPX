*     GO TO NPXRDPGT;               /* CONTINUE LOOP                 */ 02006000
*   SYSTSEGP=ASCBSTOR;              /* REAL ADDR. OF SYSTEM SEGMENT     02007000
*                                      TABLE                         */ 02008000
         L     @12,ASCBPTR                                         0702 02009000
         MVC   SYSTSEGP(4),ASCBSTOR(@12)                           0702 02010000
*   VIRTADDR=RESLOW;                /* FIRST RESTRICTED PAGE ADDR.   */ 02011000
         MVC   VIRTADDR(4),RESLOW                                  0703 02012000
*/************************************************************       */ 02013000
*/***** WARNING *****                                                */ 02014000
*/************************************************************       */ 02015000
*/* THE REMAINDER OF THIS MODULE USES ONLY REAL ADDRESSES            */ 02016000
*/************************************************************       */ 02017000
