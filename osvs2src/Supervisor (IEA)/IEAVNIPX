         ICM   @12,3,PAGEL                                         0680 01905000
         ALR   RINEXT,@12                                          0680 01906000
*   /*****************************************************************/ 01907000
*   /*                                                               */ 01908000
*   /* WAS THIS THE LAST PAGE IN THIS SEGMENT                        */ 01909000
*   /*                                                               */ 01910000
*   /*****************************************************************/ 01911000
*                                                                  0681 01912000
*   IF VIRTA2^=L16K THEN            /* NOT LAST                      */ 01913000
         CLI   VIRTA2,240                                          0681 01914000
         BNE   @RT00681                                            0681 01915000
*     GO TO NPXRDPGX;               /* DO NEXT PAGE                  */ 01916000
*   IF ADDRPGT=ZERO THEN            /* IF NO PAGE TABLE SEG IS       */ 01917000
