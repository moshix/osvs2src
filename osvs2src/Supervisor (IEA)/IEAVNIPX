         TITLE 'IEAVNIPX, NIP CLEANUP AND EXIT ROUTINE                 *00001000
                        '                                               00002000
IEAVNIPX CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(24)                                             0001 00007000
         DC    C'IEAVNIPX 78107  UZ18094 '                         0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @10,0                                               0001 00010000
@PSTART  LA    @06,4095(,@10)                                      0001 00011000
         USING @PSTART,@10                                         0001 00012000
         USING @PSTART+4095,@06                                    0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         LR    @13,@14                                             0001 00016000
*                                                                       00017000
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF          00018000
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES      00019000
** WILL ASSEMBLE PROPERLY.                                              00020000
*                                                                  0282 00021000
*   /*****************************************************************/ 00022000
*   /*                                                               */ 00023000
*   /* CALL SUBROUTINE TO GET ADDR OF RESIDENT ROUTINES              */ 00024000
*   /*                                                               */ 00025000
*   /*****************************************************************/ 00026000
*                                                                  0283 00027000
*   CALL NPXOTEST;                                                 0283 00028000
*                                                                  0283 00029000
         BAL   @14,NPXOTEST                                        0283 00030000
*   /*****************************************************************/ 00031000
*   /*                                                               */ 00032000
*   /* CALL SUBROUTINE TO SCAN RSU SYSTEM PARAMETER                  */ 00033000
*   /*                                                               */ 00034000
*   /*****************************************************************/ 00035000
*                                                                  0284 00036000
*   CALL RSUKEYWD;                  /*                       @YD03005*/ 00037000
*                                                                  0284 00038000
         BAL   @14,RSUKEYWD                                        0284 00039000
*   /*****************************************************************/ 00040000
*   /*                                                               */ 00041000
*   /* CALL SUBROUTINE TO DEFINE MASTER SCHEDULER REGION             */ 00042000
*   /*                                                               */ 00043000
*   /*****************************************************************/ 00044000
*                                                                  0285 00045000
*   CALL NPXMPA;                                                   0285 00046000
*                                                                  0285 00047000
         BAL   @14,NPXMPA                                          0285 00048000
*   /*****************************************************************/ 00049000
*   /*                                                               */ 00050000
*   /* WARNING: NO INTERRUPTS EXCEPT TYPE 1 SVC'S MAY OCCUR BEYOND   */ 00051000
*   /* THIS POINT CALL SUBROUTINE TO MOVE TRACE TABLE TO SQA         */ 00052000
*   /*                                                               */ 00053000
*   /*****************************************************************/ 00054000
*                                                                  0286 00055000
*   CALL NPXTRACE;                                                 0286 00056000
*                                                                  0286 00057000
         BAL   @14,NPXTRACE                                        0286 00058000
*   /*****************************************************************/ 00059000
*   /*                                                               */ 00060000
*   /* CALL SUBROUTINE TO FREE NIP'S BUFFERS AND SPE QUEUE           */ 00061000
*   /*                                                               */ 00062000
*   /*****************************************************************/ 00063000
*                                                                  0287 00064000
*   CALL NPXFBUF;                                                  0287 00065000
*                                                                  0287 00066000
         BAL   @14,NPXFBUF                                         0287 00067000
*   /*****************************************************************/ 00068000
*   /*                                                               */ 00069000
*   /* CALL SUBROUTINE TO FREE CONTROL BLOCKS ASSOCIATED WITH NIP'S  */ 00070000
*   /* JOB PACK QUEUE                                                */ 00071000
*   /*                                                               */ 00072000
*   /*****************************************************************/ 00073000
*                                                                  0288 00074000
*   CALL NPXFJPQ;                                                  0288 00075000
*                                                                  0288 00076000
         BAL   @14,NPXFJPQ                                         0288 00077000
*   /*****************************************************************/ 00078000
*   /*                                                               */ 00079000
*   /* INITALIZE CONTROL BLOCK'S FOR VIRTUAL EQUAL VIRTUAL AND       */ 00080000
*   /* VIRTUAL EQUAL REAL CORE                                       */ 00081000
*   /*                                                               */ 00082000
*   /*****************************************************************/ 00083000
*                                                                  0289 00084000
*   CALL NPXFAREA;                                                 0289 00085000
*                                                                  0289 00086000
         BAL   @14,NPXFAREA                                        0289 00087000
*   /*****************************************************************/ 00088000
*   /*                                                               */ 00089000
*   /* REDEFINE THE PAGE FRAME TABLE AVAILABLE QUEUE                 */ 00090000
*   /*                                                               */ 00091000
*   /*****************************************************************/ 00092000
*                                                                  0290 00093000
*   CALL NPXPFTAQ;                                                 0290 00094000
*                                                                  0290 00095000
         BAL   @14,NPXPFTAQ                                        0290 00096000
*   /*****************************************************************/ 00097000
*   /*                                                               */ 00098000
*   /* RESET TEMPORARY HOOKS USED DURING NIP PROCESSING              */ 00099000
*   /*                                                               */ 00100000
*   /*****************************************************************/ 00101000
*                                                                  0291 00102000
*   CALL NPXRTRAP;                                                 0291 00103000
*                                                                  0291 00104000
         BAL   @14,NPXRTRAP                                        0291 00105000
*   /*****************************************************************/ 00106000
*   /*                                                               */ 00107000
*   /* WARNING: DYNAMIC ADDRESS TRANSLATION FEATURE IS DISABLED      */ 00108000
*   /* DURING NPXRDAT RESET DYNAMIC ADDRESS TRANSLATION TABLES SET UP*/ 00109000
*   /* PARAMETERS FOR THE MASTER SCHEDULER LINK TO IEEVIPL           */ 00110000
*   /*                                                               */ 00111000
*   /*****************************************************************/ 00112000
*                                                                  0292 00113000
*   CALL NPXRDAT;                                                  0292 00114000
*                                                                  0292 00115000
         BAL   @14,NPXRDAT                                         0292 00116000
*   /*****************************************************************/ 00117000
*   /*                                                               */ 00118000
*   /* END OF MAIN LINE CODE FOR IEAVNIPX                            */ 00119000
*   /*                                                               */ 00120000
*   /*****************************************************************/ 00121000
*                                                                  0293 00122000
*   RESPECIFY                                                      0293 00123000
*    (RNVT,                                                        0293 00124000
*     RCVT) UNRESTRICTED;                                          0293 00125000
*                                                                  0294 00126000
*/********************************************************************/ 00127000
*/*                                                                  */ 00128000
*/*          NPXOTEST - TEST FOR THE FOLLOWING ROUTINES IN THE       */ 00129000
*/*            LINK PACK AREA AND STORE ITS ADDRESS IN THE CVT       */ 00130000
*/*                                                                  */ 00131000
*/*            IFG019RA     SVC OPEN RESIDENT ROUTER                 */ 00132000
*/*            IDDWI        VAM WINDOW INTERCEPT ROUTINE             */ 00133000
*/*            IFG0TC0A     DM RESOURCE TERM ROUTINE                 */ 00134000
*/*            IDAVBPP1      VIRTUAL BLOCK PROCESSOR   @Z40WPXA      */ 00135000
*/*                                                                  */ 00136000
*/********************************************************************/ 00137000
*                                                                  0294 00138000
*NPXOTEST:                          /* NPXOTEST ENTRY POINT          */ 00139000
*   PROCEDURE;                                                     0294 00140000
@EL00001 L     @13,4(,@13)                                         0294 00141000
@EF00001 DS    0H                                                  0294 00142000
@ER00001 BR    @14                                                 0294 00143000
NPXOTEST STM   @14,@12,@SA00002                                    0294 00144000
*   RESPECIFY                                                      0295 00145000
*    (RNVT,                                                        0295 00146000
*     RCVT) RESTRICTED;                                            0295 00147000
*                                                                  0295 00148000
*   /*****************************************************************/ 00149000
*   /*                                                               */ 00150000
*   /* CALL NPXLPAFN TO SEARCH FOR MODULE IN LPA                     */ 00151000
*   /*                                                               */ 00152000
*   /*****************************************************************/ 00153000
*                                                                  0296 00154000
*   MODNAME=SVCORTR;                /* LOAD OPEN ROUTER ROUTINE NAME */ 00155000
         MVC   MODNAME(8),@CC00073                                 0296 00156000
*   CALL NPXLPAFN;                  /* SEARCH FOR MODULE IN LPA      */ 00157000
         BAL   @14,NPXLPAFN                                        0297 00158000
*   IF MODADDR=ZERO                 /* WAS MODULE FOUND              */ 00159000
*     THEN                          /* NO, MODULE WAS NOT FOUND      */ 00160000
         L     @12,MODADDR                                         0298 00161000
         LTR   @12,@12                                             0298 00162000
         BZ    @RT00298                                            0298 00163000
*     GO TO NPXMODNF;               /* GO WRITE ERROR MESSAGE        */ 00164000
*   ELSE                                                           0300 00165000
*     ;                             /* YES, MODULE FOUND             */ 00166000
*   CVTDMSR=MODADDR;                /* PUT MODULE ADDR INTO CVT      */ 00167000
         ST    @12,CVTDMSR(,RCVT)                                  0301 00168000
*   MODNAME=VAMWINR;                /* GET VAM WINDOW INTERCEPT    0302 00169000
*                                      ROUTINE NAME                  */ 00170000
         MVC   MODNAME(8),@CC00075                                 0302 00171000
*   CALL NPXLPAFN;                  /* SEARCH FOR MODULE IN LPA      */ 00172000
         BAL   @14,NPXLPAFN                                        0303 00173000
*   IF MODADDR=ZERO THEN            /* WAS MODULE FOUND              */ 00174000
         L     @12,MODADDR                                         0304 00175000
         LTR   @12,@12                                             0304 00176000
         BZ    @RT00304                                            0304 00177000
*     GO TO NPXMODNF;               /* NO, GO WRITE ERROR MESSAGE    */ 00178000
*   CVTPWI=MODADDR;                 /* PUT MODULE ADDR INTO CVT      */ 00179000
         ST    @12,CVTPWI(,RCVT)                                   0306 00180000
*   MODNAME=VIRTBLKP;               /* GET VIRTUAL BLOCK PROCESSOR 0307 00181000
*                                      ROUTINE NAME                  */ 00182000
         MVC   MODNAME(8),@CC00079                                 0307 00183000
*   CALL NPXLPAFN;                  /* SEARCH FOR MODULE IN LPA      */ 00184000
         BAL   @14,NPXLPAFN                                        0308 00185000
*   IF MODADDR=ZERO THEN            /* WAS MODULE FOUND              */ 00186000
         L     @12,MODADDR                                         0309 00187000
         LTR   @12,@12                                             0309 00188000
         BZ    @RT00309                                            0309 00189000
*     GO TO NPXMODNF;               /* NO, GO WRITE ERROR MESSAGE    */ 00190000
*   CVTPVBP=MODADDR;                /* PUT MODULE ADDR INTO CVT      */ 00191000
         ST    @12,CVTPVBP(,RCVT)                                  0311 00192000
*   MODNAME=DMTERM;                 /* GET DATA MGT RESOURCE       0312 00193000
*                                      TERMINATION ROUTINE NAME      */ 00194000
         MVC   MODNAME(8),@CC00077                                 0312 00195000
*   CALL NPXLPAFN;                  /* SEARCH FOR MODULE IN LPA      */ 00196000
         BAL   @14,NPXLPAFN                                        0313 00197000
*   IF MODADDR=ZERO THEN            /* WAS MODULE FOUND              */ 00198000
         L     @12,MODADDR                                         0314 00199000
         LTR   @12,@12                                             0314 00200000
         BZ    @RT00314                                            0314 00201000
*     GO TO NPXMODNF;               /* NO, GO WRITE ERROR MESSAGE    */ 00202000
*   CVT0TC0A=MODADDR;               /* PUT MODULE ADDR INTO CVT      */ 00203000
         ST    @12,CVT0TC0A(,RCVT)                                 0316 00204000
*   MODNAME=RPTROUT;                /* GET RPT ROUTINE NAME          */ 00205000
         MVC   MODNAME(8),@CC00081                                 0317 00206000
*   CALL NPXLPAFN;                  /* SEARCH FOR MODULE IN LPA      */ 00207000
         BAL   @14,NPXLPAFN                                        0318 00208000
*   IF MODADDR=ZERO THEN            /* WAS MODULE FOUND              */ 00209000
         L     @12,MODADDR                                         0319 00210000
         LTR   @12,@12                                             0319 00211000
         BZ    @RT00319                                            0319 00212000
*     GO TO NPXMODNF;               /* NO, GO WRITE ERROR MESSAGE    */ 00213000
*   CVTRPT=MODADDR;                 /* PUT MODULE ADDR INTO CVT      */ 00214000
         ST    @12,CVTRPT(,RCVT)                                   0321 00215000
*   RETURN;                         /* RETURN TO CALLER              */ 00216000
*                                                                  0322 00217000
@EL00002 DS    0H                                                  0322 00218000
@EF00002 DS    0H                                                  0322 00219000
@ER00002 LM    @14,@12,@SA00002                                    0322 00220000
         BR    @14                                                 0322 00221000
*   /*****************************************************************/ 00222000
*   /*                                                               */ 00223000
*   /* IEA363I MODULE NOT FOUND IN LPA                               */ 00224000
*   /*                                                               */ 00225000
*   /*****************************************************************/ 00226000
*                                                                  0323 00227000
*NPXMODNF:                                                         0323 00228000
*   MSGMODNM=MODNAME;                                              0323 00229000
NPXMODNF MVC   MSGMODNM(8),MODNAME                                 0323 00230000
*   GENERATE REFS(RNVT,NVTWTO,IEA363I) SETS(RPARM,REXIT,RENTRY)    0324 00231000
*       (IEAPMNIP TYPE=WTO,PARAM=IEA363I);                         0324 00232000
         IEAPMNIP TYPE=WTO,PARAM=IEA363I                                00233000
*   NVTFLWSC=WSC3B;                 /* SET WAIT STATE CODE           */ 00234000
*                                                                  0325 00235000
         MVI   NVTFLWSC(RNVT),X'3B'                                0325 00236000
*   /*****************************************************************/ 00237000
*   /*                                                               */ 00238000
*   /* LINK TO NIPSWAIT TO TERMINATE INITIALIZATION                  */ 00239000
*   /*                                                               */ 00240000
*   /*****************************************************************/ 00241000
*                                                                  0326 00242000
*   GENERATE REFS(RNVT,NVTSWAIT,NVTFLWSC) SETS(REXIT,RENTRY)       0326 00243000
*       (IEAPMNIP TYPE=SWAIT);                                     0326 00244000
         IEAPMNIP TYPE=SWAIT                                            00245000
*   RESPECIFY                                                      0327 00246000
*    (RNVT,                                                        0327 00247000
*     RCVT) UNRESTRICTED;                                          0327 00248000
*                                                                  0327 00249000
*/************************************************************       */ 00250000
*/* THIS ROUTINE DEFINES THE MASTER SCHEDULER'S REGION               */ 00251000
*/*                                                                  */ 00252000
*/*    INPUT                                                         */ 00253000
*/*       FIELD     CONTROL BLOCK                                    */ 00254000
*/*       CVTSHRVM  CVT                                              */ 00255000
*/*       NVTVVPG1  NVT                                              */ 00256000
*/*                                                                  */ 00257000
*/*    OUTPUT                                                        */ 00258000
*/*       FIELD     CONTROL BLOCK                                    */ 00259000
*/*       MPASIZE   NONE                                             */ 00260000
*/*       MPADDR    NONE                                             */ 00261000
*/*                 MASTER SCHEDULER PQE                             */ 00262000
*/*                 MASTER SCHEDULER FBQE                            */ 00263000
*/************************************************************       */ 00264000
*                                                                  0328 00265000
*NPXMPA:                                                           0328 00266000
*   ENTRY;                          /* INTERNAL ROUTINE              */ 00267000
         B     @EC00328                                            0328 00268000
NPXMPA   STM   @14,@12,@SA00002                                    0328 00269000
@EC00328 DS    0H                                                  0329 00270000
*   RESPECIFY                                                      0329 00271000
*    (RNVT,                                                        0329 00272000
*     RCVT,                                                        0329 00273000
*     RPQE,                                                        0329 00274000
*     SPQESPTR,                                                    0329 00275000
*     RTCB,                                                        0329 00276000
*     RSCAN,                                                       0329 00277000
*     RSIZE) RESTRICTED;                                           0329 00278000
*                                                                  0329 00279000
*   /*****************************************************************/ 00280000
*   /*                                                               */ 00281000
*   /* WARNING THE REMAINDER OF THIS MODULE MUST STAY DISABLED       */ 00282000
*   /*                                                               */ 00283000
*   /*****************************************************************/ 00284000
*                                                                  0330 00285000
*DISABLE:                                                          0330 00286000
*   RESPECIFY                                                      0330 00287000
*    (R11,                                                         0330 00288000
*     R12,                                                         0330 00289000
*     R13,                                                         0330 00290000
*     RENTRY,                                                      0330 00291000
*     R4,                                                          0330 00292000
*     R8) RESTRICTED;                                              0330 00293000
DISABLE  DS    0H                                                  0331 00294000
*   R4=R12;                         /* SAVE R12 ACROSS GETLOCK       */ 00295000
         LR    R4,R12                                              0331 00296000
*   R8=R13;                         /* SAVE R13 ACROSS GETLOCK       */ 00297000
         LR    R8,R13                                              0332 00298000
*   GENERATE REFS(PSALITA) SETS(REXIT,R11,R12,RENTRY,R13);         0333 00299000
*                                                                  0333 00300000
   SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(PGT,IEAVNIPX.FREEL)   00301000
  SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,RELATED=(PFTE,IEAVNIPX,FREEL)  00302000
*   /*****************************************************************/ 00303000
*   /*                                                               */ 00304000
*   /* DISABLE I/O AND EXTERNAL INTERUPTS                            */ 00305000
*   /*                                                               */ 00306000
*   /*****************************************************************/ 00307000
*                                                                  0334 00308000
*   GENERATE REFS(NPXSMASK) SETS(NPXSMASK)                         0334 00309000
*       (STNSM   NPXSMASK,B'11111100');/*                    @ZA28037*/ 00310000
*                                                                  0334 00311000
         STNSM   NPXSMASK,B'11111100'                                   00312000
*   /*****************************************************************/ 00313000
*   /*                                                               */ 00314000
*   /*                                                               */ 00315000
*   /*****************************************************************/ 00316000
*                                                                  0335 00317000
*   R12=R4;                         /* RESTORE R12                   */ 00318000
         LR    R12,R4                                              0335 00319000
*   R13=R8;                         /* RESTORE R13                   */ 00320000
         LR    R13,R8                                              0336 00321000
*   RESPECIFY                                                      0337 00322000
*    (R11,                                                         0337 00323000
*     R12,                                                         0337 00324000
*     R13,                                                         0337 00325000
*     RENTRY,                                                      0337 00326000
*     R4,                                                          0337 00327000
*     R8) UNRESTRICTED;                                            0337 00328000
*   NVTMASCB->ASCBLDA->CURRGNTP=CVTGDA->PASTRT+SYSREGSZ;/* SET ADDR     00329000
*                                      OF BEGINNING OF REGION PLUS 0338 00330000
*                                      SIZE OF MASTER SYSTEM REGION     00331000
*                                                            @Z30ENXA*/ 00332000
*                                                                  0338 00333000
         L     @12,NVTMASCB(,RNVT)                                 0338 00334000
         L     @12,ASCBLDA(,@12)                                   0338 00335000
         L     @11,CVTGDA(,RCVT)                                   0338 00336000
         L     @08,@CF00059                                        0338 00337000
         AL    @08,PASTRT(,@11)                                    0338 00338000
         ST    @08,CURRGNTP(,@12)                                  0338 00339000
*   /*****************************************************************/ 00340000
*   /*                                                               */ 00341000
*   /* FREE TEMPORY ASVT USED DURING NIP                             */ 00342000
*   /*                                                               */ 00343000
*   /*****************************************************************/ 00344000
*                                                                  0339 00345000
*   RPARM0=24;                      /* LENGTH OF TEMP ASVT           */ 00346000
         LA    RPARM0,24                                           0339 00347000
*   RPARM=CVTASVT+512;              /* ADDR OF TEMP ASVT             */ 00348000
         LA    RPARM,512                                           0340 00349000
         AL    RPARM,CVTASVT(,RCVT)                                0340 00350000
*   CALL NPXFMAIN;                  /* FREE OLD ASVT                 */ 00351000
         BAL   @14,NPXFMAIN                                        0341 00352000
*   CVTASVT=NVTFASVT;               /* HOOK UP REAL ASVT             */ 00353000
         MVC   CVTASVT(4,RCVT),NVTFASVT(RNVT)                      0342 00354000
*   CVTREAL=NVTREALR;               /* SET V=R BEGINNING ADDR        */ 00355000
         MVC   CVTREAL(4,RCVT),NVTREALR(RNVT)                      0343 00356000
*   RTCB=NVTMSTCB;                  /* ADDR. OF MASTER SCHEDULER TCB */ 00357000
         L     RTCB,NVTMSTCB(,RNVT)                                0344 00358000
*   RPQE=TCBPQE;                    /* GET DUMMY PQE ADDR.           */ 00359000
         L     RPQE,TCBPQE(,RTCB)                                  0345 00360000
*   CALL NPXFPQE;                   /* CALL ROUTINE TO FREE DUMMY  0346 00361000
*                                      PQE, PQE, AND ALL FBQES FOR 0346 00362000
*                                      NIP V=R REGION                */ 00363000
         BAL   @14,NPXFPQE                                         0346 00364000
*   RPQE=NVTVPQE;                   /* GET DUMMY PQE ADDR OF NIP V=V    00365000
*                                      REGION                        */ 00366000
         L     RPQE,NVTVPQE(,RNVT)                                 0347 00367000
*   CALL NPXFPQE;                   /* CALL ROUTINE TO FREE DUMMY  0348 00368000
*                                      PQE, PQE AND ALL FBQES FOR NIP   00369000
*                                      V=V REGION                    */ 00370000
         BAL   @14,NPXFPQE                                         0348 00371000
*   RTCB=NVTMSTCB;                  /* ADDR OF MASTER SCHED TCB      */ 00372000
         L     RTCB,NVTMSTCB(,RNVT)                                0349 00373000
*   SPQESPTR=TCBMSS;                /* GET ADDR OF SPQE CHAIN OF NIP    00374000
*                                      V=R REGION                    */ 00375000
         L     SPQESPTR,TCBMSS(,RTCB)                              0350 00376000
*   CALL NPXFSPQE;                  /* CALL ROUTINE TO FREE SPQE,  0351 00377000
*                                      DQE, AND FQE FOR NIP V=R    0351 00378000
*                                      REGION                        */ 00379000
         BAL   @14,NPXFSPQE                                        0351 00380000
*   TCBMSS=ZERO;                    /* ZERO START OF SPQE CHAIN      */ 00381000
         SLR   @12,@12                                             0352 00382000
         ST    @12,TCBMSS(,RTCB)                                   0352 00383000
*   SPQESPTR=NVTVMSS;               /* GET ADDR OF SPQE CHAIN OF NIP    00384000
*                                      V=V REGION                    */ 00385000
         L     SPQESPTR,NVTVMSS(,RNVT)                             0353 00386000
*   CALL NPXFSPQE;                  /* CALL ROUTINE TO FREE SPQE,  0354 00387000
*                                      DQE, AND FQE FOR NIP V=V    0354 00388000
*                                      REGION                        */ 00389000
         BAL   @14,NPXFSPQE                                        0354 00390000
*   RETURN;                         /* RETURN TO MAINLINE            */ 00391000
         B     @EL00002                                            0355 00392000
*   RESPECIFY                                                      0356 00393000
*    (RNVT,                                                        0356 00394000
*     RCVT,                                                        0356 00395000
*     RPQE,                                                        0356 00396000
*     SPQESPTR,                                                    0356 00397000
*     RTCB,                                                        0356 00398000
*     RSCAN,                                                       0356 00399000
*     RSIZE) UNRESTRICTED;                                         0356 00400000
*                                                                  0357 00401000
*/************************************************************       */ 00402000
*/*   SUBROUTINE TO MOVE TRACE TABLE TO SQA                          */ 00403000
*/************************************************************       */ 00404000
*/*    INPUT                                                         */ 00405000
*/*       RNVT      ADDRESS OF NVT                                   */ 00406000
*/*       RCVT      ADDRESS OF CVT                                   */ 00407000
*/************************************************************       */ 00408000
*                                                                  0357 00409000
*NPXTRACE:                                                         0357 00410000
*   ENTRY;                                                         0357 00411000
NPXTRACE STM   @14,@12,@SA00002                                    0357 00412000
*   RESPECIFY                                                      0358 00413000
*    (RNVT,                                                        0358 00414000
*     RCVT,                                                        0358 00415000
*     RSIZE,                                                       0358 00416000
*     RPARM,                                                       0358 00417000
*     RSCAN,                                                       0358 00418000
*     RCURR) RESTRICTED;                                           0358 00419000
*   RESPECIFY                                                      0359 00420000
*     TRHDR BASED(RPARM);                                          0359 00421000
*   IF NVTTRACE=ZERO THEN                                          0360 00422000
         LH    @12,NVTTRACE(,RNVT)                                 0360 00423000
         LTR   @12,@12                                             0360 00424000
         BZ    @RT00360                                            0360 00425000
*     RETURN;                       /* RETURN IF NO TRACE TABLE    0361 00426000
*                                      SPECIFIED                     */ 00427000
*   CVTTRACE=CVTTRACE&TRACEOFF;     /* NO-OP TRACE FUNCTION  @YM08212*/ 00428000
         NC    CVTTRACE(2,RCVT),@CB00099                           0362 00429000
*   RSIZE=(NVTTRACE+2)*LENGTH(OLDTRACE);/* GET SIZE OF TRACE TABLE 0363 00430000
*                                      FROM NVT, ADD 2 ENTRIES FOR 0363 00431000
*                                      HEADER AND ROUNDING           */ 00432000
         AH    @12,@CH00241                                        0363 00433000
         SLA   @12,5                                               0363 00434000
         LR    RSIZE,@12                                           0363 00435000
*   R5=RSIZE;                       /* SIZE FOR GETMAIN              */ 00436000
         LR    R5,RSIZE                                            0364 00437000
*   R4=NVTMSTCB;                    /* SET UP TCB ADDR FOR BRANCH  0365 00438000
*                                      ENTERED GETMAIN               */ 00439000
         L     R4,NVTMSTCB(,RNVT)                                  0365 00440000
*   R7=NVTMASCB;                    /* SET UP ASCB ADDR FOR BRANCH 0366 00441000
*                                      ENTERED GETMAIN               */ 00442000
         L     R7,NVTMASCB(,RNVT)                                  0366 00443000
*/*  GETMAIN MAY RESULT IN THE TRACE TABLE EXTENDING INTO ASM'S CSA,    00444000
*  AND THIS IS THE REASON WHY TRACE MUST BE OFF WHEN THE TRACE HDR IS   00445000
*  UPDATED                                                   @YM08212*/ 00446000
*                                                                  0367 00447000
