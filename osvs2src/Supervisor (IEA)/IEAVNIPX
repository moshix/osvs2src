         AH    @12,RSMCNTFX(,RSMHDPTR)                             0633 01649000
         STH   @12,RSMCNTFX(,RSMHDPTR)                             0633 01650000
*         ELSE                                                     0634 01651000
*           ;                       /* ELSE CONTINUE LOOP THRU PFTES    01652000
