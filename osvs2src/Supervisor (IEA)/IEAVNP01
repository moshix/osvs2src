         TITLE 'IEAVNP01 - CONSOLE INITIALIZATION ROUTINE              *00001000
                        '                                               00002000
IEAVNP01 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(19)                                             0001 00007000
         DC    C'IEAVNP01R0300076040'                              0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                       00014000
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF          00015000
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES      00016000
** WILL ASSEMBLE PROPERLY.                                              00017000
*   NPBTAPAD=CVTXAPG;               /* PUT IO-APNDG ADDR IN TEST DEB */ 00018000
*                                                                  0133 00019000
         MVC   NPBTAPAD(3),CVTXAPG+1(RCVT)                         0133 00020000
*   /*****************************************************************/ 00021000
*   /*                                                               */ 00022000
*   /* TESTING OF CONSOLE AVAILABILITY IS BEGUN WITH THE CONSOLE     */ 00023000
*   /* SELECTED AS MASTER DURING SYSTEM GENERATION.                  */ 00024000
*   /*                                                               */ 00025000
*   /*****************************************************************/ 00026000
*                                                                  0134 00027000
*   UCMBASPT=CVTCUCB;               /* OBTAIN POINTER TO UCM BASE    */ 00028000
         L     UCMBASPT,CVTCUCB(,RCVT)                             0134 00029000
*   UCMDVEPT=UCMMCENT;              /* POINTER TO MASTER CONSOLE MAP */ 00030000
         LR    @08,UCMBASPT                                        0135 00031000
         AL    @08,@CF02045                                        0135 00032000
         L     @08,UCMMCSPT(,@08)                                  0135 00033000
         L     UCMDVEPT,UCMMCENT(,@08)                             0135 00034000
*   MSTRCNSL=UCMDVEPT;              /* SAVE SYS GEN MASTER POINTER   */ 00035000
         LR    MSTRCNSL,UCMDVEPT                                   0136 00036000
*NP1ALT:                                                           0137 00037000
*   UCMENTRY=UCMDVEPT;              /* SAVE UCM ENTRY ADDRESS        */ 00038000
NP1ALT   LR    UCMENTRY,UCMDVEPT                                   0137 00039000
*NP1BLT:                            /* THIS ROUTINE CONDUCTS CONSOLE    00040000
*                                      AVAILABILITY TESTING          */ 00041000
*   RPARM=UCMUCB;                   /* GET EBCDIC UNIT ADDRESS       */ 00042000
NP1BLT   L     RPARM,UCMUCB(,UCMDVEPT)                             0138 00043000
*   IF UCMDISPK='1'B                /* IF JES3 DUMMY CONS    @ZA05194*/ 00044000
*     THEN                          /* THEN DONT CALL NIPM   @ZA05194*/ 00045000
         TM    UCMDISPK(UCMDVEPT),B'00100000'                      0139 00046000
         BO    @RT00139                                            0139 00047000
*     GO TO NP1CMNT;                /* GET NEXT CHECK END    @ZA05194*/ 00048000
*                                                                  0140 00049000
*   /*****************************************************************/ 00050000
*   /*                                                               */ 00051000
*   /* CALL IEAVNIPM UCB FIND ROUTINE                                */ 00052000
*   /*                                                               */ 00053000
*   /*****************************************************************/ 00054000
*                                                                  0141 00055000
*   GEN(IEAPMNIP  TYPE=UCBFN,PARAM=RPARM)                          0141 00056000
*   REFS(NVTUCBFN);                                                0141 00057000
         IEAPMNIP  TYPE=UCBFN,PARAM=RPARM                               00058000
*   UCBPTR=RPARM;                   /* STORE UCB ADDRESS - THIS UNIT */ 00059000
         LR    UCBPTR,RPARM                                        0142 00060000
*   UCMUCB=UCBPTR;                  /* STORE UCB ADDRESS IN UCM      */ 00061000
         ST    UCBPTR,UCMUCB(,UCMDVEPT)                            0143 00062000
*   UCMXOR='01'X;                   /* FLAG UCM ENTRY AS TESTED      */ 00063000
*                                                                  0144 00064000
         MVI   UCMXOR(UCMDVEPT),X'01'                              0144 00065000
*   /*****************************************************************/ 00066000
*   /*                                                               */ 00067000
*   /* CHECK FOR UCB MARKED ONLINE AND READY                 @ZA01929*/ 00068000
*   /*                                                               */ 00069000
*   /*****************************************************************/ 00070000
*                                                                  0145 00071000
*   IF UCBONLI='1'B&                /* UCB MARKED ONLINE AND @ZA01929*/ 00072000
*       UCBDUC^='1'B&               /* UNIT CHECK IND.       @ZA04124*/ 00073000
*       UCBNRY^='1'B                /* DEVICE READY ?        @ZA01929*/ 00074000
*     THEN                          /* IF SO, BRANCH TO --           */ 00075000
         TM    UCBONLI(UCBPTR),B'10000000'                         0145 00076000
         BNO   @RF00145                                            0145 00077000
         TM    UCBDUC(UCBPTR),B'00100000'                          0145 00078000
         BO    @RF00145                                            0145 00079000
         TM    UCBNRY(UCBPTR),B'01000000'                          0145 00080000
         BNO   @RT00145                                            0145 00081000
*     GO TO NP1INIT;                /* ACTIVE CONSOLE INITIALIZATION */ 00082000
*NP1RSTR:                           /* THIS ROUTINE PREPARES FOR NEXT   00083000
*                                      CONSOLE TEST                  */ 00084000
*   UCMDVEPT=UCMENTRY;              /* RESTORE UCM ENTRY ADDRESS     */ 00085000
@RF00145 DS    0H                                                  0147 00086000
NP1RSTR  LR    UCMDVEPT,UCMENTRY                                   0147 00087000
*   COMPSWH='0'B;                   /* RESET COMPOSITE CONSOLE SWITCH*/ 00088000
*                                                                  0148 00089000
         NI    COMPSWH,B'11101111'                                 0148 00090000
*   /*****************************************************************/ 00091000
*   /*                                                               */ 00092000
*   /* CHECK FOR MASTER/ALTERNATE CONSOLE QUEUE SEARCH COMPLETE      */ 00093000
*   /*                                                               */ 00094000
*   /*****************************************************************/ 00095000
*                                                                  0149 00096000
*   IF ALTSRCH='1'B                 /* ALTERNATE SEARCH COMPLETED ?  */ 00097000
*     THEN                          /* IF SO -                       */ 00098000
         TM    ALTSRCH,B'00100000'                                 0149 00099000
         BO    @RT00149                                            0149 00100000
*     GO TO NP1CMNT;                /* CONTINUE UCM ENTRIES CHECK    */ 00101000
*                                                                  0150 00102000
*   /*****************************************************************/ 00103000
*   /*                                                               */ 00104000
*   /* CHECK FOR NEXT ALTERNATE CONSOLE ADDRESS                      */ 00105000
*   /*                                                               */ 00106000
*   /*****************************************************************/ 00107000
*                                                                  0151 00108000
*   IF UCMALTEN=0                   /* NO MASTER ALTERNATE CONSOLE ? */ 00109000
*     THEN                          /* IF NONE, THEN -               */ 00110000
         L     @08,UCMALTEN(,UCMDVEPT)                             0151 00111000
         LTR   @08,@08                                             0151 00112000
         BZ    @RT00151                                            0151 00113000
*     GO TO NP1UCMS;                /* BRANCH TO UCM SEARCH ROUTINE  */ 00114000
*                                                                  0152 00115000
*   /*****************************************************************/ 00116000
*   /*                                                               */ 00117000
*   /* CHECK THAT THIS ALTERNATE IS UNTESTED                         */ 00118000
*   /*                                                               */ 00119000
*   /*****************************************************************/ 00120000
*                                                                  0153 00121000
*   IF UCMALTEN->UCMXOR='01'X       /* IF ALTERNATE UNIT IS          */ 00122000
*     THEN                          /* ALREADY TESTED, THEN -        */ 00123000
         CLI   UCMXOR(@08),X'01'                                   0153 00124000
         BE    @RT00153                                            0153 00125000
*     GO TO NP1UCMS;                /* BRANCH TO UCM SEARCH ROUTINE  */ 00126000
*   UCMDVEPT=UCMALTEN;              /* SET POINTER TO CONSOLE MAP    */ 00127000
         LR    UCMDVEPT,@08                                        0155 00128000
*   GO TO NP1ALT;                   /* BRANCH TO CONSOLE TEST ROUTINE*/ 00129000
         B     NP1ALT                                              0156 00130000
*NP1UCMS:                           /* ALTERNATE SEARCH COMPLETED- 0157 00131000
*                                      PREPARE TO SEARCH UCM TABLE   */ 00132000
*   ALTSRCH='1'B;                   /* SET ALTERNATE SEARCH SWTCH OFF*/ 00133000
NP1UCMS  OI    ALTSRCH,B'00100000'                                 0157 00134000
*   UCMDVEPT=UCMVEA;                /* SET PTR TO 1ST ENTRY IN UCM   */ 00135000
         L     UCMDVEPT,UCMVEA(,UCMBASPT)                          0158 00136000
*NP1XOR:                            /* CHECK FOR THIS UNIT ALREADY 0159 00137000
*                                      TESTED                        */ 00138000
*   IF UCMXOR='00'X                 /* UNIT NOT TESTED ?             */ 00139000
*     THEN                          /* IF NOT -                      */ 00140000
NP1XOR   CLI   UCMXOR(UCMDVEPT),X'00'                              0159 00141000
         BE    @RT00159                                            0159 00142000
*     GO TO NP1ALT;                 /* BRANCH TO CONSOLE TEST ROUTINE*/ 00143000
*NP1CMNT:                                                          0161 00144000
*   UCMDVEPT=UCMDVEPT+UCMVEZ;       /* INCREMENT TO NEXT ENTRY       */ 00145000
*                                                                  0161 00146000
NP1CMNT  AL    UCMDVEPT,UCMVEZ(,UCMBASPT)                          0161 00147000
*   /*****************************************************************/ 00148000
*   /*                                                               */ 00149000
*   /* CHECK FOR END OF UCM TABLE                                    */ 00150000
*   /*                                                               */ 00151000
*   /*****************************************************************/ 00152000
*                                                                  0162 00153000
*   IF UCMDVEPT^>UCMVEL             /* PAST END OF UCM ENTRIES ?     */ 00154000
*     THEN                          /* IF NOT -                      */ 00155000
         C     UCMDVEPT,UCMVEL(,UCMBASPT)                          0162 00156000
         BNH   @RT00162                                            0162 00157000
*     GO TO NP1XOR;                 /* LOOP THRU TESTS AGAIN         */ 00158000
*                                                                  0163 00159000
*   /*****************************************************************/ 00160000
*   /*                                                               */ 00161000
*   /* CHECK FOR MASTER CONSOLE ALREADY DETERMINED                   */ 00162000
*   /*                                                               */ 00163000
*   /*****************************************************************/ 00164000
*                                                                  0164 00165000
*   IF MSTCNFD='0'B                 /* NO MASTER CONSOLE FOUND ?     */ 00166000
*     THEN                          /* IF NOT -                      */ 00167000
         TM    MSTCNFD,B'01000000'                                 0164 00168000
         BNZ   @RF00164                                            0164 00169000
*     DO;                           /* INITIALIZATION CANNOT CONTINUE*/ 00170000
*       NVTFLWSC=NVTFLWNC;          /* SET WAIT STATE CODE           */ 00171000
*                                                                  0166 00172000
         MVI   NVTFLWSC(RNVT),X'07'                                0166 00173000
*       /*************************************************************/ 00174000
*       /*                                                           */ 00175000
*       /* EXIT TO THE NIPSWAIT RTN (OF IEAVNIPM)                    */ 00176000
*       /*                                                           */ 00177000
*       /*************************************************************/ 00178000
*                                                                  0167 00179000
*       GEN(IEAPMNIP  TYPE=SWAIT)                                  0167 00180000
*       REFS(NVTSWAIT);                                            0167 00181000
         IEAPMNIP  TYPE=SWAIT                                           00182000
*     END;                                                         0168 00183000
*   UCMDVEPT=UCMVEA;                /* RESET PTR TO 1ST ENTRY IN UCM */ 00184000
@RF00164 L     UCMDVEPT,UCMVEA(,UCMBASPT)                          0169 00185000
*NP1RXOR:                           /* RESET UNIT TESTED FLAG FOR ALL   00186000
*                                      UCM ENTRIES                   */ 00187000
*   UCMXOR='00'X;                   /* TURN OFF UNIT TESTED FLAG     */ 00188000
NP1RXOR  MVI   UCMXOR(UCMDVEPT),X'00'                              0170 00189000
*   UCMDVEPT=UCMDVEPT+UCMVEZ;       /* INCREMENT TO NEXT ENTRY       */ 00190000
*                                                                  0171 00191000
         AL    UCMDVEPT,UCMVEZ(,UCMBASPT)                          0171 00192000
*   /*****************************************************************/ 00193000
*   /*                                                               */ 00194000
*   /* CHECK FOR END OF UCM TABLE                                    */ 00195000
*   /*                                                               */ 00196000
*   /*****************************************************************/ 00197000
*                                                                  0172 00198000
*   IF UCMDVEPT^>UCMVEL             /* PAST LAST UCM ENTRY ?         */ 00199000
*     THEN                          /* IF NOT -                      */ 00200000
         C     UCMDVEPT,UCMVEL(,UCMBASPT)                          0172 00201000
         BNH   @RT00172                                            0172 00202000
*     GO TO NP1RXOR;                /* LOOP THRU FLAG-RESET ROUTINE  */ 00203000
*   RETURN;                         /* RETURN TO CALLER (IEAVNIPM)   */ 00204000
@EL00001 DS    0H                                                  0174 00205000
@EF00001 DS    0H                                                  0174 00206000
@ER00001 LM    @14,@12,12(@13)                                     0174 00207000
         BR    @14                                                 0174 00208000
*NP1INIT:                           /* THIS ROUTINE BEGINS ACIVE   0175 00209000
*                                      CONSOLE INITIALIZATION        */ 00210000
*                                                                  0175 00211000
*   /*****************************************************************/ 00212000
*   /*                                                               */ 00213000
*   /* CHECK FOR A COMPOSITE CONSOLE                                 */ 00214000
*   /*                                                               */ 00215000
*   /*****************************************************************/ 00216000
*                                                                  0175 00217000
*   IF UCMCOMPC=0                   /* COMPOSITE CONSOLE ?           */ 00218000
*     THEN                          /* IF NOT -                      */ 00219000
NP1INIT  L     @11,UCMCOMPC(,UCMDVEPT)                             0175 00220000
         LTR   @11,@11                                             0175 00221000
         BZ    @RT00175                                            0175 00222000
*     GO TO NP1ACT;                 /* BRANCH TO FLAG-AS-ACTIVE RTN  */ 00223000
*                                                                  0176 00224000
*   /*****************************************************************/ 00225000
*   /*                                                               */ 00226000
*   /* CHECK FOR SWITCH INDICATING BOTH HALVES JUST TESTED           */ 00227000
*   /*                                                               */ 00228000
*   /*****************************************************************/ 00229000
*                                                                  0177 00230000
*   IF COMPSWH='1'B                 /* COMPOSITE CONSOLE SWITCH ON ? */ 00231000
*     THEN                          /* IF SO -                       */ 00232000
         TM    COMPSWH,B'00010000'                                 0177 00233000
         BNO   @RF00177                                            0177 00234000
*     DO;                           /* SET IT AS ACTIVE :            */ 00235000
*       COMPSWH='0'B;               /* SET COMP CONSOLE SWITCH OFF   */ 00236000
*                                                                  0179 00237000
         NI    COMPSWH,B'11101111'                                 0179 00238000
*       /*************************************************************/ 00239000
*       /*                                                           */ 00240000
*       /* CHECK FOR OUTPUT HALF OF COMPOSITE CONSOLE                */ 00241000
*       /*                                                           */ 00242000
*       /*************************************************************/ 00243000
*                                                                  0180 00244000
*       IF UCMOF='1'B               /* THIS HALF OUTPUT ?            */ 00245000
*         THEN                      /* IF SO, SET POINTERS TO        */ 00246000
         TM    UCMOF(UCMDVEPT),B'10000000'                         0180 00247000
         BNO   @RF00180                                            0180 00248000
*         DO;                       /* INPUT UCB, UCM                */ 00249000
*           RWORK=UCBSAVED;         /* SET POINTER TO UCB            */ 00250000
         L     RWORK,UCBSAVED                                      0182 00251000
*           UCBSAVED=UCBPTR;        /* FOR INPUT HALF                */ 00252000
         ST    UCBPTR,UCBSAVED                                     0183 00253000
*           UCBPTR=RWORK;           /* OF THIS CONSOLE               */ 00254000
         LR    UCBPTR,RWORK                                        0184 00255000
*           RWORK=UCMSAVED;         /* SET POINTER TO UCM            */ 00256000
         L     RWORK,UCMSAVED                                      0185 00257000
*           UCMSAVED=UCMDVEPT;      /* ENTRY FOR INPUT HALF          */ 00258000
         ST    UCMDVEPT,UCMSAVED                                   0186 00259000
*           UCMDVEPT=RWORK;         /* OF THIS CONSOLE               */ 00260000
         LR    UCMDVEPT,RWORK                                      0187 00261000
*         END;                                                     0188 00262000
*                                                                  0188 00263000
*       /*************************************************************/ 00264000
*       /*                                                           */ 00265000
*       /* SET ACTIVE FLAG IN UCBSTAT OF OUTPUT UCB                  */ 00266000
*       /*                                                           */ 00267000
*       /*************************************************************/ 00268000
*                                                                  0189 00269000
*       UCBSAVED->UCBSYSR='1'B;                                    0189 00270000
*                                                                  0189 00271000
@RF00180 L     @11,UCBSAVED                                        0189 00272000
         OI    UCBSYSR(@11),B'00000010'                            0189 00273000
*       /*************************************************************/ 00274000
*       /*                                                           */ 00275000
*       /* SET DEVICE ACTIVE FLAG IN OUTPUT UCME                     */ 00276000
*       /*                                                           */ 00277000
*       /*************************************************************/ 00278000
*                                                                  0190 00279000
*       UCMSAVED->UCMUF='1'B;                                      0190 00280000
*                                                                  0190 00281000
         L     @11,UCMSAVED                                        0190 00282000
         OI    UCMUF(@11),B'00010000'                              0190 00283000
*       /*************************************************************/ 00284000
*       /*                                                           */ 00285000
*       /* SET OPEN PENDING FLAG IN OUTPUT UCME                      */ 00286000
*       /*                                                           */ 00287000
*       /*************************************************************/ 00288000
*                                                                  0191 00289000
*       UCMSAVED->UCMTA='1'B;                                      0191 00290000
*                                                                  0191 00291000
         OI    UCMTA(@11),B'00001000'                              0191 00292000
*       /*************************************************************/ 00293000
*       /*                                                           */ 00294000
*       /* HAS MASTER CONSOLE ALREADY BEEN FOUND                     */ 00295000
*       /*                                                           */ 00296000
*       /*************************************************************/ 00297000
*                                                                  0192 00298000
*       IF MSTCNFD='1'B             /* MASTER FOUND                  */ 00299000
*         THEN                                                     0192 00300000
         TM    MSTCNFD,B'01000000'                                 0192 00301000
         BNO   @RF00192                                            0192 00302000
*         IF UCMSAVED->UCMALTEN=0&UCMDVEPT^=MSTRCNSL/* CONSOLE HAS NO   00303000
*                                      ALT AND IS NOT MASTER         */ 00304000
*           THEN                                                   0193 00305000
         SLR   @04,@04                                             0193 00306000
         C     @04,UCMALTEN(,@11)                                  0193 00307000
         BNE   @RF00193                                            0193 00308000
         CR    UCMDVEPT,MSTRCNSL                                   0193 00309000
         BE    @RF00193                                            0193 00310000
*           IF MSTRCNSL->UCMCOMPC=0 /* IS MASTER A COMPOSITE         */ 00311000
*             THEN                  /* NO SET ALTERNATE TO MASTER    */ 00312000
         C     @04,UCMCOMPC(,MSTRCNSL)                             0194 00313000
         BNE   @RF00194                                            0194 00314000
*             UCMSAVED->UCMALTEN=MSTRCNSL;                         0195 00315000
         ST    MSTRCNSL,UCMALTEN(,@11)                             0195 00316000
*           ELSE                    /* YES SET ALTERNATE TO OUTPUT 0196 00317000
*                                      HALF OF MASTER                */ 00318000
*             UCMSAVED->UCMOAOEN=MSTRCNSL->UCMCOMPC;               0196 00319000
         B     @RC00194                                            0196 00320000
@RF00194 L     @11,UCMCOMPC(,MSTRCNSL)                             0196 00321000
         L     @04,UCMSAVED                                        0196 00322000
         ST    @11,UCMOAOEN(,@04)                                  0196 00323000
*       GO TO NP1ACT;               /* BRANCH TO FLAG-AS-ACTIVE RTN  */ 00324000
         B     NP1ACT                                              0197 00325000
*     END;                                                         0198 00326000
*                                                                  0198 00327000
*   /*****************************************************************/ 00328000
*   /*                                                               */ 00329000
*   /* CHECK FOR OTHER HALF OF COMPOSITE TESTED                      */ 00330000
*   /*                                                               */ 00331000
*   /*****************************************************************/ 00332000
*                                                                  0199 00333000
*   IF UCMCOMPC->UCMXOR='01'X       /* OTHER HALF TESTED ?           */ 00334000
*     THEN                          /* IF SO -                       */ 00335000
@RF00177 L     @11,UCMCOMPC(,UCMDVEPT)                             0199 00336000
         CLI   UCMXOR(@11),X'01'                                   0199 00337000
         BE    @RT00199                                            0199 00338000
*     GO TO NP1RSTR;                /* BRANCH TO CHECK NEXT CONSOLE  */ 00339000
*   UCMSAVED=UCMDVEPT;              /* SAVE PTR TO OTHER HALF'S UCM  */ 00340000
         ST    UCMDVEPT,UCMSAVED                                   0201 00341000
*   UCBSAVED=UCBPTR;                /* SAVE PTR TO OTHER HALF'S UCB  */ 00342000
         ST    UCBPTR,UCBSAVED                                     0202 00343000
*   UCMDVEPT=UCMCOMPC;              /* SET UCM ENTRY ADDRESS         */ 00344000
         LR    UCMDVEPT,@11                                        0203 00345000
*   COMPSWH='1'B;                   /* SET COMP CONSOLE SWITCH ON    */ 00346000
         OI    COMPSWH,B'00010000'                                 0204 00347000
*   GO TO NP1BLT;                   /* BRANCH TO CONSOLE-TEST ROUTINE*/ 00348000
         B     NP1BLT                                              0205 00349000
*NP1HCPY:                                                          0206 00350000
*   UCMSYSB='1'B;                   /* SET HARDCOPY REQUIRED FLAG    */ 00351000
NP1HCPY  LR    @11,UCMBASPT                                        0206 00352000
         AL    @11,@CF02045                                        0206 00353000
         L     @11,UCMMCSPT(,@11)                                  0206 00354000
         OI    UCMSYSB(@11),B'01000000'                            0206 00355000
*   GO TO NP1RSTR;                  /* BRANCH TO CHECK NEXT CONSOLE  */ 00356000
         B     NP1RSTR                                             0207 00357000
*NP1ACT:                                                           0208 00358000
*   UCBSYSR='1'B;                   /* SET ACTIVE CONSOLE FLAG IN UCB*/ 00359000
NP1ACT   OI    UCBSYSR(UCBPTR),B'00000010'                         0208 00360000
*   UCMTA='1'B;                     /* SET ACTIVE FLAG IN UCM        */ 00361000
         OI    UCMTA(UCMDVEPT),B'00001000'                         0209 00362000
*   UCMUF='1'B;                     /* SET OPEN PENDING FLAG IN UCM  */ 00363000
*                                                                  0210 00364000
         OI    UCMUF(UCMDVEPT),B'00010000'                         0210 00365000
*   /*****************************************************************/ 00366000
*   /*                                                               */ 00367000
*   /* IS MASTER CONSOLE ALREADY FOUND ?                             */ 00368000
*   /*                                                               */ 00369000
*   /*****************************************************************/ 00370000
*                                                                  0211 00371000
*   IF MSTCNFD='1'B                 /* MASTER CONSOLE ALREADY FOUND ?*/ 00372000
*     THEN                          /* IF SO -                       */ 00373000
         TM    MSTCNFD,B'01000000'                                 0211 00374000
         BNO   @RF00211                                            0211 00375000
*     DO;                           /* DOES CONSOLE HAVE ALTERNATE   */ 00376000
*       IF UCMALTEN=0&UCMDVEPT^=MSTRCNSL THEN                      0213 00377000
         SLR   @11,@11                                             0213 00378000
         C     @11,UCMALTEN(,UCMDVEPT)                             0213 00379000
         BNE   @RF00213                                            0213 00380000
         CR    UCMDVEPT,MSTRCNSL                                   0213 00381000
         BE    @RF00213                                            0213 00382000
*         DO;                                                      0214 00383000
*           UCMALTEN=MSTRCNSL;                                     0215 00384000
*                                                                  0215 00385000
         ST    MSTRCNSL,UCMALTEN(,UCMDVEPT)                        0215 00386000
*           /*********************************************************/ 00387000
*           /*                                                       */ 00388000
*           /* SET ALTERNATE TO MASTER                               */ 00389000
*           /*                                                       */ 00390000
*           /*********************************************************/ 00391000
*                                                                  0216 00392000
*           IF MSTRCNSL->UCMCOMPC^=0                               0216 00393000
*                                                                  0216 00394000
*           /*********************************************************/ 00395000
*           /*                                                       */ 00396000
*           /* IS MASTER A COMPOSITE                                 */ 00397000
*           /*                                                       */ 00398000
*           /*********************************************************/ 00399000
*                                                                  0216 00400000
*             THEN                  /* MASTER IS NOT COMPOSITE       */ 00401000
         L     @08,UCMCOMPC(,MSTRCNSL)                             0216 00402000
         CR    @08,@11                                             0216 00403000
         BE    @RF00216                                            0216 00404000
*             UCMOAOEN=MSTRCNSL->UCMCOMPC;                         0217 00405000
*                                                                  0217 00406000
         ST    @08,UCMOAOEN(,UCMDVEPT)                             0217 00407000
*           /*********************************************************/ 00408000
*           /*                                                       */ 00409000
*           /* POINT TO OUTPUT HALF OF MSTR                          */ 00410000
*           /*                                                       */ 00411000
*           /*********************************************************/ 00412000
*                                                                  0218 00413000
*         END;                                                     0218 00414000
*       GO TO NP1HCPY;              /* BRANCH TO SET HARDCOPY FLAG   */ 00415000
         B     NP1HCPY                                             0219 00416000
*     END;                                                         0220 00417000
*                                                                  0220 00418000
*   /*****************************************************************/ 00419000
*   /*                                                               */ 00420000
*   /* IS THIS AN OUTPUT ONLY CONSOLE ?                              */ 00421000
*   /*                                                               */ 00422000
*   /*****************************************************************/ 00423000
*                                                                  0221 00424000
*   IF UCMDISPD='1'B                /* THIS CONSOLE OUTPUT ONLY ?    */ 00425000
*     THEN                          /* IF SO -                       */ 00426000
