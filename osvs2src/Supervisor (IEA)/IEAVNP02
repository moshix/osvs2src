         BNE   @RT00563                                            0563 01559000
*             GO TO NP2DUP02;       /* LOOP AGAIN                    */ 01560000
*                                                                  0564 01561000
*           /*********************************************************/ 01562000
*           /*                                                       */ 01563000
*           /* THIS DEVICE IS TO BE DISMOUNTED                       */ 01564000
*           /*                                                       */ 01565000
*           /*********************************************************/ 01566000
*                                                                  0565 01567000
*           ELSE                                                   0565 01568000
*             DO;                                                  0565 01569000
@RF00563 DS    0H                                                  0566 01570000
*NP2DUP10:                                                         0566 01571000
*               IOSUCB->UCBVTOC=ZERO;/* CLEAR VTOC TTR               */ 01572000
NP2DUP10 LA    @05,NP2IOSB                                         0566 01573000
         L     @05,IOSUCB(,@05)                                    0566 01574000
         SLR   @00,@00                                             0566 01575000
         ST    @00,UCBVTOC(,@05)                                   0566 01576000
*               IOSUCB->UCBVOLI=VOLZERO;/* CLEAR VOLID               */ 01577000
         MVC   UCBVOLI(6,@05),VOLZERO                              0567 01578000
*               NP2OFFDA=IOSUCB->UCBNAME;/* SET DEV NAME             */ 01579000
*                                                                  0568 01580000
         MVC   NP2OFFDA(3),UCBNAME(@05)                            0568 01581000
*               /*****************************************************/ 01582000
*               /*                                                   */ 01583000
*               /* ADVISE OPER THAT DEVICE HAS BEEN DISMOUNTED       */ 01584000
*               /*                                                   */ 01585000
*               /*****************************************************/ 01586000
*                                                                  0569 01587000
*               GEN REFS(NP2OFFM,NVTWTO)                           0569 01588000
*                   (IEAPMNIP TYPE=WTO,PARAM=NP2OFFM);             0569 01589000
         IEAPMNIP TYPE=WTO,PARAM=NP2OFFM                                01590000
*             END;                                                 0570 01591000
*         END;                                                     0571 01592000
*     END;                                                         0572 01593000
*   ELSE                                                           0573 01594000
*     ;                             /* RESTORE REGS                  */ 01595000
@RF00512 DS    0H                                                  0574 01596000
*   UCBPTR=UCBSAVE;                                                0574 01597000
         LR    UCBPTR,UCBSAVE                                      0574 01598000
*   IF UCBSAVE^=IOSUCB              /* DEVICE WE'RE ON READY         */ 01599000
*     THEN                                                         0575 01600000
