         BZ    @RT00514                                            0514 01337000
         CLC   VOLUME(6),UCBVOLI(UCBPTR)                           0514 01338000
         BE    @RF00514                                            0514 01339000
@RT00514 DS    0H                                                  0515 01340000
*         /***********************************************************/ 01341000
*         /*                                                         */ 01342000
*         /* DONT CHECK THIS UCB                                     */ 01343000
*         /*                                                         */ 01344000
*         /***********************************************************/ 01345000
*                                                                  0515 01346000
*         DO;                                                      0515 01347000
*           I2=I2+ONE;              /* BUMP TABLE PTR                */ 01348000
         AL    I2,@CF00068                                         0516 01349000
*           GO TO NP2DUP01;         /* GO CHECK NEXT UCB             */ 01350000
         B     NP2DUP01                                            0517 01351000
*         END;                                                     0518 01352000
*                                                                  0518 01353000
*       /*************************************************************/ 01354000
*       /*                                                           */ 01355000
*       /* ISSUE DUPLICATE VOLUME SERIAL MESSAGE                     */ 01356000
*       /*                                                           */ 01357000
*       /*************************************************************/ 01358000
*                                                                  0519 01359000
*       ELSE                                                       0519 01360000
*         DO;                                                      0519 01361000
*                                                                  0519 01362000
@RF00514 DS    0H                                                  0520 01363000
*/*  ISSUE DUP VOLUME SERIAL MESSAGE--ONE OF WHICH IS AN MSS VOL     */ 01364000
*                                                                  0520 01365000
*           IF NPENTRY=THREE THEN   /* SPECIAL MSG           @ZM31050*/ 01366000
         CLI   NPENTRY,3                                           0520 01367000
         BNE   @RF00520                                            0520 01368000
*             DO;                                                  0521 01369000
*               NP2DA1V=UCBNAME;    /* SET BOTH              @ZM31050*/ 01370000
         MVC   NP2DA1V(3),UCBNAME(UCBPTR)                          0522 01371000
*               NP2DA2V=UCBSAVE->UCBNAME;/* DEVICE ADDRESSES @ZM31050*/ 01372000
         MVC   NP2DA2V(3),UCBNAME(UCBSAVE)                         0523 01373000
*NP2VDUP:                                                          0524 01374000
*               GEN REFS(NP2VLST,NVTWTOR)/* ISSUE MESSAGE    @ZM31050*/ 01375000
*                   (IEAPMNIP TYPE=WTOR,PARAM=NP2VLST);            0524 01376000
NP2VDUP  IEAPMNIP TYPE=WTOR,PARAM=NP2VLST                               01377000
*               IF NP2VRPLY=UCBNAME THEN/* CHECK REPLY AND   @ZM31050*/ 01378000
         CLC   NP2VRPLY(3),UCBNAME(UCBPTR)                         0525 01379000
         BNE   @RF00525                                            0525 01380000
*                 IOSUCB=UCBPTR;    /* SAVE PTR TO UCB       @ZM31050*/ 01381000
         LA    @05,NP2IOSB                                         0526 01382000
         ST    UCBPTR,IOSUCB(,@05)                                 0526 01383000
*               ELSE                                               0527 01384000
*                 IF NP2VRPLY=UCBSAVE->UCBNAME THEN/* CHECK  @ZM31050*/ 01385000
