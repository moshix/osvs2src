         TITLE 'IEAVNP02 - UCB INITIALIZATION ROUTINE                  *00001000
                        '                                               00002000
IEAVNP02 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(22)                                             0001 00008000
         DC    C'IEAVNP02 UZ09243 77117'                           0001 00009000
IEAVNPC2 DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00216-IEAVNPC2)                                   00013000
         ENTRY IEAVNPC2                                                 00014000
IEAVNPB2 DS    0H                                                  0001 00015000
         USING *,@15                                               0001 00016000
         B     @PROLOG                                             0001 00017000
         DC    AL2(@EP00221-IEAVNPB2)                                   00018000
         ENTRY IEAVNPB2                                                 00019000
IEAVNPA2 DS    0H                                                  0001 00020000
         USING *,@15                                               0001 00021000
         B     @PROLOG                                             0001 00022000
         DC    AL2(@EP00224-IEAVNPA2)                                   00023000
         ENTRY IEAVNPA2                                                 00024000
         DROP  @15                                                      00025000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00026000
         BALR  @12,0                                               0001 00027000
@PSTART  DS    0H                                                  0001 00028000
         USING @PSTART,@12                                         0001 00029000
         ST    @13,@SA00001+4                                      0001 00030000
         LA    @14,@SA00001                                        0001 00031000
         ST    @14,8(,@13)                                         0001 00032000
         LR    @13,@14                                             0001 00033000
         AH    @15,4(,@15)                                         0001 00034000
         BR    @15                                                 0001 00035000
@EP00001 DS    0H                                                  0002 00036000
*                                                                       00037000
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF           00038000
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES       00039000
* WILL ASSEMBLE PROPERLY.                                               00040000
*                                                                  0215 00041000
*/********************************************************************/ 00042000
*/*                                                                  */ 00043000
*/*  THIS THIRD ENTRY POINT IS THE SAME AS THE SECOND ENTRY POINT    */ 00044000
*/*  EXCEPT THAT ONLY VIRTUAL UCBS ARE VERIFIED                      */ 00045000
*/*                                                                  */ 00046000
*/********************************************************************/ 00047000
*                                                                  0216 00048000
*IEAVNPC2:                          /* THIRD ENTRY POINT     @ZM31050*/ 00049000
*   ENTRY;                                                         0216 00050000
@EP00216 DS    0H                                                  0217 00051000
*   NPENTRY=THREE;                  /* INDICATE 3RD ENTRY    @ZM31050*/ 00052000
         MVI   NPENTRY,X'03'                                       0217 00053000
*   IF ICBI=NOTON THEN              /* IF MSS NOT INITIALIZED      0218 00054000
*                                                            @G24LPLN*/ 00055000
         L     @11,CVTEXT2(,RCVT)                                  0218 00056000
         L     @11,CVTICB(,@11)                                    0218 00057000
         TM    ICBI(@11),B'10000000'                               0218 00058000
         BNZ   @RF00218                                            0218 00059000
*     NP2NOMSS=ON;                  /* INDICATE FOR LATER USE      0219 00060000
*                                                            @G24LPLN*/ 00061000
         OI    NP2NOMSS,B'00010000'                                0219 00062000
*   GO TO NP2COMM;                  /* DO COMMON PROCESSING  @ZM31050*/ 00063000
         B     NP2COMM                                             0220 00064000
*/********************************************************************/ 00065000
*/*                                                                  */ 00066000
*/*  OBTAIN THE ADDRESS OF THE UCB ADDRESS TABLE, IECILK2,           */ 00067000
*/*  FROM THE CVT (CVTILK2) AND SET A TABLE INDEX, I, TO             */ 00068000
*/*  REFLECT THE FIRST TABLE ENTRY.  INCREMENT THE TABLE INDEX       */ 00069000
*/*  AND EXAMINE EACH UCB WHICH IS REPRESENTED BY A HALFWORD UCB     */ 00070000
*/*  ADDRESS ENTRY IN IECILK2.                                       */ 00071000
*/*                                                                  */ 00072000
*/*  BYPASS TESTING FOR DUMMY ENTRIES IN IECILK2 AND ANY UCB         */ 00073000
*/*  WHICH HAS BEEN PREVIOUSLY TESTED.  DEVICE TESTING IS            */ 00074000
*/*  COMPLETE WHEN THE INDEX SEARCH REACHES THE END OF IECILK2.      */ 00075000
*/*                                                                  */ 00076000
*/********************************************************************/ 00077000
*/*  SECOND ENTRY ALIAS                                              */ 00078000
*                                                                  0221 00079000
*IEAVNPB2:                          /* SECOND ENTRY ALIAS            */ 00080000
*                                                                  0221 00081000
*   /*****************************************************************/ 00082000
*   /*                                                               */ 00083000
*   /* SECOND ENTRY POINT                                            */ 00084000
*   /*                                                               */ 00085000
*   /*****************************************************************/ 00086000
*                                                                  0221 00087000
*   ENTRY;                          /* SECOND ENTRY                  */ 00088000
@EP00221 DS    0H                                                  0222 00089000
*   NPENTRY=TWO;                    /* INDICATE 2ND ENTRY    @ZM31050*/ 00090000
         MVI   NPENTRY,X'02'                                       0222 00091000
*   GO TO NP2COMM;                  /* DO COMMON PROCESSING  @ZM31050*/ 00092000
         B     NP2COMM                                             0223 00093000
*/*  FIRST ENTRY ALIAS                                               */ 00094000
*                                                                  0224 00095000
*IEAVNPA2:                          /* FIRST ENTRY ALIAS             */ 00096000
*                                                                  0224 00097000
*   /*****************************************************************/ 00098000
*   /*                                                               */ 00099000
*   /* FIRST ENTRY POINT                                             */ 00100000
*   /*                                                               */ 00101000
*   /*****************************************************************/ 00102000
*                                                                  0224 00103000
*   ENTRY;                          /* FIRST ENTRY                   */ 00104000
@EP00224 DS    0H                                                  0225 00105000
*   NPENTRY=ONE;                    /* INDICATE 1ST ENTRY    @ZM31050*/ 00106000
         MVI   NPENTRY,X'01'                                       0225 00107000
*NP2COMM:                           /* COMMON FOR ALL ENTRIES      0226 00108000
*                                                            @ZM31050*/ 00109000
*                                                                  0226 00110000
*   /*****************************************************************/ 00111000
*   /*                                                               */ 00112000
*   /* PROGRAM, SRB AND IOSB INITIALIZATION                          */ 00113000
*   /*                                                               */ 00114000
*   /*****************************************************************/ 00115000
*                                                                  0226 00116000
*   I=ONE;                          /* SET UCB LOOK UP TBL INCR      */ 00117000
NP2COMM  MVC   I(2),@CH00068                                       0226 00118000
*   IOSB=IOSB&&IOSB;                /* CLEAR IOSB                    */ 00119000
         LA    @11,NP2IOSB                                         0227 00120000
         XC    IOSB(108,@11),IOSB(@11)                             0227 00121000
*   IOSASID=ASCBASID;               /* SET ASCB ID                   */ 00122000
         L     @10,PSAAOLD                                         0228 00123000
         MVC   IOSASID(2,@11),ASCBASID(@10)                        0228 00124000
*   IOSDVRID=IOSMISID;              /* SET DRIVER ID                 */ 00125000
         MVI   IOSDVRID(@11),X'01'                                 0229 00126000
*   IOSPGAD=ADDR(NP2TERM);          /* SET TERMINATION RTN ADDR      */ 00127000
         LA    @00,NP2TERM                                         0230 00128000
         ST    @00,IOSPGAD(,@11)                                   0230 00129000
*   IOSPSLL=ON;                     /* SET LOCAL LOCK NOT WANTED     */ 00130000
         OI    IOSPSLL(@11),B'00010000'                            0231 00131000
*   IOSNRM=ADDR(NP2BR14);           /* SET NORMAL END APPENDAGE      */ 00132000
         LA    @00,NP2BR14                                         0232 00133000
         ST    @00,IOSNRM(,@11)                                    0232 00134000
*   IOSABN=ADDR(NP2ABNAP);          /* SET ABNORMAL END APPENDAGE    */ 00135000
         LA    @00,NP2ABNAP                                        0233 00136000
         ST    @00,IOSABN(,@11)                                    0233 00137000
*   SRB=SRB&&SRB;                   /* CLEAR SRB                     */ 00138000
         LA    @10,NP2SRB                                          0234 00139000
         XC    SRB(44,@10),SRB(@10)                                0234 00140000
*   SRBPARM=ADDR(NP2IOSB);          /* IOSB ADDR TO SRB              */ 00141000
         ST    @11,SRBPARM(,@10)                                   0235 00142000
*/*************************************************************      */ 00143000
*/* SEARCH FOR VALID UCB AND PASS CONTROL TO NP2DEVCK.               */ 00144000
*/* RETURN CONTROL TO IEAVNIPM WHEN TABLE IS EXHAUSTED.              */ 00145000
*/*************************************************************      */ 00146000
*                                                                  0236 00147000
*NP2SRC01:                          /* BEGINNING OF UCB SEARCH       */ 00148000
*   RSCAN=IECILK2(I);               /* UCB LOOK UP TBL ENTRY         */ 00149000
*                                                                  0236 00150000
NP2SRC01 LH    @11,I                                               0236 00151000
         ALR   @11,@11                                             0236 00152000
         L     @10,CVTILK2(,RCVT)                                  0236 00153000
         BCTR  @10,0                                               0236 00154000
         BCTR  @10,0                                               0236 00155000
         LH    RSCAN,IECILK2(@11,@10)                              0236 00156000
         N     RSCAN,@CF02875                                      0236 00157000
*   /*****************************************************************/ 00158000
*   /*                                                               */ 00159000
*   /* UCB LOOK UP TABLE IS EXHAUSTED - REPLACE SVC TABLE ENTRY IF   */ 00160000
*   /* NECESSARY AND RETURN TO IEAVNIPM                              */ 00161000
*   /*                                                               */ 00162000
*   /*****************************************************************/ 00163000
*                                                                  0237 00164000
*   IF RSCAN=TABLEND THEN                                          0237 00165000
         CL    RSCAN,@CF00064                                      0237 00166000
         BNE   @RF00237                                            0237 00167000
*     DO;                                                          0238 00168000
*       IF NP2PURGE=ON THEN         /* WAS PURGE ISSUSED     @YM03866*/ 00169000
         TM    NP2PURGE,B'00100000'                                0239 00170000
         BNO   @RF00239                                            0239 00171000
*         SVCENTRY=PGOLDSVC;        /* UPDATE IGC0001F SVC TABLE   0240 00172000
*                                      ENTRY                         */ 00173000
         LH    @11,SVC16                                           0240 00174000
         AL    @11,SVCTBPTR(,RNVT)                                 0240 00175000
         MVC   SVCENTRY(8,@11),PGOLDSVC                            0240 00176000
*       ELSE                                                       0241 00177000
*         ;                                                        0241 00178000
@RF00239 DS    0H                                                  0242 00179000
*       RETURN;                     /* RETURN TO IEAVNIPM            */ 00180000
@EL00001 L     @13,4(,@13)                                         0242 00181000
@EF00001 DS    0H                                                  0242 00182000
@ER00001 LM    @14,@12,12(@13)                                     0242 00183000
         BR    @14                                                 0242 00184000
*     END;                                                         0243 00185000
*                                                                  0243 00186000
*   /*****************************************************************/ 00187000
*   /*                                                               */ 00188000
*   /* VALIDATE UCB LOOK UP TABLE ENTRY                              */ 00189000
*   /*                                                               */ 00190000
*   /*****************************************************************/ 00191000
*                                                                  0244 00192000
*   ELSE                                                           0244 00193000
*     DO;                                                          0244 00194000
@RF00237 DS    0H                                                  0245 00195000
*       IF RSCAN=ZERO THEN          /* DUMMY UCB                     */ 00196000
         LTR   RSCAN,RSCAN                                         0245 00197000
         BZ    @RT00245                                            0245 00198000
*         GO TO NP2SRC03;           /* GO BUMP PTR                   */ 00199000
*       ELSE                        /* GOOD UCB                      */ 00200000
*         DO;                                                      0247 00201000
*           UCBPTR=RSCAN;           /* RESET UCB PTR                 */ 00202000
*                                                                  0248 00203000
         LR    UCBPTR,RSCAN                                        0248 00204000
*           /*********************************************************/ 00205000
*           /*                                                       */ 00206000
*           /* DETERMINE IF UCB SHOULD BE TESTED                     */ 00207000
*           /*                                                       */ 00208000
*           /*********************************************************/ 00209000
*                                                                  0249 00210000
*           IF UCBTESTD=NOTON THEN  /* UCB NOT TESTED                */ 00211000
         TM    UCBTESTD(UCBPTR),B'10000000'                        0249 00212000
         BNZ   @RF00249                                            0249 00213000
*             DO;                                                  0250 00214000
*               IF UCBONLI=ON THEN  /* DEVICE GENED ONLINE           */ 00215000
         TM    UCBONLI(UCBPTR),B'10000000'                         0251 00216000
         BNO   @RF00251                                            0251 00217000
*                 DO;                                              0252 00218000
*                                                                  0252 00219000
*                   /*************************************************/ 00220000
*                   /*                                               */ 00221000
*                   /* NON DIRECT ACCESS DEVICES                     */ 00222000
*                   /*                                               */ 00223000
*                   /*************************************************/ 00224000
*                                                                  0253 00225000
*                   IF UCB3DACC=NOTON THEN                         0253 00226000
*                                                                  0253 00227000
         TM    UCB3DACC(UCBPTR),B'00100000'                        0253 00228000
         BNZ   @RF00253                                            0253 00229000
*                     /***********************************************/ 00230000
*                     /*                                             */ 00231000
*                     /* IF SECOND OR THIRD ENTRY, GO CHECK NEXT UCB */ 00232000
*                     /*                                             */ 00233000
*                     /***********************************************/ 00234000
*                                                                  0254 00235000
*                     IF NPENTRY^=ONE THEN/* ENTRY 1 IS FOR NON-DA 0254 00236000
*                                                            @ZM31050*/ 00237000
*                                                                  0254 00238000
         CLI   NPENTRY,1                                           0254 00239000
         BNE   @RT00254                                            0254 00240000
*                       /*********************************************/ 00241000
*                       /*                                           */ 00242000
*                       /* GET NEXT UCB                              */ 00243000
*                       /*                                           */ 00244000
*                       /*********************************************/ 00245000
*                                                                  0255 00246000
*                       GO TO NP2SRC03;/* GO BUMP POINTER            */ 00247000
*                     ELSE                                         0256 00248000
*                       ;           /* NO-1ST ENTRY TO PROG          */ 00249000
*                                                                  0256 00250000
*                   /*************************************************/ 00251000
*                   /*                                               */ 00252000
*                   /* DIRECT ACCESS DEVICES                         */ 00253000
*                   /*                                               */ 00254000
*                   /*************************************************/ 00255000
*                                                                  0257 00256000
*                   ELSE                                           0257 00257000
*                                                                  0257 00258000
*                     /***********************************************/ 00259000
*                     /*                                             */ 00260000
*                     /* IF FIRST ENTRY, GO CHECK NEXT UCB           */ 00261000
*                     /*                                             */ 00262000
*                     /***********************************************/ 00263000
*                                                                  0257 00264000
*                     IF UCBRVDEV=NOTON THEN/* IS IT A REAL DEVICE 0257 00265000
*                                                            @ZM31050*/ 00266000
         B     @RC00253                                            0257 00267000
@RF00253 TM    UCBRVDEV(UCBPTR),B'00001000'                        0257 00268000
         BNZ   @RF00257                                            0257 00269000
*                       IF NPENTRY^=TWO THEN/* YES--IF ENTRY 1 OR 3     00270000
*                                                            @ZM31050*/ 00271000
         CLI   NPENTRY,2                                           0258 00272000
         BNE   @RT00258                                            0258 00273000
*                         GO TO NP2SRC02;/* SKIP AND GET NEXT UCB  0259 00274000
*                                                            @ZM31050*/ 00275000
*                       ELSE                                       0260 00276000
*                         ;         /* ENTRY1--PROCESS UCB MZA12345  */ 00277000
*                     ELSE          /* IT IS A VIRTUAL UCB   @ZM31050*/ 00278000
*                       IF NPENTRY^=THREE THEN/* IF ENTRY 1 OR 2   0261 00279000
*                                                            @ZM31050*/ 00280000
         B     @RC00257                                            0261 00281000
@RF00257 CLI   NPENTRY,3                                           0261 00282000
         BNE   @RT00261                                            0261 00283000
*                         GO TO NP2SRC02;/* SKIP AND GET NEXT UCB AND   00284000
*                                      CHECK SHAREABLE DEV         0262 00285000
*                                      @ZM31050,@ZA18117             */ 00286000
*                                                                  0262 00287000
*                   /*************************************************/ 00288000
*                   /*                                               */ 00289000
*                   /* GO MAKE AVAILABLE PATH TESTS                  */ 00290000
*                   /*                                               */ 00291000
*                   /*************************************************/ 00292000
*                                                                  0263 00293000
*                   CALL NP2DEVCK;                                 0263 00294000
@RC00257 DS    0H                                                  0263 00295000
@RC00253 BAL   @14,NP2DEVCK                                        0263 00296000
*                 END;                                             0264 00297000
*               ELSE                                               0265 00298000
*                 DO;               /*                       @OZ00078*/ 00299000
         B     @RC00251                                            0265 00300000
@RF00251 DS    0H                                                  0266 00301000
*                   UCBVALPH=ON;    /* TURN ON NO PATHS VALIDATED  0266 00302000
*                                                            @OZ00078*/ 00303000
         OI    UCBVALPH(UCBPTR),B'00000010'                        0266 00304000
*                   IF UCB3DACC=NOTON THEN/*                 @YM03452*/ 00305000
         TM    UCB3DACC(UCBPTR),B'00100000'                        0267 00306000
         BNZ   @RF00267                                            0267 00307000
*                     UCBVORSN=ON;  /* SET VARY OPER REASON  @YM03452*/ 00308000
         OI    UCBVORSN(UCBPTR),B'00000100'                        0268 00309000
*                   ELSE                                           0269 00310000
*                     ;                                            0269 00311000
@RF00267 DS    0H                                                  0270 00312000
*                 END;                                             0270 00313000
*             END;                                                 0271 00314000
*                                                                  0271 00315000
*           /*********************************************************/ 00316000
*           /*                                                       */ 00317000
*           /* INDICATE UCB HAS BEEN TESTED                          */ 00318000
*           /*                                                       */ 00319000
*           /*********************************************************/ 00320000
*                                                                  0272 00321000
*           ELSE                                                   0272 00322000
*             UCBTESTD=NOTON;       /* RESET ALREADY TESTED BIT      */ 00323000
         B     @RC00249                                            0272 00324000
@RF00249 NI    UCBTESTD(UCBPTR),B'01111111'                        0272 00325000
*         END;                                                     0273 00326000
@RC00249 DS    0H                                                  0274 00327000
*     END;                                                         0274 00328000
*NP2SRC02:                                                         0275 00329000
*                                                                  0275 00330000
*   /*****************************************************************/ 00331000
*   /*                                                               */ 00332000
*   /* CHECK FOR SETTING UCBRR BIT FOR SHAREABLE DEVICE      @OZ00805*/ 00333000
*   /*                                                               */ 00334000
*   /*****************************************************************/ 00335000
*                                                                  0275 00336000
*   RFY                                                            0275 00337000
*    (REG5,                                                        0275 00338000
*     REG6) RSTD;                   /*                       @OZ00805   00339000
*                                                            @OZ00805*/ 00340000
NP2SRC02 DS    0H                                                  0276 00341000
*   GEN REFS(UCBOB,UCBTBYT2,UCBTBYT3,UCBSTAT,UCBSQC,UCBFLP1);      0276 00342000
        IOSGEN DETECTOR,UCB=(RSCAN),REG=(REG5,REG6),VAR=2   @OZ00805    00343000
*   RFY                                                            0277 00344000
*    (REG5,                                                        0277 00345000
*     REG6) UNRSTD;                 /*                       @OZ00805*/ 00346000
*NP2SRC0A:                          /*                       @OZ00805*/ 00347000
*                                                                  0278 00348000
*   /*****************************************************************/ 00349000
*   /*                                                               */ 00350000
*   /* 2305 HAS MULTIPLE EXPOSURES                                   */ 00351000
*   /*                                                               */ 00352000
*   /*****************************************************************/ 00353000
*                                                                  0278 00354000
*   IF UCB3DACC=ON THEN                                            0278 00355000
NP2SRC0A TM    UCB3DACC(UCBPTR),B'00100000'                        0278 00356000
         BNO   @RF00278                                            0278 00357000
*     IF UCBMTPXP=ON THEN                                          0279 00358000
         TM    UCBMTPXP(UCBPTR),B'00001000'                        0279 00359000
         BNO   @RF00279                                            0279 00360000
*       I=I+SEVEN;                                                 0280 00361000
*                                                                  0280 00362000
         LH    @11,I                                               0280 00363000
         LA    @11,7(,@11)                                         0280 00364000
         STH   @11,I                                               0280 00365000
*   /*****************************************************************/ 00366000
*   /*                                                               */ 00367000
*   /* NOT MULTIPLE EXPOSURE DEVICE                                  */ 00368000
*   /*                                                               */ 00369000
*   /*****************************************************************/ 00370000
*                                                                  0281 00371000
*     ELSE                                                         0281 00372000
*       ;                                                          0281 00373000
*                                                                  0281 00374000
@RF00279 DS    0H                                                  0282 00375000
*   /*****************************************************************/ 00376000
*   /*                                                               */ 00377000
*   /* NO A DIRECT ACCESS DEVICE                                     */ 00378000
*   /*                                                               */ 00379000
*   /*****************************************************************/ 00380000
*                                                                  0282 00381000
*   ELSE                                                           0282 00382000
*     ;                                                            0282 00383000
@RF00278 DS    0H                                                  0283 00384000
*NP2SRC03:                          /* POINT TO NEXT UCB             */ 00385000
*                                                                  0283 00386000
*   /*****************************************************************/ 00387000
*   /*                                                               */ 00388000
*   /* BUMP POINTER TO NEXT TABLE ENTRY                              */ 00389000
*   /*                                                               */ 00390000
*   /*****************************************************************/ 00391000
*                                                                  0283 00392000
*   I=I+ONE;                        /* GET NEXT UCB ADDR             */ 00393000
NP2SRC03 LH    @11,I                                               0283 00394000
         LA    @11,1(,@11)                                         0283 00395000
         STH   @11,I                                               0283 00396000
*   GO TO NP2SRC01;                 /* GO INSPECT NEXT UCB           */ 00397000
         B     NP2SRC01                                            0284 00398000
*                                                                  0285 00399000
*/********************************************************************/ 00400000
*/*                                                                  */ 00401000
*/*  THIS ROUTINE USES THE IOSGEN MACRO TO COMPLETE THE              */ 00402000
*/*  PATHMAP IN THE UCB.                                             */ 00403000
*/*                                                                  */ 00404000
*/*  PREPARE TO ISSUE I/O DOWN ALL PATHS INDICATED AS AVAILABLE      */ 00405000
*/*  IN THE IOSGEN MAP TABLE.  I/O ISSUED SEQUENTIALLY AND USES      */ 00406000
*/*  SHOULDER TAP TO TEST PATHS IN ANOTHER CPU.                      */ 00407000
*/*                                                                  */ 00408000
*/*  SET DEVICE NOT READY FOR ALL DEVICES WHICH ARE NOT READY.       */ 00409000
*/*  SET ALL SUB-UCBS FOR MULTIPLE EXPOSURE DEVICES.                 */ 00410000
*/*                                                                  */ 00411000
*/********************************************************************/ 00412000
*                                                                  0285 00413000
*NP2DEVCK:                          /* DEVICE CHECK SUBROUTINE       */ 00414000
*   PROC OPTIONS(NOSAVE);           /* PROCEDURE                     */ 00415000
NP2DEVCK DS    0H                                                  0286 00416000
*   RFY                                                            0286 00417000
*    (REG10) RSTD;                                                 0286 00418000
*   IOSUCB=RSCAN;                   /* SET UCB ADDR                  */ 00419000
         LA    @11,NP2IOSB                                         0287 00420000
         ST    RSCAN,IOSUCB(,@11)                                  0287 00421000
*   REG10=RSAVE;                    /* SAVE SAVEAREA PTR             */ 00422000
         LR    REG10,RSAVE                                         0288 00423000
*   RSAVE=ADDR(IOSGSAVE);           /* GET IOSGEN SAVEAREA PTR       */ 00424000
*                                                                  0289 00425000
         LA    RSAVE,IOSGSAVE                                      0289 00426000
*   /*****************************************************************/ 00427000
*   /*                                                               */ 00428000
*   /* ISSUE IOSGEN TO ACQUIRE DEVICE PATH MAP                       */ 00429000
*   /*                                                               */ 00430000
*   /*****************************************************************/ 00431000
*                                                                  0290 00432000
*   GEN REFS(UCBBGN,UCBFL5,UCBUA,UCBLCI,UCBALTPH,CVTILCH,CVTPCCAT, 0290 00433000
*       CVTCSD,PCCACPUA,PCCACAFM,PCCACAT,CSDCPUAL,CATFLG,LCHTCH,UCBBASE 00434000
*       ,UCBOB)(IOSGEN MAP,TABLE=PATHMAP,UCB=RSCAN,VAR=1);         0290 00435000
         IOSGEN MAP,TABLE=PATHMAP,UCB=RSCAN,VAR=1                       00436000
*   RSAVE=REG10;                    /* RESTORE SAVEAREA REG          */ 00437000
         LR    RSAVE,REG10                                         0291 00438000
*   RFY                                                            0292 00439000
*    (REG10) UNRSTD;                                               0292 00440000
*                                                                  0292 00441000
*   /*****************************************************************/ 00442000
*   /*                                                               */ 00443000
*   /* DETERMINE IF ANY PATHS ARE AVAILABLE                          */ 00444000
*   /*                                                               */ 00445000
*   /*****************************************************************/ 00446000
*                                                                  0293 00447000
*   IF PRIME^=BOTHOFF|SECOND^=BOTHOFF THEN/* YES- PATHS ARE AVAILABLE*/ 00448000
         CLC   PRIME(2),@CB00086                                   0293 00449000
         BNE   @RT00293                                            0293 00450000
         CLC   SECOND(2),@CB00086                                  0293 00451000
         BE    @RF00293                                            0293 00452000
@RT00293 DS    0H                                                  0294 00453000
*     DO;                                                          0294 00454000
*       IOSPATH=PRADDR;             /* SET PRIMARY PATH              */ 00455000
*                                                                  0295 00456000
         LA    @11,NP2IOSB                                         0295 00457000
         LH    @00,PRADDR                                          0295 00458000
         STH   @00,IOSPATH(,@11)                                   0295 00459000
*       /*************************************************************/ 00460000
*       /*                                                           */ 00461000
*       /* TEST CPU 0 PRIMARY PATH                                   */ 00462000
*       /*                                                           */ 00463000
*       /*************************************************************/ 00464000
*                                                                  0296 00465000
*       IF PRCPUA=PATHON THEN       /* CPU 0 PRIMARY IS AVAILABLE    */ 00466000
         CLI   PRCPUA,X'01'                                        0296 00467000
         BNE   @RF00296                                            0296 00468000
*         DO;                                                      0297 00469000
*           IOSAFF=CPUID00;         /* I/O THRU CPU 0                */ 00470000
*                                                                  0298 00471000
         MVI   IOSAFF(@11),X'00'                                   0298 00472000
*           /*********************************************************/ 00473000
*           /*                                                       */ 00474000
*           /* GO ISSUE I/O                                          */ 00475000
*           /*                                                       */ 00476000
*           /*********************************************************/ 00477000
*                                                                  0299 00478000
*           CALL NP2IORTN;          /* CALL I/O ROUTINE              */ 00479000
         BAL   @14,NP2IORTN                                        0299 00480000
*         END;                                                     0300 00481000
*       ELSE                        /* CPU 0 PRIMARY IS NOT AVAIL    */ 00482000
*         UCBPPA=ON;                /* SET PRIMARY, CPU 0 OFFLINE    */ 00483000
*                                                                  0301 00484000
         B     @RC00296                                            0301 00485000
@RF00296 OI    UCBPPA(UCBPTR),B'10000000'                          0301 00486000
*       /*************************************************************/ 00487000
*       /*                                                           */ 00488000
*       /* TEST CPU 1 PRIMARY PATH                                   */ 00489000
*       /*                                                           */ 00490000
*       /*************************************************************/ 00491000
*                                                                  0302 00492000
*       IF PRCPUB=PATHON THEN       /* CPU 1 PRIMARY IS AVAILABLE    */ 00493000
@RC00296 CLI   PRCPUB,X'01'                                        0302 00494000
         BNE   @RF00302                                            0302 00495000
*         DO;                                                      0303 00496000
*           IOSAFF=CPUID01;         /* I/0 THRU CPU 1                */ 00497000
*                                                                  0304 00498000
         LA    @11,NP2IOSB                                         0304 00499000
         MVI   IOSAFF(@11),X'01'                                   0304 00500000
*           /*********************************************************/ 00501000
*           /*                                                       */ 00502000
*           /* GO ISSUE I/O                                          */ 00503000
*           /*                                                       */ 00504000
*           /*********************************************************/ 00505000
*                                                                  0305 00506000
*           CALL NP2IORTN;          /* CALL I/O ROUTINE              */ 00507000
         BAL   @14,NP2IORTN                                        0305 00508000
*         END;                                                     0306 00509000
*       ELSE                        /* CPU 1 PRIMARY IS NOT AVAIL    */ 00510000
*         UCBPPB=ON;                /* SET PRIMARY, CPU 1 OFFLINE    */ 00511000
*                                                                  0307 00512000
         B     @RC00302                                            0307 00513000
@RF00302 OI    UCBPPB(UCBPTR),B'00100000'                          0307 00514000
*       /*************************************************************/ 00515000
*       /*                                                           */ 00516000
*       /* TEST FOR ALTERNATE PATH                                   */ 00517000
*       /*                                                           */ 00518000
*       /*************************************************************/ 00519000
*                                                                  0308 00520000
*       IF UCBALTPH=ON THEN         /* THERE IS AN ALTERNATE PATH    */ 00521000
@RC00302 TM    UCBALTPH(UCBPTR),B'00000001'                        0308 00522000
         BNO   @RF00308                                            0308 00523000
*         DO;                                                      0309 00524000
*           IOSPATH=SECADDR;        /* SET SECONDARY PATH            */ 00525000
*                                                                  0310 00526000
         LA    @11,NP2IOSB                                         0310 00527000
         LH    @00,SECADDR                                         0310 00528000
         STH   @00,IOSPATH(,@11)                                   0310 00529000
*           /*********************************************************/ 00530000
*           /*                                                       */ 00531000
*           /* TEST CPU 0 SECONDARY PATH                             */ 00532000
*           /*                                                       */ 00533000
*           /*********************************************************/ 00534000
*                                                                  0311 00535000
*           IF SECPUA=PATHON THEN   /* CPU 0 SECONDARY IS AVAIL      */ 00536000
         CLI   SECPUA,X'01'                                        0311 00537000
         BNE   @RF00311                                            0311 00538000
*             DO;                                                  0312 00539000
*               IOSAFF=CPUID00;     /* I/O THRU CPU 0                */ 00540000
*                                                                  0313 00541000
         MVI   IOSAFF(@11),X'00'                                   0313 00542000
*               /*****************************************************/ 00543000
*               /*                                                   */ 00544000
*               /* GO ISSUE I/O                                      */ 00545000
*               /*                                                   */ 00546000
*               /*****************************************************/ 00547000
*                                                                  0314 00548000
*               CALL NP2IORTN;      /* CALL I/O ROUTINE              */ 00549000
         BAL   @14,NP2IORTN                                        0314 00550000
*             END;                                                 0315 00551000
*           ELSE                    /* CPU 0 SECONDARY IS NOT AVAIL  */ 00552000
*             UCBSPA=ON;            /* SET SECONDARY, CPU 0 OFFLINE  */ 00553000
*                                                                  0316 00554000
         B     @RC00311                                            0316 00555000
@RF00311 OI    UCBSPA(UCBPTR),B'01000000'                          0316 00556000
*           /*********************************************************/ 00557000
*           /*                                                       */ 00558000
*           /* TEST CPU 1 SECONDARY PATH                             */ 00559000
*           /*                                                       */ 00560000
*           /*********************************************************/ 00561000
*                                                                  0317 00562000
*           IF SECPUB=PATHON THEN   /* CPU 1 SECONDARY IS AVAIL      */ 00563000
@RC00311 CLI   SECPUB,X'01'                                        0317 00564000
         BNE   @RF00317                                            0317 00565000
*             DO;                                                  0318 00566000
*               IOSAFF=CPUID01;     /* I/O THRU CPU 1                */ 00567000
*                                                                  0319 00568000
         LA    @11,NP2IOSB                                         0319 00569000
         MVI   IOSAFF(@11),X'01'                                   0319 00570000
*               /*****************************************************/ 00571000
*               /*                                                   */ 00572000
*               /* GO ISSUE I/O                                      */ 00573000
*               /*                                                   */ 00574000
*               /*****************************************************/ 00575000
*                                                                  0320 00576000
*               CALL NP2IORTN;      /* CALL I/O ROUTINE              */ 00577000
         BAL   @14,NP2IORTN                                        0320 00578000
*             END;                                                 0321 00579000
*           ELSE                    /* CPU 1 SECONDARY IS NOT AVAIL  */ 00580000
*             UCBSPB=ON;            /* SET SECONDARY, CPU 1 OFFLINE  */ 00581000
         B     @RC00317                                            0322 00582000
@RF00317 OI    UCBSPB(UCBPTR),B'00010000'                          0322 00583000
*           UCBTESTD=ON;            /* INDICATE DEVICE TESTED        */ 00584000
@RC00317 OI    UCBTESTD(UCBPTR),B'10000000'                        0323 00585000
*         END;                                                     0324 00586000
*                                                                  0324 00587000
*       /*************************************************************/ 00588000
*       /*                                                           */ 00589000
*       /* THERE IS NO ALTERNATE PATH                                */ 00590000
*       /*                                                           */ 00591000
*       /*************************************************************/ 00592000
*                                                                  0325 00593000
*       ELSE                                                       0325 00594000
*         DO;                       /* SET BOTH SECONDARYS OFFLINE   */ 00595000
         B     @RC00308                                            0325 00596000
@RF00308 DS    0H                                                  0326 00597000
*           UCBSPA=ON;                                             0326 00598000
*           UCBSPB=ON;                                             0327 00599000
         OI    UCBSPA(UCBPTR),B'01010000'                          0327 00600000
*         END;                                                     0328 00601000
*     END;                                                         0329 00602000
*   ELSE                                                           0330 00603000
*     DO;                                                          0330 00604000
         B     @RC00293                                            0330 00605000
@RF00293 DS    0H                                                  0331 00606000
*       UCBVHRSN=ON;                /* SET VARY HIERARCHY REASON     */ 00607000
         OI    UCBVHRSN(UCBPTR),B'00000010'                        0331 00608000
*       GO TO NP2DEV01;             /* GO TAKE DEVICE OFFLINE        */ 00609000
         B     NP2DEV01                                            0332 00610000
*     END;                                                         0333 00611000
*                                                                  0333 00612000
*   /*****************************************************************/ 00613000
*   /*                                                               */ 00614000
*   /* INDICATE IF DEVICE IS OR IS NOT AVAILABLE                     */ 00615000
*   /*                                                               */ 00616000
*   /*****************************************************************/ 00617000
*                                                                  0334 00618000
*   IF UCBCHM=CHMALLOF|UCBONLI=NOTON THEN/* DEVICE CANNOT BE REACHED */ 00619000
@RC00293 CLI   UCBCHM(UCBPTR),X'F0'                                0334 00620000
         BE    @RT00334                                            0334 00621000
         TM    UCBONLI(UCBPTR),B'10000000'                         0334 00622000
         BNZ   @RF00334                                            0334 00623000
@RT00334 DS    0H                                                  0335 00624000
*     DO;                                                          0335 00625000
*       UCBVORSN=ON;                /* SET VARY OPERATOR REASON      */ 00626000
         OI    UCBVORSN(UCBPTR),B'00000100'                        0336 00627000
*NP2DEV01:                          /* NO PATHS TO DEVICE            */ 00628000
*       UCBONLI=NOTON;              /* SET DEVICE OFFLINE            */ 00629000
NP2DEV01 NI    UCBONLI(UCBPTR),B'01111111'                         0337 00630000
*       UCBCHM=CHMALLON;            /* REVERSE ALL PATHS             */ 00631000
         MVI   UCBCHM(UCBPTR),X'00'                                0338 00632000
*     END;                                                         0339 00633000
*   ELSE                                                           0340 00634000
*     ;                             /* DEVICE CAN BE REACHED         */ 00635000
*                                                                  0340 00636000
@RF00334 DS    0H                                                  0341 00637000
*   /*****************************************************************/ 00638000
*   /*                                                               */ 00639000
*   /* INDICATE IF DEVICE IS OR IS NOT READY                         */ 00640000
*   /*                                                               */ 00641000
*   /*****************************************************************/ 00642000
*                                                                  0341 00643000
*   IF NOTREADY=ON THEN                                            0341 00644000
         TM    NOTREADY,B'01000000'                                0341 00645000
         BNO   @RF00341                                            0341 00646000
*     DO;                                                          0342 00647000
*       UCBNOTRD=ON;                /* SET UCB NOT READY             */ 00648000
         OI    UCBNOTRD(UCBPTR),B'01000000'                        0343 00649000
*       NOTREADY=NOTON;             /* RESET NOT READY INDICATOR     */ 00650000
         NI    NOTREADY,B'10111111'                                0344 00651000
*       IF UCB3TAPE=ON&CVTDSTAT=ON| /* TAPE OR               @YM04665*/ 00652000
*           UCB3DACC=ON&            /* DA AND                @YM04665*/ 00653000
*           CVTDSTAT=ON THEN        /* SYSGEN OPTION         @YM04665*/ 00654000
         TM    UCB3TAPE(UCBPTR),B'10000000'                        0345 00655000
         BNO   @GL00003                                            0345 00656000
         TM    CVTDSTAT(RCVT),B'00010000'                          0345 00657000
         BO    @RT00345                                            0345 00658000
@GL00003 TM    UCB3DACC(UCBPTR),B'00100000'                        0345 00659000
         BNO   @RF00345                                            0345 00660000
         TM    CVTDSTAT(RCVT),B'00010000'                          0345 00661000
         BNO   @RF00345                                            0345 00662000
@RT00345 DS    0H                                                  0346 00663000
*         DO;                       /* YES                   @YM04665*/ 00664000
*           UCBONLI=NOTON;          /* MARK OFFLINE FOR      @YM04665*/ 00665000
         NI    UCBONLI(UCBPTR),B'01111111'                         0347 00666000
*           UCBVORSN=ON;            /* OPERATOR REASON       @YM04665*/ 00667000
         OI    UCBVORSN(UCBPTR),B'00000100'                        0348 00668000
*         END;                                                     0349 00669000
*     END;                                                         0350 00670000
*   ELSE                                                           0351 00671000
*     ;                                                            0351 00672000
@RF00341 DS    0H                                                  0352 00673000
*NP2DEV02:                          /* DEVICE PATH CHECKS COMPLETE   */ 00674000
*   RETURN TO NP2SRC0A;             /* RETURN TO MAIN ROUTINE      0352 00675000
*                                                            @OZ00805*/ 00676000
@RC00341 DS    0H                                                  0352 00677000
NP2DEV02 LA    @14,NP2SRC0A                                        0352 00678000
@EL00002 DS    0H                                                  0352 00679000
@EF00002 DS    0H                                                  0352 00680000
@ER00002 BR    @14                                                 0352 00681000
*   END NP2DEVCK;                                                  0353 00682000
*                                                                  0354 00683000
*/*************************************************************      */ 00684000
*/*                                                                  */ 00685000
*/*  THIS ROUTINE CALLS NP2SHRDA IF THE DEVICE IS SHARED,            */ 00686000
*/*  CHECKS THE RETURN, AND DETERMINES IF FURTHER I/O IS             */ 00687000
*/*  REQUIRED.  IT SETS UP THE CHANNEL PROGRAMS FOR DA AND           */ 00688000
*/*  NON-DA DEVICES AND CALLS THE NP2STIO ROUTINE.                   */ 00689000
*/*                                                                  */ 00690000
*/*************************************************************      */ 00691000
*                                                                  0354 00692000
*NP2IORTN:                                                         0354 00693000
*   PROC OPTIONS(NOSAVE);                                          0354 00694000
NP2IORTN DS    0H                                                  0355 00695000
*   REXITIOR=REXIT;                 /* SAVE RETURN REG               */ 00696000
         ST    REXIT,REXITIOR                                      0355 00697000
*   IF UCB3DACC=ON THEN                                            0356 00698000
         TM    UCB3DACC(UCBPTR),B'00100000'                        0356 00699000
         BNO   @RF00356                                            0356 00700000
*     IF UCBSYSR=ON&                /*                       @YM04127*/ 00701000
*         UCBRR=ON|                 /*                       @YM04127*/ 00702000
*         UCBSHRUP=ON|              /*                       @OZ00805*/ 00703000
*         UCBVTOC=ZERO              /*                       @YM04127*/ 00704000
*       THEN                        /* THIS IS A DA DEV              */ 00705000
         TM    UCBSYSR(UCBPTR),B'00000010'                         0357 00706000
         BNO   @GL00007                                            0357 00707000
         TM    UCBRR(UCBPTR),B'00100000'                           0357 00708000
         BO    @RT00357                                            0357 00709000
@GL00007 L     @11,UCBEXTPT(,UCBPTR)                               0357 00710000
         TM    UCBSHRUP(@11),B'01000000'                           0357 00711000
         BO    @RT00357                                            0357 00712000
         L     @00,UCBVTOC(,UCBPTR)                                0357 00713000
         LTR   @00,@00                                             0357 00714000
         BNZ   @RF00357                                            0357 00715000
@RT00357 DS    0H                                                  0358 00716000
*       DO;                                                        0358 00717000
*         IF UCBRR=ON|UCBSHRUP=ON   /*                       @ZM30468*/ 00718000
*           THEN                    /* THIS DEV IS SHAREABLE         */ 00719000
         TM    UCBRR(UCBPTR),B'00100000'                           0359 00720000
         BO    @RT00359                                            0359 00721000
         L     @11,UCBEXTPT(,UCBPTR)                               0359 00722000
         TM    UCBSHRUP(@11),B'01000000'                           0359 00723000
         BNO   @RF00359                                            0359 00724000
@RT00359 DS    0H                                                  0360 00725000
*           DO;                                                    0360 00726000
*                                                                  0360 00727000
*             /*******************************************************/ 00728000
*             /*                                                     */ 00729000
*             /* GO TO ISSUE RELEASE CCW FOR SHARED TEST             */ 00730000
*             /*                                                     */ 00731000
*             /*******************************************************/ 00732000
*                                                                  0361 00733000
*             CALL NP2SHRDA;        /* GO TO SHARED DA RTN           */ 00734000
         BAL   @14,NP2SHRDA                                        0361 00735000
*             IF UCBONLI=NOTON|IOSCOD=IOSGDPCC THEN/* DEVICE IS    0362 00736000
*                                      OFFLINE OR DOES NOT EXIST     */ 00737000
         TM    UCBONLI(UCBPTR),B'10000000'                         0362 00738000
         BZ    @RT00362                                            0362 00739000
         LA    @11,NP2IOSB                                         0362 00740000
         CLI   IOSCOD(@11),X'4D'                                   0362 00741000
         BE    @RT00362                                            0362 00742000
*               GO TO NP2IOR02;     /* GO FINISH UP                  */ 00743000
*             ELSE                                                 0364 00744000
*               IOSBYP=NOTON;       /* DONT BYPASS CHANNEL PROGRAM 0364 00745000
*                                      PREFIX                        */ 00746000
         NI    IOSBYP(@11),B'01111111'                             0364 00747000
*           END;                                                   0365 00748000
*         ELSE                                                     0366 00749000
*           ;                                                      0366 00750000
@RF00359 DS    0H                                                  0367 00751000
*         IF NP2NOMSS=ON THEN       /* IF NO MSS             @G24LPLN*/ 00752000
         TM    NP2NOMSS,B'00010000'                                0367 00753000
         BO    @RT00367                                            0367 00754000
*           GO TO NP2IOR01;         /* SET UP NOP CCW        @G24LPLN*/ 00755000
*                                                                  0368 00756000
*         /***********************************************************/ 00757000
*         /*                                                         */ 00758000
*         /* SET UP TO READ DA VOLID                                 */ 00759000
*         /*                                                         */ 00760000
*         /***********************************************************/ 00761000
*                                                                  0369 00762000
*         IOSRST=ADDR(CCW1);        /* SET REAL CHNL PGM ADDR        */ 00763000
         LA    @11,NP2IOSB                                         0369 00764000
         LA    @10,CCW1                                            0369 00765000
         ST    @10,IOSRST(,@11)                                    0369 00766000
*         IOSVST=ADDR(CCW1);        /* SET VIRT CHNL PGM ADDR        */ 00767000
         ST    @10,IOSVST(,@11)                                    0370 00768000
*       END;                                                       0371 00769000
*     ELSE                                                         0372 00770000
*       GO TO NP2IOR01;                                            0372 00771000
*                                                                  0372 00772000
*   /*****************************************************************/ 00773000
*   /*                                                               */ 00774000
*   /* SET UP NOP CHANNEL PROGRAM                                    */ 00775000
*   /*                                                               */ 00776000
*   /*****************************************************************/ 00777000
*                                                                  0373 00778000
*   ELSE                                                           0373 00779000
*     DO;                                                          0373 00780000
         B     @RC00356                                            0373 00781000
@RF00356 DS    0H                                                  0374 00782000
*NP2IOR01:                          /* NOP ISSUED AGAINST ALL DEVICES*/ 00783000
*       IOSRST=ADDR(CCW4);          /* SET REAL CHNL PGM ADDR        */ 00784000
NP2IOR01 LA    @11,NP2IOSB                                         0374 00785000
         LA    @10,CCW4                                            0374 00786000
         ST    @10,IOSRST(,@11)                                    0374 00787000
*       IOSVST=ADDR(CCW4);          /* SET VIRT CHNL PGM ADDR        */ 00788000
         ST    @10,IOSVST(,@11)                                    0375 00789000
*       IOSNERP=ON;                 /* BYPASS ERP PROCESSING         */ 00790000
*       IOSBYP=ON;                  /* BYPASS IOS CHNL PGM PREFIX    */ 00791000
         OI    IOSNERP(@11),B'10001000'                            0377 00792000
*     END;                                                         0378 00793000
*                                                                  0378 00794000
*   /*****************************************************************/ 00795000
*   /*                                                               */ 00796000
*   /* GO ISSUE I/O                                                  */ 00797000
*   /*                                                               */ 00798000
*   /*****************************************************************/ 00799000
*                                                                  0379 00800000
*   CALL NP2STIO;                   /* GO ISSUE I/O                  */ 00801000
*                                                                  0379 00802000
@RC00356 BAL   @14,NP2STIO                                         0379 00803000
*   /*****************************************************************/ 00804000
*   /*                                                               */ 00805000
*   /* CHECK RESULTS OF I/O                                          */ 00806000
*   /*                                                               */ 00807000
*   /*****************************************************************/ 00808000
*                                                                  0380 00809000
*   IF IOSCOD=IOSNRMC THEN          /* I/O COMPLETED SUCCESSFULLY    */ 00810000
         LA    @11,NP2IOSB                                         0380 00811000
         CLI   IOSCOD(@11),X'7F'                                   0380 00812000
         BNE   @RF00380                                            0380 00813000
*     IF UCB3DACC=ON&NP2NOMSS=NOTON /*                       @G24LPLN*/ 00814000
*       THEN                        /* THIS IS A DA DEVICE           */ 00815000
         TM    UCB3DACC(UCBPTR),B'00100000'                        0381 00816000
         BNO   @RF00381                                            0381 00817000
         TM    NP2NOMSS,B'00010000'                                0381 00818000
         BNZ   @RF00381                                            0381 00819000
*       IF UCBVTOC=ZERO|UCBSYSR=ON THEN/* SET TTR OF VTOC    @YM06580*/ 00820000
*                                                                  0382 00821000
         L     @00,UCBVTOC(,UCBPTR)                                0382 00822000
         LTR   @00,@00                                             0382 00823000
         BZ    @RT00382                                            0382 00824000
         TM    UCBSYSR(UCBPTR),B'00000010'                         0382 00825000
         BNO   @RF00382                                            0382 00826000
@RT00382 DS    0H                                                  0383 00827000
*         /***********************************************************/ 00828000
*         /*                                                         */ 00829000
*         /* GO CHECK FOR DUPLICATE VOLUME SERIALS                   */ 00830000
*         /*                                                         */ 00831000
*         /***********************************************************/ 00832000
*                                                                  0383 00833000
*         CALL NP2LDUP;             /* GO CHECK FOR DUPLICATE VOLIDS */ 00834000
         BAL   @14,NP2LDUP                                         0383 00835000
*       ELSE                                                       0384 00836000
*         ;                                                        0384 00837000
@RF00382 DS    0H                                                  0385 00838000
*     ELSE                                                         0385 00839000
*       ;                                                          0385 00840000
*                                                                  0385 00841000
@RF00381 DS    0H                                                  0386 00842000
*   /*****************************************************************/ 00843000
*   /*                                                               */ 00844000
*   /* GO CHECK I/O ERRORS                                           */ 00845000
*   /*                                                               */ 00846000
*   /*****************************************************************/ 00847000
*                                                                  0386 00848000
*   ELSE                                                           0386 00849000
*     DO;                                                          0386 00850000
         B     @RC00380                                            0386 00851000
@RF00380 DS    0H                                                  0387 00852000
*       CALL NP2ERRCK;              /* GO CHECK FOR I/O ERRORS       */ 00853000
         BAL   @14,NP2ERRCK                                        0387 00854000
*     END;                                                         0388 00855000
*NP2IOR02:                          /* PREPARE TO RETURN TO CALLER   */ 00856000
*   IOSBYP=NOTON;                   /* RESET CHNL PGM PREFIX BYPASS  */ 00857000
@RC00380 DS    0H                                                  0389 00858000
NP2IOR02 LA    @11,NP2IOSB                                         0389 00859000
         NI    IOSBYP(@11),B'01111111'                             0389 00860000
*   REXIT=REXITIOR;                 /* RESTORE RETURN REG            */ 00861000
         L     REXIT,REXITIOR                                      0390 00862000
*   END NP2IORTN;                   /* RETURN TO NP2DEVCK            */ 00863000
@EL00003 DS    0H                                                  0391 00864000
@EF00003 DS    0H                                                  0391 00865000
@ER00003 BR    @14                                                 0391 00866000
*                                                                  0392 00867000
*/*************************************************************      */ 00868000
*/*                                                                  */ 00869000
*/*  THIS ROUTINE CHECKS THE RESERVE/RELEASE CAPABILITY OF           */ 00870000
*/*  ALL THE DA DEVICES THAT WERE DEFINED DURING SYSGEN AS           */ 00871000
*/*  BEING SHAREABLE BETWEEN CPUS.                                   */ 00872000
*/*                                                                  */ 00873000
*/*************************************************************      */ 00874000
*                                                                  0392 00875000
*NP2SHRDA:                                                         0392 00876000
*   PROC OPTIONS(NOSAVE);                                          0392 00877000
NP2SHRDA DS    0H                                                  0393 00878000
*   REXITSHR=REXIT;                 /* SAVE RETURN REG               */ 00879000
         ST    REXIT,REXITSHR                                      0393 00880000
*   NP2NWAIT=ON;                    /* INDICATE TO BYPASS ISSUANCE OF   00881000
*                                      WAIT AND THAT NP2SHRDA ISSUED    00882000
*                                      CHNL PGM                      */ 00883000
*                                                                  0394 00884000
         OI    NP2NWAIT,B'10000000'                                0394 00885000
*   /*****************************************************************/ 00886000
*   /*                                                               */ 00887000
*   /* PREPARE TO ISSUE RELEASE                                      */ 00888000
*   /*                                                               */ 00889000
*   /*****************************************************************/ 00890000
*                                                                  0395 00891000
*   IOSBYP=ON;                      /* BYPASS IOS CHNL PGM PREFIX    */ 00892000
         LA    @11,NP2IOSB                                         0395 00893000
         OI    IOSBYP(@11),B'10000000'                             0395 00894000
*   IOSRST=ADDR(CCW5);              /* SET REAL CHNL PGM ADDR        */ 00895000
         LA    @10,CCW5                                            0396 00896000
         ST    @10,IOSRST(,@11)                                    0396 00897000
*   IOSVST=ADDR(CCW5);              /* SET VIRT CHNL PGM ADDR        */ 00898000
         ST    @10,IOSVST(,@11)                                    0397 00899000
*   ECB=ZERO;                       /* CLEAR ECB                     */ 00900000
*                                                                  0398 00901000
         SLR   @00,@00                                             0398 00902000
         ST    @00,ECB                                             0398 00903000
