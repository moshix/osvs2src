         C     UCBSAVE,IOSUCB(,@05)                                0575 01602000
         BE    @RF00575                                            0575 01603000
*     NOTREADY=NOTON;               /* YES-REST NOTREADY      @YM6785*/ 01604000
         NI    NOTREADY,B'10111111'                                0576 01605000
*   ELSE                                                           0577 01606000
*     ;                             /*                        @YM6785*/ 01607000
@RF00575 DS    0H                                                  0578 01608000
*   IOSUCB=UCBSAVE;                                                0578 01609000
         LA    @05,NP2IOSB                                         0578 01610000
         ST    UCBSAVE,IOSUCB(,@05)                                0578 01611000
*   REXIT=REXITDUP;                                                0579 01612000
         L     REXIT,REXITDUP                                      0579 01613000
*   END NP2LDUP;                    /* RETURN TO NP2IORTN            */ 01614000
@EL00006 DS    0H                                                  0580 01615000
@EF00006 DS    0H                                                  0580 01616000
@ER00006 BR    @14                                                 0580 01617000
*                                                                  0581 01618000
*/*************************************************************      */ 01619000
*/*                                                                  */ 01620000
*/*  THIS ROUTINE PRINTS MESSAGES FOR DA DEVICES INDICATING          */ 01621000
*/*  THAT A VOLUME IS UNLABELED OR THAT A  PERMANENT I/O             */ 01622000
*/*  WAS ENCOUNTERED ON A DA DEVICE.                                 */ 01623000
*/*                                                                  */ 01624000
*/*************************************************************      */ 01625000
*                                                                  0581 01626000
*NP2ERRCK:                                                         0581 01627000
*   PROC OPTIONS(NOSAVE);                                          0581 01628000
NP2ERRCK DS    0H                                                  0582 01629000
*   REXITERR=REXIT;                 /* SAVE RETURN REG               */ 01630000
         ST    REXIT,REXITERR                                      0582 01631000
*   IF IOSCOD^=IOSGDPCC THEN        /* NOT CC=3                      */ 01632000
         LA    @11,NP2IOSB                                         0583 01633000
         CLI   IOSCOD(@11),X'4D'                                   0583 01634000
         BE    @RF00583                                            0583 01635000
*     DO;                                                          0584 01636000
*       IF UCB3DACC=ON THEN         /* THIS IS A DA DEV              */ 01637000
         TM    UCB3DACC(UCBPTR),B'00100000'                        0585 01638000
         BNO   @RF00585                                            0585 01639000
*         DO;                                                      0586 01640000
*           IF IOSSNS=IOSSNSBD THEN                                0587 01641000
         CLC   IOSSNS(2,@11),@CB02440                              0587 01642000
         BE    @RT00587                                            0587 01643000
*             GOTO NP2ERR01;        /* SENSE WAS UNSUCCESSFUL      0588 01644000
*                                                            @YM3031P*/ 01645000
*           ELSE                                                   0589 01646000
*             IF IOSNS1B4=ON THEN   /* NO RECORD FOUND               */ 01647000
         TM    IOSNS1B4(@11),B'00001000'                           0589 01648000
         BNO   @RF00589                                            0589 01649000
*               DO;                 /* TELL OPER PACK IS NOT LABLED  */ 01650000
*                 NP2UNIT=UCBNAME;                                 0591 01651000
*                                                                  0591 01652000
         MVC   NP2UNIT(3),UCBNAME(UCBPTR)                          0591 01653000
*                 /***************************************************/ 01654000
*                 /*                                                 */ 01655000
*                 /* ADVISE OPERATOR THAT PACK IS UNLABLED           */ 01656000
*                 /*                                                 */ 01657000
*                 /***************************************************/ 01658000
*                                                                  0592 01659000
*                 GEN REFS(NP2NLBLM,NVTWTO)                        0592 01660000
*                     (IEAPMNIP TYPE=WTO,PARAM=NP2NLBLM);          0592 01661000
         IEAPMNIP TYPE=WTO,PARAM=NP2NLBLM                               01662000
*                 UCBONLI=NOTON;    /* SET DEV OFFLINE               */ 01663000
         NI    UCBONLI(UCBPTR),B'01111111'                         0593 01664000
*               END;                                               0594 01665000
*             ELSE                                                 0595 01666000
*               IF IOSNS0B1=NOTON THEN/* INTERVENTION NOT REQUIRED   */ 01667000
         B     @RC00589                                            0595 01668000
@RF00589 LA    @11,NP2IOSB                                         0595 01669000
         TM    IOSNS0B1(@11),B'01000000'                           0595 01670000
         BNZ   @RF00595                                            0595 01671000
*                 DO;                                              0596 01672000
*                                                                  0596 01673000
*                   /*************************************************/ 01674000
*                   /*                                               */ 01675000
*                   /* CONVERT OP CODE                               */ 01676000
*                   /*                                               */ 01677000
*                   /*************************************************/ 01678000
*                                                                  0597 01679000
*NP2ERR01:                                                         0597 01680000
*                   RPARM=IOSCSWCA-8;/* GET ERROR CCW        @YM3031P*/ 01681000
NP2ERR01 LA    @05,NP2IOSB                                         0597 01682000
         L     RPARM,IOSCSWCA-1(,@05)                              0597 01683000
         LA    RPARM,0(,RPARM)                                     0597 01684000
         SL    RPARM,@CF00055                                      0597 01685000
*                   CONVERT(1)=RPARM->CCWCMD;/* GET OP CODE          */ 01686000
         IC    @05,CCWCMD(,RPARM)                                  0598 01687000
         STC   @05,CONVERT                                         0598 01688000
*                   DIGIT=LENGTH(NP2SNSCC);                        0599 01689000
         LA    DIGIT,2                                             0599 01690000
*                   CALL NP2CVRT;   /* GO CONVERT TO EBCDIC          */ 01691000
         BAL   @14,NP2CVRT                                         0600 01692000
*                   NP2SNSCC=CONVERT(1:LENGTH(NP2SNSCC));          0601 01693000
*                                                                  0601 01694000
         MVC   NP2SNSCC(2),CONVERT                                 0601 01695000
*                   /*************************************************/ 01696000
*                   /*                                               */ 01697000
*                   /* CONVERT STATUS BYTES - CHECK FOR INVALID CSW  */ 01698000
*                   /*                                               */ 01699000
*                   /*************************************************/ 01700000
*                                                                  0602 01701000
*                   DO;                                            0602 01702000
*                     IF RPARM->CCWCMD=ZERO&IOSTATUS=ZERO THEN     0603 01703000
         CLI   CCWCMD(RPARM),0                                     0603 01704000
         BNE   @RF00603                                            0603 01705000
         SLR   @11,@11                                             0603 01706000
         LA    @10,NP2IOSB                                         0603 01707000
         LH    @00,IOSTATUS(,@10)                                  0603 01708000
         N     @00,@CF02875                                        0603 01709000
         CR    @11,@00                                             0603 01710000
         BNE   @RF00603                                            0603 01711000
*                       DO;                                        0604 01712000
*                         CONVERT(1:1)=IOSCOD;/* GET IOS COMP CODE   */ 01713000
         MVC   CONVERT(1),IOSCOD(@10)                              0605 01714000
*                         CONVERT(2:2)=ZERO;/* SET REST TO ZERO      */ 01715000
         STC   @11,CONVERT+1                                       0606 01716000
*                       END;                                       0607 01717000
*                     ELSE                                         0608 01718000
*                       CONVERT(1:2)=IOSTATUS;/* GET STATUS          */ 01719000
         B     @RC00603                                            0608 01720000
@RF00603 LA    @11,NP2IOSB                                         0608 01721000
         MVC   CONVERT(2),IOSTATUS(@11)                            0608 01722000
*                     DIGIT=LENGTH(NP2SNSST);                      0609 01723000
@RC00603 LA    DIGIT,4                                             0609 01724000
*                     CALL NP2CVRT; /* GO CONVERT TO EBCDIC          */ 01725000
         BAL   @14,NP2CVRT                                         0610 01726000
*                     NP2SNSST=CONVERT(1:LENGTH(NP2SNSST));        0611 01727000
         MVC   NP2SNSST(4),CONVERT                                 0611 01728000
*                   END;                                           0612 01729000
*                                                                  0612 01730000
*                   /*************************************************/ 01731000
*                   /*                                               */ 01732000
*                   /* CONVERT SENSE BYTES                           */ 01733000
*                   /*                                               */ 01734000
*                   /*************************************************/ 01735000
*                                                                  0613 01736000
*                   IF IOSTSA(7:7)=ON THEN/* UNIT EXCEPTION OCCURED  */ 01737000
         LA    @11,NP2IOSB                                         0613 01738000
         TM    IOSTSA(@11),B'00000010'                             0613 01739000
         BNO   @RF00613                                            0613 01740000
*                     DO;                                          0614 01741000
*                       CONVERT(1:2)=IOSSNS;/* GET SENSE             */ 01742000
