*                 ELSE                                             0529 01392000
*                   DO;             /* REPLY INVALID         @ZM31050*/ 01393000
         B     @RC00527                                            0529 01394000
@RF00527 DS    0H                                                  0530 01395000
*                     GEN REFS(NVTWTO,NP2SRESM)                    0530 01396000
*                         (IEAPMNIP TYPE=WTO,PARAM=NP2SRESM);      0530 01397000
         IEAPMNIP TYPE=WTO,PARAM=NP2SRESM                               01398000
*                     GO TO NP2VDUP;/* REISSUE MESSAGE       @ZM31050*/ 01399000
         B     NP2VDUP                                             0531 01400000
*                   END;                                           0532 01401000
*               IF IOSUCB->UCBRVDEV=ON THEN/* VALID REPLY, IF      0533 01402000
*                                                            @ZM31050*/ 01403000
@RC00527 DS    0H                                                  0533 01404000
@RC00525 LA    @05,NP2IOSB                                         0533 01405000
         L     @05,IOSUCB(,@05)                                    0533 01406000
         TM    UCBRVDEV(@05),B'00001000'                           0533 01407000
         BNO   @RF00533                                            0533 01408000
*                 DO;               /* VIRTUAL DEVICE--DISMOUNT      */ 01409000
*                                                                  0534 01410000
*                   /*************************************************/ 01411000
*                   /*                                               */ 01412000
*                   /* ISSUE DISMOUNT MACRO FOR MSS VOLUME           */ 01413000
*                   /*                                               */ 01414000
*                   /*************************************************/ 01415000
*                                                                  0535 01416000
*                   DCL                                            0535 01417000
*                     MDREG1 REG(1) PTR;/* ADDRESS OF REQUEST BLOCK     01418000
*                                                            @Y30LB02*/ 01419000
*                                                                  0535 01420000
*                   /*************************************************/ 01421000
*                   /*                                               */ 01422000
*                   /* ***********************************************/ 01423000
*                   /* **********                            @Y30LB02*/ 01424000
*                   /* MACMEAN MSSC DEMOUNT MACRO            @Y30LB02*/ 01425000
*                   /* VS1-4/VS2-3 VERSION--01/19/76 LEVEL 00        */ 01426000
*                   /*                                       @ZDR2048*/ 01427000
*                   /* THIS IS THE STANDARD FORM - DEMOUNT MACRO     */ 01428000
*                   /*                                       @Y30LB02*/ 01429000
*                   /* ***********************************************/ 01430000
*                   /* **********                            @Y30LB02*/ 01431000
*                   /*                                               */ 01432000
*                   /*************************************************/ 01433000
*                                                                  0536 01434000
*                   RFY                                            0536 01435000
*                     MDREG1 RSTD;  /* RESTRICT REG 1        @Y30LB02*/ 01436000
*                   MDREG1=ADDR(DMLEN);/* R1 PTR TO LIST     @Y30LB02*/ 01437000
         LA    MDREG1,DMLEN                                        0537 01438000
*                   DCL                                            0538 01439000
*                     1 ICBDMLST STATIC LOCAL,/* DEMOUNT REQUEST   0538 01440000
*                                      BLOCK                 @Y30LB02*/ 01441000
*                      2 DMLEN FIXED(31) INIT('1C'X),/* LENGTH OF  0538 01442000
*                                      DEMOUNT BLOCK         @Y30LB02*/ 01443000
*                      2 DMOPCD FIXED(8) INIT('06'X),/* DEMOUNT    0538 01444000
*                                      OPCODE                @Y30LB02*/ 01445000
*                      2 DMNTRES1 CHAR(1) INIT('00'X),/* INIT      0538 01446000
*                                      RESERVED AREA TO ZERO @YM02186*/ 01447000
*                      2 DMNTFLAG BIT(16) INIT('0000000000000000'B),    01448000
*                                   /* FLAGS AREA            @YM02186*/ 01449000
*                       3 DMBITRE1 BIT(1),/* ONE RESERVED BIT      0538 01450000
*                                                            @YM02186*/ 01451000
*                       3 DMDELRSP BIT(1),/* DEMOUNT DELAYED RESPONSE   01452000
*                                      ATTRIBUTE             @Y30LB02*/ 01453000
*                       3 DMVUAVAL BIT(1),/* DEMOUNT VUA VALID     0538 01454000
*                                      ATTRIBUTE             @ZDR2048*/ 01455000
*                       3 DMBITRES BIT(13),/* THIRTEEN RESERVED BITS    01456000
*                                                            @ZDR2048*/ 01457000
*                      2 DMVOL CHAR(6) INIT(''B),/* INIT VOLID TO  0538 01458000
*                                      ZERO                  @Y30LB02*/ 01459000
*                      2 DMNTASID CHAR(2) INIT(''B),/* INIT ASID AREA   01460000
*                                      TO ZERO               @Y30LB02*/ 01461000
*                      2 DMNTECB FIXED(31) INIT(0),/* DEMOUNT EVENT     01462000
*                                      CONTROL BLOCK         @Y30LB02*/ 01463000
*                      2 DMHOST CHAR(5) INIT(''B),/* INIT HOSTID AREA   01464000
*                                                            @Y30LB02*/ 01465000
*                      2 DMNTRES2 CHAR(1) INIT(''B),/* INIT RESERVED    01466000
*                                      AREA TO ZERO          @ZDR2048*/ 01467000
*                      2 DMNTVUA FIXED(15) INIT(0);/* VIRTUAL UNIT 0538 01468000
*                                      ADDRESS               @ZDR2048*/ 01469000
*                   DMVOL=(IOSUCB->UCBVOLI);/* VALUE OF VOLID      0539 01470000
*                                                            @Y30LB02*/ 01471000
         MVC   DMVOL(6),UCBVOLI(@05)                               0539 01472000
*                   SVC(126);       /* INVOKE THE SSC SVC    @Y30LB02*/ 01473000
         SVC   126                                                 0540 01474000
*                   RFY                                            0541 01475000
*                     MDREG1 UNRSTD;/* UNREST REG 1          @Y30LB02   01476000
*                                                            @ZM31050*/ 01477000
*                 END;                                             0542 01478000
*               NOTREADY=ON;        /* INDICATE ONE DEVICE NOT READY    01479000
*                                                            @ZM31050*/ 01480000
@RF00533 OI    NOTREADY,B'01000000'                                0543 01481000
*               GO TO NP2DUP10;     /* GO CLEAR UCB          @ZM31050*/ 01482000
         B     NP2DUP10                                            0544 01483000
*             END;                                                 0545 01484000
*           NP2DA1=UCBNAME;                                        0546 01485000
@RF00520 MVC   NP2DA1(3),UCBNAME(UCBPTR)                           0546 01486000
*           NP2DA2=UCBSAVE->UCBNAME;                               0547 01487000
*                                                                  0547 01488000
         MVC   NP2DA2(3),UCBNAME(UCBSAVE)                          0547 01489000
*           /*********************************************************/ 01490000
*           /*                                                       */ 01491000
*           /* ADVISE OPER OF DUPLICATE VOLIDS                       */ 01492000
*           /*                                                       */ 01493000
*           /*********************************************************/ 01494000
*                                                                  0548 01495000
*           GEN REFS(NP2DISMM,NVTWTO)(IEAPMNIP TYPE=WTO,PARAM=NP2DISMM) 01496000
*               ;                                                  0548 01497000
         IEAPMNIP TYPE=WTO,PARAM=NP2DISMM                               01498000
*NP2DUP02:                          /* PROCESS DUP VOLUME SERIALS    */ 01499000
*                                                                  0549 01500000
*           /*********************************************************/ 01501000
*           /*                                                       */ 01502000
*           /* DUPLICATE VOLUME SERIAL PROCESSING LOOP               */ 01503000
*           /*                                                       */ 01504000
*           /*********************************************************/ 01505000
*                                                                  0549 01506000
*           IF IOSUCB=UCBPTR THEN   /* UCB ADDRS ARE EQUAL           */ 01507000
NP2DUP02 LA    @05,NP2IOSB                                         0549 01508000
         C     UCBPTR,IOSUCB(,@05)                                 0549 01509000
         BNE   @RF00549                                            0549 01510000
*             DO;                                                  0550 01511000
*               IOSUCB=UCBSAVE;     /* SET UCB ADDR                  */ 01512000
         ST    UCBSAVE,IOSUCB(,@05)                                0551 01513000
*               IOSPATH=UCBSAVE->UCBCHAN;/* SET CCUU                 */ 01514000
         MVC   IOSPATH(2,@05),UCBCHAN(UCBSAVE)                     0552 01515000
*             END;                                                 0553 01516000
*           ELSE                    /* UCB ADDRS ARE NOT EQUAL       */ 01517000
*             DO;                                                  0554 01518000
         B     @RC00549                                            0554 01519000
@RF00549 DS    0H                                                  0555 01520000
*               IOSUCB=UCBPTR;      /* SET UCB ADDR                  */ 01521000
         LA    @05,NP2IOSB                                         0555 01522000
         ST    UCBPTR,IOSUCB(,@05)                                 0555 01523000
*               IOSPATH=UCBCHAN;    /* SET CCUU                      */ 01524000
         MVC   IOSPATH(2,@05),UCBCHAN(UCBPTR)                      0556 01525000
*             END;                                                 0557 01526000
*                                                                  0557 01527000
*           /*********************************************************/ 01528000
*           /*                                                       */ 01529000
*           /* SET UP FOR NOP CHANNEL PROGRAM                        */ 01530000
*           /*                                                       */ 01531000
*           /*********************************************************/ 01532000
*                                                                  0558 01533000
*           IOSRST=ADDR(CCW4);      /* REAL CHNL PGM ADDR            */ 01534000
@RC00549 LA    @05,NP2IOSB                                         0558 01535000
         LA    @11,CCW4                                            0558 01536000
         ST    @11,IOSRST(,@05)                                    0558 01537000
*           IOSVST=ADDR(CCW4);      /* VIRT CHNL PGM ADDR            */ 01538000
         ST    @11,IOSVST(,@05)                                    0559 01539000
*           IOSBYP=ON;              /* BYPASS CHNL PGM PREFIX        */ 01540000
*                                                                  0560 01541000
         OI    IOSBYP(@05),B'10000000'                             0560 01542000
*           /*********************************************************/ 01543000
*           /*                                                       */ 01544000
*           /* GO ISSUE I/O                                          */ 01545000
*           /*                                                       */ 01546000
*           /*********************************************************/ 01547000
*                                                                  0561 01548000
*           CALL NP2STIO;           /* GO TO ISSUE I/O               */ 01549000
         BAL   @14,NP2STIO                                         0561 01550000
*           IOSBYP=NOTON;           /* ALLOW CHNL PGM PREFIX         */ 01551000
         LA    @05,NP2IOSB                                         0562 01552000
         NI    IOSBYP(@05),B'01111111'                             0562 01553000
*           IF NOTREADY=NOTON&IOSCOD^=IOSGDPCC THEN/* DEV IS READY 0563 01554000
*                                      AND I/O WAS GOOD              */ 01555000
         TM    NOTREADY,B'01000000'                                0563 01556000
         BNZ   @RF00563                                            0563 01557000
