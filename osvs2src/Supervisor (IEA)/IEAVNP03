*                 DO;               /* GET REST OF SYSP INPUT        */ 00891000
*                   SCANREG=CONTAD; /* GET CONT ADDRESS              */ 00892000
         LR    SCANREG,@10                                         0540 00893000
*                   GO TO NP3ALPHA;                                0541 00894000
         B     NP3ALPHA                                            0541 00895000
*                 END;                                             0542 00896000
*                                                                  0542 00897000
*               /*****************************************************/ 00898000
*               /*                                                   */ 00899000
*               /* INVALID - CONT HAS BEEN SPECIFIED WITHOUT A CONT  */ 00900000
*               /* ADDR                                              */ 00901000
*               /*                                                   */ 00902000
*               /*****************************************************/ 00903000
*                                                                  0543 00904000
*               ELSE                                               0543 00905000
*                 GO TO NP3SYINV;                                  0543 00906000
*             END;                                                 0544 00907000
*           ELSE                                                   0545 00908000
*             GO TO NP3SETXX;       /* GO SET UP TO READ SPECIFIED 0545 00909000
*                                      LIST                          */ 00910000
*         END;                                                     0546 00911000
*       ELSE                                                       0547 00912000
*         GO TO NP3SYINV;           /* INVALID - XX OF IEASYSXX IS 0547 00913000
