*     THEN                                                         1086 02873000
NP3IFLOC L     @10,NP3LST06                                        1086 02874000
         CLC   LNKVOLID(6,SCANREG),NMNTVS(@10)                     1086 02875000
         BNE   @RF01086                                            1086 02876000
*     LNKFLAG='FF'X;                /* YES - NULL ENTRY              */ 02877000
         MVI   LNKFLAG(SCANREG),X'FF'                              1087 02878000
*   SCANREG=SCANREG+LENGTH(LNKNAME);/* POINT TO NEXT ENTR            */ 02879000
@RF01086 LA    SCANREG,51(,SCANREG)                                1088 02880000
*   IF SCANREG=GETPTR               /* IS THIS THE END OF THE ENTRIES*/ 02881000
