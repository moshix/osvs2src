*       /*************************************************************/ 00871000
*                                                                  0534 00872000
*NP3ALPHA:                                                         0534 00873000
*       IF NP3SCAN2>='AA'&NP3SCAN2<='99' THEN                      0534 00874000
NP3ALPHA CLC   NP3SCAN2(2,SCANREG),@CC02448                        0534 00875000
         BL    @RF00534                                            0534 00876000
         CLC   NP3SCAN2(2,SCANREG),@CC02449                        0534 00877000
         BH    @RF00534                                            0534 00878000
*         DO;                                                      0535 00879000
*           IF NP3SCAN5=CONTCHAR    /* ARE NEXT CHARS                */ 00880000
*             THEN                  /* CONT'                         */ 00881000
         CLC   NP3SCAN5(5,SCANREG),CONTCHAR                        0536 00882000
         BNE   @RF00536                                            0536 00883000
*             DO;                                                  0537 00884000
*               IF CONTAD^=0        /* IS CONT PTR                   */ 00885000
*                 THEN              /* NON-ZERO                      */ 00886000
         L     @10,NP3CENT                                         0538 00887000
         L     @10,CONTAD(,@10)                                    0538 00888000
