*                   /*************************************************/ 01628000
*                   /*                                               */ 01629000
*                   /* INCREMENT OFFSET INTO PARMTABS BY ENTRY       */ 01630000
*                   /* CELLSIZE                                      */ 01631000
*                   /*                                               */ 01632000
*                   /*************************************************/ 01633000
*                                                                  0745 01634000
*                   INDEXREG=LENGTH(OPLIBENT)+INDEXREG;            0745 01635000
*                                                                  0745 01636000
         ALR   INDEXREG,@10                                        0745 01637000
*                   /*************************************************/ 01638000
*                   /*                                               */ 01639000
*                   /* BRANCH TO SET UP FOR NEXT ENTRY MERGE         */ 01640000
*                   /*                                               */ 01641000
*                   /*************************************************/ 01642000
*                                                                  0746 01643000
*                   GO TO NP3NXFLD;                                0746 01644000
         B     NP3NXFLD                                            0746 01645000
*                 END;                                             0747 01646000
*               ELSE                                               0748 01647000
*                 DO;                                              0748 01648000
*                                                                  0748 01649000
@RF00741 DS    0H                                                  0749 01650000
*                   /*************************************************/ 01651000
*                   /*                                               */ 01652000
*                   /* MOVE THE OPERATOR ENTRY CONTENT INTO THE      */ 01653000
*                   /* PARMLIB ENTRY                                 */ 01654000
*                   /*                                               */ 01655000
*                   /*************************************************/ 01656000
*                                                                  0749 01657000
*                   PARMTABC=OPERTABC;                             0749 01658000
*                                                                  0749 01659000
         MVC   PARMTABC(16,PARMREG),OPERTABC(OPERREG)              0749 01660000
*                   /*************************************************/ 01661000
*                   /*                                               */ 01662000
*                   /* BRANCH TO SET UP FOR NEXT ENTRY MERGE         */ 01663000
*                   /*                                               */ 01664000
*                   /*************************************************/ 01665000
*                                                                  0750 01666000
*                   GO TO NP3NXFLD;                                0750 01667000
         B     NP3NXFLD                                            0750 01668000
*                 END;                                             0751 01669000
*             END;                                                 0752 01670000
*         END;                                                     0753 01671000
*       ELSE                        /* OPERATOR DID NOT SPECIFY    0754 01672000
*                                      PARAM. FOR THIS CURRENT     0754 01673000
*                                      PARMTAB ENTRY                 */ 01674000
*         GO TO NP3NXFLD;           /* BRANCH TO SETUP FOR NEXT ENTRY   01675000
*                                      MERGE                         */ 01676000
*     END;                                                         0755 01677000
*   ELSE                            /* MERGE OPERATION IS COMPLETED  */ 01678000
*     GO TO NP3RTAD0;               /* RETURN TO CALLER              */ 01679000
*                                                                  0756 01680000
@RF00714 L     @10,NP3RTADR                                        0756 01681000
         BR    @10                                                 0756 01682000
*   /*****************************************************************/ 01683000
*   /*                                                               */ 01684000
*   /* SETUP FOR NEXT PARMTAB'S ENTRY MERGE                          */ 01685000
*   /*                                                               */ 01686000
*   /*****************************************************************/ 01687000
*                                                                  0757 01688000
*NP3NXFLD:                                                         0757 01689000
*   COUNTREG=COUNTREG-1;            /* DECREMENT PARMTAB ENTRY COUNT */ 01690000
NP3NXFLD BCTR  COUNTREG,0                                          0757 01691000
*   INDEXREG=LENGTH(OPLIBENT)+INDEXREG;/* UPDATE INDEX BY ENTRY SIZE */ 01692000
*                                                                  0758 01693000
         LA    INDEXREG,16(,INDEXREG)                              0758 01694000
*   /*****************************************************************/ 01695000
*   /*                                                               */ 01696000
*   /* UPDATE MAPPING BASE ADDRESS FOR PARMLIB PARMTAB               */ 01697000
*   /*                                                               */ 01698000
*   /*****************************************************************/ 01699000
*                                                                  0759 01700000
*   PARMREG=INDEXREG+ADDR(PTABLN);                                 0759 01701000
*                                                                  0759 01702000
         LA    PARMREG,PTABLN                                      0759 01703000
         ALR   PARMREG,INDEXREG                                    0759 01704000
*   /*****************************************************************/ 01705000
*   /*                                                               */ 01706000
*   /* UPDATE MAPPING BASE ADDRESS FOR OPERATOR PARMTAB              */ 01707000
*   /*                                                               */ 01708000
*   /*****************************************************************/ 01709000
*                                                                  0760 01710000
*   OPERREG=INDEXREG+ADDR(OPTABLN);                                0760 01711000
         LA    OPERREG,OPTABLN                                     0760 01712000
