*                 DO;               /* ZERO ENTRY FOR THIS PARM IN 0735 01587000
*                                      OPERTAB AND PARMTAB           */ 01588000
*                   OPLIBENT=OPLIBENT&&OPLIBENT;                   0736 01589000
         XC    OPLIBENT(16,@10),OPLIBENT(@10)                      0736 01590000
*                   NP3CENT=PARMREG;/* POINT TO PARMTAB              */ 01591000
         ST    PARMREG,NP3CENT                                     0737 01592000
*                   OPLIBENT=OPLIBENT&&OPLIBENT;                   0738 01593000
