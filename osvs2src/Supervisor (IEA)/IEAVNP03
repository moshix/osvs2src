*               NIPPTADR=NIPPTADR&&NIPPTADR;/* ZERO ENTRY ADDR Y02752*/ 05392000
         L     @10,NIPPTADR(,PARMREG)                              1604 05393000
         LR    @05,@10                                             1604 05394000
         XR    @05,@10                                             1604 05395000
         ST    @05,NIPPTADR(,PARMREG)                              1604 05396000
*             END;                  /*                         Y02752*/ 05397000
*         END;                      /*                         Y02752*/ 05398000
@RC01600 DS    0H                                                  1607 05399000
*     END;                          /*                         Y02752*/ 05400000
@RF01598 DS    0H                                                  1608 05401000
*   GO TO NPMSAVE;                  /* RETURN TO CALLER        Y02752*/ 05402000
@RF01596 L     @10,NPMSAVE                                         1608 05403000
         BR    @10                                                 1608 05404000
*                                                                  1609 05405000
*   /*****************************************************************/ 05406000
*   /*                                                               */ 05407000
*   /* NIP PROMPT CONTROL ROUTINE                              Y02752*/ 05408000
*   /*                                                               */ 05409000
*   /*****************************************************************/ 05410000
*                                                                  1609 05411000
*NPRMTRA3:                                                         1609 05412000
