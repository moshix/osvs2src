*                                                                  1348 04095000
*   /*****************************************************************/ 04096000
*   /*                                                               */ 04097000
*   /* THIS ROUTINE OBTAINS PAREAS FROM SUBPOOL 252. IF THE GETMAIN  */ 04098000
*   /* IS UNSUCCESSFULL,THE OPERATOR IS NOTIFIED AN ACTION IS TAKEN  */ 04099000
*   /* TO PLACE THE SYSTEM IN A DISABLED WAIT STATE.                 */ 04100000
*   /*                                                               */ 04101000
*   /*****************************************************************/ 04102000
*                                                                  1348 04103000
*NP3PAGM:                                                          1348 04104000
*   NP3RTRN=REXIT;                  /* SAVE CALLERS RETURN ADDRESS   */ 04105000
*                                                                  1348 04106000
NP3PAGM  ST    REXIT,NP3RTRN                                       1348 04107000
*   /*****************************************************************/ 04108000
*   /*                                                               */ 04109000
*   /* THIS GENERATES A GETMAIN MACRO TO GET A 2K BUFFER FROM SUBPOOL*/ 04110000
*   /* 0 .                                                           */ 04111000
*   /*                                                               */ 04112000
*   /*****************************************************************/ 04113000
*                                                                  1349 04114000
*   GENERATE;                                                      1349 04115000
*                                                                  1349 04116000
         GETMAIN    LC,LA=PAREA01,A=NP3GBFR,SP=0    ISSUE GETMAIN       04117000
*   /*****************************************************************/ 04118000
*   /*                                                               */ 04119000
*   /* CHECK FOR SUCCESSFULL GETMAIN - 0=YES, 4=NO                   */ 04120000
*   /*                                                               */ 04121000
*   /*****************************************************************/ 04122000
*                                                                  1350 04123000
*   IF GETMRTCD=0                   /* WAS GETMAIN SUCCESSFULL       */ 04124000
*     THEN                          /* YES, 2K BUFFER WAS GOTTEN     */ 04125000
         SLR   @14,@14                                             1350 04126000
         CR    GETMRTCD,@14                                        1350 04127000
         BNE   @RF01350                                            1350 04128000
*     DO;                                                          1351 04129000
*       RESPECIFY                                                  1352 04130000
*         NIPPAHDR BASED(NP3GBFR);  /* RESET POINTER                 */ 04131000
*       NIPPAQ=0;                   /* CLEAR FIRST HALF OF HEADER    */ 04132000
         L     @10,NP3GBFR                                         1353 04133000
         ST    @14,NIPPAQ(,@10)                                    1353 04134000
*       NIPPABYT=0;                 /* CLEAR SECOND HALF OF HEADER   */ 04135000
*                                                                  1354 04136000
         ST    @14,NIPPABYT(,@10)                                  1354 04137000
*       /*************************************************************/ 04138000
*       /*                                                           */ 04139000
*       /* CHECK TO SEE IF THIS IS THE INITIAL PAREA,,FIRST 2K BUFFER*/ 04140000
*       /* GOTTEN                                                    */ 04141000
*       /*                                                           */ 04142000
*       /*************************************************************/ 04143000
*                                                                  1355 04144000
*       IF NP3ADBFR='1'B            /* IS THIS THE INITIAL PAREA     */ 04145000
*         THEN                      /* NO, THIS IS NOT INITIAL PAREA */ 04146000
         TM    NP3ADBFR,B'00100000'                                1355 04147000
         BNO   @RF01355                                            1355 04148000
*         DO;                                                      1356 04149000
*           RESPECIFY                                              1357 04150000
*             NIPPAHDR BASED(NP3PAREA);                            1357 04151000
*           NIPPAQ=NP3GBFR;         /* CHAIN THIS BUFFER TO LAST   1358 04152000
*                                      BUFFER GOTTEN                 */ 04153000
         L     @09,NP3PAREA                                        1358 04154000
         ST    @10,NIPPAQ(,@09)                                    1358 04155000
*           RESPECIFY                                              1359 04156000
*             NIPPAHDR BASED(NP3PBFR);                             1359 04157000
*         END;                                                     1360 04158000
*       ELSE                        /* YES, THIS IS FIRST PAREA      */ 04159000
*         DO;                                                      1361 04160000
         B     @RC01355                                            1361 04161000
@RF01355 DS    0H                                                  1362 04162000
*           NP3ADBFR='1'B;          /* SET INITIAL PAREA GOTTEN FLAG    04163000
*                                      ON                            */ 04164000
         OI    NP3ADBFR,B'00100000'                                1362 04165000
*         END;                                                     1363 04166000
*       NP3PAREA=NP3GBFR;           /* SAVE NEW BUFFER ADDRESS       */ 04167000
@RC01355 L     @10,NP3GBFR                                         1364 04168000
         ST    @10,NP3PAREA                                        1364 04169000
*       RESPECIFY                                                  1365 04170000
*         NIPPAHDR BASED(NP3GBFR);  /* RESET POINTER                 */ 04171000
*       NIPPABYT=NP3GBFR+LENGTH(NIPPAHDR);/* SET NEXT AVAILABLE BYTE    04172000
*                                      ADDRESS IN HEADER             */ 04173000
         LA    @09,8                                               1366 04174000
         ALR   @09,@10                                             1366 04175000
         ST    @09,NIPPABYT(,@10)                                  1366 04176000
*       RESPECIFY                                                  1367 04177000
*         NIPPAHDR BASED(NP3PBFR);  /* RESET POINTER                 */ 04178000
*       GO TO NP3RTRN0;             /* RETURN TO CALLER              */ 04179000
         L     @10,NP3RTRN                                         1368 04180000
         BR    @10                                                 1368 04181000
*     END;                                                         1369 04182000
*   ELSE                                                           1370 04183000
*     ;                             /* NO ,2K BUFFER WAS NOT       1370 04184000
*                                      ALLOCATED ,AN ERROR CONDITION    04185000
*                                      EXIST.                        */ 04186000
@RF01350 DS    0H                                                  1371 04187000
*   GO TO NP3SW38;                  /* GO TELL OPERATOR GETMAIN    1371 04188000
*                                      FAILED DURING INITIALIZATION  */ 04189000
         B     NP3SW38                                             1371 04190000
*/********************************************************************/ 04191000
*/*                                                                  */ 04192000
*/* MACRO-      IEAPSPIO                                             */ 04193000
*/*                                                                  */ 04194000
*/* STATUS-     VERSION DATE=10/24/73, VS/2 RELEASE 2                */ 04195000
*/*                                                                  */ 04196000
*/* FUNCTION-   IEAPSPIO IS PART OF THE  SUPERVISOR                  */ 04197000
*/*             INITIALIZATION PROCESS FOR VS2.                      */ 04198000
*/*                                                                  */ 04199000
*/*             THIS MACRO GENERATES INLINE PL/S STATEMENTS          */ 04200000
*/*             WHICH FORM A SUBROUTINE TO FIND AND READ TEXT        */ 04201000
*/*             RECORDS FROM SYS1.PARMLIB.                           */ 04202000
*/*                                                                  */ 04203000
*/*  REGISTER DEPENDENCIES-                                          */ 04204000
*/*                                                                  */ 04205000
*/*                 1. IEAPSPIO DEPENDS ON THE INCLUDING ROUTINES    */ 04206000
*/*                    BASE REGISTERS FOR ADDRESSABILITY             */ 04207000
*/*                                                                  */ 04208000
*/*                 2. REGISTER 3 MUST CONTAIN THE ADDRESS OF        */ 04209000
*/*                    THE CVT ON ENTRY                              */ 04210000
*/*                                                                  */ 04211000
*/*                 3. REGISTER 2 MUST CONTAIN THE ADDRESS OF        */ 04212000
*/*                    THE NVT ON ENTRY                              */ 04213000
*/*                                                                  */ 04214000
*/*                 4. REGISTER 14 MUST CONTAIN THE ADDRESS OF       */ 04215000
*/*                    THE CALLERS RETURN POINT ON ENTRY             */ 04216000
*/*                                                                  */ 04217000
*/* ENTRY POINTS-                                                    */ 04218000
*/*                                                                  */ 04219000
*/*    TWO CALLING SEQUENCRS ARE AVAILABLE.  ONE PROVIDES            */ 04220000
*/*    COMPATIBILITY WITH THE VS2-1 VERSION OF THIS MACRO,           */ 04221000
*/*    THE OTHER CALLING SEQUENCE WAS DEVELOPED EXPRESSLY            */ 04222000
*/*    FOR VS2-2.                                                    */ 04223000
*/*                                                                  */ 04224000
*/*        VS2-1 COMPATIBLE VERSION-                                 */ 04225000
*/*                                                                  */ 04226000
*/*            1. CALL NIPPMPDS TO LOCATE (VIA BLDL) THE MEMBER      */ 04227000
*/*               OF SYS1.PARMLIB                                    */ 04228000
*/*                                                                  */ 04229000
*/*            2. CALL NIPPMTXT TO READ A TEXT RECORD FROM           */ 04230000
*/*               SYS1.PARMLIB                                       */ 04231000
*/*                                                                  */ 04232000
*/*        VS2-2 VERSION-                                            */ 04233000
*/*                                                                  */ 04234000
*/*            1. CALL NIPPMMEM TO LOCATE THE MEMBER AND RETURN      */ 04235000
*/*               A TEXT RECORD                                      */ 04236000
*/*                                                                  */ 04237000
*/*            2. REPEAT CALL TO NIPPMMEM TO READ ADDITIONAL         */ 04238000
*/*               TEXT RECORDS                                       */ 04239000
*/*                                                                  */ 04240000
*/* OUTPUT (NORMAL) -                                                */ 04241000
*/*                                                                  */ 04242000
*/*     THE ROUTINE WILL SAVE AND RESTORE ALL GENERAL REGISTERS      */ 04243000
*/*     WITH THE EXCEPTION OF RPARM.  ON A SUCCESSFUL READ,          */ 04244000
*/*     RPARM (REGISTER 1) WILL BE SET TO POINT TO THE FIRST NON-    */ 04245000
*/*     BLANK CHARACTER IN THE TEXT RECORD.  IF THE RECORD           */ 04246000
*/*     COLUMNS 1 THRU 72 ALL CONTAIN BLANKS, RPARM WILL             */ 04247000
*/*     POINT TO A BLANK IN COLUMN 72.  IF COLUMNS 1 THRU 71         */ 04248000
*/*     ARE BLANK BUT COLUMN 72 IS NON-BLANK, THE ROUTINE            */ 04249000
*/*     WILL AUTOMATICALLY ATTEMPT TO READ THE NEXT RECORD.  IN      */ 04250000
*/*     ALL CASES, COLUMN 72 WILL BE SET TO BLANK AND COLUMN 73 WILL */ 04251000
*/*     BE SET TO X'FF'.                                             */ 04252000
*/*                                                                  */ 04253000
*/* EXIT (NORMAL)-                                                   */ 04254000
*/*                                                                  */ 04255000
*/*     VS2-1 AND VS2-2 VERSIONS RETURN TO THE ADDRESS FOUND         */ 04256000
*/*     IN REGISTER 14 ON ENTRY.                                     */ 04257000
*/*                                                                  */ 04258000
*/* EXIT (ERROR)-                                                    */ 04259000
*/*                                                                  */ 04260000
*/*     VS2-1 COMPATIBLE VERSION-                                    */ 04261000
*/*                                                                  */ 04262000
*/*         RETURN TO THE ADDRESS IN REGISTER 14 +4                  */ 04263000
*/*                                                                  */ 04264000
*/*         NOTE- WHEN AN ERROR OCCURS ON AN ATTEMP TO READ A RECORD */ 04265000
*/*               THAT DOES NOT EXIST (E-O-F),                       */ 04266000
*/*               IT IS CONSIDERED A PERMANENT I/O ERROR             */ 04267000
*/*                                                                  */ 04268000
*/*     VS2-2 VERSION-                                               */ 04269000
*/*                                                                  */ 04270000
*/*        RETURN TO THE ADDRESS FOUND IN REGISTER 14 ON ENTRY       */ 04271000
*/*        WITH REGISTER 1 (RPARM) SET TO A NEGATIVE VALUE.  IN THE  */ 04272000
*/*        CASE WHERE RPARM IS NEGATIVE ON RETURN, THE NIPPARC FIELD */ 04273000
*/*        OF THE NIP PATAMETER AREA WILL INDICATE THE REASON FOR    */ 04274000
*/*        FAILURE AS FOLLOWS-                                       */ 04275000
*/*                            4 - MEMBER NOT FOUND                  */ 04276000
*/*                            8 - I/O ERROR DURING BLDL             */ 04277000
*/*                            12- I/O ERROR DURING READ             */ 04278000
*/*                            16- END-OF-FILE                       */ 04279000
*/*                                                                  */ 04280000
*/*     COMMON TO BOTH VERSIONS-                                     */ 04281000
*/*                                                                  */ 04282000
*/*        AN INTERPRETIVE MESSAGE WILL BE WRITTEN TO THE OPERATOR   */ 04283000
*/*        FOR EACH OF THE FIRST THREE ERROR CONDITIONS LISTED       */ 04284000
*/*        ABOVE.  THE CALLER MAY SUPPRESS THE 'MEMBER NOT FOUND'    */ 04285000
*/*        MESSAGE BY SETTING THE NIPPAFL1 FLAG FIELD TO BINARY      */ 04286000
*/*        ONE IN THE NIP PARAMETER AREA BEFORE CALLING THIS         */ 04287000
*/*        ROUTINE.  NO MESSAGE WILL BE WRITTEN FOR THE E-O-F        */ 04288000
*/*        CONDITION IF THE VS2-2 VERSION IS USED.                   */ 04289000
*/*                                                                  */ 04290000
*/* MACROS-                                                          */ 04291000
*/*     THE FOLLOWING MAPPING MACROS MUST BE                         */ 04292000
*/*     INCLUDED IN THE MODULE WHICH INCLUDES                        */ 04293000
*/*     IEAPSPIO-                                                    */ 04294000
*/*                                       IEZIOB                     */ 04295000
*/*                                       IHANVT                     */ 04296000
*/*                                       IHAECB                     */ 04297000
*/*                                       IEAPPNIP                   */ 04298000
*/*                                       IEFUCBOB                   */ 04299000
*/*                                       CVT                        */ 04300000
*/*                                                                  */ 04301000
*/* NOTES-                                                           */ 04302000
*/*        THE FIRST FOUR LETTERS OF ALL IEAPSPIO SYMBOLES ARE:      */ 04303000
*/*        NIPP OR IPPM.                                             */ 04304000
*/*                                                                  */ 04305000
*/*        REGISTERS 0, 1, 14, AND 15 WILL BE UNRESTRICTED AT THE    */ 04306000
*/*        END OF THE SEGMENT GENERATED BY IEAPSPIO.                 */ 04307000
*/*                                                                  */ 04308000
*/*        NIPPAREA WILL BE BASED ON NVTPAREA AND IOBSTDRD WILL BE   */ 04309000
*/*        BASED ON THE ADDR OF NIPPAIOB AT THE END OF THE SEGMENT   */ 04310000
*/*        GENERATED BY IEAPSPIO                                     */ 04311000
*/*                                                                  */ 04312000
*/*        IEAPSPIO MUST BE INCLUDED IN A CODE SEGMENT,              */ 04313000
*/*        NOT AT THE TOP OF THE PROCEDURE WITH THE                  */ 04314000
*/*        MAPPING MACROS LISTED ABOVE.                              */ 04315000
*/*                                                            Y02752*/ 04316000
*/********************************************************************/ 04317000
*                                                                  1372 04318000
*   /*****************************************************************/ 04319000
*   /*                                                               */ 04320000
*   /* REGISTERS                                                     */ 04321000
*   /*                                                               */ 04322000
*   /*****************************************************************/ 04323000
*                                                                  1372 04324000
*   DCL                                                            1372 04325000
*     NIPPMR00 REG(0) PTR(24);                                     1372 04326000
*   DCL                                                            1373 04327000
*     NIPPMR01 REG(1) PTR(24);                                     1373 04328000
*   DCL                                                            1374 04329000
*     NIPPMR02 REG(2) PTR(24);                                     1374 04330000
*   DCL                                                            1375 04331000
*     NIPPMR06 REG(6) PTR(24);                                     1375 04332000
*   DCL                                                            1376 04333000
*     NIPPMR09 REG(9) PTR(24);                                     1376 04334000
*   DCL                                                            1377 04335000
*     NIPPMR12 REG(12) PTR(24);                                    1377 04336000
*   DCL                                                            1378 04337000
*     NIPPMR13 REG(13) PTR(24);                                    1378 04338000
*   DCL                                                            1379 04339000
*     NIPPMR14 REG(14) PTR(24);                                    1379 04340000
*   DCL                                                            1380 04341000
*     NIPPMR15 REG(15) PTR(24);                                    1380 04342000
*                                                                  1380 04343000
*   /*****************************************************************/ 04344000
*   /*                                                               */ 04345000
*   /* SAVE AREA FOR CALLER'S REGISTERS                              */ 04346000
*   /*                                                               */ 04347000
*   /*****************************************************************/ 04348000
*                                                                  1381 04349000
*   DCL                                                            1381 04350000
*     NIPPMREG(16) FIXED(31);                                      1381 04351000
*                                                                  1381 04352000
*   /*****************************************************************/ 04353000
*   /*                                                               */ 04354000
*   /* SAVE AREA FOR CALLER'S RETURN ADDRESS                         */ 04355000
*   /*                                                               */ 04356000
*   /*****************************************************************/ 04357000
*                                                                  1382 04358000
*   DCL                                                            1382 04359000
*     NIPPMADR PTR(31);                                            1382 04360000
*                                                                  1382 04361000
*   /*****************************************************************/ 04362000
*   /*                                                               */ 04363000
*   /* POINTER TO DCB FOR BLDL                                       */ 04364000
*   /*                                                               */ 04365000
*   /*****************************************************************/ 04366000
*                                                                  1383 04367000
*   DCL                                                            1383 04368000
*     NIPPMDCB PTR(31);                                            1383 04369000
*                                                                  1383 04370000
*   /*****************************************************************/ 04371000
*   /*                                                               */ 04372000
*   /* POINTER TO BLDL HEADER FOR BLDL                               */ 04373000
*   /*                                                               */ 04374000
*   /*****************************************************************/ 04375000
*                                                                  1384 04376000
*   DCL                                                            1384 04377000
*     NIPPMBDL PTR(31);                                            1384 04378000
*                                                                  1384 04379000
*   /*****************************************************************/ 04380000
*   /*                                                               */ 04381000
*   /* NORMAL RETURN TO CALLER                                       */ 04382000
*   /*                                                               */ 04383000
*   /*****************************************************************/ 04384000
*                                                                  1385 04385000
*   DCL                                                            1385 04386000
*     NIPPMAD0 LABEL BASED(NIPPMADR);                              1385 04387000
*                                                                  1385 04388000
*   /*****************************************************************/ 04389000
*   /*                                                               */ 04390000
*   /* ERROR RETURN TO CALLER                                        */ 04391000
*   /*                                                               */ 04392000
*   /*****************************************************************/ 04393000
*                                                                  1386 04394000
*   DCL                                                            1386 04395000
*     NIPPMAD4 LABEL BASED(NIPPMADR+4);                            1386 04396000
*                                                                  1386 04397000
*   /*****************************************************************/ 04398000
*   /*                                                               */ 04399000
*   /* ESTABLISH BASE FOR NIP COMMUNICATION AREA               Y02752*/ 04400000
*   /*                                                               */ 04401000
*   /*****************************************************************/ 04402000
*                                                                  1387 04403000
*   RESPECIFY                                                      1387 04404000
*     NIPPAREA BASED(NVTPAREA);     /*                         Y02752*/ 04405000
*                                                                  1387 04406000
*   /*****************************************************************/ 04407000
*   /*                                                               */ 04408000
*   /* ONE BYTE SCANNING FIELD                                       */ 04409000
*   /*                                                               */ 04410000
*   /*****************************************************************/ 04411000
*                                                                  1388 04412000
*   DCL                                                            1388 04413000
*     NIPPMBYT CHAR(1) BASED;                                      1388 04414000
*                                                                  1388 04415000
*   /*****************************************************************/ 04416000
*   /*                                                               */ 04417000
*   /* TEMPORARY POINTER                                       Y02752*/ 04418000
*   /*                                                               */ 04419000
*   /*****************************************************************/ 04420000
*                                                                  1389 04421000
*   DCL                                                            1389 04422000
*     NIPPMKPR PTR(31);             /*                         Y02752*/ 04423000
*                                                                  1389 04424000
*   /*****************************************************************/ 04425000
*   /*                                                               */ 04426000
*   /* INPUT RECORD READ FROM PARMLIB                          Y02752*/ 04427000
*   /*                                                               */ 04428000
*   /*****************************************************************/ 04429000
*                                                                  1390 04430000
*   DCL                                                            1390 04431000
