*                                                                  1122 03016000
*   /*****************************************************************/ 03017000
*   /*                                                               */ 03018000
*   /* THIS SUBROUTINE (NP3SCAN) SCANS THE SYSTEM PARAMETERS FOR     */ 03019000
*   /* VALIDITY. AFTER VERIFYING ONE LINE OF PARAMETERS,IT BRANCHES  */ 03020000
*   /* BACK TO ITS CALLER AT: +0 FOR VALID PARAMETER AND SUBPARAMETER*/ 03021000
*   /* OF PARAMETER +4 FOR INVALID PARAMETER OR SUBPARAMETER OF      */ 03022000
*   /* PARAMETER +8 FOR CONTINUATION OF PARAMETERS OR SUBPARAMETERS  */ 03023000
*   /*                                                               */ 03024000
*   /*****************************************************************/ 03025000
*                                                                  1122 03026000
*   RESPECIFY                                                      1122 03027000
*     SYMBLREG RESTRICTED;          /* BASE ADDRESS OF SYMBOL TABLE     03028000
*                                      FOR MAPPING                   */ 03029000
*   RESPECIFY                                                      1123 03030000
*     OFFSTREG RESTRICTED;          /* CONTAINS OFFSET INTO SYMBOL 1123 03031000
*                                      TABLE FOR PARAMETER VALIDATION*/ 03032000
*   RESPECIFY                                                      1124 03033000
*     NP3SCAN5 BASED(SCANREG);                                     1124 03034000
*   RESPECIFY                                                      1125 03035000
*     OPLIBENT BASED(NP3CENT);                                     1125 03036000
*NP3SCAN:                                                          1126 03037000
*   NP3RTRN=REXIT;                  /* SAVE RETURN ADDRESS OF CALLER */ 03038000
NP3SCAN  ST    REXIT,NP3RTRN                                       1126 03039000
*NP3SCNXT:                                                         1127 03040000
*   SCANREG=NP3NCHAR;               /* SET BASE ADDRESS FOR MAPPING     03041000
*                                      INTO PARAMETER CHARACTER    1127 03042000
*                                                              STRING*/ 03043000
*                                                                  1127 03044000
NP3SCNXT L     SCANREG,NP3NCHAR                                    1127 03045000
*   /*****************************************************************/ 03046000
*   /*                                                               */ 03047000
*   /* CHECK TO IF CURRENT CHARACTER IS BLANK OR COMMA. IF YES       */ 03048000
*   /* ,UPDATE TO NEXT CHARACTER IN PARAMETER STRING AND REPEAT      */ 03049000
*   /* CHARACTER CHECK.                                              */ 03050000
*   /*                                                               */ 03051000
*   /*****************************************************************/ 03052000
*                                                                  1128 03053000
*NP3PCHAR:                                                         1128 03054000
*   IF NP3SCANR=NP3BLANK            /* IS THIS A BLANK CHARACTER OR  */ 03055000
*       |NP3SCANR=NP3COMMA          /* A COMMA CHARACTER             */ 03056000
*     THEN                          /* YES,IT IS A BLANK OR A COMMA  */ 03057000
NP3PCHAR CLC   NP3SCANR(1,SCANREG),NP3BLANK                        1128 03058000
         BE    @RT01128                                            1128 03059000
         CLC   NP3SCANR(1,SCANREG),NP3COMMA                        1128 03060000
         BNE   @RF01128                                            1128 03061000
@RT01128 DS    0H                                                  1129 03062000
*     DO;                                                          1129 03063000
*       SCANREG=1+SCANREG;          /* UPDATE TO NEXT CHARACTER      */ 03064000
         LA    SCANREG,1(,SCANREG)                                 1130 03065000
*       GO TO NP3PCHAR;             /* GO BACK TO CHARACTER CHECK    */ 03066000
         B     NP3PCHAR                                            1131 03067000
*     END;                                                         1132 03068000
*   ELSE                                                           1133 03069000
*     ;                             /* NO, IT IS NOT A BLANK OR A  1133 03070000
*                                      COMMA,FALL THROUGH            */ 03071000
*                                                                  1133 03072000
@RF01128 DS    0H                                                  1134 03073000
*   /*****************************************************************/ 03074000
*   /*                                                               */ 03075000
*   /* CHECK TO SEE IF CONTINUE SUBPARAMETER SWITCH IS ON.IF YES,THEN*/ 03076000
*   /* BRANCH TO SETUP FOR CONTINUATION OF SUBPARAMETERS FROM        */ 03077000
*   /* PREVIOUS LINE OF SYSTEM PARAMETERS                            */ 03078000
*   /*                                                               */ 03079000
*   /*****************************************************************/ 03080000
*                                                                  1134 03081000
*   IF NP3CONT='1'B                 /* IS CONTINUE PARAMETER FLAG ON */ 03082000
*     THEN                          /* YES,IT IS ON                  */ 03083000
         TM    NP3CONT,B'10000000'                                 1134 03084000
         BO    @RT01134                                            1134 03085000
*     GO TO NP3CHNDL;               /* BRANCH TO CONTINUE          1135 03086000
*                                      SUBPARAMETER BALANCING CHECK     03087000
*                                      SETUP                         */ 03088000
*   ELSE                            /* NO,CONTINUE PARAMETER FLAG NOT   03089000
*                                      ON                            */ 03090000
*     DO;                                                          1136 03091000
*       NP3CCHAR=SCANREG;           /* SET CURRENT CHARACTER POINTER    03092000
*                                      TO ADDRESS OF SCANNED       1137 03093000
*                                      CHARACTER                     */ 03094000
         ST    SCANREG,NP3CCHAR                                    1137 03095000
*     END;                                                         1138 03096000
*                                                                  1138 03097000
*   /*****************************************************************/ 03098000
*   /*                                                               */ 03099000
*   /* CHECK FOR END OF CURRENT LINE OF PARAMETER CHARACTER          */ 03100000
*   /* STRING.LOOKS FOR A QUOTE.                                     */ 03101000
*   /*                                                               */ 03102000
*   /*****************************************************************/ 03103000
*                                                                  1139 03104000
*   IF NP3SCANR=NP3QUOTE            /* IS THIS END OF LINE           */ 03105000
*     THEN                          /* YES,IT IS END OF LINE         */ 03106000
         CLC   NP3SCANR(1,SCANREG),NP3QUOTE                        1139 03107000
         BNE   @RF01139                                            1139 03108000
*     DO;                                                          1140 03109000
*       IF OPSPCONT^='00000000000000000000000000000000'X/* IS 'CONT'    03110000
*                                      SPECIFIED                     */ 03111000
*         THEN                      /* YES- RESET 'CONT' PARM ENTRY     03112000
*                                      TO ZERO                       */ 03113000
         L     @10,NP3CTAB                                         1141 03114000
