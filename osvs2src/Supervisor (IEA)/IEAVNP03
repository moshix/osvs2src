*   DCL                                                            1399 04502000
*     NIPPMTND LABEL GENERATED INTERNAL;                           1399 04503000
*                                                                  1399 04504000
*   /*****************************************************************/ 04505000
*   /*                                                               */ 04506000
*   /* MAPPING AREAS INTO ERROR MESSAGES                             */ 04507000
*   /*                                                               */ 04508000
*   /*****************************************************************/ 04509000
*                                                                  1400 04510000
*   DCL                                                            1400 04511000
*     NIPPMME1 CHAR(8) BASED(ADDR(NIPPMMB1));                      1400 04512000
*   DCL                                                            1401 04513000
*     NIPPMME2 CHAR(8) BASED(ADDR(NIPPMMB2));                      1401 04514000
*   DCL                                                            1402 04515000
*     NIPPMNAM CHAR(8) INIT(' ');   /* MEMBER NAME SAVE AREA   Y02752*/ 04516000
*   DCL                                                            1403 04517000
*     NIPPMNEG FIXED(31) INIT('FFFFFFFF'X);/* NEGATIVE VALUE   Y02752*/ 04518000
*   RESPECIFY                                                      1404 04519000
*     IOBSTDRD BASED(ADDR(NIPPAIOB));/* IOB BASE               Y02752*/ 04520000
*   RESPECIFY                                                      1405 04521000
*    (NIPPMR14,                                                    1405 04522000
*     NIPPMR15,                                                    1405 04523000
*     NIPPMR00,                                                    1405 04524000
*     NIPPMR01) RESTRICTED;         /*                         Y02752*/ 04525000
*NIPPMMEM:                                                         1406 04526000
*   NIPPMADR=NIPPMR14;              /* SAVE CALLER'S RETURN REGISTER    04527000
*                                                              Y02752*/ 04528000
NIPPMMEM ST    NIPPMR14,NIPPMADR                                   1406 04529000
*   GENERATE REFS(NIPPMR01,NIPPMR02) SETS(NIPPMREG);               1407 04530000
          STM   NIPPMR02,NIPPMR01,NIPPMREG   SAVE REGISTERS     Y02752  04531000
*   NIPPARC=NIPPARC&&NIPPARC;       /* SET RETURN CODE TO ZERO Y02752*/ 04532000
         L     @10,NVTPAREA(,RNVT)                                 1408 04533000
         L     @09,NIPPARC(,@10)                                   1408 04534000
         LR    @07,@09                                             1408 04535000
         XR    @07,@09                                             1408 04536000
         ST    @07,NIPPARC(,@10)                                   1408 04537000
*   IF NIPPMNAM=NIPPANAM            /* IS THIS NEW MEMBER NAME Y02752*/ 04538000
*     THEN                                                         1409 04539000
         CLC   NIPPMNAM(8),NIPPANAM(@10)                           1409 04540000
         BE    @RT01409                                            1409 04541000
*     GO TO NIPPMPRD;               /* NO- GO READ RECORD      Y02752*/ 04542000
*   NIPPMNAM=NIPPANAM;              /* YES- SAVE MEMBER NAME   Y02752*/ 04543000
         MVC   NIPPMNAM(8),NIPPANAM(@10)                           1411 04544000
*   GO TO NIPPMENT;                 /* GO FIND MEMBER          Y02752*/ 04545000
         B     NIPPMENT                                            1412 04546000
*NIPPMPDS:                                                         1413 04547000
*   NIPPMADR=NIPPMR14;              /* SAVE CALLER'S RETURN ADDR     */ 04548000
