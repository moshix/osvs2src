         OI    NIPPTLST(@10),B'01000000'                           0726 01557000
*               GO TO NP3NXFLD;     /* BRANCH TO SET UP FOR NEXT   0727 01558000
*                                      ENTRY MERG                    */ 01559000
         B     NP3NXFLD                                            0727 01560000
*             END;                                                 0728 01561000
*           ELSE                                                   0729 01562000
*             DO;                                                  0729 01563000
