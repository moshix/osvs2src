         BR    @10                                                 1290 03886000
*         END;                                                     1291 03887000
*       ELSE                        /* NO, CONTINUE SUBPARAMETERS NOT   03888000
*                                      SPECIFIED, IMPLIES          1292 03889000
*                                      SUBPARAMETERS ARE INVALID     */ 03890000
*         GO TO NP3PRINV;           /* BRANCH TO INVALID           1292 03891000
*                                      SUBPARAMETERS RETURN HANDLER  */ 03892000
*     END;                                                         1293 03893000
*   ELSE                            /* NO, CHARACTER IS NOT A QUOTE  */ 03894000
*     GO TO NP3PRINV;               /* BRANCH TO INVALID           1294 03895000
*                                      SUBPARAMETERS RETURN HANDLER  */ 03896000
*NP3RECNT:                                                         1295 03897000
*   SCANREG=1+SCANREG;              /* UPDATE SCAN POINTER           */ 03898000
