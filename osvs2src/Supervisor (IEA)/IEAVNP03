         TM    NIPPTMRG(@10),B'10000000'                           0741 01602000
         BNO   @RF00741                                            0741 01603000
*                 DO;                                              0742 01604000
*                                                                  0742 01605000
*                   /*************************************************/ 01606000
*                   /*                                               */ 01607000
*                   /* DECREMENT COUNT CONTROL ELEMENT FOR THIS ENTRY*/ 01608000
*                   /*                                               */ 01609000
*                   /*************************************************/ 01610000
*                                                                  0743 01611000
*                   COUNTREG=COUNTREG-1;                           0743 01612000
*                                                                  0743 01613000
         BCTR  COUNTREG,0                                          0743 01614000
*                   /*************************************************/ 01615000
*                   /*                                               */ 01616000
*                   /* MOVE OPERATOR SPECIFIED PARAMETER ENTRY       */ 01617000
*                   /* CONTENTS INTO PARMLIB PARMTAB ENTRY.          */ 01618000
*                   /*                                               */ 01619000
*                   /*************************************************/ 01620000
*                                                                  0744 01621000
*                   PARMTABO=OPERTABC;                             0744 01622000
*                                                                  0744 01623000
         LA    @10,16                                              0744 01624000
         LR    @09,PARMREG                                         0744 01625000
