*                                      PARENS                        */ 03790000
NP3PRINV ICM   @10,15,NP3PAREN                                     1271 03791000
         BZ    @RF01271                                            1271 03792000
*     DO;                                                          1272 03793000
*       CAUSE04=IMBPAR;                                            1273 03794000
         MVC   CAUSE04(20),IMBPAR                                  1273 03795000
*     END;                                                         1274 03796000
*NP3PINV2:                                                         1275 03797000
*   NP3NCHAR=SCANREG;               /* SET NEXT CHAR. POINTER        */ 03798000
*                                                                  1275 03799000
@RF01271 DS    0H                                                  1275 03800000
NP3PINV2 ST    SCANREG,NP3NCHAR                                    1275 03801000
*   /*****************************************************************/ 03802000
*   /*                                                               */ 03803000
*   /* WTO INVALID PARAMETER SYNTAX MESSAGE                          */ 03804000
*   /*                                                               */ 03805000
*   /*****************************************************************/ 03806000
*                                                                  1276 03807000
*   CALL NP3PINV;                   /*                         Y02752*/ 03808000
*                                                                  1276 03809000
         BAL   @14,NP3PINV                                         1276 03810000
*   /*****************************************************************/ 03811000
*   /*                                                               */ 03812000
*   /* RETURN TO NIP PROMPT CONTROL SEQUENCE IF IN PROMPT MODE Y02752*/ 03813000
*   /*                                                               */ 03814000
*   /*****************************************************************/ 03815000
*                                                                  1277 03816000
*   IF NVTSYSP='1'B                 /*                         Y02752*/ 03817000
*     THEN                          /*                         Y02752*/ 03818000
         TM    NVTSYSP(RNVT),B'01000000'                           1277 03819000
         BNO   @RF01277                                            1277 03820000
*     DO;                           /*                         Y02752*/ 03821000
*       RETCODE=12;                 /* INVALID MSG ISSUED RTN CODE 1279 03822000
*                                                              Y02752*/ 03823000
         MVC   RETCODE(4),@CF00036                                 1279 03824000
*       GO TO NPRMRTN1;             /* RETURN TO IEAVNPA3 CALL     1280 03825000
*                                      SEQUENCE                Y02752*/ 03826000
