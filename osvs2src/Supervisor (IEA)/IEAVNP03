*   SAVESCAN=SCANREG;               /* SAVE SYSP POINTER             */ 00929000
*                                                                  0553 00930000
         ST    SCANREG,SAVESCAN                                    0553 00931000
*   /*****************************************************************/ 00932000
*   /*                                                               */ 00933000
