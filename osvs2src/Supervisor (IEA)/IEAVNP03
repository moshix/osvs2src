*   GO TO NP3MRGST;                 /* BRANCH TO MERGE SEQUENCE      */ 01714000
*                                                                  0761 01715000
         B     NP3MRGST                                            0761 01716000
*   /*****************************************************************/ 01717000
*   /*                                                               */ 01718000
*   /* THIS SUBROUTINE SETS UP TO ISSUE A PARAMETER IGNORE MESSAGE TO*/ 01719000
*   /* THE OPERATOR AND CONVERTS THE NEGATIVE CONTENT OF THE CURRENT */ 01720000
*   /* MERGE ENTRY IN THE PARMLIB PARMTAB                            */ 01721000
*   /*                                                               */ 01722000
*   /*****************************************************************/ 01723000
*                                                                  0762 01724000
*   RESPECIFY                                                      0762 01725000
*     SYMBLREG RESTRICTED;          /* BASED ADDRESS OF IGNORED    0762 01726000
*                                      PARAMETER SYMBOL FROM SYMBOL     01727000
*                                      TABLE                         */ 01728000
*NP3IGNOP:                                                         0763 01729000
*   SYMBLREG=INDEXREG/2+ADDR(NP3SYMBL);/* LOCATE ERROR PARAMETER   0763 01730000
*                                      SYMBOL IN SYMBOL TABLE        */ 01731000
NP3IGNOP LR    SYMBLREG,INDEXREG                                   0763 01732000
         SRL   SYMBLREG,1                                          0763 01733000
         LA    @10,NP3SYMBL                                        0763 01734000
         ALR   SYMBLREG,@10                                        0763 01735000
*   PRMIGNOR=SYMBLID;               /* MOVE SYMBOL INTO IGNORE     0764 01736000
*                                      MESSAGE                       */ 01737000
*                                                                  0764 01738000
         MVC   PRMIGNOR(8),SYMBLID(SYMBLREG)                       0764 01739000
*   /*****************************************************************/ 01740000
*   /*                                                               */ 01741000
*   /* CHECKS THE LENGTH OF SYMBOL MOVED INTO IGNORE MESSAGE TEXT FOR*/ 01742000
*   /* VALUE GREATER THEN SEVEN. IF YES, NO OVERSTRIKE OF LENGTH     */ 01743000
*   /* VALUE IN TEXT IS NEEDED.                                      */ 01744000
*   /*                                                               */ 01745000
*   /*****************************************************************/ 01746000
*                                                                  0765 01747000
*   IF SYMLNGTH>LENGTH(SYMBSTRT)-1  /* LENGTH VALUE GREATER THEN 7   */ 01748000
*     THEN                          /* YES, NO OVERSTRIKE IS NEEDED  */ 01749000
         CLI   SYMLNGTH,7                                          0765 01750000
         BH    @RT00765                                            0765 01751000
*     GO TO NP3IMSG;                /* BRANCH TO NIPWTO INTERFACE  0766 01752000
*                                      BRANCH CALL                   */ 01753000
*   ELSE                            /* NO, OVERSTRIKE OF LENGTH VALUE   01754000
*                                      NEEDED                        */ 01755000
*     DO;                                                          0767 01756000
*       SYMBREP=NP3BLANK;           /* MOVE BLANK ONTO LENGTH VALUE  */ 01757000
