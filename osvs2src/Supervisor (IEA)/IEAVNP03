         TITLE 'IEAVNP03 - PARAMETER ANALYSIS/LINKLIB                  *00001000
                        '                                               00002000
IEAVNP03 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(19)                                             0001 00007000
         DC    C'IEAVNP03R0300076086'                              0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@00,12(@13)                                     0001 00010000
         STM   @02,@12,28(@13)                                     0001 00011000
         BALR  @11,0                                               0001 00012000
@PSTART  LA    @12,4095(,@11)                                      0001 00013000
         USING @PSTART,@11                                         0001 00014000
         USING @PSTART+4095,@12                                    0001 00015000
          USING @PSTART+8190,@13                                        00016000
          ST    @13,NP3R13                 SAVE SAVEAREA ADDR           00017000
          LA    @13,4095(0,@12)            R13 = THIRD BASE REG         00018000
*                                                                       00019000
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF          00020000
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES      00021000
** WILL ASSEMBLE PROPERLY.                                              00022000
*                                                                  0373 00023000
*   /*****************************************************************/ 00024000
*   /*                                                               */ 00025000
*   /* RESTRICT BASE ADDRESS REGISTER FOR THE CVT                    */ 00026000
*   /*                                                               */ 00027000
*   /*****************************************************************/ 00028000
*                                                                  0374 00029000
*   NP3SAV1=RPARM;                  /* SAVE PARAMETER ADDR     Y02752*/ 00030000
         ST    RPARM,NP3SAV1                                       0374 00031000
*   NP3FLAGS=NP3FLAGS&&NP3FLAGS;    /* SET CONTROL FLAG OFF          */ 00032000
         XC    NP3FLAGS(3),NP3FLAGS                                0375 00033000
*   NP3MSGCL=''B;                   /* CLEAR ERROR TEXT FIELD  Y02752*/ 00034000
         XC    NP3MSGCL(8),NP3MSGCL                                0376 00035000
*   RESPECIFY                                                      0377 00036000
*    (NP3BKADR,                                                    0377 00037000
*     NP3BKLN) RESTRICTED;          /*                         Y02752*/ 00038000
*                                                                  0377 00039000
*   /*****************************************************************/ 00040000
*   /*                                                               */ 00041000
*   /* TRANSFER CONTROL TO THE NIP PROMPT CALL SEQUENCE IF THE NIP03 */ 00042000
*   /* INITIALIZATION FUNCTION HAS COMPLETED.                  Y02752*/ 00043000
*   /*                                                               */ 00044000
*   /*****************************************************************/ 00045000
*                                                                  0378 00046000
*   IF NVTSYSP='1'B                 /*                         Y02752*/ 00047000
*     THEN                          /*                         Y02752*/ 00048000
         TM    NVTSYSP(RNVT),B'01000000'                           0378 00049000
         BNO   @RF00378                                            0378 00050000
*     CALL NPRMTRA3;                /*                         Y02752*/ 00051000
         BAL   @14,NPRMTRA3                                        0379 00052000
*   ELSE                            /*                         Y02752*/ 00053000
*     DO;                           /*                         Y02752   00054000
*                                                              Y02752*/ 00055000
         B     @RC00378                                            0380 00056000
@RF00378 DS    0H                                                  0381 00057000
*       RESPECIFY                                                  0381 00058000
*         RPARM UNRESTRICTED;       /*                         Y02752*/ 00059000
*       NP3BKADR=ADDR(OPTABLN);     /* ADDR OF AREA TO BE CLEARED  0382 00060000
*                                                              Y02752*/ 00061000
         LA    NP3BKADR,OPTABLN                                    0382 00062000
*       NP3BKLN=LENGTH(OPTABLN);    /* LENGHT OF AREA TO BE CLEARED     00063000
*                                                              Y02752*/ 00064000
*                                                                  0383 00065000
         LA    NP3BKLN,640                                         0383 00066000
*       /*************************************************************/ 00067000
*       /*                                                           */ 00068000
*       /* CLEAR OPERATOR PARM TABLE                           Y02752*/ 00069000
*       /*                                                           */ 00070000
*       /*************************************************************/ 00071000
*                                                                  0384 00072000
*       CALL NP3CLRIT;              /*                         Y02752   00073000
*                                                              Y02752*/ 00074000
         BAL   @14,NP3CLRIT                                        0384 00075000
*       NP3BKADR=ADDR(PTABLN);      /* ADDR OF AREA TO BE CLEARED  0385 00076000
*                                                              Y02752*/ 00077000
         LA    NP3BKADR,PTABLN                                     0385 00078000
*       NP3BKLN=LENGTH(PTABLN);     /* LENGHT OF AREA TO BE CLEARED     00079000
*                                                              Y02752*/ 00080000
*                                                                  0386 00081000
         LA    NP3BKLN,640                                         0386 00082000
*       /*************************************************************/ 00083000
*       /*                                                           */ 00084000
*       /* CLEAR PARMLIB PARM TABLE                            Y02752*/ 00085000
*       /*                                                           */ 00086000
*       /*************************************************************/ 00087000
*                                                                  0387 00088000
*       CALL NP3CLRIT;              /*                         Y02752*/ 00089000
         BAL   @14,NP3CLRIT                                        0387 00090000
*       RESPECIFY                                                  0388 00091000
*        (NP3BKADR,                                                0388 00092000
*         NP3BKLN) UNRESTRICTED;    /*                         Y02752   00093000
*                                                              Y02752*/ 00094000
*                                                                  0388 00095000
*       /*************************************************************/ 00096000
*       /*                                                           */ 00097000
*       /* CALL ROUTINE TO ANALYZE PARAMETERS FROM OPERATOR    Y02752*/ 00098000
*       /*                                                           */ 00099000
*       /*************************************************************/ 00100000
*                                                                  0389 00101000
*       CALL NP3OPSP;               /*                         Y02752   00102000
*                                                              Y02752*/ 00103000
*                                                                  0389 00104000
         BAL   @14,NP3OPSP                                         0389 00105000
*       /*************************************************************/ 00106000
*       /*                                                           */ 00107000
*       /* CALL ROUTINE TO DEFINE INITIAL PARMAREA BASE        Y02752*/ 00108000
*       /*                                                           */ 00109000
*       /*************************************************************/ 00110000
*                                                                  0390 00111000
*       CALL NP3PBASE;              /*                         Y02752   00112000
*                                                              Y02752*/ 00113000
*                                                                  0390 00114000
         BAL   @14,NP3PBASE                                        0390 00115000
*       /*************************************************************/ 00116000
*       /*                                                           */ 00117000
*       /* CALL ROUTINE TO OPEN SYS1.PARMLIB                   Y02752*/ 00118000
*       /*                                                           */ 00119000
*       /*************************************************************/ 00120000
*                                                                  0391 00121000
*       CALL NP3PMLIB;              /*                         Y02752   00122000
*                                                              Y02752*/ 00123000
*                                                                  0391 00124000
         BAL   @14,NP3PMLIB                                        0391 00125000
*       /*************************************************************/ 00126000
*       /*                                                           */ 00127000
*       /* CALL ROUTINE TO READ AND ANALYZE PARMLIB PARAMETERS Y02752*/ 00128000
*       /*                                                           */ 00129000
*       /*************************************************************/ 00130000
*                                                                  0392 00131000
*       CALL NP3SYSP;               /*                         Y02752   00132000
*                                                              Y02752*/ 00133000
*                                                                  0392 00134000
         BAL   @14,NP3SYSP                                         0392 00135000
*       /*************************************************************/ 00136000
*       /*                                                           */ 00137000
*       /* CALL ROUTINE TO MERGE OPERATOR AND PARMLIB INPUT    Y02752*/ 00138000
*       /*                                                           */ 00139000
*       /*************************************************************/ 00140000
*                                                                  0393 00141000
*       CALL NP3PLMRG;              /*                         Y02752   00142000
*                                                              Y02752*/ 00143000
*                                                                  0393 00144000
         BAL   @14,NP3PLMRG                                        0393 00145000
*       /*************************************************************/ 00146000
*       /*                                                           */ 00147000
*       /* CALL ROUTINE TO INITIALIZE PARMTAB (IN PARMAREA)    Y02752*/ 00148000
*       /*                                                           */ 00149000
*       /*************************************************************/ 00150000
*                                                                  0394 00151000
*       CALL NP3PTAB;               /*                         Y02752   00152000
*                                                              Y02752*/ 00153000
         BAL   @14,NP3PTAB                                         0394 00154000
*       NVTSYSP='1'B;               /* INDICATE THAT THE NIP03     0395 00155000
*                                      PARMLIB INITIALIZATION      0395 00156000
*                                      FUNCTION HAS COMPLETED  Y02752*/ 00157000
         OI    NVTSYSP(RNVT),B'01000000'                           0395 00158000
*       NP3SLR13=NP3R13;            /* SAVE R13 TO ALLOW SECONDARY 0396 00159000
*                                      ENTRY FOR NIP PROMPT    Y02752   00160000
*                                                              Y02752*/ 00161000
*                                                                  0396 00162000
         MVC   NP3SLR13(4),NP3R13                                  0396 00163000
*       /*************************************************************/ 00164000
*       /*                                                           */ 00165000
*       /* CALL ROUTINE TO OPEN SYS1.LINKLIB                   Y02752*/ 00166000
*       /*                                                           */ 00167000
*       /*************************************************************/ 00168000
*                                                                  0397 00169000
*       CALL NP3LKLIB;              /*                         Y02752   00170000
*                                                              Y02752*/ 00171000
*                                                                  0397 00172000
         BAL   @14,NP3LKLIB                                        0397 00173000
*       /*************************************************************/ 00174000
*       /*                                                           */ 00175000
*       /* CALL ROUTINE TO DEFINE LINKLIB CONCATENATIONS       Y02752*/ 00176000
*       /*                                                           */ 00177000
*       /*************************************************************/ 00178000
*                                                                  0398 00179000
*       CALL NP3LCAT;               /*                         Y02752   00180000
*                                                              Y02752*/ 00181000
         BAL   @14,NP3LCAT                                         0398 00182000
*       NP3R13=NP3SLR13;            /* RESTORE R13 SAVE AREA   Y02752*/ 00183000
         MVC   NP3R13(4),NP3SLR13                                  0399 00184000
*     END;                          /*                         Y02752   00185000
*                                                              Y02752*/ 00186000
*                                                                  0400 00187000
*   /*****************************************************************/ 00188000
*   /*                                                               */ 00189000
*   /* EXIT TO IEAVNIPM CONTROL ROUTINE                        Y02752*/ 00190000
*   /*                                                               */ 00191000
*   /*****************************************************************/ 00192000
*                                                                  0401 00193000
*   GO TO NP3EXIT;                  /*                         Y02752   00194000
*                                                              Y02752*/ 00195000
*                                                                  0401 00196000
         B     NP3EXIT                                             0401 00197000
*   /*****************************************************************/ 00198000
*   /*                                                               */ 00199000
*   /* NIP PROMPT SERVICE ROUTINE ENTRY POINT                  Y02752*/ 00200000
*   /* - USED INTERNALLY BY NP3LCAT -                          Y02752*/ 00201000
*   /*                                                               */ 00202000
*   /*****************************************************************/ 00203000
*                                                                  0402 00204000
*IEAVNPA3:                          /*                         Y02752*/ 00205000
*   GENERATE NOSEQFLOW FLOWS(IEAVNP03) EXIT;                       0402 00206000
IEAVNPA3 DS    0H                                                  0402 00207000
           STM   @13,@14,NPA3RSAV-IEAVNPA3(@15)  SAVE REGS 13 AND 14    00208000
           LA    @13,NPA3RSAV-IEAVNPA3(@15)      SAVE AREA ADDR         00209000
           LA    @14,IEAVNPA3-IEAVNP03(0)        ENTRY ADDR DIFFERENCE  00210000
           SR    @15,@14           NIP03 ENTRY POINT                    00211000
           BALR  @14,@15           GOTO NIP03 MAIN ENTRY                00212000
NP3AEXT    LM    @13,@14,NPA3RSAV-NP3AEXT(@14)  RESTORE REGS 13 AND 14  00213000
           BR    @14               RETURN TO CALLER                     00214000
NPA3RSAV   DC    18F'0'            REGISTER SAVE AREA                   00215000
*                                                                  0403 00216000
*   /*****************************************************************/ 00217000
*   /*                                                               */ 00218000
*   /* ANALYZE PARAMETERS FROM OPERATOR                              */ 00219000
*   /*                                                               */ 00220000
*   /*****************************************************************/ 00221000
*                                                                  0403 00222000
*NP3OPSP:                                                          0403 00223000
*   NP3RTADR=REXIT;                 /* SAVE CALLERS RETURN ADDR      */ 00224000
NP3OPSP  ST    REXIT,NP3RTADR                                      0403 00225000
*   NP3CTAB=ADDR(OPTABLN);          /* GET ADDR OF OPERATOR PARMTAB  */ 00226000
         LA    @10,OPTABLN                                         0404 00227000
         ST    @10,NP3CTAB                                         0404 00228000
*   NP3CENT=ADDR(OPSPPGPM);         /* POINT TO PAGE(PARMLIB) ENTRY  */ 00229000
         LA    @10,OPSPPGPM(,@10)                                  0405 00230000
         ST    @10,NP3CENT                                         0405 00231000
*   MRGOP='1'B;                     /* TURN MERGE BIT ON             */ 00232000
*                                                                  0406 00233000
         OI    MRGOP(@10),B'10000000'                              0406 00234000
*   /*****************************************************************/ 00235000
*   /*                                                               */ 00236000
*   /* PICKUP ORIGIN SPE FROM NVT                                    */ 00237000
*   /*                                                               */ 00238000
*   /*****************************************************************/ 00239000
*                                                                  0407 00240000
*   NP3DSPE=ADDR(NVTSPE);           /* SET BASE ADDRESS FOR SPE      */ 00241000
         LA    @10,NVTSPE(,RNVT)                                   0407 00242000
         ST    @10,NP3DSPE                                         0407 00243000
*   SCANREG=NIPSPEA;                /* POINT TO OPERATOR REPLY       */ 00244000
*                                                                  0408 00245000
         L     SCANREG,NIPSPEA(,@10)                               0408 00246000
*   /*****************************************************************/ 00247000
*   /*                                                               */ 00248000
*   /* CHECK OPERATOR REPLY FOR U' OR EOB (DEFAULT REPLY TO USE      */ 00249000
*   /* IEASYS00 MEMBER OF SYS1.PARMLIB FOR SYSTEM PARAMETERS).       */ 00250000
*   /*                                                               */ 00251000
*   /*****************************************************************/ 00252000
*                                                                  0409 00253000
*   IF OPREPLY1=NP3UQ               /* IS THIS U' OR                 */ 00254000
*       |OPREPLY1=NP3BLNKQ          /* IS THIS EOB                   */ 00255000
*     THEN                          /* IT IS U' OR EOB               */ 00256000
         CLC   OPREPLY1(2,SCANREG),NP3UQ                           0409 00257000
         BE    @RT00409                                            0409 00258000
         CLC   OPREPLY1(2,SCANREG),NP3BLNKQ                        0409 00259000
         BNE   @RF00409                                            0409 00260000
@RT00409 DS    0H                                                  0410 00261000
*     GO TO NP3RTAD0;               /* RETURN AND DEFINE INITIAL   0410 00262000
*                                      PARMAREA BASE                 */ 00263000
         L     @10,NP3RTADR                                        0410 00264000
         BR    @10                                                 0410 00265000
*   ELSE                                                           0411 00266000
*     ;                             /* NOT U' OR EOB , FALL THROUGH  */ 00267000
*                                                                  0411 00268000
@RF00409 DS    0H                                                  0412 00269000
*   /*****************************************************************/ 00270000
*   /*                                                               */ 00271000
*   /* CHECK OPERATOR REPLY FOR U,L' (DEFAULT REPLY TO USE IEASYS00  */ 00272000
*   /* MEMBER OF SYS1.PARMLIB FOR SYSTEM PARAMETERS WITH LIST OPTION */ 00273000
*   /* SPECIFIED). IS THIS U,L'                                Y02752*/ 00274000
*   /*                                                               */ 00275000
*   /*****************************************************************/ 00276000
*                                                                  0412 00277000
*   IF OPREPLY2=NP3UCLQ             /*                         Y02752*/ 00278000
*     THEN                          /* IT IS U,L'                    */ 00279000
         CLC   OPREPLY2(4,SCANREG),NP3UCLQ                         0412 00280000
         BNE   @RF00412                                            0412 00281000
*     DO;                                                          0413 00282000
*       NVTFLLST='1'B;              /* SET "LIST" FLAG ON, IN NVT    */ 00283000
         OI    NVTFLLST(RNVT),B'10000000'                          0414 00284000
*       GO TO NP3RTAD0;             /* RETURN AND DEFINE INITIAL   0415 00285000
*                                      PARMAREA BASE                 */ 00286000
         L     @10,NP3RTADR                                        0415 00287000
         BR    @10                                                 0415 00288000
*     END;                                                         0416 00289000
*                                                                  0416 00290000
*   /*****************************************************************/ 00291000
*   /*                                                               */ 00292000
*   /* SETUP POINTERS AND CONTROL SWITCHES FOR ENTRY INTO THE NP3SCAN*/ 00293000
*   /* SUBROUTINE FOR SCANNING OPERATOR REPLY PARAMETER CHARACTER    */ 00294000
*   /* STRING.                                                       */ 00295000
*   /*                                                               */ 00296000
*   /*****************************************************************/ 00297000
*                                                                  0417 00298000
*NP3OPSCN:                                                         0417 00299000
*   NP3CONT='0'B;                   /* SET CONTINUE PARAMETER FLAG 0417 00300000
*                                      OFF                           */ 00301000
@RF00412 DS    0H                                                  0417 00302000
NP3OPSCN NI    NP3CONT,B'01111111'                                 0417 00303000
*   OPSPCONT=OPSPCONT&&OPSPCONT;    /* ZERO CONT PARM                */ 00304000
         L     @10,NP3CTAB                                         0418 00305000
         XC    OPSPCONT(16,@10),OPSPCONT(@10)                      0418 00306000
*   NP3INID='0000'X;                /* SET INPUT ID TO ZERO          */ 00307000
         MVC   NP3INID(2),@CB02317                                 0419 00308000
*NP3SREPL:                                                         0420 00309000
*   NP3NCHAR=NIPSPEA;               /* POINT TO FIRST CHARACTER OF 0420 00310000
*                                      CURRENT SYMBOL IN PARAMETER 0420 00311000
*                                      CHARACTER STRING              */ 00312000
NP3SREPL L     @10,NP3DSPE                                         0420 00313000
         MVC   NP3NCHAR(4),NIPSPEA(@10)                            0420 00314000
*NP3SCALL:                                                         0421 00315000
*   CALL NP3SCAN;                   /* BRANCH TO SCAN SUBROUTINE FOR    00316000
*                                      PARAMETER VALIDATION          */ 00317000
*                                                                  0421 00318000
NP3SCALL BAL   @14,NP3SCAN                                         0421 00319000
*   /*****************************************************************/ 00320000
*   /*                                                               */ 00321000
*   /* THE FOLLOWING GENERATED STATEMENTS ARE A BRANCH TABLE RETURN +*/ 00322000
*   /* 0 FROM NP3SCAN INDICATES A VALID PARAMETER RETURN + 4 FROM    */ 00323000
*   /* NP3SCAN INDICATES AN INVALID PARAMETER RETURN + 8 FROM NP3SCAN*/ 00324000
*   /* INDICATES A CONTINUATION OF OF SUBPARAMETERS OR END OF ALL    */ 00325000
*   /* OPERATOR REPLY PARAMETERS.                                    */ 00326000
*   /*                                                               */ 00327000
*   /*****************************************************************/ 00328000
*                                                                  0422 00329000
*   GENERATE FLOWS(NP3OPIA,NP3OPRES,NP3OPCNT);                     0422 00330000
*                                                                  0422 00331000
         B     NP3OPIA             GO CHECK FOR OPERATOR OPI            00332000
         B     NP3OPRES            GO TO INVALID PARAMETER HANDLING     00333000
         B     NP3OPCNT            GO CHECK FOR CONTINUATION            00334000
*   /*****************************************************************/ 00335000
*   /*                                                               */ 00336000
*   /* IEAVNPA3 SUBROUTINE ENTRY POINT                         Y02752*/ 00337000
*   /*                                                               */ 00338000
*   /*****************************************************************/ 00339000
*                                                                  0423 00340000
*NPOPCNT:                                                          0423 00341000
*   NPRMRTN1=REXIT;                 /*                         Y02752   00342000
*                                                              Y02752*/ 00343000
*                                                                  0423 00344000
NPOPCNT  ST    REXIT,NPRMRTN1                                      0423 00345000
*   /*****************************************************************/ 00346000
*   /*                                                               */ 00347000
*   /* CHECKS FOR NON-ZERO CHAIN POINTER IN CURRENT SPE TO NEXT SPE  */ 00348000
*   /* FOR CONTINUATION OF SYSTEM PARAMETERS.                        */ 00349000
*   /*                                                               */ 00350000
*   /*****************************************************************/ 00351000
*                                                                  0424 00352000
*NP3OPCNT:                                                         0424 00353000
*   IF NIPSPEQ=0                    /* IS SPE CHAIN POINTER ZERO   0424 00354000
*                                      VALUE                         */ 00355000
*     THEN                          /* YES- NO VALID CONTINUATION    */ 00356000
NP3OPCNT L     @10,NP3DSPE                                         0424 00357000
         L     @10,NIPSPEQ(,@10)                                   0424 00358000
         LTR   @10,@10                                             0424 00359000
         BZ    @RT00424                                            0424 00360000
*     GO TO NP3OPIA;                /* BRANCH TO INVALID PREVIOUS  0425 00361000
*                                      PARAMETER CHECKING            */ 00362000
*   ELSE                            /* SPE CHAIN POINTER IS NON-ZERO */ 00363000
*     DO;                                                          0426 00364000
*       NP3TSPE=NIPSPEQ;            /* SAVE SPE CHAIN POINTER        */ 00365000
         ST    @10,NP3TSPE                                         0427 00366000
*       NP3DSPE=NP3TSPE;            /* SET BASE ADDRESS FOR NEW SPE  */ 00367000
         ST    @10,NP3DSPE                                         0428 00368000
*       GO TO NP3SREPL;             /* GO TO SCAN CONTINUATION FOR 0429 00369000
*                                      MORE PARAMETER ANALYSIS       */ 00370000
         B     NP3SREPL                                            0429 00371000
*     END;                                                         0430 00372000
*NP3OPRTY:                                                         0431 00373000
*   NP3RTADR=REXIT;                 /* SAVE CALLERS RETURN ADDR      */ 00374000
*                                                                  0431 00375000
NP3OPRTY ST    REXIT,NP3RTADR                                      0431 00376000
*   /*****************************************************************/ 00377000
*   /*                                                               */ 00378000
*   /* SET CURRENT PARMTAB POINTER TO ADDRESS OF OPERATOR'S PARMTAB  */ 00379000
*   /*                                                               */ 00380000
*   /*****************************************************************/ 00381000
*                                                                  0432 00382000
*   NP3CTAB=ADDR(OPTABLN);          /* POINT TO OPERATOR PARMTAB     */ 00383000
*                                                                  0432 00384000
         LA    @10,OPTABLN                                         0432 00385000
         ST    @10,NP3CTAB                                         0432 00386000
*   /*****************************************************************/ 00387000
*   /*                                                               */ 00388000
*   /* IF PROMPT MODE, RETURN TO PROMPT CALL SEQUENCE          Y02752*/ 00389000
*   /*                                                               */ 00390000
*   /*****************************************************************/ 00391000
*                                                                  0433 00392000
*   IF NVTSYSP='1'B                 /*                         Y02752*/ 00393000
*     THEN                          /*                         Y02752*/ 00394000
         TM    NVTSYSP(RNVT),B'01000000'                           0433 00395000
         BNO   @RF00433                                            0433 00396000
*     DO;                           /*                         Y02752*/ 00397000
*       RETCODE=4;                  /* EXTRANEOUS INFO FOUND   Y02752*/ 00398000
         MVC   RETCODE(4),@CF00371                                 0435 00399000
*       GOTO NPRMRTN1;              /* RETURN TO IEAVNPA3 CALL     0436 00400000
*                                      SEQUENCE                Y02752*/ 00401000
         L     @10,NPRMRTN1                                        0436 00402000
         BR    @10                                                 0436 00403000
*     END;                          /*                         Y02752   00404000
*                                                              Y02752*/ 00405000
*                                                                  0437 00406000
*   /*****************************************************************/ 00407000
*   /*                                                               */ 00408000
*   /* GENERATE THE INTERFACE BRANCH MACRO CALL TO NIPWTOR FOR       */ 00409000
*   /* RESPECIFY PARAMETERS MESSAGE TO OPERATOR                      */ 00410000
*   /*                                                               */ 00411000
*   /*****************************************************************/ 00412000
*                                                                  0438 00413000
*   GENERATE REFS(NVTWTOR) DEFS(NP3OPRES);                         0438 00414000
@RF00433 DS    0H                                                  0438 00415000
NP3OPRES IEAPMNIP   TYPE=WTOR,PARAM=PLIST01  CALL WTOR SERVICE ROUTINE  00416000
*   RESPECIFY                                                      0439 00417000
*     RPARM RESTRICTED;             /* PARAMETER INTERFACE REG(1)    */ 00418000
*   RESPECIFY                                                      0440 00419000
*     OPREPLY1 BASED(RPARM);        /* MAP TO OPERATOR REPLY         */ 00420000
*                                                                  0440 00421000
*   /*****************************************************************/ 00422000
*   /*                                                               */ 00423000
*   /* CHECK FOR OPERATOR REPLY OF EOB (CANCEL), WHICH IMPLIES       */ 00424000
*   /* OPERATOR DID NOT SPECIFY ANY SYSTEM PARAMETERS                */ 00425000
*   /*                                                               */ 00426000
*   /*****************************************************************/ 00427000
*                                                                  0441 00428000
*   IF OPREPLY1=NP3BLNKQ            /* DID OPERATOR CANCEL           */ 00429000
*     THEN                          /* YES,OPERATOR CANCELLED        */ 00430000
         CLC   OPREPLY1(2,RPARM),NP3BLNKQ                          0441 00431000
         BE    @RT00441                                            0441 00432000
*     GO TO NP3OPIA;                /* GO TO OPERATOR SPECIFIED "OPI"   00433000
*                                      CHECK HANDLER                 */ 00434000
*   ELSE                            /* NO,OPERATOR HAS RESPECIFIED 0443 00435000
*                                      SYSTEM PARAMETERS             */ 00436000
*     DO;                                                          0443 00437000
*       NP3OPSV=RPARM;              /* SAVE ADDRESS OF OPERATOR    0444 00438000
*                                      RESPECIFIED PARAMETERS        */ 00439000
         ST    RPARM,NP3OPSV                                       0444 00440000
*     END;                                                         0445 00441000
*   RESPECIFY                                                      0446 00442000
*     RPARM UNRESTRICTED;           /* PARAMETER INTERFACE REG1      */ 00443000
*   RESPECIFY                                                      0447 00444000
*     OPREPLY1 BASED(SCANREG);      /* RESET TO ORIGINAL MAPPING BASE   00445000
*                                      ADDRESS                 Y02752*/ 00446000
*                                                                  0447 00447000
*   /*****************************************************************/ 00448000
*   /*                                                               */ 00449000
