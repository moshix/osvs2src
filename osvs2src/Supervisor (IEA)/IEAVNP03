NP3EQUSB SLR   @10,@10                                             1184 03297000
         IC    @10,SLN                                             1184 03298000
         LR    @05,@10                                             1184 03299000
         BCTR  @05,0                                               1184 03300000
         ALR   @05,SYMBLREG                                        1184 03301000
         ST    @05,NP3EQADR                                        1184 03302000
*   /*****************************************************************/ 03303000
*   /*                                                               */ 03304000
*   /* CHECKS TO SEE IF THIS PARAMETER HAS SUBPARAMETERS             */ 03305000
*   /*                                                               */ 03306000
*   /*****************************************************************/ 03307000
*                                                                  1185 03308000
*   IF NP3SUBSP=NP3EQUAL            /* DOES PARAMETER HAVE "=" SIGN  */ 03309000
*     THEN                          /* YES, SUBPARAMETERS ASSOCIATED    03310000
*                                      WITH THIS PARAMETER           */ 03311000
         CLC   NP3SUBSP(1,@05),NP3EQUAL                            1185 03312000
         BE    @RT01185                                            1185 03313000
*     GO TO NP3BALN;                /* BRANCH TO SUBPARAMETER BALANCE   03314000
*                                      CHECK SETUP                   */ 03315000
*   ELSE                            /* NO,SUBPARAMETERS NOT        1187 03316000
*                                      ASSOCIATED WITH THIS PARAMETER*/ 03317000
*     DO;                                                          1187 03318000
*       NP3EQADR=SCANREG+SLN;       /* UPDATE MAPPING BASE ADDRESS TO   03319000
*                                      POINT TO PARAMETER SYMBOL IN     03320000
*                                      CHARACTER STRING              */ 03321000
         ALR   @10,SCANREG                                         1188 03322000
         ST    @10,NP3EQADR                                        1188 03323000
*       GO TO NP3NBALN;             /* BRANCH TO KEYWORD ENDING CHECK*/ 03324000
         B     NP3NBALN                                            1189 03325000
*     END;                                                         1190 03326000
*                                                                  1190 03327000
*   /*****************************************************************/ 03328000
*   /*                                                               */ 03329000
*   /* THIS SETS UP FOR ENTRY INTO NP3NXSYM FOR CHECKING THE         */ 03330000
*   /* BALANCING OF THE SUBPARAMETERS AND TO SET THE POSITION OF THE */ 03331000
*   /* NEXT PARAMETER IN THE CURRENT CHARACTER STRING                */ 03332000
*   /*                                                               */ 03333000
*   /*****************************************************************/ 03334000
*                                                                  1191 03335000
*NP3BALN:                                                          1191 03336000
*   SCANREG=NP3CCHAR+SLN;           /* SET MAPPING BASE ADDRESS TO 1191 03337000
*                                      SUBPARAMETERS                 */ 03338000
NP3BALN  SLR   SCANREG,SCANREG                                     1191 03339000
         IC    SCANREG,SLN                                         1191 03340000
         AL    SCANREG,NP3CCHAR                                    1191 03341000
*   CALL NP3NXSYM;                  /* BRANCH TO BALANCING CHECK     */ 03342000
*                                                                  1192 03343000
         BAL   @14,NP3NXSYM                                        1192 03344000
*   /*****************************************************************/ 03345000
*   /*                                                               */ 03346000
*   /* THIS IS A GENERATED BRANCH TABLE FOR RETURN FROM NP3NXSYM.    */ 03347000
*   /*                                                               */ 03348000
*   /*****************************************************************/ 03349000
*                                                                  1193 03350000
*   GENERATE FLOWS(NP3ENTER,NP3SCRNV,NP3SCCNT) NOSEQFLOW;          1193 03351000
*                                                                  1193 03352000
         B     NP3ENTER            VALID SUBPARAMETERS BRANCH RETURN    03353000
         B     NP3SCRNV            INVALID SUBPARAMETERS BRANCH RETURN  03354000
         B     NP3SCCNT                                                 03355000
*   /*****************************************************************/ 03356000
*   /*                                                               */ 03357000
*   /* CALCULATE THE ADDRESS OF ENTRY IN PARMTAB FOR THIS VALID      */ 03358000
*   /* PARAMETER AND SET BASE                                        */ 03359000
*   /*                                                               */ 03360000
*   /*****************************************************************/ 03361000
*                                                                  1194 03362000
*NP3ENTER:                                                         1194 03363000
*   NP3CENT=NP3CTAB+(INDXELMT*LENGTH(OPLIBENT));                   1194 03364000
NP3ENTER SLR   @10,@10                                             1194 03365000
         IC    @10,INDXELMT                                        1194 03366000
         SLA   @10,4                                               1194 03367000
         AL    @10,NP3CTAB                                         1194 03368000
         ST    @10,NP3CENT                                         1194 03369000
*   ORAD=NP3CCHAR+SLN;              /* SET ADDRESS OF SUBPARAMETERS     03370000
*                                      IN PARMTAB ENTRY              */ 03371000
         SLR   @05,@05                                             1195 03372000
         IC    @05,SLN                                             1195 03373000
         AL    @05,NP3CCHAR                                        1195 03374000
         ST    @05,ORAD(,@10)                                      1195 03375000
*   CONTAD=CONTAD&&CONTAD;          /* ZERO CONTINUE ADDRESS         */ 03376000
         L     @15,CONTAD(,@10)                                    1196 03377000
         LR    @14,@15                                             1196 03378000
         XR    @14,@15                                             1196 03379000
         ST    @14,CONTAD(,@10)                                    1196 03380000
*   ORLN=NP3NCHAR-ORAD;             /* SET PARM LENGTH FOR THIS ENTRY*/ 03381000
         LCR   @05,@05                                             1197 03382000
         AL    @05,NP3NCHAR                                        1197 03383000
         STH   @05,ORLN(,@10)                                      1197 03384000
*   CONTLN=CONTLN&&CONTLN;          /* ZERO CONTINUE LENGTH          */ 03385000
         XC    CONTLN(2,@10),CONTLN(@10)                           1198 03386000
*NP3SCTID:                                                         1199 03387000
*   INSRCID=NP3INID;                /* SET INPUT SOURCE ID           */ 03388000
NP3SCTID L     @10,NP3CENT                                         1199 03389000
         MVC   INSRCID(2,@10),NP3INID                              1199 03390000
*   OPFLAG=CUROPT;                  /* SET OPTION FLAGS IN ENTRY TO     03391000
*                                      CURRENT OPTION FLAGS          */ 03392000
         MVC   OPFLAG(1,@10),CUROPT                                1200 03393000
*   CUROPT='00'X;                   /* TURN CURRENT OPTION FLAGS OFF */ 03394000
         MVI   CUROPT,X'00'                                        1201 03395000
*   GO TO NP3SCNXT;                 /* SCAN NEXT PARM                */ 03396000
*                                                                  1202 03397000
         B     NP3SCNXT                                            1202 03398000
*   /*****************************************************************/ 03399000
*   /*                                                               */ 03400000
*   /* THIS CHECKS TO SEE THAT A PARAMETER WHICH HAS NO ASSOCIATED   */ 03401000
*   /* SUBPARAMETERS IS TERMINATED CORRECTLY.                        */ 03402000
*   /*                                                               */ 03403000
*   /*****************************************************************/ 03404000
*                                                                  1203 03405000
*NP3NBALN:                                                         1203 03406000
*   IF NP3SUBSP=NP3BLANK            /* IS CHARACTER A BLANK          */ 03407000
*       |NP3SUBSP=NP3COMMA          /* IS CHARACTER A COMMA          */ 03408000
*       |NP3SUBSP=NP3QUOTE          /* IS CHARACTER A QUOTE          */ 03409000
*     THEN                          /* IT IS ONE OF THEM,PARAMETER IS   03410000
*                                      VALID                         */ 03411000
NP3NBALN L     @10,NP3EQADR                                        1203 03412000
         CLC   NP3SUBSP(1,@10),NP3BLANK                            1203 03413000
         BE    @RT01203                                            1203 03414000
         CLC   NP3SUBSP(1,@10),NP3COMMA                            1203 03415000
         BE    @RT01203                                            1203 03416000
         CLC   NP3SUBSP(1,@10),NP3QUOTE                            1203 03417000
         BNE   @RF01203                                            1203 03418000
@RT01203 DS    0H                                                  1204 03419000
*     DO;                                                          1204 03420000
*       NP3NCHAR=NP3CCHAR+SLN;      /* UPDATE POINTER TO NEXT      1205 03421000
*                                      PARAMETER CHARACTER           */ 03422000
         SLR   @10,@10                                             1205 03423000
         IC    @10,SLN                                             1205 03424000
         AL    @10,NP3CCHAR                                        1205 03425000
         ST    @10,NP3NCHAR                                        1205 03426000
*       GO TO NP3ENTER;             /* BRANCH TO PARMTAB ADDRESS MARK*/ 03427000
         B     NP3ENTER                                            1206 03428000
*     END;                                                         1207 03429000
*   ELSE                            /* NOT A VALID PARAMETER         */ 03430000
*     DO;                                                          1208 03431000
@RF01203 DS    0H                                                  1209 03432000
*       CAUSE04=IMPDEL;             /* SET CAUSE IN INVALID SYNTAX 1209 03433000
*                                      MESSAGE                       */ 03434000
         MVC   CAUSE04(20),IMPDEL                                  1209 03435000
*       GO TO NP3SCINV;                                            1210 03436000
         B     NP3SCINV                                            1210 03437000
*     END;                                                         1211 03438000
*                                                                  1211 03439000
*   /*****************************************************************/ 03440000
*   /*                                                               */ 03441000
*   /* THIS SUBROUTINE IS ENTERED WHEN A CONTINUATION OF             */ 03442000
*   /* SUBPARAMETERS HAS BEEN RECOGNIZED BY NP3SCAN.                 */ 03443000
*   /*                                                               */ 03444000
*   /*****************************************************************/ 03445000
*                                                                  1212 03446000
*NP3CHNDL:                                                         1212 03447000
*   NP3CONT='0'B;                   /* RESET CONTINUE SUBPARAMETERS     03448000
*                                      FLAG TO OFF STATUS            */ 03449000
NP3CHNDL NI    NP3CONT,B'01111111'                                 1212 03450000
*   NP3BALCK=SCANREG;               /* SAVE POINTER TO CONTINUED   1213 03451000
*                                      SUBPARAMETERS                 */ 03452000
         ST    SCANREG,NP3BALCK                                    1213 03453000
*   CALL NP3CTSYM;                  /* BRANCH TO BALANCE CHECK     1214 03454000
*                                      ROUTINE AT ITS CONTINUATION 1214 03455000
*                                      ENTRY POINT                   */ 03456000
*                                                                  1214 03457000
         BAL   @14,NP3CTSYM                                        1214 03458000
*   /*****************************************************************/ 03459000
*   /*                                                               */ 03460000
*   /* THIS IS A GENERATED BRANCH TABLE FOR THE RETURN BRANCH FROM   */ 03461000
*   /* NP3NXSYM.                                                     */ 03462000
*   /*                                                               */ 03463000
*   /*****************************************************************/ 03464000
*                                                                  1215 03465000
*   GENERATE FLOWS(NP3CHNTR,NP3SCRNV,NP3NGCNT) NOSEQFLOW;          1215 03466000
         B     NP3CHNTR            VALID SUBPARAMETER BRANCH ENTRY      03467000
         B     NP3SCRNV            INVALID SUBPARAMETER BRANCH ENTRY    03468000
         B     NP3NGCNT                                                 03469000
*NP3NGCNT:                                                         1216 03470000
*   CAUSE04=INVCON;                 /* MOVE CAUSE INTO INVALID SYNTAX   03471000
*                                      MESSAGE                       */ 03472000
NP3NGCNT MVC   CAUSE04(20),INVCON                                  1216 03473000
*   GO TO NP3SCINV;                 /* GIVE MESSAGE                  */ 03474000
*                                                                  1217 03475000
         B     NP3SCINV                                            1217 03476000
*   /*****************************************************************/ 03477000
*   /*                                                               */ 03478000
*   /* CALCULATE THE ADDRESS OF ENTRY IN PARMTAB FOR THIS VALID      */ 03479000
*   /* PARAMETER                                                     */ 03480000
*   /*                                                               */ 03481000
*   /*****************************************************************/ 03482000
*                                                                  1218 03483000
*NP3CHNTR:                                                         1218 03484000
*   NP3CENT=NP3CTAB+(INDXELMT*LENGTH(OPLIBENT));                   1218 03485000
NP3CHNTR SLR   @10,@10                                             1218 03486000
         IC    @10,INDXELMT                                        1218 03487000
         SLA   @10,4                                               1218 03488000
         AL    @10,NP3CTAB                                         1218 03489000
         ST    @10,NP3CENT                                         1218 03490000
*   CONTAD=NP3BALCK;                /* SET ADDRESS OF CONTINUED    1219 03491000
*                                      SUBPARAMETERS                 */ 03492000
         L     @05,NP3BALCK                                        1219 03493000
         ST    @05,CONTAD(,@10)                                    1219 03494000
*   ORAD=NP3CCHAR+SLN;              /* SET ADDRESS OF BEGINNING OF 1220 03495000
*                                      SUBPARAMETERS                 */ 03496000
         SLR   @15,@15                                             1220 03497000
         IC    @15,SLN                                             1220 03498000
         AL    @15,NP3CCHAR                                        1220 03499000
         ST    @15,ORAD(,@10)                                      1220 03500000
*   CONTLN=NP3NCHAR-CONTAD;         /* SET CONT PARM LENGTH          */ 03501000
         LCR   @05,@05                                             1221 03502000
         AL    @05,NP3NCHAR                                        1221 03503000
         STH   @05,CONTLN(,@10)                                    1221 03504000
*   ORLN=PRENDPTR-ORAD;             /* SET PARM LENGTH               */ 03505000
         LCR   @15,@15                                             1222 03506000
         AL    @15,PRENDPTR                                        1222 03507000
         STH   @15,ORLN(,@10)                                      1222 03508000
*   GO TO NP3SCTID;                                                1223 03509000
         B     NP3SCTID                                            1223 03510000
*                                                                  1224 03511000
*   /*****************************************************************/ 03512000
*   /*                                                               */ 03513000
*   /* THIS SUBROUTINE VERIFIES THE BALANCING OF SYNTACTICAL         */ 03514000
*   /* CHARACTERS ASSOCIATED WITH SUBPARAMETERS. IT ALSO CHECKS FOR  */ 03515000
*   /* AN OPERATES ON THE "CONT" SUBPARAMETERS OPTION.IT SECOND      */ 03516000
*   /* OBJECTIVE IS TO LOCATE THE NEXT CHARACTER FOLLOWING THE       */ 03517000
*   /* COMPLETED SUBPARAMETERS AND PLACE ITS ADDRESS IN THE NEXT     */ 03518000
*   /* CHARACTER POINTER. THE NEXT CHARACTER POINTER IS USED BY      */ 03519000
*   /* NP3SCAN AS THE POSITION IN THE CHARACTER STRING TO BEGIN      */ 03520000
*   /* SCANNING FOR THE NEXT SYSTEM PARAMETER TO VERIFY.             */ 03521000
*   /*                                                               */ 03522000
*   /*****************************************************************/ 03523000
*                                                                  1224 03524000
*   RESPECIFY                                                      1224 03525000
*     NP3SCAN5 BASED(SCANREG);                                     1224 03526000
*                                                                  1224 03527000
*   /*****************************************************************/ 03528000
*   /*                                                               */ 03529000
*   /* ENTRY FOR CONTINUED PARAMETERS                                */ 03530000
*   /*                                                               */ 03531000
*   /*****************************************************************/ 03532000
*                                                                  1225 03533000
*NP3CTSYM:                                                         1225 03534000
*   NP3SRTRN=REXIT;                 /* SAVE CALLER'S RETURN ADDRESS  */ 03535000
*                                                                  1225 03536000
NP3CTSYM ST    REXIT,NP3SRTRN                                      1225 03537000
*   /*****************************************************************/ 03538000
*   /*                                                               */ 03539000
*   /* TEST FOR IMBEDDED PARM OPTION                                 */ 03540000
*   /*                                                               */ 03541000
*   /*****************************************************************/ 03542000
*                                                                  1226 03543000
*   CALL NP3POPT;                                                  1226 03544000
         BAL   @14,NP3POPT                                         1226 03545000
*   GO TO NP3PCHCK;                                                1227 03546000
         B     NP3PCHCK                                            1227 03547000
*NP3NXSYM:                                                         1228 03548000
*   NP3SRTRN=REXIT;                 /* SAVE CALLER'S RETURN ADDRESS  */ 03549000
NP3NXSYM ST    REXIT,NP3SRTRN                                      1228 03550000
*   NP3PAREN=0;                     /* SET PARENTHESIS COUNT TO ZERO */ 03551000
         SLR   @10,@10                                             1229 03552000
         ST    @10,NP3PAREN                                        1229 03553000
*   CUROPT='00'X;                   /* TURN CURRENT OPTION FLAGS OFF */ 03554000
*                                                                  1230 03555000
         MVI   CUROPT,X'00'                                        1230 03556000
*   /*****************************************************************/ 03557000
*   /*                                                               */ 03558000
*   /* IS CHARACTER A BLANK OR A QUOTE                               */ 03559000
*   /*                                                               */ 03560000
*   /*****************************************************************/ 03561000
*                                                                  1231 03562000
*   IF NP3SCANR=' '|NP3SCANR=NP3QUOTE THEN                         1231 03563000
         CLI   NP3SCANR(SCANREG),C' '                              1231 03564000
         BE    @RT01231                                            1231 03565000
         CLC   NP3SCANR(1,SCANREG),NP3QUOTE                        1231 03566000
         BE    @RT01231                                            1231 03567000
*     GO TO NP3SNDER;               /* GO GIVE ERROR MESSAGE         */ 03568000
*                                                                  1232 03569000
*   /*****************************************************************/ 03570000
*   /*                                                               */ 03571000
*   /* IS CHARACTER A COMMA                                          */ 03572000
*   /*                                                               */ 03573000
*   /*****************************************************************/ 03574000
*                                                                  1233 03575000
*   IF NP3SCANR=NP3COMMA THEN       /* YES - CANCEL                  */ 03576000
         CLC   NP3SCANR(1,SCANREG),NP3COMMA                        1233 03577000
         BNE   @RF01233                                            1233 03578000
*     DO;                                                          1234 03579000
*       CURCANOP='1'B;              /* SET CANCEL FLAG IN CURRENT  1235 03580000
*                                      OPTION BYTE                   */ 03581000
         OI    CURCANOP,B'00100000'                                1235 03582000
*       GO TO NP3OUT;               /* RETURN                        */ 03583000
         B     NP3OUT                                              1236 03584000
*     END;                                                         1237 03585000
*                                                                  1237 03586000
*   /*****************************************************************/ 03587000
*   /*                                                               */ 03588000
*   /* CHECK CURRENT CHARACTER FOR LEFT PARENTHESIS                  */ 03589000
*   /*                                                               */ 03590000
*   /*****************************************************************/ 03591000
*                                                                  1238 03592000
*NP3PCHCK:                                                         1238 03593000
*   IF SCNPAREN^=LPAREN             /* CHARACTER OTHER THAN LEFT   1238 03594000
*                                      PAREN                         */ 03595000
*     THEN                          /* YES, NOT A LEFT PAREN         */ 03596000
*                                                                  1238 03597000
@RF01233 DS    0H                                                  1238 03598000
NP3PCHCK CLC   SCNPAREN(1,SCANREG),LPAREN                          1238 03599000
         BE    @RF01238                                            1238 03600000
*     /***************************************************************/ 03601000
*     /*                                                             */ 03602000
*     /* CHECK CURRENT CHARACTER FOR RIGHT PARENTHESIS               */ 03603000
*     /*                                                             */ 03604000
*     /***************************************************************/ 03605000
*                                                                  1239 03606000
*     IF SCNPAREN^=RPAREN           /* CHARACTER OTHER THAN RIGHT  1239 03607000
*                                      PAREN                         */ 03608000
*       THEN                        /* YES, NOT A RIGHT PAREN        */ 03609000
*                                                                  1239 03610000
         CLC   SCNPAREN(1,SCANREG),RPAREN                          1239 03611000
         BE    @RF01239                                            1239 03612000
*       /*************************************************************/ 03613000
*       /*                                                           */ 03614000
*       /* CHECK CURRENT CHARACTER FOR COMMA,QUOTE BLANK             */ 03615000
*       /*                                                           */ 03616000
*       /*************************************************************/ 03617000
*                                                                  1240 03618000
*       IF NP3SCANR=NP3COMMA        /* CHARACTER = COMMA             */ 03619000
*           |NP3SCANR=NP3QUOTE      /* CHAR. = QUOTE                 */ 03620000
*           |NP3SCANR=NP3BLANK      /* CHAR. = BLANK                 */ 03621000
*         THEN                      /* YES, CHARACTER = TO ONE OF  1240 03622000
*                                      THEM                          */ 03623000
*                                                                  1240 03624000
         CLC   NP3SCANR(1,SCANREG),NP3COMMA                        1240 03625000
         BE    @RT01240                                            1240 03626000
         CLC   NP3SCANR(1,SCANREG),NP3QUOTE                        1240 03627000
         BE    @RT01240                                            1240 03628000
         CLC   NP3SCANR(1,SCANREG),NP3BLANK                        1240 03629000
         BNE   @RF01240                                            1240 03630000
@RT01240 DS    0H                                                  1241 03631000
*         /***********************************************************/ 03632000
*         /*                                                         */ 03633000
*         /* CHECK FOR PAREN COUNT OF ZERO, WHICH IMPLIES            */ 03634000
*         /* SUBPARAMETERS HAVE BEEN COMPLETELY BALANCED CHECKED     */ 03635000
*         /*                                                         */ 03636000
*         /***********************************************************/ 03637000
*                                                                  1241 03638000
*         IF NP3PAREN=0             /* COUNT = 0                     */ 03639000
*           THEN                    /* YES                           */ 03640000
         ICM   @10,15,NP3PAREN                                     1241 03641000
         BNZ   @RF01241                                            1241 03642000
*           DO;                     /* SET NEXT CHARACTER POINTER    */ 03643000
*NP3OUT:                                                           1243 03644000
*             NP3NCHAR=SCANREG;     /* RETURN TO CALLER AT VALID   1243 03645000
*                                      PARAMETER ENTRY               */ 03646000
NP3OUT   ST    SCANREG,NP3NCHAR                                    1243 03647000
*             GO TO NP3SRTN0;                                      1244 03648000
         L     @10,NP3SRTRN                                        1244 03649000
         BR    @10                                                 1244 03650000
*           END;                                                   1245 03651000
*         ELSE                      /* PAREN COUNT NOT ZERO,FALL   1246 03652000
*                                      THROUGH                       */ 03653000
*           DO;                     /* CHECK CHARACTER FOR COMMA     */ 03654000
@RF01241 DS    0H                                                  1247 03655000
*             IF NP3SCANR=NP3COMMA THEN/* YES, CHARACTER IS          */ 03656000
         CLC   NP3SCANR(1,SCANREG),NP3COMMA                        1247 03657000
         BNE   @RF01247                                            1247 03658000
*               DO;                 /* A COMMA                       */ 03659000
*                 NP3SUBCK='1'B;    /* TURN SUBPARAM CHECK SWITCH ON */ 03660000
         OI    NP3SUBCK,B'00001000'                                1249 03661000
*                 GO TO NP3RECNT;   /* BRANCH TO POINTER UPDATE      */ 03662000
         B     NP3RECNT                                            1250 03663000
*               END;                                               1251 03664000
*             ELSE                  /* NO, CHARACTER NOT A COMMA     */ 03665000
*               GO TO NP3ENDL;      /* BRANCH TO END OF CHARACTER  1252 03666000
*                                      STRING CHECK                  */ 03667000
*           END;                                                   1253 03668000
*       ELSE                        /* NO, CHARACTER NOT A COMMA OR A   03669000
*                                      QUOTE OR A BLANK              */ 03670000
*         GO TO NP3RECNT;           /* BRANCH TO CHARACTER POINTER 1254 03671000
*                                      UPDATE                        */ 03672000
*     ELSE                          /* YES, CHARACTER IS A RIGHT   1255 03673000
*                                      PAREN                         */ 03674000
*       DO;                                                        1255 03675000
*                                                                  1255 03676000
@RF01239 DS    0H                                                  1256 03677000
*         /***********************************************************/ 03678000
*         /*                                                         */ 03679000
*         /* DECREMENT PARENTHESIS COUNT BY ONE(1)                   */ 03680000
*         /*                                                         */ 03681000
*         /***********************************************************/ 03682000
*                                                                  1256 03683000
*         NP3PAREN=NP3PAREN-1;                                     1256 03684000
         L     @10,NP3PAREN                                        1256 03685000
         BCTR  @10,0                                               1256 03686000
         ST    @10,NP3PAREN                                        1256 03687000
*         GO TO NP3PRENC;           /* BRANCH TO PAREN COUNT CHK     */ 03688000
         B     NP3PRENC                                            1257 03689000
*       END;                                                       1258 03690000
*   ELSE                            /* YES, CHARACTER IS A LEFT PAREN*/ 03691000
*     DO;                                                          1259 03692000
*                                                                  1259 03693000
@RF01238 DS    0H                                                  1260 03694000
*       /*************************************************************/ 03695000
*       /*                                                           */ 03696000
*       /* INCREMENT PARENTHESIS COUNT BY ONE(1)                     */ 03697000
*       /*                                                           */ 03698000
*       /*************************************************************/ 03699000
*                                                                  1260 03700000
*       NP3PAREN=1+NP3PAREN;                                       1260 03701000
         LA    @10,1                                               1260 03702000
         AL    @10,NP3PAREN                                        1260 03703000
         ST    @10,NP3PAREN                                        1260 03704000
*       GO TO NP3PRENC;             /* BRANCH TO PAREN COUNT CHECK   */ 03705000
         B     NP3PRENC                                            1261 03706000
*     END;                                                         1262 03707000
*                                                                  1262 03708000
*   /*****************************************************************/ 03709000
*   /*                                                               */ 03710000
*   /* CHECK CURRENT COUNT OF PARENTHESIS FOR THIS SUBPARAMETER      */ 03711000
*   /* CHARACTER STRING FOR GREATER THAN ZERO                        */ 03712000
*   /*                                                               */ 03713000
*   /*****************************************************************/ 03714000
*                                                                  1263 03715000
*NP3PRENC:                                                         1263 03716000
*   IF NP3PAREN>0                   /* IS COUNT GREATER THAN ZERO    */ 03717000
*     THEN                                                         1263 03718000
NP3PRENC L     @10,NP3PAREN                                        1263 03719000
         SLR   @05,@05                                             1263 03720000
         CR    @10,@05                                             1263 03721000
         BH    @RT01263                                            1263 03722000
*     GO TO NP3RECNT;               /* YES - GREATER THAN ZERO       */ 03723000
*                                                                  1264 03724000
*   /*****************************************************************/ 03725000
*   /*                                                               */ 03726000
*   /* CHECK CURRENT COUNT OF PARENTHESIS FOR THIS SUBPARAMETER      */ 03727000
*   /* CHARACTER STRING FOR LESS THAN ZERO                           */ 03728000
*   /*                                                               */ 03729000
*   /*****************************************************************/ 03730000
*                                                                  1265 03731000
*   IF NP3PAREN<0                   /* COUNT LESS THAN ZERO          */ 03732000
*     THEN                                                         1265 03733000
         CR    @10,@05                                             1265 03734000
         BL    @RT01265                                            1265 03735000
*     GO TO NP3PRINV;               /* YES,COUNT LESS THAN ZERO      */ 03736000
*                                                                  1266 03737000
*   /*****************************************************************/ 03738000
*   /*                                                               */ 03739000
*   /* UPDATE SCAN POINTER BY ONE(1)                                 */ 03740000
*   /*                                                               */ 03741000
*   /*****************************************************************/ 03742000
*                                                                  1267 03743000
*   SCANREG=1+SCANREG;                                             1267 03744000
*                                                                  1267 03745000
         LA    SCANREG,1(,SCANREG)                                 1267 03746000
*   /*****************************************************************/ 03747000
*   /*                                                               */ 03748000
*   /* THE PARENTHESIS COUNT IS ZERO. THE NEXT CHARACTER SHOULD BE A */ 03749000
*   /* COMMA OR A BLANK OR A QUOTE. IF THE CHARACTER IS ONE OF THESE */ 03750000
*   /* THEN THE SUBPARAMETERS HAVE BEEN TERMINATED PROPERLY. IF      */ 03751000
*   /* NOT,THEN THE SUBPARAMETERS ARE NOT SPECIFIED CORRECTLY        */ 03752000
*   /*                                                               */ 03753000
*   /*****************************************************************/ 03754000
*                                                                  1268 03755000
*   IF NP3SCANR=NP3COMMA            /* CHARACTER A COMMA             */ 03756000
*       |NP3SCANR=NP3BLANK          /* CHAR. A BLANK                 */ 03757000
*       |NP3SCANR=NP3QUOTE          /* CHAR. A QUOTE                 */ 03758000
*     THEN                          /* YES, CHARACTER IS ONE OF THEM,   03759000
*                                      SUBPARAMETERS ARE VALID       */ 03760000
         CLC   NP3SCANR(1,SCANREG),NP3COMMA                        1268 03761000
         BE    @RT01268                                            1268 03762000
         CLC   NP3SCANR(1,SCANREG),NP3BLANK                        1268 03763000
         BE    @RT01268                                            1268 03764000
         CLC   NP3SCANR(1,SCANREG),NP3QUOTE                        1268 03765000
         BE    @RT01268                                            1268 03766000
*     GO TO NP3OUT;                 /* BRANCH TO CALLER RETURN     1269 03767000
*                                      HANDLER                       */ 03768000
*                                                                  1269 03769000
*   /*****************************************************************/ 03770000
*   /*                                                               */ 03771000
*   /* NO, CHARACTER IS NONE OF THEM, INVALID SUBPARAMETERS          */ 03772000
*   /*                                                               */ 03773000
*   /*****************************************************************/ 03774000
*                                                                  1270 03775000
*NP3SNDER:                                                         1270 03776000
*   CAUSE04=IMPDEL;                 /* CAUSE IS IMPROPER DELIMETER   */ 03777000
*                                                                  1270 03778000
NP3SNDER MVC   CAUSE04(20),IMPDEL                                  1270 03779000
*   /*****************************************************************/ 03780000
*   /*                                                               */ 03781000
*   /* PAREN COUNT IS LESS THAN ZERO WHICH IMPLIES THE SUBPARAMETERS */ 03782000
*   /* WERE SPECIFIED INCORRECTLY                                    */ 03783000
*   /*                                                               */ 03784000
*   /*****************************************************************/ 03785000
*                                                                  1271 03786000
*NP3PRINV:                                                         1271 03787000
*   IF NP3PAREN^=0                  /* IS PAREN CTR ZERO             */ 03788000
