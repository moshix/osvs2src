*     GO TO NP3SYXIT;               /* YES - RETURN TO CALLER        */ 01424000
*   IF NP3SCAN5^='SYSP='            /* IS PARM SYSP=                 */ 01425000
*     THEN                                                         0695 01426000
         CLC   NP3SCAN5(5,SCANREG),@CC02498                        0695 01427000
