*   MULTSYSP='0'B;                  /* TURN MULTI SYSP SWITCH OFF    */ 01446000
         NI    MULTSYSP,B'10111111'                                0701 01447000
*   GO TO NP3SYRTY;                 /* GO ANALYZE NEW PARMS          */ 01448000
         B     NP3SYRTY                                            0702 01449000
*NP3SYXIT:                                                         0703 01450000
*   GO TO NP3SYPA0;                 /* RETURN TO CALLER              */ 01451000
NP3SYXIT L     @10,NP3SYPAD                                        0703 01452000
         BR    @10                                                 0703 01453000
*                                                                  0704 01454000
*   /*****************************************************************/ 01455000
*   /*                                                               */ 01456000
*   /* THIS SUBROUTINE MERGES THE OPERATOR SPECIFIED SYSTEM          */ 01457000
*   /* PARAMETERS WITH THOSE SYSTEM PARAMETERS SPECIFIED THROUGH THE */ 01458000
*   /* IEASYSXX MEMBERS OF SYS1.PARMLIB. SPECIFICALLY, THE OPERATORS */ 01459000
*   /* PARMTAB IS MERGED INTO THE PARMLIB PARMTAB                    */ 01460000
*   /*                                                               */ 01461000
*   /*****************************************************************/ 01462000
*                                                                  0704 01463000
*   RESPECIFY                                                      0704 01464000
*     OPERREG RESTRICTED;           /* POINTS TO OPERATOR PARMTAB    */ 01465000
*   RESPECIFY                                                      0705 01466000
*     PARMREG RESTRICTED;           /* POINTS TO PARMLIB PARMTAB     */ 01467000
*   RESPECIFY                                                      0706 01468000
*     INDEXREG RESTRICTED;          /* CONTAINS CELL INCREMENT FOR 0706 01469000
*                                      PARMTAB'S                     */ 01470000
*   RESPECIFY                                                      0707 01471000
*     COUNTREG RESTRICTED;          /* TRACKS COUNT CYCLE FOR MERGE     01472000
*                                      OPERATION                     */ 01473000
*NP3PLMRG:                                                         0708 01474000
*   NP3RTADR=REXIT;                 /* SAVE CALLER'S RETURN ADDRESS  */ 01475000
NP3PLMRG ST    REXIT,NP3RTADR                                      0708 01476000
*   OPERREG=ADDR(OPTABLN);          /* SET MAPPING BASE ADDRESS FOR     01477000
*                                      OPERATOR PARMTAB              */ 01478000
         LA    OPERREG,OPTABLN                                     0709 01479000
*   PARMREG=ADDR(PTABLN);           /* SET MAPPING BASE ADDRESS FOR     01480000
*                                      PARMLIB PARMTAB               */ 01481000
         LA    PARMREG,PTABLN                                      0710 01482000
*   NP3CTAB=PARMREG;                /* MAP PARMTAB TO PARMLIB ADDRESS*/ 01483000
*                                                                  0711 01484000
         ST    PARMREG,NP3CTAB                                     0711 01485000
*   /*****************************************************************/ 01486000
*   /*                                                               */ 01487000
*   /* GET NUMBER OF ENTRIES IN PARAMETER ADDRESS TABLE              */ 01488000
*   /*                                                               */ 01489000
*   /*****************************************************************/ 01490000
*                                                                  0712 01491000
*   COUNTREG=LENGTH(PARMTAB)/LENGTH(PTABSTRT);                     0712 01492000
         LA    COUNTREG,40                                         0712 01493000
*   INDEXREG=0;                     /* SET INDEX CONTROL ELEMENT TO     01494000
*                                      ZERO ORIGIN                   */ 01495000
*                                                                  0713 01496000
         SLR   INDEXREG,INDEXREG                                   0713 01497000
*   /*****************************************************************/ 01498000
*   /*                                                               */ 01499000
*   /* BEGIN MERGE SEQUENCE FOR OPERATOR/PARMLIB PARMTAB'S OPERATORS */ 01500000
*   /* PARMTAB IS MERGED INTO THE PARMLIB PARMTAB CHECK TO SEE IF ALL*/ 01501000
*   /* PARMTAB ENTRIES HAVE BEEN MERGED                              */ 01502000
*   /*                                                               */ 01503000
*   /*****************************************************************/ 01504000
*                                                                  0714 01505000
*NP3MRGST:                                                         0714 01506000
*   IF COUNTREG^=0                  /* IS ENTRY COUNT GREATER THEN 0 */ 01507000
*     THEN                          /* YES, MERGE SEQUENCE NOT DONE  */ 01508000
NP3MRGST LTR   COUNTREG,COUNTREG                                   0714 01509000
         BZ    @RF00714                                            0714 01510000
*     DO;                                                          0715 01511000
*       NP3CENT=PARMREG;            /* POINT TO PLIBTAB              */ 01512000
*                                                                  0716 01513000
         ST    PARMREG,NP3CENT                                     0716 01514000
*       /*************************************************************/ 01515000
*       /*                                                           */ 01516000
*       /* HAS THIS PARAMETER BEEN CANCELED                          */ 01517000
*       /*                                                           */ 01518000
*       /*************************************************************/ 01519000
*                                                                  0717 01520000
*       IF CANOPT='1'B THEN         /* YES -                         */ 01521000
         L     @10,NP3CENT                                         0717 01522000
         TM    CANOPT(@10),B'00100000'                             0717 01523000
         BNO   @RF00717                                            0717 01524000
*         OPLIBENT=OPLIBENT&&OPLIBENT;/* ZERO PLIBTAB ENTRY          */ 01525000
*                                                                  0718 01526000
         XC    OPLIBENT(16,@10),OPLIBENT(@10)                      0718 01527000
*       /*************************************************************/ 01528000
*       /*                                                           */ 01529000
*       /* CHECK TO SEE IF OPERATOR PARMTAB ENTRY IS EMPTY           */ 01530000
*       /*                                                           */ 01531000
*       /*************************************************************/ 01532000
*                                                                  0719 01533000
*       IF OPERVAL^=0               /* ENTRY GREATER THEN ZERO       */ 01534000
*         THEN                      /* YES, THE OPERATOR HAS       0719 01535000
*                                      SPECIFIED PARAMETERS FOR THIS    01536000
*                                      CURRENT PARMTAB ENTRY         */ 01537000
