*    (NIPPMR01,                                                    1517 04969000
*     NIPPMR00,                                                    1517 04970000
*     NIPPMR14,                                                    1517 04971000
*     NIPPMR15)                     /*                         Y02752*/ 04972000
*         UNRESTRICTED;                                            1517 04973000
*                                                                  1517 04974000
*   /*****************************************************************/ 04975000
*   /*                                                               */ 04976000
*   /* GENERATED MACRO PARAMETER LISTS FOR MESSAGES                  */ 04977000
*   /*                                                               */ 04978000
*   /*****************************************************************/ 04979000
*                                                                  1518 04980000
*   GENERATE DATA;                                                 1518 04981000
*                                                                  1518 04982000
*   /*****************************************************************/ 04983000
*   /*                                                               */ 04984000
*   /* GENERATED MACRO PARAMETER LISTS FOR INTERFACE BRANCHES        */ 04985000
*   /*                                                               */ 04986000
*   /*****************************************************************/ 04987000
*                                                                  1519 04988000
*   GENERATE DATA;                                                 1519 04989000
*                                                                  1520 04990000
*   /*****************************************************************/ 04991000
*   /*                                                               */ 04992000
*   /* LOCATE PARAMETER LIST AREA                                    */ 04993000
*   /*                                                               */ 04994000
*   /*****************************************************************/ 04995000
*                                                                  1520 04996000
*   GENERATE DATA;                                                 1520 04997000
*                                                                  1521 04998000
*   /*****************************************************************/ 04999000
*   /*                                                               */ 05000000
*   /* THE NP3MATCH ROUTINE COMPARES THE LIST OF VALID         Y02752*/ 05001000
*   /* PARAMETERS TO THE DATA ADDRESED BY SCANREG.             Y02752*/ 05002000
*   /* RETURN CODES: MATCH FOUND - 0                           Y02752*/ 05003000
*   /* NO MATCH - 4                                            Y02752*/ 05004000
*   /*                                                               */ 05005000
*   /*****************************************************************/ 05006000
*                                                                  1521 05007000
*NP3MATCH:                          /*                         Y02752   05008000
*                                                              Y02752*/ 05009000
*   RESPECIFY                                                      1521 05010000
*    (SYMBLREG,                                                    1521 05011000
*     OFFSTREG,                                                    1521 05012000
*     RNVT,                                                        1521 05013000
*     SCANREG) RESTRICTED;          /*                         Y02752*/ 05014000
NP3MATCH DS    0H                                                  1522 05015000
*   RESPECIFY                                                      1522 05016000
*     NP3SCAN5 BASED(SCANREG);      /*                         Y02752*/ 05017000
*   RESPECIFY                                                      1523 05018000
*     OPLIBENT BASED(NP3CENT);      /*                         Y02752*/ 05019000
*                                                                  1523 05020000
*   /*****************************************************************/ 05021000
*   /*                                                               */ 05022000
*   /* ENTRY POINT TO SYMBOL TAB SCAN                          Y02752*/ 05023000
*   /*                                                               */ 05024000
*   /*****************************************************************/ 05025000
*                                                                  1524 05026000
*   NP3MSAVR=REXIT;                 /* SAVE RETURN ADDR        Y02752*/ 05027000
         ST    REXIT,NP3MSAVR                                      1524 05028000
*   INDXELMT=0;                     /* INIT INDEX              Y02752*/ 05029000
*                                                                  1525 05030000
         MVI   INDXELMT,X'00'                                      1525 05031000
*   /*****************************************************************/ 05032000
*   /*                                                               */ 05033000
*   /* CALCULATE OFFSET TO SYMBOL TABLE                        Y02752*/ 05034000
*   /*                                                               */ 05035000
*   /*****************************************************************/ 05036000
*                                                                  1526 05037000
*NP3OFST:                                                          1526 05038000
*   OFFSTREG=INDXELMT*LENGTH(SYMBSTRT);/*                      Y02752   05039000
*                                                              Y02752*/ 05040000
*                                                                  1526 05041000
NP3OFST  SLR   OFFSTREG,OFFSTREG                                   1526 05042000
         IC    OFFSTREG,INDXELMT                                   1526 05043000
         SLA   OFFSTREG,3                                          1526 05044000
*   /*****************************************************************/ 05045000
*   /*                                                               */ 05046000
*   /* SET BASE ADDRESS OF SYMBOL IN SYMBOL TABLE FOR VALIDITY CHECK */ 05047000
*   /* ON PARAMETER                                            Y02752*/ 05048000
*   /*                                                               */ 05049000
*   /*****************************************************************/ 05050000
*                                                                  1527 05051000
*   SYMBLREG=OFFSTREG+ADDR(NP3SYMBL);/*                        Y02752   05052000
*                                                              Y02752*/ 05053000
         LA    SYMBLREG,NP3SYMBL                                   1527 05054000
         ALR   SYMBLREG,OFFSTREG                                   1527 05055000
*   IF SYMBLREG>=ADDR(SYMBEND) THEN                                1528 05056000
         LA    @10,SYMBEND                                         1528 05057000
         CR    SYMBLREG,@10                                        1528 05058000
         BL    @RF01528                                            1528 05059000
*     DO;                                                          1529 05060000
*       RENTRY=4;                   /* INDICATE NO MATCH     @Y30LPXD*/ 05061000
*                                                                  1530 05062000
         LA    RENTRY,4                                            1530 05063000
*       /*************************************************************/ 05064000
*       /*                                                           */ 05065000
*       /* UPDATE SLN TO LENGTH OF INVALID CHARACTER STRING  @Y30LPXD*/ 05066000
*       /*                                                           */ 05067000
*       /*************************************************************/ 05068000
*                                                                  1531 05069000
*       DO SLN=0 TO 7 BY 1 WHILE NP3SCANR^='''';/*           @Y30LPXD*/ 05070000
         SLR   @10,@10                                             1531 05071000
         STC   @10,SLN                                             1531 05072000
@DL01531 CLI   NP3SCANR(SCANREG),C''''                             1531 05073000
         BE    @DC01531                                            1531 05074000
*         SCANREG=SCANREG+1;        /* UPDATE NP3SCANR BASE TO NEXT     05075000
*                                      CHARACTER             @Y30LPXD*/ 05076000
         LA    SCANREG,1(,SCANREG)                                 1532 05077000
*       END;                                                       1533 05078000
         AL    @10,@CF00034                                        1533 05079000
         STC   @10,SLN                                             1533 05080000
         CH    @10,@CH00168                                        1533 05081000
         BNH   @DL01531                                            1533 05082000
@DC01531 DS    0H                                                  1534 05083000
*       SCANREG=SCANREG-SLN;        /* RESET SCANREG         @Y30LPXD*/ 05084000
         SLR   @10,@10                                             1534 05085000
         IC    @10,SLN                                             1534 05086000
         SLR   SCANREG,@10                                         1534 05087000
*       GO TO NP3MSAVR;             /* RETURN TO CALLER      @Y30LPXD*/ 05088000
         L     @10,NP3MSAVR                                        1535 05089000
         BR    @10                                                 1535 05090000
*     END;                                                         1536 05091000
*                                                                  1536 05092000
*   /*****************************************************************/ 05093000
*   /*                                                               */ 05094000
*   /* SET SYMBOL LENGTH VALUE TO MAXIMUM MINUS ONE(1)       @Y30LPXD*/ 05095000
*   /*                                                               */ 05096000
*   /*****************************************************************/ 05097000
*                                                                  1537 05098000
*   SLN=LENGTH(SYMBSTRT)-1;         /*                       @Y30LPXD   05099000
*                                                              Y02752*/ 05100000
*                                                                  1537 05101000
@RF01528 LA    @10,7                                               1537 05102000
         STC   @10,SLN                                             1537 05103000
*   /*****************************************************************/ 05104000
*   /*                                                               */ 05105000
*   /* SET MAPPING BASE ADDRESS TO LENGTH ELEMENT IN CURRENT SYMBOL  */ 05106000
*   /* TABLE ENTRY                                             Y02752*/ 05107000
*   /*                                                               */ 05108000
*   /*****************************************************************/ 05109000
*                                                                  1538 05110000
*   NP3SYMLN=SYMBLREG+SLN;          /*                         Y02752   05111000
*                                                              Y02752*/ 05112000
*                                                                  1538 05113000
         ALR   @10,SYMBLREG                                        1538 05114000
         ST    @10,NP3SYMLN                                        1538 05115000
*   /*****************************************************************/ 05116000
*   /*                                                               */ 05117000
*   /* CHECKS FOR SYMBOL LENGTH. IF GREATER THAN 7,THEN SYMBOL LENGTH*/ 05118000
*   /* VALUE IS SET TO 8. ELSE, SYMBOL LENGTH                  Y02752*/ 05119000
*   /*                                                               */ 05120000
*   /*****************************************************************/ 05121000
*                                                                  1539 05122000
*   IF NP3LCHAR>SLN                 /* IS SYMBOL LENGTH GREATER THAN    05123000
*                                      7                       Y02752*/ 05124000
*     THEN                          /* YES,LENGTH GREATER THEN 7   1539 05125000
*                                                              Y02752*/ 05126000
         CLC   NP3LCHAR(1,@10),SLN                                 1539 05127000
         BNH   @RF01539                                            1539 05128000
*     SLN=LENGTH(SYMBSTRT);         /* SET MAXIMUM LENGTH      Y02752*/ 05129000
         MVI   SLN,X'08'                                           1540 05130000
*   ELSE                            /* NO, LENGTH NOT GREATER THEN 7    05131000
*                                                              Y02752*/ 05132000
*     SLN=NP3LCHAR;                 /* SET LENGTH TO SYMBOL LENGTH 1541 05133000
*                                                              Y02752*/ 05134000
*                                                                  1541 05135000
         B     @RC01539                                            1541 05136000
@RF01539 L     @10,NP3SYMLN                                        1541 05137000
         MVC   SLN(1),NP3LCHAR(@10)                                1541 05138000
*   /*****************************************************************/ 05139000
*   /*                                                               */ 05140000
*   /* CHECK FOR SYMBOL IN SYMBOL TABLE EQUAL TO PARAMETER     Y02752*/ 05141000
*   /*                                                               */ 05142000
*   /*****************************************************************/ 05143000
*                                                                  1542 05144000
*   IF NP3TABSB(1:SLN)=NP3SCAN5(1:SLN)/* ARE SYMBOLS EQUAL   @Y30LPXD*/ 05145000
*     THEN                                                         1542 05146000
@RC01539 SLR   @10,@10                                             1542 05147000
         IC    @10,SLN                                             1542 05148000
         BCTR  @10,0                                               1542 05149000
         EX    @10,@SC02880                                        1542 05150000
         BNE   @RF01542                                            1542 05151000
*     DO;                                                          1543 05152000
*       RENTRY=0;                   /* INDICATE MATCH        @Y30LPXD*/ 05153000
         SLR   RENTRY,RENTRY                                       1544 05154000
*       GOTO NP3MSAVR;              /* RETURN TO CALLER      @Y30LPXD*/ 05155000
         L     @10,NP3MSAVR                                        1545 05156000
         BR    @10                                                 1545 05157000
*     END;                                                         1546 05158000
*   INDXELMT=1                      /* UPDATE INDEX FOR NEXT SYMBOL     05159000
*                                                            @Y30LPXD*/ 05160000
*       +INDXELMT;                                                 1547 05161000
@RF01542 IC    @10,INDXELMT                                        1547 05162000
         LA    @10,1(,@10)                                         1547 05163000
         STC   @10,INDXELMT                                        1547 05164000
*   GOTO NP3OFST;                   /* ATTEMP ANOTHER MATCH  @Y30LPXD*/ 05165000
         B     NP3OFST                                             1548 05166000
*                                                                  1549 05167000
*   /*****************************************************************/ 05168000
*   /*                                                               */ 05169000
*   /* THIS ROUTINE MOVES THE ENTRIES IN THE TEMPORARY         Y02752*/ 05170000
*   /* PARMTAB INTO THE PARAMETER TABLE USED BY THE RIM'S            */ 05171000
*   /*                                                               */ 05172000
*   /*****************************************************************/ 05173000
*                                                                  1549 05174000
*NPMOVEP:                                                          1549 05175000
*   RESPECIFY                                                      1549 05176000
*    (RNVT,                                                        1549 05177000
*     SCANREG,                                                     1549 05178000
*     PLIBREG,                                                     1549 05179000
*     COUNTREG) RESTRICTED;         /*                         Y02752*/ 05180000
NPMOVEP  DS    0H                                                  1550 05181000
*   RESPECIFY                                                      1550 05182000
*    (PARMREG) RESTRICTED;                                         1550 05183000
*   RESPECIFY                                                      1551 05184000
*     NIPPAHDR BASED(PARMPTR);                                     1551 05185000
*   RESPECIFY                                                      1552 05186000
*     NIPPAREA BASED(PARMPTR);                                     1552 05187000
*   RESPECIFY                                                      1553 05188000
*     OPLIBENT BASED(PLIBREG);                                     1553 05189000
*   NPMSAVE=REXIT;                  /* SAVE RETURN ADDR        Y02752*/ 05190000
         ST    REXIT,NPMSAVE                                       1554 05191000
*   STRINGSZ=ORLN+CONTLN+LENGTH('''');/* GET LENGTH OF PARM STRING +    05192000
*                                      QUOTE                   Y02752*/ 05193000
*                                                                  1555 05194000
         SLR   @10,@10                                             1555 05195000
         ICM   @10,3,ORLN(PLIBREG)                                 1555 05196000
         SLR   @05,@05                                             1555 05197000
         ICM   @05,3,CONTLN(PLIBREG)                               1555 05198000
         ALR   @10,@05                                             1555 05199000
         LA    @10,1(,@10)                                         1555 05200000
         STH   @10,STRINGSZ                                        1555 05201000
*   /*****************************************************************/ 05202000
*   /*                                                               */ 05203000
*   /* WILL THE PARM STRING FIT IN THIS PARMAREA'S FREE SPACE  Y02752*/ 05204000
*   /*                                                               */ 05205000
*   /*****************************************************************/ 05206000
*                                                                  1556 05207000
*FREEAREA:                                                         1556 05208000
*   IF STRINGSZ>ADDR(NIPPAREA)+2048-NIPPABYT THEN/* NO - TRY NEXT  1556 05209000
*                                      PARMAREA                Y02752*/ 05210000
FREEAREA L     @10,PARMPTR                                         1556 05211000
         LA    @05,2048                                            1556 05212000
         ALR   @05,@10                                             1556 05213000
         SL    @05,NIPPABYT(,@10)                                  1556 05214000
         SLR   @15,@15                                             1556 05215000
         ICM   @15,3,STRINGSZ                                      1556 05216000
         CR    @05,@15                                             1556 05217000
         BNL   @RF01556                                            1556 05218000
*     DO;                                                          1557 05219000
*       IF NIPPAQ=0                 /* IS THERE ANOTHER PARMAREA ON     05220000
*                                      THE QUEUE               Y02752*/ 05221000
*         THEN                                                     1558 05222000
         ICM   @05,15,NIPPAQ(@10)                                  1558 05223000
         BNZ   @RF01558                                            1558 05224000
*         CALL NP3PAGM;             /* N0 - FREE AREA NOT FOUND GET     05225000
*                                      ANOTHER PARMAREA        Y02752*/ 05226000
         BAL   @14,NP3PAGM                                         1559 05227000
*       PARMPTR=NIPPAQ;             /* POINT TO NEXT PARMAREA  Y02752*/ 05228000
@RF01558 L     @10,PARMPTR                                         1560 05229000
         L     @10,NIPPAQ(,@10)                                    1560 05230000
         ST    @10,PARMPTR                                         1560 05231000
*       GO TO FREEAREA;             /* CHECK FREE AREA         Y02752*/ 05232000
         B     FREEAREA                                            1561 05233000
*     END;                          /*                         Y02752*/ 05234000
*   NIPPTADR=NIPPABYT;              /* SET START ADRRESS OF PARMAREA    05235000
*                                      PARAMETERS IN PARMTAB   Y02752*/ 05236000
@RF01556 L     @10,PARMPTR                                         1563 05237000
         MVC   NIPPTADR(4,PARMREG),NIPPABYT(@10)                   1563 05238000
*   RESPECIFY                                                      1564 05239000
*     NP3SCAN5 BASED(ADPTR);        /*                         Y02752*/ 05240000
*   RESPECIFY                                                      1565 05241000
*     MSGBUFF BASED(NIPPABYT);      /*                         Y02752*/ 05242000
*   ADPTR=ORAD;                     /* POINT TO PLIBTAB PARMETER   1566 05243000
*                                      STRING ORGIN            Y02752*/ 05244000
         MVC   ADPTR(4),ORAD(PLIBREG)                              1566 05245000
*   STRINGSZ=ORLN;                  /* SET PARM STRING LENGTH      1567 05246000
*                                      (WITHOUT CONTINUATION)  Y02752*/ 05247000
         MVC   STRINGSZ(2),ORLN(PLIBREG)                           1567 05248000
*   CONTDONE='0'B;                  /* SET CONT SWITCH TO ZERO Y02752*/ 05249000
         NI    CONTDONE,B'11111110'                                1568 05250000
*NP3NULL:                                                          1569 05251000
*   IF NP3SCANR^='FF'X              /* IS THIS CHAR X'FF'      Y02752*/ 05252000
*     THEN                          /* N0 - MOVE CHAR TO PARMTAB   1569 05253000
*                                                              Y02752*/ 05254000
NP3NULL  L     @10,ADPTR                                           1569 05255000
         CLI   NP3SCANR(@10),X'FF'                                 1569 05256000
         BE    @RF01569                                            1569 05257000
*     DO;                                                          1570 05258000
*       MSGBUFF1=NP3SCANR;          /* MOVE PARAMETER FROM PLIBTAB TO   05259000
*                                      PARMTAB                 Y02752*/ 05260000
         L     @05,PARMPTR                                         1571 05261000
         L     @15,NIPPABYT(,@05)                                  1571 05262000
         MVC   MSGBUFF1(1,@15),NP3SCANR(@10)                       1571 05263000
*       NIPPABYT=NIPPABYT+1;        /* POINT TO NEXT BYTE IN PARMTAB    05264000
*                                                              Y02752*/ 05265000
         LA    @14,1                                               1572 05266000
         ALR   @15,@14                                             1572 05267000
         ST    @15,NIPPABYT(,@05)                                  1572 05268000
*       ADPTR=ADPTR+1;              /* POINT TO NEXT BYTE IN PLIBTAB    05269000
*                                                              Y02752*/ 05270000
         ALR   @10,@14                                             1573 05271000
         ST    @10,ADPTR                                           1573 05272000
*       STRINGSZ=STRINGSZ-1;        /* DECREMENT STRING BY ONE Y02752*/ 05273000
         SLR   @10,@10                                             1574 05274000
         ICM   @10,3,STRINGSZ                                      1574 05275000
         BCTR  @10,0                                               1574 05276000
         STH   @10,STRINGSZ                                        1574 05277000
*       IF STRINGSZ=0               /* IF ALL OF STRING HAS BEEN   1575 05278000
*                                                              Y02752*/ 05279000
*         THEN                      /* MOVED, GO CHECK FOR CONT    1575 05280000
*                                                              Y02752*/ 05281000
         LTR   @10,@10                                             1575 05282000
         BZ    @RT01575                                            1575 05283000
*         GO TO CONTCHK;                                           1576 05284000
*       ELSE                                                       1577 05285000
*         GO TO NP3NULL;            /* OTHERWISE,CHECK NEXT CHAR   1577 05286000
*                                                              Y02752*/ 05287000
         B     NP3NULL                                             1577 05288000
*     END;                                                         1578 05289000
*   ELSE                            /* CHAR IS NULL (X'FF')    Y02752*/ 05290000
*     DO;                           /* SKIP NULL CHAR AND DO   Y02752*/ 05291000
@RF01569 DS    0H                                                  1580 05292000
*       ADPTR=ADPTR+1;              /* NOT MOVE TO PARMTAB     Y02752*/ 05293000
         LA    @10,1                                               1580 05294000
         AL    @10,ADPTR                                           1580 05295000
         ST    @10,ADPTR                                           1580 05296000
*       STRINGSZ=STRINGSZ-1;        /* DECREMENT SIZE          Y02752*/ 05297000
         SLR   @10,@10                                             1581 05298000
         ICM   @10,3,STRINGSZ                                      1581 05299000
         BCTR  @10,0                                               1581 05300000
         STH   @10,STRINGSZ                                        1581 05301000
*       GO TO NP3NULL;              /* GO CHECK NEXT CHAR      Y02752*/ 05302000
         B     NP3NULL                                             1582 05303000
*     END;                          /*                         Y02752*/ 05304000
*CONTCHK:                                                          1584 05305000
*   IF CONTDONE^='1'B               /* HAS CONTINUATION BEEN MOVED 1584 05306000
*                                                              Y02752*/ 05307000
*     THEN                                                         1584 05308000
CONTCHK  TM    CONTDONE,B'00000001'                                1584 05309000
         BO    @RF01584                                            1584 05310000
*     DO;                           /* NO - CHECK FOR CONTINUAT    1585 05311000
*                                                              Y02752*/ 05312000
*       IF CONTAD^=0                /* NO - IS THERE CINTINUAT Y02752*/ 05313000
*         THEN                      /* YES - MOVE CONT         Y02752*/ 05314000
         L     @10,CONTAD(,PLIBREG)                                1586 05315000
         LTR   @10,@10                                             1586 05316000
         BZ    @RF01586                                            1586 05317000
*         DO;                                                      1587 05318000
*           CONTDONE='1'B;          /* INDICATE CONTINUATION IS BEING   05319000
*                                      MOVED                   Y02752*/ 05320000
         OI    CONTDONE,B'00000001'                                1588 05321000
*           ADPTR=CONTAD;           /* SET BASE TO CONT STRING Y02752*/ 05322000
         ST    @10,ADPTR                                           1589 05323000
*           STRINGSZ=CONTLN;        /* SET CONT STRING LENGTH  Y02752*/ 05324000
         MVC   STRINGSZ(2),CONTLN(PLIBREG)                         1590 05325000
*           GO TO NP3NULL;          /* GO DO THE MOVE          Y02752*/ 05326000
         B     NP3NULL                                             1591 05327000
*         END;                      /*                         Y02752*/ 05328000
*     END;                          /*                         Y02752*/ 05329000
*                                                                  1593 05330000
@RF01586 DS    0H                                                  1594 05331000
*   /*****************************************************************/ 05332000
*   /*                                                               */ 05333000
*   /* EITHER THERE WAS NO CONTINUATION OR THE CONTINUATION    Y02752*/ 05334000
*   /* HAS BEEN MOVED                                          Y02752*/ 05335000
*   /*                                                               */ 05336000
*   /*****************************************************************/ 05337000
*                                                                  1594 05338000
*   MSGBUFF1=QUOTE;                 /* ADD QUOTE TO END OF PARAMETER    05339000
*                                      STRING                  Y02752*/ 05340000
@RF01584 L     @10,PARMPTR                                         1594 05341000
         L     @05,NIPPABYT(,@10)                                  1594 05342000
         MVC   MSGBUFF1(1,@05),QUOTE                               1594 05343000
*   NIPPABYT=NIPPABYT+1;            /* UPDATE NEXT AVAILABLE BYT   1595 05344000
*                                                              Y02752*/ 05345000
*                                                                  1595 05346000
         AL    @05,@CF00034                                        1595 05347000
         ST    @05,NIPPABYT(,@10)                                  1595 05348000
*   /*****************************************************************/ 05349000
*   /*                                                               */ 05350000
*   /* CHECK FOR NULL REPLY Y02752                                   */ 05351000
*   /*                                                               */ 05352000
*   /*****************************************************************/ 05353000
*                                                                  1596 05354000
*   IF NIPPTADR^=0 THEN                                            1596 05355000
         L     @10,NIPPTADR(,PARMREG)                              1596 05356000
         LTR   @10,@10                                             1596 05357000
         BZ    @RF01596                                            1596 05358000
*     DO;                                                          1597 05359000
*                                                                  1597 05360000
*       /*************************************************************/ 05361000
*       /*                                                           */ 05362000
*       /* CHECK FOR LIST OPTION ONLY REPLY Y02752                   */ 05363000
*       /*                                                           */ 05364000
*       /*************************************************************/ 05365000
*                                                                  1598 05366000
*       IF NIPPTADR->NP3SCAN8(1:3)='()''' THEN                     1598 05367000
         CLC   NP3SCAN8(3,@10),@CC02828                            1598 05368000
         BNE   @RF01598                                            1598 05369000
*         DO;                                                      1599 05370000
*                                                                  1599 05371000
*           /*********************************************************/ 05372000
*           /*                                                       */ 05373000
*           /* PROMPT IN NIP PROMPT MODE IF ERROR OPTION NOT         */ 05374000
*           /* SPECIFIED                                             */ 05375000
*           /*                                                       */ 05376000
*           /*********************************************************/ 05377000
*                                                                  1600 05378000
*           IF NVTSYSP='1'B&NPEPRMT='0'B THEN                      1600 05379000
         TM    NVTSYSP(RNVT),B'01000000'                           1600 05380000
         BNO   @RF01600                                            1600 05381000
         TM    NPEPRMT,B'10000000'                                 1600 05382000
         BNZ   @RF01600                                            1600 05383000
*             RETCODE=4;                                           1601 05384000
         MVC   RETCODE(4),@CF00371                                 1601 05385000
*           ELSE                                                   1602 05386000
*             DO;                   /* NORMAL EOB PROCESSING   Y02752*/ 05387000
         B     @RC01600                                            1602 05388000
@RF01600 DS    0H                                                  1603 05389000
*               NIPPTCAN='1'B;      /* SET CANCEL FLAG         Y02752*/ 05390000
