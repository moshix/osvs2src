*   /*****************************************************************/ 00976000
*   /*                                                               */ 00977000
*   /* WTO 'SYSP INPUT TERMINATED IN IEASYSXX' MESSAGE               */ 00978000
*   /*                                                               */ 00979000
*   /*****************************************************************/ 00980000
*                                                                  0565 00981000
*   GENERATE REFS(NVTWTO);                                         0565 00982000
          IEAPMNIP TYPE=WTO,PARAM=MESG07 CALL WTO SERVICE ROUTINE       00983000
*   SYSPRTRY='1'B;                  /* INDICATE SECOND LEVEL ENTRY TO   00984000
*                                      SYSP ROUTINE                  */ 00985000
         OI    SYSPRTRY,B'00000100'                                0566 00986000
