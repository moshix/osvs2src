*NP3SYINV:                                                         0690 01404000
*   SYSPNAME=SYSPCHAR;                                             0690 01405000
*                                                                  0690 01406000
NP3SYINV MVC   SYSPNAME(4),SYSPCHAR                                0690 01407000
*   /*****************************************************************/ 01408000
*   /*                                                               */ 01409000
*   /* MESSAGE - INVALID SYSP PARM - RESPECIFY OR CANCEL             */ 01410000
*   /*                                                               */ 01411000
*   /*****************************************************************/ 01412000
*                                                                  0691 01413000
*   GENERATE REFS(NVTWTOR);                                        0691 01414000
          IEAPMNIP TYPE=WTOR,PARAM=PLIST10 CALL WTOR SERVICE ROUTINE    01415000
*   SCANREG=ADDR(RPLYBUF);          /* SET SCAN LIST POINTER TO    0692 01416000
*                                      OPERATOR REPLY                */ 01417000
         LA    SCANREG,RPLYBUF                                     0692 01418000
*   IF NP3SCAN2=NP3BLNKQ            /* DID OPERATOR REPLY CANCEL   0693 01419000
*                                      (EOB)                         */ 01420000
*     THEN                                                         0693 01421000
         CLC   NP3SCAN2(2,SCANREG),NP3BLNKQ                        0693 01422000
