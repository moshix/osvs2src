*   END NP10IPSP;                   /* END OF IPS SYSTEM PARAMETER 0411 01113000
*                                      PROCESSING                    */ 01114000
*                                                                  0412 01115000
*/********************************************************************/ 01116000
*/*                                                                  */ 01117000
*/*  PROCEDURE NAME: NP10VLDP                                        */ 01118000
*/*                                                                  */ 01119000
*/*  FUNCTION:                                                       */ 01120000
*/*  CHECK THE INPUT FOR IPS/OPT SYSTEM PARAMETERS.                  */ 01121000
*/*  DETERMINE IF DEFAULT SYSTEM PARAMETER VALUE IS REQUESTED.       */ 01122000
*/*  OTHERWISE IT POINTS TO TWO ALPHANUMERIC CHARACTERS WHICH ARE    */ 01123000
*/*  USED AS SUFFIXES FOR THE MEMBER NAME OF THE IPS/OPT LIST.       */ 01124000
*/*                                                                  */ 01125000
*/********************************************************************/ 01126000
*                                                                  0412 01127000
*NP10VLDP:                                                         0412 01128000
*   PROC;                                                          0412 01129000
         ST    @05,@TF00002                                        0412 01130000
NP10VLDP STM   @14,@12,12(@13)                                     0412 01131000
*   SCNPTR=NIPPTADR;                /* ADDR OF INPUT STRING          */ 01132000
         L     SCNPTR,NIPPTADR(,NPTEPTR)                           0413 01133000
*   IF PARMCHAR=LPAREN THEN         /* IFNORE DELIMITER              */ 01134000
         CLI   PARMCHAR(SCNPTR),C'('                               0414 01135000
         BNE   @RF00414                                            0414 01136000
*     SCNPTR=SCNPTR+ONE;            /* SKIP LEFT PARENTHESIS         */ 01137000
         AL    SCNPTR,@CF00062                                     0415 01138000
*/*  CHECK FOR 2 ALPHANUMERIC CHARACTERS                             */ 01139000
*                                                                  0416 01140000
*   DO I=ONE TO TWO;                                               0416 01141000
@RF00414 LA    I,1                                                 0416 01142000
@DL00416 DS    0H                                                  0417 01143000
*     IF PARMCHAR<CHARA|(PARMCHAR>CHARZ&PARMCHAR<CHAR0)|PARMCHAR>CHAR9  01144000
*       THEN                        /* CHAR IS NON-ALPHANUMERIC      */ 01145000
         CLI   PARMCHAR(SCNPTR),C'A'                               0417 01146000
         BL    @RT00417                                            0417 01147000
         CLI   PARMCHAR(SCNPTR),C'Z'                               0417 01148000
         BNH   @GL00007                                            0417 01149000
         CLI   PARMCHAR(SCNPTR),C'0'                               0417 01150000
         BL    @RT00417                                            0417 01151000
@GL00007 CLI   PARMCHAR(SCNPTR),C'9'                               0417 01152000
         BNH   @RF00417                                            0417 01153000
@RT00417 DS    0H                                                  0418 01154000
*       RETURN TO NP10ELBL;         /* RETURN TO CALLER AT A LABEL 0418 01155000
*                                      WHICH PROCESSES ERROR         */ 01156000
         LR    @14,ELBLPTR                                         0418 01157000
         LM    @15,@12,16(@13)                                     0418 01158000
         BR    @14                                                 0418 01159000
*     ELSE                          /* CHAR IS ALPHANUMERIC          */ 01160000
*       SCNPTR=SCNPTR+ONE;          /* POINT TO NEXT CHAR            */ 01161000
@RF00417 AL    SCNPTR,@CF00062                                     0419 01162000
*   END;                            /* COMPLETED INPUT VALIDATION    */ 01163000
         AL    I,@CF00062                                          0420 01164000
         C     I,@CF00074                                          0420 01165000
         BNH   @DL00416                                            0420 01166000
*/*  STORE SUFFIX IN NP10WSUF AND STORE MEMBER NAME IN PARMAREA      */ 01167000
*/*  TO COMMUNICATE WITH NIP READ SERVICE ROUTINE                    */ 01168000
*                                                                  0421 01169000
*   SCNPTR=SCNPTR-TWO;              /* POINT TO SUFFIX               */ 01170000
         BCTR  SCNPTR,0                                            0421 01171000
         BCTR  SCNPTR,0                                            0421 01172000
*   NP10WSUF=SUFFIX;                /* SAVE SUFFIX                   */ 01173000
         MVC   NP10WSUF(2),SUFFIX(SCNPTR)                          0422 01174000
*   NIPPANAM(1:3)=IEANAME;          /* STORE MEMBER NAME             */ 01175000
         L     @12,NVTPAREA(,RNVT)                                 0423 01176000
         MVC   NIPPANAM(3,@12),@CC02719                            0423 01177000
*   NIPPANAM(4:6)=NP10LIST;                                        0424 01178000
         MVC   NIPPANAM+3(3,@12),NP10LIST                          0424 01179000
*   NIPPANAM(7:8)=NP10WSUF;                                        0425 01180000
*                                                                  0425 01181000
         MVC   NIPPANAM+6(2,@12),NP10WSUF                          0425 01182000
*/*  SET SWITCH TO INDICATE WHETHER LISTING IS REQUESTED ACCORDING   */ 01183000
*/*  TO THE LIST FLAG IN THE IPS/OPT PARMTAB ENTRY                   */ 01184000
*                                                                  0426 01185000
*   IF NIPPTLST='1'B                /* TEST IF LIST IS REQUESTED     */ 01186000
*     THEN                          /* LISTING IS REQUESTED          */ 01187000
         TM    NIPPTLST(NPTEPTR),B'01000000'                       0426 01188000
         BNO   @RF00426                                            0426 01189000
*     NP10LST='1'B;                 /* SET LIST SWITCH ON            */ 01190000
         OI    NP10LST,B'00100000'                                 0427 01191000
*   ELSE                            /* LISTING IS NOT REQUESTED      */ 01192000
*     NP10LST='0'B;                 /* SET LIST SWITCH OFF           */ 01193000
         B     @RC00426                                            0428 01194000
@RF00426 NI    NP10LST,B'11011111'                                 0428 01195000
*   NP10IOER='0'B;                  /* RESET I/O ERROR INDICATOR     */ 01196000
@RC00426 DS    0H                                                  0430 01197000
*   NP10EOF='0'B;                   /* RESET END OF FILE INDICATOR   */ 01198000
         NI    NP10IOER,B'00111111'                                0430 01199000
*   RETURN;                         /* RETURN TO CALLER              */ 01200000
@EL00005 DS    0H                                                  0431 01201000
@EF00005 DS    0H                                                  0431 01202000
@ER00005 LM    @14,@12,12(@13)                                     0431 01203000
         BR    @14                                                 0431 01204000
*   END NP10VLDP;                   /* END VALIDATION ROUTINE        */ 01205000
*                                                                  0433 01206000
*/********************************************************************/ 01207000
*/*                                                                  */ 01208000
*/* PROCEDURE NAME: NP10ADJP                                         */ 01209000
*/*                                                                  */ 01210000
*/*  FUNCTION:                                                       */ 01211000
*/*  THIS ROUTINE ADJUSTS THE INPUT NUMBER BY THE ADJUSTMENT FACTOR  */ 01212000
*/*  OF THE IPLING CPU                                               */ 01213000
*/*  THE INPUT NO. IS IN NP10ADJN                                    */ 01214000
*/*                                                                  */ 01215000
*/********************************************************************/ 01216000
*                                                                  0433 01217000
*NP10ADJP:                                                         0433 01218000
*   PROC;                                                          0433 01219000
NP10ADJP STM   @14,@12,@SA00006                                    0433 01220000
*   RESPECIFY                                                      0434 01221000
*    (R14,                                                         0434 01222000
*     R15) RSTD;                                                   0434 01223000
*   GEN REFS(NP10ADJN,NP10ADJX,NP10ADJR,NP10CPMP,FZERO,FONE,R14,R15);   01224000
         L     R15,NP10ADJN              PICK UP INPUT NO.              01225000
         M     R14,NP10CPMP              N = N*CPU ADJ*1000*SCALING     01226000
         D     R14,NP10ADJX              N = N/1024000                  01227000
         C     R14,NP10ADJR              NEED ROUND UP                  01228000
         BNL   CARRY                     YES                            01229000
         C     R15,FZERO                 ADJUSTED TOO LOW   @G50IPEB    01230000
         BNE   NOCARRY                   NO....KEEP VALUE   @G50IPEB    01231000
CARRY    A     R15,FONE                  ROUND UP BY 1                  01232000
NOCARRY  EQU   *                                                        01233000
         ST    R15,NP10ADJN              STORE RESULT                   01234000
*   RESPECIFY                                                      0436 01235000
*    (R14,                                                         0436 01236000
*     R15) UNRSTD;                                                 0436 01237000
*   RETURN;                         /* RETURN TO CALLER              */ 01238000
@EL00006 DS    0H                                                  0437 01239000
@EF00006 DS    0H                                                  0437 01240000
@ER00006 LM    @14,@12,@SA00006                                    0437 01241000
         BR    @14                                                 0437 01242000
*   END NP10ADJP;                                                  0438 01243000
*                                                                  0439 01244000
*/********************************************************************/ 01245000
*/*                                                                  */ 01246000
*/*  ENTRY POINT NAME: IRARMRDR                                      */ 01247000
*/*                                                                  */ 01248000
*/*  FUNCTION:                                                       */ 01249000
*/*  INVOKE NIPPMMEM SUBROUTINE TO READ A RECORD FROM A MEMBER OF    */ 01250000
*/*  SYS1.PARMLIB.  THE MEMBER NAME HAS BEEN PLACED IN THE NIPPANAM  */ 01251000
*/*  FIELD OF THE NIP PARAMETER AREA (NIPPAREA).                     */ 01252000
*/*  CHECK RETURN CODE FROM NIPPMMEM.                                */ 01253000
*/*  IF RETURN CODE INDICATES SUCCESS, GET A BUFFER FROM SUBPOOL 0,  */ 01254000
*/*  MOVE TEXT RECORD TO BUFFER, IF LISTING HAS BEEN REQUESTED,      */ 01255000
*/*  WRITE RECORD ON OPERATOR'S CONSOLE, AND RETURN TO CALLER.       */ 01256000
*/*  IF RETURN CODE FROM NIPPMMEM INDICATES MEMBER NOT FOUND OR I/O  */ 01257000
*/*  ERROR CONDITION, SET NP10IOER FLAG ON AND RETURN TO CALLER.     */ 01258000
*/*                                                                  */ 01259000
*/********************************************************************/ 01260000
*                                                                  0439 01261000
*   GEN CODE DEFS(IRARMRDR) REFS(RMRDRSAV,RMRDRPML) NOEXIT SEQFLOW;     01262000
         ENTRY IRARMRDR                                                 01263000
RMRDRBSF EQU   0                         DISPLACEMENT FOR BASE REG 1    01264000
RMRDRSAP EQU   4                         OFFSET FOR SAVEAREA PTR        01265000
RMRDRRTP EQU   8                         OFFSET FOR RETURN PTR          01266000
RMRDRREG EQU   12                        OFFSET FOR REG SAVEAREA        01267000
RMRDRRG0 EQU   20                        OFFSET FOR REG 0 SAVEAREA      01268000
RMRDRBSS EQU   4095                      DISPLACEMENT FOR BASE REG 2    01269000
RMRDRPDP EQU   0                         DISPLACEMENT OF PARM LIST      01270000
RMRDRPLN EQU   12                        LENGTH OF PARM LIST            01271000
IRARMRDR DS    0H                                                       01272000
         STM   @14,@12,RMRDRREG(@13)     SAVE REGS                      01273000
         BALR  @15,RMRDRBSF              SETUP ENTRY ADDRESSABILITY     01274000
         LA    @10,*-@PSTART             DISPLACEMENT TO MODULE BASE    01275000
         LCR   @10,@10                   NEGATIVE FOR SUBTRACT          01276000
         ALR   @10,@15                   SETUP MODULE ADDRESSABILITY    01277000
         LA    @11,RMRDRBSS(,@10)        DISPLACEMENT TO BASE+4095      01278000
         USING @PSTART,@10                                              01279000
         USING @PSTART+RMRDRBSS,@11                                     01280000
         ST    @13,RMRDRSAV+RMRDRSAP     SAVE SAVEAREA PTR              01281000
         LA    @14,RMRDRSAV                                             01282000
         ST    @14,RMRDRRTP(,@13)        SAVE RETURN PTR                01283000
         LR    @13,@14                                                  01284000
         MVC   RMRDRPML(RMRDRPLN),RMRDRPDP(@01) SET UP PARM LIST        01285000
*   RESPECIFY                                                      0440 01286000
*    (R0,                                                          0440 01287000
*     R1,                                                          0440 01288000
*     R14,                                                         0440 01289000
*     R15) RSTD;                                                   0440 01290000
*   IF NP10EOF='1'B THEN            /* REACH END OF FILE ?           */ 01291000
         TM    NP10EOF,B'01000000'                                 0441 01292000
         BNO   @RF00441                                            0441 01293000
*     DO;                                                          0442 01294000
*       R15=FAIL;                   /* RETURN UNSUCCESSFUL           */ 01295000
         LA    R15,4                                               0443 01296000
*       GO TO NP10REND;             /* RETURN                        */ 01297000
         B     NP10REND                                            0444 01298000
*     END;                                                         0445 01299000
*   RNVT=NP10NVTP;                  /* ADDR OF NVT                   */ 01300000
@RF00441 L     RNVT,NP10NVTP                                       0446 01301000
*   CVTPTR=FLCCVT;                  /* ADDR OF CVT                   */ 01302000
         L     CVTPTR,FLCCVT                                       0447 01303000
*   R0=RECORDSZ;                    /* SUBPOOL NO=0,SIZE=88          */ 01304000
         LA    R0,88                                               0448 01305000
*   GEN( GETMAIN R,LV=(0));         /* GETMAIN FROM SUBPOOL 0        */ 01306000
          GETMAIN R,LV=(0)                                              01307000
*   NP10RBPT=R1;                    /* ADDR OF BUFFER                */ 01308000
         L     @12,RMRDRPM1                                        0450 01309000
         ST    R1,NP10RBPT(,@12)                                   0450 01310000
*   NP10RBLN=RECORDSZ;              /* SIZE OF BUFFER                */ 01311000
         LA    @12,88                                              0451 01312000
         L     @05,RMRDRPM2                                        0451 01313000
         ST    @12,NP10RBLN(,@05)                                  0451 01314000
*   NP10RBSP=RECORDSZ;              /* SUBPOOL INFORMATION           */ 01315000
         L     @05,RMRDRPM3                                        0452 01316000
         ST    @12,NP10RBSP(,@05)                                  0452 01317000
*   CALL NP10SPIO;                  /* READ A RECORD FROM          0453 01318000
*                                      SYS1.PARMLIB                  */ 01319000
         BAL   @14,NP10SPIO                                        0453 01320000
*   RESPECIFY                                                      0454 01321000
*     RPARM RESTRICTED;                                            0454 01322000
*   IF RPARM>=ZERO                  /* TEST RETURN CODE              */ 01323000
*     THEN                          /* SUCCESSFUL READ               */ 01324000
         LTR   RPARM,RPARM                                         0455 01325000
         BM    @RF00455                                            0455 01326000
*     DO;                                                          0456 01327000
*       RESPECIFY                                                  0457 01328000
*         RPARM UNRESTRICTED;                                      0457 01329000
*       NP10RBUF(1:1)=' ';          /* INSERT BLANK IN 1ST BYTE      */ 01330000
         L     @12,RMRDRPM1                                        0458 01331000
         L     @12,NP10RBPT(,@12)                                  0458 01332000
         MVI   NP10RBUF(@12),C' '                                  0458 01333000
*       NP10RBUF(2:81)=NIPPARCD;    /* MOVE IN DATA                  */ 01334000
         L     @05,NVTPAREA(,RNVT)                                 0459 01335000
         MVC   NP10RBUF+1(80,@12),NIPPARCD(@05)                    0459 01336000
*       IF NP10LST='1'B THEN        /* LISTING REQUESTED             */ 01337000
         TM    NP10LST,B'00100000'                                 0460 01338000
         BNO   @RF00460                                            0460 01339000
*         DO;                                                      0461 01340000
*           MSGLSTP(1:71)=NIPPARCD; /* MOVE TO WTO BUFFER            */ 01341000
         MVC   MSGLSTP(71),NIPPARCD(@05)                           0462 01342000
*           GEN(IEAPMNIP TYPE=WTO,PARAM=MSGLSTS)                   0463 01343000
*           REFS(NVTWTO);           /* WTO TEXT RECORD               */ 01344000
         IEAPMNIP TYPE=WTO,PARAM=MSGLSTS                                01345000
*         END;                                                     0464 01346000
*       R15=SUCCESS;                /* RETURN SUCCESSFUL             */ 01347000
@RF00460 SLR   R15,R15                                             0465 01348000
*       GO TO NP10REND;             /* RETURN                        */ 01349000
         B     NP10REND                                            0466 01350000
*     END;                                                         0467 01351000
*   ELSE                            /* UNSUCCESSFUL READ             */ 01352000
*     DO;                                                          0468 01353000
@RF00455 DS    0H                                                  0469 01354000
*       NP10EOF='1'B;               /* SET END OF FILE INDICATOR     */ 01355000
         OI    NP10EOF,B'01000000'                                 0469 01356000
*       IF NIPPARC^=EOFCOND         /* TEST CAUSE OF READ FAILURE    */ 01357000
*         THEN                      /* NOT END-OF-FILE               */ 01358000
         L     @12,NVTPAREA(,RNVT)                                 0470 01359000
         CLC   NIPPARC(4,@12),@CF00792                             0470 01360000
         BE    @RF00470                                            0470 01361000
*         NP10IOER='1'B;            /* SET I/O ERROR FLAG            */ 01362000
         OI    NP10IOER,B'10000000'                                0471 01363000
*       R15=FAIL;                   /* RETURN UNSUCCESSFUL           */ 01364000
@RF00470 LA    R15,4                                               0472 01365000
*       GO TO NP10REND;             /* RETURN                        */ 01366000
         B     NP10REND                                            0473 01367000
*     END;                                                         0474 01368000
*NP10REND:                                                         0475 01369000
*   ;                               /* EXIT CODE                     */ 01370000
NP10REND DS    0H                                                  0476 01371000
*   GEN CODE EXIT NOSEQFLOW;                                       0476 01372000
         L     @13,RMRDRSAP(,@13)        RESTORE SAVEAREA PTR           01373000
         L     @14,RMRDRREG(,@13)        RESTORE REG 14                 01374000
         LM    @00,@12,RMRDRRG0(@13)     RESTORE REGS 0-12              01375000
         BR    @14                                                      01376000
*   RESPECIFY                                                      0477 01377000
*    (R0,                                                          0477 01378000
*     R1,                                                          0477 01379000
*     R14,                                                         0477 01380000
*     R15) UNRSTD;                                                 0477 01381000
*                                                                  0477 01382000
*/********************************************************************/ 01383000
*/*                                                                  */ 01384000
*/*  PROCEDURE NAME: NP10SPIO                                        */ 01385000
*/*                                                                  */ 01386000
*/*  FUNCTION:  SEE IEAPSPIO                                         */ 01387000
*/*                                                                  */ 01388000
*/********************************************************************/ 01389000
*                                                                  0478 01390000
*NP10SPIO:                                                         0478 01391000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* NIP I/O SERVICE RTN           */ 01392000
NP10SPIO DS    0H                                                  0479 01393000
*/********************************************************************/ 01394000
*/*                                                                  */ 01395000
*/* MACRO-      IEAPSPIO                                             */ 01396000
*/*                                                                  */ 01397000
*/* STATUS-     VERSION DATE=10/24/73, VS/2 RELEASE 2                */ 01398000
*/*                                                                  */ 01399000
*/* FUNCTION-   IEAPSPIO IS PART OF THE  SUPERVISOR                  */ 01400000
*/*             INITIALIZATION PROCESS FOR VS2.                      */ 01401000
*/*                                                                  */ 01402000
*/*             THIS MACRO GENERATES INLINE PL/S STATEMENTS          */ 01403000
*/*             WHICH FORM A SUBROUTINE TO FIND AND READ TEXT        */ 01404000
*/*             RECORDS FROM SYS1.PARMLIB.                           */ 01405000
*/*                                                                  */ 01406000
*/*  REGISTER DEPENDENCIES-                                          */ 01407000
*/*                                                                  */ 01408000
*/*                 1. IEAPSPIO DEPENDS ON THE INCLUDING ROUTINES    */ 01409000
*/*                    BASE REGISTERS FOR ADDRESSABILITY             */ 01410000
*/*                                                                  */ 01411000
*/*                 2. REGISTER 3 MUST CONTAIN THE ADDRESS OF        */ 01412000
*/*                    THE CVT ON ENTRY                              */ 01413000
*/*                                                                  */ 01414000
*/*                 3. REGISTER 2 MUST CONTAIN THE ADDRESS OF        */ 01415000
*/*                    THE NVT ON ENTRY                              */ 01416000
*/*                                                                  */ 01417000
*/*                 4. REGISTER 14 MUST CONTAIN THE ADDRESS OF       */ 01418000
*/*                    THE CALLERS RETURN POINT ON ENTRY             */ 01419000
*/*                                                                  */ 01420000
*/* ENTRY POINTS-                                                    */ 01421000
*/*                                                                  */ 01422000
*/*    TWO CALLING SEQUENCRS ARE AVAILABLE.  ONE PROVIDES            */ 01423000
*/*    COMPATIBILITY WITH THE VS2-1 VERSION OF THIS MACRO,           */ 01424000
*/*    THE OTHER CALLING SEQUENCE WAS DEVELOPED EXPRESSLY            */ 01425000
*/*    FOR VS2-2.                                                    */ 01426000
*/*                                                                  */ 01427000
*/*        VS2-1 COMPATIBLE VERSION-                                 */ 01428000
*/*                                                                  */ 01429000
*/*            1. CALL NIPPMPDS TO LOCATE (VIA BLDL) THE MEMBER      */ 01430000
*/*               OF SYS1.PARMLIB                                    */ 01431000
*/*                                                                  */ 01432000
*/*            2. CALL NIPPMTXT TO READ A TEXT RECORD FROM           */ 01433000
*/*               SYS1.PARMLIB                                       */ 01434000
*/*                                                                  */ 01435000
*/*        VS2-2 VERSION-                                            */ 01436000
*/*                                                                  */ 01437000
*/*            1. CALL NIPPMMEM TO LOCATE THE MEMBER AND RETURN      */ 01438000
*/*               A TEXT RECORD                                      */ 01439000
*/*                                                                  */ 01440000
*/*            2. REPEAT CALL TO NIPPMMEM TO READ ADDITIONAL         */ 01441000
*/*               TEXT RECORDS                                       */ 01442000
*/*                                                                  */ 01443000
*/* OUTPUT (NORMAL) -                                                */ 01444000
*/*                                                                  */ 01445000
*/*     THE ROUTINE WILL SAVE AND RESTORE ALL GENERAL REGISTERS      */ 01446000
*/*     WITH THE EXCEPTION OF RPARM.  ON A SUCCESSFUL READ,          */ 01447000
*/*     RPARM (REGISTER 1) WILL BE SET TO POINT TO THE FIRST NON-    */ 01448000
*/*     BLANK CHARACTER IN THE TEXT RECORD.  IF THE RECORD           */ 01449000
*/*     COLUMNS 1 THRU 72 ALL CONTAIN BLANKS, RPARM WILL             */ 01450000
*/*     POINT TO A BLANK IN COLUMN 72.  IF COLUMNS 1 THRU 71         */ 01451000
*/*     ARE BLANK BUT COLUMN 72 IS NON-BLANK, THE ROUTINE            */ 01452000
*/*     WILL AUTOMATICALLY ATTEMPT TO READ THE NEXT RECORD.  IN      */ 01453000
*/*     ALL CASES, COLUMN 72 WILL BE SET TO BLANK AND COLUMN 73 WILL */ 01454000
*/*     BE SET TO X'FF'.                                             */ 01455000
*/*                                                                  */ 01456000
*/* EXIT (NORMAL)-                                                   */ 01457000
*/*                                                                  */ 01458000
*/*     VS2-1 AND VS2-2 VERSIONS RETURN TO THE ADDRESS FOUND         */ 01459000
*/*     IN REGISTER 14 ON ENTRY.                                     */ 01460000
*/*                                                                  */ 01461000
*/* EXIT (ERROR)-                                                    */ 01462000
*/*                                                                  */ 01463000
*/*     VS2-1 COMPATIBLE VERSION-                                    */ 01464000
*/*                                                                  */ 01465000
*/*         RETURN TO THE ADDRESS IN REGISTER 14 +4                  */ 01466000
*/*                                                                  */ 01467000
*/*         NOTE- WHEN AN ERROR OCCURS ON AN ATTEMP TO READ A RECORD */ 01468000
*/*               THAT DOES NOT EXIST (E-O-F),                       */ 01469000
*/*               IT IS CONSIDERED A PERMANENT I/O ERROR             */ 01470000
*/*                                                                  */ 01471000
*/*     VS2-2 VERSION-                                               */ 01472000
*/*                                                                  */ 01473000
*/*        RETURN TO THE ADDRESS FOUND IN REGISTER 14 ON ENTRY       */ 01474000
*/*        WITH REGISTER 1 (RPARM) SET TO A NEGATIVE VALUE.  IN THE  */ 01475000
*/*        CASE WHERE RPARM IS NEGATIVE ON RETURN, THE NIPPARC FIELD */ 01476000
*/*        OF THE NIP PATAMETER AREA WILL INDICATE THE REASON FOR    */ 01477000
*/*        FAILURE AS FOLLOWS-                                       */ 01478000
*/*                            4 - MEMBER NOT FOUND                  */ 01479000
*/*                            8 - I/O ERROR DURING BLDL             */ 01480000
*/*                            12- I/O ERROR DURING READ             */ 01481000
*/*                            16- END-OF-FILE                       */ 01482000
*/*                                                                  */ 01483000
*/*     COMMON TO BOTH VERSIONS-                                     */ 01484000
*/*                                                                  */ 01485000
*/*        AN INTERPRETIVE MESSAGE WILL BE WRITTEN TO THE OPERATOR   */ 01486000
*/*        FOR EACH OF THE FIRST THREE ERROR CONDITIONS LISTED       */ 01487000
*/*        ABOVE.  THE CALLER MAY SUPPRESS THE 'MEMBER NOT FOUND'    */ 01488000
*/*        MESSAGE BY SETTING THE NIPPAFL1 FLAG FIELD TO BINARY      */ 01489000
*/*        ONE IN THE NIP PARAMETER AREA BEFORE CALLING THIS         */ 01490000
*/*        ROUTINE.  NO MESSAGE WILL BE WRITTEN FOR THE E-O-F        */ 01491000
*/*        CONDITION IF THE VS2-2 VERSION IS USED.                   */ 01492000
*/*                                                                  */ 01493000
*/* MACROS-                                                          */ 01494000
*/*     THE FOLLOWING MAPPING MACROS MUST BE                         */ 01495000
*/*     INCLUDED IN THE MODULE WHICH INCLUDES                        */ 01496000
*/*     IEAPSPIO-                                                    */ 01497000
*/*                                       IEZIOB                     */ 01498000
*/*                                       IHANVT                     */ 01499000
*/*                                       IHAECB                     */ 01500000
*/*                                       IEAPPNIP                   */ 01501000
*/*                                       IEFUCBOB                   */ 01502000
*/*                                       CVT                        */ 01503000
*/*                                                                  */ 01504000
*/* NOTES-                                                           */ 01505000
*/*        THE FIRST FOUR LETTERS OF ALL IEAPSPIO SYMBOLES ARE:      */ 01506000
*/*        NIPP OR IPPM.                                             */ 01507000
*/*                                                                  */ 01508000
*/*        REGISTERS 0, 1, 14, AND 15 WILL BE UNRESTRICTED AT THE    */ 01509000
*/*        END OF THE SEGMENT GENERATED BY IEAPSPIO.                 */ 01510000
*/*                                                                  */ 01511000
*/*        NIPPAREA WILL BE BASED ON NVTPAREA AND IOBSTDRD WILL BE   */ 01512000
*/*        BASED ON THE ADDR OF NIPPAIOB AT THE END OF THE SEGMENT   */ 01513000
*/*        GENERATED BY IEAPSPIO                                     */ 01514000
*/*                                                                  */ 01515000
*/*        IEAPSPIO MUST BE INCLUDED IN A CODE SEGMENT,              */ 01516000
*/*        NOT AT THE TOP OF THE PROCEDURE WITH THE                  */ 01517000
*/*        MAPPING MACROS LISTED ABOVE.                              */ 01518000
*/*                                                            Y02752*/ 01519000
*/********************************************************************/ 01520000
*                                                                  0479 01521000
*   /*****************************************************************/ 01522000
*   /*                                                               */ 01523000
*   /* REGISTERS                                                     */ 01524000
*   /*                                                               */ 01525000
*   /*****************************************************************/ 01526000
*                                                                  0479 01527000
*   DCL                                                            0479 01528000
*     NIPPMR00 REG(0) PTR(24);                                     0479 01529000
*   DCL                                                            0480 01530000
*     NIPPMR01 REG(1) PTR(24);                                     0480 01531000
*   DCL                                                            0481 01532000
*     NIPPMR02 REG(2) PTR(24);                                     0481 01533000
*   DCL                                                            0482 01534000
*     NIPPMR06 REG(6) PTR(24);                                     0482 01535000
*   DCL                                                            0483 01536000
*     NIPPMR09 REG(9) PTR(24);                                     0483 01537000
*   DCL                                                            0484 01538000
*     NIPPMR12 REG(12) PTR(24);                                    0484 01539000
*   DCL                                                            0485 01540000
*     NIPPMR13 REG(13) PTR(24);                                    0485 01541000
*   DCL                                                            0486 01542000
*     NIPPMR14 REG(14) PTR(24);                                    0486 01543000
*   DCL                                                            0487 01544000
*     NIPPMR15 REG(15) PTR(24);                                    0487 01545000
*                                                                  0487 01546000
*   /*****************************************************************/ 01547000
*   /*                                                               */ 01548000
*   /* SAVE AREA FOR CALLER'S REGISTERS                              */ 01549000
*   /*                                                               */ 01550000
*   /*****************************************************************/ 01551000
