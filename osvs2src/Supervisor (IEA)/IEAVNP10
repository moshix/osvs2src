         OC    @TS00001(1),NIPPMMK1                                0579 02008000
         CLI   @TS00001,X'FF'                                      0579 02009000
         BNE   @RF00579                                            0579 02010000
*             DO;                                                  0580 02011000
*               NIPPARC=16;         /* INDICATE EOF CONDITION  Y02752*/ 02012000
         MVC   NIPPARC(4,@12),@CF00792                             0581 02013000
*               GOTO NIPPMPXT;      /* RETURN TO CALLER        Y02752*/ 02014000
         B     NIPPMPXT                                            0582 02015000
*             END;                  /*                         Y02752*/ 02016000
*                                                                  0583 02017000
*           /*********************************************************/ 02018000
*           /*                                                       */ 02019000
*           /* DOES THE I/O ERROR REPRESENT END OF CYL?      @ZA07472*/ 02020000
*           /*                                                       */ 02021000
*           /*********************************************************/ 02022000
*                                                                  0584 02023000
*           IF NIPPAECC='41'X&      /*                       @ZA07472*/ 02024000
*               (NIPPMCS2|NIPPMMK2)='FF'X&/*                 @ZA07472*/ 02025000
*               (IOBSENS1&NIPPMMK3)='20'X THEN/*             @ZA07472*/ 02026000
*                                                                  0584 02027000
@RF00579 L     @12,NVTPAREA(,RNVT)                                 0584 02028000
         CLI   NIPPAECC(@12),X'41'                                 0584 02029000
         BNE   @RF00584                                            0584 02030000
         MVC   @TS00001(1),NIPPMCS2+81(@12)                        0584 02031000
         OC    @TS00001(1),NIPPMMK2                                0584 02032000
         CLI   @TS00001,X'FF'                                      0584 02033000
         BNE   @RF00584                                            0584 02034000
