*               GOTO NIPPMRCR;      /* CONTINUE TO PROCESS TEXT    0589 02057000
*                                                            @ZA07472*/ 02058000
         B     NIPPMRCR                                            0589 02059000
*             END;                  /*                       @ZA07472*/ 02060000
*         END;                      /*                         Y02752*/ 02061000
*       GOTO NIPPMTER;              /* INDICATE I/O ERROR      Y02752*/ 02062000
         B     NIPPMTER                                            0592 02063000
*     END;                          /*                         Y02752*/ 02064000
*   RESPECIFY                                                      0594 02065000
*     NIPPMBYT BASED(NIPPMR01);                                    0594 02066000
@RF00575 DS    0H                                                  0595 02067000
*NIPPMRCR:                          /*                       @ZA07472*/ 02068000
*   NIPPMR01=ADDR(NIPPARCD);        /* R1 = ADDR OF TEXT RECORD READ    02069000
*                                      IN                            */ 02070000
*                                                                  0595 02071000
