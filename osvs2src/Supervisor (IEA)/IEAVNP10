*   GENERATE;                                                      0574 01967000
*                                                                  0574 01968000
         WAIT  ECB=(1)                  AWAIT I/O COMPLETION            01969000
*   /*****************************************************************/ 01970000
*   /*                                                               */ 01971000
*   /* TEST FOR I/O ERROR AFTER PARMLIB READ                   Y02752*/ 01972000
*   /*                                                               */ 01973000
*   /*****************************************************************/ 01974000
*                                                                  0575 01975000
*   IF NIPPAECC^='7F'X              /* SKIP IF READ SUCCESSFUL Y02752*/ 01976000
*     THEN                                                         0575 01977000
         L     @12,NVTPAREA(,RNVT)                                 0575 01978000
         CLI   NIPPAECC(@12),X'7F'                                 0575 01979000
         BE    @RF00575                                            0575 01980000
*     DO;                                                          0576 01981000
*       IF NIPPMNAM=NIPPANAM        /* SEE IF MEMBER NAMES MATCH   0577 01982000
*                                                              Y02752*/ 01983000
*         THEN                                                     0577 01984000
