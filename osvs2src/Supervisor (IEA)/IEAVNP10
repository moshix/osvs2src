         NC    @TS00001(1),NIPPMMK3                                0584 02036000
         CLI   @TS00001,X'20'                                      0584 02037000
         BNE   @RF00584                                            0584 02038000
*             /*******************************************************/ 02039000
*             /*                                                     */ 02040000
*             /* IF YES, MAKE SEEK ADDRESS POINT TO START OF NEXT    */ 02041000
*             /* CYLINDER                                    @ZA07472*/ 02042000
*             /*                                                     */ 02043000
*             /*******************************************************/ 02044000
*                                                                  0585 02045000
*             DO;                   /*                       @ZA07472*/ 02046000
*               NIPPMCC=NIPPMCC+1;  /* INCREMENT SEEK@ TO NEXT CYL,     02047000
*                                                            @ZA07472*/ 02048000
         AL    @12,@CF00879                                        0586 02049000
         LA    @09,1                                               0586 02050000
         AH    @09,NIPPMCC(,@12)                                   0586 02051000
         STH   @09,NIPPMCC(,@12)                                   0586 02052000
*               NIPPMHH=NIPPMHH&&NIPPMHH;/* HEAD 0,          @ZA07472*/ 02053000
