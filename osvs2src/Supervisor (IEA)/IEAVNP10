         TITLE 'IEAVNP10 - SYSTEM RESOURCES MANAGER INITIALIZATION     *00001000
                        '                                               00002000
IEAVNP10 CSECT ,                                                   0001 00003000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00004000
         BALR  @10,0                                               0001 00005000
@PSTART  LA    @11,4095(,@10)                                      0001 00006000
         USING @PSTART,@10                                         0001 00007000
         USING @PSTART+4095,@11                                    0001 00008000
         ST    @13,@SA00001+4                                      0001 00009000
         LA    @14,@SA00001                                        0001 00010000
         ST    @14,8(,@13)                                         0001 00011000
         LR    @13,@14                                             0001 00012000
*                                                                       00013000
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF           00014000
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES       00015000
* WILL ASSEMBLE PROPERLY.                                               00016000
NP10MSG  DS    0H                                                  0129 00017000
*   NP10NVTP=RNVT;                  /* SAVE ADDR OF NVT              */ 00018000
         ST    RNVT,NP10NVTP                                       0137 00019000
*   RMCTPTR=CVTOPCTP;               /* ADDR OF RMCT                  */ 00020000
         L     RMCTPTR,CVTOPCTP(,CVTPTR)                           0138 00021000
*   RMPTPTR=RMCTRMPT;               /* ADDR OF RMPT                  */ 00022000
         L     RMPTPTR,RMCTRMPT(,RMCTPTR)                          0139 00023000
*/*  INITIALIZE RMCT WITH THE ADDRESS OF THE SYSTEM RESOURCES MGR'S  */ 00024000
*/*  GLOBAL SAVE AREA                                                */ 00025000
*                                                                  0140 00026000
*   RMCTRMSA=WSAGOPTM;              /* ADDR OF SRM'S SAVE AREA       */ 00027000
         L     @12,CVTSPSA(,CVTPTR)                                0140 00028000
         L     @12,WSAGOPTM(,@12)                                  0140 00029000
         ST    @12,RMCTRMSA(,RMCTPTR)                              0140 00030000
*/*  UPON ENTRY IEAVNP10 OBTAINS THE CPU MODEL NUMBER AND THE        */ 00031000
*/*  CORRESPONDING ADJUSTMENT FACTOR                                 */ 00032000
*/*  SEARCH THE ARGUMENTS OF IRARMCMD FOR A CPU MODEL NUMBER         */ 00033000
*/*  IDENTICAL TO THE ONE OF THE IPLING CPU                          */ 00034000
*                                                                  0141 00035000
*   NP10CPML=CVTMDL;                /* EXTRACT CPU MODEL #   @G51CPEB*/ 00036000
         LR    @12,CVTPTR                                          0141 00037000
         AL    @12,@CF03481                                        0141 00038000
         MVC   NP10CPML(2),CVTMDL-250(@12)                         0141 00039000
*   NP10CPTR=ADDR(IRARMCMD);        /* ADDR FIRST ENTRY      @G51CPEB*/ 00040000
         L     NP10CPTR,@CA02898                                   0142 00041000
*   DO WHILE NP10CPUM^=ZERO&        /* SEARCH ENTIRE TABLE   @G51CPEB*/ 00042000
*         NP10CPUM^=NP10CPML;       /* OR UNTIL MATCH        @G51CPEB*/ 00043000
         B     @DE00143                                            0143 00044000
@DL00143 DS    0H                                                  0144 00045000
*     NP10CPTR=NP10CPTR+LENGTH(NP10CPUE);/* NEXT             @G51CPEB*/ 00046000
         AL    NP10CPTR,@CF00060                                   0144 00047000
*   END;                            /* COMPLETE SEARCH FOR CPU MODEL    00048000
*                                      NO.                   @G51CPEB*/ 00049000
@DE00143 LH    @12,NP10CPUM(,NP10CPTR)                             0145 00050000
         N     @12,@CF03482                                        0145 00051000
         LTR   @12,@12                                             0145 00052000
         BZ    @DC00143                                            0145 00053000
         CLC   NP10CPUM(2,NP10CPTR),NP10CPML                       0145 00054000
         BNE   @DL00143                                            0145 00055000
@DC00143 DS    0H                                                  0146 00056000
*/* IF FAILED TO MATCH CPU MODEL NUMBER USE DEFAULT                  */ 00057000
*                                                                  0146 00058000
*   IF NP10CPUM=ZERO THEN                                          0146 00059000
         LH    @12,NP10CPUM(,NP10CPTR)                             0146 00060000
         N     @12,@CF03482                                        0146 00061000
         LTR   @12,@12                                             0146 00062000
         BNZ   @RF00146                                            0146 00063000
*     DO;                                                          0147 00064000
*       NP10CPTR=ADDR(IRARMCMD);    /* ADDR FIRST ENTRY      @G51CPEB*/ 00065000
         L     NP10CPTR,@CA02898                                   0148 00066000
*       DO WHILE NP10CPUM^=I155;    /* DEFAULT IS 155        @G51CPEB*/ 00067000
         B     @DE00149                                            0149 00068000
@DL00149 DS    0H                                                  0150 00069000
*         NP10CPTR=NP10CPTR+LENGTH(NP10CPUE);/* NEXT         @G51CPEB*/ 00070000
         AL    NP10CPTR,@CF00060                                   0150 00071000
*       END;                        /* FOUND 155 ENTRY       @G51CPEB*/ 00072000
@DE00149 CLC   NP10CPUM(2,NP10CPTR),@CH02693                       0151 00073000
         BNE   @DL00149                                            0151 00074000
*     END;                          /* END CPUM = 0          @G51CPEB*/ 00075000
*   NP10CPMP=NP10CPUA;              /* CPU ADJUSTMENT        @G51CPEB*/ 00076000
@RF00146 L     @12,NP10CPUA(,NP10CPTR)                             0153 00077000
         ST    @12,NP10CPMP                                        0153 00078000
*/*  ADJUST EACH CPU MODEL DEPENDENT FIELDS BY THE ADJUSTMENT FACTOR */ 00079000
*/*  IN THE FOLLOWING TABLES :                                       */ 00080000
*/*  CCT - CPU CONTROL TABLE                                         */ 00081000
*/*  MCT - MAIN STORAGE CONTROL TABLE                                */ 00082000
*                                                                  0154 00083000
*NP10ADJ:                                                          0154 00084000
*   NP10ADJN=ADJSHIFT*NP10INST;     /* 16* NO. OF MICROSECS FOR    0154 00085000
*                                      10,000 INSTRUCTIONS           */ 00086000
NP10ADJ  L     @12,NP10INST                                        0154 00087000
         SLA   @12,4                                               0154 00088000
         ST    @12,NP10ADJN                                        0154 00089000
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */ 00090000
         BAL   @14,NP10ADJP                                        0155 00091000
*   RMCTADJC=NP10ADJN;                                             0156 00092000
         L     @12,NP10ADJN                                        0156 00093000
         ST    @12,RMCTADJC(,RMCTPTR)                              0156 00094000
*   NP10ADJN=CCCAPMET;                                             0157 00095000
         L     @12,CCCAPMET(,RMCTPTR)                              0157 00096000
         ST    @12,NP10ADJN                                        0157 00097000
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */ 00098000
         BAL   @14,NP10ADJP                                        0158 00099000
*   CCCAPMET=NP10ADJN;                                             0159 00100000
         L     @12,NP10ADJN                                        0159 00101000
         ST    @12,CCCAPMET(,RMCTPTR)                              0159 00102000
*   NP10ADJN=CCCAPMIN;                                             0160 00103000
         L     @12,CCCAPMIN(,RMCTPTR)                              0160 00104000
         ST    @12,NP10ADJN                                        0160 00105000
*   CALL NP10ADJP;                  /* ADJUST NO.FOR IPLING CPU      */ 00106000
         BAL   @14,NP10ADJP                                        0161 00107000
*   CCCAPMIN=NP10ADJN;                                             0162 00108000
         L     @12,NP10ADJN                                        0162 00109000
         ST    @12,CCCAPMIN(,RMCTPTR)                              0162 00110000
*   NP10ADJN=CCCAPMAX;                                             0163 00111000
         L     @12,CCCAPMAX(,RMCTPTR)                              0163 00112000
         ST    @12,NP10ADJN                                        0163 00113000
*   CALL NP10ADJP;                  /* ADJUST NO.FOR IPLING CPU      */ 00114000
         BAL   @14,NP10ADJP                                        0164 00115000
*   CCCAPMAX=NP10ADJN;                                             0165 00116000
         L     @12,NP10ADJN                                        0165 00117000
         ST    @12,CCCAPMAX(,RMCTPTR)                              0165 00118000
*   NP10ADJN=CCCAPDEL;                                             0166 00119000
         L     @12,CCCAPDEL(,RMCTPTR)                              0166 00120000
         ST    @12,NP10ADJN                                        0166 00121000
*   CALL NP10ADJP;                  /* ADJUST NO.FOR IPLING CPU      */ 00122000
         BAL   @14,NP10ADJP                                        0167 00123000
*   CCCAPDEL=NP10ADJN;                                             0168 00124000
         L     @12,NP10ADJN                                        0168 00125000
         ST    @12,CCCAPDEL(,RMCTPTR)                              0168 00126000
*   NP10ADJN=CCCMNUIN;                                             0169 00127000
         L     @12,CCCMNUIN(,RMCTPTR)                              0169 00128000
         ST    @12,NP10ADJN                                        0169 00129000
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */ 00130000
         BAL   @14,NP10ADJP                                        0170 00131000
*   CCCMNUIN=NP10ADJN;                                             0171 00132000
         L     @12,NP10ADJN                                        0171 00133000
         ST    @12,CCCMNUIN(,RMCTPTR)                              0171 00134000
*   NP10ADJN=CCCMNSIN;                                             0172 00135000
         L     @12,CCCMNSIN(,RMCTPTR)                              0172 00136000
         ST    @12,NP10ADJN                                        0172 00137000
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */ 00138000
         BAL   @14,NP10ADJP                                        0173 00139000
*   CCCMNSIN=NP10ADJN;                                             0174 00140000
         L     @12,NP10ADJN                                        0174 00141000
         ST    @12,CCCMNSIN(,RMCTPTR)                              0174 00142000
*   NP10ADJN=CCCAPDIV;                                             0175 00143000
         LH    @12,CCCAPDIV(,RMCTPTR)                              0175 00144000
         ST    @12,NP10ADJN                                        0175 00145000
*   CALL NP10ADJP;                  /* ADJUST NO.FOR IPLING CPU      */ 00146000
         BAL   @14,NP10ADJP                                        0176 00147000
*   CCCAPDIV=NP10ADJN;                                             0177 00148000
         L     @12,NP10ADJN                                        0177 00149000
         STH   @12,CCCAPDIV(,RMCTPTR)                              0177 00150000
*   NP10ADJN=CCCSIGUR;                                             0178 00151000
         LH    @12,CCCSIGUR(,RMCTPTR)                              0178 00152000
         ST    @12,NP10ADJN                                        0178 00153000
*   CALL NP10ADJP;                  /* ADJUST NO.FOR IPLING CPU      */ 00154000
         BAL   @14,NP10ADJP                                        0179 00155000
*   CCCSIGUR=NP10ADJN;                                             0180 00156000
*                                                                  0180 00157000
         L     @12,NP10ADJN                                        0180 00158000
         STH   @12,CCCSIGUR(,RMCTPTR)                              0180 00159000
*/*  FOR EACH PROCESS RATE DEPENDENT ROUTINE, ADJUST THE ROUTINE     */ 00160000
*/*  INVOCATION INTERVAL BY THE CPU ADJUSTMENT FACTOR OF THE IPLING  */ 00161000
*/*  CPU                                                             */ 00162000
*                                                                  0181 00163000
*   RESPECIFY                                                      0181 00164000
*     WKREGP RSTD;                                                 0181 00165000
*   WKREGP=RMCTEPPR;                                               0182 00166000
         L     WKREGP,RMCTEPPR(,RMCTPTR)                           0182 00167000
*   DO WHILE(EPPRPTR^=ZERO);                                       0183 00168000
         B     @DE00183                                            0183 00169000
@DL00183 DS    0H                                                  0184 00170000
*     RMEPPTR=EPPRPTR;              /* ADDR OF RMEP BLOCK FOR PROCESS   00171000
*                                      RATE DEPENDENT RTN            */ 00172000
         L     RMEPPTR,EPPRPTR(,WKREGP)                            0184 00173000
*     NP10ADJN=RMEPINT;             /* ADJUST INVOCATION INTERVAL    */ 00174000
         L     @12,RMEPINT(,RMEPPTR)                               0185 00175000
         ST    @12,NP10ADJN                                        0185 00176000
*     CALL NP10ADJP;                /* ADJUST NO. FOR IPLING CPU     */ 00177000
         BAL   @14,NP10ADJP                                        0186 00178000
*     RMEPINT=NP10ADJN;                                            0187 00179000
         L     @12,NP10ADJN                                        0187 00180000
         ST    @12,RMEPINT(,RMEPPTR)                               0187 00181000
*     WKREGP=WKREGP+LENGTH(EPPRPTR);/* POINT TO NEXT ENTRY PROCESSER    00182000
*                                      RATE DEPENDENT RTN TABLE      */ 00183000
         AL    WKREGP,@CF00054                                     0188 00184000
*   END;                                                           0189 00185000
@DE00183 SLR   @12,@12                                             0189 00186000
         C     @12,EPPRPTR(,WKREGP)                                0189 00187000
         BNE   @DL00183                                            0189 00188000
*   RESPECIFY                                                      0190 00189000
*     WKREGP UNRSTD;                                               0190 00190000
*                                                                  0190 00191000
*/*  OBTAIN THE NUMBER OF ONLINE CPUS AND INITIALIZE PROCESSOR       */ 00192000
*/*  DEPENDENT FIELDS IN I/O CONTROL TABLE                           */ 00193000
*/* IF THE NUMBER OF ONLINE CPUS IS INVALID, USE DEFAULT NO.         */ 00194000
*                                                                  0191 00195000
*   IF CSDCPUOL>ZERO&CSDCPUOL<=MAXCPUOL THEN/* NO. OF ONLINE CPUS  0191 00196000
*                                      VALID                         */ 00197000
         L     @08,CVTCSD(,CVTPTR)                                 0191 00198000
         LH    @08,CSDCPUOL(,@08)                                  0191 00199000
         CR    @08,@12                                             0191 00200000
         BNH   @RF00191                                            0191 00201000
         C     @08,@CF00074                                        0191 00202000
         BH    @RF00191                                            0191 00203000
*     CCVCPUCT=CSDCPUOL;            /* NO. OF ONLINE CPUS            */ 00204000
         STH   @08,CCVCPUCT(,RMCTPTR)                              0192 00205000
*   ELSE                            /* NO. OF ONLINE CPUS INVALID    */ 00206000
*     CCVCPUCT=DEFCPUOL;            /* USE DEFAULT VALUE             */ 00207000
         B     @RC00191                                            0193 00208000
@RF00191 LH    @12,DEFCPUOL                                        0193 00209000
         STH   @12,CCVCPUCT(,RMCTPTR)                              0193 00210000
*   ICCHIUTH=ICCINHIT(CCVCPUCT);                                   0194 00211000
@RC00191 LH    @12,CCVCPUCT(,RMCTPTR)                              0194 00212000
         ALR   @12,@12                                             0194 00213000
         LH    @08,ICCINHIT-2(@12,RMCTPTR)                         0194 00214000
         STH   @08,ICCHIUTH(,RMCTPTR)                              0194 00215000
*   ICCLOUTH=ICCINLOT(CCVCPUCT);                                   0195 00216000
         LH    @08,ICCINLOT-2(@12,RMCTPTR)                         0195 00217000
         STH   @08,ICCLOUTH(,RMCTPTR)                              0195 00218000
*   ICCDAUTH=ICCINDAT(CCVCPUCT);                                   0196 00219000
*                                                                  0196 00220000
         LH    @12,ICCINDAT-2(@12,RMCTPTR)                         0196 00221000
         STH   @12,ICCDAUTH(,RMCTPTR)                              0196 00222000
*/*  OBTAIN THE NUMBER OF LOGICAL CHANNELS                           */ 00223000
*/*                                                                  */ 00224000
*                                                                  0197 00225000
*   IOCOMPTR=CVTIXAVL;              /* ADDR OF IOCOM                 */ 00226000
         L     IOCOMPTR,CVTIXAVL(,CVTPTR)                          0197 00227000
*   IOCXPTR=IOCOMEX;                /* ADDR OF IOCX                  */ 00228000
         L     IOCXPTR,IOCOMEX(,IOCOMPTR)                          0198 00229000
*/* IF THE NUMBER OF LOGICAL CHANNELS IS INVALID, USE DEFAULT NO.    */ 00230000
*                                                                  0199 00231000
*   IF IOXLCHCT>ZERO THEN           /* # OF LOGICAL CHANNELS VALID   */ 00232000
         CLI   IOXLCHCT(IOCXPTR),0                                 0199 00233000
         BNH   @RF00199                                            0199 00234000
*     ICCLCHN=IOXLCHCT;             /* NO. OF LOGICAL CHANNELS       */ 00235000
         SLR   @12,@12                                             0200 00236000
         IC    @12,IOXLCHCT(,IOCXPTR)                              0200 00237000
         STH   @12,ICCLCHN(,RMCTPTR)                               0200 00238000
*   ELSE                            /* # OF LOGICAL CHANNELS INVALID */ 00239000
*     ICCLCHN=DEFLCHN;              /* USE DEFAULT VALUE             */ 00240000
         B     @RC00199                                            0201 00241000
@RF00199 LH    @12,DEFLCHN                                         0201 00242000
         STH   @12,ICCLCHN(,RMCTPTR)                               0201 00243000
*/*  CONSTRUCT AND INITIALIZE SYSTEM RESOURCES MANAGER LOGICAL       */ 00244000
*/*  CHANNEL TABLE (RLCT) IN SQA                                     */ 00245000
*                                                                  0202 00246000
*   RESPECIFY                                                      0202 00247000
*    (R0,                                                          0202 00248000
*     R1,                                                          0202 00249000
*     R14,                                                         0202 00250000
*     R15) RESTRICTED;                                             0202 00251000
@RC00199 DS    0H                                                  0203 00252000
*   NP10STSZ=ICCLCHN*LENGTH(RLCT);  /* NEEDED STORAGE FOR RLCT       */ 00253000
         LH    @05,ICCLCHN(,RMCTPTR)                               0203 00254000
         MH    @05,@CH03478                                        0203 00255000
         STCM  @05,7,NP10STSZ                                      0203 00256000
*   R0=NP10P245;                    /* SUBPOOL 245                   */ 00257000
         L     R0,NP10P245                                         0204 00258000
*   GEN(GETMAIN R,LV=(0));          /* GETMAIN FROM SQA              */ 00259000
         GETMAIN R,LV=(0)                                               00260000
*   RLCTPTR=R1;                     /* ADDR OF RLCT                  */ 00261000
         LR    RLCTPTR,R1                                          0206 00262000
*   RESPECIFY                                                      0207 00263000
*    (R0,                                                          0207 00264000
*     R1,                                                          0207 00265000
*     R14,                                                         0207 00266000
*     R15) UNRESTRICTED;                                           0207 00267000
*   RMCTRLCT=RLCTPTR;               /* ADDR OF FIRST RLCT ENTRY      */ 00268000
         ST    RLCTPTR,RMCTRLCT(,RMCTPTR)                          0208 00269000
*   ICCLCLST=RMCTRLCT+(ICCLCHN-ONE)*LENGTH(RLCT);/* ADDR OF LAST RLCT   00270000
*                                      ENTRY                         */ 00271000
         L     @05,RMCTRLCT(,RMCTPTR)                              0209 00272000
         LH    @15,ICCLCHN(,RMCTPTR)                               0209 00273000
         BCTR  @15,0                                               0209 00274000
         MH    @15,@CH03478                                        0209 00275000
         ALR   @05,@15                                             0209 00276000
         ST    @05,ICCLCLST(,RMCTPTR)                              0209 00277000
*/*  INITIALIZE EACH RESOURCE MANAGER LOGICAL CHANNEL BLOCK          */ 00278000
*/*  TO INDICATE THE LOGICAL CHANNEL NUMBER                          */ 00279000
*                                                                  0210 00280000
*   X=DIM(NP10LCHP);                /* NO. OF BIT PATTERNS           */ 00281000
         LA    X,32                                                0210 00282000
*   DO I=ONE TO ICCLCHN;            /* NO. OF LOGICAL CHANNELS       */ 00283000
         LA    I,1                                                 0211 00284000
         B     @DE00211                                            0211 00285000
@DL00211 DS    0H                                                  0212 00286000
*     Y=I//X;                       /* GET INDEX TO APPROPRIATE BIT     00287000
*                                      PATTERN                       */ 00288000
         LR    @00,I                                               0212 00289000
         SRDA  @00,32                                              0212 00290000
         DR    @00,X                                               0212 00291000
         LR    Y,@00                                               0212 00292000
*     IF Y=ZERO                     /* IF LOGICAL CHANNEL NO. IS A 0213 00293000
*                                      MULTIPLE OF THE NO. OF BITS 0213 00294000
*                                      USED IN THE PATTERN           */ 00295000
*       THEN                        /* LOGICAL CHANNEL NO. = NO.OF 0213 00296000
*                                      BITS USED IN THE PATTERN      */ 00297000
         LTR   Y,Y                                                 0213 00298000
         BNZ   @RF00213                                            0213 00299000
*       Y=X;                                                       0214 00300000
         LR    Y,X                                                 0214 00301000
*     NP10RLCT=''B;                 /* SET RLCT TO ZEROES            */ 00302000
@RF00213 XC    NP10RLCT(36,RLCTPTR),NP10RLCT(RLCTPTR)              0215 00303000
*     RLCTBIT=NP10LCHP(Y);          /* INITIALIZE RLCTBIT            */ 00304000
         LR    @06,Y                                               0216 00305000
         SLA   @06,2                                               0216 00306000
         LA    @01,NP10LCHP-4(@06)                                 0216 00307000
         MVC   RLCTBIT(4,RLCTPTR),0(@01)                           0216 00308000
*     RLCTPTR=RLCTPTR+LENGTH(RLCT); /* ADVANCE PTR TO RLCT           */ 00309000
         AL    RLCTPTR,@CF03478                                    0217 00310000
*   END;                            /* COMPLETED INITIALIZATION FOR     00311000
*                                      RLCT                          */ 00312000
         AL    I,@CF00062                                          0218 00313000
@DE00211 CH    I,ICCLCHN(,RMCTPTR)                                 0218 00314000
         BNH   @DL00211                                            0218 00315000
*/*  INVOKE SYSTEM PARAMETER PROCESSORS                              */ 00316000
*/*  THREE SYSTEM PARAMETERS ARE PROCESSED BY IEAVNP10 - APG,OPT,IPS */ 00317000
*/*  IF A SYSTEM PARAMETER IS SPECIFIED INCORRECTLY, THE OPERATOR    */ 00318000
*/*  IS PROMPTED TO RESPECIFY IT AND THE SAME SYSTEM PARAMETER       */ 00319000
*/*  PROCESSOR IS INVOKED TO RE-PROCESS THE PARAMETER                */ 00320000
*                                                                  0219 00321000
*   RESPECIFY                                                      0219 00322000
*     RCODE RESTRICTED;                                            0219 00323000
*   DO NP10SYCT=                    /* LOOP THRU SYSTEM PARM TABLE   */ 00324000
*         ONE TO DIM(NP10SYSP);                                    0220 00325000
         LA    NP10SYCT,1                                          0220 00326000
@DL00220 DS    0H                                                  0221 00327000
*     PROCPTR=NP10SYSP(NP10SYCT);   /* ADDR OF PROCESSOR             */ 00328000
         LR    @14,NP10SYCT                                        0221 00329000
         MH    @14,@CH00758                                        0221 00330000
         L     PROCPTR,NP10SYSP-12(@14)                            0221 00331000
*NP10SPM:                                                          0222 00332000
*     CALL NP10PROC;                /* INVOKE SYSTEM PARMAMETER    0222 00333000
*                                      PROCESSOR                     */ 00334000
NP10SPM  LR    @15,PROCPTR                                         0222 00335000
         BALR  @14,@15                                             0222 00336000
*     IF RCODE^=SUCCESS             /* TEST RETURN CODE              */ 00337000
*       THEN                        /* NEED TO PROMPT FOR NEW INPUT  */ 00338000
         LTR   RCODE,RCODE                                         0223 00339000
         BZ    @RF00223                                            0223 00340000
*       DO;                                                        0224 00341000
*         NP10PARM=NP10SYSK(NP10SYCT);/* PARM KEYWORD                */ 00342000
         LR    @14,NP10SYCT                                        0225 00343000
         MH    @14,@CH00758                                        0225 00344000
         LA    @12,NP10SYSK-12(@14)                                0225 00345000
         MVC   NP10PARM(8),0(@12)                                  0225 00346000
*         GEN REFS(NVTPRMPT,NP10PARM)                              0226 00347000
*             (IEAPMNIP TYPE=PRMPT,PARAM=NP10PARM,OPT=ERROR);      0226 00348000
         IEAPMNIP TYPE=PRMPT,PARAM=NP10PARM,OPT=ERROR                   00349000
*         GO TO NP10SPM;            /* RE-PROCESS THE SYSTEM PARM    */ 00350000
         B     NP10SPM                                             0227 00351000
*       END;                                                       0228 00352000
*   END;                            /* COMPLETED ALL SYSTEM PARM   0229 00353000
*                                      PROCESSING                    */ 00354000
@RF00223 AL    NP10SYCT,@CF00062                                   0229 00355000
         C     NP10SYCT,@CF00293                                   0229 00356000
         BNH   @DL00220                                            0229 00357000
*   RESPECIFY                                                      0230 00358000
*     RCODE UNRESTRICTED;                                          0230 00359000
*                                                                  0230 00360000
*/*  INITIALIZE AUTOMATIC PRIORITY GROUP (APG) DEPENDENT FIELDS      */ 00361000
*/*  IN CPU CONTROL TABLE (CCT)                                      */ 00362000
*                                                                  0231 00363000
*   CVTAPG=CVTAPG*SIXTEEN;                                         0231 00364000
         SLR   @12,@12                                             0231 00365000
         IC    @12,CVTAPG(,CVTPTR)                                 0231 00366000
         SLA   @12,4                                               0231 00367000
         STC   @12,CVTAPG(,CVTPTR)                                 0231 00368000
*   CCCAPGDP=CCCAPGDP+CVTAPG;                                      0232 00369000
         LH    @05,CCCAPGDP(,RMCTPTR)                              0232 00370000
         ALR   @05,@12                                             0232 00371000
         STH   @05,CCCAPGDP(,RMCTPTR)                              0232 00372000
*   CCCAPBDP=CCCAPBDP+CVTAPG;                                      0233 00373000
         LH    @15,CCCAPBDP(,RMCTPTR)                              0233 00374000
         ALR   @15,@12                                             0233 00375000
         STH   @15,CCCAPBDP(,RMCTPTR)                              0233 00376000
*   CCCAPLDP=CCCAPLDP+CVTAPG;                                      0234 00377000
         LH    @15,CCCAPLDP(,RMCTPTR)                              0234 00378000
         ALR   @15,@12                                             0234 00379000
         STH   @15,CCCAPLDP(,RMCTPTR)                              0234 00380000
*   CCCAPMDP=CCCAPMDP+CVTAPG;                                      0235 00381000
         LH    @15,CCCAPMDP(,RMCTPTR)                              0235 00382000
         ALR   @15,@12                                             0235 00383000
         STH   @15,CCCAPMDP(,RMCTPTR)                              0235 00384000
*   CCCSIGDP=CCCSIGDP+CVTAPG;                                      0236 00385000
         AH    @12,CCCSIGDP(,RMCTPTR)                              0236 00386000
         STH   @12,CCCSIGDP(,RMCTPTR)                              0236 00387000
*   CVTAPG=CCCAPGDP;                /* APG DISPATCHING PRIOTIRY      */ 00388000
         STC   @05,CVTAPG(,CVTPTR)                                 0237 00389000
*/*  INITIALIZATION FOR ADDRESS SPACES CREATED PRIOR TO IEAVNP10     */ 00390000
*/*  CHECK EACH OF THE POSSIBLE ASVTENTY ENTRIES FOR ADDRESS SPACES  */ 00391000
*/*  THAT HAVE BEEN ASSIGNED                                         */ 00392000
*                                                                  0238 00393000
*   ASVTPTR=CVTASVT;                /* ADDR OF ASVT                  */ 00394000
         L     ASVTPTR,CVTASVT(,CVTPTR)                            0238 00395000
*   RESPECIFY                                                      0239 00396000
*    (R0,                                                          0239 00397000
*     R1,                                                          0239 00398000
*     R14,                                                         0239 00399000
*     R15) RESTRICTED;                                             0239 00400000
*   ASVTEPTR=ADDR(ASVTENTY);        /* POINT TO 1ST ASVTENTY ENTRY   */ 00401000
         LA    ASVTEPTR,ASVTENTY(,ASVTPTR)                         0240 00402000
*   DO NP10ASID=ONE TO ASVTMAXU;    /* MAX NO. OF ADDR SPACES        */ 00403000
         LA    @12,1                                               0241 00404000
         B     @DE00241                                            0241 00405000
@DL00241 DS    0H                                                  0242 00406000
*     IF ASVTAVL='0'B THEN          /* ASID HAS BEEN ASSIGNED        */ 00407000
         TM    ASVTAVL(ASVTEPTR),B'10000000'                       0242 00408000
         BNZ   @RF00242                                            0242 00409000
*       DO;                                                        0243 00410000
*         ASVTE->ASCBASXB->ASXBOUSB->OUSBNAME=OUSBCHAR;/* INIT OUSB     00411000
*                                      WITH ACRONYM                  */ 00412000
         L     @12,ASVTE(,ASVTEPTR)                                0244 00413000
         L     @12,ASCBASXB(,@12)                                  0244 00414000
         L     @12,ASXBOUSB(,@12)                                  0244 00415000
         MVC   OUSBNAME(4,@12),@CC02717                            0244 00416000
*         R1=STARTMEM;              /* INDICATES MEMORY ORIGIN       */ 00417000
         LA    R1,1                                                0245 00418000
*         GEN(SYSEVENT MEMCREAT,ASIDL=NP10ASID)                    0246 00419000
*         REFS(NP10ASID);           /* ISSUE MEMCREAT SYSEVENT       */ 00420000
*                                                                  0246 00421000
         SYSEVENT MEMCREAT,ASIDL=NP10ASID                               00422000
*         /***********************************************************/ 00423000
*         /*                                                         */ 00424000
*         /* TURN ON THE TRANSWAP COMPLETE BIT FOR ANY WIRED IN      */ 00425000
*         /* ADDRESS SPACE. OTHERWISE, THE SYSEVENT TRANSWAP ISSUED  */ 00426000
*         /* BY THE INITIATOR WILL FAIL. (WIRED IN ADDRESS SPACES ARE*/ 00427000
*         /* NONSWAPPABLE AT INITATT TIME. ALL OTHER ADDRESS SPACES  */ 00428000
*         /* ARE SWAPPABLE AT THIS TIME.                     @ZA16887*/ 00429000
*         /*                                                         */ 00430000
*         /***********************************************************/ 00431000
*                                                                  0247 00432000
*         ASVTE->ASCBOUCB->OUCBTSWC='1'B;/*                  @ZA16887*/ 00433000
         L     @12,ASVTE(,ASVTEPTR)                                0247 00434000
         L     @12,ASCBOUCB(,@12)                                  0247 00435000
         OI    OUCBTSWC(@12),B'00001000'                           0247 00436000
*       END;                                                       0248 00437000
*     ASVTEPTR=ASVTEPTR+LENGTH(ASVTENTY);/* POINT TO NEXT ASVTENTY 0249 00438000
*                                      ENTRY                         */ 00439000
@RF00242 AL    ASVTEPTR,@CF00054                                   0249 00440000
*   END;                                                           0250 00441000
         LA    @12,1                                               0250 00442000
         AH    @12,NP10ASID                                        0250 00443000
@DE00241 STH   @12,NP10ASID                                        0250 00444000
         C     @12,ASVTMAXU(,ASVTPTR)                              0250 00445000
         BNH   @DL00241                                            0250 00446000
*   RESPECIFY                                                      0251 00447000
*    (R0,                                                          0251 00448000
*     R1,                                                          0251 00449000
*     R14,                                                         0251 00450000
*     R15) UNRESTRICTED;                                           0251 00451000
*                                                                  0251 00452000
*/*  COMPLETED SRM INITIALIZATION                                    */ 00453000
*/*  GET SRM LOCK                                                    */ 00454000
*                                                                  0252 00455000
*   GEN REFS(PSALITA,FLC);                                         0252 00456000
NP10GSRM SETLOCK OBTAIN,TYPE=SRM,MODE=UNCOND,REGS=USE,                XX00457000
               RELATED=(SRM,IEAVNP10(NP10RSRM))                         00458000
*   RMCTINIT='1'B;                  /* INDICATE SRM INIT COMPLETED   */ 00459000
         OI    RMCTINIT(RMCTPTR),B'00010000'                       0253 00460000
*/*  RELEASE SRM LOCK                                                */ 00461000
*                                                                  0254 00462000
*   GEN REFS(PSALITA,FLC);                                         0254 00463000
*                                                                  0254 00464000
NP10RSRM SETLOCK RELEASE,TYPE=SRM,REGS=USE,                           XX00465000
               RELATED=(SRM,IEAVNP10(NP10GSRM))                         00466000
*/*  END OF SYSTEM RESOURCES MANAGER INITIALIZATION,                 */ 00467000
*/*  RETURN TO NIP CONTROL                                           */ 00468000
*                                                                  0255 00469000
*   RETURN;                         /* RETURN TO CALLER              */ 00470000
@EL00001 L     @13,4(,@13)                                         0255 00471000
@EF00001 DS    0H                                                  0255 00472000
@ER00001 LM    @14,@12,12(@13)                                     0255 00473000
         BR    @14                                                 0255 00474000
*                                                                  0256 00475000
*/********************************************************************/ 00476000
*/*                                                                  */ 00477000
*/*  PROCEDURE NAME: NP10APGP                                        */ 00478000
*/*                                                                  */ 00479000
*/*  FUNCTION:                                                       */ 00480000
*/*  APG SYSTEM PARAMETER PROCESSOR VALIDATES THE APG SYSTEM         */ 00481000
*/*  PARAMETER AND STORES THE APG VALUE IN THE CVT                   */ 00482000
*/*                                                                  */ 00483000
*/********************************************************************/ 00484000
*                                                                  0256 00485000
*NP10APGP:                                                         0256 00486000
*   PROC;                                                          0256 00487000
NP10APGP STM   @14,@12,12(@13)                                     0256 00488000
*   DECLARE                                                        0257 00489000
*     NP10APG FIXED(15),            /* APG VALUE                     */ 00490000
*     NP10DGTC FIXED(15),           /* NO. OF DIGITS                 */ 00491000
*     NP10DGT CHAR(1),              /* APG PARM INPUT CHAR           */ 00492000
*     DEFAPG FIXED(8) CONSTANT(7)   /* DEFAULT APG VALUE=7           */ 00493000
*         ;                                                        0257 00494000
*                                                                  0257 00495000
*/*  DETERMINE IF DEFAULT SYSTEM PARAMETER VALUE IS REQUESTED        */ 00496000
*/*  THE APG ENTRY IN PARMTAB IS ZERO IF DEFAULT VALUE IS REQUESTED  */ 00497000
*/*  BY THE INSTALLATION, OTHERWISE APG POINTS TO A CHARACTER STRING */ 00498000
*/*  WHICH CONTAINS THE APG VALUE AND A QUOTE AS CLOSING DELIMITER   */ 00499000
*                                                                  0258 00500000
*   IF APG=ZERO                     /* TEST FOR DEFAULT              */ 00501000
*     THEN                          /* DEFAULT APG IS REQUESTED      */ 00502000
         SLR   @12,@12                                             0258 00503000
         L     @05,NVTPTAB(,RNVT)                                  0258 00504000
         C     @12,APG(,@05)                                       0258 00505000
         BNE   @RF00258                                            0258 00506000
*     DO;                                                          0259 00507000
*       CVTAPG=DEFAPG;              /* USE DEFAULT APG VALUE         */ 00508000
         MVI   CVTAPG(CVTPTR),X'07'                                0260 00509000
*       RETURN CODE(SUCCESS);       /* RETURN SUCCESSFUL             */ 00510000
         LR    @15,@12                                             0261 00511000
         L     @14,12(,@13)                                        0261 00512000
         LM    @00,@12,20(@13)                                     0261 00513000
         BR    @14                                                 0261 00514000
*     END;                                                         0262 00515000
*                                                                  0262 00516000
*/*  VERIFY THAT THE INPUT IS NUMERIC AND DETERMINE THE NUMBER OF    */ 00517000
*/*  DIGITS IN THE INPUT                                             */ 00518000
*                                                                  0263 00519000
*   ELSE                            /* APG PARM IS SPECIFIED         */ 00520000
*     DO;                                                          0263 00521000
@RF00258 DS    0H                                                  0264 00522000
*       NPTEPTR=ADDR(APG);          /* ADDR OF APG PARMTAB ENTRY     */ 00523000
         L     NPTEPTR,NVTPTAB(,RNVT)                              0264 00524000
         LA    NPTEPTR,APG(,NPTEPTR)                               0264 00525000
*       SCNPTR=NIPPTADR;            /* ADDR OF INPUT STRING          */ 00526000
         L     SCNPTR,NIPPTADR(,NPTEPTR)                           0265 00527000
*       IF PARMCHAR=LPAREN THEN     /* IGNORE DELIMITER              */ 00528000
         CLI   PARMCHAR(SCNPTR),C'('                               0266 00529000
         BNE   @RF00266                                            0266 00530000
*         SCNPTR=SCNPTR+ONE;        /* SKIP LEFT PARENTHESIS         */ 00531000
         AL    SCNPTR,@CF00062                                     0267 00532000
*       NP10DGTC=ZERO;              /* NO. OF DIGITS FOR APG         */ 00533000
@RF00266 SLR   NP10DGTC,NP10DGTC                                   0268 00534000
*       NP10APG=ZERO;               /* APG VALUE=0                   */ 00535000
         SLR   NP10APG,NP10APG                                     0269 00536000
*       DO I=ONE                    /* CHECK UP TO 2 DIGITS          */ 00537000
*             TO THREE WHILE(PARMCHAR^=QUOTE&PARMCHAR^=RPAREN);/*  0270 00538000
*                                      TEST FOR DELIMITERS           */ 00539000
         LA    I,1                                                 0270 00540000
@DL00270 CLI   PARMCHAR(SCNPTR),C''''                              0270 00541000
         BE    @DC00270                                            0270 00542000
         CLI   PARMCHAR(SCNPTR),C')'                               0270 00543000
         BE    @DC00270                                            0270 00544000
*         IF PARMCHAR>=CHAR0&PARMCHAR<=CHAR9/* TEST FOR NUMERIC      */ 00545000
*           THEN                    /* CHAR IS NUMERIC               */ 00546000
         CLI   PARMCHAR(SCNPTR),C'0'                               0271 00547000
         BL    @RF00271                                            0271 00548000
         CLI   PARMCHAR(SCNPTR),C'9'                               0271 00549000
         BH    @RF00271                                            0271 00550000
*           DO;                                                    0272 00551000
*             NP10DGTC=NP10DGTC+ONE;/* INCREMENT NO. OF DIGITS FOR 0273 00552000
*                                      APG                           */ 00553000
         LA    @12,1                                               0273 00554000
         ALR   NP10DGTC,@12                                        0273 00555000
*             NP10DGT=DGTMASK;      /* CONVERT INPUT TO DECIMAL      */ 00556000
         MVC   NP10DGT(1),DGTMASK                                  0274 00557000
*             NP10DGT=NP10DGT&PARMCHAR;                            0275 00558000
         NC    NP10DGT(1),PARMCHAR(SCNPTR)                         0275 00559000
*             NP10APG=NP10APG*TEN+NP10DGT;/* COMBINE SEVERAL DECIMAL    00560000
*                                      NUMBERS                       */ 00561000
         LR    @09,NP10APG                                         0276 00562000
         MH    @09,@CH02141                                        0276 00563000
         SLR   @15,@15                                             0276 00564000
         IC    @15,NP10DGT                                         0276 00565000
         ALR   @09,@15                                             0276 00566000
         LR    NP10APG,@09                                         0276 00567000
*             SCNPTR=SCNPTR+ONE;    /* INCREMENT PTR TO CHAR         */ 00568000
         ALR   SCNPTR,@12                                          0277 00569000
*           END;                                                   0278 00570000
*         ELSE                      /* CHAR IS NON-NUMERIC           */ 00571000
*           RETURN CODE(FAIL);      /* RETURN UNSUCCESSFUL           */ 00572000
         B     @RC00271                                            0279 00573000
@RF00271 LA    @15,4                                               0279 00574000
         L     @14,12(,@13)                                        0279 00575000
         LM    @00,@12,20(@13)                                     0279 00576000
         BR    @14                                                 0279 00577000
*       END;                        /* COMPLETED CHECKING INPUT      */ 00578000
@RC00271 AL    I,@CF00062                                          0280 00579000
         C     I,@CF00293                                          0280 00580000
         BNH   @DL00270                                            0280 00581000
@DC00270 DS    0H                                                  0281 00582000
*       IF NP10DGTC=ZERO            /* TEST FOR NULL STRING          */ 00583000
*           |NP10DGTC>TWO           /* TEST FOR MORE THAN THE NO. OF    00584000
*                                      DIGITS ALLOWED                */ 00585000
*           |NP10APG>APGMAX         /* TEST FOR RANGE                */ 00586000
*         THEN                      /* INVALID APG PARM VALUE        */ 00587000
         LTR   NP10DGTC,NP10DGTC                                   0281 00588000
         BZ    @RT00281                                            0281 00589000
         C     NP10DGTC,@CF00074                                   0281 00590000
         BH    @RT00281                                            0281 00591000
         C     NP10APG,@CF01393                                    0281 00592000
         BNH   @RF00281                                            0281 00593000
@RT00281 DS    0H                                                  0282 00594000
*         RETURN CODE(FAIL);        /* RETURN UNSUCCESSFUL           */ 00595000
         LA    @15,4                                               0282 00596000
         L     @14,12(,@13)                                        0282 00597000
         LM    @00,@12,20(@13)                                     0282 00598000
         BR    @14                                                 0282 00599000
*       CVTAPG=NP10APG;             /* STORE APG VALUE IN CVT        */ 00600000
@RF00281 STC   NP10APG,CVTAPG(,CVTPTR)                             0283 00601000
*       RETURN CODE(SUCCESS);       /* RETURN SUCCESSFUL             */ 00602000
         SLR   @15,@15                                             0284 00603000
         L     @14,12(,@13)                                        0284 00604000
         LM    @00,@12,20(@13)                                     0284 00605000
         BR    @14                                                 0284 00606000
*     END;                                                         0285 00607000
*   END NP10APGP;                   /* END OF APG SYSTEM PARAMETER 0286 00608000
*                                      PROCESSING                    */ 00609000
*                                                                  0287 00610000
*/********************************************************************/ 00611000
*/*                                                                  */ 00612000
*/*  PROCEDURE NAME: NP10OPTP                                        */ 00613000
*/*                                                                  */ 00614000
*/*  FUNCTION:                                                       */ 00615000
*/*  OPT SYSTEM PARAMETER PROCESSOR VALIDATES THE OPT SYSTEM         */ 00616000
*/*  PARAMETER AND INVOKES THE OPT LIST PROCESSOR (IRARMOPT)         */ 00617000
*/*  TO PROCESS THE OPT LIST                                         */ 00618000
*/*  IT WRITES AN ERROR MESSAGE TO THE OPERATOR FOR EACH CATEGORY    */ 00619000
*/*  OF OPT PARAMETERS IN THE OPT LIST IN ERROR                      */ 00620000
*/*                                                                  */ 00621000
*/********************************************************************/ 00622000
*                                                                  0287 00623000
*NP10OPTP:                                                         0287 00624000
*   PROC;                                                          0287 00625000
*                                                                  0287 00626000
NP10OPTP STM   @14,@12,@SA00003                                    0287 00627000
*/*  DETERMINE IF DEFAULT SYSTEM PARAMETER VALUE IS REQUESTED        */ 00628000
*/*  THE OPT ENTRY IN PARMTAB IS ZERO IF DEFAULT VALUE IS REQUESTED  */ 00629000
*/*  BY THE INSTALLATION, OTHERWISE OPT POINTS TO A CHARACTER STRING */ 00630000
*                                                                  0288 00631000
*   IF OPT^=ZERO                    /* CHECK IF OPT HAS BEEN SPEC    */ 00632000
*     THEN                          /* YES                           */ 00633000
         L     @12,NVTPTAB(,RNVT)                                  0288 00634000
         L     @08,OPT(,@12)                                       0288 00635000
         LTR   @08,@08                                             0288 00636000
         BZ    @RF00288                                            0288 00637000
*     DO;                                                          0289 00638000
*       NP10LIST=OPTNAME;           /* THE CHARS 'OPT'               */ 00639000
         MVC   NP10LIST(3),@CC02723                                0290 00640000
*       NPTEPTR=ADDR(OPT);          /* ADDR OF PARMTAB OPT ENTRY     */ 00641000
         LA    NPTEPTR,OPT(,@12)                                   0291 00642000
*       ELBLPTR=ADDR(NP10OPTE);     /* LABEL FOR OPT ERROR PROCESSING*/ 00643000
         LA    ELBLPTR,NP10OPTE                                    0292 00644000
*       CALL NP10VLDP;              /* INVOKE SYSTEM PARAMETER     0293 00645000
*                                      VALIDATION ROUTINE            */ 00646000
         BAL   @14,NP10VLDP                                        0293 00647000
*/*  INVOKE OPT LIST PROCESSOR TO SCAN AND PROCESS THE DATA IN       */ 00648000
*/*  THE OPT LIST                                                    */ 00649000
*                                                                  0294 00650000
*       NP10WOPE=ZERO;              /* CLEAR OPT ERROR INDICATOR     */ 00651000
         SLR   @12,@12                                             0294 00652000
         ST    @12,NP10WOPE                                        0294 00653000
*       CALL IRARMOPT(NP10WOPE,     /* OPT ERROR INDICATOR           */ 00654000
*           NP10WST);               /* STATUS INDICATOR              */ 00655000
         L     @15,@CV02795                                        0295 00656000
         LA    @01,@AL00295                                        0295 00657000
         BALR  @14,@15                                             0295 00658000
*/*  CHECK FOR I/O ERROR                                             */ 00659000
*                                                                  0296 00660000
*       IF NP10IOER='0'B            /* NO I/O ERROR FOUND            */ 00661000
*         THEN                      /* NO I/O ERROR                  */ 00662000
         TM    NP10IOER,B'10000000'                                0296 00663000
         BNZ   @RF00296                                            0296 00664000
*         DO;                                                      0297 00665000
*                                                                  0297 00666000
*/*  CHECK IF ANY ERROR HAS BEEN FOUND IN THE OPT LIST               */ 00667000
*/*  A BIT IN NP10WOPE IS SET BY THE OPT LIST PROCESSOR IF THERE IS  */ 00668000
*/*  ANY ERROR IN A CATEGORY OF OPT PARAMETERS                       */ 00669000
*/*  FOR EACH CATEGORY IN ERROR, WRITE MESSAGE IEA874I TO OPERATOR   */ 00670000
*                                                                  0298 00671000
*           IF NP10WOPE^=ZERO       /* TEST FOR ERROR                */ 00672000
*             THEN                  /* ERROR FOUND                   */ 00673000
         L     @12,NP10WOPE                                        0298 00674000
         LTR   @12,@12                                             0298 00675000
         BZ    @RF00298                                            0298 00676000
*             DO;                                                  0299 00677000
*               MSG3I2=NP10WSUF;    /* INSERT SUFFIX FOR MEMBER NAME    00678000
*                                      IN MESSAGE                    */ 00679000
         MVC   MSG3I2(2),NP10WSUF                                  0300 00680000
*               DO I=               /* CHECK ERROR INDICATOR         */ 00681000
*                     ONE TO DIM(NP10CGT);/* NO. OF CATEGORIES       */ 00682000
         LA    I,1                                                 0301 00683000
@DL00301 DS    0H                                                  0302 00684000
*                 IF(NP10WOPE&NP10MASK)=NP10MASK THEN/* ERROR IN   0302 00685000
*                                      CATEGORY                      */ 00686000
         L     @12,NP10MASK                                        0302 00687000
         L     @08,NP10WOPE                                        0302 00688000
         NR    @08,@12                                             0302 00689000
         CLR   @08,@12                                             0302 00690000
         BNE   @RF00302                                            0302 00691000
*                   DO;                                            0303 00692000
*                     MSG3I1=NP10CGID(I);/* INSERT CATEGORY ID IN MSG*/ 00693000
         LR    @12,I                                               0304 00694000
         MH    @12,@CH00293                                        0304 00695000
         LA    @08,NP10CGID-3(@12)                                 0304 00696000
         MVC   MSG3I1(3),0(@08)                                    0304 00697000
*                     GEN(IEAPMNIP TYPE=WTO,PARAM=MSG3S)           0305 00698000
*                     REFS(NVTWTO); /* WTO IEA874I MESSAGE           */ 00699000
         IEAPMNIP TYPE=WTO,PARAM=MSG3S                                  00700000
*                   END;                                           0306 00701000
*                 NP10WOPE=NP10WOPE/TWO;/* SHIFT ONE BIT TO RIGHT    */ 00702000
@RF00302 L     @12,NP10WOPE                                        0307 00703000
         SRL   @12,1                                               0307 00704000
         ST    @12,NP10WOPE                                        0307 00705000
*               END;                /* COMPLETED CHECKING FOR OPT  0308 00706000
*                                      ERROR                         */ 00707000
         AL    I,@CF00062                                          0308 00708000
         C     I,@CF00293                                          0308 00709000
         BNH   @DL00301                                            0308 00710000
*             END;                                                 0309 00711000
*         END;                                                     0310 00712000
*       ELSE                        /* I/O ERROR FOUND OR MEMBER NOT    00713000
*                                      FOUND                         */ 00714000
*         GO TO NP10OPTE;           /* ERROR                         */ 00715000
*     END;                                                         0312 00716000
*                                                                  0312 00717000
@RC00296 DS    0H                                                  0313 00718000
*/*  SCALE THE RESOURCE COEFFICIENT FACTORS                          */ 00719000
*                                                                  0313 00720000
*   RMPTCPU=RMPTCPU*RCSCALE/TEN;    /* SCALE CPU RESOURCE FACTOR   0313 00721000
*                                      COEFFICIENT                   */ 00722000
@RF00288 LA    @12,10                                              0313 00723000
         L     @01,RMPTCPU(,RMPTPTR)                               0313 00724000
         M     @00,@CF00685                                        0313 00725000
         DR    @00,@12                                             0313 00726000
         ST    @01,RMPTCPU(,RMPTPTR)                               0313 00727000
*   RMPTIOC=RMPTIOC*RCSCALE/TEN;    /* SCALE IOC RESOURCE FACTOR   0314 00728000
*                                      COEFFICIENT                   */ 00729000
         L     @01,RMPTIOC(,RMPTPTR)                               0314 00730000
         M     @00,@CF00685                                        0314 00731000
         DR    @00,@12                                             0314 00732000
         ST    @01,RMPTIOC(,RMPTPTR)                               0314 00733000
*/*  ADJUST CPU DEPENDENT FIELDS IN RMPT BY CPU ADJUSTMENT FACTOR    */ 00734000
*                                                                  0315 00735000
*   NP10ADJN=RMPTTOM;                                              0315 00736000
         L     @12,RMPTTOM(,RMPTPTR)                               0315 00737000
         ST    @12,NP10ADJN                                        0315 00738000
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */ 00739000
         BAL   @14,NP10ADJP                                        0316 00740000
*   RMPTTOM=NP10ADJN;                                              0317 00741000
         L     @12,NP10ADJN                                        0317 00742000
         ST    @12,RMPTTOM(,RMPTPTR)                               0317 00743000
*   NP10ADJN=RMPTTOL;                                              0318 00744000
         L     @12,RMPTTOL(,RMPTPTR)                               0318 00745000
         ST    @12,NP10ADJN                                        0318 00746000
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */ 00747000
         BAL   @14,NP10ADJP                                        0319 00748000
*   RMPTTOL=NP10ADJN;                                              0320 00749000
*                                                                  0320 00750000
         L     @12,NP10ADJN                                        0320 00751000
         ST    @12,RMPTTOL(,RMPTPTR)                               0320 00752000
*/*  CONVERT THE SERVICE UNITS IN THE FOLLOWING RMPT FIELDS TO THE   */ 00753000
*/*  TIME (IN 1/1024 OF A SECOND) NEEDED TO EXECUTE THESE            */ 00754000
*/*  SERVICE UNITS.                                                  */ 00755000
*                                                                  0321 00756000
*   NP10ADJN=RMPTERV*NP10INST/K;    /* CONVERT TO MILLISECS          */ 00757000
         L     @01,RMPTERV(,RMPTPTR)                               0321 00758000
         M     @00,NP10INST                                        0321 00759000
         D     @00,@CF02671                                        0321 00760000
         ST    @01,NP10ADJN                                        0321 00761000
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */ 00762000
         BAL   @14,NP10ADJP                                        0322 00763000
*   RMPTERV=NP10ADJN;                                              0323 00764000
         L     @12,NP10ADJN                                        0323 00765000
         ST    @12,RMPTERV(,RMPTPTR)                               0323 00766000
*   RETURN CODE(SUCCESS);           /* RETURN WITH SUCCESS           */ 00767000
         SLR   @15,@15                                             0324 00768000
         L     @14,@SA00003                                        0324 00769000
         LM    @00,@12,@SA00003+8                                  0324 00770000
         BR    @14                                                 0324 00771000
*NP10OPTE:                                                         0325 00772000
*   RETURN CODE(FAIL);              /* RETURN WITH FAILURE           */ 00773000
NP10OPTE LA    @15,4                                               0325 00774000
         L     @14,@SA00003                                        0325 00775000
         LM    @00,@12,@SA00003+8                                  0325 00776000
         BR    @14                                                 0325 00777000
*   END NP10OPTP;                   /* END OF OPT SYSTEM PARAMETER 0326 00778000
*                                      PROCESSING                    */ 00779000
*                                                                  0327 00780000
*/********************************************************************/ 00781000
*/*                                                                  */ 00782000
*/*  PROCEDURE NAME: NP10IPSP                                        */ 00783000
*/*                                                                  */ 00784000
*/*  FUNCTION:                                                       */ 00785000
*/*  IPS SYSTEM PARAMETER PROCESSOR VALIDATES THE IPS SYSTEM         */ 00786000
*/*  PARAMETER AND INVOKES THE IPS LIST PROCESSOR (IRARMIPS)         */ 00787000
*/*  TO PROCESS THE IPS LIST                                         */ 00788000
*/*  IF THE IPS LIST IS INVALID OR I/O ERROR HAS OCCURRED,           */ 00789000
*/*  IT RETURNS TO THE CALLER WITH RETURN CODE=4                     */ 00790000
*/********************************************************************/ 00791000
*                                                                  0327 00792000
*NP10IPSP:                                                         0327 00793000
*   PROC;                                                          0327 00794000
NP10IPSP STM   @14,@12,@SA00004                                    0327 00795000
*   NP10LIST=IPSNAME;               /* THE CHARS 'IPS'               */ 00796000
         MVC   NP10LIST(3),@CC02721                                0328 00797000
*   NPTEPTR=ADDR(IPS);              /* ADDR OF PARMLIB IPS ENTRY     */ 00798000
         L     @12,NVTPTAB(,RNVT)                                  0329 00799000
         LA    NPTEPTR,IPS(,@12)                                   0329 00800000
*/*  DETERMINE IF DEFAULT SYSTEM PARAMETER VALUE IS REQUESTED        */ 00801000
*/*  THE IPS ENTRY IN PARMTAB IS ZERO IF DEFAULT VALUE IS REQUESTED  */ 00802000
*/*  BY THE INSTALLATION, OTHERWISE IPS POINTS TO A CHARACTER STRING */ 00803000
*/*  WHICH CONTAINS THE SUFFIX FOR THE IPS LIST                      */ 00804000
*                                                                  0330 00805000
*   IF IPS=ZERO THEN                /* DEFAULT HAS BEEN REQUESTED    */ 00806000
