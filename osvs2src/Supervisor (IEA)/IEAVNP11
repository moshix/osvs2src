         TITLE 'IEAVNP11-NIP MASTER CATLG OPEN                         *00001000
                        '                                               00002000
IEAVNP11 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVNP11  75.330'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @11,0                                               0001 00011000
@PSTART  LA    @12,4095(,@11)                                      0001 00012000
         USING @PSTART,@11                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
         ST    @13,@SA00001+4                                      0001 00015000
         LA    @14,@SA00001                                        0001 00016000
         ST    @14,8(,@13)                                         0001 00017000
         LR    @13,@14                                             0001 00018000
*                                                                       00019000
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF           00020000
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES       00021000
* WILL ASSEMBLE PROPERLY.                                               00022000
         STM R2,R3,LOCPTR                                               00023000
*   SW='00000000'B;                 /* INITIALIZE SWITCHES           */ 00024000
         MVI   SW,B'00000000'                                      0174 00025000
*   COREPTR1=ZERO;                                                 0175 00026000
         SLR   @10,@10                                             0175 00027000
         ST    @10,COREPTR1                                        0175 00028000
*   WKLEN=LENGTH(CATLGBUF)*TEN+     /* GET CORE FOR THE CATALOG      */ 00029000
*       LENGTH(AMBWKBUF);           /* RECORDS AND AMBWORK AREAS     */ 00030000
         MVC   WKLEN(2),@CH02492                                   0176 00031000
*   CALL GETWORK;                   /* GET WORKING STORAGE           */ 00032000
         BAL   @14,GETWORK                                         0177 00033000
*   DATAWAPT=WKCORE;                /* SET PTR TO DATA AMBWORK       */ 00034000
         L     @10,WKCORE                                          0178 00035000
         ST    @10,DATAWAPT                                        0178 00036000
*   INDXWAPT=DATAWAPT+              /* SET PTR TO INDEX AMBWORK      */ 00037000
*       LENGTH(AMBWKBUF)/TWO;                                      0179 00038000
         LA    @07,1536                                            0179 00039000
         LR    @15,@10                                             0179 00040000
         ALR   @15,@07                                             0179 00041000
         ST    @15,INDXWAPT                                        0179 00042000
*   CATBFPTR=WKCORE+LENGTH(AMBWKBUF);                              0180 00043000
         LA    @14,3072                                            0180 00044000
         ALR   @14,@10                                             0180 00045000
         ST    @14,CATBFPTR                                        0180 00046000
*   CWAPT=DATAWAPT;                 /* ADDRESSABILITY                */ 00047000
         LR    CWAPT,@10                                           0181 00048000
*   AMALEN=LENGTH(AMBWKBUF)/TWO;    /* SET LENGTH FOR FREEMAIN ECT   */ 00049000
         STH   @07,AMALEN(,CWAPT)                                  0182 00050000
*   CWAPT=INDXWAPT;                 /* ADDRESSABILITY                */ 00051000
         LR    CWAPT,@15                                           0183 00052000
*   AMALEN=LENGTH(AMBWKBUF)/TWO;    /* SET LENGTH FOR FREEMAIN ECT   */ 00053000
         STH   @07,AMALEN(,CWAPT)                                  0184 00054000
*   DICTSTR=ADDR(ZDT);              /* INITIALIZE DICTIONARY STR ADDR*/ 00055000
         LA    @10,ZDT                                             0185 00056000
         ST    @10,DICTSTR                                         0185 00057000
*   DICTEND=ADDR(ZCS);              /* INITIALIZE DICTIONARY END ADDR*/ 00058000
         LA    @10,ZCS                                             0186 00059000
         ST    @10,DICTEND                                         0186 00060000
*   DICTTYP=ADDR(ZTYP);             /* I ADDR OF ITYPEXT DICT FLD    */ 00061000
         LA    @10,ZTYP                                            0187 00062000
         ST    @10,DICTTYP                                         0187 00063000
*   DICTTYP=DICTTYP->DICTST;        /* DISPL TO ITYPEXT FLD          */ 00064000
         SLR   @15,@15                                             0188 00065000
         IC    @15,DICTST(,@10)                                    0188 00066000
         ST    @15,DICTTYP                                         0188 00067000
*   CALL RDDIR;                     /* READ SYS1.NUCLEUS DIRECTORY   */ 00068000
         BAL   @14,RDDIR                                           0189 00069000
*   CALL RDMEMBR;                   /* READ 1 RECORD FROM THE MEMBER    00070000
*                                      SYSCATLG                      */ 00071000
         BAL   @14,RDMEMBR                                         0190 00072000
*   CALL VOLMNT;                    /* SEE TO IT THAT THE CATALOG  0191 00073000
*                                      VOLUME IS MOUNTED             */ 00074000
         BAL   @14,VOLMNT                                          0191 00075000
*   CALL BLDBLOCK;                  /* CREAT DEB, DCB, IOB, & ECB TO    00076000
*                                      READ USING EXCP               */ 00077000
         BAL   @14,BLDBLOCK                                        0192 00078000
*   CALL RDDSCB;                    /* READ F1 & F4 DSCB TO GET    0193 00079000
*                                      ADDRESS OF MASTER CATALOG     */ 00080000
         BAL   @14,RDDSCB                                          0193 00081000
*   CALL RDCATLG;                   /* READ THE 10 CATALOG SELF    0194 00082000
*                                      DEFINING RECORDS              */ 00083000
         BAL   @14,RDCATLG                                         0194 00084000
*   CALL LDAMBWRK;                  /* THANSFER DATA FROM THE CATLG     00085000
*                                      RECORDS TH THE AMBWORKS       */ 00086000
         BAL   @14,LDAMBWRK                                        0195 00087000
*   CALL BLDACB;                    /* BUILD AN ACB FOR THE CATLG    */ 00088000
         BAL   @14,BLDACB                                          0196 00089000
*   CALL BLDCAXWA;                  /* BUILD A CAXWA FOR THE CATLG   */ 00090000
         BAL   @14,BLDCAXWA                                        0197 00091000
*   CALL BLDAMCBS;                  /* BUILD AN AMCBS FOR THE CATLG  */ 00092000
         BAL   @14,BLDAMCBS                                        0198 00093000
*   CALL CALLOPEN;                  /* CALL NIP-VSAM-OPEN            */ 00094000
         BAL   @14,CALLOPEN                                        0199 00095000
*   ACBDDNM(1:4)=COREPTR1;          /* SAVE CORE MAPPING BLK PTR     */ 00096000
         L     @10,ACBPTR                                          0200 00097000
         MVC   ACBDDNM(4,@10),COREPTR1                             0200 00098000
*   FREELEN=LENGTH(CATLGBUF)*TEN+   /* FREE CATALOG IO BUFFER        */ 00099000
*       LENGTH(AMBWKBUF);           /* AND AMBWORK AREA BUFFER       */ 00100000
         MVC   FREELEN(2),@CH02492                                 0201 00101000
*   FREEAD=DATAWAPT;                                               0202 00102000
         L     @10,DATAWAPT                                        0202 00103000
         ST    @10,FREEAD                                          0202 00104000
*   CALL FREEWORK;                  /* GET RID OF THE REMAINING      */ 00105000
         BAL   @14,FREEWORK                                        0203 00106000
*   CALL FREEMAIN;                  /* CORE HELD FOR CONTROL BLOCKS  */ 00107000
*                                                                  0204 00108000
         BAL   @14,FREEMAIN                                        0204 00109000
*   /*****************************************************************/ 00110000
*   /*                                                               */ 00111000
*   /* E X I T                                                       */ 00112000
*   /*                                                               */ 00113000
*   /*****************************************************************/ 00114000
*                                                                  0205 00115000
*CALLOPEN:                                                         0205 00116000
*                                                                  0205 00117000
*   /*****************************************************************/ 00118000
*   /*                                                               */ 00119000
*   /* CALL THE NIP-VSAM-OPEN MODULE                                 */ 00120000
*   /*                                                               */ 00121000
*   /*****************************************************************/ 00122000
*                                                                  0205 00123000
*   PROC;                                                          0205 00124000
@EL00001 L     @13,4(,@13)                                         0205 00125000
@EF00001 DS    0H                                                  0205 00126000
@ER00001 LM    @14,@12,12(@13)                                     0205 00127000
         BR    @14                                                 0205 00128000
CALLOPEN STM   @14,@12,@SA00002                                    0205 00129000
*   PARMLIST(1)=ADDR(IFGACB);       /* SET PARAMET LIST              */ 00130000
         L     @10,ACBPTR                                          0206 00131000
         ST    @10,PARMLIST                                        0206 00132000
*   PARMLIST(2)=DATAWAPT;           /* DATA WORK AREA                */ 00133000
         L     @10,DATAWAPT                                        0207 00134000
         ST    @10,PARMLIST+4                                      0207 00135000
*   PARMLIST(THREE)=INDXWAPT;       /* INDEX WORK AREA               */ 00136000
         L     @10,INDXWAPT                                        0208 00137000
         ST    @10,PARMLIST+8                                      0208 00138000
*   RFY                                                            0209 00139000
*    (R0,                                                          0209 00140000
*     R1,                                                          0209 00141000
*     R15) RSTD;                                                   0209 00142000
*   FF=ONE;                         /* NBR OF FIELDS FOR BLDL        */ 00143000
         MVC   FF(2),@CH00079                                      0210 00144000
*   LL=H58;                         /* WORK AREA LENGTH FOR BLDL     */ 00145000
         MVC   LL(2),@CH02332                                      0211 00146000
*   BLDLNAME='IEAVNP1A';            /* FIND NIP VSAM OPEN            */ 00147000
         MVC   BLDLNAME(8),@CC02388                                0212 00148000
*   R1=NVTDCBSN;                                                   0213 00149000
         L     @14,NVTPTR                                          0213 00150000
         L     R1,NVTDCBSN(,@14)                                   0213 00151000
*   GEN REFS(PARMLIST) SETS(R14,R15,TEMPSR);                       0214 00152000
        STM   R2,R3,TEMPSR           SAVE REGS                          00153000
        LM    R2,R3,LOCPTR           BASE REGS FOR NVT/CVT              00154000
        BLDL  (R1),BLDLTBL                                              00155000
        LM    R2,R3,LOCPTR           BASE REGS FOR NVT/CVT              00156000
        L     R1,NVTDCBSN(,R2)                                          00157000
        LOAD  DE=BLDLNAME,DCB=(R1)   LOAD NIP VSAM OPEN                 00158000
        LA    R1,PARMLIST            PTR TO PARMLIST                    00159000
        LTR    R15,R0                TEST RETURN CODE                   00160000
        BZ    NOLOAD                 LOAD OK, IF NO BRANCH              00161000
        BALR  R14,R15                GO TO NIP VSAM OPEN                00162000
        DELETE DE=BLDLNAME           DELETE NIP VSAM OPEN               00163000
NOLOAD  LM    R2,R3,TEMPSR           BR HERE IF LOAD FAILED             00164000
*   IF R15^=ZERO THEN               /* IF OPEN FAILED                */ 00165000
         LTR   R15,R15                                             0215 00166000
         BZ    @RF00215                                            0215 00167000
*     DO;                                                          0216 00168000
*       NVTFLWSC=NVTWCN11;          /* QUIT                          */ 00169000
         L     @14,NVTPTR                                          0217 00170000
         MVI   NVTFLWSC(@14),X'64'                                 0217 00171000
*       CALL SWAIT;                 /* GO TO SYSTEM WAIT ROUTINE     */ 00172000
         BAL   @14,SWAIT                                           0218 00173000
*     END;                                                         0219 00174000
*   CORELAST=R1;                    /* SAVE PTR TO CORE MAP BLOCK    */ 00175000
@RF00215 L     @14,COREPTRN                                        0220 00176000
         ST    R1,CORELAST(,@14)                                   0220 00177000
*   RFY                                                            0221 00178000
*    (R0,                                                          0221 00179000
*     R1,                                                          0221 00180000
*     R15) UNRSTD;                                                 0221 00181000
*   END CALLOPEN;                                                  0222 00182000
@EL00002 DS    0H                                                  0222 00183000
@EF00002 DS    0H                                                  0222 00184000
@ER00002 LM    @14,@12,@SA00002                                    0222 00185000
         BR    @14                                                 0222 00186000
*RDDIR:                                                            0223 00187000
*                                                                  0223 00188000
*   /*****************************************************************/ 00189000
*   /*                                                               */ 00190000
*   /* READ THE DIRECTORY OF SYS1.NUCLEUS & CONVERT TTR TO MMBBCCHHR */ 00191000
*   /*                                                               */ 00192000
*   /*****************************************************************/ 00193000
*                                                                  0223 00194000
*   PROC;                                                          0223 00195000
RDDIR    STM   @14,@12,@SA00003                                    0223 00196000
*   FF=ONE;                         /* NBR OF ITEMS IN BLDLLIST      */ 00197000
         MVC   FF(2),@CH00079                                      0224 00198000
*   LL=LENGTH(BLDLTBL)-FOUR;                                       0225 00199000
         MVC   LL(2),@CH02494                                      0225 00200000
*   BLDLNAME='SYSCATLG';            /* NAME TO SEARCH ON             */ 00201000
         MVC   BLDLNAME(8),@CC02393                                0226 00202000
*   RFY                                                            0227 00203000
*    (R0,                                                          0227 00204000
*     R1,                                                          0227 00205000
*     R15) RSTD;                                                   0227 00206000
*   R1=NVTDCBSN;                    /* ADDR OF DCB FOR SYS1.NUCLEUS  */ 00207000
         L     @14,NVTPTR                                          0228 00208000
         L     R1,NVTDCBSN(,@14)                                   0228 00209000
*   GEN REFS(R1) SETS(R0,R1,R15,TTRK);                             0229 00210000
        STM   R2,R3,TEMPSR                                              00211000
        LM    R2,R3,LOCPTR                                              00212000
        BLDL  (R1),BLDLTBL                                              00213000
        LM    R2,R3,TEMPSR                                              00214000
*   IF R15^=ZERO THEN               /* SUCESSFUL?                    */ 00215000
         LTR   R15,R15                                             0230 00216000
         BZ    @RF00230                                            0230 00217000
*     DO;                           /* NO, DO                        */ 00218000
*       NVTFLWSC=NVTWCN07;          /* SET WAIT CODE                 */ 00219000
         L     @14,NVTPTR                                          0232 00220000
         MVI   NVTFLWSC(@14),X'5C'                                 0232 00221000
*       CALL SWAIT;                 /* GO TO SYSTEM WAIT ROUTINE     */ 00222000
         BAL   @14,SWAIT                                           0233 00223000
*     END;                                                         0234 00224000
*   RFY                                                            0235 00225000
*     IHADCB BASED(NVTDCBSN);                                      0235 00226000
@RF00230 DS    0H                                                  0236 00227000
*   R1=DCBDEBAD;                    /* ADDR OF DEB OF SYS1.NUCLEUS TO   00228000
*                                      R1                            */ 00229000
         L     @14,NVTPTR                                          0236 00230000
         L     @14,NVTDCBSN(,@14)                                  0236 00231000
         L     R1,DCBDEBAD+40(,@14)                                0236 00232000
*   R0=TTRK;                        /* TTR TO BE CONVERTED           */ 00233000
         L     R0,TTRK                                             0237 00234000
*   R15=CVTPCNVT;                   /* PT TO CONVERT ROUTINE         */ 00235000
         L     @10,CVTPTR                                          0238 00236000
         L     R15,CVTPCNVT(,@10)                                  0238 00237000
*   R14=ADDR(DRWSEEK);              /* ADDRESS OF OUTPUT AREA        */ 00238000
         LA    R14,DRWAAREA                                        0239 00239000
         LA    R14,DRWSEEK(,R14)                                   0239 00240000
*   GEN SETS(DRWSEEK);                                             0240 00241000
        STM   R0,R15,TEMPSR            SAVE REGS                        00242000
        LA    R8,TEMPSR                                                 00243000
        LR    R2,R14                         POINTER TO OUTPUT AREA     00244000
        BALR  R14,R15                  GOTO TTR CONVERT RTN             00245000
        LM    R0,R15,0(R8)             RECOVER REGS                     00246000
*   RFY                                                            0241 00247000
*    (R0,                                                          0241 00248000
*     R1,                                                          0241 00249000
*     R15) UNRSTD;                                                 0241 00250000
*   END RDDIR;                                                     0242 00251000
@EL00003 DS    0H                                                  0242 00252000
@EF00003 DS    0H                                                  0242 00253000
@ER00003 LM    @14,@12,@SA00003                                    0242 00254000
         BR    @14                                                 0242 00255000
*RDMEMBR:                                                          0243 00256000
*                                                                  0243 00257000
*   /*****************************************************************/ 00258000
*   /*                                                               */ 00259000
*   /* READ THE MEMBER SYSCATLG FROM SYS1.NUCLEUS                    */ 00260000
*   /*                                                               */ 00261000
*   /*****************************************************************/ 00262000
*                                                                  0243 00263000
*   PROC;                                                          0243 00264000
*                                                                  0243 00265000
RDMEMBR  STM   @14,@12,@SA00004                                    0243 00266000
*/* BUILD DEB                                                        */ 00267000
*                                                                  0244 00268000
*   DRWDEXT='01'X;                  /* NUMBER OF EXTENTS = 1         */ 00269000
         LA    @10,DRWAAREA                                        0244 00270000
         MVI   DRWDEXT(@10),X'01'                                  0244 00271000
*   DRWDDCB=ADDR(DRWDCB);           /* DCB PTR                       */ 00272000
         LA    @15,DRWDCB(,@10)                                    0245 00273000
         STCM  @15,7,DRWDDCB(@10)                                  0245 00274000
*   DRWDID='0F'X;                   /* SET DEB ID                    */ 00275000
         MVI   DRWDID(@10),X'0F'                                   0246 00276000
*   DRWDAPP=CVTXAPG;                /* IOS APP.TAB ADDR              */ 00277000
         L     @01,CVTPTR                                          0247 00278000
         MVC   DRWDAPP(3,@10),CVTXAPG+1(@01)                       0247 00279000
*   DRWDEXS='04'X;                  /* EXT SCALE                     */ 00280000
         MVI   DRWDEXS(@10),X'04'                                  0248 00281000
*   DRWDMOD='00'X;                  /* SET FILE MASK                 */ 00282000
         MVI   DRWDMOD(@10),X'00'                                  0249 00283000
*   DRWECC='FFFFFFFF'X;             /* SET TO END OF PACK            */ 00284000
         MVC   DRWECC(4,@10),@CB02401                              0250 00285000
*   DRWNTK='7FFF'X;                 /* NBR OF TRKS                   */ 00286000
         MVC   DRWNTK(2,@10),@CB02402                              0251 00287000
*   DRWDEBP=ADDR(DRWDEB);           /* DEBDEB PTR                    */ 00288000
         ST    @10,DRWDEBP(,@10)                                   0252 00289000
*   DRWDUCB=DCBDEBA->DRWDUCB;       /* UCB POINTER IN DEB            */ 00290000
         L     @01,NVTPTR                                          0253 00291000
         L     @01,NVTDCBSN(,@01)                                  0253 00292000
         L     @01,DCBDEBA+39(,@01)                                0253 00293000
         LA    @01,0(,@01)                                         0253 00294000
         MVC   DRWDUCB(3,@10),DRWDUCB(@01)                         0253 00295000
*/* BUILD AN IOB                                                     */ 00296000
*                                                                  0254 00297000
*   DRWIECB=ADDR(DRWECB);           /* ECB PTR INTO IOB              */ 00298000
         LA    @15,DRWECB(,@10)                                    0254 00299000
         ST    @15,DRWIECB(,@10)                                   0254 00300000
*   DRWSCP=ADDR(DRWCCW1);           /* START OF CCW'S                */ 00301000
         LA    @15,DRWCCW1(,@10)                                   0255 00302000
         ST    @15,DRWSCP(,@10)                                    0255 00303000
*   DRWIDCB=ADDR(DRWDCB);           /* DCB PTR INTO IOB              */ 00304000
         LA    @14,DRWDCB(,@10)                                    0256 00305000
         ST    @14,DRWIDCB(,@10)                                   0256 00306000
*   DRWIOB(1:2)='0200'X;            /* SET IOB UNRELATED FLAG CLEAR     00307000
*                                      OTHER FLAGS                   */ 00308000
         MVC   DRWIOB(2,@10),@CB02403                              0257 00309000
*   DRWECB=ZERO;                    /* CLEAR ECB                     */ 00310000
         SLR   @14,@14                                             0258 00311000
         ST    @14,DRWECB(,@10)                                    0258 00312000
*/* BUILD CHANNEL PROGRAM TO READ MEMBER                             */ 00313000
*                                                                  0259 00314000
*   RFY                                                            0259 00315000
*     R1 RSTD;                                                     0259 00316000
*   RFY                                                            0260 00317000
*     WCCW BASED(R1);                                              0260 00318000
*   R1=ADDR(DRWCCW1);               /* PT TO CHAN PGM AREA           */ 00319000
         LR    R1,@15                                              0261 00320000
*   CCWCMD='31'X;                   /* SEARCH ID EQUAL COMMAND       */ 00321000
         MVI   CCWCMD(R1),X'31'                                    0262 00322000
*   CCWDTA=ADDR(DRWSEEK)+THREE;     /* ARGUMENT (STARTING PLACE      */ 00323000
         LA    @00,DRWSEEK(,@10)                                   0263 00324000
         AL    @00,@CF00295                                        0263 00325000
         STCM  @00,7,CCWDTA(R1)                                    0263 00326000
*   CCWFLG='40'X;                   /* COMMAND CHAINING              */ 00327000
         MVI   CCWFLG(R1),X'40'                                    0264 00328000
*   CCWBTE=FIVE;                    /* BYTE COUNT                    */ 00329000
         MVC   CCWBTE(2,R1),@CH00936                               0265 00330000
*   R1=R1+EIGHT;                    /* NEXT CCW                      */ 00331000
         LA    @14,8                                               0266 00332000
         ALR   R1,@14                                              0266 00333000
*   CCWCMD='08'X;                   /* TIC                           */ 00334000
         MVI   CCWCMD(R1),X'08'                                    0267 00335000
*   CCWDTA=ADDR(DRWCCW1);           /* TIC BACK TO CCW1              */ 00336000
         STCM  @15,7,CCWDTA(R1)                                    0268 00337000
*   R1=R1+EIGHT;                                                   0269 00338000
         ALR   R1,@14                                              0269 00339000
*   CCWCMD='06'X;                   /* READ DATA                     */ 00340000
         MVI   CCWCMD(R1),X'06'                                    0270 00341000
*   CCWDTA=ADDR(DRWDSCB);           /* BUFFER AREA ADDR              */ 00342000
         LA    @00,DRWDSCB(,@10)                                   0271 00343000
         STCM  @00,7,CCWDTA(R1)                                    0271 00344000
*   CCWBTE=H80;                     /* BYTE COUNT                    */ 00345000
         MVC   CCWBTE(2,R1),@CH02334                               0272 00346000
*   R1=ADDR(DRWIOB);                /* POINT TO IOB                  */ 00347000
         LA    R1,DRWIOB(,@10)                                     0273 00348000
*   GEN REFS(DRWIOB,R1) SETS(SYSCATLG);                            0274 00349000
        EXCP (1)                          EXECUTE CHANNEL PROGTAM       00350000
*   R1=ADDR(DRWECB);                                               0275 00351000
         LA    R1,DRWAAREA                                         0275 00352000
         LA    R1,DRWECB(,R1)                                      0275 00353000
*   GEN SETS(DRWECB);                                              0276 00354000
        WAIT ECB=(1)                      WAIT FOR CHANNEL PROG         00355000
*   IF DRWECMP^='7F'X THEN          /* CHECK FOR ERROR               */ 00356000
         LA    @10,DRWAAREA                                        0277 00357000
         CLI   DRWECMP(@10),127                                    0277 00358000
         BE    @RF00277                                            0277 00359000
*     DO;                           /* QUIT                          */ 00360000
*       R1=ADDR(DRWIOB);            /* POINT TO IOB                  */ 00361000
         LA    R1,DRWIOB(,@10)                                     0279 00362000
*       GEN(LM 2,3,LOCPTR);         /* CVT AND NVT POINTERS          */ 00363000
         LM 2,3,LOCPTR                                                  00364000
*       GEN(IEAPMNIP TYPE=SENSE,PARAM=R1);/* TYPE SENSE DATE         */ 00365000
         IEAPMNIP TYPE=SENSE,PARAM=R1                                   00366000
*       NVTFLWSC=NVTWCN07;          /* SET WAIT CODE                 */ 00367000
         L     @10,NVTPTR                                          0282 00368000
         MVI   NVTFLWSC(@10),X'5C'                                 0282 00369000
*       CALL SWAIT;                 /* GO TO SYSTEM WAIT ROUTINE     */ 00370000
         BAL   @14,SWAIT                                           0283 00371000
*     END;                                                         0284 00372000
*   RFY                                                            0285 00373000
*     R1 UNRSTD;                                                   0285 00374000
*   END RDMEMBR;                                                   0286 00375000
@EL00004 DS    0H                                                  0286 00376000
@EF00004 DS    0H                                                  0286 00377000
@ER00004 LM    @14,@12,@SA00004                                    0286 00378000
         BR    @14                                                 0286 00379000
*VOLMNT:                                                           0287 00380000
*                                                                  0287 00381000
*   /*****************************************************************/ 00382000
*   /*                                                               */ 00383000
*   /* VERIFY THAT THE VOLUME IS MOUNTED AND GET UCB POINTER         */ 00384000
*   /*                                                               */ 00385000
*   /*****************************************************************/ 00386000
*                                                                  0287 00387000
*   PROC;                                                          0287 00388000
VOLMNT   STM   @14,@05,@SA00005                                    0287 00389000
         STM   @07,@12,@SA00005+32                                 0287 00390000
*   CWAPT=DATAWAPT;                 /* ADDRESSABILTIY FOR AMBWORK    */ 00391000
         L     CWAPT,DATAWAPT                                      0288 00392000
*   RFY                                                            0289 00393000
*     NIPMNTPL BASED(ADDR(MNTPARM));/* ADDRESSING FOR PARM LIST      */ 00394000
