         L     @09,BUFCBAD(,BUFCPTR)                               0681 01601000
         ST    @09,IX4                                             0681 01602000
*   IX5=ADDR(IDAIXSPL);             /* ADDRESS OF WORK AREA          */ 01603000
         LA    @09,IXSPAREA                                        0682 01604000
         ST    @09,IX5                                             0682 01605000
*   IX6=CTGENT;                     /* POINTER TO THE DSNAME         */ 01606000
         L     @09,CTGPLPTR                                        0683 01607000
         L     @09,CTGENT(,@09)                                    0683 01608000
         ST    @09,IX6                                             0683 01609000
*   GEN REFS(IBFLPF,IBFLPL,IXHLEO,IXHLL,IXHSEO,IXSEKON,IXSHEP,IXSLEP,   01610000
*       IXSPEC,IXSSEP,IXHFLPLN,IX4,IX5,IX6);                       0684 01611000
        L     RBUFC,IX4                                                 01612000
        L     RWORK3,IX5                                                01613000
        L     RPARM1,IX6                                                01614000
        SR    RWORK1,RWORK1            CLEAR REGISTER                   01615000
        STC   RWORK1,IXSPEC(RWORK3)    CLEAR PREVIOUS COUNTER           01616000
        SR    RWORK0,RWORK0            CLEAR REGISTER                   01617000
        IC    RWORK0,IXHFLPLN(RBUFC)   PICK UP F+L+POINTER LENGTH       01618000
        STC   RWORK0,IXSEKON(RWORK3)   SAVE IN WORK AREA                01619000
        OI    IXSBFLG(RWORK3),X80      SET FLAG SHOWING SECTION SEARCH  01620000
        ICM   RWORK1,K3,IXHLL(RBUFC)   GET RECORD LENGTH FROM HEADER    01621000
        AR    RWORK1,RBUFC             ADD TO BEGINNING ADD. OF BUFFER  01622000
        SR    RWORK1,RWORK0            BACK UP OVER F,L, POINTER        01623000
        ST    RWORK1,IXSLEP(RWORK3)    SAVE LOW KEY ADDRESS             01624000
        SR    RWORK1,RWORK1            CLEAR REG                        01625000
        ICM   RWORK1,K3,IXHSEO(RBUFC)  GET OFFSET TO 1ST SECTION        01626000
        AR    RWORK1,RBUFC             ADD BEGINNING ADD TO FORM TRUE   01627000
        SR    R15,R15                   ADDRESS                         01628000
COMFN1  SR    RWORK2,RWORK2            CLEAR REGISTERS                  01629000
COMFN2  SR    RWORK4,RWORK4             ...                             01630000
        SR    RWORK5,RWORK5             ...                             01631000
COMFN3  ST    RWORK1,IXSHEP(RWORK3)    SAVE STARTING POINT              01632000
        IC    RWORK2,IBFLPF(RWORK1)    PICK UP F FIELD                  01633000
        CR    RWORK2,R15               COMPARE IT TO NO. HITS           01634000
        BNE   SCIB2                    MISMATCH                         01635000
        TM    IBFLPL(RWORK1),X255     IS THE L FIELD ZERO               01636000
        BZ    SCIB9                    YES, DISCONTINUE THIS SEARCH     01637000
        IC    RWORK5,IBFLPL(RWORK1)    GET LENGTH OF KEY                01638000
        SR    RWORK1,RWORK5            MOVE POINTER TO BEGINNING OF     01639000
COMAI   IC    RWORK4,DZ(RPARM1,R15)     KEY--PICK UP ARG. CHARACTER     01640000
        IC    RWORK2,DZ(RWORK1)        PICK UP INDEX CHARACTER          01641000
        CR    RWORK4,RWORK2            ARE THEY EQUAL                   01642000
        BNE   SCIB1                    NO                               01643000
        LA    R15,ONE(R15)               YES, BUMP UP NO. HITS          01644000
        LA    RWORK1,ONE(RWORK1)       MOVE POINTER TO NEXT CHARACTER   01645000
        BCT   RWORK5,COMAI             BACK TO COMPARE IF ANY ARE LEFT  01646000
SCIB9   TM    IXSBFLG(RWORK3),X80      IS THIS A SECTION SEARCH         01647000
        BNZ   SCIB10                   YES                              01648000
        SR    R15,R15                  CLEAR REG  INDICATING SUCCESS    01649000
*SCIB91:                                                           0685 01650000
*   RETURN CODE(R15);               /* RETURN TO CALLER              */ 01651000
SCIB91   L     @14,@SA00017                                        0685 01652000
         LM    @00,@12,@SA00017+8                                  0685 01653000
         BR    @14                                                 0685 01654000
*   GEN;                                                           0686 01655000
SCIB1   BH    SCIB3                    IS THE ARG GREATER--YES          01656000
        B     SCIB9                    NO, SEARCH COMPLETE              01657000
SCIB2   BL    SCIB9                    IF F LESS THAN N GO              01658000
SCIB3   TM    IXSBFLG(RWORK3),X80     F HIGHER, IS THIS SECT. SEARCH    01659000
        BZ    SCIB5                    NO, INNER SECT, SEARCH           01660000
*                                                                       01661000
*   THE FOLLOWING CODE IS REQUIRED TO TEST FOR END OF BUFFER, THAT IS,  01662000
*   NO MORE SECTIONS FOLLOW TO BE SEARCHED.  IF NOT END OF SECTION      01663000
*   POINTERS MUST BE UPDATED TO NEXT SECTION.  ALSO THE LOW KEY ADD.    01664000
*   OF THE NEXT SECTION MUST BE SAVED. THE NO OF CHARACTERS WHICH HAVE  01665000
*   COMPARED EQUAL (HITS) IS ALSO SAVED FOR LATER USE IN SEARCHING      01666000
*   WITHIN THE SECTION.                                                 01667000
*                                                                       01668000
        ICM   RWORK2,K3,IXHLEO(RBUFC)  GET DISP. OF HIGH SECT.          01669000
        AR    RWORK2,RBUFC             MAKE THAT A POINTER              01670000
        CR    RWORK2,RWORK1            HAVE WE MOVED BEYOND END         01671000
        BNL   SCIB7                    YES, NO MATCH IN THIS BUFFER     01672000
        L     RWORK1,IXSHEP(RWORK3)    POINT BACK TO OLD BASE           01673000
        LR    RWORK5,RWORK1            GET SAME ADDRESS IN RW5          01674000
        IC    RWORK4,IBFLPL(RWORK1)    GET L FROM INDEX                 01675000
        STC   R15,IXSPEC(RWORK3)       SAVE FOR PREVIOUS ENTRY COUNT    01676000
        SR    RWORK5,RWORK4            MOVE BACK TO KEY AREA            01677000
        SH    RWORK5,HLF2              MOVE TO POINTER                  01678000
        ICM   RWORK4,K3,DZ(RWORK5)     PICK UP DISP. TO NEXT SECTION    01679000
        SR    RWORK5,RWORK0            MOVE ADDRESS PAST POINT, L,F     01680000
        ST    RWORK5,IXSLEP(RWORK3)    SAVE LOW ENTRY OF SECT IN WORK   01681000
        SR    RWORK1,RWORK4            UPDATE TO NEXT SECTION           01682000
        B     COMFN1                   BACK TO CONTINUE SEARCH          01683000
*                                                                       01684000
*   THE FOLLOWING CODE UPDATES POINTERS TO CONTINUE SEARCH              01685000
*   WITHIN A SECTION.  WE CANNOT EXIT WITHOUT LOCATING AN ENTRY         01686000
*   SINCE A SECTION SEARCH CAUSED US TO DO THE INNER SECTION SEARCH     01687000
*                                                                       01688000
SCIB5   L     RWORK1,IXSHEP(RWORK3)    START WITH ORIGINAL POINTER      01689000
        IC    RWORK4,IBFLPL(RWORK1)    GET L VALUE                      01690000
        SR    RWORK1,RWORK4            BACK OVER KEY                    01691000
        SR    RWORK1,RWORK0            BACK OVER POINTER,L,F            01692000
        STC   R15,IXSPEC(RWORK3)       SAVE NO. MATCHES FOR PREV. CNT.  01693000
        B     COMFN2                   BACK FOR SEARCH                  01694000
*                                                                       01695000
*  BLOCK HAS BEEN SEARCHED AND NO SECTION ENTRY WAS GREATER THAN OR     01696000
*  EQUAL TO THE KEY -  EXIT TO CALLER WITH R15 = 4                      01697000
*                                                                       01698000
SCIB7   LH    R15,HLF4                                                  01699000
        B     SCIB91                   EXIT                             01700000
*                                                                       01701000
*   THE FOLLOWING CODE IS ENTERED WHEN A SECTION SEARCH HAS BEEN        01702000
*   TERMINATED BY A SATISFIED SEARCH.  AT THIS POINT IT IS NECESSARY    01703000
*   TO INITIALIZE FOR AN INNER SECTION SEARCH                           01704000
*                                                                       01705000
SCIB10  IC    R15,IXSPEC(RWORK3)       RESTORE NO. MATCHES TO WHAT IT   01706000
*                                       WAS AT BEGINNING OF SECTION     01707000
        L     RWORK4,IXSHEP(RWORK3)    GET AND STORE                    01708000
        ST    RWORK4,IXSSEP(RWORK3)     ADDRESS OF SECTION ENTRY        01709000
*                                        IN WORK AREA                   01710000
        L     RWORK1,IXSLEP(RWORK3)    PICK UP ADDRESS OF 1ST. ENTRY    01711000
*                                       IN THIS SECTION                 01712000
        XI    IXSBFLG(RWORK3),X80      TURN OFF SECTION SEARCH          01713000
        B     COMFN2                    LOOP BACK                       01714000
        DS    0H                       ASSURE ALIGNMENT                 01715000
HLF2    DC    H'2'                                                      01716000
HLF4    DC    H'4'                                                      01717000
DZ      EQU   0                        DEFINE ZERO DISPLACEMENT         01718000
RWORK0  EQU   0                                                         01719000
RWORK1  EQU   5                                                         01720000
RWORK2  EQU   6                                                         01721000
RWORK3  EQU   7                                                         01722000
RWORK4  EQU   3                                                         01723000
RWORK5  EQU   9                                                         01724000
RBUFC   EQU   4                                                         01725000
RPARM1  EQU   8                                                         01726000
X80     EQU  X'80'                                                      01727000
X255    EQU  X'FF'                                                      01728000
ONE     EQU  1                                                          01729000
K3      EQU  3                                                          01730000
LNTHINDX EQU  512                                                       01731000
*   END SCIB;                                                      0687 01732000
@EL00017 DS    0H                                                  0687 01733000
@EF00017 DS    0H                                                  0687 01734000
