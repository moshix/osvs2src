         TITLE 'IEAVNP12 - NIP LOCATE                                  *00001000
                        '                                               00002000
IEAVNP12 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVNP12  77.018'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @11,8(,@13)                                         0001 00020000
         LR    @13,@11                                             0001 00021000
*                                                                       00022000
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF           00023000
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES       00024000
* WILL ASSEMBLE PROPERLY.                                               00025000
         STM R2,R3,LOCPTR                                               00026000
*   ERRCD=ZERO;                                                    0238 00027000
         SLR   @15,@15                                             0238 00028000
         ST    @15,ERRCD                                           0238 00029000
*   SW='00000000'B;                 /* SET ALL FLAGS OFF             */ 00030000
         MVI   SW,B'00000000'                                      0239 00031000
*   DICTSTR=ADDR(ZDT);              /* DICTIONARY STARTING ADD       */ 00032000
         LA    @15,ZDT                                             0240 00033000
         ST    @15,DICTSTR                                         0240 00034000
*   DICTEND=ADDR(ZCS);                                             0241 00035000
         LA    @15,ZCS                                             0241 00036000
         ST    @15,DICTEND                                         0241 00037000
*   DICTTYP=ADDR(ZTYP);             /* ITYPENT FLD OF DICT           */ 00038000
         LA    @15,ZTYP                                            0242 00039000
         ST    @15,DICTTYP                                         0242 00040000
*   DICTTYP=DICTTYP->DICTST;        /* DISPLAC IN CATALOG            */ 00041000
         SLR   @14,@14                                             0243 00042000
         IC    @14,DICTST(,@15)                                    0243 00043000
         ST    @14,DICTTYP                                         0243 00044000
*   WKLEN=LENGTH(CATLGBUF)*TEN;     /* GET CORE TO HOLD CATALOG      */ 00045000
         MVC   WKLEN(2),@CH02590                                   0244 00046000
*   CALL GETWORK;                   /* RECORD READ                   */ 00047000
         BAL   @14,GETWORK                                         0245 00048000
*   CATBFPTR=WKCORE;                /* SAVE POINTER                  */ 00049000
         L     @15,WKCORE                                          0246 00050000
         ST    @15,CATBFPTR                                        0246 00051000
*   DO IX=TEN TO 1 BY-1;            /* INIT CATBUFPTRS               */ 00052000
         LA    IX,10                                               0247 00053000
@DL00247 DS    0H                                                  0248 00054000
*     BFCTR(IX)=ZERO;               /* USE COUNT TO 0                */ 00055000
         SLR   @06,@06                                             0248 00056000
         LR    @15,IX                                              0248 00057000
         SLA   @15,3                                               0248 00058000
         STC   @06,BFCTR-8(@15)                                    0248 00059000
*     BFCI(IX)=ZERO;                /* CI NBR IS 0                   */ 00060000
         LA    @14,BFCI-8(@15)                                     0249 00061000
         STCM  @06,7,0(@14)                                        0249 00062000
*     BFPTR(IX)=ADDR(CCACPE(IX));   /* BUFFER POINTER                */ 00063000
         LR    @06,IX                                              0250 00064000
         SLA   @06,4                                               0250 00065000
         LA    @06,CCACPE-16(@06)                                  0250 00066000
         ST    @06,BFPTR-8(@15)                                    0250 00067000
*   END;                                                           0251 00068000
         BCTR  IX,0                                                0251 00069000
         LTR   IX,IX                                               0251 00070000
         BP    @DL00247                                            0251 00071000
*   BFCTR(1)=H255;                  /* INITIAL ENTRY REC NEEDED      */ 00072000
         MVI   BFCTR,X'FF'                                         0252 00073000
*   GEN SETS(CTGPLPTR);                                            0253 00074000
        L     R1,@SA00001+K4           POINT TO CALLERS SAVEAREA        00075000
        L     R1,O24(,R1)              LOAD CALLERS PARM LIST PTR       00076000
        ST    R1,CTGPLPTR              STORE CATLG PARM LIST POINTER    00077000
*   CALL CTLBKPTR;                  /* SET POINTERS TO THE CATALOG 0254 00078000
*                                      CONTROL BLOCKS THAT WE NEED   */ 00079000
         BAL   @14,CTLBKPTR                                        0254 00080000
*   IF CTGAM0=OFFX THEN             /* DO WE HAVE A O.S. LOCATE?     */ 00081000
         L     @09,CTGPLPTR                                        0255 00082000
         TM    CTGAM0(@09),B'00000001'                             0255 00083000
         BNZ   @RF00255                                            0255 00084000
*     DO;                           /* YES DO                        */ 00085000
*       CALL MAPOSCPL;              /* CREATE A CPL FPL FROM THE THE    00086000
*                                      CAMLST THE USER GAVE          */ 00087000
         BAL   @14,MAPOSCPL                                        0257 00088000
*       CALL GETTRNM;               /* GET TRUE NAME RECORD          */ 00089000
         BAL   @14,GETTRNM                                         0258 00090000
*       CALL FINDCI;                /* GET A RECORD BY CI NBR        */ 00091000
         BAL   @14,FINDCI                                          0259 00092000
*       CPX=CPE0;                   /* INIT RECORD POINTER           */ 00093000
         ST    CPE0,CPX                                            0260 00094000
*       CTGFLPTR=CTGFIELD(ONE);     /* SET PTR TO FIELD REQUEST      */ 00095000
         L     @08,CTGPLPTR                                        0261 00096000
         L     @08,CTGFIELD(,@08)                                  0261 00097000
         ST    @08,CTGFLPTR                                        0261 00098000
*       CALL CTINTFAC;              /* CATLG INTERFACE               */ 00099000
         BAL   @14,CTINTFAC                                        0262 00100000
*       CAMWORK=CAMPTR3;            /* POINT TO OUTPUT AREA          */ 00101000
         L     @08,CAMLSTPT                                        0263 00102000
         L     CAMWORK,CAMPTR3(,@08)                               0263 00103000
*       CALL MAPCPLOS;              /* MAP FROM CPL-FPL FORM TO THAT    00104000
*                                      REQUIRED BY O.S.              */ 00105000
         BAL   @14,MAPCPLOS                                        0264 00106000
*     END;                                                         0265 00107000
*   ELSE                            /* WE HAVE A VSAM LOCATE         */ 00108000
*     DO;                                                          0266 00109000
         B     @RC00255                                            0266 00110000
@RF00255 DS    0H                                                  0267 00111000
*       IF CTGNAME=ON THEN          /* 44 BYTE NAME?                 */ 00112000
         L     @09,CTGPLPTR                                        0267 00113000
         TM    CTGNAME(@09),B'00000100'                            0267 00114000
         BNO   @RF00267                                            0267 00115000
*         DO;                                                      0268 00116000
*           CALL GETTRNM;           /* GET TRUE NAME RECORD          */ 00117000
         BAL   @14,GETTRNM                                         0269 00118000
*         END;                                                     0270 00119000
*       ELSE                        /* LOCATE BY CI NBR              */ 00120000
*         CIWORK=CIWORK2;           /* MOVE CI NBR FROM CPL          */ 00121000
         B     @RC00267                                            0271 00122000
@RF00267 L     @01,CTGPLPTR                                        0271 00123000
         L     @01,CTGENT(,@01)                                    0271 00124000
         MVC   CIWORK(3),CIWORK2(@01)                              0271 00125000
*       CALL FINDCI;                /* GET PROPER CI INTO CORE       */ 00126000
@RC00267 BAL   @14,FINDCI                                          0272 00127000
*       CPX=CPE0;                                                  0273 00128000
         ST    CPE0,CPX                                            0273 00129000
*       GPFLDPTR=CTGWKA+FOUR;       /* SET OUTPUT POINTER            */ 00130000
         L     @01,CTGPLPTR                                        0274 00131000
         L     @15,CTGWKA(,@01)                                    0274 00132000
         LA    GPFLDPTR,4                                          0274 00133000
         ALR   GPFLDPTR,@15                                        0274 00134000
*       CTWKLIMT=WORKLEN+CTGWKA;    /* SET WORK AREA LIMIT           */ 00135000
         LH    @08,WORKLEN(,@15)                                   0275 00136000
         ALR   @08,@15                                             0275 00137000
         ST    @08,CTWKLIMT                                        0275 00138000
*       DO IX=1 TO CTGNOFLD;        /* PROCESS ALL FIELDS REQUEST    */ 00139000
         LA    IX,1                                                0276 00140000
         B     @DE00276                                            0276 00141000
@DL00276 DS    0H                                                  0277 00142000
*         CTGFLPTR=CTGFIELD(IX);    /* FIELD PTR FOR CAT INTERFAC    */ 00143000
         LR    @08,IX                                              0277 00144000
         SLA   @08,2                                               0277 00145000
         L     @06,CTGPLPTR                                        0277 00146000
         L     @08,CTGFIELD-4(@08,@06)                             0277 00147000
         ST    @08,CTGFLPTR                                        0277 00148000
*         CTGFLPT(ONE)=GPFLDPTR;    /* SET OUTPUT POINTER            */ 00149000
         ST    GPFLDPTR,CTGFLPT(,@08)                              0278 00150000
*         CALL CTINTFAC;            /* CATALOG INTERFACE             */ 00151000
         BAL   @14,CTINTFAC                                        0279 00152000
*         CTGFLNG(ONE)=GPFLDPTR-CTGFLPT(ONE);/* SET LENGTH           */ 00153000
         L     @08,CTGFLPTR                                        0280 00154000
         LR    @06,GPFLDPTR                                        0280 00155000
         SL    @06,CTGFLPT(,@08)                                   0280 00156000
         ST    @06,CTGFLNG(,@08)                                   0280 00157000
*         IF CTGFLNG(ONE)=ZERO THEN /* SET OUTPUT PTR ONLY IF DATA   */ 00158000
         SLR   @15,@15                                             0281 00159000
         CR    @06,@15                                             0281 00160000
         BNE   @RF00281                                            0281 00161000
*           CTGFLPT(ONE)=ZERO;      /* INDICATE NO VALUE             */ 00162000
         ST    @15,CTGFLPT(,@08)                                   0282 00163000
*       END;                                                       0283 00164000
@RF00281 AL    IX,@CF00077                                         0283 00165000
@DE00276 L     @08,CTGPLPTR                                        0283 00166000
         SLR   @06,@06                                             0283 00167000
         IC    @06,CTGNOFLD(,@08)                                  0283 00168000
         CR    IX,@06                                              0283 00169000
         BNH   @DL00276                                            0283 00170000
*     END;                                                         0284 00171000
*EXIT:                                                             0285 00172000
*   IF OSSW=ON THEN                 /* WAS IT AN O.S. TYPE LOCATE?   */ 00173000
@RC00255 DS    0H                                                  0285 00174000
EXIT     TM    OSSW,B'00000001'                                    0285 00175000
         BNO   @RF00285                                            0285 00176000
*     DO;                           /* YES DO                        */ 00177000
*       FREEAD=CTGPLPTR;            /* ADDRESS OF CORE TO FREE       */ 00178000
         L     @15,CTGPLPTR                                        0287 00179000
         ST    @15,FREEAD                                          0287 00180000
*       FREELEN=CTGDDNM;            /* LENGTH OF CORE TO FREE        */ 00181000
         L     @15,CTGDDNM(,@15)                                   0288 00182000
         ST    @15,FREELEN                                         0288 00183000
*       CALL FREEWORK;              /* FREE WORKING STORAGE          */ 00184000
         BAL   @14,FREEWORK                                        0289 00185000
*     END;                                                         0290 00186000
*   FREELEN=LENGTH(CATLGBUF)*TEN;   /* BUFFER LENGTH                 */ 00187000
@RF00285 MVC   FREELEN(4),@CF02590                                 0291 00188000
*   FREEAD=CATBFPTR;                                               0292 00189000
         L     @15,CATBFPTR                                        0292 00190000
         ST    @15,FREEAD                                          0292 00191000
*   CALL FREEWORK;                  /* CALL FREEMAIN ROUTINE         */ 00192000
         BAL   @14,FREEWORK                                        0293 00193000
*   RETURN CODE(ERRCD);             /* RETURN TO USER (NORMAL EXIT)  */ 00194000
*                                                                  0294 00195000
         L     @09,ERRCD                                           0294 00196000
         L     @13,4(,@13)                                         0294 00197000
         L     @00,@SIZDATD                                        0294 00198000
         LR    @01,@11                                             0294 00199000
         FREEMAIN R,LV=(0),A=(1)                                        00200000
         LR    @15,@09                                             0294 00201000
         L     @14,12(,@13)                                        0294 00202000
         LM    @00,@12,20(@13)                                     0294 00203000
         BR    @14                                                 0294 00204000
*   /*****************************************************************/ 00205000
*   /*                                                               */ 00206000
*   /* E X I T                                                       */ 00207000
*   /*                                                               */ 00208000
*   /*****************************************************************/ 00209000
*                                                                  0295 00210000
*MAPOSCPL:                                                         0295 00211000
*                                                                  0295 00212000
*   /*****************************************************************/ 00213000
*   /*                                                               */ 00214000
*   /* MAP AN OS CAMLIST INTO A VSAM CPL-FPL COMBINATION             */ 00215000
*   /*                                                               */ 00216000
*   /*****************************************************************/ 00217000
*                                                                  0295 00218000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0295 00219000
@EL00001 L     @13,4(,@13)                                         0295 00220000
@EF00001 L     @00,@SIZDATD                                        0295 00221000
         LR    @01,@11                                             0295 00222000
         FREEMAIN R,LV=(0),A=(1)                                        00223000
@ER00001 LM    @14,@12,12(@13)                                     0295 00224000
         BR    @14                                                 0295 00225000
MAPOSCPL DS    0H                                                  0296 00226000
*   GEN(ST     R14,RSAVA);                                         0296 00227000
         ST     R14,RSAVA                                               00228000
*   WKLEN=LENGTH(CTGPL)+FOUR        /* GET CORE FOR SIMULATED CPL    */ 00229000
*       +LENGTH(CTGFL)              /* AND AN FPL                    */ 00230000
*       +H44                        /* ROOM TO COPY NAME             */ 00231000
*       +H272;                      /* AND A CATALOG WORK AREA       */ 00232000
         MVC   WKLEN(2),@CH02591                                   0297 00233000
*   CALL GETWORK;                   /* GET WORKING STORAGE           */ 00234000
         BAL   @14,GETWORK                                         0298 00235000
*   OSSW=ON;                        /* SET O.S. LOCATE FLAG          */ 00236000
         OI    OSSW,B'00000001'                                    0299 00237000
*   CAMLSTPT=CTGPLPTR;              /* SAVE THE CAMLST POINTER       */ 00238000
         L     @09,CTGPLPTR                                        0300 00239000
         ST    @09,CAMLSTPT                                        0300 00240000
*   CTGPLPTR=WKCORE;                                               0301 00241000
         L     @09,WKCORE                                          0301 00242000
         ST    @09,CTGPLPTR                                        0301 00243000
*   CTGDDNM=WKLEN;                  /* SAVE FOR FREEMAIN             */ 00244000
         LH    @15,WKLEN                                           0302 00245000
         ST    @15,CTGDDNM(,@09)                                   0302 00246000
*   CTGFLPTR=WKCORE+LENGTH(CTGPL)+FOUR;/* LEAVE ROOM FOR CTGFIELD    */ 00247000
         LA    @15,32                                              0303 00248000
         ALR   @15,@09                                             0303 00249000
         ST    @15,CTGFLPTR                                        0303 00250000
*   CTGOPTN1='84'X;                 /* SET OPTION FLAGS              */ 00251000
         MVI   CTGOPTN1(@09),X'84'                                 0304 00252000
*   CTGOPTN2='00'X;                 /*                               */ 00253000
         MVI   CTGOPTN2(@09),X'00'                                 0305 00254000
*   CTGOPTN3='20'X;                                                0306 00255000
         MVI   CTGOPTN3(@09),X'20'                                 0306 00256000
*   CTGOPTN4='00'X;                 /* SET LAST OPTION BYTE          */ 00257000
         MVI   CTGOPTN4(@09),X'00'                                 0307 00258000
*   CTGENT=ADDR(CTGFL)+LENGTH(CTGFL);/* POINTER TO DATA SET NAME     */ 00259000
         AL    @15,@CF00727                                        0308 00260000
         ST    @15,CTGENT(,@09)                                    0308 00261000
*   DSNAME=' ';                     /* SET AREA TO BLANKS            */ 00262000
         MVI   DSNAME+1(@15),C' '                                  0309 00263000
         MVC   DSNAME+2(42,@15),DSNAME+1(@15)                      0309 00264000
         MVI   DSNAME(@15),C' '                                    0309 00265000
*   DO IX=1 TO 44 WHILE(CAMPTR1->DSNAME(IX:IX)^=' ');              0310 00266000
         LA    IX,1                                                0310 00267000
@DL00310 L     @09,CAMLSTPT                                        0310 00268000
         L     @09,CAMPTR1(,@09)                                   0310 00269000
         LR    @06,@09                                             0310 00270000
         ALR   @06,IX                                              0310 00271000
         BCTR  @06,0                                               0310 00272000
         CLI   DSNAME(@06),C' '                                    0310 00273000
         BE    @DC00310                                            0310 00274000
*     DSNAME(IX:IX)=CAMPTR1->DSNAME(IX:IX);                        0311 00275000
         L     @06,CTGPLPTR                                        0311 00276000
         L     @06,CTGENT(,@06)                                    0311 00277000
         ALR   @06,IX                                              0311 00278000
         BCTR  @06,0                                               0311 00279000
         ALR   @09,IX                                              0311 00280000
         BCTR  @09,0                                               0311 00281000
         MVC   DSNAME(1,@06),DSNAME(@09)                           0311 00282000
*   END;                                                           0312 00283000
         AL    IX,@CF00077                                         0312 00284000
         C     IX,@CF02048                                         0312 00285000
         BNH   @DL00310                                            0312 00286000
@DC00310 DS    0H                                                  0313 00287000
*   CTGWKA=WKCORE+WKLEN-H272;       /* POINT TO WORK AREA            */ 00288000
         L     @15,CTGPLPTR                                        0313 00289000
         LH    @14,WKLEN                                           0313 00290000
         AL    @14,WKCORE                                          0313 00291000
         SL    @14,@CF02483                                        0313 00292000
         ST    @14,CTGWKA(,@15)                                    0313 00293000
*   WORKLEN=H260;                   /* SET WORK AREA LENGTH TO WHAT     00294000
*                                      WE CAN MAP BACK               */ 00295000
         LA    @07,260                                             0314 00296000
         STH   @07,WORKLEN(,@14)                                   0314 00297000
*   GPFLDPTR=CTGWKA+FOUR;           /* SET START                     */ 00298000
         LA    GPFLDPTR,4                                          0315 00299000
         ALR   GPFLDPTR,@14                                        0315 00300000
*   CTWKLIMT=CTGWKA+WORKLEN;        /* SET END OF AREA               */ 00301000
         ALR   @14,@07                                             0316 00302000
         ST    @14,CTWKLIMT                                        0316 00303000
*   CTGNOFLD=ONE;                   /* INDICATE ONE FIELD            */ 00304000
         MVI   CTGNOFLD(@15),X'01'                                 0317 00305000
*   CTGFIELD(ONE)=CTGFLPTR;         /* POINT TO FPL                  */ 00306000
         L     @14,CTGFLPTR                                        0318 00307000
         ST    @14,CTGFIELD(,@15)                                  0318 00308000
*   CTGFLDNM=ADDR('OSLOCATE');      /* POINT TO COMBO FIELD FOR    0319 00309000
*                                      ANSWERING OS LOCATES          */ 00310000
         LA    @15,@CC02522                                        0319 00311000
         ST    @15,CTGFLDNM(,@14)                                  0319 00312000
*   GEN(L      R14,RSAVA);          /* RESTORE RETURN ADDRESS        */ 00313000
         L      R14,RSAVA                                               00314000
*   END MAPOSCPL;                                                  0321 00315000
@EL00002 DS    0H                                                  0321 00316000
@EF00002 DS    0H                                                  0321 00317000
@ER00002 BR    @14                                                 0321 00318000
*MAPCPLOS:                                                         0322 00319000
*                                                                  0322 00320000
*   /*****************************************************************/ 00321000
*   /*                                                               */ 00322000
*   /* MAP THE CATALOG DATA FROM THE CPL-FPL-WORKAREA INTO THE CAMLST*/ 00323000
*   /* WORK AREA                                                     */ 00324000
*   /*                                                               */ 00325000
*   /*****************************************************************/ 00326000
*                                                                  0322 00327000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0322 00328000
MAPCPLOS DS    0H                                                  0323 00329000
*   GEN(ST     R14,RSAVA);          /* SAVE RETURN ADDRESS           */ 00330000
         ST     R14,RSAVA                                               00331000
*   HLD=GPFLDPTR-TWELVE;            /* SAVE ADDRESS OF LAST ENTRY    */ 00332000
         LR    HLD,GPFLDPTR                                        0324 00333000
         SL    HLD,@CF00729                                        0324 00334000
*   GPFLDPTR=CTGWKA+FOUR;           /* POINT TO START OF 1ST VALUE   */ 00335000
         LA    GPFLDPTR,4                                          0325 00336000
         L     @07,CTGPLPTR                                        0325 00337000
         AL    GPFLDPTR,CTGWKA(,@07)                               0325 00338000
*   IF GPFLDPTR>HLD THEN            /* ARE THERE VALUES?             */ 00339000
         CR    GPFLDPTR,HLD                                        0326 00340000
         BNH   @RF00326                                            0326 00341000
*     DO;                                                          0327 00342000
*       ERRCD=RCDSNF;               /* NO, SET ERROR CODE            */ 00343000
         MVC   ERRCD(4),@CF01430                                   0328 00344000
*       RETURN TO EXIT;             /* BAIL OUT                      */ 00345000
         LA    @14,EXIT                                            0329 00346000
@EL00003 DS    0H                                                  0329 00347000
@EF00003 DS    0H                                                  0329 00348000
@ER00003 BR    @14                                                 0329 00349000
*     END;                                                         0330 00350000
*   ELSE                            /* TRANSFER THE DATA             */ 00351000
*     DO;                                                          0331 00352000
@RF00326 DS    0H                                                  0332 00353000
*       CAMVOLPT(ONE)=GPVOLPTR;     /* MOVE ONE VOLUME PTR           */ 00354000
         MVC   CAMVOLPT(12,CAMWORK),GPVOLPTR(GPFLDPTR)             0332 00355000
*       GPFLDPTR=GPFLDPTR+TWELVE;   /* STEP WORKAREA POINTER         */ 00356000
         AL    GPFLDPTR,@CF00729                                   0333 00357000
*       IX=ONE;                     /* INITIALIZE INDEX              */ 00358000
         LA    IX,1                                                0334 00359000
*       DO GPFLDPTR=GPFLDPTR TO HLD BY TWELVE/* LOOP THROUGH VALUES  */ 00360000
*             WHILE(IX^<TWENTY);    /* COLLECTED FROM THE CATALOG    */ 00361000
         B     @DE00335                                            0335 00362000
@DL00335 C     IX,@CF01432                                         0335 00363000
         BL    @DC00335                                            0335 00364000
*         DO IX1=1 TO IX            /* NO DUPLICATE VOLUME ENTRIES   */ 00365000
*               WHILE(CAMVOLSR(IX1)^=GPVOLSER);                    0336 00366000
         LA    IX1,1                                               0336 00367000
         B     @DE00336                                            0336 00368000
@DL00336 LR    @03,IX1                                             0336 00369000
         MH    @03,@CH00729                                        0336 00370000
         ALR   @03,CAMWORK                                         0336 00371000
         AL    @03,@CF02604                                        0336 00372000
         CLC   CAMVOLSR-6(6,@03),GPVOLSER(GPFLDPTR)                0336 00373000
         BE    @DC00336                                            0336 00374000
*         END;                                                     0337 00375000
         AL    IX1,@CF00077                                        0337 00376000
@DE00336 CR    IX1,IX                                              0337 00377000
         BNH   @DL00336                                            0337 00378000
@DC00336 DS    0H                                                  0338 00379000
*         IF IX1=IX THEN            /* DID WE FIND A DUPLICATE?      */ 00380000
         CR    IX1,IX                                              0338 00381000
         BNE   @RF00338                                            0338 00382000
*           DO;                     /* NO MOVE VOLUME POINTER        */ 00383000
*             CAMVOLPT(IX)=GPVOLPTR;/*                               */ 00384000
         LR    @08,IX                                              0340 00385000
         MH    @08,@CH00729                                        0340 00386000
         ALR   @08,CAMWORK                                         0340 00387000
         AL    @08,@CF02605                                        0340 00388000
         MVC   CAMVOLPT-2(12,@08),GPVOLPTR(GPFLDPTR)               0340 00389000
*             IX=IX+ONE;            /* STEP INDEX                    */ 00390000
         AL    IX,@CF00077                                         0341 00391000
*           END;                                                   0342 00392000
*       END;                                                       0343 00393000
@RF00338 AL    GPFLDPTR,@CF00729                                   0343 00394000
@DE00335 CR    GPFLDPTR,HLD                                        0343 00395000
         BNH   @DL00335                                            0343 00396000
@DC00335 DS    0H                                                  0344 00397000
*       CAMWKCNT=IX;                /* SET COUNT OF ENTRIES          */ 00398000
         STH   IX,CAMWKCNT(,CAMWORK)                               0344 00399000
*     END;                                                         0345 00400000
*   GEN(L      R14,RSAVA);          /* RESTORE ADDRESS               */ 00401000
         L      R14,RSAVA                                               00402000
