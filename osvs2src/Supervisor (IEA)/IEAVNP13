IEAVNP13 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVNP13  74.240'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @11,0                                               0001 00011000
@PSTART  LA    @12,4095(,@11)                                      0001 00012000
         USING @PSTART,@11                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
         ST    @13,@SA00001+4                                      0001 00015000
         LA    @14,@SA00001                                        0001 00016000
         ST    @14,8(,@13)                                         0001 00017000
         LR    @13,@14                                             0001 00018000
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF          00019000
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES      00020000
** WILL ASSEMBLE PROPERLY.                                              00021000
*/*                                                                 */  00022000
*/*      MODULE IEAVNP13 MASTER SCHEDULER R.I.M.                    */  00023000
*/*      THE FUNCTION IS TO BUILD A TABLE FOR SMF WITH IT'S         */  00024000
*/*      MEMBER NAME IN FOR SMF PARAMETERS.                         */  00025000
*/*      THEN TO GET THE PARMTAB PARAMETERS FOR 'CMD=' AND          */  00026000
*/*      BUILD A QUEUE OF ACTUAL SYSTEM COMMANDS, AND TO SET        */  00027000
*/*      THE TIME OF DAY BIT IF TOD= IS IN THE COMMNDXX MEMBER      */  00028000
*/*      ELSE SET THE DEFAULT TO NO PROMPT.                         */  00029000
*/*      ENDING WITH TIME ZONE PROCESSING , PUTTING THE CONSTANT    */  00030000
*/*      IN THE CVT 'CVTTZ' IF PARMTZ MEMBER IS IN PARMLIB....      */  00031000
*/*                                                                 */  00032000
*CVTPTR=RCVT;                                                      0202 00033000
         ST    RCVT,CVTPTR                                         0202 00034000
*MSTODWTO=ONEBIT;                           /* SET TOD BIT TO DEFALT*/  00035000
         L     @10,CVTPTR                                          0203 00036000
         L     @10,CVTMSER(,@10)                                   0203 00037000
         OI    MSTODWTO(@10),B'01000000'                           0203 00038000
*CMDBAD=ZEROBIT;                            /* SET IT TO ZERO INIT  */  00039000
         NI    CMDBAD,B'10111111'                                  0204 00040000
*R2SAVE=RNVT;                               /* SAVE PRT TO NVT      */  00041000
         ST    RNVT,R2SAVE                                         0205 00042000
*EXITSAVE=REXIT;                                 /* SAVE RETURN ADR.*/  00043000
         ST    REXIT,EXITSAVE                                      0206 00044000
*RFY NVT BASED(R2SAVE);                                            0207 00045000
*RFY PARMTAB BASED(NVTPTAB);                                       0208 00046000
*CALL SMFPROC;                              /* PROCESS SMF PARMS    */  00047000
         BAL   @14,SMFPROC                                         0209 00048000
*CALL CMDPROC;                              /* PROCESS CMD PARMS    */  00049000
         BAL   @14,CMDPROC                                         0210 00050000
*IF CMD=ZERO THEN                                /* TEST FOR DEFAULT*/  00051000
         L     @10,R2SAVE                                          0211 00052000
         L     @10,NVTPTAB(,@10)                                   0211 00053000
         CLC   CMD(4,@10),ZERO                                     0211 00054000
         BNE   @RF00211                                            0211 00055000
*  DO;                                           /* DO THIS IF SO   */  00056000
*    COMMEM=COMMND00;                      /* SET MEM NAME TO COMMND*/  00057000
         MVC   COMMEM(8),@CC01973                                  0213 00058000
*    NIPPAFL1=ONEBIT;                       /* STOP NOFIND MSG.     */  00059000
         OI    NIPPAFL1(@10),B'10000000'                           0214 00060000
*   END;                                                           0215 00061000
*ELSE                                       /* NO DEFAULT GET PARMS */  00062000
*  IF CMDPARMS  (1:1)=LPARAN THEN           /* TEST FOR ONLY ONE NUM*/  00063000
         B     @RC00211                                            0216 00064000
@RF00211 L     @10,R2SAVE                                          0216 00065000
         L     @10,NVTPTAB(,@10)                                   0216 00066000
         L     @10,CMD(,@10)                                       0216 00067000
         CLI   CMDPARMS(@10),C'('                                  0216 00068000
         BNE   @RF00216                                            0216 00069000
*    DO;                                    /* SET BY THE OPERATOR  */  00070000
*    COMMEM=COMMND00;                       /*SET MEMBER CONSTANT   */  00071000
         MVC   COMMEM(8),@CC01973                                  0218 00072000
*    COMNUM (1:2) =CMDPARMS  (2:3)|UPCASE;  /* GET 1ST DIGIT OF NUM */  00073000
         MVC   COMNUM(2),CMDPARMS+1(@10)                           0219 00074000
         OC    COMNUM(2),@CC01971                                  0219 00075000
*     PTRSAVE=CMD+4;                       /* SAVE FOR NEXT MEMBER */   00076000
         AH    @10,@CH00327                                        0220 00077000
         ST    @10,PTRSAVE                                         0220 00078000
*    END;                                   /* END DO LOOP          */  00079000
*  ELSE                                     /* ONE MEM IN PARMS     */  00080000
*    DO;                                    /* SET BY THE OPERATOR  */  00081000
         B     @RC00216                                            0222 00082000
@RF00216 DS    0H                                                  0223 00083000
*     COMMEM=COMMND00;                      /* SET MEM COSTANT      */  00084000
         MVC   COMMEM(8),@CC01973                                  0223 00085000
*      COMNUM  (1:2)=CMDPARMS  (1:2)|UPCASE; /* GET 1ST DIGIT OF NUM*/  00086000
         L     @10,R2SAVE                                          0224 00087000
         L     @10,NVTPTAB(,@10)                                   0224 00088000
         L     @10,CMD(,@10)                                       0224 00089000
         MVC   COMNUM(2),CMDPARMS(@10)                             0224 00090000
         OC    COMNUM(2),@CC01971                                  0224 00091000
*    END;                                                          0225 00092000
*ALLDONE=NO;                                /* SET FOR DO WHILE     */  00093000
@RC00216 DS    0H                                                  0226 00094000
@RC00211 NI    ALLDONE,B'01111111'                                 0226 00095000
*DO WHILE ALLDONE=NO;                       /* ANY ERROR GET OUT    */  00096000
         B     @DE00227                                            0227 00097000
@DL00227 DS    0H                                                  0228 00098000
*  NIPPANAM=COMMEM;                    /* CURR MEMBER FOR READ FIND */  00099000
         L     @10,R2SAVE                                          0228 00100000
         L     @09,NVTPAREA(,@10)                                  0228 00101000
         MVC   NIPPANAM(8,@09),COMMEM                              0228 00102000
*  RNVT=R2SAVE;                        /* SET THE NVT PTR           */  00103000
         LR    RNVT,@10                                            0229 00104000
*  RCVT=CVTPTR;                        /* SET THE CVT PTR           */  00105000
         L     RCVT,CVTPTR                                         0230 00106000
*  CALL NIPPMMEM;                      /* NIP FIND + READ ROUTINE   */  00107000
         BAL   @14,NIPPMMEM                                        0231 00108000
*  IF RPARM> ZERO THEN                 /* R1 POSITIVE ALL IS OK     */  00109000
         C     RPARM,ZERO                                          0232 00110000
         BNH   @RF00232                                            0232 00111000
*    DO;                                                           0233 00112000
*      RECADDR=RPARM;                  /* SAVE PTR TO REC JUST READ */  00113000
         LR    RECADDR,RPARM                                       0234 00114000
*/*                                                          @YM03617*/ 00115000
*/* TEST FOR COM=' AT THE BEGINNING OF THE BUFFER.           @YM03617*/ 00116000
*/* IF IT IS COM=', GO PROCESS COMMANDS FROM PARMLIB.        @YM03617*/ 00117000
*/* IF IT IS NOT, GO CHECK FOR TOD= .                        @YM03617*/ 00118000
*/*                                                          @YM03617*/ 00119000
*      RFY COMREC BASED(RECADDR);      /* DSECT FOR THE CMD RECORD  */  00120000
*      IF COMREC (1:5)=COM THEN        /* TEST FOR COM=' A SYS. CMD */  00121000
         CLC   COMREC(5,RECADDR),@CC01951                          0236 00122000
         BNE   @RF00236                                            0236 00123000
*        CALL COMRECD;                 /* PROCESS THE COMMAND       */  00124000
         BAL   @14,COMRECD                                         0237 00125000
*      ELSE                            /* MIGHT BE A TOD=' RECORD   */  00126000
*        CALL TODRECD;                 /* SO PROCESS FOR TOD        */  00127000
         B     @RC00236                                            0238 00128000
@RF00236 BAL   @14,TODRECD                                         0238 00129000
*    END;                                                          0239 00130000
*  ELSE                                /* HAD AN I/O ERROR OR EOF   */  00131000
*    IF NIPPARC=SIXTEEN THEN           /* TEST FOR END OF FILE      */  00132000
         B     @RC00232                                            0240 00133000
@RF00232 L     @10,R2SAVE                                          0240 00134000
         L     @10,NVTPAREA(,@10)                                  0240 00135000
         CLC   NIPPARC(4,@10),@CF00147                             0240 00136000
         BNE   @RF00240                                            0240 00137000
*      CALL EOFRTN;                    /* GET EOF ROUTINE           */  00138000
         BAL   @14,EOFRTN                                          0241 00139000
*    ELSE                              /* IT WAS AN I/O ERROR       */  00140000
*      IF CMD=ZERO&NIPPARC=FOUR THEN                               0242 00141000
         B     @RC00240                                            0242 00142000
@RF00240 L     @10,R2SAVE                                          0242 00143000
         L     @09,NVTPTAB(,@10)                                   0242 00144000
         CLC   CMD(4,@09),ZERO                                     0242 00145000
         BNE   @RF00242                                            0242 00146000
         L     @10,NVTPAREA(,@10)                                  0242 00147000
         CLC   NIPPARC(4,@10),@CF00327                             0242 00148000
         BNE   @RF00242                                            0242 00149000
*        DO;                           /* TEST FOR NO FIND          */  00150000
*          RPARM=SIXTEEN;              /* SHOW NO ERRORS            */  00151000
         LA    RPARM,16                                            0244 00152000
*          ALLDONE=YES;                /* SO STOP DO WHILE          */  00153000
         OI    ALLDONE,B'10000000'                                 0245 00154000
*        END;                                                      0246 00155000
*       ELSE                                                       0247 00156000
*         ALLDONE=YES;                 /* SO STOP THE DO WHILE      */  00157000
         B     @RC00242                                            0247 00158000
@RF00242 OI    ALLDONE,B'10000000'                                 0247 00159000
*END;                                  /* DO WHILE ALLDONE=NO END   */  00160000
@RC00242 DS    0H                                                  0248 00161000
@RC00240 DS    0H                                                  0248 00162000
@RC00232 DS    0H                                                  0248 00163000
@DE00227 TM    ALLDONE,B'10000000'                                 0248 00164000
         BZ    @DL00227                                            0248 00165000
*IF RPARM^=SIXTEEN THEN                /* IT MEANS THAT AN I/O ERROR*/  00166000
         CH    RPARM,@CH00147                                      0249 00167000
         BE    @RF00249                                            0249 00168000
*  CALL ERROREND;                      /* TO CLEAN UP ALL GET MAINS */  00169000
         BAL   @14,ERROREND                                        0250 00170000
*CALL TZRTN;                           /* DO THE TIME ZONE CONSTANT */  00171000
@RF00249 BAL   @14,TZRTN                                           0251 00172000
*CALL LOGRTN;                          /* PROCESS LOG PARMS         */  00173000
         BAL   @14,LOGRTN                                          0252 00174000
*REXIT=EXITSAVE;                            /* SET RETURN ADDR      */  00175000
         L     REXIT,EXITSAVE                                      0253 00176000
*RETURN;                                                           0254 00177000
@EL00001 L     @13,4(,@13)                                         0254 00178000
@EF00001 DS    0H                                                  0254 00179000
@ER00001 LM    @14,@12,12(@13)                                     0254 00180000
         BR    @14                                                 0254 00181000
*/*      END OF IEAVNP13 MAINLINE CODE   */                        0255 00182000
*/*                                                                 */  00183000
*/*      THE SMF PROCESSOR WILL SYNTAX THE PARMTAB PARAMETER        */  00184000
*/*      FOR ONLY ONE ENTRY FOR THE MEMBER NAME. IT WILL THEN       */  00185000
*/*      BUILD A TABLE TO BE PASSED TO SMF INITIALITION RTN..       */  00186000
*/*      IF THE PARMTAB POINTER IS ZERO IT MEANS TO USE THE         */  00187000
*/*      DEFAULT MEMBER FOR SMF IF SO THE POINTER IN THE M/S RES.   */  00188000
*/*      DATA AREA WILL BE SET TO ZERO..IN THE CASE OF BAD SYNTAX   */  00189000
*/*      IT WILL PROMPT THE OPERATOR TO RESPECIFY IT..              */  00190000
*/*      INPUT...  1 SMF ( PARMTAB PTR TO SMF PARAMETER )           */  00191000
*/*                                                                 */  00192000
*/*      OUTPUT... 1 MSSMFPRM  ( PTR TO SMF TABLE )                 */  00193000
*/*                2 SMF TABLE ( HAS THE SMFPRMXX MEMBER NAME INIT) */  00194000
*SMFPROC:                              /* SMF PROCESSING ROUTINE    */  00195000
*PROC;        /* INTERNAL PROCEDURE        */                      0255 00196000
SMFPROC  STM   @14,@12,@SA00002                                    0255 00197000
*ALLDONE=NO;                           /* SET FOR DO LOOP           */  00198000
         NI    ALLDONE,B'01111111'                                 0256 00199000
*DO WHILE ALLDONE=NO;                  /* DO SMF UNTIL ERROR = YES  */  00200000
         B     @DE00257                                            0257 00201000
@DL00257 DS    0H                                                  0258 00202000
*  RFY SMFPARM BASED(SMF);                                         0258 00203000
*  RNVT=ZERO;                                                      0259 00204000
         L     @10,ZERO                                            0259 00205000
         LR    RNVT,@10                                            0259 00206000
*  RPARM=ZERO;                                                     0260 00207000
         LR    RPARM,@10                                           0260 00208000
*  IF SMF=ZERO THEN                    /* TEST FOR DEFAULT USED     */  00209000
         L     @09,R2SAVE                                          0261 00210000
         L     @09,NVTPTAB(,@09)                                   0261 00211000
         C     @10,SMF(,@09)                                       0261 00212000
         BNE   @RF00261                                            0261 00213000
*    DO;                                                           0262 00214000
*      MSSMFPRM=ZERO;                  /* PTR TO SMF TABLE = ZERO   */  00215000
         L     @09,CVTPTR                                          0263 00216000
         L     @09,CVTMSER(,@09)                                   0263 00217000
         ST    @10,MSSMFPRM(,@09)                                  0263 00218000
*      ALLDONE=YES;                    /* GET OUT OF DO LOOP        */  00219000
         OI    ALLDONE,B'10000000'                                 0264 00220000
*    END;                                                          0265 00221000
*  ELSE                                /* SYNTAX TEST PARMTAB PARMS */  00222000
*   DO;                                                            0266 00223000
         B     @RC00261                                            0266 00224000
@RF00261 DS    0H                                                  0267 00225000
*    IF SMFPARM (1:1)^=LPARAN THEN          /* TEST FOR PARANS IN PA*/  00226000
         L     @10,R2SAVE                                          0267 00227000
         L     @10,NVTPTAB(,@10)                                   0267 00228000
         L     @10,SMF(,@10)                                       0267 00229000
         CLI   SMFPARM(@10),C'('                                   0267 00230000
         BE    @RF00267                                            0267 00231000
*      DO;                                  /* PROCESS FOR NO PARANS*/  00232000
*        PTRSAVE=SMF;                       /* SAVE START ADDROF PAR*/  00233000
         ST    @10,PTRSAVE                                         0269 00234000
*        RFY SMFPARM BASED(PTRSAVE);        /* BASE SMF PARMS ON SAV*/  00235000
*        TRT (SMFPARM,TRTTAB);         /* TRANS AND TEST ALPHMERIC  */  00236000
         L     @10,PTRSAVE                                         0271 00237000
         TRT   SMFPARM(10,@10),TRTTAB                              0271 00238000
*        IF RNVT=ONE THEN                   /* MUST STOP ON QUOTE   */  00239000
         CH    RNVT,@CH00037                                       0272 00240000
         BNE   @RF00272                                            0272 00241000
*          IF SMFPARM  (3:3)=QUOTE THEN     /* MUST BE A QUOTE      */  00242000
         L     @10,PTRSAVE                                         0273 00243000
         CLI   SMFPARM+2(@10),C''''                                0273 00244000
         BNE   @RF00273                                            0273 00245000
*            ALLDONE=YES;                   /* ALL WAS OK SO GET OUT*/  00246000
         OI    ALLDONE,B'10000000'                                 0274 00247000
*          ELSE;                                 /* ERROR PRPT OPR. */  00248000
@RF00273 DS    0H                                                  0276 00249000
*        ELSE;                         /* ERROR SO PROMPT THE OPR.  */  00250000
@RF00272 DS    0H                                                  0277 00251000
*      END;                                                        0277 00252000
*    ELSE                                                          0278 00253000
*      DO;                                  /* IT WAS A LPARAN SOTRT*/  00254000
         B     @RC00267                                            0278 00255000
@RF00267 DS    0H                                                  0279 00256000
*        PTRSAVE=SMF+ONE;                   /* START ON 2ND CHAR PAR*/  00257000
         L     @10,R2SAVE                                          0279 00258000
         L     @10,NVTPTAB(,@10)                                   0279 00259000
         LA    @09,1                                               0279 00260000
         AL    @09,SMF(,@10)                                       0279 00261000
         ST    @09,PTRSAVE                                         0279 00262000
*        RFY SMFPARM BASED(PTRSAVE);        /* BASE THE PARM RECORD */  00263000
*        TRT (SMFPARM,TRTTAB);              /* TRTFOR ALPHAMERIC    */  00264000
         L     @10,PTRSAVE                                         0281 00265000
         TRT   SMFPARM(10,@10),TRTTAB                              0281 00266000
*        IF RNVT = EIGHT THEN               /* IT STOPED ON RPARAN  */  00267000
         CH    RNVT,@CH00132                                       0282 00268000
         BNE   @RF00282                                            0282 00269000
*          DO;                              /* TEST FOR QUOTE AFTER */  00270000
*            IF RPARM=PTRSAVE+TWO THEN      /* IS RPARAN IN OK POS. */  00271000
         L     @10,PTRSAVE                                         0284 00272000
         LA    @09,2                                               0284 00273000
         ALR   @09,@10                                             0284 00274000
         CR    RPARM,@09                                           0284 00275000
         BNE   @RF00284                                            0284 00276000
*              IF SMFPARM (4:4)= QUOTE THEN   /* ' AFTER RPARAN ?   */  00277000
         CLI   SMFPARM+3(@10),C''''                                0285 00278000
         BNE   @RF00285                                            0285 00279000
*                ALLDONE=YES;                 /* ALLOK GET OUTOF DO */  00280000
         OI    ALLDONE,B'10000000'                                 0286 00281000
*              ELSE;                        /* NULL FOR IF = TO ''' */  00282000
@RF00285 DS    0H                                                  0288 00283000
*            ELSE;                          /* NULL FOR IF )) IS OK */  00284000
@RF00284 DS    0H                                                  0289 00285000
*          END;                             /* END FOR DO RCVT=8    */  00286000
*        ELSE;                              /* NULL FOR RCVT=8   */     00287000
@RF00282 DS    0H                                                  0291 00288000
*      END;                                 /* END FOR RPARAN ELSE  */  00289000
*  IF ALLDONE=YES THEN                      /*  IF NOT IT IS ERROR  */  00290000
@RC00267 TM    ALLDONE,B'10000000'                                 0292 00291000
         BNO   @RF00292                                            0292 00292000
*    DO;                                    /* IF NOT IT IS ERROR   */  00293000
*      RLENGTH=GETLGTH;                /* SET  LENGTH OF GETMAIN    */  00294000
         L     RLENGTH,@CF01967                                    0294 00295000
*      GEN( GETMAIN R,LV=(0) );             /* DO THE GETMAIN       */  00296000
          GETMAIN R,LV=(0)                                              00297000
*      RFY SMFMEM BASED(RPARM);             /* BASE SMF DSECT       */  00298000
*      MSSMFPRM=RPARM;                      /* SET PTR TO TABLE     */  00299000
         L     @10,CVTPTR                                          0297 00300000
         L     @10,CVTMSER(,@10)                                   0297 00301000
         ST    RPARM,MSSMFPRM(,@10)                                0297 00302000
*      TABLGTH=LGTH12;                      /* SET TABLE LENGTH     */  00303000
         MVC   TABLGTH(2,RPARM),@CH00033                           0298 00304000
*      MEMNAME=SMFPRM;                      /* SET MEMBER NAME      */  00305000
         MVC   MEMNAME(6,RPARM),@CC01969                           0299 00306000
*      MEMNUM (1:2)=SMFPARM (1:2)|UPCASE;   /* SET MEMBER NUMBER    */  00307000
         L     @10,PTRSAVE                                         0300 00308000
         MVC   MEMNUM(2,RPARM),SMFPARM(@10)                        0300 00309000
         OC    MEMNUM(2,RPARM),@CC01971                            0300 00310000
*    END;                                   /* END OF TAB BUILD DO  */  00311000
*  ELSE                                     /* HERE BECAUSE OF ERROR*/  00312000
*    DO;                                    /* SO PROMPT THE OPERATR*/  00313000
         B     @RC00292                                            0302 00314000
@RF00292 DS    0H                                                  0303 00315000
*    RNVT=R2SAVE;                           /* SET NVT PTR */      0303 00316000
         L     RNVT,R2SAVE                                         0303 00317000
*    GEN REFS(NVTPRMPT) (IEAPMNIP TYPE=PRMPT,PARAM=BADSMF,OPT=ERROR );  00318000
         IEAPMNIP TYPE=PRMPT,PARAM=BADSMF,OPT=ERROR                     00319000
*    END;                                   /* END THE PROMPT DO LOP*/  00320000
*  RFY SMFPARM BASED(SMF);                                         0306 00321000
@RC00292 DS    0H                                                  0307 00322000
* END;                                      /* END OF SMF PROCESS   */  00323000
*END;                                                              0308 00324000
@RC00261 DS    0H                                                  0308 00325000
@DE00257 TM    ALLDONE,B'10000000'                                 0308 00326000
         BZ    @DL00257                                            0308 00327000
*END SMFPROC;                                                      0309 00328000
@EL00002 DS    0H                                                  0309 00329000
@EF00002 DS    0H                                                  0309 00330000
@ER00002 LM    @14,@12,@SA00002                                    0309 00331000
         BR    @14                                                 0309 00332000
*/*                                                                 */  00333000
*/*      THIS IS THE COMMAND PARAMETERS SYNTAX CHECKER FOR PARMTAB  */  00334000
*/*      THE SYNTAX OF THE PARAMETERS ARE CMD=NN OR CMD=(NN,NN,NN)  */  00335000
*/*      ANY ERRORS THE PROC WILL PROMPT THE SYSTEM OPERATOR        */  00336000
*/*      TO RESPECIFY....                                           */  00337000
*/*      INPUT...  1 CMD ( PARMTAB PTR TO CMD= PARAMETERS)          */  00338000
*/*                                                                 */  00339000
*/*      OUTPUT... 1 #MEMBERS  ( THE NUMBER OF COMMND MEMBERS TO    */  00340000
*/*                  BE PROCESSED BY COMRECD ROUNTINE )             */  00341000
*/*                                                                 */  00342000
*/*                                                                 */  00343000
*CMDPROC:                                                          0310 00344000
*PROC;                                                             0310 00345000
CMDPROC  STM   @14,@12,@SA00003                                    0310 00346000
*ALLDONE=NO;                                /* SET FOR DO LOOP      */  00347000
         NI    ALLDONE,B'01111111'                                 0311 00348000
*#PROCESD=ZERO;                             /* SET IT TO ZERO       */  00349000
         MVC   #PROCESD(2),ZERO+2                                  0312 00350000
*DO WHILE ALLDONE=NO;                       /* THE CMD DO LOOP      */  00351000
         B     @DE00313                                            0313 00352000
@DL00313 DS    0H                                                  0314 00353000
*  RFY CMDPARMS BASED(CMD);                                        0314 00354000
*  RNVT=ZERO;                                                      0315 00355000
         L     @10,ZERO                                            0315 00356000
         LR    RNVT,@10                                            0315 00357000
*  RPARM=ZERO;                                                     0316 00358000
         LR    RPARM,@10                                           0316 00359000
*  PTRSAVE=CMD;                             /* SAVE ADDR OF CMD     */  00360000
         L     @09,R2SAVE                                          0317 00361000
         L     @09,NVTPTAB(,@09)                                   0317 00362000
         L     @09,CMD(,@09)                                       0317 00363000
         ST    @09,PTRSAVE                                         0317 00364000
*  #MEMBERS=ONE;                            /* #MEM 3PROC TOZERO    */  00365000
         MVC   #MEMBERS(2),@CH00037                                0318 00366000
*  IF CMD=ZERO THEN                    /* USE THE COMMND00 DEFAULT  */  00367000
         CR    @09,@10                                             0319 00368000
         BNE   @RF00319                                            0319 00369000
*    ALLDONE=YES;                       /* GET OUT OF LOOP          */  00370000
         OI    ALLDONE,B'10000000'                                 0320 00371000
*  ELSE                                /* DO PARMTAB PARMAMETERS    */  00372000
*    IF CMDPARMS (1:1)=LPARAN THEN          /* TEST MULT PARMS..    */  00373000
         B     @RC00319                                            0321 00374000
@RF00319 L     @10,R2SAVE                                          0321 00375000
         L     @10,NVTPTAB(,@10)                                   0321 00376000
         L     @10,CMD(,@10)                                       0321 00377000
         CLI   CMDPARMS(@10),C'('                                  0321 00378000
         BNE   @RF00321                                            0321 00379000
*      DO;                                  /* YES TRT THE PARMS    */  00380000
*        PTRSAVE=PTRSAVE+ONE;               /* POINT TO NEXT CHAR   */  00381000
         LA    @10,1                                               0323 00382000
         AL    @10,PTRSAVE                                         0323 00383000
