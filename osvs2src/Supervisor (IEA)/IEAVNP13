*          DCL NIPPMTND LABEL GENERATED INTERNAL;                  0712 01688000
*                                                                  0712 01689000
*          /* MAPPING AREAS INTO ERROR MESSAGES                      */ 01690000
*          DCL NIPPMME1 CHAR(8) BASED(ADDR(NIPPMMB1));             0713 01691000
*          DCL NIPPMME2 CHAR(8) BASED(ADDR(NIPPMMB2));             0714 01692000
*          DCL NIPPMNAM CHAR(8) INIT(' '); /*MEMBER NAME SAVE AREA 0715 01693000
*                                                              Y02752*/ 01694000
*          DCL NIPPMNEG FIXED(31) INIT('FFFFFFFF'X); /*NEGATIVE VALUE   01695000
*                                                              Y02752*/ 01696000
*         RESPECIFY IOBSTDRD BASED(ADDR(NIPPAIOB));/* IOB BASE Y02752*/ 01697000
*                                                                  0717 01698000
*         RESPECIFY (NIPPMR14,NIPPMR15,NIPPMR00,NIPPMR01) RESTRICTED;/* 01699000
*                                                              Y02752*/ 01700000
*NIPPMMEM: NIPPMADR=NIPPMR14;  /* SAVE CALLER'S RETURN REGISTER    0719 01701000
