*                                                                  0702 01655000
*          /* ESTABLISH BASE FOR NIP COMMUNICATION AREA        Y02752*/ 01656000
*          RESPECIFY NIPPAREA BASED(NVTPAREA);              /* Y02752*/ 01657000
*                                                                  0703 01658000
*          /* ONE BYTE SCANNING FIELD                                */ 01659000
*          DCL NIPPMBYT CHAR(1) BASED;                             0704 01660000
*                                                                  0704 01661000
*          /* TEMPORARY POINTER                                Y02752*/ 01662000
*          DCL NIPPMKPR PTR(31);                            /* Y02752*/ 01663000
*                                                                  0705 01664000
*                                                                  0705 01665000
*          /* INPUT RECORD READ FROM PARMLIB                   Y02752*/ 01666000
*          DCL NIPPMBUF CHAR(80) BASED(NIPPMR01);           /* Y02752*/ 01667000
*                                                                  0706 01668000
*          /* MBBCCHHR OF PARMLIB MEMBERS                            */ 01669000
*          DCL 1 NIPPMMBC CHAR(8),                                 0707 01670000
*                2 NIPPMMBB CHAR(3),                               0707 01671000
*                2 NIPPMCCH CHAR(5);                               0707 01672000
*                                                                  0707 01673000
*          /* LOW ORDER 7 BYTES OF THE CSW STORED IN THE PARMLIB   0708 01674000
*             IOB BY EXCP                                      Y02752*/ 01675000
*          DCL 1 NIPPMCSW BASED(ADDR(IOBCSW)) BDY(BYTE),    /* Y02752*/ 01676000
*                2 * CHAR(3),                               /* Y02752*/ 01677000
*                2 NIPPMCS2 BIT(8),   /* I/O OPERATION STATUS FLAGS     01678000
*                                                           /* Y02752*/ 01679000
*                2 * CHAR(3);                               /* Y02752*/ 01680000
*                                                                  0708 01681000
*          /* MASK TO TEST THE STATUS FLAGS OF THE PARMLIB CSW Y02752*/ 01682000
