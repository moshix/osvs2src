         MVCL  WKREGE,WKREG0                                            00372000
*/* P UPDATE TABLE HEADER                                            */ 00373000
*        APPBAL = APPSIZE + APPBAL;    /* NEW BALANCE OF UNUSED CORE */ 00374000
         LH    @15,APPSIZE(,AIDPTR)                                0241 00375000
         LH    @14,APPBAL(,AIDPTR)                                 0241 00376000
         ALR   @14,@15                                             0241 00377000
         STH   @14,APPBAL(,AIDPTR)                                 0241 00378000
*        APPSIZE = 2 * APPSIZE;        /* NEW TOTAL TABLE SIZE       */ 00379000
         ALR   @15,@15                                             0242 00380000
         STH   @15,APPSIZE(,AIDPTR)                                0242 00381000
*/* P POINT CVT TO NEW TABLE                                         */ 00382000
*        RPARM = CVTAIDVT;             /* PICK UP PTR TO OLD TABLE   */ 00383000
         L     RPARM,CVTAIDVT(,RCVT)                               0243 00384000
*        CVTAIDVT = AIDPTR;            /* STORE PTR TO NEW TABLE     */ 00385000
         ST    AIDPTR,CVTAIDVT(,RCVT)                              0244 00386000
*/* P ADJUST TARGET POINTER INTO NEW TABLE                           */ 00387000
*        TGTPTR = AIDPTR + (TGTPTR - RPARM); /* SAME OFFSET, NEW BASE*/ 00388000
         LR    @00,TGTPTR                                          0245 00389000
         SLR   @00,RPARM                                           0245 00390000
         ALR   @00,AIDPTR                                          0245 00391000
         LR    TGTPTR,@00                                          0245 00392000
*/* P FREE CORE FOR OLD TABLE                                        */ 00393000
*        WKREG0 = RPARM -> APPSIZE;    /* GET SIZE OF AREA TO FREE   */ 00394000
         LH    WKREG0,APPSIZE(,RPARM)                              0246 00395000
*        GEN (FREEMAIN RU,LV=(0),A=(1),SP=245); /* FREEMAIN  @ZA06784*/ 00396000
         FREEMAIN RU,LV=(0),A=(1),SP=245                                00397000
*/* R RETURN TO MAIN LINE                                            */ 00398000
*        REXIT = SUBRTN;               /* RESTORE RETURN POINT       */ 00399000
         LR    REXIT,SUBRTN                                        0248 00400000
*        RESPECIFY SUBRTN UNRSTD;      /* RELEASE REGISTER FOR PLS   */ 00401000
*        END EXTEND;                   /* RETURN TO MAIN-LINE        */ 00402000
@EL00002 DS    0H                                                  0250 00403000
@EF00002 DS    0H                                                  0250 00404000
@ER00002 BR    @14                                                 0250 00405000
*/*IEAVNP16: END                                                     */ 00406000
*        GEN (EJECT);                                              0251 00407000
         EJECT                                                          00408000
*/*                                                                  */ 00409000
*/********************************************************************/ 00410000
*/*                                                                  */ 00411000
*/* INCLUDE NIP SERVICE ROUTINE FOR READING PARMLIB                  */ 00412000
*/*                                                                  */ 00413000
*/********************************************************************/ 00414000
*/*                                                                  */ 00415000
*/********************************************************************/ 00416000
*/*                                                                  */ 00417000
*/* MACRO-      IEAPSPIO                                             */ 00418000
*/*                                                                  */ 00419000
*/* STATUS-     VERSION DATE=10/24/73, VS/2 RELEASE 2                */ 00420000
*/*                                                                  */ 00421000
*/* FUNCTION-   IEAPSPIO IS PART OF THE  SUPERVISOR                  */ 00422000
*/*             INITIALIZATION PROCESS FOR VS2.                      */ 00423000
*/*                                                                  */ 00424000
*/*             THIS MACRO GENERATES INLINE PL/S STATEMENTS          */ 00425000
*/*             WHICH FORM A SUBROUTINE TO FIND AND READ TEXT        */ 00426000
*/*             RECORDS FROM SYS1.PARMLIB.                           */ 00427000
*/*                                                                  */ 00428000
*/*  REGISTER DEPENDENCIES-                                          */ 00429000
*/*                                                                  */ 00430000
*/*                 1. IEAPSPIO DEPENDS ON THE INCLUDING ROUTINES    */ 00431000
*/*                    BASE REGISTERS FOR ADDRESSABILITY             */ 00432000
*/*                                                                  */ 00433000
*/*                 2. REGISTER 3 MUST CONTAIN THE ADDRESS OF        */ 00434000
*/*                    THE CVT ON ENTRY                              */ 00435000
*/*                                                                  */ 00436000
*/*                 3. REGISTER 2 MUST CONTAIN THE ADDRESS OF        */ 00437000
*/*                    THE NVT ON ENTRY                              */ 00438000
*/*                                                                  */ 00439000
*/*                 4. REGISTER 14 MUST CONTAIN THE ADDRESS OF       */ 00440000
*/*                    THE CALLERS RETURN POINT ON ENTRY             */ 00441000
*/*                                                                  */ 00442000
*/* ENTRY POINTS-                                                    */ 00443000
*/*                                                                  */ 00444000
*/*    TWO CALLING SEQUENCRS ARE AVAILABLE.  ONE PROVIDES            */ 00445000
*/*    COMPATIBILITY WITH THE VS2-1 VERSION OF THIS MACRO,           */ 00446000
*/*    THE OTHER CALLING SEQUENCE WAS DEVELOPED EXPRESSLY            */ 00447000
*/*    FOR VS2-2.                                                    */ 00448000
*/*                                                                  */ 00449000
*/*        VS2-1 COMPATIBLE VERSION-                                 */ 00450000
*/*                                                                  */ 00451000
*/*            1. CALL NIPPMPDS TO LOCATE (VIA BLDL) THE MEMBER      */ 00452000
*/*               OF SYS1.PARMLIB                                    */ 00453000
*/*                                                                  */ 00454000
*/*            2. CALL NIPPMTXT TO READ A TEXT RECORD FROM           */ 00455000
*/*               SYS1.PARMLIB                                       */ 00456000
*/*                                                                  */ 00457000
*/*        VS2-2 VERSION-                                            */ 00458000
*/*                                                                  */ 00459000
*/*            1. CALL NIPPMMEM TO LOCATE THE MEMBER AND RETURN      */ 00460000
*/*               A TEXT RECORD                                      */ 00461000
*/*                                                                  */ 00462000
*/*            2. REPEAT CALL TO NIPPMMEM TO READ ADDITIONAL         */ 00463000
*/*               TEXT RECORDS                                       */ 00464000
*/*                                                                  */ 00465000
*/* OUTPUT (NORMAL) -                                                */ 00466000
*/*                                                                  */ 00467000
*/*     THE ROUTINE WILL SAVE AND RESTORE ALL GENERAL REGISTERS      */ 00468000
*/*     WITH THE EXCEPTION OF RPARM.  ON A SUCCESSFUL READ,          */ 00469000
*/*     RPARM (REGISTER 1) WILL BE SET TO POINT TO THE FIRST NON-    */ 00470000
*/*     BLANK CHARACTER IN THE TEXT RECORD.  IF THE RECORD           */ 00471000
*/*     COLUMNS 1 THRU 72 ALL CONTAIN BLANKS, RPARM WILL             */ 00472000
*/*     POINT TO A BLANK IN COLUMN 72.  IF COLUMNS 1 THRU 71         */ 00473000
*/*     ARE BLANK BUT COLUMN 72 IS NON-BLANK, THE ROUTINE            */ 00474000
*/*     WILL AUTOMATICALLY ATTEMPT TO READ THE NEXT RECORD.  IN      */ 00475000
*/*     ALL CASES, COLUMN 72 WILL BE SET TO BLANK AND COLUMN 73 WILL */ 00476000
*/*     BE SET TO X'FF'.                                             */ 00477000
*/*                                                                  */ 00478000
*/* EXIT (NORMAL)-                                                   */ 00479000
*/*                                                                  */ 00480000
*/*     VS2-1 AND VS2-2 VERSIONS RETURN TO THE ADDRESS FOUND         */ 00481000
*/*     IN REGISTER 14 ON ENTRY.                                     */ 00482000
*/*                                                                  */ 00483000
*/* EXIT (ERROR)-                                                    */ 00484000
*/*                                                                  */ 00485000
*/*     VS2-1 COMPATIBLE VERSION-                                    */ 00486000
*/*                                                                  */ 00487000
*/*         RETURN TO THE ADDRESS IN REGISTER 14 +4                  */ 00488000
*/*                                                                  */ 00489000
*/*         NOTE- WHEN AN ERROR OCCURS ON AN ATTEMP TO READ A RECORD */ 00490000
*/*               THAT DOES NOT EXIST (E-O-F),                       */ 00491000
*/*               IT IS CONSIDERED A PERMANENT I/O ERROR             */ 00492000
*/*                                                                  */ 00493000
*/*     VS2-2 VERSION-                                               */ 00494000
*/*                                                                  */ 00495000
*/*        RETURN TO THE ADDRESS FOUND IN REGISTER 14 ON ENTRY       */ 00496000
*/*        WITH REGISTER 1 (RPARM) SET TO A NEGATIVE VALUE.  IN THE  */ 00497000
*/*        CASE WHERE RPARM IS NEGATIVE ON RETURN, THE NIPPARC FIELD */ 00498000
*/*        OF THE NIP PATAMETER AREA WILL INDICATE THE REASON FOR    */ 00499000
*/*        FAILURE AS FOLLOWS-                                       */ 00500000
*/*                            4 - MEMBER NOT FOUND                  */ 00501000
*/*                            8 - I/O ERROR DURING BLDL             */ 00502000
*/*                            12- I/O ERROR DURING READ             */ 00503000
*/*                            16- END-OF-FILE                       */ 00504000
*/*                                                                  */ 00505000
*/*     COMMON TO BOTH VERSIONS-                                     */ 00506000
*/*                                                                  */ 00507000
*/*        AN INTERPRETIVE MESSAGE WILL BE WRITTEN TO THE OPERATOR   */ 00508000
*/*        FOR EACH OF THE FIRST THREE ERROR CONDITIONS LISTED       */ 00509000
*/*        ABOVE.  THE CALLER MAY SUPPRESS THE 'MEMBER NOT FOUND'    */ 00510000
*/*        MESSAGE BY SETTING THE NIPPAFL1 FLAG FIELD TO BINARY      */ 00511000
*/*        ONE IN THE NIP PARAMETER AREA BEFORE CALLING THIS         */ 00512000
*/*        ROUTINE.  NO MESSAGE WILL BE WRITTEN FOR THE E-O-F        */ 00513000
*/*        CONDITION IF THE VS2-2 VERSION IS USED.                   */ 00514000
*/*                                                                  */ 00515000
*/* MACROS-                                                          */ 00516000
*/*     THE FOLLOWING MAPPING MACROS MUST BE                         */ 00517000
*/*     INCLUDED IN THE MODULE WHICH INCLUDES                        */ 00518000
*/*     IEAPSPIO-                                                    */ 00519000
*/*                                       IEZIOB                     */ 00520000
*/*                                       IHANVT                     */ 00521000
*/*                                       IHAECB                     */ 00522000
*/*                                       IEAPPNIP                   */ 00523000
*/*                                       IEFUCBOB                   */ 00524000
*/*                                       CVT                        */ 00525000
*/*                                                                  */ 00526000
*/* NOTES-                                                           */ 00527000
*/*        THE FIRST FOUR LETTERS OF ALL IEAPSPIO SYMBOLES ARE:      */ 00528000
*/*        NIPP OR IPPM.                                             */ 00529000
*/*                                                                  */ 00530000
*/*        REGISTERS 0, 1, 14, AND 15 WILL BE UNRESTRICTED AT THE    */ 00531000
*/*        END OF THE SEGMENT GENERATED BY IEAPSPIO.                 */ 00532000
*/*                                                                  */ 00533000
*/*        NIPPAREA WILL BE BASED ON NVTPAREA AND IOBSTDRD WILL BE   */ 00534000
*/*        BASED ON THE ADDR OF NIPPAIOB AT THE END OF THE SEGMENT   */ 00535000
*/*        GENERATED BY IEAPSPIO                                     */ 00536000
*/*                                                                  */ 00537000
*/*        IEAPSPIO MUST BE INCLUDED IN A CODE SEGMENT,              */ 00538000
*/*        NOT AT THE TOP OF THE PROCEDURE WITH THE                  */ 00539000
*/*        MAPPING MACROS LISTED ABOVE.                              */ 00540000
*/*                                                            Y02752*/ 00541000
*/********************************************************************/ 00542000
*                                                                  0252 00543000
*                                                                  0252 00544000
*                                                                  0252 00545000
*          /* REGISTERS                                              */ 00546000
*          DCL NIPPMR00 REG(0) PTR(24);                            0252 00547000
*          DCL NIPPMR01 REG(1) PTR(24);                            0253 00548000
*          DCL NIPPMR02 REG(2) PTR(24);                            0254 00549000
*          DCL NIPPMR06 REG(6) PTR(24);                            0255 00550000
*          DCL NIPPMR09 REG(9) PTR(24);                            0256 00551000
*          DCL NIPPMR12 REG(12) PTR(24);                           0257 00552000
*          DCL NIPPMR13 REG(13) PTR(24);                           0258 00553000
*          DCL NIPPMR14 REG(14) PTR(24);                           0259 00554000
*          DCL NIPPMR15 REG(15) PTR(24);                           0260 00555000
*                                                                  0260 00556000
*          /* SAVE AREA FOR CALLER'S REGISTERS                       */ 00557000
*          DCL NIPPMREG (16) FIXED(31);                            0261 00558000
*                                                                  0261 00559000
*                                                                  0261 00560000
*          /* SAVE AREA FOR CALLER'S RETURN ADDRESS                  */ 00561000
*          DCL NIPPMADR PTR(31);                                   0262 00562000
*                                                                  0262 00563000
*          /* POINTER TO DCB FOR BLDL                                */ 00564000
*          DCL NIPPMDCB PTR(31);                                   0263 00565000
*                                                                  0263 00566000
*          /* POINTER TO BLDL HEADER FOR BLDL                        */ 00567000
*          DCL NIPPMBDL PTR(31);                                   0264 00568000
*                                                                  0264 00569000
*          /* NORMAL RETURN TO CALLER                                */ 00570000
*          DCL NIPPMAD0 LABEL BASED(NIPPMADR);                     0265 00571000
*                                                                  0265 00572000
*          /* ERROR RETURN TO CALLER                                 */ 00573000
*          DCL NIPPMAD4 LABEL BASED(NIPPMADR+4);                   0266 00574000
*                                                                  0266 00575000
*          /* ESTABLISH BASE FOR NIP COMMUNICATION AREA        Y02752*/ 00576000
*          RESPECIFY NIPPAREA BASED(NVTPAREA);              /* Y02752*/ 00577000
*                                                                  0267 00578000
*          /* ONE BYTE SCANNING FIELD                                */ 00579000
*          DCL NIPPMBYT CHAR(1) BASED;                             0268 00580000
*                                                                  0268 00581000
*          /* TEMPORARY POINTER                                Y02752*/ 00582000
*          DCL NIPPMKPR PTR(31);                            /* Y02752*/ 00583000
*                                                                  0269 00584000
*                                                                  0269 00585000
*          /* INPUT RECORD READ FROM PARMLIB                   Y02752*/ 00586000
*          DCL NIPPMBUF CHAR(80) BASED(NIPPMR01);           /* Y02752*/ 00587000
*                                                                  0270 00588000
*          /* MBBCCHHR OF PARMLIB MEMBERS                            */ 00589000
*          DCL 1 NIPPMMBC CHAR(8),                                 0271 00590000
*                2 NIPPMMBB CHAR(3),                               0271 00591000
*                2 NIPPMCCH CHAR(5);                               0271 00592000
*                                                                  0271 00593000
*          /* LOW ORDER 7 BYTES OF THE CSW STORED IN THE PARMLIB   0272 00594000
*             IOB BY EXCP                                      Y02752*/ 00595000
*          DCL 1 NIPPMCSW BASED(ADDR(IOBCSW)) BDY(BYTE),    /* Y02752*/ 00596000
*                2 * CHAR(3),                               /* Y02752*/ 00597000
*                2 NIPPMCS2 BIT(8),   /* I/O OPERATION STATUS FLAGS     00598000
*                                                           /* Y02752*/ 00599000
*                2 * CHAR(3);                               /* Y02752*/ 00600000
*                                                                  0272 00601000
*          /* MASK TO TEST THE STATUS FLAGS OF THE PARMLIB CSW Y02752*/ 00602000
*          DCL  NIPPMMK1 BIT(8) BDY(BYTE) INIT('11110010'B);/* Y02752*/ 00603000
*                                                                  0273 00604000
*          /* INTERNALLY GENERATED LABELS                            */ 00605000
*          DCL NIPPMMB1 LABEL GENERATED INTERNAL;                  0274 00606000
*          DCL NIPPMMB2 LABEL GENERATED INTERNAL;                  0275 00607000
*          DCL NIPPMTND LABEL GENERATED INTERNAL;                  0276 00608000
*                                                                  0276 00609000
*          /* MAPPING AREAS INTO ERROR MESSAGES                      */ 00610000
*          DCL NIPPMME1 CHAR(8) BASED(ADDR(NIPPMMB1));             0277 00611000
*          DCL NIPPMME2 CHAR(8) BASED(ADDR(NIPPMMB2));             0278 00612000
*          DCL NIPPMNAM CHAR(8) INIT(' '); /*MEMBER NAME SAVE AREA 0279 00613000
*                                                              Y02752*/ 00614000
*          DCL NIPPMNEG FIXED(31) INIT('FFFFFFFF'X); /*NEGATIVE VALUE   00615000
*                                                              Y02752*/ 00616000
*         RESPECIFY IOBSTDRD BASED(ADDR(NIPPAIOB));/* IOB BASE Y02752*/ 00617000
*                                                                  0281 00618000
*         RESPECIFY (NIPPMR14,NIPPMR15,NIPPMR00,NIPPMR01) RESTRICTED;/* 00619000
*                                                              Y02752*/ 00620000
*NIPPMMEM: NIPPMADR=NIPPMR14;  /* SAVE CALLER'S RETURN REGISTER    0283 00621000
*                                                              Y02752*/ 00622000
NIPPMMEM LR    NIPPMADR,NIPPMR14                                   0283 00623000
*         GENERATE REFS(NIPPMR01,NIPPMR02) SETS(NIPPMREG);         0284 00624000
          STM   NIPPMR02,NIPPMR01,NIPPMREG   SAVE REGISTERS     Y02752  00625000
*          NIPPARC=NIPPARC && NIPPARC; /*SET RETURN CODE TO ZERO   0285 00626000
*                                                              Y02752*/ 00627000
         L     @11,NVTPAREA(,RNVT)                                 0285 00628000
         L     @10,NIPPARC(,@11)                                   0285 00629000
         ST    @11,@TF00001                                        0285 00630000
         LR    @11,@10                                             0285 00631000
         XR    @11,@10                                             0285 00632000
         L     @10,@TF00001                                        0285 00633000
         ST    @11,NIPPARC(,@10)                                   0285 00634000
*          IF NIPPMNAM = NIPPANAM      /*IS THIS NEW MEMBER NAME   0286 00635000
*                                                              Y02752*/ 00636000
*              THEN GO TO NIPPMPRD;    /*NO- GO READ RECORD    Y02752*/ 00637000
         L     @11,@TF00001                                        0286 00638000
         CLC   NIPPMNAM(8),NIPPANAM(@11)                           0286 00639000
         BE    @RT00286                                            0286 00640000
*          NIPPMNAM = NIPPANAM;        /*YES- SAVE MEMBER NAME Y02752*/ 00641000
         L     @11,@TF00001                                        0288 00642000
         MVC   NIPPMNAM(8),NIPPANAM(@11)                           0288 00643000
*          GO TO NIPPMENT;             /*GO FIND MEMBER        Y02752*/ 00644000
*                                                                  0289 00645000
         B     NIPPMENT                                            0289 00646000
*NIPPMPDS: NIPPMADR = NIPPMR14;          /* SAVE CALLER'S RETURN ADDR*/ 00647000
NIPPMPDS LR    NIPPMADR,NIPPMR14                                   0290 00648000
*          GENERATE REFS(NIPPMR01,NIPPMR02) SETS(NIPPMREG);        0291 00649000
         STM   NIPPMR02,NIPPMR01,NIPPMREG       SAVE REGS       Y02752  00650000
*          NIPPMNAM = NIPPMNAM && NIPPMNAM;  /*CLEAR MEMBER NAME   0292 00651000
*                                                              Y02752*/ 00652000
         XC    NIPPMNAM(8),NIPPMNAM                                0292 00653000
*NIPPMENT: NIPPMR01 = ADDR(NIPPADCB) - 44;      /* R1= ADDR OF DCB   */ 00654000
NIPPMENT L     @11,NVTPAREA(,RNVT)                                 0293 00655000
         LA    NIPPMR01,NIPPADCB(,@11)                             0293 00656000
         SL    NIPPMR01,@CF01924                                   0293 00657000
*          NIPPMR00 = ADDR(NIPPABDL);    /* R0= ADDR OF BLDL LIST    */ 00658000
*                                                                  0294 00659000
         LA    NIPPMR00,NIPPABDL(,@11)                             0294 00660000
*          /* READ PDS ENTRY FOR MEMBER                              */ 00661000
*          GENERATE;                                               0295 00662000
          BLDL (1),(0)                                                  00663000
*          NIPPARC = NIPPMR15;   /* SET RETURN CODE            Y02752*/ 00664000
*                                                                  0296 00665000
         L     @11,NVTPAREA(,RNVT)                                 0296 00666000
         ST    NIPPMR15,NIPPARC(,@11)                              0296 00667000
*          IF NIPPMR15 = 4               /* WAS MEMBER NAME FOUND  0297 00668000
*                                                              Y02752*/ 00669000
*             THEN GO TO NIPPMPFN;       /* NO - TELL OPERATOR NOT 0297 00670000
*                                           FOUND                    */ 00671000
         C     NIPPMR15,@CF00039                                   0297 00672000
         BE    @RT00297                                            0297 00673000
*          IF NIPPMR15 = 8               /* WAS THERE AN I/O ERROR   */ 00674000
*             THEN GO TO NIPPMPER;       /* YES - TELL OPERATOR    0299 00675000
*                                           I/O ERROR                */ 00676000
*                                                                  0299 00677000
         C     NIPPMR15,@CF00034                                   0299 00678000
         BE    @RT00299                                            0299 00679000
*          /* SET UP PARAMETERS FOR CONVERSION ROUTINE               */ 00680000
*                                                                  0301 00681000
*          NIPPMR00 = NIPPATTR;          /* R0 = ACTUAL TTR          */ 00682000
         SLR   NIPPMR00,NIPPMR00                                   0301 00683000
         ICM   NIPPMR00,7,NIPPATTR(@11)                            0301 00684000
*          GENERATE;                                               0302 00685000
         SLL   0,8(0)                    ZERO LOW BYTE IN REG 0         00686000
*          NIPPMR01 = ADDR(NIPPADEB);    /* R1 = ADDR OF DEB         */ 00687000
         L     NIPPMR01,NVTPAREA(,RNVT)                            0303 00688000
         LA    NIPPMR01,NIPPADEB(,NIPPMR01)                        0303 00689000
*          NIPPMR15 = CVTPCNVT;          /* R15 = ADDR OF CONVERSION    00690000
*                                           ROUTINE            Y02752*/ 00691000
         L     NIPPMR15,CVTPCNVT(,RCVT)                            0304 00692000
*          /* CONVERT TTR TO CCHHR FORMAT                      Y02752*/ 00693000
*          GENERATE REFS(NIPPMR02,NIPPMR13)                        0305 00694000
*                    SETS(NIPPMR14,NIPPMR15,NIPPMMBC);     /*      0305 00695000
*                                                                  0305 00696000
         STM   NIPPMR02,NIPPMR13,NIPPMSAV SAVE REGISTERS        Y02752  00697000
         LA    NIPPMR02,NIPPMMBC          CONVERSION AREA       Y02752  00698000
         BALR  NIPPMR14,NIPPMR15          CALL CONVERT RTN      Y02752  00699000
**         RESTORE CSECT AND DSECT BASE REGISTERS               Y02752  00700000
*NIPPMRST  DS  0H                                               Y02752  00701000
NIPPMRST LM    NIPPMR02,NIPPMR13,NIPPMSAV-NIPPMRST(NIPPMR14)    Y02752  00702000
         BC    15,NIPPMSKP                SKIP SAVE AREA        Y02752  00703000
*NIPPMSAV  DS  0F                                               Y02752  00704000
NIPPMSAV DC    12F'0'                     DECLARE REG SAVE AREA Y02752  00705000
*NIPPMSKP DS  0H                                                Y02752  00706000
NIPPMSKP DS    0H                         RESUME EXECUTION      Y02752  00707000
*          IOBSEEK = NIPPMMBC;           /* RESET PARMLIB IOBSEEK TO    00708000
*                                           THIS MBBCCHHR            */ 00709000
         L     @11,NVTPAREA(,RNVT)                                 0306 00710000
         MVC   IOBSEEK+72(8,@11),NIPPMMBC                          0306 00711000
*          NIPPASID = NIPPMCCH;          /* RESET PARMLIB SEARCH ID     00712000
*                                           TO THIS CCHHR            */ 00713000
         MVC   NIPPASID(5,@11),NIPPMCCH                            0307 00714000
*          IF NIPPMNAM = NIPPANAM   /*SEE IF MEM NAMES MATCH   Y02752*/ 00715000
*              THEN GO TO NIPPMPRD; /*YES- NEW VERSION- GO READ    0308 00716000
*                                                              Y02752*/ 00717000
         CLC   NIPPMNAM(8),NIPPANAM(@11)                           0308 00718000
         BE    @RT00308                                            0308 00719000
*         GENERATE REFS(NIPPMR01,NIPPMR02) SETS(NIPPMREG);         0310 00720000
          LM    NIPPMR02,NIPPMR01,NIPPMREG    RESTORE REGS      Y02752  00721000
*          GO TO NIPPMAD0;               /* NORMAL RETURN TO CALLER     00722000
*                                                              Y02752*/ 00723000
*                                                                  0311 00724000
         BR    NIPPMADR                                            0311 00725000
*NIPPMPER: NIPPMME1 = NIPPANAM;          /* MOVE MEMBER NAME TO I/O     00726000
*                                           ERROR MESSAGE            */ 00727000
*                                                                  0312 00728000
NIPPMPER LA    @11,NIPPMMB1                                        0312 00729000
         L     @10,NVTPAREA(,RNVT)                                 0312 00730000
         MVC   NIPPMME1(8,@11),NIPPANAM(@10)                       0312 00731000
*          /* WTO I/O ERROR DURING BLDL MESSAGE                      */ 00732000
*          GENERATE REFS(NVTWTO) SETS(RPARM,REXIT,RENTRY);         0313 00733000
*                                                                  0313 00734000
          IEAPMNIP TYPE=WTO,PARAM=NIPPMMS1 CALL WTO SERVICE ROUTINE     00735000
*          GO TO NIPPMPXT;               /* ERROR RETURN TO CALLER   */ 00736000
*                                                                  0314 00737000
         B     NIPPMPXT                                            0314 00738000
*NIPPMPFN: IF NIPPMNAM = NIPPANAM  /*SEE IF MEMBER NAMES MATCH Y02752*/ 00739000
*              THEN IF NIPPAFL1 = '1'B  /*YES- SUPPRESS MESSAGE?   0315 00740000
*                                                              Y02752*/ 00741000
NIPPMPFN L     @11,NVTPAREA(,RNVT)                                 0315 00742000
         CLC   NIPPMNAM(8),NIPPANAM(@11)                           0315 00743000
         BNE   @RF00315                                            0315 00744000
*                 THEN GO TO NIPPMPXT;  /*YES- GO EXIT TO CALLER   0316 00745000
*                                                              Y02752*/ 00746000
         TM    NIPPAFL1(@11),B'10000000'                           0316 00747000
         BO    @RT00316                                            0316 00748000
*                 ELSE;                                     /* Y02752*/ 00749000
*          NIPPMME2 = NIPPANAM;          /* MOVE MEMBER NAME TO NOT     00750000
*                                           FOUND MESSAGE      Y02752*/ 00751000
*                                                                  0319 00752000
@RF00315 LA    @11,NIPPMMB2                                        0319 00753000
