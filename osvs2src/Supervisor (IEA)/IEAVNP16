*/* P GET CORE FOR THIS AMOUNT                                       */ 00355000
*        GEN (GETMAIN RU,LV=(0),SP=245); /* EXECUTE GETMAIN MACRO    */ 00356000
         GETMAIN RU,LV=(0),SP=245                                       00357000
*/* P MOVE OLD TABLE TO NEW & ZERO UNUSED BALANCE                    */ 00358000
*        WKREG0 = AIDPTR;              /* SOURCE PTR FOR MOVE LONG   */ 00359000
         LR    WKREG0,AIDPTR                                       0234 00360000
*        WKREGE = RPARM;     /* TARGET PTR FOR MOVE LONG     @ZA06784*/ 00361000
         LR    WKREGE,RPARM                                        0235 00362000
*        GEN (LH WKREG1,APPSIZE(,AIDPTR)); /* LEN OF SRC FLD @ZA06784*/ 00363000
         LH WKREG1,APPSIZE(,AIDPTR)                                     00364000
*        AIDPTR = WKREGE;    /* POINTER TO NEW TABLE         @ZA06784*/ 00365000
         LR    AIDPTR,WKREGE                                       0237 00366000
*        WKREGF = WKREG1;    /* LENGTH OF OLD TABLE          @ZA06784*/ 00367000
