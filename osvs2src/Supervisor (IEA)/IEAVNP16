         CR    @15,@09                                             0215 00295000
         BNH   @RF00215                                            0215 00296000
*                    DO;                                           0216 00297000
*                      IF (PRCOUNT * IDLNGTH) > APPBAL /* CHECK IF   */ 00298000
*                                      /* ENOUGH ROOM FOR THIS MOVE  */ 00299000
*                        THEN                                      0217 00300000
         ALR   @15,@15                                             0217 00301000
         CH    @15,APPBAL(,AIDPTR)                                 0217 00302000
         BNH   @RF00217                                            0217 00303000
*                          CALL EXTEND; /* NO, EXTEND THE NAME TABLE */ 00304000
         BAL   @14,EXTEND                                          0218 00305000
*                      APPCOUNT(ENTSUB) = PRCOUNT; /* COUNT OF NAMES */ 00306000
@RF00217 LR    @09,ENTSUB                                          0219 00307000
         SLA   @09,2                                               0219 00308000
