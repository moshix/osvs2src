         TITLE 'IEAVNP16--BUILD EXCP APPENDAGE NAME TABLE              *00001000
                        '                                               00002000
IEAVNP16 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVNP16  75.305'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF          00018000
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES      00019000
** WILL ASSEMBLE PROPERLY.                                              00020000
         EJECT                                                          00021000
*/* P INITIALIZE NIP SERVICE ROUTINE FOR READING PARMLIB             */ 00022000
*        NIPPANAM = 'IEAAPP00';        /* PROVIDE NAME OF MEMBER     */ 00023000
         L     @09,NVTPAREA(,RNVT)                                 0137 00024000
         MVC   NIPPANAM(8,@09),@CC01850                            0137 00025000
*/* S GETMAIN: GET CORE FOR NAME TABLE                               */ 00026000
*        WKREG0 = INITLNTH;            /* BEGIN WITH 128 BYTE TABLE  */ 00027000
         LA    WKREG0,128                                          0138 00028000
*        GEN (GETMAIN RU,LV=(0),SP=245); /* EXECUTE GETMAIN MACRO    */ 00029000
         GETMAIN RU,LV=(0),SP=245                                       00030000
*/* P STORE POINTER TO TABLE IN CVT                                  */ 00031000
*        AIDPTR = RPARM;               /* SET PTR TO APPENDAGE ID TBL*/ 00032000
         LR    AIDPTR,RPARM                                        0140 00033000
*        CVTAIDVT = AIDPTR;            /* STORE IN CVT               */ 00034000
         ST    AIDPTR,CVTAIDVT(,RCVT)                              0141 00035000
*/* P ZERO TABLE, FILL IN HEADER FIELDS                              */ 00036000
*        APPNMTBL (1:128) = '00'X;     /* ZERO OUT THE TABLE         */ 00037000
         XC    APPNMTBL+1(127,AIDPTR),APPNMTBL+1(AIDPTR)           0142 00038000
         MVI   APPNMTBL(AIDPTR),X'00'                              0142 00039000
*        APPSIZE = INITLNTH;           /* INITIAL TABLE SIZE IS 128  */ 00040000
         LA    @09,128                                             0143 00041000
         STH   @09,APPSIZE(,AIDPTR)                                0143 00042000
*        TGTPTR = ADDR(APPNAMID);      /* INITIALIZE TARGET POINTER  */ 00043000
         LA    TGTPTR,APPNAMID(,AIDPTR)                            0144 00044000
*        APPBAL = APPSIZE - (TGTPTR - AIDPTR); /* BAL OF UNUSED CORE */ 00045000
         LR    @15,TGTPTR                                          0145 00046000
         SLR   @15,AIDPTR                                          0145 00047000
         SLR   @09,@15                                             0145 00048000
         STH   @09,APPBAL(,AIDPTR)                                 0145 00049000
*/*GETFIRST: I GET FIRST RECORD FROM IEAAPP00                        */ 00050000
*GETFIRST: CALL NIPPMMEM;              /* READ PARMLIB               */ 00051000
GETFIRST BAL   @14,NIPPMMEM                                        0146 00052000
*/* D (YES,EXCEPTN:RRT,NO,) EXCEPTION RETURN CODE?                   */ 00053000
*        IF WKREG1 < 0                 /* TEST FOR AN ERROR RETURN   */ 00054000
*          THEN                        /* ERROR IF REG 1 IS NEGATIVE */ 00055000
         LTR   WKREG1,WKREG1                                       0147 00056000
         BM    @RT00147                                            0147 00057000
*            GO TO EXCEPTN;            /* GO ANALYZE THE EXCEPTION   */ 00058000
*/* D (NO,GETFIRST,YES,) VALID ENTRY KEYWORD?                        */ 00059000
*        SRCPTR = RPARM;               /* SET POINTER TO SOURCE TEXT */ 00060000
         LR    SRCPTR,RPARM                                        0149 00061000
*        DO ENTSUB = 1 TO 5;           /* ESTABLISH ENTRY SUBSCRIPT  */ 00062000
         LA    ENTSUB,1                                            0150 00063000
@DL00150 DS    0H                                                  0151 00064000
*          IF IPTYPE = APPTYPE(ENTSUB) /* MATCH INPUT TO CONSTANTS   */ 00065000
*            THEN                      /* CHK IF VALID ENTRY KEYWORD */ 00066000
         LR    @09,ENTSUB                                          0151 00067000
         MH    @09,@CH00080                                        0151 00068000
         LA    @01,APPTYPE-7(@09)                                  0151 00069000
         CLC   IPTYPE(7,SRCPTR),0(@01)                             0151 00070000
         BNE   @RF00151                                            0151 00071000
*              DO;                     /* CONTINUE IF VALID          */ 00072000
*/* P SET SOURCE POINTER TO FIRST INPUT NAME                         */ 00073000
*SETSRC:         SRCPTR = SRCPTR + LENGTH(APPTYPE); /* STEP PAST     */ 00074000
SETSRC   AL    SRCPTR,@CF00080                                     0153 00075000
*                                      /* THE APPENDAGE TYPE KEYWORD */ 00076000
*                DO WKREG1 = 1 TO 64;  /* SCAN THE NEXT 64 COLUMNS   */ 00077000
         LA    WKREG1,1                                            0154 00078000
@DL00154 DS    0H                                                  0155 00079000
*                  IF IPNAMID (1:1) ^= ' ' /* FIND 1ST NON-BLANK     */ 00080000
*                    THEN              /* THEN TEST IF COLUMN 73     */ 00081000
         CLI   IPNAMID(SRCPTR),C' '                                0155 00082000
         BE    @RF00155                                            0155 00083000
*                      IF IPNAMID (1:1) ^= 'FF'X /* IF NOT COL 73    */ 00084000
*                        THEN          /* THEN GO INITIALIZE TYPE    */ 00085000
         CLI   IPNAMID(SRCPTR),X'FF'                               0156 00086000
         BNE   @RT00156                                            0156 00087000
*                          GO TO INIT; /* GO INITIALIZE THIS TYPE    */ 00088000
*                      ELSE            /* OTHERWISE CARD IS BLANK    */ 00089000
*                          GO TO WRAPUP; /* RECORD ALL BLANK - QUIT  */ 00090000
         B     WRAPUP                                              0158 00091000
*                  ELSE                /* OTHERWISE COMPRESS BLANK   */ 00092000
*                      SRCPTR = SRCPTR + 1; /* COMPRESS THIS BLANK   */ 00093000
@RF00155 AL    SRCPTR,@CF00043                                     0159 00094000
*                END;                                              0160 00095000
         AL    WKREG1,@CF00043                                     0160 00096000
         C     WKREG1,@CF01857                                     0160 00097000
         BNH   @DL00154                                            0160 00098000
*              END;                                                0161 00099000
*        END;                                                      0162 00100000
@RF00151 AL    ENTSUB,@CF00043                                     0162 00101000
         C     ENTSUB,@CF00071                                     0162 00102000
         BNH   @DL00150                                            0162 00103000
*        GO TO GETFIRST;               /* MUST START WITH KEYWORD    */ 00104000
         B     GETFIRST                                            0163 00105000
*/*INIT: P SET OFFSET TO FIRST NAME THIS TYPE                        */ 00106000
*INIT:   APPOFFST(ENTSUB) = TGTPTR - ADDR(APPNAMID);               0164 00107000
INIT     LR    @09,ENTSUB                                          0164 00108000
         SLA   @09,2                                               0164 00109000
         LA    @15,APPNAMID(,AIDPTR)                               0164 00110000
         LCR   @15,@15                                             0164 00111000
         ALR   @15,TGTPTR                                          0164 00112000
         STH   @15,APPOFFST-4(@09,AIDPTR)                          0164 00113000
*/* D (YES,CPYPRIME,NO,) ROOM FOR NAMES FROM PRIME TABLE?            */ 00114000
*        TBLPTR = APPLIST(ENTSUB);     /* FIND ENTRY IN PRIME TABLE  */ 00115000
         L     TBLPTR,APPLIST-4(@09)                               0165 00116000
*        IF (PRCOUNT * IDLNGTH) <= APPBAL /* ROOM FOR THIS MOVE?     */ 00117000
*          THEN                        /* TEST IF ROOM FOR MOVE      */ 00118000
         LH    @09,PRCOUNT(,TBLPTR)                                0166 00119000
         ALR   @09,@09                                             0166 00120000
         CH    @09,APPBAL(,AIDPTR)                                 0166 00121000
         BNH   @RT00166                                            0166 00122000
*            GO TO CPYPRIME;           /* YES, COPY PRIME TABLE      */ 00123000
*/* S EXTEND: EXTEND THE NAME TABLE                                  */ 00124000
*        CALL EXTEND;                  /* CALL SUBROUTINE TO EXTEND  */ 00125000
         BAL   @14,EXTEND                                          0168 00126000
*/*CPYPRIME: P MOVE COUNT & NAMES FROM PRIME TABLE TO NAME TABLE     */ 00127000
*CPYPRIME: APPCOUNT(ENTSUB) = PRCOUNT; /* COPY COUNT FIELD           */ 00128000
CPYPRIME LR    @09,ENTSUB                                          0169 00129000
         SLA   @09,2                                               0169 00130000
         LH    @15,PRCOUNT(,TBLPTR)                                0169 00131000
         STH   @15,APPCOUNT-4(@09,AIDPTR)                          0169 00132000
*        IF PRCOUNT = 0                /* ANY NAMES IN PRIME TABLE?  */ 00133000
*          THEN                        /* TEST IF NAMES IN PRIME TBL */ 00134000
         LTR   @15,@15                                             0170 00135000
         BZ    @RT00170                                            0170 00136000
*            GO TO MOVEIDS;            /* NO, GO MOVE FROM PARMLIB   */ 00137000
*        MOVLEN = PRCOUNT * IDLNGTH;   /* SET LENGTH OF MOVE         */ 00138000
         ALR   @15,@15                                             0172 00139000
         LR    MOVLEN,@15                                          0172 00140000
*        OPNAMID (1:MOVLEN) = PRNAMES (1:MOVLEN); /* MOVE NAMES      */ 00141000
         LR    @09,MOVLEN                                          0173 00142000
         BCTR  @09,0                                               0173 00143000
         EX    @09,@SM01951                                        0173 00144000
*        TGTPTR = TGTPTR + MOVLEN;     /* STEP TARGET POINTER        */ 00145000
         ALR   TGTPTR,MOVLEN                                       0174 00146000
*        APPBAL = APPBAL - MOVLEN;     /* DECREMENT BALANCE OF CORE  */ 00147000
         LH    @09,APPBAL(,AIDPTR)                                 0175 00148000
         SLR   @09,MOVLEN                                          0175 00149000
         STH   @09,APPBAL(,AIDPTR)                                 0175 00150000
*/*MOVEIDS: P DECREMENT BALANCE OF UNUSED CORE FOR NEXT NAME         */ 00151000
*MOVEIDS:  APPBAL = APPBAL - IDLNGTH;  /* SUBTRACT SIZE OF 1 NAME    */ 00152000
MOVEIDS  LH    @09,APPBAL(,AIDPTR)                                 0176 00153000
         BCTR  @09,0                                               0176 00154000
         BCTR  @09,0                                               0176 00155000
         STH   @09,APPBAL(,AIDPTR)                                 0176 00156000
*/* D (YES,MOVEID1,NO,) ROOM FOR ONE MORE NAME?                      */ 00157000
*/* S EXTEND: EXTEND THE NAME TABLE                                  */ 00158000
*        IF APPBAL < 0                 /* IF END OF TABLE            */ 00159000
*          THEN                        /* TEST IF AT END OF TABLE    */ 00160000
         LTR   @09,@09                                             0177 00161000
         BNM   @RF00177                                            0177 00162000
*            CALL EXTEND;              /* YES, GO TO EXTEND ROUTINE  */ 00163000
         BAL   @14,EXTEND                                          0178 00164000
*/*MOVEID1: P MOVE ONE NAME ID TO TABLE IF VALID                     */ 00165000
*MOVEID1:                              /* IS NAME ID VALID     YM2893*/ 00166000
*        IF IPNAMID (1:1) ^= CHARW &   /* IS 1ST CHAR VALID    YM5776*/ 00167000
*           IPNAMID (1:1) ^= CHARX &   /* IS 1ST CHAR VALID    YM5776*/ 00168000
*           IPNAMID (1:1) ^= CHARY &   /* IS 1ST CHAR VALID    YM5776*/ 00169000
*           IPNAMID (1:1) ^= CHARZ     /* IS 1ST CHAR VALID    YM2893*/ 00170000
*          THEN                        /* IF NOT W, X, Y OR Z  YM2893*/ 00171000
@RF00177 DS    0H                                                  0179 00172000
MOVEID1  CLI   IPNAMID(SRCPTR),C'W'                                0179 00173000
         BE    @RF00179                                            0179 00174000
         CLI   IPNAMID(SRCPTR),C'X'                                0179 00175000
         BE    @RF00179                                            0179 00176000
         CLI   IPNAMID(SRCPTR),C'Y'                                0179 00177000
         BE    @RF00179                                            0179 00178000
         CLI   IPNAMID(SRCPTR),C'Z'                                0179 00179000
         BNE   @RT00179                                            0179 00180000
*            GO TO WRAPUP;             /* WRAP UP TABLE BUILD  YM2893*/ 00181000
*        DO  WKREG1 = 36 TO 1 BY -1;   /* IS 2ND CHAR VALID    YM2893*/ 00182000
@RF00179 LA    WKREG1,36                                           0181 00183000
@DL00181 DS    0H                                                  0182 00184000
*          IF IPNAMID (2:2) = VALIDTBL(WKREG1) /* IS IT VALID  YM2893*/ 00185000
*            THEN                      /* VALID IF IN TABLE    YM2893*/ 00186000
