         CLC   IPNAMID+1(1,SRCPTR),0(@09)                          0182 00188000
         BE    @RT00182                                            0182 00189000
*              GO TO MOVEID2;          /* IF VALID, GO MOVE ID YM2893*/ 00190000
*        END;                          /* IF 2ND IS NOT VALID, YM2893*/ 00191000
         BCTR  WKREG1,0                                            0184 00192000
         LTR   WKREG1,WKREG1                                       0184 00193000
         BP    @DL00181                                            0184 00194000
*        GO TO WRAPUP;                 /* WRAP UP TABLE BUILD  YM2893*/ 00195000
         B     WRAPUP                                              0185 00196000
*MOVEID2:                              /* MOVE ID TO APP TABLE YM2893*/ 00197000
*        OPNAMID = IPNAMID;            /* MOVE ONE ID TO TABLE YM2893*/ 00198000
MOVEID2  MVC   OPNAMID(2,TGTPTR),IPNAMID(SRCPTR)                   0186 00199000
*/* P INCREMENT COUNT OF NAMES                                       */ 00200000
*        APPCOUNT(ENTSUB) = APPCOUNT(ENTSUB) + 1; /* BUMP BY ONE     */ 00201000
         LR    @09,ENTSUB                                          0187 00202000
         SLA   @09,2                                               0187 00203000
         LA    @15,1                                               0187 00204000
         LH    @14,APPCOUNT-4(@09,AIDPTR)                          0187 00205000
         ALR   @14,@15                                             0187 00206000
         STH   @14,APPCOUNT-4(@09,AIDPTR)                          0187 00207000
