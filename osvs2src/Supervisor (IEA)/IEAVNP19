*       ICBSAMP=SAMPDEF;            /* PROVIDE DEFAULT       @Y30LB04*/ 00513000
*                                                                  0339 00514000
         MVC   ICBSAMP(2,@14),SAMPDEF                              0339 00515000
*       /*************************************************************/ 00516000
*       /*                                                           */ 00517000
*       /* ACTIVITY SAMPLING TIME                            @Y30LB04*/ 00518000
*       /*                                                           */ 00519000
*       /*************************************************************/ 00520000
*                                                                  0340 00521000
*       ICBV=ON;                    /* ASSUME MSVC WILL INITIALIZE 0340 00522000
*                                                            @Y30LB04*/ 00523000
*                                                                  0340 00524000
         OI    ICBV(@14),B'00100000'                               0340 00525000
*       /*************************************************************/ 00526000
*       /*                                                           */ 00527000
*       /* NOTE -- MSVC INITIALIZATION TURNS THIS BIT OFF IF @Y30LB04*/ 00528000
*       /* UNSUCCESSFUL -- BIT MUST BE ON UNTIL MSVC         @Y30LB04*/ 00529000
*       /* INITIALIZATION EXECUTES                           @Y30LB04*/ 00530000
*       /* GET AND CLEAR STORAGE FOR MSSC SRB                @Y30LB04*/ 00531000
*       /*                                                           */ 00532000
*       /*************************************************************/ 00533000
*                                                                  0341 00534000
*       R0=LENGTH(SRB);             /* AMOUNT OF STORAGE     @Y30LB04*/ 00535000
         LA    R0,44                                               0341 00536000
*       R1=ZERO;                    /*                       @Y30LB04*/ 00537000
         SLR   R1,R1                                               0342 00538000
*       R15=SP245;                  /* SP 245, CONDITIONAL GET     0343 00539000
*                                                            @Y30LB04*/ 00540000
         LA    R15,245                                             0343 00541000
*IGETMAI2:                          /* GET CONTROL BLOCK STORAGE   0344 00542000
*                                                            @Y30LB04*/ 00543000
*       GEN REFS(R0,R1,R15) SETS(R0,R1,R15);                       0344 00544000
IGETMAI2 DS    0H                                                  0344 00545000
*                                     /* WITH GETMAIN MACRO @Y30LB04*/  00546000
