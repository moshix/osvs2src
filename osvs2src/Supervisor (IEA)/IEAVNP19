*     IF UCBPTR^=ZERO&UCBPTR^=FOXES THEN/* VALID UCB ADDR?   @ZDR2048*/ 02296000
         LH    @05,UCBPTR                                          0753 02297000
         N     @05,@CF02747                                        0753 02298000
         LTR   @05,@05                                             0753 02299000
         BZ    @RF00753                                            0753 02300000
         SLR   @15,@15                                             0753 02301000
         ICM   @15,3,FOXES                                         0753 02302000
         CR    @05,@15                                             0753 02303000
         BE    @RF00753                                            0753 02304000
*       DO;                         /* VALID UCB             @ZDR2048*/ 02305000
*                                                                  0754 02306000
*         /***********************************************************/ 02307000
*         /*                                                         */ 02308000
*         /* TEST IF THE UCB IS VIRTUAL DASD                 @ZDR2048*/ 02309000
*         /*                                                         */ 02310000
*         /***********************************************************/ 02311000
*                                                                  0755 02312000
*         IF UCB3DACC=ON&UCBUNTYP='09'X&UCBRVDEV=ON/*        @ZDR2048*/ 02313000
*           THEN                    /*                       @ZDR2048*/ 02314000
         TM    UCB3DACC(@05),B'00100000'                           0755 02315000
         BNO   @RF00755                                            0755 02316000
         CLI   UCBUNTYP(@05),X'09'                                 0755 02317000
         BNE   @RF00755                                            0755 02318000
         TM    UCBRVDEV(@05),B'00001000'                           0755 02319000
         BNO   @RF00755                                            0755 02320000
*           DO;                     /* VIR UCB TO PROCESS    @ZDR2048*/ 02321000
*             IF FAILCALL=ON THEN   /* CALLED BY             @ZDR2048   02322000
*                                      NP19FAIL SUB          @ZDR2048*/ 02323000
         TM    FAILCALL,B'01000000'                                0757 02324000
         BNO   @RF00757                                            0757 02325000
*               DO;                 /* NO MSS CLEAR          @ZDR2048   02326000
*                                      UCB CODE              @ZDR2048*/ 02327000
*                 UCBCHM=C1ZERO;    /*                       @ZDR2048   02328000
*                                      CLEAR CHANNEL MASK    @ZDR2048*/ 02329000
         MVI   UCBCHM(@05),X'00'                                   0759 02330000
*                 UCBNRY=ON;        /*                       @ZDR2048   02331000
*                                      SET NOT ENTRY         @ZDR2048*/ 02332000
         OI    UCBNRY(@05),B'01000000'                             0760 02333000
*                 UCBONLI=OFF;      /*                       @ZDR2048   02334000
*                                      SET TO OFFLINE        @ZDR2048*/ 02335000
         NI    UCBONLI(@05),B'01111111'                            0761 02336000
*               END;                /*                       @ZDR2048*/ 02337000
*             ELSE                  /* CALLED BY MIANLINE    @ZDR2048*/ 02338000
*               DO;                 /* OFFLINE DEMOUNT CODE  @ZDR2048*/ 02339000
*                                                                  0763 02340000
         B     @RC00757                                            0763 02341000
@RF00757 DS    0H                                                  0764 02342000
*                 /***************************************************/ 02343000
*                 /*                                                 */ 02344000
*                 /* CHECK FOR OFFLINE OR NULL VOLID BEFORE DEMOUNT  */ 02345000
*                 /*                                         @G24LB02*/ 02346000
*                 /*                                                 */ 02347000
*                 /***************************************************/ 02348000
*                                                                  0764 02349000
*                 IF UCBONLI=OFF|UCBVOLI=NULVOLI THEN              0764 02350000
         LH    @05,UCBPTR                                          0764 02351000
         N     @05,@CF02747                                        0764 02352000
