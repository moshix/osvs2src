         TITLE 'IEAVNP19 -- THE MSS RIM                                *00001000
                        '                                               00002000
IEAVNP19 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVNP19  78.166'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @04,0                                               0001 00011000
@PSTART  LA    @12,4095(,@04)                                      0001 00012000
         USING @PSTART,@04                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
         ST    @13,@SA00001+4                                      0001 00015000
         LA    @14,@SA00001                                        0001 00016000
         ST    @14,8(,@13)                                         0001 00017000
         LR    @13,@14                                             0001 00018000
*                                                                       00019000
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF           00020000
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES       00021000
* WILL ASSEMBLE PROPERLY.                                               00022000
*   NVTPTR=RNVT;                    /* FIND THE NVT          @G24LB02*/ 00023000
         ST    RNVT,NVTPTR                                         0218 00024000
*   TERMERR=OFF;                    /* CLEAR ERR SWITCH      @G24LB02*/ 00025000
*   MINERR=OFF;                     /* CLEAR MINOR ERROR SWITCH    0220 00026000
*                                                            @G24LB02*/ 00027000
         NI    TERMERR,B'00111111'                                 0220 00028000
*   FAILCALL=OFF;                   /* CLEAR SWITCH          @ZDR2048*/ 00029000
         NI    FAILCALL,B'10111111'                                0221 00030000
*   RESPECIFY                                                      0222 00031000
*     NVT BASED(NVTPTR);            /*                       @G24LB02*/ 00032000
*   RFY                                                            0223 00033000
*    (RCVT,                                                        0223 00034000
*     RNVT) UNRSTD;                 /* INPUT REGISTERS       @G24LB02*/ 00035000
*   RESPECIFY                                                      0224 00036000
*     PARMTAB BASED(NVTPTAB);       /* & FOR PARAMETERS      @G24LB02*/ 00037000
*   RESPECIFY                                                      0225 00038000
*     NIPPAREA BASED(NVTPAREA);     /*                       @G24LB02*/ 00039000
*   RESPECIFY                                                      0226 00040000
*     NIPPTE BASED(PURGE);          /*                       @G24LB02*/ 00041000
*   IF PURGE^=NOPURG THEN                                          0227 00042000
         L     @05,NVTPTR                                          0227 00043000
         L     @05,NVTPTAB(,@05)                                   0227 00044000
         L     @05,PURGE(,@05)                                     0227 00045000
         LTR   @05,@05                                             0227 00046000
         BZ    @RF00227                                            0227 00047000
*     DO;                                                          0228 00048000
*       PURGBIT=ON;                 /* SET THE PURGE INDICATOR ON  0229 00049000
*                                                            @G24LB02*/ 00050000
         OI    PURGBIT,B'10000000'                                 0229 00051000
*     END;                                                         0230 00052000
*                                                                  0230 00053000
*/********************************************************** @G24LB02*/ 00054000
*                                                                  0231 00055000
*   CALL MSCUCBS;                   /* FIND OUT IF THERE ARE ANY MSCS   00056000
*                                                            @G24LB02*/ 00057000
@RF00227 BAL   @14,MSCUCBS                                         0231 00058000
*/********************************************************** @G24LB02*/ 00059000
*                                                                  0232 00060000
*   IF MSCUCBA1^=NOMSCS             /* ARE THERE ANY MSCS ?  @G24LB02*/ 00061000
*     THEN                          /* AT LEAST 1 MSC SYSGENED     0232 00062000
*                                                            @G24LB02*/ 00063000
         L     @05,MSCUCBA1                                        0232 00064000
         LTR   @05,@05                                             0232 00065000
         BZ    @RF00232                                            0232 00066000
*     DO;                           /* THIS PATH FOR MSS USE @G24LB02*/ 00067000
*/********************************************************** @G24LB02*/ 00068000
*                                                                  0234 00069000
*       CALL CBSETUP;               /* SET UP MSSC CONTROL BLOCK   0234 00070000
*                                                            @G24LB02*/ 00071000
         BAL   @14,CBSETUP                                         0234 00072000
*/********************************************************** @G24LB02*/ 00073000
*                                                                  0235 00074000
*       IF CLEANCOD=ALLCLEAN THEN   /* ANY ERROR SO FAR ?    @G24LB02*/ 00075000
         L     @05,CLEANCOD                                        0235 00076000
         LTR   @05,@05                                             0235 00077000
         BNZ   @RF00235                                            0235 00078000
*         DO;                       /*                       @G24LB02*/ 00079000
*/********************************************************** @G24LB02*/ 00080000
*                                                                  0237 00081000
*           CALL ADDRESSS;          /* FILL IN ADDRESSES FOR MSSC  0237 00082000
*                                                            @G24LB02*/ 00083000
         BAL   @14,ADDRESSS                                        0237 00084000
*/********************************************************** @G24LB02*/ 00085000
*                                                                  0238 00086000
*         END;                      /*                       @G24LB02*/ 00087000
*       ELSE                        /* SET TERMINATION ERROR SWICTH     00088000
*                                      ON                    @G24LB02*/ 00089000
*         TERMERR=ON;               /* SET TERMINAL ERR ON   @G24LB02*/ 00090000
         B     @RC00235                                            0239 00091000
@RF00235 OI    TERMERR,B'01000000'                                 0239 00092000
*       IF CLEANCOD=ALLCLEAN THEN   /* ANY ERROR SO FAR ?    @G24LB02*/ 00093000
@RC00235 L     @05,CLEANCOD                                        0240 00094000
         LTR   @05,@05                                             0240 00095000
         BNZ   @RF00240                                            0240 00096000
*         DO;                       /* SET UP THE IOS ATTENTION INDEX   00097000
*                                                            @G24LB02*/ 00098000
*           CALL ATTENTNX;          /*                       @G24LB02*/ 00099000
         BAL   @14,ATTENTNX                                        0242 00100000
*         END;                      /*                       @G24LB02*/ 00101000
*       ELSE                        /* CONTINUE WITH MSSC          0244 00102000
*                                      INITIALIZATION        @G24LB02*/ 00103000
*         TERMERR=ON;               /* SET TERMINAL ERR ON   @G24LB02*/ 00104000
         B     @RC00240                                            0244 00105000
@RF00240 OI    TERMERR,B'01000000'                                 0244 00106000
*       IF CLEANCOD=ALLCLEAN THEN   /* ANY ERROR SO FAR ?    @G24LB02*/ 00107000
@RC00240 L     @05,CLEANCOD                                        0245 00108000
         LTR   @05,@05                                             0245 00109000
         BNZ   @RF00245                                            0245 00110000
*         DO;                       /* SET UP THE IOS ATTENTION INDEX   00111000
*                                                            @G24LB02*/ 00112000
*/********************************************************** @G24LB02*/ 00113000
*                                                                  0247 00114000
*           CALL MSG05LCD;          /* INITIALIZE THE MSC(S) @G24LB02*/ 00115000
         BAL   @14,MSG05LCD                                        0247 00116000
*           IF MINERR=ON THEN                                      0248 00117000
         TM    MINERR,B'10000000'                                  0248 00118000
         BNO   @RF00248                                            0248 00119000
*             ICBB=ON;              /*                       @G24LB02*/ 00120000
         L     @05,CVTPTR                                          0249 00121000
         L     @05,CVTEXT2(,@05)                                   0249 00122000
         L     @05,CVTICB(,@05)                                    0249 00123000
         OI    ICBB(@05),B'00010000'                               0249 00124000
*/********************************************************** @G24LB02*/ 00125000
*                                                                  0250 00126000
*         END;                      /*                       @G24LB02*/ 00127000
@RF00248 DS    0H                                                  0251 00128000
*       IF TERMERR=OFF THEN         /* WAS TERMINAL ERR SW SET?    0251 00129000
*                                                            @G24LB02*/ 00130000
@RF00245 TM    TERMERR,B'01000000'                                 0251 00131000
         BNZ   @RF00251                                            0251 00132000
*         DO;                       /*                       @G24LB02*/ 00133000
*           CALL VVOLUMES;          /*                       @G24LB02*/ 00134000
*                                                                  0253 00135000
         BAL   @14,VVOLUMES                                        0253 00136000
*           /*********************************************************/ 00137000
*           /*                                                       */ 00138000
*           /* VOLZERS ISSUES DEMOUNT VUA FOR OFFLINE VUCB(S) AND    */ 00139000
*           /*                                               @G24LB02*/ 00140000
*           /* SHOULD ONLY DO SO IF WE HAD A NORMAL INITIALIZATION   */ 00141000
*           /* AND WE DID NOT PURGE                          @G24LB02*/ 00142000
*           /*                                                       */ 00143000
*           /*********************************************************/ 00144000
*                                                                  0254 00145000
*           IF ICBB=OFF&PURGBIT=OFF THEN                           0254 00146000
         L     @05,CVTPTR                                          0254 00147000
         L     @05,CVTEXT2(,@05)                                   0254 00148000
         L     @05,CVTICB(,@05)                                    0254 00149000
         TM    ICBB(@05),B'00010000'                               0254 00150000
         BNZ   @RF00254                                            0254 00151000
         TM    PURGBIT,B'10000000'                                 0254 00152000
         BNZ   @RF00254                                            0254 00153000
*             DO;                   /*                       @G24LB02*/ 00154000
*               CALL VOLZERS;       /*                       @G24LB02*/ 00155000
         BAL   @14,VOLZERS                                         0256 00156000
*             END;                  /*                       @G24LB02*/ 00157000
*         END;                      /*                       @G24LB02*/ 00158000
@RF00254 DS    0H                                                  0259 00159000
*NOMSSC:                            /* CHECK IF NO MSSC      @G24LB02*/ 00160000
*       IF CLEANCOD^=ALLCLEAN       /* ANY MSSC OR MSC PROBLEM ?   0259 00161000
*                                                            @G24LB02*/ 00162000
*         THEN                      /* UNABLE TO USE MSSC OR 3850  0259 00163000
*                                                            @G24LB02*/ 00164000
@RF00251 DS    0H                                                  0259 00165000
NOMSSC   L     @05,CLEANCOD                                        0259 00166000
         LTR   @05,@05                                             0259 00167000
         BZ    @RF00259                                            0259 00168000
*         CALL NP19FAIL;            /* INVOKE FAILURE PROCESSING   0260 00169000
*                                                            @G24LB02*/ 00170000
         BAL   @14,NP19FAIL                                        0260 00171000
*       ELSE                                                       0261 00172000
*         ;                         /* 3850 IS READY FOR VIRTUAL   0261 00173000
*                                      VOLUME MOUNTS         @G24LB02*/ 00174000
@RF00259 DS    0H                                                  0262 00175000
*     END;                          /*                       @G24LB02*/ 00176000
*   ELSE                                                           0263 00177000
*     ;                             /* NO MSC UCBS -- JUST RETURN  0263 00178000
*                                                            @G24LB02*/ 00179000
@RF00232 DS    0H                                                  0264 00180000
*                                                                  0264 00181000
*/********************************************************** @G24LB02*/ 00182000
*/*                                                          @G24LB02*/ 00183000
*/*  PROCEDURE NAME - MSG05LCD                               @G24LB02*/ 00184000
*/*                                                          @G24LB02*/ 00185000
*/*  FUNCTION - THIS ROUTINE SETS UP ARGUMENTS FOR AND       @G24LB02*/ 00186000
*/*             INVOKES THE LOAD MACRO.  THE LOAD MACRO      @G24LB02*/ 00187000
*/*             WILL CAUSE ICBMSG05 TO BE LOADED INTO THE    @G24LB02*/ 00188000
*/*             SYSTEM. AFTER THE LOAD IS COMPLETED,         @G24LB02*/ 00189000
*/*             IS CALLED TO DO MSC INITIALIZATION.          @G24LB02*/ 00190000
*/*             THE DELETE MACRO IS USED TO DELETE ICBMSG05. @G24LB02*/ 00191000
*/*                                                          @G24LB02*/ 00192000
*/*  INPUT - PARAMETERS ARE SET IN THIS ROUTINE              @G24LB02*/ 00193000
*/*                                                          @G24LB02*/ 00194000
*/*  OUTPUT - R15 IS SET TO THE ADDRESS OF ICBMSG05          @G24LB02*/ 00195000
*/*           PASS THE PURGE PARAMETER TO ICBMSG05           @G24LB02*/ 00196000
*/********************************************************   @G24LB02*/ 00197000
*                                                                  0264 00198000
*MSG05LCD:                                                         0264 00199000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @G24LB02*/ 00200000
@EL00001 L     @13,4(,@13)                                         0264 00201000
@EF00001 DS    0H                                                  0264 00202000
@ER00001 LM    @14,@12,12(@13)                                     0264 00203000
         BR    @14                                                 0264 00204000
MSG05LCD DS    0H                                                  0265 00205000
*   RESPECIFY                                                      0265 00206000
*     R14 RESTRICTED;               /*                       @G24LB02*/ 00207000
*   M5LCDR14=R14;                   /* SAVE REG 14           @G24LB02*/ 00208000
         ST    R14,M5LCDR14                                        0266 00209000
*   RESPECIFY                                                      0267 00210000
*     R14 UNRESTRICTED;             /*                       @G24LB02*/ 00211000
*   RFY                                                            0268 00212000
*    (R0,                                                          0268 00213000
*     R1,                                                          0268 00214000
*     R15) RSTD;                    /*                       @G24LB02*/ 00215000
*   R1=NVTDCBSN;                    /*                       @G24LB02*/ 00216000
         L     @14,NVTPTR                                          0269 00217000
         L     R1,NVTDCBSN(,@14)                                   0269 00218000
*   GEN REFS(LOADER1,R1) SETS(R0);                                 0270 00219000
*                                                         /*@G24LB02*/  00220000
LOAD05  LOAD EP=ICBMSG05,DCB=(1),ERRET=LOADER1,RELATED=(ICBMSG05,IEAVN**00221000
               P19(MSG05LCD))                                 @G24LB02  00222000
*   GOTO LOADGD;                    /* BRANCH, LOAD GOOD     @G24LB02*/ 00223000
         B     LOADGD                                              0271 00224000
*LOADER1:                           /*                       @G24LB02*/ 00225000
*   FAILCODE=R1;                    /* SAVE FAIL CODE        @G24LB02*/ 00226000
LOADER1  ST    R1,FAILCODE                                         0272 00227000
*   CLEANCOD=LNM5FAIL;              /* SETUP CLEANUP CODE    @G24LB02*/ 00228000
         MVC   CLEANCOD(4),@CF02461                                0273 00229000
*   MINERR=ON;                      /* MINOR ERROR INIT WILL CONT  0274 00230000
*                                                            @G24LB02*/ 00231000
         OI    MINERR,B'10000000'                                  0274 00232000
*   R14=M5LCDR14;                   /*                       @G24LB02*/ 00233000
         L     R14,M5LCDR14                                        0275 00234000
*   RETURN;                         /*                        @24LB02*/ 00235000
@EL00002 DS    0H                                                  0276 00236000
@EF00002 DS    0H                                                  0276 00237000
@ER00002 BR    @14                                                 0276 00238000
*LOADGD:                            /*                       @G24LB02*/ 00239000
*   R15=R0;                         /* SAVE ADDRESS OF ICBMSG05 IN 0277 00240000
*                                      R15                   @G24LB02*/ 00241000
LOADGD   LR    R15,R0                                              0277 00242000
*   R1=ADDR(PURGINDC);              /* SETUP PARM PTR FOR ICBMSG05 0278 00243000
*                                                            @G24LB02*/ 00244000
         LA    R1,PURGINDC                                         0278 00245000
*   GEN REFS(R14,R15);                                             0279 00246000
*                                                         /*@G24LB02*/  00247000
       BALR     R14,R15     BR AND LINK TO ICBMSG05           @G24LB02  00248000
DEL05  DELETE EP=ICBMSG05,RELATED=(ICBMSG05,IEAVNP19(MSG05LCD))         00249000
*   RESPECIFY                                                      0280 00250000
*     R14 RESTRICTED;               /*                       @G24LB02*/ 00251000
*   R14=M5LCDR14;                   /* RESTORE REGISTER 14   @G24LB02*/ 00252000
         L     R14,M5LCDR14                                        0281 00253000
*   RESPECIFY                                                      0282 00254000
*     R14 UNRESTRICTED;             /*                       @G24LB02*/ 00255000
*   RETURN;                         /*                       @G24LB02*/ 00256000
         B     @EL00002                                            0283 00257000
*   END MSG05LCD;                   /*                       @G24LB02*/ 00258000
*/********************************************************** @G24LB02*/ 00259000
*/*                                                          @G24LB02*/ 00260000
*/*   MSCUCBS      SEGMENT OF IEAVNP19 WHICH IDENTIFIES      @G24LB02*/ 00261000
*/*                 UCBS OF THE MSC DEVICE TYPE              @G24LB02*/ 00262000
*/*                                                          @G24LB02*/ 00263000
*/*    NOTE THAT IN THIS VERSION THERE MAY BE ONLY TWO MSCS, @G24LB02*/ 00264000
*/*    AND THE FIRST TWO UCBS IN THE UCB ADDRESS LIST ARE    @G24LB02*/ 00265000
*/*    THOSE FOUND AND REMEMBERED FOR MSSC USE               @G24LB02*/ 00266000
*/*                                                          @G24LB02*/ 00267000
*/*   INPUT:       A POINTER TO LIST OF UCBS VIA THE CVT     @G24LB02*/ 00268000
*/*                                                          @G24LB02*/ 00269000
*/*  PROCESSING:   SEARCH ALL UCBS UNTIL TWO ARE FOUND FOR   @G24LB02*/ 00270000
*/*                THE MSC DEVICE TYPE, OR UNTIL THERE ARE   @G24LB02*/ 00271000
*/*                NO MORE UCBS IN THE SYSTEM                @G24LB02*/ 00272000
*/*                TURNS ON THE UCBALOC BIT FOR EACH MSC UCB @G24LB02*/ 00273000
*/*                FOUND                                     @G24LB02*/ 00274000
*/*                TURNS OFF THE UCBQISCE BIT FOR EACH MSC   @ZM31123*/ 00275000
*/*                UCB, TO ALLOW I/O TO BE SENT TO THE MSC   @ZM31123*/ 00276000
*/*                                                          @G24LB02*/ 00277000
*/*  OUTPUT:       THE ADDRESS OF THE FIRST MSC UCB FOUND,   @G24LB02*/ 00278000
*/*                IF ANY                                    @G24LB02*/ 00279000
*/*                THE ADDRESS OF THE SECOND MSC UCB FOUND,  @G24LB02*/ 00280000
*/*                IF ANY                                    @G24LB02*/ 00281000
*/*                THE CALLING MODULE CHECKS TO SEE IF THE   @G24LB02*/ 00282000
*/*                ADDRESS OF THE FIRST MSC FOUND IS ZERO.   @G24LB02*/ 00283000
*/*                IF SO, IEAVNP19 ASSUMES THE IPL WILL NOT  @G24LB02*/ 00284000
*/*                BE USING MSS, AS NO SYSGEN SUPPORT FOR    @G24LB02*/ 00285000
*/*                3850 WAS PROVIDED.                        @G24LB02*/ 00286000
*/*                                                          @G24LB02*/ 00287000
*/*      MESSAGES:  NONE ARE WRITTEN OR CAUSED TO BE WRITTEN @G24LB02*/ 00288000
*/*                                                          @G24LB02*/ 00289000
*/********************************************************** @G24LB02*/ 00290000
*                                                                  0285 00291000
*MSCUCBS:                                                          0285 00292000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB02*/ 00293000
MSCUCBS  DS    0H                                                  0286 00294000
*   DCL                                                            0286 00295000
*     MSCRET POINTER(31);           /* FOR RETURN ADDRESS    @G24LB02*/ 00296000
*   DCL                                                            0287 00297000
*     MSCDEVC FIXED(8) CONSTANT(66);/* HEX 42 IN MSC UCB     @G24LB02*/ 00298000
*   RFY                                                            0288 00299000
*     R14 RSTD;                     /* FOR RETURN ADDRESS    @G24LB02*/ 00300000
*   MSCRET=R14;                     /* SAVE RETURN ADDRESS   @G24LB02*/ 00301000
         ST    R14,MSCRET                                          0289 00302000
*   RFY                                                            0290 00303000
*     R14 UNRSTD;                   /* FREE UP REGISTER 14   @G24LB02*/ 00304000
*   LISTA=CVTILK2;                  /* ADDR OF UCB ADDRESS LIST    0291 00305000
*                                                            @G24LB02*/ 00306000
         L     @14,CVTPTR                                          0291 00307000
         L     @14,CVTILK2(,@14)                                   0291 00308000
         ST    @14,LISTA                                           0291 00309000
*   DO WHILE UCBPTR^=FOXES&MSCUCBA2=LT2MSCS;/* FIND MSCS     @G24LB02*/ 00310000
         B     @DE00292                                            0292 00311000
@DL00292 DS    0H                                                  0293 00312000
*     UCBPTR=LISTA->BEGUPTR;        /* ADDRESS OF ONE UCB    @G24LB02*/ 00313000
         L     @14,LISTA                                           0293 00314000
         LH    @14,BEGUPTR(,@14)                                   0293 00315000
         N     @14,@CF02747                                        0293 00316000
         STH   @14,UCBPTR                                          0293 00317000
*     IF UCBPTR^=ZERO&UCBPTR^=FOXES /* IS IT VALID?          @G24LB02*/ 00318000
*       THEN                        /* THIS IS A VALID UCB ADDRESS 0294 00319000
*                                                            @G24LB02*/ 00320000
         LH    @14,UCBPTR                                          0294 00321000
         N     @14,@CF02747                                        0294 00322000
         LTR   @14,@14                                             0294 00323000
         BZ    @RF00294                                            0294 00324000
         SLR   @05,@05                                             0294 00325000
         ICM   @05,3,FOXES                                         0294 00326000
         CR    @14,@05                                             0294 00327000
         BE    @RF00294                                            0294 00328000
*       DO;                         /* SEE IF IT IS AN MSC UCB,    0295 00329000
*                                                            @G24LB02*/ 00330000
*         IF UCB3UREC=ON&UCBTBYT4=MSCDEVC/* AN MSC UCB ?     @G24LB02*/ 00331000
*           THEN                    /* YES, AN MSC UCB       @G24LB02*/ 00332000
         TM    UCB3UREC(@14),B'00001000'                           0296 00333000
         BNO   @RF00296                                            0296 00334000
         CLI   UCBTBYT4(@14),66                                    0296 00335000
         BNE   @RF00296                                            0296 00336000
*           DO;                     /* NEED TO PROCESS AND REMEMBER     00337000
*                                      IT                    @Y30LB04*/ 00338000
*             UCBALOC=ON;           /* (MSSC CONVENTION)     @Y30LB04*/ 00339000
         OI    UCBALOC(@14),B'00001000'                            0298 00340000
*             UCBQISCE=OFF;         /* TURN OFF QUIESCE BIT  @ZM31123*/ 00341000
         NI    UCBQISCE(@14),B'11111110'                           0299 00342000
*             IF MSCUCBA1=NOMSCS    /* IS THIS THE FIRST MSC UCB ? 0300 00343000
*                                                            @Y30LB04*/ 00344000
*               THEN                /* THIS IS FIRST MSC UCB @Y30LB04*/ 00345000
         L     @05,MSCUCBA1                                        0300 00346000
         LTR   @05,@05                                             0300 00347000
         BNZ   @RF00300                                            0300 00348000
*               MSCUCBA1=UCBPTR;    /* YES, SAVE ITS ADDRESS @Y30LB04*/ 00349000
         ST    @14,MSCUCBA1                                        0301 00350000
*             ELSE                  /* NOT FIRST MSC UCB, MUST BE 2ND   00351000
*                                                            @ZM31096*/ 00352000
*               DO;                 /* SEE IF ANOTHER MSC UCB      0302 00353000
*                                                            @ZM31096*/ 00354000
         B     @RC00300                                            0302 00355000
@RF00300 DS    0H                                                  0303 00356000
*                 IF UCBPTR=MSCUCBA1/* HAVE FOUND SAME MSC?  @ZM31096*/ 00357000
*                   THEN                                           0303 00358000
         LH    @14,UCBPTR                                          0303 00359000
         N     @14,@CF02747                                        0303 00360000
         C     @14,MSCUCBA1                                        0303 00361000
         BE    @RT00303                                            0303 00362000
*                   ;               /* YES, NOT A DIFFERENT MSC UCB     00363000
*                                                            @ZM31096*/ 00364000
*                 ELSE              /* NO, THIS IS ANOTHER MSC UCB 0305 00365000
*                                                            @ZM31096*/ 00366000
*                   MSCUCBA2=UCBPTR;/* SAVE ADDR AS SECOND   @ZM31096*/ 00367000
         LH    @14,UCBPTR                                          0305 00368000
         N     @14,@CF02747                                        0305 00369000
         ST    @14,MSCUCBA2                                        0305 00370000
*               END;                /*                       @ZM31096*/ 00371000
*           END;                    /* PROCESSING FOR AN MSC UCB   0307 00372000
*                                                            @G24LB02*/ 00373000
*         ELSE                      /* THIS IS NOT AN MSC UCB      0308 00374000
*                                                            @G24LB02*/ 00375000
*           ;                       /* SKIP IT               @G24LB02*/ 00376000
@RF00296 DS    0H                                                  0309 00377000
*       END;                        /* FINDING OUT IF THIS IS AN MSC    00378000
*                                      UCB                   @G24LB02*/ 00379000
*     ELSE                          /* NO UCB FOR THIS LIST ENTRY  0310 00380000
*                                                            @G24LB02*/ 00381000
*       ;                           /* ON TO THE NEXT LIST ENTRY   0310 00382000
*                                                            @G24LB02*/ 00383000
@RF00294 DS    0H                                                  0311 00384000
*     LISTA=LISTA+ALINC;            /* INCREMENT THROUGH UCB LIST  0311 00385000
*                                                            @G24LB02*/ 00386000
@RC00294 LA    @14,2                                               0311 00387000
         AL    @14,LISTA                                           0311 00388000
         ST    @14,LISTA                                           0311 00389000
*   END;                            /* OF THE SEARCH FOR MSC UCBS  0312 00390000
*                                                            @G24LB02*/ 00391000
@DE00292 CLC   UCBPTR(2),FOXES                                     0312 00392000
         BE    @DC00292                                            0312 00393000
         L     @14,MSCUCBA2                                        0312 00394000
         LTR   @14,@14                                             0312 00395000
         BZ    @DL00292                                            0312 00396000
@DC00292 DS    0H                                                  0313 00397000
*   RFY                                                            0313 00398000
*     R14 RSTD;                     /* FOR RETURN ADDRESS    @G24LB02*/ 00399000
*   R14=MSCRET;                     /* RESTORE RETURN ADDRESS      0314 00400000
*                                                            @G24LB02*/ 00401000
         L     R14,MSCRET                                          0314 00402000
*   RFY                                                            0315 00403000
*     R14 UNRSTD;                   /* FOR RETURN TO CALLER  @G24LB02*/ 00404000
*   END MSCUCBS;                    /* HAVE FOUND ALL MSC UCBS     0316 00405000
*                                      EXPECTED              @G24LB02*/ 00406000
@EL00003 DS    0H                                                  0316 00407000
@EF00003 DS    0H                                                  0316 00408000
@ER00003 BR    @14                                                 0316 00409000
*                                                                  0317 00410000
*/********************************************************** @G24LB02*/ 00411000
*/*                                                          @G24LB02*/ 00412000
*/*   CBSETUP      SEGMENT OF IEAVNP19 WHICH SETS UP THE     @G24LB02*/ 00413000
*/*                MSSC CONTROL BLOCK, ICB2SSCB              @G24LB02*/ 00414000
*/*                                                          @G24LB02*/ 00415000
*/*   INPUT:       POINTER TO THE CVT IN RCVT                @G24LB02*/ 00416000
*/*                                                          @G24LB02*/ 00417000
*/*  PROCESSING:  OBTAINS FIXED STORAGE FOR THE MSSC         @Y30LB04*/ 00418000
*/*               CONTROL BLOCK, ICB2SSCB,                   @Y30LB04*/ 00419000
*/*               CLEARS THAT STORAGE, AND                   @Y30LB04*/ 00420000
*/*               PLACES ITS ADDRESS INTO CVTICB, WHICH      @Y30LB04*/ 00421000
*/*               IS BOTH THE ADDRESS OF THE MSSC CONTROL    @Y30LB04*/ 00422000
*/*               BLOCK AND AN INDICATION TO OTHER MODULES   @Y30LB04*/ 00423000
*/*               THAT THERE IS AN MSSC IN THIS SYSTEM.      @Y30LB04*/ 00424000
*/*               OBTAINS FIXED STORAGE FOR THE MSSC SRB,    @Y30LB04*/ 00425000
*/*               CLEARS THE SRB STORAGE,                    @Y30LB04*/ 00426000
*/*               AND PLACES A POINTER TO THE MSSC SRB INTO  @Y30LB04*/ 00427000
*/*                THE MSSC CONTROL BLOCK                    @Y30LB04*/ 00428000
*/*                                                          @Y30LB04*/ 00429000
*/*   OUTPUT:      PARTIALLY INITIALIZED MSSC CONTROL BLOCK  @Y30LB04*/ 00430000
*/*                A POINTER IN THE CVT WHICH IS USED TO     @Y30LB04*/ 00431000
*/*                NOTIFY OTHER COMPONENTS THAT MSSC IS IN   @Y30LB04*/ 00432000
*/*                THIS SYSTEM, AND TO FIND THE LOCATION OF  @Y30LB04*/ 00433000
*/*                THE MSSC CONTROL BLOCK, ICB2SSCB          @Y30LB04*/ 00434000
*/*                ON ERROR, FAILURE AND CLEANUP CODES ARE   @Y30LB04*/ 00435000
*/*                PASSED TO THE MAIN SEGMENT                @Y30LB04*/ 00436000
*/*                                                          @Y30LB04*/ 00437000
*/********************************************************** @Y30LB04*/ 00438000
*                                                                  0317 00439000
*CBSETUP:                                                          0317 00440000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @Y30LB04*/ 00441000
CBSETUP  DS    0H                                                  0318 00442000
*   DCL                                                            0318 00443000
*     CBRET POINTER(31);            /* FOR RETURN ADDRESS    @Y30LB04*/ 00444000
*   RFY                                                            0319 00445000
*     R14 RSTD;                     /* FOR RETURN ADDRESS    @Y30LB04*/ 00446000
*   CBRET=R14;                      /* SAVE RETURN ADDRESS   @Y30LB04*/ 00447000
         ST    R14,CBRET                                           0320 00448000
*   RFY                                                            0321 00449000
*     R14 UNRSTD;                   /* FREE UP REGISTER FOURTEEN   0321 00450000
*                                                            @Y30LB04*/ 00451000
*/*  GET STORAGE FOR MSSC CONTRO BLOCK                       @Y30LB04*/ 00452000
*                                                                  0322 00453000
*   RFY                                                            0322 00454000
*    (R0,                                                          0322 00455000
*     R1,                                                          0322 00456000
*     R15) RSTD;                    /* FOR GETMAIN           @Y30LB04*/ 00457000
*   R0=LENGTH(ICB2SSCB);            /* AMOUNT OF STORAGE     @Y30LB04*/ 00458000
         LA    R0,532                                              0323 00459000
*   R1=ZERO;                        /*                       @Y30LB04*/ 00460000
         SLR   R1,R1                                               0324 00461000
*   R15=SP245;                      /* SP 245, CONDITIONAL GET     0325 00462000
*                                                            @Y30LB04*/ 00463000
         LA    R15,245                                             0325 00464000
*IGETMAI1:                          /* GET CONTROL BLOCK STORAGE   0326 00465000
*                                                            @Y30LB04*/ 00466000
*   GEN REFS(R0,R1,R15) SETS(R0,R1,R15);                           0326 00467000
IGETMAI1 DS    0H                                                  0326 00468000
*                                     /* WITH GETMAIN MACRO @Y30LB04*/  00469000
        GETMAIN RC,LV=(R0),SP=(R15),BNDRY=DBLWD,                      **00470000
               RELATED=('FREED IF MSSC INITLZ FAILS IEAVNP19(IFREEMA1,**00471000
               IFREEMA3),ICBINIT(IFREEMA2)')                  @Y30LB04  00472000
*   IF R15=ZERO                     /* WAS GETMAIN SUCCESSFUL?     0327 00473000
*                                                            @Y30LB04*/ 00474000
*     THEN                          /* YES, HAVE MSSC CONTROL BLOCK     00475000
*                                                            @Y30LB04*/ 00476000
         LTR   R15,R15                                             0327 00477000
         BNZ   @RF00327                                            0327 00478000
*     DO;                           /* INITIALIZE IT         @Y30LB04*/ 00479000
*       CVTICB=R1;                  /* SAVE ADDRESS IN THE CVT     0329 00480000
*                                                            @Y30LB04*/ 00481000
