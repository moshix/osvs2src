         ST    R1,CVTICB(,@14)                                     0329 00484000
*       R0=R1;                      /* ADDRESS OF STORAGE    @Y30LB04*/ 00485000
         LR    R0,R1                                               0330 00486000
*       R1=LENGTH(ICB2SSCB);        /* SIZE OF CONTROL BLOCK @Y30LB04*/ 00487000
         LA    R1,532                                              0331 00488000
*       R15=ZERO;                   /*                       @Y30LB04*/ 00489000
         SLR   R15,R15                                             0332 00490000
*       RFY                                                        0333 00491000
*         R14 RSTD;                 /* RESTRICT 14 FOR THE MVCL    0333 00492000
*                                                            @Y30LB04*/ 00493000
*       MVCL(R0,R14);               /* ZERO GOTTEN STORAGE   @Y30LB04*/ 00494000
         MVCL  R0,R14                                              0334 00495000
*       RFY                                                        0335 00496000
*         R14 UNRSTD;               /* FREE UP REGISTER 14   @Y30LB04*/ 00497000
*/*   PUT IDENTIFIER INTO MSSC CONTROL BLOCK                 @Y30LB04*/ 00498000
*                                                                  0336 00499000
*       ICBSSNAM=SSCBNAME;          /* THE MSSC CONTROL BLOCK      0336 00500000
*                                                            @Y30LB04*/ 00501000
         L     @14,CVTPTR                                          0336 00502000
         L     @14,CVTEXT2(,@14)                                   0336 00503000
         L     @14,CVTICB(,@14)                                    0336 00504000
         MVC   ICBSSNAM(8,@14),@CC02477                            0336 00505000
*       ICBPUCB=MSCUCBA1;           /* TRY FIRST MSC AS PRIMARY    0337 00506000
*                                                            @Y30LB04*/ 00507000
         L     @05,MSCUCBA1                                        0337 00508000
