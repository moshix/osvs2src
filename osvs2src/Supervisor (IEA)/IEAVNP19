         BZ    @RT00764                                            0764 02354000
         CLC   UCBVOLI(6,@05),NULVOLI                              0764 02355000
         BNE   @RF00764                                            0764 02356000
@RT00764 DS    0H                                                  0765 02357000
*                   DO;             /* DEMOUNT VUA CODE      @ZDR2048   02358000
*                                      MACRO INVOCATION:     @ZA28964   02359000
*                                      ICBMNTDE TYPE(DMNT)VUA(UCBCHAN   02360000
*                                      )VOLID(UCBVOLI)       @ZA28964*/ 02361000
*                     DCL                                          0766 02362000
*                       MDREG1 REG(1) PTR;/* ADDRESS OF REQUEST BLOCK   02363000
*                                                            @Y30LB02   02364000
*                                      ******************************   02365000
*                                      **************************  0766 02366000
*                                                            @Y30LB02   02367000
*                                      MACMEAN MSSC DEMOUNT MACRO  0766 02368000
*                                                            @Y30LB02   02369000
*                                      VERSION - VS/2 REL 3 77/12/15    02370000
*                                      LEVEL 04              @ZA28964   02371000
*                                      THIS IS THE STANDARD FORM - 0766 02372000
*                                      DEMOUNT MACRO         @Y30LB02   02373000
*                                      ******************************   02374000
*                                      **************************  0766 02375000
*                                                            @Y30LB02*/ 02376000
*                     RFY                                          0767 02377000
*                       MDREG1 RSTD;/* RESTRICT REG 1        @Y30LB02*/ 02378000
*                     MDREG1=ADDR(DMLEN);/* R1 PTR TO LIST   @Y30LB02*/ 02379000
         LA    MDREG1,DMLEN                                        0768 02380000
*                     DCL                                          0769 02381000
*                       1 ICBDMLST STATIC LOCAL,/* DEMOUNT REQUEST 0769 02382000
*                                      BLOCK                 @Y30LB02*/ 02383000
*                        2 DMLEN FIXED(31) INIT('1C'X),/* LENGTH OF     02384000
*                                      DEMOUNT BLOCK         @Y30LB02*/ 02385000
*                        2 DMOPCD FIXED(8) INIT('06'X),/* DEMOUNT  0769 02386000
*                                      OPCODE                @Y30LB02*/ 02387000
*                        2 DMNTRES1 CHAR(1) INIT('00'X),           0769 02388000
*                                                                  0769 02389000
*/*INIT RESERVED AREA TO ZERO @YM02186                               */ 02390000
*                                                                  0769 02391000
