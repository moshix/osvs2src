*   /*                                                               */ 01171000
*   /*****************************************************************/ 01172000
*                                                                  0510 01173000
*   R9=ADDR(FABNAM);                /* MODULE NAME           @Y30LB04*/ 01174000
         LA    R9,FABNAM                                           0510 01175000
*   ENTRYPTR=ZERO;                  /* ZERO ADDRESS FOR RETURN     0511 01176000
*                                                            @Y30LB04*/ 01177000
         SLR   @08,@08                                             0511 01178000
         ST    @08,ENTRYPTR                                        0511 01179000
*   CALL QCVADS;                    /* HUNTS FOR MODULE ADDRESS    0512 01180000
*                                                            @Y30LB04*/ 01181000
         BAL   @14,QCVADS                                          0512 01182000
*   IF ENTRYPTR=ZERO                /* WAS IEFAB4UV MISSING  @Y30LB04*/ 01183000
*     THEN                          /* NO ALLOCATION SERVICE ROUTINE    01184000
*                                                            @Y30LB04*/ 01185000
         L     @08,ENTRYPTR                                        0513 01186000
         LTR   @08,@08                                             0513 01187000
         BNZ   @RF00513                                            0513 01188000
*     DO;                           /* NO ADDRESS RETURNED   @Y30LB04*/ 01189000
*       ENTRYPTR=ADDR(FABNAM);      /* MODULE NAME FOR MESSAGE     0515 01190000
*                                                            @Y30LB04*/ 01191000
         LA    @08,FABNAM                                          0515 01192000
         ST    @08,ENTRYPTR                                        0515 01193000
*       CLEANCOD=LFABFAIL;          /* SET CLEANUP CODE      @Y30LB04*/ 01194000
         MVC   CLEANCOD(4),@CF02449                                0516 01195000
*       GO TO ADRET;                /* LEAVE THIS SEGMENT    @Y30LB04*/ 01196000
         B     ADRET                                               0517 01197000
*     END;                          /* TRACKING LOST IEFAB4UV      0518 01198000
*                                                            @Y30LB04*/ 01199000
