*   CWAPT=INDXWAPT;                 /* PT TO INDEX AMB WORK AREA   0834 01682000
*                                                            @ZM00062*/ 01683000
         L     CWAPT,INDXWAPT                                      0834 01684000
*   IF CWAPT^=ZERO THEN             /* IF WORK AREA EXIST    @ZM00062*/ 01685000
         LTR   CWAPT,CWAPT                                         0835 01686000
         BZ    @RF00835                                            0835 01687000
*     CTVOLUCB=R1;                  /* STORE UCB POINTER     @ZM00062*/ 01688000
         ALR   @09,CWAPT                                           0836 01689000
         ST    R1,CTVOLUCB(,@09)                                   0836 01690000
*   RFY                                                            0837 01691000
*     R1 UNRSTD;                                                   0837 01692000
@RF00835 DS    0H                                                  0838 01693000
*   RET=RSAVLA;                                                    0838 01694000
         L     RET,RSAVLA                                          0838 01695000
*   END VMNTVER;                                                   0839 01696000
         B     @EL00013                                            0839 01697000
*BLDDDEB:                                                          0840 01698000
*                                                                  0840 01699000
*   /*****************************************************************/ 01700000
*   /*                                                               */ 01701000
*   /* THIS ROUTINE BUILDS A DUMMY DEB IT IS CHAINED OFF OF THE      */ 01702000
*   /* CURRENT TCB. THE DEB POINTS TO THE ACB AND THE ACB POINTS BACK*/ 01703000
*   /*                                                               */ 01704000
*   /*****************************************************************/ 01705000
*                                                                  0840 01706000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0840 01707000
BLDDDEB  DS    0H                                                  0841 01708000
*   RSAVLA=RET;                                                    0841 01709000
         ST    RET,RSAVLA                                          0841 01710000
*   CORELEN=DDEBLEN;                /* REQUEST LENGTH FOR DUMMY DEB  */ 01711000
         LH    @09,DDEBLEN                                         0842 01712000
         ST    @09,CORELEN                                         0842 01713000
*   CALL GETCORE;                   /* GET CORE DDEB                 */ 01714000
         BAL   @14,GETCORE                                         0843 01715000
*   RDEBAD=COREADDR                 /* ADDR OF START OF AREA         */ 01716000
*       +LENGTH(DEBPREFX);          /* POINT TO AFTER DEB PREFIX     */ 01717000
         L     @09,COREADDR                                        0844 01718000
         LA    RDEBAD,16                                           0844 01719000
         ALR   RDEBAD,@09                                          0844 01720000
*   ACBDEB=RDEBAD;                  /* SET PTR IN ACB                */ 01721000
         L     @15,ACBPTR                                          0845 01722000
         STCM  RDEBAD,7,ACBDEB(@15)                                0845 01723000
*   DDEBADR=RDEBAD-LENGTH(DEB);                                    0846 01724000
         LR    DDEBADR,RDEBAD                                      0846 01725000
         SL    DDEBADR,@CF03057                                    0846 01726000
*   RESPECIFY                                                      0847 01727000
*     DEB BASED(DDEBADR);           /* POINT TO IMAGINARY START OF 0847 01728000
*                                      DEB ADDRESSES PREFIX ONLY     */ 01729000
*   DEBLNGTH=DDEBLEN/EIGHT;         /* INIT LENGTH IN DWORDS         */ 01730000
         LH    @00,DDEBLEN                                         0848 01731000
         SRDA  @00,32                                              0848 01732000
         D     @00,@CF00058                                        0848 01733000
         STC   @01,DEBLNGTH(,DDEBADR)                              0848 01734000
*   DEBTCBB=ADDR(TCB);              /* INIT POINTER TO TCB           */ 01735000
         L     @01,NVTPTR                                          0849 01736000
         MVC   DEBTCBB(3,RDEBAD),NVTMSTCB+1(@01)                   0849 01737000
*   DEBDCBB=ADDR(IFGACB);           /* INIT PTR TO ACB               */ 01738000
         STCM  @15,7,DEBDCBB(RDEBAD)                               0850 01739000
*   DEBDEBID=IDXDEB;                /* INIT ID IN DEB                */ 01740000
         MVI   DEBDEBID(RDEBAD),B'00001111'                        0851 01741000
*   DDEBADR=COREADDR;               /* SAVE DDEB ADDR                */ 01742000
         LR    DDEBADR,@09                                         0852 01743000
*   RESPECIFY                                                      0853 01744000
*     DEB BASED(DEBAPPAD);                                         0853 01745000
*   RET=RSAVLA;                                                    0854 01746000
         L     RET,RSAVLA                                          0854 01747000
*   END BLDDDEB;                                                   0855 01748000
@EL00014 DS    0H                                                  0855 01749000
@EF00014 DS    0H                                                  0855 01750000
@ER00014 BR    @14                                                 0855 01751000
*BLDAMB:                                                           0856 01752000
*                                                                  0856 01753000
*   /*****************************************************************/ 01754000
*   /*                                                               */ 01755000
*   /* BUILD AN AMB                                                  */ 01756000
*   /*                                                               */ 01757000
*   /*****************************************************************/ 01758000
*                                                                  0856 01759000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0856 01760000
BLDAMB   DS    0H                                                  0857 01761000
*   RSAVLA=RET;                                                    0857 01762000
         ST    RET,RSAVLA                                          0857 01763000
*   CORELEN=LENGTH(IDAAMB);         /* SET FOR CORE REQ              */ 01764000
         MVC   CORELEN(4),@CF04197                                 0858 01765000
*   CALL GETCORE;                   /* GET CORE FOR AMB              */ 01766000
         BAL   @14,GETCORE                                         0859 01767000
*   AMBPT=COREADDR;                 /* LOAD AMB PTR                  */ 01768000
         L     AMBPT,COREADDR                                      0860 01769000
*   AMBDORGA=ON;                    /* DSORG INDICATES VSAM          */ 01770000
         OI    AMBDORGA(AMBPT),B'00001000'                         0861 01771000
*   AMBEXFG=AMBEXOFF;               /* SET LOCK BIT OFF              */ 01772000
         OI    AMBEXFG(AMBPT),B'00000010'                          0862 01773000
*   AMBCDSN=DSNCCI;                 /*                               */ 01774000
         MVC   AMBCDSN(3,AMBPT),DSNCCI                             0863 01775000
*   AMBCACB=CTHSHCAT;               /* SAVE CATLG ACB PTR            */ 01776000
         L     @09,CTHSHCAT(,CWAPT)                                0864 01777000
         ST    @09,AMBCACB(,AMBPT)                                 0864 01778000
*   AMBDDSN=CTHSHDSN;               /* AMD DATA SET NAME             */ 01779000
         MVC   AMBDDSN(3,AMBPT),CTHSHDSN(CWAPT)                    0865 01780000
*   AMBID=IDXAMB;                   /* SET IDENT                     */ 01781000
         MVI   AMBID(AMBPT),B'01000000'                            0866 01782000
*   AMBLEN=LENGTH(IDAAMB);          /* SET LENGTH                    */ 01783000
         MVC   AMBLEN(2,AMBPT),@CH04197                            0867 01784000
*   AMBAMETH=ACBAMETH;              /* COPY ACCESS METHOD FIELD      */ 01785000
         L     @09,ACBPTR                                          0868 01786000
         IC    @07,ACBAMETH(,@09)                                  0868 01787000
         STC   @07,AMBAMETH(,AMBPT)                                0868 01788000
*   IF AMBLCAT=ON THEN              /* IF OPENING THE CATALOG        */ 01789000
         L     @07,ACBAMBL(,@09)                                   0869 01790000
         TM    AMBLCAT(@07),B'00000010'                            0869 01791000
         BNO   @RF00869                                            0869 01792000
*     DO;                           /* THEN DO                       */ 01793000
*       AMBCACB=ADDR(IFGACB);       /* SET FLAG                      */ 01794000
         ST    @09,AMBCACB(,AMBPT)                                 0871 01795000
*       AMBMCAT=ON;                 /* SET FLAG                      */ 01796000
         OI    AMBMCAT(AMBPT),B'00100000'                          0872 01797000
*     END;                                                         0873 01798000
*   ELSE                            /* NOT A CATLOG                  */ 01799000
*     AMBCACB=CBSACB;               /* POINT TO ACB FOR CATALOG      */ 01800000
         B     @RC00869                                            0874 01801000
@RF00869 L     @09,CVTPTR                                          0874 01802000
         L     @09,CVTCBSP(,@09)                                   0874 01803000
         L     @09,CBSACB(,@09)                                    0874 01804000
         ST    @09,AMBCACB(,AMBPT)                                 0874 01805000
*   IF CWAPT=DATAWAPT THEN          /* IS AMB FOR DATA               */ 01806000
@RC00869 C     CWAPT,DATAWAPT                                      0875 01807000
         BNE   @RF00875                                            0875 01808000
*     DO;                           /* YES                           */ 01809000
*       AMBTYPE=AMBDATA;            /* INDICATE DATA AMB             */ 01810000
         NI    AMBTYPE(AMBPT),B'10111111'                          0877 01811000
*       AMBLDTA=ADDR(IDAAMB);       /* SET PTR IN AMBL               */ 01812000
         L     @09,ACBPTR                                          0878 01813000
         L     @09,ACBAMBL(,@09)                                   0878 01814000
         ST    AMBPT,AMBLDTA(,@09)                                 0878 01815000
*       IF SWPGSP=ON THEN           /* IF A PAGE SPACE               */ 01816000
         TM    SWPGSP,B'01000000'                                  0879 01817000
         BNO   @RF00879                                            0879 01818000
*         AMBPSDS=ON;               /* SO INDICATE                   */ 01819000
         OI    AMBPSDS(AMBPT),B'10000000'                          0880 01820000
*     END;                                                         0881 01821000
*   ELSE                            /* AMBL FOR INDEX                */ 01822000
*     DO;                                                          0882 01823000
         B     @RC00875                                            0882 01824000
@RF00875 DS    0H                                                  0883 01825000
*       AMBTYPE=AMBINDEX;           /* INDICATE INDEX AMB            */ 01826000
         OI    AMBTYPE(AMBPT),B'01000000'                          0883 01827000
*       AMBLDTA->AMBLINK=AMBPT;     /* PT TO DATA AMB                */ 01828000
