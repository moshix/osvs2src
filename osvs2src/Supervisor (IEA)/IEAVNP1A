         TITLE 'IEAVNP1A - NIP VSAM OPEN                               *00001000
                        '                                               00002000
IEAVNP1A CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVNP1A  77.018'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @10,0                                               0001 00011000
@PSTART  LA    @11,4095(,@10)                                      0001 00012000
         LA    @12,4095(,@11)                                      0001 00013000
         USING @PSTART,@10                                         0001 00014000
         USING @PSTART+4095,@11                                    0001 00015000
         USING @PSTART+8190,@12                                    0001 00016000
         L     @00,@SIZDATD                                        0001 00017000
         GETMAIN  R,LV=(0)                                              00018000
         LR    @08,@01                                             0001 00019000
         USING @DATD,@08                                           0001 00020000
         ST    @13,@SA00001+4                                      0001 00021000
         LM    @00,@01,20(@13)                                     0001 00022000
         ST    @08,8(,@13)                                         0001 00023000
         LR    @13,@08                                             0001 00024000
         MVC   @PC00001(12),0(@01)                                 0001 00025000
*                                                                       00026000
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF           00027000
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES       00028000
* WILL ASSEMBLE PROPERLY.                                               00029000
         STM  R2,R3,LOCPTR                                              00030000
*   MAINPOOL=NIPPOOL;               /* MOVE FOR BETTER ACCESS        */ 00031000
         L     @09,ACBPTR                                          0321 00032000
         L     @09,NIPSPPTR(,@09)                                  0321 00033000
         SLR   @15,@15                                             0321 00034000
         IC    @15,NIPPOOL(,@09)                                   0321 00035000
         ST    @15,MAINPOOL                                        0321 00036000
*   COREPTR1=ZERO;                  /* INITIALIZE MAPPING BLOCK PTR  */ 00037000
         SLR   @09,@09                                             0322 00038000
         ST    @09,COREPTR1                                        0322 00039000
*   BITBANK='00000000'B;            /* INIT SWITCHES                 */ 00040000
         MVI   BITBANK,B'00000000'                                 0323 00041000
*   ERFLAG=ZERO;                    /* CLEAR ERROR FLAG              */ 00042000
         STH   @09,ERFLAG                                          0324 00043000
*   COREAVIL=ZERO;                  /* INIT CORE AVAILABLE FIELD     */ 00044000
         ST    @09,COREAVIL                                        0325 00045000
*   CALL CATIN;                     /* GET CATLG INFO                */ 00046000
         BAL   @14,CATIN                                           0326 00047000
*   CALL BLDAMBL;                   /* CREATE AN AMBL                */ 00048000
         BAL   @14,BLDAMBL                                         0327 00049000
*   IF NOCATLG=ON THEN              /* IF NOT A CATALOG              */ 00050000
         TM    NOCATLG,B'10000000'                                 0328 00051000
         BNO   @RF00328                                            0328 00052000
*     CALL VMNTVER;                 /* VOLUME MOUNT                  */ 00053000
         BAL   @14,VMNTVER                                         0329 00054000
*   CALL BLDDDEB;                   /* BUILD DUMMY DEB               */ 00055000
@RF00328 BAL   @14,BLDDDEB                                         0330 00056000
*   DO IX1=CTAMBNO TO 1 BY-1;       /* BUILD BLOCKS PER AMB          */ 00057000
         L     @09,CTAMBNO                                         0331 00058000
         ST    @09,IX1                                             0331 00059000
         B     @DE00331                                            0331 00060000
@DL00331 DS    0H                                                  0332 00061000
*     CWAPT=WAPT(CTAMBNO+ONE-IX1);  /* POINT TO AMBWORK              */ 00062000
         LA    @07,1                                               0332 00063000
         AL    @07,CTAMBNO                                         0332 00064000
         SLR   @07,@09                                             0332 00065000
         SLA   @07,2                                               0332 00066000
         L     CWAPT,WAPT-4(@07)                                   0332 00067000
*     CALL BLDAMB;                  /* BUILD THE AMB                 */ 00068000
         BAL   @14,BLDAMB                                          0333 00069000
*     CALL BLDAMDSB;                /* BUILD THE AMDSB               */ 00070000
         BAL   @14,BLDAMDSB                                        0334 00071000
*     CALL BLDIWA;                  /* BUILD THE INSERT WORK AREA    */ 00072000
         BAL   @14,BLDIWA                                          0335 00073000
*     IF IX1=CTAMBNO THEN           /* 1ST TIME                      */ 00074000
         CLC   IX1(4),CTAMBNO                                      0336 00075000
         BNE   @RF00336                                            0336 00076000
*       CALL BUFNOSET;              /* COMPUTE BUFFER NBR            */ 00077000
         BAL   @14,BUFNOSET                                        0337 00078000
*     CALL BLDBUFC;                 /* BLD BUFFER CTL BLKS           */ 00079000
@RF00336 BAL   @14,BLDBUFC                                         0338 00080000
*     CALL BLDIOMB;                 /* BLD IOMB                      */ 00081000
         BAL   @14,BLDIOMB                                         0339 00082000
*     CALL BLDBFRS;                 /* BUILD BUFFERS                 */ 00083000
         BAL   @14,BLDBFRS                                         0340 00084000
*     CALL BLDCPA;                  /* CHANNEL PROGRAM AREA          */ 00085000
         BAL   @14,BLDCPA                                          0341 00086000
*     CALL BLDARDB;                 /* ADDR RANGE DEF BLK            */ 00087000
         BAL   @14,BLDARDB                                         0342 00088000
*     IF AMBTYPE=AMBDATA THEN       /* BUILD PLACEHOLDERS FOR        */ 00089000
         TM    AMBTYPE(AMBPT),B'01000000'                          0343 00090000
         BNZ   @RF00343                                            0343 00091000
*       CALL BLDPLHDR;              /* DATA AMB ONLY                 */ 00092000
         BAL   @14,BLDPLHDR                                        0344 00093000
*     CALL BLDLPMB;                 /* LOGICAL PHYSICAL MAPPING      */ 00094000
@RF00343 BAL   @14,BLDLPMB                                         0345 00095000
*     CALL BLDEDB;                  /* EXTENT DEFINITION             */ 00096000
         BAL   @14,BLDEDB                                          0346 00097000
*     CALL BLDRDEB;                 /* BUILD REAL DEB        @Z30SSRL*/ 00098000
         BAL   @14,BLDRDEB                                         0347 00099000
*     CALL CHNBLKS;                 /* CHAIN BLOCKS                  */ 00100000
         BAL   @14,CHNBLKS                                         0348 00101000
*   END;                                                           0349 00102000
         SLR   @09,@09                                             0349 00103000
         BCTR  @09,0                                               0349 00104000
         AL    @09,IX1                                             0349 00105000
         ST    @09,IX1                                             0349 00106000
@DE00331 LTR   @09,@09                                             0349 00107000
         BP    @DL00331                                            0349 00108000
*EXIT:                                                             0350 00109000
*                                                                  0350 00110000
*   /*****************************************************************/ 00111000
*   /*                                                               */ 00112000
*   /* CLEAN UP AND RETURN TO CALLER                                 */ 00113000
*   /*                                                               */ 00114000
*   /*****************************************************************/ 00115000
*                                                                  0350 00116000
*   CALL FREEMAIN;                  /* FREE ANY REMAINING UNUSED SQA    00117000
*                                                            @YM08548*/ 00118000
EXIT     BAL   @14,FREEMAIN                                        0350 00119000
*   RESPECIFY                                                      0351 00120000
*    (R0,                                                          0351 00121000
*     R1) RESTRICTED;                                              0351 00122000
*                                                                  0351 00123000
*   /*****************************************************************/ 00124000
*   /*                                                               */ 00125000
*   /* TO MUCH ERROR?                                                */ 00126000
*   /*                                                               */ 00127000
*   /*****************************************************************/ 00128000
*                                                                  0352 00129000
*   IF ERFLAG>ZERORBA THEN          /* TO MUCH ERROR                 */ 00130000
         LH    @09,ERFLAG                                          0352 00131000
         CH    @09,ZERORBA                                         0352 00132000
         BNH   @RF00352                                            0352 00133000
*     DO;                                                          0353 00134000
*       ACBAMBL=ZERO;               /* CLEAR POINTER                 */ 00135000
         L     @09,ACBPTR                                          0354 00136000
         SLR   @15,@15                                             0354 00137000
         ST    @15,ACBAMBL(,@09)                                   0354 00138000
*       COREPTRN=COREPTR1;          /* INIT PTR                      */ 00139000
         L     @09,COREPTR1                                        0355 00140000
         ST    @09,COREPTRN                                        0355 00141000
*       DO WHILE COREPTRN^=ZERO;    /* FREE CONTROL BLOCKS           */ 00142000
         B     @DE00356                                            0356 00143000
@DL00356 DS    0H                                                  0357 00144000
*         DO IX=CORENBR TO 1 BY-1;  /* FREE WHAT THIS COREBLK PTS    */ 00145000
         L     @09,COREPTRN                                        0357 00146000
         LH    @09,CORENBR(,@09)                                   0357 00147000
         ST    @09,IX                                              0357 00148000
         B     @DE00357                                            0357 00149000
@DL00357 DS    0H                                                  0358 00150000
*           R0=CORER0(IX);          /* SET FREEMAIN REG              */ 00151000
         L     @15,COREPTRN                                        0358 00152000
         SLA   @09,3                                               0358 00153000
         L     R0,CORER0-8(@09,@15)                                0358 00154000
*           R1=COREPTR(IX);         /* SET FREEMAIN REG              */ 00155000
         L     R1,COREPTR-8(@09,@15)                               0359 00156000
*           GEN(STM   R2,R3,TEMPSR);/* SAVE REGS                     */ 00157000
         STM   R2,R3,TEMPSR                                             00158000
*           GEN(LM    R2,R3,LOCPTR);/* LOAD PTRS FOR SERVICE RTN     */ 00159000
         LM    R2,R3,LOCPTR                                             00160000
*           GEN(FREEMAIN R,LV=(0),A=(1));/* FREE CONTROL BLOCK       */ 00161000
         FREEMAIN R,LV=(0),A=(1)                                        00162000
*           GEN(LM     R2,R3,TEMPSR);/* RESTORE REGS                 */ 00163000
         LM     R2,R3,TEMPSR                                            00164000
*         END;                                                     0364 00165000
         SLR   @09,@09                                             0364 00166000
         BCTR  @09,0                                               0364 00167000
         AL    @09,IX                                              0364 00168000
         ST    @09,IX                                              0364 00169000
@DE00357 LTR   @09,@09                                             0364 00170000
         BP    @DL00357                                            0364 00171000
*         COREPTR1=CORELAST;        /* STEP TO NEXT MAPPING BLOCK    */ 00172000
         L     @09,COREPTRN                                        0365 00173000
         L     @15,CORELAST(,@09)                                  0365 00174000
         ST    @15,COREPTR1                                        0365 00175000
*         R0=COREDESC;              /* SET FREEMAIN REG              */ 00176000
         L     R0,COREDESC(,@09)                                   0366 00177000
*         R1=COREPTRN;                                             0367 00178000
         LR    R1,@09                                              0367 00179000
*         GEN;                                                     0368 00180000
        STM   R2,R3,TEMPSR             SAVE REGS                        00181000
        LM    R2,R3,LOCPTR             LOAD PTRS FOR SERVICE ROUTINE    00182000
        FREEMAIN  R,LV=(0),A=(1)       CLEAR                            00183000
        LM    R2,R3,TEMPSR                                              00184000
*         COREPTRN=COREPTR1;        /* NEXT MAPPING BLOCK            */ 00185000
         L     @09,COREPTR1                                        0369 00186000
         ST    @09,COREPTRN                                        0369 00187000
*       END;                                                       0370 00188000
@DE00356 L     @09,COREPTRN                                        0370 00189000
         LTR   @09,@09                                             0370 00190000
         BNZ   @DL00356                                            0370 00191000
*     END;                                                         0371 00192000
*   ELSE                            /* NOT TO MUCH ERROR             */ 00193000
*     DO;                                                          0372 00194000
*                                                                  0372 00195000
         B     @RC00352                                            0372 00196000
@RF00352 DS    0H                                                  0373 00197000
*/* VSAM DATA SET IS UN OFFICIALLY OPEN                              */ 00198000
*                                                                  0373 00199000
*       ACBOPEN=ON;                 /* SET OPEN FLAG                 */ 00200000
         L     @09,ACBPTR                                          0373 00201000
         OI    ACBOPEN(@09),B'00010000'                            0373 00202000
*       IF NOCATLG=ON THEN          /* ARE WE OPENING THE CATALOG    */ 00203000
         TM    NOCATLG,B'10000000'                                 0374 00204000
         BNO   @RF00374                                            0374 00205000
*         DO;                       /* NO , DO                       */ 00206000
*                                                                  0375 00207000
*           /*********************************************************/ 00208000
*           /*                                                       */ 00209000
*           /* FREE AMBWORKS                                         */ 00210000
*           /*                                                       */ 00211000
*           /*********************************************************/ 00212000
*                                                                  0376 00213000
*           IF DATAWAPT^=ZERO THEN  /* DID WE GET AN AMBWORK         */ 00214000
         L     @09,DATAWAPT                                        0376 00215000
         LTR   @09,@09                                             0376 00216000
         BZ    @RF00376                                            0376 00217000
*             DO;                                                  0377 00218000
*               FREELEN=SWKLEN;     /* SET LENGTH                    */ 00219000
         LH    @15,SWKLEN                                          0378 00220000
         STH   @15,FREELEN                                         0378 00221000
*               FREEAD=DATAWAPT;    /* SET ADDRESS OF CORE TO BE   0379 00222000
*                                      FREED                         */ 00223000
         ST    @09,FREEAD                                          0379 00224000
*               CALL FREEWORK;      /* CALL FREE WORKING STORAGE     */ 00225000
         BAL   @14,FREEWORK                                        0380 00226000
*             END;                                                 0381 00227000
*           IF INDXWAPT^=ZERO THEN  /* IS THERE AN INDEX WORKAREA    */ 00228000
@RF00376 L     @09,INDXWAPT                                        0382 00229000
         LTR   @09,@09                                             0382 00230000
         BZ    @RF00382                                            0382 00231000
*             DO;                   /* YES, DO                       */ 00232000
*               FREELEN=SWKLEN;     /* SET LENGTH                    */ 00233000
         LH    @15,SWKLEN                                          0384 00234000
         STH   @15,FREELEN                                         0384 00235000
*               FREEAD=INDXWAPT;    /* SET ADDRESS                   */ 00236000
         ST    @09,FREEAD                                          0385 00237000
*               CALL FREEWORK;      /* CALL FREE WORKING STORAGE     */ 00238000
         BAL   @14,FREEWORK                                        0386 00239000
*             END;                                                 0387 00240000
*         END;                                                     0388 00241000
@RF00382 DS    0H                                                  0389 00242000
*       R0=COREPTR1;                /* SETUP TO RETURN MAPPING BLK 0389 00243000
*                                      PTR                           */ 00244000
@RF00374 L     R0,COREPTR1                                         0389 00245000
*       GEN REFS(R13) SETS(R0,R1);                                 0390 00246000
        L     R1,K4(R13)                                                00247000
        ST    R0,O24(R1)           MAPPING BLOCK PTR IN REG SAVE AREA   00248000
*     END;                                                         0391 00249000
*   RETURN CODE(ERFLAG);            /* RETURN TO CALLER              */ 00250000
@RC00352 LH    @09,ERFLAG                                          0392 00251000
         L     @13,4(,@13)                                         0392 00252000
         L     @00,@SIZDATD                                        0392 00253000
         LR    @01,@08                                             0392 00254000
         FREEMAIN R,LV=(0),A=(1)                                        00255000
         LR    @15,@09                                             0392 00256000
         L     @14,12(,@13)                                        0392 00257000
         LM    @00,@12,20(@13)                                     0392 00258000
         BR    @14                                                 0392 00259000
*   RFY                                                            0393 00260000
*    (R0,                                                          0393 00261000
*     R1) UNRSTD;                                                  0393 00262000
*CATIN:                                                            0394 00263000
*                                                                  0394 00264000
*   /*****************************************************************/ 00265000
*   /*                                                               */ 00266000
*   /* IF NEEDED, GET INFO FROM CATALOG. IN EITHER CASE MAKE         */ 00267000
*   /* PRELIMINARY PREPARATIONS TO USE CATALOG INFORMATION.          */ 00268000
*   /*                                                               */ 00269000
*   /*****************************************************************/ 00270000
*                                                                  0394 00271000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0394 00272000
@EL00001 L     @13,4(,@13)                                         0394 00273000
@EF00001 L     @00,@SIZDATD                                        0394 00274000
         LR    @01,@08                                             0394 00275000
         FREEMAIN R,LV=(0),A=(1)                                        00276000
@ER00001 LM    @14,@12,12(@13)                                     0394 00277000
         BR    @14                                                 0394 00278000
CATIN    DS    0H                                                  0395 00279000
*   RFY                                                            0395 00280000
*     IDAAMDSB BASED(ADDR(CTAMDSB));                               0395 00281000
*   RSAVLA=RET;                                                    0396 00282000
         ST    RET,RSAVLA                                          0396 00283000
*   IF NOCATLG=ON THEN              /* ARE WE OPENING THE CATLG?     */ 00284000
         TM    NOCATLG,B'10000000'                                 0397 00285000
         BNO   @RF00397                                            0397 00286000
*     CALL CATLGINT;                /* NO GET INFO FROM CATLG        */ 00287000
         BAL   @14,CATLGINT                                        0398 00288000
*   ELSE                            /* WE HAVE A CATLG               */ 00289000
*     DO;                           /* INFO EXISTS MOVE POINTERS     */ 00290000
         B     @RC00397                                            0399 00291000
@RF00397 DS    0H                                                  0400 00292000
*       DATAWAPT=DWKPTR;            /* POINTS TO DATA AMBWORK        */ 00293000
         L     @09,DWKPTR                                          0400 00294000
         ST    @09,DATAWAPT                                        0400 00295000
*       INDXWAPT=IWKPTR;            /* POINTS TO INDEX AMBWORK       */ 00296000
         L     @09,IWKPTR                                          0401 00297000
         ST    @09,INDXWAPT                                        0401 00298000
*     END;                                                         0402 00299000
*                                                                  0402 00300000
*/* MOVE SOME INFO FROM AMBWORK'S TO CLUSTER DESCRIPTION             */ 00301000
*                                                                  0403 00302000
*   CWAPT=DATAWAPT;                 /* POINT TO DATA WORKAREA        */ 00303000
@RC00397 L     CWAPT,DATAWAPT                                      0403 00304000
*   AMACATLG=ADDR(IDACLDSC);        /* ADDR OF CLUSTER DESCRIPTION   */ 00305000
         LA    @09,IDACLDSC                                        0404 00306000
         ST    @09,AMACATLG(,CWAPT)                                0404 00307000
*   CTDDSN=CTHSHDSN;                /* CI NBR FOR DATA ENTRY         */ 00308000
         MVC   CTDDSN(3),CTHSHDSN(CWAPT)                           0405 00309000
*   CTCISIZD=CTCISIZ;               /* CI SIZE FOR DATA              */ 00310000
         L     @09,CTCISIZ(,CWAPT)                                 0406 00311000
         ST    @09,CTCISIZD                                        0406 00312000
*   CTFLG1=CTAUTH;                  /* SET AUTHORIZATION             */ 00313000
         MVC   CTFLG1(1),CTAUTH(CWAPT)                             0407 00314000
*   IF CTRDONLY=ON THEN             /* READ ONLY?                    */ 00315000
         TM    CTRDONLY(CWAPT),B'00000100'                         0408 00316000
         BNO   @RF00408                                            0408 00317000
*     CTFLG1=ZERO;                  /* YES, INDICATE                 */ 00318000
         MVI   CTFLG1,X'00'                                        0409 00319000
*   CTAMBNO=ONE;                    /* INDICATE ONE AMB              */ 00320000
@RF00408 MVC   CTAMBNO(4),@CF00076                                 0410 00321000
*   IF INDXWAPT^=ZERO THEN          /* IS THERE AN INDEX?            */ 00322000
         L     @09,INDXWAPT                                        0411 00323000
         LTR   @09,@09                                             0411 00324000
         BZ    @RF00411                                            0411 00325000
*     DO;                           /* YES, DO                       */ 00326000
*       CWAPT=INDXWAPT;             /* SET PTR TO INDEX AMBWORK      */ 00327000
         LR    CWAPT,@09                                           0413 00328000
*       AMACATLG=ADDR(IDACLDSC);    /* SET CI SIZE FOR INDEX         */ 00329000
         LA    @09,IDACLDSC                                        0414 00330000
         ST    @09,AMACATLG(,CWAPT)                                0414 00331000
*       CTCISIZI=CTCISIZ;           /* SET CI SIZE FOR INDEX         */ 00332000
         L     @09,CTCISIZ(,CWAPT)                                 0415 00333000
         ST    @09,CTCISIZI                                        0415 00334000
*       IF CTRDONLY='0'B THEN       /* IS INDEX READ ONLY?           */ 00335000
         TM    CTRDONLY(CWAPT),B'00000100'                         0416 00336000
         BNZ   @RF00416                                            0416 00337000
*         CTFLG1=ZERO;              /* CLUSTER IS READ ONLY          */ 00338000
         MVI   CTFLG1,X'00'                                        0417 00339000
*       CTAMBNO=TWO;                /* INDICATE 2 AMBS               */ 00340000
@RF00416 MVC   CTAMBNO(4),@CF00044                                 0418 00341000
*     END;                                                         0419 00342000
*   ELSE                            /* NO INDEX                      */ 00343000
*     CTCISIZI=ZERO;                /* INDICATE NO INDEX             */ 00344000
         B     @RC00411                                            0420 00345000
@RF00411 SLR   @09,@09                                             0420 00346000
         ST    @09,CTCISIZI                                        0420 00347000
*   RET=RSAVLA;                     /* RETURN ADDRESS                */ 00348000
@RC00411 L     RET,RSAVLA                                          0421 00349000
*   END CATIN;                                                     0422 00350000
@EL00002 DS    0H                                                  0422 00351000
@EF00002 DS    0H                                                  0422 00352000
@ER00002 BR    @14                                                 0422 00353000
*CATLGINT:                                                         0423 00354000
*                                                                  0423 00355000
*   /*****************************************************************/ 00356000
*   /*                                                               */ 00357000
*   /* CREATE AND USE AND INTERFACE TO THE CATALOG INPUT IS THE      */ 00358000
*   /* DATASET NAME OUTPUT IS ONE OR 2 AMBWORK AREAS                 */ 00359000
*   /*                                                               */ 00360000
*   /*****************************************************************/ 00361000
*                                                                  0423 00362000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0423 00363000
CATLGINT DS    0H                                                  0424 00364000
*   RSAVLB=RET;                     /* SAVE RETURN ADDRESS           */ 00365000
         ST    RET,RSAVLB                                          0424 00366000
*   WKLEN=SWKLEN*TWO;               /* INDICATE LENGTH FOR GETMAIN   */ 00367000
         LH    @09,SWKLEN                                          0425 00368000
         ALR   @09,@09                                             0425 00369000
         STH   @09,WKLEN                                           0425 00370000
*   CALL GETWORK;                   /* GET WORKING STORAGE           */ 00371000
         BAL   @14,GETWORK                                         0426 00372000
*   CTGPLPTR=WKCORE;                /* PT TO CAT PARMLIST            */ 00373000
         L     @03,WKCORE                                          0427 00374000
         LR    CTGPLPTR,@03                                        0427 00375000
*   CTGFLPTR=CTGPLPTR+              /* PT TO LIST FIELD              */ 00376000
*       LENGTH(CTGPL)+              /* WHICH FOLLOWS CAT PARMLIST    */ 00377000
*       LENGTH(CTGFIELD)*SIX;       /* ALLOW FOR 6 FIELD PTRS        */ 00378000
         LA    CTGFLPTR,52                                         0428 00379000
         ALR   CTGFLPTR,CTGPLPTR                                   0428 00380000
*   CATWKPTR=CTGFLPTR+              /* PT TO CATLG WORKAREA          */ 00381000
*       (LENGTH(CTGFL)+             /* WHICH FOLLOWS FIELD LISTS     */ 00382000
*       LENGTH(CTGFLDAT))*H18;      /* ALLOW FOR 18 FIELD LISTS      */ 00383000
         LA    CATWKPTR,432                                        0429 00384000
         ALR   CATWKPTR,CTGFLPTR                                   0429 00385000
*   CATWKPTR=((CATWKPTR+SEVEN)/EIGHT)*EIGHT;/* ROUND TO DWORD BDY    */ 00386000
         LA    @00,7                                               0430 00387000
         ALR   @00,CATWKPTR                                        0430 00388000
         SRDA  @00,32                                              0430 00389000
         D     @00,@CF00058                                        0430 00390000
         SLA   @01,3                                               0430 00391000
         LR    CATWKPTR,@01                                        0430 00392000
*   WORKLEN=WKCORE+SWKLEN*TWO-      /* COMPUTE LENGTH OF WORK AREA   */ 00393000
*       CATWKPTR;                   /*                               */ 00394000
         LH    @07,SWKLEN                                          0431 00395000
         ALR   @07,@07                                             0431 00396000
         ALR   @03,@07                                             0431 00397000
         SLR   @03,CATWKPTR                                        0431 00398000
         STH   @03,WORKLEN(,CATWKPTR)                              0431 00399000
*   CATBLKLN=CATWKPTR-CTGPLPTR;     /* LENGTH OF CTGPL & CTGFL BLOCKS*/ 00400000
         LR    @15,CATWKPTR                                        0432 00401000
         SLR   @15,CTGPLPTR                                        0432 00402000
         ST    @15,CATBLKLN                                        0432 00403000
*   WKLEN=SWKLEN*TWO;               /* SET LENGTH FOR GETMAIN        */ 00404000
         STH   @07,WKLEN                                           0433 00405000
*   CALL GETWORK;                   /* GET SPAC FOR AMBWORKS         */ 00406000
         BAL   @14,GETWORK                                         0434 00407000
*   DATAWAPT=WKCORE;                /* SET PTR TO DATA AMBWORK       */ 00408000
         L     @07,WKCORE                                          0435 00409000
         ST    @07,DATAWAPT                                        0435 00410000
*   INDXWAPT=WKCORE+SWKLEN;         /* SET PTR TO INDEX AMBWORK      */ 00411000
         LH    @04,SWKLEN                                          0436 00412000
