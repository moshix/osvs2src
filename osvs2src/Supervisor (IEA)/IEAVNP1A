*   WORKPTR5=WORKPTR5+WORKPTR3;     /* ADJUST AMB WORK POINTERS      */ 01550000
         L     @04,WORKPTR3                                        0794 01551000
         ALR   WORKPTR5,@04                                        0794 01552000
*   WORKPTR6=WORKPTR6+WORKPTR3;                                    0795 01553000
         AL    @04,WORKPTR6                                        0795 01554000
         ST    @04,WORKPTR6                                        0795 01555000
*   RET=RSAVLE;                                                    0796 01556000
         L     RET,RSAVLE                                          0796 01557000
*   RFY                                                            0797 01558000
*     IDAAMDSB BASED(AMBDSB);                                      0797 01559000
*   END AMBWKRB;                                                   0798 01560000
@EL00011 DS    0H                                                  0798 01561000
@EF00011 DS    0H                                                  0798 01562000
@ER00011 BR    @14                                                 0798 01563000
*BLDAMBL:                                                          0799 01564000
*                                                                  0799 01565000
*   /*****************************************************************/ 01566000
*   /*                                                               */ 01567000
*   /* THIS ROUTINE BUILD THE AMBL. INPUT IS THE ACB, AND THE        */ 01568000
*   /* IDACLDSG                                                      */ 01569000
*   /*                                                               */ 01570000
*   /*****************************************************************/ 01571000
*                                                                  0799 01572000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0799 01573000
BLDAMBL  DS    0H                                                  0800 01574000
*   RSAVLA=RET;                                                    0800 01575000
         ST    RET,RSAVLA                                          0800 01576000
*   CWAPT=DATAWAPT;                 /* POINT TO DATA WORK AREA       */ 01577000
         L     CWAPT,DATAWAPT                                      0801 01578000
*   CORELEN=LENGTH(IDAAMBL)+        /* LENGTH OF AMBL +              */ 01579000
*       (CTAMBNO-THREE)*FOUR;       /* AMB POINTERS                  */ 01580000
         L     @09,CTAMBNO                                         0802 01581000
         SL    @09,@CF00088                                        0802 01582000
         SLA   @09,2                                               0802 01583000
         AL    @09,@CF04206                                        0802 01584000
         ST    @09,CORELEN                                         0802 01585000
*   CALL GETCORE;                   /* GET & 0 CORE FOR AMBL         */ 01586000
         BAL   @14,GETCORE                                         0803 01587000
*   ACBAMBL=COREADDR;               /* POINTER IN ACB                */ 01588000
         L     @09,COREADDR                                        0804 01589000
         L     @01,ACBPTR                                          0804 01590000
         ST    @09,ACBAMBL(,@01)                                   0804 01591000
*   IF NOCATLG=OFFX THEN            /* IF CATALOG                    */ 01592000
         TM    NOCATLG,B'10000000'                                 0805 01593000
         BNZ   @RF00805                                            0805 01594000
*     AMBLCAT=ON;                   /* SO INDICATE IN AMBL           */ 01595000
         OI    AMBLCAT(@09),B'00000010'                            0806 01596000
*   AMBLLEN=CORELEN;                /* SET AMBL LENGTH               */ 01597000
@RF00805 L     @09,ACBPTR                                          0807 01598000
         L     @15,ACBAMBL(,@09)                                   0807 01599000
         L     @14,CORELEN                                         0807 01600000
         STC   @14,AMBLLEN(,@15)                                   0807 01601000
*   AMBLNUM=CTAMBNO;                /* SET NUMBER OF AMB'S           */ 01602000
         L     @14,CTAMBNO                                         0808 01603000
         STH   @14,AMBLNUM(,@15)                                   0808 01604000
*   AMBLACB=ADDR(IFGACB);           /* BACK POINTER TO ACB           */ 01605000
         ST    @09,AMBLACB(,@15)                                   0809 01606000
*   AMBLID=IDXAMBL;                 /* SET ID                        */ 01607000
         MVI   AMBLID(@15),B'01010000'                             0810 01608000
*   AMBLFLG1=AMBLFLG1|CTFLG1;       /* SET AUTHORITY                 */ 01609000
         OC    AMBLFLG1(1,@15),CTFLG1                              0811 01610000
*   AMBLWRIT=ON;                                                   0812 01611000
         OI    AMBLWRIT(@15),B'00100000'                           0812 01612000
*   AMBLFIX=ON;                     /* SET FIX INDICATOR     @Z30SSRL*/ 01613000
         OI    AMBLFIX(@15),B'00001000'                            0813 01614000
*   IF ACBSTRNO^=ZERO THEN          /* IF IT EXISTS,                 */ 01615000
         CLI   ACBSTRNO(@09),0                                     0814 01616000
         BE    @RF00814                                            0814 01617000
*     AMBLNST=ACBSTRNO;             /* GET NBR OF STRINGS FROM ACB   */ 01618000
         IC    @14,ACBSTRNO(,@09)                                  0815 01619000
         STC   @14,AMBLNST(,@15)                                   0815 01620000
*   ELSE                            /* NO STRINGS NOT SPECIFIED      */ 01621000
*     AMBLNST=NSTDEFLT;             /* SET NST TO DEFAULT            */ 01622000
         B     @RC00814                                            0816 01623000
@RF00814 L     @09,ACBPTR                                          0816 01624000
         L     @09,ACBAMBL(,@09)                                   0816 01625000
         MVI   AMBLNST(@09),X'01'                                  0816 01626000
*   RET=RSAVLA;                                                    0817 01627000
@RC00814 L     RET,RSAVLA                                          0817 01628000
*   END BLDAMBL;                                                   0818 01629000
@EL00012 DS    0H                                                  0818 01630000
@EF00012 DS    0H                                                  0818 01631000
@ER00012 BR    @14                                                 0818 01632000
*VMNTVER:                                                          0819 01633000
*                                                                  0819 01634000
*   /*****************************************************************/ 01635000
*   /*                                                               */ 01636000
*   /* VOLUME MOUNT                                                  */ 01637000
*   /*                                                               */ 01638000
*   /*****************************************************************/ 01639000
*                                                                  0819 01640000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0819 01641000
VMNTVER  DS    0H                                                  0820 01642000
*   RSAVLA=RET;                                                    0820 01643000
         ST    RET,RSAVLA                                          0820 01644000
*   CWAPT=DATAWAPT;                 /* ADDRESSABILITY                */ 01645000
         L     CWAPT,DATAWAPT                                      0821 01646000
*   MNTPARM=MNTPARM&&MNTPARM;       /* CLEAR PARAMETER AREA          */ 01647000
         XC    MNTPARM(24),MNTPARM                                 0822 01648000
*   RFY                                                            0823 01649000
*     NIPMNTPL BASED(ADDR(MNTPARM));/* ADDR FOR PARM LIST            */ 01650000
*   NMNTVS=CTVOLSER;                /* MOVE VOLUME SERIAL NBR TO PARM*/ 01651000
         LA    @09,MNTPARM                                         0824 01652000
         MVC   NMNTVS(6,@09),CTVOLSER+232(CWAPT)                   0824 01653000
*   NMNTDSA=ADDR(CTDATANM);         /* ADDR OF DS NAME               */ 01654000
         LA    @04,CTDATANM(,CWAPT)                                0825 01655000
         STCM  @04,15,NMNTDSA(@09)                                 0825 01656000
*   RFY                                                            0826 01657000
*     R1 RSTD;                                                     0826 01658000
*   GEN REFS(NVTMOUNT);                                            0827 01659000
         STM   R2,R3,TEMPSR                                             01660000
        LM    R2,R3,LOCPTR                                              01661000
        IEAPMNIP TYPE=MOUNT,PARAM=MNTPARM,OPT=(,LC,LI)                  01662000
PSTPRM   LM    R2,R3,TEMPSR                                             01663000
*   IF R1=ZERO THEN                 /* IS MOUNTED?                   */ 01664000
         LTR   R1,R1                                               0828 01665000
         BNZ   @RF00828                                            0828 01666000
*     DO;                           /* NO, DO                        */ 01667000
*       ERFLAG=MNTERR;                                             0830 01668000
         LH    @09,MNTERR                                          0830 01669000
         STH   @09,ERFLAG                                          0830 01670000
*       RETURN TO EXIT;             /* GET OUT                       */ 01671000
         LA    @14,EXIT                                            0831 01672000
@EL00013 DS    0H                                                  0831 01673000
@EF00013 DS    0H                                                  0831 01674000
@ER00013 BR    @14                                                 0831 01675000
*     END;                                                         0832 01676000
*   CTVOLUCB=R1;                    /* STORE ADDRESS OF UCB          */ 01677000
@RF00828 LA    @09,232                                             0833 01678000
         LR    @04,CWAPT                                           0833 01679000
         ALR   @04,@09                                             0833 01680000
