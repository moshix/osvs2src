         ST    AMBPT,AMBLINK(,@07)                                 0884 01832000
*       AMBLDTA->AMBPIXP=AMBPT;                                    0885 01833000
         ST    AMBPT,AMBPIXP(,@07)                                 0885 01834000
*       AMBLIX=ADDR(IDAAMB);                                       0886 01835000
         ST    AMBPT,AMBLIX(,@09)                                  0886 01836000
*     END;                                                         0887 01837000
*   IF ACBUBF=ON&                   /* IF USER BUFFS SPECIFIED       */ 01838000
*       AMBTYPE=AMBDATA THEN        /* IF DATA AMB                   */ 01839000
