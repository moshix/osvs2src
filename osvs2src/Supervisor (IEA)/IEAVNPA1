*      2 UCBFCBOP BIT(8),           /* RESERVED (1403) OR FCB OPTIONS   03005000
*                                      (3211) (O FOR OPTION)         */ 03006000
*       3 UCBFCBO1 BIT(1),          /* FCB IMAGE IS A DEFAULT IMAGE  */ 03007000
*       3 UCBRSV44 BIT(1),          /* RESERVED                      */ 03008000
*       3 UCBRSV45 BIT(1),          /* RESERVED                      */ 03009000
*       3 UCBRSV46 BIT(1),          /* RESERVED                      */ 03010000
*       3 UCBRSV47 BIT(1),          /* RESERVED                      */ 03011000
*       3 UCBRSV48 BIT(1),          /* RESERVED                      */ 03012000
*       3 UCBRSV49 BIT(1),          /* RESERVED                      */ 03013000
*       3 UCBRSV50 BIT(1),          /* RESERVED                      */ 03014000
*      2 UCBRSV51 PTR(8),           /* RESERVED                      */ 03015000
*      2 UCBERCNT PTR(8),           /* CONTAINS A COUNT OF THE ERRORS   03016000
*                                      THAT HAVE OCCURRED. THE COUNT,   03017000
*                                      WHICH MAY WRAP AROUND, IS   0117 03018000
*                                      WRITTEN IN STANDARD OBR     0117 03019000
*                                      RECORDS (ONE PER ERROR) AND IN   03020000
*                                      NEW DEVICE-DEPENDENT OBR    0117 03021000
*                                      RECORDS (0 TO 3 PER ERROR) AND   03022000
*                                      SERVE TO RELATE TO EACH OTHER    03023000
*                                      THE STANDARD AND            0117 03024000
*                                      DEVICE-DEPENDENT OBR RECORDS     03025000
*                                      THAT PERTAIN TO EACH ERROR  0117 03026000
*                                      (3211)                        */ 03027000
*      2 UCBFCBID CHAR(4) BDY(WORD),/* THE FCB IMAGE IDENTIFICATION  */ 03028000
*      2 UCBERADR PTR(31);          /* THE ADDRESS OF THE ERP LOGOUT    03029000
*                                      AREA                          */ 03030000
*   DECLARE                                                        0118 03031000
*     UCBBGN BASED(UCBPTR-512) CHAR(536) BDY(WORD);                0118 03032000
*                                                                  0119 03033000
*/********************************************************************/ 03034000
*/*                                                                  */ 03035000
*/*      INCLUDE UNIT CONTROL MODULE DSECT                           */ 03036000
*/*                                                                  */ 03037000
*/********************************************************************/ 03038000
*/*      %INCLUDE SYSLIB(IEECUCM)                                    */ 03039000
*/* **************************************************************** */ 03040000
*/*                                                                  */ 03041000
*/*                 UNIT CONTROL MODULE (UCM) DEFINITION             */ 03042000
*/*                                                                  */ 03043000
*/*  OS/VS2 RELEASE 3 PTF, 03/19/75, LEVEL=3                         */ 03044000
*/*                                                                  */ 03045000
*/*  METHOD OF ACCESS                                                */ 03046000
*/*      PL/S - DCL UCMEXTPT PTR  (IF %UCMSYSTM='')                  */ 03047000
*/*             DCL (UCMMCSPT,UCMBASPT) PTR                          */ 03048000
*/*             DCL (UCMEILPT,UCMUEWPT,UCMDVEPT) PTR                 */ 03049000
*/*                                                                  */ 03050000
*/*             THIS MACRO ASSUMES IT IS MAPPING AN OS/VS2 UCM.      */ 03051000
*/*             IF AN OS/VS1 UCM MAPPING IS DESIRED, SPECIFY         */ 03052000
*/*             %UCMSYSTM='AOS1' BEFORE THE INCLUDE FOR THIS MACRO.  */ 03053000
*/*             DO NOT SET UCMSYSTM TO 'AOS2' BECAUSE NULL MEANS     */ 03054000
*/*             OS/VS2.                                              */ 03055000
*/*                                                                  */ 03056000
*/*      BAL  - DSECTS ARE PRODUCED UNLESS DSECT=NO IS SPECIFIED.    */ 03057000
*/*             DSECT=NO IS TO BE SPECIFIED ONLY AT SYSGEN.  USING   */ 03058000
*/*             ON A DSECT NAME GIVES ADDRESSABILITY FOR ALL SYMBOLS */ 03059000
*/*             IN THAT DSECT.  IF FORMAT=OLD IS SPECIFIED OR        */ 03060000
*/*             DEFAULTED, 3 DSECTS ARE PRODUCED.  IF FORMAT=NEW     */ 03061000
*/*             IS SPECIFIED, 6 DSECTS ARE PRODUCED.  THE DSECT      */ 03062000
*/*             NAMES AND THEIR CONTENTS ARE                         */ 03063000
*/*                                                                  */ 03064000
*/*             CONTENTS                    OLD       NEW            */ 03065000
*/*             UCM EXTENSION (OS/VS2 ONLY) UCM2EXT   UCM2EXT        */ 03066000
*/*             MCS PREFIX                  UCMPRFX   UCMPRFX        */ 03067000
*/*             UCM BASE                    **        UCM            */ 03068000
*/*             EVENT INDICATION LIST                 UCMEIL         */ 03069000
*/*             USER EXIT WORK AREA                   UCMEXIT        */ 03070000
*/*             INDIVIDUAL DEVICE ENTRY MAP           UCMLIST        */ 03071000
*/*                                                                  */ 03072000
*/*             ** MEANS NAME ON DSECT WHICH IMMEDIATELY PRECEDED    */ 03073000
*/*                THIS MACRO CALL.  THIS NAME IS PICKED UP VIA THE  */ 03074000
*/*                &SYSECT OPTION.  THE UCM BASE AND THE NEXT 3      */ 03075000
*/*                SECTIONS ARE ALL MAPPED IN THE SAME DSECT IF      */ 03076000
*/*                FORMAT=OLD.                                       */ 03077000
*/*                                                                  */ 03078000
*/*             THIS MACRO ASSUMES A DEFAULT OF SYS=AOS2 WHICH MEANS */ 03079000
*/*             A MAPPING OF AN OS/VS2 UCM IS DESIRED.  IF A MAPPING */ 03080000
*/*             OF AN OS/VS1 UCM IS DESIRED, PUT SYS=AOS1 AS A       */ 03081000
*/*             PARAMETER ON THE MACRO CALL.                         */ 03082000
*/*                                                                  */ 03083000
*/* **************************************************************** */ 03084000
*/********************************************************************/ 03085000
*/*                                                                  */ 03086000
*/*                 UNIT CONTROL MODULE (UCM) EXTENSION              */ 03087000
*/*                 (PRESENT IN OS/VS2 ONLY)                         */ 03088000
*/*                                                                  */ 03089000
*/********************************************************************/ 03090000
*                                                                  0119 03091000
*   DECLARE                                                        0119 03092000
*     1 UCM2EXT BASED(UCMEXTPT) BDY(DWORD),/* UCM EXTENSION START    */ 03093000
*      2 UCM2WID FIXED(15),         /* ASID OF USER WAITING ON     0119 03094000
*                                      UCMWQECB MDC021               */ 03095000
*      2 UCM2RID FIXED(15),         /* ASID OF USER WAITING ON     0119 03096000
*                                      UCMRQECB MDC022               */ 03097000
*      2 UCM2PST PTR(31),           /* BRANCH ENTRY POINT INTO 'POST'   03098000
*                                      ROUTINE                       */ 03099000
*      2 UCM2STA PTR(31),           /* POINTER TO STAR WORK AREA   0119 03100000
*                                      (SDWA)                        */ 03101000
*       3 UCM2SFLG BIT(8),          /* STAR CONTROL FLAGS            */ 03102000
*        4 UCM2SDWA BIT(1),         /* SDWA OBTAINED                 */ 03103000
*        4 UCM2SENT BIT(1),         /* STAR ENTERED                  */ 03104000
*        4 UCM2DTAK BIT(1),         /* DUMP TAKEN                    */ 03105000
*        4 UCM2DSTR BIT(1),         /* DUMP STARTED                  */ 03106000
*        4 UCMRSV32 BIT(1),         /* RESERVED                      */ 03107000
*        4 UCMRSV33 BIT(1),         /* RESERVED                      */ 03108000
*        4 UCMRSV34 BIT(1),         /* RESERVED                      */ 03109000
*        4 UCMRV008 BIT(1),         /* RESERVED (MDC055)       YM5195*/ 03110000
*       3 UCM2STAA PTR(24),         /* ADDR OF SDWA, OR ZERO         */ 03111000
*      2 UCM2PAD PTR(31);           /* RESERVED                      */ 03112000
*/********************************************************************/ 03113000
*/*                                                                  */ 03114000
*/*                                                                  */ 03115000
*/*            MULTIPLE CONSOLE SUPPORT (MCS) UCM PREFIX             */ 03116000
*/*                                                                  */ 03117000
*/*            MCS IS STANDARD IN OS/VS.  THE MCS PREFIX IS ALWAYS   */ 03118000
*/*            PRESENT.                                              */ 03119000
*/*                                                                  */ 03120000
*/*                                                                  */ 03121000
*/********************************************************************/ 03122000
*                                                                  0120 03123000
*   DECLARE                                                        0120 03124000
*     1 UCMPRFX BASED(UCMMCSPT) BDY(WORD),/* MCS UCM PREFIX START    */ 03125000
*      2 UCMMCENT PTR(31),          /* ADDRESS OF MASTER CONSOLE UCM    03126000
*                                      ENTRY                         */ 03127000
*      2 UCMSAVE0 CHAR(72) BDY(WORD),/* RESIDENT REGISTER SAVE AREA     03128000
*                                      FOR IEACVTSK (18 WORDS TOTAL) */ 03129000
*       3 UCMSVA0 FIXED(31),        /* WORD 1                        */ 03130000
*       3 UCMSVB0 FIXED(31),        /* WORD 2                        */ 03131000
*       3 UCMSVC0 FIXED(31),        /* WORD 3                        */ 03132000
*       3 UCMSVD0 FIXED(31),        /* WORD 4                        */ 03133000
*       3 UCMSVE0 FIXED(31),        /* WORD 5                        */ 03134000
*       3 UCMSVF0 FIXED(31),        /* WORD 6                        */ 03135000
*       3 UCMSVG0 FIXED(31),        /* WORD 7                        */ 03136000
*       3 UCMSVH0 FIXED(31),        /* WORD 8                        */ 03137000
*       3 UCMSVI0 FIXED(31),        /* WORD 9                        */ 03138000
*       3 UCMSVJ0 FIXED(31),        /* WORD 10                       */ 03139000
*       3 UCMSVK0 FIXED(31),        /* WORD 11                       */ 03140000
*       3 UCMSVL0 FIXED(31),        /* WORD 12                       */ 03141000
*       3 UCMSVM0 FIXED(31),        /* WORD 13                       */ 03142000
*       3 UCMSVN0 FIXED(31),        /* WORD 14                       */ 03143000
*       3 UCMSVO0 FIXED(31),        /* WORD 15                       */ 03144000
*       3 UCMSVP0 FIXED(31),        /* WORD 16                       */ 03145000
*       3 UCMSVQ0 FIXED(31),        /* WORD 17                       */ 03146000
*       3 UCMSVR0 FIXED(31),        /* WORD 18                       */ 03147000
*      2 UCMDOME PTR(31),           /* ADDRESS OF FIRST DOM ELEMENT  */ 03148000
*      2 UCMWTOX PTR(31),           /* ADDRESS OF WTO/R EXIT ROUTINE    03149000
*                                      (IEECVXIT)                    */ 03150000
*      2 UCMSFLGS CHAR(2),          /* SYSTEM CONTROL FLAGS (2 BYTES)*/ 03151000
*       3 UCMSFLG1 BIT(8),          /* BYTE1 OF SYSTEM CONTROL FLAGS */ 03152000
*        4 UCMRSV01 BIT(1),         /* RESERVED                      */ 03153000
*        4 UCMSYSB BIT(1),          /* HARD COPY SUPPORT REQUIRED    */ 03154000
*        4 UCMSYSC BIT(1),          /* COMMANDS TO HARD COPY         */ 03155000
*        4 UCMSYSD BIT(1),          /* CONSOLE SWITCH FOR MASTER     */ 03156000
*        4 UCMSYSE BIT(1),          /* NO CONSOLES ACTIVE            */ 03157000
*        4 UCMSYSF BIT(1),          /* GRAPHIC CONSOLES ACTIVE       */ 03158000
*        4 UCMSYSG BIT(1),          /* HARD COPY DEVICE IS SYSLOG    */ 03159000
*        4 UCMRSV35 BIT(1),         /* RESERVED ICB464               */ 03160000
*       3 UCMSFLG2 BIT(8),          /* BYTE 2 OF SYSTEM CONTROL FLAGS*/ 03161000
*        4 UCMSYSI BIT(1),          /* WQE HOUSEKEEPING REQUIRED     */ 03162000
*        4 UCMSYSJ BIT(1),          /* HARD COPY TO BE WRITTEN       */ 03163000
*        4 UCMSYSK BIT(1),          /* NEW CONSOLE IS COMPOSITE      */ 03164000
*        4 UCMSYSL BIT(1),          /* 'OPEN' BEING ISSUED TO RING 0120 03165000
*                                      CONSOLE BELL                  */ 03166000
*        4 UCMSYSM BIT(1),          /* FAILING CONSOLE IS COMPOSITE  */ 03167000
*        4 UCMSYSN BIT(1),          /* MOD 85 OPR CONSOLE WITH CRT 0120 03168000
*                                      DISPLAY                       */ 03169000
*        4 UCMSYSO BIT(1),          /* DUMMY ATTENTION BY WTL        */ 03170000
*        4 UCMRSV02 BIT(1),         /* RESERVED                      */ 03171000
*      2 UCMOWTOR CHAR(2),          /* DEFAULT VALUES FOR OLD WTO/R     03172000
*                                      MACROS                        */ 03173000
*      2 UCMCMID FIXED(31),         /* CURRENT MESSAGE IDENTIFICATION   03174000
*                                      NUMBER                        */ 03175000
*      2 UCMHCUCM PTR(31),          /* ADDRESS OF HARD COPY UCM ENTRY   03176000
*                                      OR ZERO                       */ 03177000
*      2 UCMXCT PTR(8),             /* EXTERNAL REQUEST COUNT        */ 03178000
*      2 UCMUEXIT PTR(24),          /* ADDRESS OF USER EXIT DATA (OR    03179000
*                                      ZERO)                         */ 03180000
*      2 UCMHRDRT CHAR(2),          /* HARD COPY ROUTING CODE      0120 03181000
*                                      ASSIGNMENTS                   */ 03182000
*      2 UCMRSV03 FIXED(15),        /* RESERVED                      */ 03183000
*      2 UCMXSA CHAR(24) BDY(WORD), /* 6 WORD SVC 72 PARAMETER LIST  */ 03184000
*       3 UCM1WD PTR(31),           /* POINTER TO 3RD WORD OF SVC 72    03185000
*                                      PARM LIST                     */ 03186000
*       3 UCM2WD PTR(31),           /* 2ND WORD-SVC 72 PARM LIST     */ 03187000
*       3 UCM3WD PTR(31),           /* 3RD WORD-SVC 72 PARM LIST     */ 03188000
*       3 UCM4WD PTR(31),           /* 4TH WORD-SVC 72 PARM LIST     */ 03189000
*       3 UCM5WD PTR(31),           /* 5TH WORD-SVC 72 PARM LIST     */ 03190000
*       3 UCM6WD PTR(31),           /* 6TH WORD-SVC 72 PARM LIST     */ 03191000
*      2 UCMQRTN PTR(31),           /* ADDRESS OF ENQ ROUTINE      0120 03192000
*                                      (IEECMENQ) ENTRY POINT        */ 03193000
*      2 UCMRUTCK FIXED(31),        /* ROUTE CHECKING DATA           */ 03194000
*      2 UCMDOMRT PTR(31),          /* ADDRESS OF DOM ROUTINE      0120 03195000
*                                      (IEECMDOM) ENTRY POINT      0120 03196000
*                                      (OS/VS1) RESERVED (OS/VS2)  0120 03197000
*                                                              YM2704*/ 03198000
*      2 UCMTPPTR PTR(31),          /* ADDRESS OF 2740 DEVICE SUPPORT   03199000
*                                      PROCESSOR (OR ZERO)           */ 03200000
*      2 UCMNPECB FIXED(31),        /* NIP ECB - POSTED WHEN NIP   0120 03201000
*                                      ROUTINE'S HARD COPY CAN BE  0120 03202000
*                                      WRITTEN                       */ 03203000
*      2 UCMLOGAD PTR(31),          /* ADDRESS OF WTL BUFFER MDC035  */ 03204000
*      2 UCMDTINT FIXED(31),        /* DYNAMIC DISPLAY TIME INTERVAL */ 03205000
*      2 UCMSDS1 BIT(8),            /* SDS FLAGS                     */ 03206000
*       3 UCMSDS1A BIT(1),          /* STCMDS TO HARDCOPY            */ 03207000
*       3 UCMSDS1B BIT(1),          /* INCMDS TO HARDCOPY            */ 03208000
*       3 UCMRSV04 BIT(1),          /* RESERVED                      */ 03209000
*       3 UCMRSV05 BIT(1),          /* RESERVED                      */ 03210000
*       3 UCMRSV06 BIT(1),          /* RESERVED                      */ 03211000
*       3 UCMRSV07 BIT(1),          /* RESERVED                      */ 03212000
*       3 UCMRSV08 BIT(1),          /* RESERVED                      */ 03213000
*       3 UCMRSV09 BIT(1),          /* RESERVED                      */ 03214000
*      2 UCMSDS2 CHAR(1),           /* RESERVED FOR SDS FLAGS        */ 03215000
*      2 UCMGMCNT FIXED(15);        /* WQE DYNAMIC BUFFER COUNT    0120 03216000
*                                      (OS/VS1) (MDC050)       XM4886*/ 03217000
*/****************  END OF UCM MCS PREFIX ****************************/ 03218000
*/*      POINTER(S) TO UCM MCS PREFIX (AND UCM EXTENSION IN OS/VS2)  */ 03219000
*/*      LOCATED IMMEDIATELY PRECEDING UCM BASE SECTION              */ 03220000
*                                                                  0121 03221000
*   DECLARE                                                        0121 03222000
*     UCM2PTR PTR(31) BASED(UCMBASPT-8);/* ADDRESS OF UCM EXTENSION     03223000
