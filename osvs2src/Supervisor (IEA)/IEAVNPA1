*      2 UCMRQLM1 PTR(8),           /* IPL-SPECIFIED ORE BUFFER LIMIT   03314000
*                                      MDC010                        */ 03315000
*      2 UCMRV001 CHAR(1),          /* RESERVED MDC024               */ 03316000
*      2 UCMWQLM1 FIXED(15),        /* IPL-SPECIFIED WQE BUFFER LIMIT   03317000
*                                      MDC011                        */ 03318000
*      2 UCMCRAAD PTR(31),          /* POINTER TO ATTENTION INTERRUPT   03319000
*                                      ROUTINE MDC012                */ 03320000
*      2 UCMRP2AD PTR(31),          /* POINTER TO REPLY PROCESSOR, 0123 03321000
*                                      STAGE 2 MDC013                */ 03322000
*      2 UCMRSV61 FIXED(15),        /* RESERVED MDC014               */ 03323000
*      2 UCMCTID FIXED(15),         /* ASID OF COMMUNICATIONS TASK 0123 03324000
*                                      MDC015                        */ 03325000
*      2 UCMMBEND PTR(31),          /* POINTER TO LAST ELEMENT ON  0123 03326000
*                                      MONITOR MESSAGE BLOCK QUEUE 0123 03327000
*                                      MDC026                        */ 03328000
*      2 UCMWECBH PTR(31),          /* POINTER TO START OF WQE ECB 0123 03329000
*                                      CHAIN MDC027                  */ 03330000
*      2 UCMWECBT PTR(31),          /* POINTER TO END OF WQE ECB   0123 03331000
*                                      CHAIN MDC028                  */ 03332000
*      2 UCMOECBH PTR(31),          /* POINTER TO START OF ORE ECB 0123 03333000
*                                      CHAIN MDC029                  */ 03334000
*      2 UCMOECBT PTR(31),          /* POINTER TO END OF ORE ECB   0123 03335000
*                                      CHAIN MDC030                  */ 03336000
*      2 UCMORECP FIXED(31),        /* ORE CELLPOOL ID MDC031        */ 03337000
*      2 UCMWQECP FIXED(31),        /* WQE CELLPOOL ID MDC032        */ 03338000
*      2 UCMASCB PTR(31),           /* ASCB ADDRESS OF COMMUNICATIONS   03339000
*                                      TASK MDC036                   */ 03340000
*      2 UCMSWCH PTR(31),           /* ADDRESS OF CONSOLE SWITCH   0123 03341000
*                                      ROUTINE MDC040                */ 03342000
*      2 UCMFRRAD PTR(31),          /* ADDRESS OF COMMUNICATIONS   0123 03343000
*                                      TASK'S RECOVERY ROUTINE     0123 03344000
*                                      (IEAVMFRR) MDC047             */ 03345000
*      2 UCMWAKUP PTR(31),          /* ADDRESS OF COMMUNICATIONS   0123 03346000
*                                      TASK'S POST ERROR RECOVERY  0123 03347000
*                                      ROUTINE (IEAVMEST, ALIAS FOR     03348000
*                                      IEAVMFRR) MDC048              */ 03349000
*      2 UCMJES3T PTR(31),          /* ADDRESS OF SUBSYSTEM TCB    0123 03350000
*                                      (MDC300)              @ZA03627*/ 03351000
*      2 UCMRSV42 BIT(8),           /* RESERVED MDC033               */ 03352000
*       3 UCMRSV43 BIT(1),          /* RESERVED MDC033               */ 03353000
*       3 UCMRSV44 BIT(1),          /* RESERVED MDC033               */ 03354000
*       3 UCMRSV45 BIT(1),          /* RESERVED MDC033               */ 03355000
*       3 UCMRSV46 BIT(1),          /* RESERVED MDC033               */ 03356000
*       3 UCMRSV47 BIT(1),          /* RESERVED MDC033               */ 03357000
*       3 UCMRSV48 BIT(1),          /* RESERVED MDC033               */ 03358000
*       3 UCMRSV49 BIT(1),          /* RESERVED MDC033               */ 03359000
*       3 UCMRSV50 BIT(1),          /* RESERVED MDC033               */ 03360000
*      2 UCMRSV51 BIT(8),           /* RESERVED MDC033               */ 03361000
*       3 UCMRSV52 BIT(1),          /* RESERVED MDC033               */ 03362000
