*/*                 UNIT CONTROL MODULE (UCM) BASE                   */ 03230000
*/*                                                                  */ 03231000
*/*                                                                  */ 03232000
*/********************************************************************/ 03233000
*                                                                  0123 03234000
*   DECLARE                                                        0123 03235000
*     1 UCM BASED(UCMBASPT) BDY(DWORD),/* START OF UCM BASE          */ 03236000
*/*                 FIXED ECBS                                       */ 03237000
*                                                                  0123 03238000
*      2 UCMXECB FIXED(31),         /* EXTERNAL INTERRUPT ECB        */ 03239000
*      2 UCMAECB FIXED(31),         /* ATTENTION INTERRUPT ECB       */ 03240000
*      2 UCMOECB FIXED(31),         /* WTO/WTOR REQUEST ECB          */ 03241000
*      2 UCMDECB FIXED(31),         /* DOM REQUEST ECB               */ 03242000
*       3 UCMLECB FIXED(31),        /* WTL REQUEST ECB               */ 03243000
*      2 UCMARECB FIXED(31),        /* CONSOLE RECOVERY ECB (OS/VS2)    03244000
*                                      MDC019                        */ 03245000
*      2 UCMLSTP PTR(31),           /* ADDRESS OF EVENT INDICA- TION    03246000
*                                      LIST (EIL)                    */ 03247000
*/*                 WTO/WTOR CONTROL FIELDS                          */ 03248000
*                                                                  0123 03249000
*      2 UCMWTOQ PTR(31),           /* ADDRESS OF FIRST WQE (SYSOUT     03250000
*                                      QUEUE)                        */ 03251000
*      2 UCMRPYQ PTR(31),           /* ADDRESS OF FIRST RQE (REPLY 0123 03252000
*                                      QUEUE ELEMENT                 */ 03253000
*      2 UCMRPYI CHAR(13) BDY(WORD),/* REPLY I.D. ASSIGNMENTS        */ 03254000
*       3 * BIT(100),               /* 100 BITS USED FOR REPLY I.D.     03255000
*                                      ASSIGNMENT PATTERN            */ 03256000
*      2 UCMRQLM PTR(8),            /* REPLY I.D. ASSIGNMENT LIMIT   */ 03257000
*      2 UCMWQLM FIXED(15),         /* WQE BUFFER LIMIT              */ 03258000
*      2 UCMRQECB FIXED(31),        /* REPLY REQUEST WAITING ECB     */ 03259000
*      2 UCMWQECB FIXED(31),        /* BUFF REQUEST WAITING ECB      */ 03260000
*      2 UCMRQNR FIXED(15),         /* CURRENT RQE COUNT             */ 03261000
*      2 UCMWQNR FIXED(15),         /* CURRENT WQE COUNT             */ 03262000
*      2 UCMWQEND PTR(31),          /* ADDRESS OF LAST WQE (OR ZERO) */ 03263000
*      2 UCMPXA PTR(31),            /* ADDRESS OF COMMUNICATIONS TASK   03264000
*                                      TCB                           */ 03265000
*      2 UCMPXB CHAR(1),                                           0123 03266000
*       3 UCMMODE BIT(8),           /* MODE FLAGS                    */ 03267000
*        4 UCMRSV11 BIT(1),         /* RESERVED                      */ 03268000
*        4 UCMNOCON BIT(1),         /* NO CONSOLES CONDITION EXISTS     03269000
*                                      (OS/VS2) MDC016               */ 03270000
*        4 UCMTPUTA BIT(1),         /* TPUTTER IS ACTIVE (OS/VS2)  0123 03271000
*                                      MDC017                        */ 03272000
*        4 UCMRSV14 BIT(1),         /* RESERVED MDC018               */ 03273000
*        4 UCMAMFA BIT(1),          /* ACCEPT 'VARY' CMD WITH MSTCONS   03274000
*                                      OPND FROM ANY MCS SECONDARY 0123 03275000
*                                      CONSOLE                       */ 03276000
*        4 UCMOGCE BIT(1),          /* ONLY GRAPHIC CONSOLES EXIST   */ 03277000
*        4 UCMMCS BIT(1),           /* MCS GENERATED WITH      SYSTEM*/ 03278000
*        4 UCMFIX BIT(1),           /* CONTROL PROGRAM MODE 0 =    0123 03279000
*                                      OS/VS2 1 = OS/VS1             */ 03280000
*      2 UCMCORE CHAR(1),           /* WTO PURGE ROUTINE SWS         */ 03281000
*      2 UCMMODEL PTR(8),           /* SYSTEM MODEL NUMBER           */ 03282000
*      2 UCMINCR BIT(8),            /* ERROR HANDLING FLAGS USED BY     03283000
*                                      CONSOLE INITIALIZATION ROUTINE*/ 03284000
*/*      THE FOLLOWING FIELDS ARE USED FOR ACCESSING UCM INDIVIDUAL  */ 03285000
*/*      DEVICE ENTRIES.  THEY MUST BE DEFINED IN THE ORDER SHOWN.   */ 03286000
*                                                                  0123 03287000
*      2 UCMVDATA CHAR(12) BDY(WORD),/* UCM ENTRY ACCESSING DATA     */ 03288000
*       3 UCMVEA PTR(31),           /* ADDRESS OF FIRST UCM ENTRY    */ 03289000
*       3 UCMVEZ FIXED(31),         /* LENGTH OF A UCM ENTRY (IN   0123 03290000
*                                      BYTES)                        */ 03291000
*       3 UCMVEL PTR(31),           /* ADDRESS OF LAST UCM ENTRY     */ 03292000
*/*      SAVE AREAS FOR REFRESHABILTY ROUTINES                       */ 03293000
*                                                                  0123 03294000
*      2 UCMSAVE3(14) FIXED(31),    /* SAVE AREA FOR ED2             */ 03295000
*      2 UCMSAVE4(16) FIXED(31),    /* SAVE AREA FOR IEAVCTSK MDC034 */ 03296000
*      2 UCMR9SV FIXED(31),         /* SAVE AREA FOR ED2             */ 03297000
*      2 UCMMNTR PTR(31),           /* ADDRESS OF MONITOR ROUTINE  0123 03298000
*                                      MDC003                        */ 03299000
*      2 UCMMNECB FIXED(31),        /* ECB INDICATING MONITOR TPUTS     03300000
*                                      TO DO MDC004                  */ 03301000
*      2 UCMTRECB FIXED(31),        /* ECB INDICATING TPUTTER SHOULD    03302000
*                                      TERMINATE MDC005              */ 03303000
*      2 UCMMQPTR PTR(31),          /* POINTER TO FIRST ELEMENT ON 0123 03304000
*                                      MONITOR QUEUE MDC006          */ 03305000
*      2 UCMMQEND PTR(31),          /* POINTER TO LAST ELEMENT ON  0123 03306000
*                                      MONITOR QUEUE MDC007          */ 03307000
