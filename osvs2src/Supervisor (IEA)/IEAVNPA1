*                                                                  0134 03717000
*   DECLARE                                                        0134 03718000
*     1 NIPPAHDR CHAR(8) BDY(DWORD) BASED,                         0134 03719000
*      2 NIPPAQ PTR(31),            /* CHAIN PTR TO NEXT ON QUEUE    */ 03720000
*      2 NIPPABYT PTR(31);          /* PTR - NEXT AVAIL. BYTE        */ 03721000
*/*   PARAMETER AREA                                                 */ 03722000
*                                                                  0135 03723000
*   DECLARE                                                        0135 03724000
*     1 NIPPAREA BDY(DWORD) BASED,  /* PARAMETER AREA BEGINNING      */ 03725000
*      2 * CHAR(LENGTH(NIPPAHDR)),  /* PARMAREA HEADER               */ 03726000
*                                                                  0135 03727000
*     /***************************************************************/ 03728000
*     /*                                                             */ 03729000
*     /* HERE BEGINS: FOR INITIAL PARMAREA - INITIAL FIELDS FOR OTHER*/ 03730000
*     /* PARMAREAS - RECORD BUFFER AREA                              */ 03731000
*     /*                                                             */ 03732000
*     /***************************************************************/ 03733000
*                                                                  0135 03734000
*      2 NIPPARC FIXED(31),         /* RETURN CODE FROM IEAPSPIO     */ 03735000
*      2 NIPPABDL CHAR(18),         /* PARMLIB BLDL ENTRY            */ 03736000
*       3 NIPPABDH CHAR(4),         /* BLDL HEADER                   */ 03737000
*        4 NIPPA2BF CHAR(1),        /* BEGINNING OF BUFFER AREA      */ 03738000
*       3 NIPPANAM CHAR(8),         /* PARMLIB MEMBER NAME           */ 03739000
*       3 NIPPATTR CHAR(3),         /* MEMBER TTR ENTRY              */ 03740000
*       3 * CHAR(3),                /* * RESERVED FOR BLDL ENTRY *   */ 03741000
*      2 * CHAR(2),                 /* UNASSIGNED                    */ 03742000
*      2 NIPPAIOD CHAR(216) BDY(DWORD),/* I/O DATA AREA              */ 03743000
*       3 NIPPATXT CHAR(32),        /* CCW'S - READ TEXT RECORDS     */ 03744000
*       3 NIPPASID CHAR(5),         /* SEARCH ID FOR NEXT TEXT RCD   */ 03745000
*       3 * CHAR(3),                /* ** UNASSIGNED **              */ 03746000
*       3 NIPPAIOB CHAR(40),        /* PARMLIB IOB                   */ 03747000
*       3 NIPPADCB CHAR(4),         /* PARMLIB DCB                   */ 03748000
*       3 NIPPAECB CHAR(4),         /* PARMLIB ECB                   */ 03749000
*        4 NIPPAECC CHAR(1),        /* - PARMLIB ECB CONDITION CODE  */ 03750000
*        4 * CHAR(3),               /* - REST OF PARMLIB ECB         */ 03751000
