*/*    SUBROUTINE NPADEVSR                                           */ 04760000
*/*      DETERMINES IF THE SPECIFIED HARDCOPY DEVICE                 */ 04761000
*/*      OR LOG IS USABLE                                            */ 04762000
*/************************************************************       */ 04763000
*/*    INPUT                                                         */ 04764000
*/*       DEVICE    THREE CHARACTER HEX DEVICE ADDRESS               */ 04765000
*/*       UCMBASPT  UCM FOUNDATION BASE REGISTER                     */ 04766000
*/*       UCMMCSPT  UCM PREFIX BASE REGISTER                         */ 04767000
*/*    OUTPUT                                                        */ 04768000
*/*       UCMDISPB                                                   */ 04769000
*/*       UCMHCUCM                                                   */ 04770000
*/*    RETURN CODES                                                  */ 04771000
*/*       ZERO      VALID HARDCOPY DEVICE                            */ 04772000
*/*       FOUR      INVALID HARDCOPY DEVICE                          */ 04773000
*/*    EXTERNAL INTERFACES                                           */ 04774000
*/*       UCBFN                                                      */ 04775000
*/************************************************************       */ 04776000
*                                                                  0381 04777000
*NPADEVSR:                                                         0381 04778000
*   PROCEDURE OPTIONS(NOSAVEAREA);  /* NPADEVSR ENTRY POINT          */ 04779000
         B     @PB00002                                            0381 04780000
NPADEVSR STM   @14,@12,12(@13)                                     0381 04781000
*   RESPECIFY                                                      0382 04782000
*    (RPARM,                                                       0382 04783000
*     RNVT,                                                        0382 04784000
*     RCVT,                                                        0382 04785000
*     RPTAB,                                                       0382 04786000
*     RCODE,                                                       0382 04787000
*     UCMBASPT,                                                    0382 04788000
*     UCMMCSPT,                                                    0382 04789000
*     RUCME,                                                       0382 04790000
*     RSCAN,                                                       0382 04791000
*     UCBPTR) RESTRICTED;                                          0382 04792000
*   RPARM=DEVICE;                   /* GET EBCDIC DEVICE ADDR. FOR 0383 04793000
*                                      NIPUCBFN                      */ 04794000
*                                                                  0383 04795000
         L     RPARM,DEVICE                                        0383 04796000
*   /*****************************************************************/ 04797000
*   /*                                                               */ 04798000
*   /* CALL NIP UCB FIND ROUTINE                                     */ 04799000
*   /*                                                               */ 04800000
*   /*****************************************************************/ 04801000
*                                                                  0384 04802000
*   GEN REFS(RNVT,NVTUCBFN,RPARM) SETS(RPARM,REXIT,RENTRY)         0384 04803000
*       (IEAPMNIP     TYPE=UCBFN,PARAM=RPARM);                     0384 04804000
         IEAPMNIP     TYPE=UCBFN,PARAM=RPARM                            04805000
*   UCBPTR=RPARM;                   /* PICK UP ADDRESS OF UCB        */ 04806000
         LR    UCBPTR,RPARM                                        0385 04807000
*   IF UCBPTR=0 THEN                /* TEST FOR UCB FOUND            */ 04808000
         LTR   UCBPTR,UCBPTR                                       0386 04809000
         BZ    @RT00386                                            0386 04810000
*     GO TO NPADEINV;               /* UCB NOT FOUND - DEVICE IS   0387 04811000
*                                      UNACCEPTABLE                  */ 04812000
*   IF UCBONLI='0'B|                /* UCB ONLINE?           @ZA01929*/ 04813000
*       UCBDUC='1'B|                /* UNIT CHECK IND.       @ZA04124*/ 04814000
*       UCBNRY='1'B THEN            /* OR NOT READY          @ZA01929*/ 04815000
         TM    UCBONLI(UCBPTR),B'10000000'                         0388 04816000
         BZ    @RT00388                                            0388 04817000
         TM    UCBDUC(UCBPTR),B'00100000'                          0388 04818000
         BO    @RT00388                                            0388 04819000
         TM    UCBNRY(UCBPTR),B'01000000'                          0388 04820000
         BO    @RT00388                                            0388 04821000
*     GOTO NPADEUNA;                /* DEVICE IS OFFLINE     @ZA01929   04822000
*                                      UNIT CHECK STATUS     @ZA04124   04823000
*                                      OR NOT READY - NOT    @ZA01929   04824000
*                                      AVAILABLE AS          @ZA01929   04825000
*                                      HARDCOPY              @ZA01929*/ 04826000
*   DO RUCME=UCMVEA TO UCMVEL BY UCMVEZ;/* LOOP THROUGH ALL UCM    0390 04827000
*                                      DEVICE ENTRIES UNTIL THE    0390 04828000
*                                      SPECIFIED CONSOLE IS FOUND    */ 04829000
         L     RUCME,UCMVEA(,UCMBASPT)                             0390 04830000
         B     @DE00390                                            0390 04831000
@DL00390 DS    0H                                                  0391 04832000
*     IF UCBPTR=UCMUCB THEN         /* IS THIS SPECIFIED CONSOLE     */ 04833000
         C     UCBPTR,UCMUCB(,RUCME)                               0391 04834000
         BNE   @RF00391                                            0391 04835000
*       DO;                         /* YES, THIS IS SPEC DEVICE      */ 04836000
*         IF UCMDISPC='1'B THEN     /* TEST FOR GRAPHICS CONSOLE     */ 04837000
         TM    UCMDISPC(RUCME),B'00100000'                         0393 04838000
         BO    @RT00393                                            0393 04839000
*           GO TO NPADEINV;         /* YES, IT IS A GRAPHICS CONSOLE,   04840000
*                                      WHICH IS INVALID AS THE     0394 04841000
*                                      HARDCOPY CONSOLE              */ 04842000
*         IF UCMOF='0'B THEN        /* TEST FOR OUTPUT CAPABILITY    */ 04843000
         TM    UCMOF(RUCME),B'10000000'                            0395 04844000
         BZ    @RT00395                                            0395 04845000
*           GO TO NPADEINV;         /* INPUT ONLY CONSOLE - INVALID     04846000
*                                      AS THE HARDCOPY CONSOLE       */ 04847000
*         ELSE                      /* VALID HARDCOPY CONSOLE        */ 04848000
*           DO;                     /* FLAG DEVICE AS HARDCOPY       */ 04849000
*             UCMDISPB='1'B;        /* FLAG ENTRY AS HARDCOPY CONSOLE*/ 04850000
         OI    UCMDISPB(RUCME),B'01000000'                         0398 04851000
*             UCMHCUCM=RUCME;       /* PUT ENTRY ADDRESS IN UCM    0399 04852000
*                                      PREFIX                        */ 04853000
         ST    RUCME,UCMHCUCM(,UCMMCSPT)                           0399 04854000
*             RETURN CODE(ZERORC);  /* RETURN TO CALLER - VALID    0400 04855000
*                                      HARDCOPY CONSOLE SPECIFIED    */ 04856000
         SLR   @15,@15                                             0400 04857000
         L     @14,12(,@13)                                        0400 04858000
         LM    @00,@12,20(@13)                                     0400 04859000
         BR    @14                                                 0400 04860000
*           END;                                                   0401 04861000
*       END;                                                       0402 04862000
*   END;                            /* END OF UCM DEVICE ENTRY SCAN     04863000
*                                      LOOP - TEST NEXT DEVICE ENTRY */ 04864000
@RF00391 AL    RUCME,UCMVEZ(,UCMBASPT)                             0403 04865000
@DE00390 C     RUCME,UCMVEL(,UCMBASPT)                             0403 04866000
         BNH   @DL00390                                            0403 04867000
*NPADEINV:                          /* INVALID HARDCOPY DEVICE       */ 04868000
*   RETURN CODE(FOURRC);            /* RETURN TO CALLER              */ 04869000
NPADEINV LA    @15,4                                               0404 04870000
         L     @14,12(,@13)                                        0404 04871000
         LM    @00,@12,20(@13)                                     0404 04872000
         BR    @14                                                 0404 04873000
*NPADEUNA:                          /* HARDCOPY CONSOLE OFFLINE      */ 04874000
*   RETURN CODE(EIGHTRC);           /* RETURN TO CALLER              */ 04875000
NPADEUNA LA    @15,8                                               0405 04876000
         L     @14,12(,@13)                                        0405 04877000
         LM    @00,@12,20(@13)                                     0405 04878000
         BR    @14                                                 0405 04879000
*   RESPECIFY                                                      0406 04880000
*    (RPARM,                                                       0406 04881000
*     RNVT,                                                        0406 04882000
*     RCVT,                                                        0406 04883000
*     RPTAB,                                                       0406 04884000
*     RCODE,                                                       0406 04885000
*     UCMBASPT,                                                    0406 04886000
*     UCMMCSPT,                                                    0406 04887000
*     RUCME,                                                       0406 04888000
*     RSCAN,                                                       0406 04889000
*     UCBPTR) UNRESTRICTED;                                        0406 04890000
*   END NPADEVSR;                   /* END OF SUBROUTINE             */ 04891000
@PB00002 DS    0H                                                  0408 04892000
*   GENERATE NOREFS NOSETS(EJECT);                                 0408 04893000
*                                                                  0408 04894000
         EJECT                                                          04895000
*/************************************************************       */ 04896000
*/*    SUBROUTINE NPAVNUM                                            */ 04897000
*/*      VALIDATES A DECIMAL PARAMETER AND CONVERTS IT TO            */ 04898000
*/*      ITS BINARY VALUE                                            */ 04899000
*/************************************************************       */ 04900000
*/*    INPUT                                                         */ 04901000
*/*       SCANP     ADDRESS OF FIRST BTYE OF EBCDIC PARAMETER        */ 04902000
*/*       PCT       MAXIMUM SIZE IN BYTES OF PARAMETER               */ 04903000
*/*    OUTPUT                                                        */ 04904000
*/*       VALUE     HEXADECIMAL VALUE OF EBCDIC PARAMETER            */ 04905000
*/*       SCANP     ADDRESS OF DELIMETER ENDING PARAMETER            */ 04906000
*/*    RETURN CODES                                                  */ 04907000
*/*       ZERO      PARAMETER ALL NUMERIC AND                        */ 04908000
*/*                 LENGTH DOES NOT EXCEED PCT SPECIFICATION         */ 04909000
*/*       FOUR      PARAMETER NOT ALL NUMERIC OR                     */ 04910000
*/*                 LENGTH EXCEEDS PCT SPECIFICATION                 */ 04911000
*/***** WARNING *****                                                */ 04912000
*/* THE MAXIMUM VALUE FOR PCT IS 4                                   */ 04913000
*/************************************************************       */ 04914000
*                                                                  0409 04915000
*NPAVNUM:                                                          0409 04916000
*   PROCEDURE OPTIONS(NOSAVEAREA);  /* NPAVNUM ENTRY POINT           */ 04917000
         B     @PB00003                                            0409 04918000
NPAVNUM  STM   @14,@12,12(@13)                                     0409 04919000
*   RESPECIFY                                                      0410 04920000
*    (RNVT,                                                        0410 04921000
*     RCVT,                                                        0410 04922000
*     RPTAB,                                                       0410 04923000
*     RSCAN,                                                       0410 04924000
*     RVALUE) RESTRICTED;                                          0410 04925000
*   RSCAN=SCANP;                    /* PICK UP ADDR OF PARAMETER     */ 04926000
         L     RSCAN,SCANP                                         0411 04927000
*   M=PCT+1;                        /* CALCULATE INDEX AFTER END     */ 04928000
         LA    @10,1                                               0412 04929000
         LH    @08,PCT                                             0412 04930000
         ALR   @08,@10                                             0412 04931000
         STH   @08,M                                               0412 04932000
*   DO N=1 TO M;                    /* SCAN FOR A DELIMITER          */ 04933000
         B     @DE00413                                            0413 04934000
@DL00413 DS    0H                                                  0414 04935000
*     IF SCAN(N)=''''|SCAN(N)=' '|SCAN(N)=','|SCAN(N)=')'/* TEST FOR    04936000
*                                      A VALID DELIMITER - BLANK,  0414 04937000
*                                      COMMA, OR CLOSE PARENTHESIS   */ 04938000
*       THEN                        /* DELIMITER FOUND               */ 04939000
         ALR   @10,RSCAN                                           0414 04940000
         BCTR  @10,0                                               0414 04941000
         CLI   SCAN(@10),C''''                                     0414 04942000
         BE    @RT00414                                            0414 04943000
         LH    @10,N                                               0414 04944000
         ALR   @10,RSCAN                                           0414 04945000
         BCTR  @10,0                                               0414 04946000
         CLI   SCAN(@10),C' '                                      0414 04947000
         BE    @RT00414                                            0414 04948000
         LH    @10,N                                               0414 04949000
         ALR   @10,RSCAN                                           0414 04950000
         BCTR  @10,0                                               0414 04951000
         CLI   SCAN(@10),C','                                      0414 04952000
         BE    @RT00414                                            0414 04953000
         LH    @10,N                                               0414 04954000
         ALR   @10,RSCAN                                           0414 04955000
         BCTR  @10,0                                               0414 04956000
         CLI   SCAN(@10),C')'                                      0414 04957000
         BE    @RT00414                                            0414 04958000
*       GO TO NPAVNUMB;             /* CONVERT PARAMETER TO BINARY   */ 04959000
*     ELSE                          /* DELIMITER NOT FOUND           */ 04960000
*       IF SCANZ(N)^='1111'B        /* TEST FOR NUMERIC CHARACTER    */ 04961000
*         THEN                      /* NOT NUMERIC                   */ 04962000
         LH    @10,N                                               0416 04963000
         ALR   @10,RSCAN                                           0416 04964000
         BCTR  @10,0                                               0416 04965000
         TM    SCANZ(@10),B'11110000'                              0416 04966000
         BNO   @RT00416                                            0416 04967000
*         GO TO NPAVNINV;           /* INVALID PARAMETER             */ 04968000
*   END;                            /* END OF SCAN LOOP - TEST NEXT     04969000
*                                      CHARACTER                     */ 04970000
         LH    @08,N                                               0418 04971000
         AH    @08,@CH00051                                        0418 04972000
         LR    @10,@08                                             0418 04973000
@DE00413 STH   @10,N                                               0418 04974000
         CH    @10,M                                               0418 04975000
         BNH   @DL00413                                            0418 04976000
*NPAVNINV:                          /* INVALID PARAMETER             */ 04977000
*   RETURN CODE(FOURRC);            /* RETURN TO CALLER              */ 04978000
NPAVNINV LA    @15,4                                               0419 04979000
         L     @14,12(,@13)                                        0419 04980000
         LM    @00,@12,20(@13)                                     0419 04981000
         BR    @14                                                 0419 04982000
*NPAVNUMB:                          /* A DELIMITER WAS FOUND AFTER A    04983000
*                                      POSSIBLY GOOD PARAMETER       */ 04984000
*   IF N=1 THEN                     /* TEST FOR NO DIGITS SPEC       */ 04985000
NPAVNUMB LH    @10,N                                               0420 04986000
         CH    @10,@CH00051                                        0420 04987000
         BE    @RT00420                                            0420 04988000
*     GO TO NPAVNINV;               /* NO VALUE GIVEN - INVALID    0421 04989000
*                                      PARAMETER SPECIFIED           */ 04990000
*   M=N-1;                          /* NUMBER OF BYTES IN PARAMETER  */ 04991000
         BCTR  @10,0                                               0422 04992000
         STH   @10,M                                               0422 04993000
*   I=MAXPCTD-M;                    /* FIRST BYTE TO BE CONVERTED    */ 04994000
         LCR   @10,@10                                             0423 04995000
         AH    @10,@CH00054                                        0423 04996000
         STH   @10,I                                               0423 04997000
*   CONV=CONV&&CONV;                /* CLEAR CONVERSION WORK AREA    */ 04998000
         XC    CONV(8),CONV                                        0424 04999000
*   CONV2(I:MAXPCT)=SCAN(1:M);      /* MOVE PARAMETER TO WORK AREA   */ 05000000
         LA    @08,CONV2-1(@10)                                    0425 05001000
         LCR   @10,@10                                             0425 05002000
         AH    @10,@CH00046                                        0425 05003000
         EX    @10,@SM02048                                        0425 05004000
*   GENERATE REFS(CONV1,CONV2) SETS(CONV2,RVALUE)/* CONVERT PARAMETER   05005000
*                                      FROM EBCDIC TO BINARY         */ 05006000
*       ;                                                          0426 05007000
          PACK  CONV2,CONV2            PACK PARAMETER                   05008000
          CVB   RVALUE,CONV1           CONVERT TO BINARY                05009000
*   VALUE=RVALUE;                   /* VALUE OF PARAMETER            */ 05010000
         STH   RVALUE,VALUE                                        0427 05011000
*   SCANP=RSCAN+M;                  /* CALCULATE ADDRESS OF DELIMITER*/ 05012000
         LR    @10,RSCAN                                           0428 05013000
         AH    @10,M                                               0428 05014000
         ST    @10,SCANP                                           0428 05015000
*   RETURN CODE(ZERORC);            /* RETURN TO CALLER              */ 05016000
         SLR   @15,@15                                             0429 05017000
         L     @14,12(,@13)                                        0429 05018000
         LM    @00,@12,20(@13)                                     0429 05019000
         BR    @14                                                 0429 05020000
*   RESPECIFY                                                      0430 05021000
*    (RNVT,                                                        0430 05022000
*     RCVT,                                                        0430 05023000
*     RPTAB,                                                       0430 05024000
*     RSCAN,                                                       0430 05025000
*     RVALUE) UNRESTRICTED;                                        0430 05026000
*   END NPAVNUM;                    /* END SUBROUTINE                */ 05027000
@PB00003 DS    0H                                                  0432 05028000
*   GENERATE NOREFS NOSETS(EJECT);                                 0432 05029000
*                                                                  0432 05030000
         EJECT                                                          05031000
*/************************************************************       */ 05032000
*/*    SUBROUTINE NPACONT                                            */ 05033000
*/*       SUBROUTINE TO CHECK PARAMETER FOR CONTINUATION             */ 05034000
*/************************************************************       */ 05035000
*/*    INPUT                                                         */ 05036000
*/*       RSCAN     ADDRESS OF PARAMETER                             */ 05037000
*/*       MSG4P     PARAMETER TYPE FOR WTOR                          */ 05038000
*/*    RETURN CODES                                                  */ 05039000
*/*       ZERO      NO CONTINUATION                                  */ 05040000
*/*       FOUR      VALID CONTINUATION                               */ 05041000
*/*       EIGHT     INVALID CONTINUATION                             */ 05042000
*/************************************************************       */ 05043000
*                                                                  0433 05044000
*NPACONT:                                                          0433 05045000
*   PROCEDURE OPTIONS(NOSAVEAREA);  /* NPACONT ENTRY POINT           */ 05046000
         B     @PB00004                                            0433 05047000
NPACONT  STM   @14,@12,12(@13)                                     0433 05048000
*   RESPECIFY                                                      0434 05049000
*    (RNVT,                                                        0434 05050000
*     RCVT,                                                        0434 05051000
*     RPTAB,                                                       0434 05052000
*     RSCAN,                                                       0434 05053000
*     RTSCAN) RESTRICTED;                                          0434 05054000
*   DO RTSCAN=RSCAN TO BUFEND;      /* LOOP THROUGH PARAMETER BUFFER    05055000
*                                      LOOKING FOR 'CONT'            */ 05056000
         LR    RTSCAN,RSCAN                                        0435 05057000
         B     @DE00435                                            0435 05058000
@DL00435 DS    0H                                                  0436 05059000
*     IF TSCAN=CONTCK THEN          /* TEST FOR 'CONT'               */ 05060000
         CLC   TSCAN(4,RTSCAN),CONTCK                              0436 05061000
         BE    @RT00436                                            0436 05062000
*       GO TO NPACOCON;             /* 'CONT' FOUND - LET OPERATOR 0437 05063000
*                                      CONTINUE SPECIFICATION        */ 05064000
*   END;                            /* END OF LOOP - CONTINUE SCAN   */ 05065000
         AH    RTSCAN,@CH00051                                     0438 05066000
@DE00435 C     RTSCAN,BUFEND                                       0438 05067000
         BNH   @DL00435                                            0438 05068000
*   RETURN CODE(ZERORC);            /* 'CONT' NOT FOUND - RETURN TO     05069000
*                                      CALLER - INDICATE NO        0439 05070000
*                                      CONTINUATION                  */ 05071000
         SLR   @15,@15                                             0439 05072000
         L     @14,12(,@13)                                        0439 05073000
         LM    @00,@12,20(@13)                                     0439 05074000
         BR    @14                                                 0439 05075000
*NPACOCON:                          /* 'CONT' FOUND IN INPUT -     0440 05076000
*                                      REQUEST CONTINUATION          */ 05077000
*   CONTBUF=CONTBUF&&CONTBUF;       /* CLEAR BUFFER                  */ 05078000
*                                                                  0440 05079000
NPACOCON XC    CONTBUF(80),CONTBUF                                 0440 05080000
*   /*****************************************************************/ 05081000
*   /*                                                               */ 05082000
*   /* IEA330A CONTINUE PRM PARM                                     */ 05083000
*   /*                                                               */ 05084000
*   /*****************************************************************/ 05085000
*                                                                  0441 05086000
*   GENERATE REFS(RNVT,NVTWTOR,MSGL330A,MSG4P) SETS(RPARM,REXIT,RENTRY, 05087000
*       MSGBUF)(IEAPMNIP TYPE=WTOR,PARAM=MSGL330A);/* WTOR MESSAGE   */ 05088000
         IEAPMNIP TYPE=WTOR,PARAM=MSGL330A                              05089000
*   DO RTSCAN=ADDR(CONTBUF) TO CONTEND;/* SCAN CONTINUATION BUFFER 0442 05090000
*                                      FOR 'CONT'                    */ 05091000
         LA    RTSCAN,CONTBUF                                      0442 05092000
         B     @DE00442                                            0442 05093000
@DL00442 DS    0H                                                  0443 05094000
*     IF TSCAN=CONTCK THEN          /* TEST FOR 'CONT'               */ 05095000
         CLC   TSCAN(4,RTSCAN),CONTCK                              0443 05096000
         BNE   @RF00443                                            0443 05097000
*       RETURN CODE(EIGHTRC);       /* 'CONT' FOUND - INVALID      0444 05098000
*                                      CONTINUATION - RETURN TO    0444 05099000
*                                      CALLER WITH RC = 8            */ 05100000
         LA    @15,8                                               0444 05101000
         L     @14,12(,@13)                                        0444 05102000
         LM    @00,@12,20(@13)                                     0444 05103000
         BR    @14                                                 0444 05104000
*   END;                            /* END OF CONTINUATION BUFFER  0445 05105000
*                                      SCAN LOOP                     */ 05106000
@RF00443 AH    RTSCAN,@CH00051                                     0445 05107000
@DE00442 C     RTSCAN,CONTEND                                      0445 05108000
         BNH   @DL00442                                            0445 05109000
*   RETURN CODE(FOURRC);            /* RETURN TO CALLER - VALID    0446 05110000
*                                      CONTINUATION SPECIFIED        */ 05111000
         LA    @15,4                                               0446 05112000
         L     @14,12(,@13)                                        0446 05113000
         LM    @00,@12,20(@13)                                     0446 05114000
         BR    @14                                                 0446 05115000
*   RESPECIFY                                                      0447 05116000
*    (RNVT,                                                        0447 05117000
*     RCVT,                                                        0447 05118000
*     RPTAB,                                                       0447 05119000
*     RSCAN,                                                       0447 05120000
*     RTSCAN) UNRESTRICTED;                                        0447 05121000
*   END NPACONT;                    /* END OF SUBROUTINE             */ 05122000
*   END IEAVNPA1                    /* END OF MODULE                 */ 05123000
*                                                                  0449 05124000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 05125000
*/*%INCLUDE SYSLIB  (IHANVT  )                                       */ 05126000
*/*%INCLUDE SYSLIB  (CVT     )                                       */ 05127000
*/*%INCLUDE SYSLIB  (IEFUCBOB)                                       */ 05128000
*/*%INCLUDE SYSLIB  (IEECUCM )                                       */ 05129000
*/*%INCLUDE SYSLIB  (IEAPMNIP)                                       */ 05130000
*/*%INCLUDE SYSLIB  (IEAPPNIP)                                       */ 05131000
*                                                                  0449 05132000
*       ;                                                          0449 05133000
         B     @EL00001                                            0449 05134000
@DATA    DS    0H                                                       05135000
@CH00051 DC    H'1'                                                     05136000
@CH00048 DC    H'2'                                                     05137000
@CH00044 DC    H'3'                                                     05138000
@CH00046 DC    H'4'                                                     05139000
@CH00054 DC    H'5'                                                     05140000
@CH00181 DC    H'6'                                                     05141000
@CH00059 DC    H'8'                                                     05142000
@CH00037 DC    H'16'                                                    05143000
@CH02006 DC    H'20'                                                    05144000
@CH01434 DC    H'100'                                                   05145000
@CH02005 DC    H'32767'                                                 05146000
@CH00038 DC    XL2'FFFF'                                                05147000
@SM02048 MVC   0(0,@08),SCAN(RSCAN)                                     05148000
         DS    0F                                                       05149000
@SA00001 DS    18F                                                      05150000
         DS    0F                                                       05151000
@CF00042 DC    F'65536'                                                 05152000
@CF02043 DC    F'-4'                                                    05153000
@CF00040 DC    XL4'F340'                                                05154000
         DS    0D                                                       05155000
SCANP    DS    A                                                        05156000
