*       3 UCBOLDSM BIT(1),          /* OLTEP COMMUNICATING DIRECTLY     02193000
*                                      WITH THE MASS STORAGE CONTROL    02194000
*                                      (MSC), NOT THROUGH THE MASS 0104 02195000
*                                      STORAGE SYSTEM COMMUNICATOR 0104 02196000
*                                      (MSSC) (MDC301)       @Z30LP9A*/ 02197000
*       3 UCBRSV02 BIT(1),          /* RESERVED                      */ 02198000
*       3 UCBRV011 BIT(1),          /* RESERVED MDC013               */ 02199000
*       3 UCBMONT BIT(1),           /* VOLUME TO BE MOUNTED IS TO BE    02200000
*                                      RETAINED OR CONTAIN A PASSED     02201000
*                                      DATA SET. (SET BY DEVICE    0104 02202000
*                                      ALLOCATION OR DATA MANAGEMENT    02203000
*                                      FOR OS/VS2) MDC014            */ 02204000
*      2 UCBFL5 BIT(8),             /* FLAGS                         */ 02205000
*       3 UCBDCC BIT(1),            /* DISCONNECT COMMAND CHAIN    0104 02206000
*                                      DEVICE MDC098                 */ 02207000
*       3 UCBAF BIT(1),             /* ATTENTION FOR THIS CONSOLE  0104 02208000
*                                      DEVICE IS TO BE PROCESSED BY     02209000
*                                      THE COMMUNICATIONS TASK ICB321*/ 02210000
*        4 UCBAMV BIT(1),           /* SUCCESSFUL COMPARISON CHECKING   02211000
*                                      OF THE ACCESS METHOD CATALOG     02212000
*                                      AND THE VTOC (VSAM DIRECT   0104 02213000
*                                      ACCESS DEVICES ONLY) ICB437   */ 02214000
*       3 UCBSASK BIT(1),           /* DEVICE REQUIRES STAND ALONE 0104 02215000
*                                      SEEK MDC099                   */ 02216000
*       3 UCBVSDR BIT(1),           /* DEVICE HAS VARIABLE LENGTH  0104 02217000
*                                      SDR'S                         */ 02218000
*       3 UCBENVRD BIT(1),          /* DEVICE RETURNS ENVIRONMENTAL     02219000
*                                      DATA MDC100                   */ 02220000
*       3 UCBNALOC BIT(1),          /* THIS OFFLINE DEVICE IS BEING     02221000
*                                      USED BY A SYSTEM COMPONENT. 0104 02222000
*                                      THE DEVICE STATUS MUST NOT  0104 02223000
*                                      CHANGE TO ONLINE NOR WILL IT     02224000
*                                      BE ALLOCATED. THE LAST PATH/     02225000
*                                      CHANNEL/CPU TO THE DEVICE MUST   02226000
*                                      NOT BE VARY'ED OFFLINE. THE 0104 02227000
*                                      DEVICE IS UNAVAILABLE FOR   0104 02228000
*                                      USAGE BY ANOTHER SYSTEM     0104 02229000
*                                      COMPONENT WHICH PROCESSES   0104 02230000
*                                      OFFLINE DEVICES. TO SET THIS     02231000
*                                      INDICATOR ON, A COMPONENT MUST   02232000
*                                      OBTAIN VIA ENQ, EXCLUSIVE,  0104 02233000
*                                      SYSTEM LEVEL CONTROL OF     0104 02234000
*                                      RESOURCE SYSIEFSD, Q4.      0104 02235000
*                                      SERIALIZATION IS NOT REQUIRED    02236000
*                                      TO TURN THIS INDICATOR OFF. 0104 02237000
*                                      MDC027                        */ 02238000
*       3 UCBALTCU BIT(1),          /* DEVICE HAS AN ALTERNATE     0104 02239000
*                                      CONTROL UNIT ADDRESS          */ 02240000
*       3 UCBALTPH BIT(1),          /* DEVICE HAS AN ALTERNATE PATH  */ 02241000
*      2 UCBID CHAR(1),             /* UCB IDENTIFICATION (FF)       */ 02242000
*      2 UCBSTAT BIT(8),            /* DEVICE STATUS                 */ 02243000
*       3 UCBONLI BIT(1),           /* DEVICE IS ONLINE              */ 02244000
*       3 UCBCHGS BIT(1),           /* DEVICE STATUS IS TO BE CHANGED   02245000
*                                      FROM ONLINE TO OFFLINE, AND 0104 02246000
*                                      EITHER ALLOCATION IS ENQUEUED    02247000
*                                      ON DEVICES OR THE DEVICE IS 0104 02248000
*                                      ALLOCATED. (BIT 0 IS ALSO ON.)*/ 02249000
*       3 UCBRESV BIT(1),           /* THE MOUNT STATUS OF THE VOLUME   02250000
*                                      ON THIS DEVICE IS RESERVED    */ 02251000
*       3 UCBUNLD BIT(1) ABNORMAL,  /* UNLOAD OPERATOR COMMAND HAS 0104 02252000
*                                      BEEN ADDRESSED TO THIS DEVICE.   02253000
*                                      THE DEVICE IS NOT YET       0104 02254000
*                                      UNLOADED. MDC167              */ 02255000
