*   /*****************************************************************/ 03565000
*   /*                                                               */ 03566000
*   /* ** NIPWTO MESSAGE HEADER THE FOLLOWING STRUCTURE MAPS THE     */ 03567000
*   /* NIPWTO MESSAGE HEADER THAT IS GENERATED BY THE BAL MACRO      */ 03568000
*   /* -IEAPMNIP TYPE=HEADER- AND IS USED TO PASS INFORMATION TO THE */ 03569000
*   /* NIPWTO SERVICE ROUTINE. THE MESSAGE LENGTH FIELD, NWTOLNG,    */ 03570000
*   /* INCLUDES THE LENGTH OF THE MESSAGE HEADER ITSELF.             */ 03571000
*   /*                                                               */ 03572000
*   /*****************************************************************/ 03573000
*                                                                  0128 03574000
*   DECLARE                                                        0128 03575000
*     1 NWTOHDR BASED,              /* MESSAGE HEADER START          */ 03576000
*      2 NWTOLNG FIXED(15),         /* MESSAGE LENGTH                */ 03577000
*      2 NWTOFL CHAR(2),            /* NIP MESSAGE FLAGS             */ 03578000
*       3 NWTOFLNH BIT(1),          /* NOT TO BE HARDCOPIED          */ 03579000
*       3 NWTOFLNC BIT(1),          /* SUPRESS BLANK COMPRESS        */ 03580000
*       3 * BIT(14);                /* RESERVED                      */ 03581000
*                                                                  0128 03582000
*   /*****************************************************************/ 03583000
*   /*                                                               */ 03584000
*   /* ** END OF NIPWTO MESSAGE HEADER ** NIPWTOR PARAMETER LIST THE */ 03585000
*   /* FOLLOWING STRUCTURE MAPS THE NIPWTOR PARAMETER LIST THAT IS   */ 03586000
*   /* GENERATED BY THE BAL MACRO -IEAPMNIP TYPE=PLIST- AND IS USED  */ 03587000
*   /* TO PASS INFORMATION TO THE NIPWTOR SERVICE ROUTINE.           */ 03588000
*   /*                                                               */ 03589000
*   /*****************************************************************/ 03590000
*                                                                  0129 03591000
*   DECLARE                                                        0129 03592000
*     1 NWTORLST BASED,             /* PARM LIST START               */ 03593000
*      2 NWTORRPA PTR(31),          /* REPLY AREA ADDRESS            */ 03594000
*      2 NWTORECB PTR(31),          /* REPLY ECB ADDRESS             */ 03595000
*      2 NMTORRDL FIXED(15),        /* REPLY AREA LENGTH             */ 03596000
*      2 NWTORFL CHAR(1),           /* NIP WTOR FLAGS                */ 03597000
*       3 NWTORFLA BIT(1),          /* ASYNCHRONOUS REQUEST          */ 03598000
*       3 * BIT(3),                 /* RESERVED                      */ 03599000
*       3 NWTORFLB BIT(1),          /* PROVIDE SQA BUFFER            */ 03600000
*       3 * BIT(3),                 /* RESERVED                      */ 03601000
