*/*                                                                  */ 03372000
*/*                 EVENT INDICATION LIST (EIL)                      */ 03373000
*/*                                                                  */ 03374000
*/********************************************************************/ 03375000
*                                                                  0124 03376000
*   DECLARE                                                        0124 03377000
*     1 UCMEIL BASED(UCMEILPT) BDY(WORD),/* BEGINNING OF EIL         */ 03378000
*      2 * PTR(8),                  /* LENGTH OF EIL IN WORDS        */ 03379000
*      2 UCMRPYL CHAR(1),           /* LAST ASSIGNED REPLY I.D.      */ 03380000
*      2 UCMRTCT PTR(8),            /* ROUTE COUNT                   */ 03381000
*      2 UCMRSV15 PTR(8),           /* RESERVED                      */ 03382000
*      2 UCMNIPTR PTR(31),          /* ADDRESS OF NIP'S 2K WTL BUFFER*/ 03383000
*      2 UCMXECBA PTR(31),          /* ADDRESS OF EXTERNAL INTERRUPT    03384000
*                                      ECB                           */ 03385000
*      2 UCMAECBA PTR(31),          /* ADDRESS OF ATTENTION INTERRUPT   03386000
*                                      ECB                           */ 03387000
*      2 UCMOECBA PTR(31),          /* ADDRESS OF WTO/R REQUEST ECB  */ 03388000
*      2 UCMDECBA PTR(31),          /* ADDRESS OF DOM REQUEST ECB    */ 03389000
*      2 UCMRECBA PTR(31);          /* ADDRESS OF RMS REQUEST ECB  0124 03390000
*                                      (OS/VS1) CONSOLE RECOVERY ECB    03391000
*                                      (OS/VS2) MDC019               */ 03392000
*/*      THE FOLLOWING PART OF THE EIL IS A VARIABLE LIST OF         */ 03393000
*/*      POINTERS TO I/O REQUEST ECBS FOR EACH CONSOLE DEVICE        */ 03394000
*/*      DEFINED AT SYSGEN.  IN OS/VS1, THE LIST CONTAINS A MINIMUM  */ 03395000
*/*      OF 1 ENTRY.  IN OS/VS2, THE LIST CONTAINS A MINIMUM OF 2    */ 03396000
*/*      ENTRIES.  THE LIST IS VARIABLE ONLY AT SYSGEN.  THE LAST    */ 03397000
*/*      ENTRY HAS A HIGH-ORDER BYTE OF X'80'.                       */ 03398000
*                                                                  0125 03399000
*   DECLARE                                                        0125 03400000
*     1 UCMIECBA DEF(UCMRECBA) POS(5) BDY(WORD),                   0125 03401000
*      2 UCMIECBF CHAR(1),          /* ECB LIST-END FLAG             */ 03402000
*      2 UCMIECBP PTR(24);          /* PTR TO I/O REQUEST ECB        */ 03403000
*/********************************************************************/ 03404000
*/*                                                                  */ 03405000
