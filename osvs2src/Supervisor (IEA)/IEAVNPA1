*   /* DEFINE HARDCOPY CONSOLE                                       */ 04038000
*   /*                                                               */ 04039000
*   /*****************************************************************/ 04040000
*                                                                  0185 04041000
*                                                                  0185 04042000
*/************************************************************       */ 04043000
*/*    SUBROUTINE NP7HDCPY                                           */ 04044000
*/*       DEFINES THE HARDCOPY CONSOLE                               */ 04045000
*/************************************************************       */ 04046000
*/*    INPUT                                                         */ 04047000
*/*       CVTCUCB                                                    */ 04048000
*/*       HARDCPY SYSTEM PARAMETER                                   */ 04049000
*/*       UCM                                                        */ 04050000
*/*    OUTPUT                                                        */ 04051000
*/*       UCMDISPB                                                   */ 04052000
*/*       UCMHCUCM                                                   */ 04053000
*/*       UCMHRDRT                                                   */ 04054000
*/*       UCMSDS1A                                                   */ 04055000
*/*       UCMSDS1B                                                   */ 04056000
*/*       UCMSYSC                                                    */ 04057000
*/*       UCMSYSG                                                    */ 04058000
*/*    INTERNAL INTERFACES                                           */ 04059000
*/*       NP7DEVSR                                                   */ 04060000
*/*       NP7CONT                                                    */ 04061000
*/*       NP7VNUM                                                    */ 04062000
*/*    EXTERNAL INTERFACES                                           */ 04063000
*/*       NIP WTOR                                                   */ 04064000
*/************************************************************       */ 04065000
*                                                                  0185 04066000
*   RESPECIFY                                                      0185 04067000
*    (RPARM,                                                       0185 04068000
*     RNVT,                                                        0185 04069000
*     RCVT,                                                        0185 04070000
*     RPTAB,                                                       0185 04071000
*     RCODE,                                                       0185 04072000
*     UCMBASPT,                                                    0185 04073000
*     UCMMCSPT,                                                    0185 04074000
*     RSCAN,                                                       0185 04075000
*     RTFIELD,                                                     0185 04076000
*     RTCODE,                                                      0185 04077000
*     UCBPTR) RESTRICTED;                                          0185 04078000
*   UCMBASPT=CVTCUCB;               /* SET UP UCM BASE POINTER       */ 04079000
         L     UCMBASPT,CVTCUCB(,RCVT)                             0186 04080000
*   UCMMCSPT=UCMPRFXP;              /* SET UP UCM PREFIX POINTER     */ 04081000
         LR    @14,UCMBASPT                                        0187 04082000
         AL    @14,@CF02043                                        0187 04083000
