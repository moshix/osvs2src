         TITLE 'IEAVNPM3, NIP OPEN/MOUNT ROUTINE                       *00001000
                        '                                               00002000
IEAVNPM3 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVNPM3  74.354'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @11,0                                               0001 00011000
@PSTART  LA    @12,4095(,@11)                                      0001 00012000
         USING @PSTART,@11                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
         ST    @13,@SA00001+4                                      0001 00015000
         LA    @14,@SA00001                                        0001 00016000
         ST    @14,8(,@13)                                         0001 00017000
         LR    @13,@14                                             0001 00018000
*                                                                       00019000
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF          00020000
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES      00021000
** WILL ASSEMBLE PROPERLY.                                              00022000
NP2ABNOR BR  14                                                         00023000
         EJECT                                                          00024000
         EJECT                                                          00025000
*                                                                  0281 00026000
         B     @PB00002                                            0281 00027000
NP2BDEB  STM   @14,@12,@SA00002                                    0281 00028000
*   /*****************************************************************/ 00029000
*   /*                                                               */ 00030000
*   /* ROUND BUILD AREA ADDR TO DOUBLEWORD BOUNDARY                  */ 00031000
*   /*                                                               */ 00032000
*   /*****************************************************************/ 00033000
*                                                                  0282 00034000
*   DEBPTR=(DEBPTR+7)&ROUNDDWD;                                    0282 00035000
*                                                                  0282 00036000
         LA    @10,7                                               0282 00037000
         AL    @10,DEBPTR                                          0282 00038000
         N     @10,@CF00078                                        0282 00039000
         ST    @10,DEBPTR                                          0282 00040000
*   /*****************************************************************/ 00041000
*   /*                                                               */ 00042000
*   /* IS THIS A TAPE DEVICE                                         */ 00043000
*   /*                                                               */ 00044000
*   /*****************************************************************/ 00045000
*                                                                  0283 00046000
*   IF UCB3TAPE^='1'B THEN          /* NOT A TAPE                    */ 00047000
         L     @15,UCBPTR                                          0283 00048000
         TM    UCB3TAPE(@15),B'10000000'                           0283 00049000
         BNO   @RT00283                                            0283 00050000
*     GO TO NP2BDASD;               /* PROCESS DASD                  */ 00051000
*   DEBBASIC=DEBBASIC&&DEBBASIC;    /* CLEAR DEB                     */ 00052000
*                                                                  0285 00053000
         XC    DEBBASIC(32,@10),DEBBASIC(@10)                      0285 00054000
*   /*****************************************************************/ 00055000
*   /*                                                               */ 00056000
*   /* SEE IF NUCLEUS BUFFER REQUEST                                 */ 00057000
*   /*                                                               */ 00058000
*   /*****************************************************************/ 00059000
*                                                                  0286 00060000
*   IF NOPNFLLB='1'B THEN           /* NUCLEUS BUFFER REQUEST        */ 00061000
*                                                                  0286 00062000
         L     @10,NPB2PPTR                                        0286 00063000
         TM    NOPNFLLB(@10),B'00000100'                           0286 00064000
         BNO   @RF00286                                            0286 00065000
*     /***************************************************************/ 00066000
*     /*                                                             */ 00067000
*     /* UPDATE BUILD AREA ADDRESS                                   */ 00068000
*     /*                                                             */ 00069000
*     /***************************************************************/ 00070000
*                                                                  0287 00071000
*     NVTNUCND=DEBPTR+LENGTH(DEBBASIC)+LENGTH(DEBBAS02);/*   @YM05571*/ 00072000
*                                                                  0287 00073000
         L     @10,DEBPTR                                          0287 00074000
         AH    @10,@CH00962                                        0287 00075000
         ST    @10,NVTNUCND(,RNVT)                                 0287 00076000
*   /*****************************************************************/ 00077000
*   /*                                                               */ 00078000
*   /* SET UP DEB TAPE PORTION                                       */ 00079000
*   /*                                                               */ 00080000
*   /*****************************************************************/ 00081000
*                                                                  0288 00082000
*   DEBEXSCL=NDASD;                 /* SET EXTENT SCALE FOR TAPE     */ 00083000
@RF00286 L     @10,DEBPTR                                          0288 00084000
         MVI   DEBEXSCL(@10),X'02'                                 0288 00085000
*   DEBSUCBB=UCBPTR;                /* PUT UCB ADDR IN DEB           */ 00086000
*                                                                  0289 00087000
         L     @15,UCBPTR                                          0289 00088000
         STCM  @15,7,DEBSUCBB(@10)                                 0289 00089000
*   /*****************************************************************/ 00090000
*   /*                                                               */ 00091000
*   /* 7 OR 9 TRACK TAPE                                             */ 00092000
*   /*                                                               */ 00093000
*   /*****************************************************************/ 00094000
*                                                                  0290 00095000
*   IF UCB2OPT0='1'B THEN           /* 7 TRACK TAPE                  */ 00096000
*                                                                  0290 00097000
         TM    UCB2OPT0(@15),B'10000000'                           0290 00098000
         BNO   @RF00290                                            0290 00099000
*     /***************************************************************/ 00100000
*     /*                                                             */ 00101000
*     /* SET DEVICE MODIFIER TO 7 TRACK TAPE MODE                    */ 00102000
*     /*                                                             */ 00103000
*     /***************************************************************/ 00104000
*                                                                  0291 00105000
*     DEBSDVM=TAPE7;                                               0291 00106000
         MVI   DEBSDVM(@10),X'93'                                  0291 00107000
*   ELSE                            /* 9 TRACK TAPE                  */ 00108000
*                                                                  0292 00109000
*     /***************************************************************/ 00110000
*     /*                                                             */ 00111000
*     /* SET DEVICE MODIFIER TO 9 TRACK TAPE MODE                    */ 00112000
*     /*                                                             */ 00113000
*     /***************************************************************/ 00114000
*                                                                  0292 00115000
*     DEBSDVM=TAPE9;                                               0292 00116000
         B     @RC00290                                            0292 00117000
@RF00290 L     @10,DEBPTR                                          0292 00118000
         MVI   DEBSDVM(@10),X'C3'                                  0292 00119000
*   GO TO NP2BDCOM;                 /* DO COMMON DEB BUILD           */ 00120000
         B     NP2BDCOM                                            0293 00121000
*NP2BDASD:                          /* BUILD DASD DEB                */ 00122000
*                                                                  0294 00123000
*   /*****************************************************************/ 00124000
*   /*                                                               */ 00125000
*   /* SEE IF NUCLEUS BUFFER REQUEST                                 */ 00126000
*   /*                                                               */ 00127000
*   /*****************************************************************/ 00128000
*                                                                  0294 00129000
*   IF NOPNFLLB='1'B THEN                                          0294 00130000
NP2BDASD L     @10,NPB2PPTR                                        0294 00131000
         TM    NOPNFLLB(@10),B'00000100'                           0294 00132000
         BNO   @RF00294                                            0294 00133000
*     DO;                                                          0295 00134000
*       DEBPTR=DEBPTR+LENGTH(DEBPRE);/* INCR PAST PREFIX             */ 00135000
         LA    @10,8                                               0296 00136000
         L     @00,DEBPTR                                          0296 00137000
         ALR   @00,@10                                             0296 00138000
         ST    @00,DEBPTR                                          0296 00139000
*       DEBPRE=DEBPRE&&DEBPRE;      /* CLEAR PREFIX                  */ 00140000
         SLR   @00,@10                                             0297 00141000
         LR    @10,@00                                             0297 00142000
         XC    DEBPRE(8,@10),DEBPRE(@10)                           0297 00143000
*     END;                                                         0298 00144000
*   ELSE                                                           0299 00145000
*     ;                                                            0299 00146000
@RF00294 DS    0H                                                  0300 00147000
*   DEBBASIC=DEBBASIC&&DEBBASIC;    /* CLEAR BASIC SECTION OF DEB AND   00148000
*                                      ONE EXTENT                    */ 00149000
*                                                                  0300 00150000
         L     @10,DEBPTR                                          0300 00151000
         XC    DEBBASIC(32,@10),DEBBASIC(@10)                      0300 00152000
*   /*****************************************************************/ 00153000
*   /*                                                               */ 00154000
*   /* INITIALIZE BASIC SECTION OF DEB                               */ 00155000
*   /*                                                               */ 00156000
*   /*****************************************************************/ 00157000
*                                                                  0301 00158000
*   DEBEXSCL=DASDEX;                /* DASD EXTENT SCALE             */ 00159000
*                                                                  0301 00160000
         MVI   DEBEXSCL(@10),X'04'                                 0301 00161000
*   /*****************************************************************/ 00162000
*   /*                                                               */ 00163000
*   /* INITIALIZE COMMON BASIC SECTION OF DEB                        */ 00164000
*   /*                                                               */ 00165000
*   /*****************************************************************/ 00166000
*                                                                  0302 00167000
*NP2BDCOM:                                                         0302 00168000
*   DCBDEBAD=DEBPTR;                /* PUT DEB ADDR IN DCB           */ 00169000
NP2BDCOM L     @10,DEBPTR                                          0302 00170000
         L     @15,DCBPTR                                          0302 00171000
         STCM  @10,7,DCBDEBAD(@15)                                 0302 00172000
*   DEBDCBB=DCBPTR;                 /* PUT DCB ADDR IN DEB           */ 00173000
         STCM  @15,7,DEBDCBB(@10)                                  0303 00174000
*   DEBPROTG=IDPRIOR;               /* DEB IDENTIFIER & PRIORITY     */ 00175000
         MVI   DEBPROTG(@10),X'0F'                                 0304 00176000
*   DEBAPPB=CVTXAPG;                /* APPENDAGE TABLE ADDRESS       */ 00177000
         MVC   DEBAPPB(3,@10),CVTXAPG+1(RCVT)                      0305 00178000
*   RETURN;                         /* RETURN TO CALLER              */ 00179000
@EL00002 DS    0H                                                  0306 00180000
@EF00002 DS    0H                                                  0306 00181000
@ER00002 LM    @14,@12,@SA00002                                    0306 00182000
         BR    @14                                                 0306 00183000
*   GENERATE NOREFS NOSETS(EJECT);                                 0307 00184000
*                                                                  0307 00185000
         EJECT                                                          00186000
*   /*****************************************************************/ 00187000
*   /*                                                               */ 00188000
*   /* DEB INITIALIZATION EXTENT PORTION INITIALIZES THE EXTENT      */ 00189000
*   /* DESCRIPTION FORTION OF THE DEB FOR ONE OR MORE EXTENTS IN A   */ 00190000
*   /* SINGLE VOLUME DATASET INTERNAL INTERFACES NP2RDSCB EXTERNAL   */ 00191000
*   /* INTERFACES TTR CONVERT IN IGC018                              */ 00192000
*   /*                                                               */ 00193000
*   /*****************************************************************/ 00194000
*                                                                  0308 00195000
*NP2BXTNT:                                                         0308 00196000
*   ENTRY;                          /* NP2BXTNT ENTRY POINT          */ 00197000
         B     @EC00308                                            0308 00198000
NP2BXTNT STM   @14,@12,@SA00002                                    0308 00199000
@EC00308 DS    0H                                                  0309 00200000
*/* READ IN FORMAT 1 DSCB                                            */ 00201000
*                                                                  0309 00202000
*   FORMAT='1';                     /* FORMAT 1 DSCB                 */ 00203000
*                                                                  0309 00204000
         MVI   FORMAT,C'1'                                         0309 00205000
*   /*****************************************************************/ 00206000
*   /*                                                               */ 00207000
*   /* READ IN FORMAT 1 DSCB                                         */ 00208000
*   /*                                                               */ 00209000
*   /*****************************************************************/ 00210000
*                                                                  0310 00211000
*   CALL NP2RDSCB;                  /* DSCB READ                     */ 00212000
*                                                                  0310 00213000
         BAL   @14,NP2RDSCB                                        0310 00214000
*   /*****************************************************************/ 00215000
*   /*                                                               */ 00216000
*   /*                                                               */ 00217000
*   /*****************************************************************/ 00218000
*                                                                  0311 00219000
*/* CHECK FOR SUCCESSFUL RETURN                                      */ 00220000
*                                                                  0311 00221000
*   GENERATE REFS(RCODE) NOSETS FLOWS(NP2RSUC,NP2NSUC);            0311 00222000
*                                                                  0311 00223000
            B *+4(RCODE)                                                00224000
            B NP2RSUC                                                   00225000
            B NP2NSUC                                                   00226000
*/* FIND OUT HOW MANY EXTENTS                                        */ 00227000
*                                                                  0312 00228000
*NP2RSUC:                                                          0312 00229000
*   IF NOPNFLSX='1'B THEN                                          0312 00230000
NP2RSUC  L     @10,NPB2PPTR                                        0312 00231000
         TM    NOPNFLSX(@10),B'00000001'                           0312 00232000
         BNO   @RF00312                                            0312 00233000
*     EXTCTR=1;                     /* SINGLE EXTENT WANTED          */ 00234000
         MVI   EXTCTR,X'01'                                        0313 00235000
*   ELSE                                                           0314 00236000
*     DO;                                                          0314 00237000
         B     @RC00312                                            0314 00238000
@RF00312 DS    0H                                                  0315 00239000
*       NOEXT=DS1NOEPV;             /* SAVE NUMBER OF EXTENTS        */ 00240000
         LA    @10,NP2F1BUF                                        0315 00241000
         SLR   @00,@00                                             0315 00242000
         IC    @00,DS1NOEPV-44(,@10)                               0315 00243000
         STC   @00,NOEXT                                           0315 00244000
*       EXTCTR=MIN(NOEXT,3);        /* SET UP TO PROCESS UP TO THREE    00245000
*                                      EXTENTS                       */ 00246000
         LA    @10,3                                               0316 00247000
         CR    @00,@10                                             0316 00248000
         BNH   *+6                                                      00249000
         LR    @00,@10                                             0316 00250000
         STC   @00,EXTCTR                                          0316 00251000
*     END;                                                         0317 00252000
*                                                                  0317 00253000
*/* INITALIZE DEB EXTENT FIELDS                                      */ 00254000
*                                                                  0318 00255000
*   EXTPTR=ADDR(DS1EXT1);           /* PTR TO FIRST EXTENT FIELD     */ 00256000
@RC00312 LA    @10,NP2F1BUF                                        0318 00257000
         LA    @10,DS1EXT1-44(,@10)                                0318 00258000
         ST    @10,EXTPTR                                          0318 00259000
*   DEBPTR=DCBDEBAD;                /* PICK UP DEB ADDR FROM DCB     */ 00260000
         L     @10,DCBPTR                                          0319 00261000
         SLR   @00,@00                                             0319 00262000
         ICM   @00,7,DCBDEBAD(@10)                                 0319 00263000
         ST    @00,DEBPTR                                          0319 00264000
*   DEBDAPTR=ADDR(DEBBASND);        /* ADDR OF DEB EXTENSION         */ 00265000
         LR    @10,@00                                             0320 00266000
         LA    @10,DEBBASND(,@10)                                  0320 00267000
         ST    @10,DEBDAPTR                                        0320 00268000
*/* CHECK FOR CONCATENATION                                          */ 00269000
*                                                                  0321 00270000
*   IF DEBRERR='1'B THEN                                           0321 00271000
         LR    @15,@00                                             0321 00272000
         TM    DEBRERR(@15),B'00000001'                            0321 00273000
         BNO   @RF00321                                            0321 00274000
*     DEBDAPTR=DEBDAPTR+DEBNMEXT*LENGTH(DEBDASD);                  0322 00275000
         LR    @15,@00                                             0322 00276000
         SLR   @00,@00                                             0322 00277000
         IC    @00,DEBNMEXT(,@15)                                  0322 00278000
         SLA   @00,4                                               0322 00279000
         ALR   @10,@00                                             0322 00280000
         ST    @10,DEBDAPTR                                        0322 00281000
*   ELSE                                                           0323 00282000
*     ;                                                            0323 00283000
*                                                                  0323 00284000
@RF00321 DS    0H                                                  0324 00285000
*/* IF SINGLE EXTENT DS SET CYLINDER FINLE MASK                      */ 00286000
*                                                                  0324 00287000
*   IF NOPNFLSX='1'B THEN                                          0324 00288000
         L     @10,NPB2PPTR                                        0324 00289000
         TM    NOPNFLSX(@10),B'00000001'                           0324 00290000
         BNO   @RF00324                                            0324 00291000
*     DO;                                                          0325 00292000
*       DEBDVMOD=CFMSK;             /* SET CYLINDER FILE MASK        */ 00293000
         L     @10,DEBDAPTR                                        0326 00294000
         MVI   DEBDVMOD(@10),X'00'                                 0326 00295000
*       GO TO NP2CCHH;              /* GET CCHH FROM DSCB            */ 00296000
         B     NP2CCHH                                             0327 00297000
*     END;                                                         0328 00298000
*   ELSE                                                           0329 00299000
*     ;                                                            0329 00300000
@RF00324 DS    0H                                                  0330 00301000
*NP2EXTEN:                                                         0330 00302000
*                                                                  0330 00303000
*/* PUT IN EITHER TRACK OR CYLINDER FILE MASK                        */ 00304000
*                                                                  0330 00305000
*   IF EXTENT(1)=CYLNDR             /* IS EXTENT CYLINDER BOUND      */ 00306000
*     THEN                                                         0330 00307000
NP2EXTEN L     @10,EXTPTR                                          0330 00308000
         CLI   EXTENT(@10),X'81'                                   0330 00309000
         BNE   @RF00330                                            0330 00310000
*     DEBDVMOD=CFMSK;               /* CYLINDER FILE MASK            */ 00311000
         L     @10,DEBDAPTR                                        0331 00312000
         MVI   DEBDVMOD(@10),X'00'                                 0331 00313000
*   ELSE                                                           0332 00314000
*     DEBDVMOD=TFMSK;               /* TRACK FILE MASK               */ 00315000
         B     @RC00330                                            0332 00316000
@RF00330 L     @10,DEBDAPTR                                        0332 00317000
         MVI   DEBDVMOD(@10),X'18'                                 0332 00318000
*/* MOVE START & END CCHH FROM DSCB TO DEB EXTENT                    */ 00319000
*                                                                  0333 00320000
*NP2CCHH:                           /* SET UP DEB EXTENT             */ 00321000
*   DEBUCBA=UCBPTR;                 /* SET UCB ADDR IN DEB EXTENT    */ 00322000
@RC00330 DS    0H                                                  0333 00323000
NP2CCHH  L     @10,DEBDAPTR                                        0333 00324000
         MVC   DEBUCBA(3,@10),UCBPTR+1                             0333 00325000
*   DEBBINUM=0;                     /* ZERO OUT BIN NO               */ 00326000
         SLR   @15,@15                                             0334 00327000
         STH   @15,DEBBINUM(,@10)                                  0334 00328000
*   DEBSTRCC=EXTENT(3:4);           /* START CCHH                    */ 00329000
         L     @14,EXTPTR                                          0335 00330000
         MVC   DEBSTRCC(2,@10),EXTENT+2(@14)                       0335 00331000
*   DEBSTRHH=EXTENT(5:6);                                          0336 00332000
         MVC   DEBSTRHH(2,@10),EXTENT+4(@14)                       0336 00333000
*   DEBENDCC=EXTENT(7:8);           /* END CCHH                      */ 00334000
         MVC   DEBENDCC(2,@10),EXTENT+6(@14)                       0337 00335000
*   DEBENDHH=EXTENT(9:10);                                         0338 00336000
*                                                                  0338 00337000
         MVC   DEBENDHH(2,@10),EXTENT+8(@14)                       0338 00338000
*/* DETERMINE RELATIVE TRACK ADDRESSES                               */ 00339000
*                                                                  0339 00340000
*   NVTSAVE=RNVT;                   /* SAVE POINTER TO NVT           */ 00341000
         ST    RNVT,NVTSAVE                                        0339 00342000
*   MBB=0;                          /* SET BIN TO 0'S                */ 00343000
         STCM  @15,7,MBB                                           0340 00344000
*   M=DEBNMEXT;                     /* SET UP NUMBER OF EXTENT TO USE   00345000
*                                      IN CONVERSION           YM2720*/ 00346000
         L     @10,DEBPTR                                          0341 00347000
         MVC   M(1),DEBNMEXT(@10)                                  0341 00348000
*   CCHH=EXTENT(3:6);               /* START CCHHR                   */ 00349000
         MVC   CCHH(4),EXTENT+2(@14)                               0342 00350000
*   R=0;                            /* ZERO OUT RECORD               */ 00351000
         MVI   R,X'00'                                             0343 00352000
*   A=1;                            /* FIRST CALL TO IGC018          */ 00353000
         MVC   A(4),@CF00121                                       0344 00354000
*NP2CONVT:                                                         0345 00355000
*   RPARM=DEBPTR;                   /* ADDR OF DEB                   */ 00356000
NP2CONVT L     RPARM,DEBPTR                                        0345 00357000
*   RCCHH=ADDR(MBBCCHHR);                                          0346 00358000
         LA    RCCHH,MBBCCHHR                                      0346 00359000
*   RESPECIFY                                                      0347 00360000
*    (RENTRY,                                                      0347 00361000
*     RBASE1,                                                      0347 00362000
*     RSAVE,                                                       0347 00363000
*     RTTR,                                                        0347 00364000
*     RBASE6) RESTRICTED;                                          0347 00365000
*   RENTRY=CVTPRLTV;                /* ADDR OF IGC018                */ 00366000
         L     RENTRY,CVTPRLTV(,RCVT)                              0348 00367000
*   RBASE6=RBASE1;                  /* SAVE BASE REG IN NON-VOLATILE    00368000
*                                      REG                           */ 00369000
         LR    RBASE6,RBASE1                                       0349 00370000
*   R13SAVE=RSAVE;                  /* SAVE REG 13 OVER CALL         */ 00371000
*                                                                  0350 00372000
         ST    RSAVE,R13SAVE                                       0350 00373000
*   /*****************************************************************/ 00374000
*   /*                                                               */ 00375000
*   /*                                                               */ 00376000
*   /*****************************************************************/ 00377000
*                                                                  0351 00378000
*   GENERATE REFS(RPARM,RCCHH,RENTRY,RBASE6,MBBCCHHR) SETS(REXIT,RBASE1 00379000
*       ,RBASE2,RTTR,RNVT,RSAVE);                                  0351 00380000
*                                                                  0351 00381000
          BALR REXIT,RENTRY                                             00382000
          LR    RBASE1,RBASE6                                           00383000
          LA    RBASE2,4095(0,RBASE1)                                   00384000
*   /*****************************************************************/ 00385000
*   /*                                                               */ 00386000
*   /*                                                               */ 00387000
*   /*****************************************************************/ 00388000
*                                                                  0352 00389000
*   RSAVE=R13SAVE;                  /* RESTORE REG 13                */ 00390000
         L     RSAVE,R13SAVE                                       0352 00391000
*   TRACK(A)=RTTR;                  /* SAVE TTR                      */ 00392000
         L     @14,A                                               0353 00393000
         LR    @10,@14                                             0353 00394000
         SLA   @10,2                                               0353 00395000
         ST    RTTR,TRACK-4(@10)                                   0353 00396000
*   RESPECIFY                                                      0354 00397000
*    (RBASE6,                                                      0354 00398000
*     RSAVE,                                                       0354 00399000
*     RENTRY,                                                      0354 00400000
*     RTTR) UNRESTRICTED;                                          0354 00401000
*   RNVT=NVTSAVE;                   /* RESTORE NVT ADDR              */ 00402000
         L     RNVT,NVTSAVE                                        0355 00403000
*/* HAVE BOTH START & END CCHH BEEN CONVERTED                        */ 00404000
*                                                                  0356 00405000
*   IF A=2 THEN                                                    0356 00406000
         LA    @10,2                                               0356 00407000
         CR    @14,@10                                             0356 00408000
         BE    @RT00356                                            0356 00409000
*     GO TO NP2CALC;                /* CALCULATE THE NO OF TRACKS    */ 00410000
*   ELSE                                                           0358 00411000
*     DO;                                                          0358 00412000
*       A=2;                        /* CONVERT END ADDR              */ 00413000
         ST    @10,A                                               0359 00414000
*       CCHH=EXTENT(7:10);          /* END CCHHR                     */ 00415000
         L     @10,EXTPTR                                          0360 00416000
         MVC   CCHH(4),EXTENT+6(@10)                               0360 00417000
*       GO TO NP2CONVT;             /* GO TO IGC018                  */ 00418000
         B     NP2CONVT                                            0361 00419000
*     END;                                                         0362 00420000
*NP2CALC:                                                          0363 00421000
*   NMTRK=TRACK(2)-TRACK(1);        /* NO OF TRACKS                  */ 00422000
NP2CALC  L     @10,TRACK+4                                         0363 00423000
         SL    @10,TRACK                                           0363 00424000
         ST    @10,NMTRK                                           0363 00425000
*   DEBNMTRK=NMTRK1+1;              /* PUT NUMBER OF TRACKS IN DEB   */ 00426000
         LA    @10,1                                               0364 00427000
         SLR   @00,@00                                             0364 00428000
         ICM   @00,3,NMTRK1                                        0364 00429000
         ALR   @00,@10                                             0364 00430000
         L     @15,DEBDAPTR                                        0364 00431000
         STH   @00,DEBNMTRK(,@15)                                  0364 00432000
*   EXTCTR=EXTCTR-1;                /* DECREMENT COUNTER             */ 00433000
         SLR   @00,@00                                             0365 00434000
         IC    @00,EXTCTR                                          0365 00435000
         BCTR  @00,0                                               0365 00436000
         STC   @00,EXTCTR                                          0365 00437000
*/* SEE IF THERE ARE ANY MORE EXTENTS TO PROCESS                     */ 00438000
*                                                                  0366 00439000
*   IF EXTCTR=0 THEN                                               0366 00440000
         CLI   EXTCTR,0                                            0366 00441000
         BE    @RT00366                                            0366 00442000
*     GO TO NP2SEXT;                /* SEE IF SINGLE EXTENT SPECIFIED*/ 00443000
*   ELSE                                                           0368 00444000
