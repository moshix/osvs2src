         TITLE 'IEAVNPS5, SVC AND ESR TABLE INITIALIZATION ROUTINE     *00001000
                        '                                               00002000
IEAVNPS5 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVNPS5  74.170'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @10,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@10                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
*                                                                       00018000
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF          00019000
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES      00020000
** WILL ASSEMBLE PROPERLY.                                              00021000
*   SCVTSPTR=CVTABEND;              /* GET ADDRESS OF SECONDARY CVT  */ 00022000
         L     SCVTSPTR,CVTABEND(,CVTPTR)                          0094 00023000
*   CALL NPS5SVCT;                  /* INIT SVC TABLE                */ 00024000
         BAL   @14,NPS5SVCT                                        0095 00025000
*   CALL NPS5ESRT;                  /* INIT ESR TABLE                */ 00026000
         BAL   @14,NPS5ESRT                                        0096 00027000
*   RETURN;                         /* RETURN TO CALLER              */ 00028000
@EL00001 L     @13,4(,@13)                                         0097 00029000
@EF00001 DS    0H                                                  0097 00030000
@ER00001 LM    @14,@12,12(@13)                                     0097 00031000
         BR    @14                                                 0097 00032000
*   GENERATE NOREFS NOSETS(EJECT);                                 0098 00033000
*                                                                  0098 00034000
         EJECT                                                          00035000
*/********************************************************************/ 00036000
*/*                                                                  */ 00037000
*/* SEG NAME- NPS5SVCT                                               */ 00038000
*/*                                                                  */ 00039000
*/* FUNCTION- SET THE SVC TABLE ENTRIES WHICH REPRESENT TYPE 3 OR 4  */ 00040000
*/*           SVC'S TO THE LPA ADDRESSING THE SVC. IF AN SVC IS      */ 00041000
*/*           FOUND IN THE LPA, ITS SVC ENTRY IS SET TO THE          */ 00042000
*/*           ADDRESS OF IRCERRUR                                    */ 00043000
*/*                                                                  */ 00044000
*/* INPUT-    1. RCVT     -  POINTER TO CVT                          */ 00045000
*/*           2. SCVTSPTR -  POINTER TO SECONDARY CVT                */ 00046000
*/*           3. SCVTSVCT -  POINTER TO THE SVC TABLE                */ 00047000
*/*           4. RNVT     -  POINTER TO NVT                          */ 00048000
*/*           5. NVTABSAV -  ABEND SVC TABLE ENTRY (SEE NOTE #1)     */ 00049000
*/*           6. NVTLOCAT -  LOCATE SVC TABLE ENTRY (SEE NOTE #1)    */ 00050000
*/*                                                                  */ 00051000
*/* OUTPUT-   1. IF THE TYPE 3 OR 4 SVC ENTRY WAS FOUND IN THE       */ 00052000
*/*              LPA, SVCEP IS INITIALIZED WITH ITS ENTRY POINT.     */ 00053000
*/*           2. IF THE TYPE 3 OR 4 SVC ENTRY WAS NOT FOUND IN       */ 00054000
*/*              THE LPA, SVCEP IS INITIALIZED WITH THE ADDRESS      */ 00055000
*/*              OF IGCERROR. ALSO, MESSAGE IEA363I IS ISSUED        */ 00056000
*/*              INDICATING REQUESTED MODULE WAS NOT FOUND IN        */ 00057000
*/*              THE LPA.                                            */ 00058000
*/*                                                                  */ 00059000
*/* NOTES-    1. SINCE NIP TRAPS ABEND AND LOCATE SVC'S IT WAS       */ 00060000
*/*              NECESSARY TO MOVE THE ORIGINAL SVC TABLE VALUES     */ 00061000
*/*              TO THE NVT. THIS ROUTINE WILL INITIALIZE THE        */ 00062000
*/*              NVTABSAV FOR ABEND AND NVTLOCAT FOR LOCATE SVC'S.   */ 00063000
*/*              AT THE END OF NIP PROCESSING, THESE VALUES ARE      */ 00064000
*/*              MOVED FROM THE NVT BACK TO THEIR ORIGINAL           */ 00065000
*/*              POSITION IN THE SVC TABLE.                          */ 00066000
*/*                                                                  */ 00067000
*/* CALLER-      IEAVNPS5 MAINLINE                                   */ 00068000
*/*                                                                  */ 00069000
*/********************************************************************/ 00070000
*                                                                  0099 00071000
*   GENERATE NOREFS NOSETS(EJECT);                                 0099 00072000
*                                                                  0099 00073000
         EJECT                                                          00074000
*/*NPS5SVCT: CHART (AMODE,AREF)                                      */ 00075000
*/* HEADER                                                         0100 00076000
*/* 05/16/74                                                       0100 00077000
*/* NPS5SVCT    *** SVC TABLE INITIALIZATION ***     PAGE #          */ 00078000
*/*       E  NPS5SVCT                                                */ 00079000
*/*       P  GET ADDRESS OF SVC 13 IN NVT                            */ 00080000
*/*       P  INIT. HIGHEST SVC NO. FOR COUNTER OF ENTRIES (RI=255)   */ 00081000
*/* %N0:  D  (YES,%N7,NO,) HAVE ALL ENTRIES BEEN PROCESSED RI<0      */ 00082000
*/*       D  (NO,%N1,YES,) LOCATE SVC ENTRY (RI = 26)                */ 00083000
*/*       P  (,%N3) SVC TAB PTR INIT TO LOCATE ENTRY IN NVTLOCAT     */ 00084000
*/* %N1:  D  (NO,%N2,YES,) ABEND SVC ENTRY (RI = 13)                 */ 00085000
*/*       P  (,%N3) SVC TABLE PTR INIT, TO ABEND ENTRY IN NVTABSAV   */ 00086000
*/* %N2:  P  SVC TABLE PTR INIT. (@SVC TAB + (RI * 8))               */ 00087000
*/* %N3:  D  (YES,%N4,NO,%N5)THIS ENTRY TYPE 3 OR 4                  */ 00088000
*/* %N4:  P  CONSTRUCT THE SVC NAME (SVCNAM = IGC0NNNN)              */ 00089000
*/*       S  NPFNDSVC: SEARCH ACTIVE, THEN PLPA FOR MODULE NAME      */ 00090000
*/*       D  (NO,%N6,YES,) MODULE FOUND (RCODE = 0)                  */ 00091000
*/*       P  INIT SVC ENTRY POINT (SVCEP = RPARM)                    */ 00092000
*/* %N5:  P  (,%N0) DECREMENT ENTRY COUNTER BY 1 (RI = RI - 1)       */ 00093000
*/* %N6:  P  MOVE ADDRESS OF IGCERROR TO SVCEP                       */ 00094000
*/*       P  CLEAR THE LOCK BYTES                                    */ 00095000
*/*       P  MOVE THE SVC NAME (SVCNAM) TO ERROR MESSAGE             */ 00096000
*/*       P  (,%N5) IEAPMNIP TYPE=WTO: ISSUE MSG IEA363I             */ 00097000
*/* %N7:  R  RETURN TO CALLER                                        */ 00098000
*/*END NPS5SVCT                                                      */ 00099000
*                                                                  0100 00100000
*   GENERATE NOREFS NOSETS(EJECT);                                 0100 00101000
         EJECT                                                          00102000
*NPS5SVCT:                                                         0101 00103000
*   PROC OPTIONS(SAVE(REXIT));                                     0101 00104000
         B     @PB00002                                            0101 00105000
NPS5SVCT ST    @14,@SA00002                                        0101 00106000
*   RESPECIFY                                                      0102 00107000
*    (RI) RESTRICTED;                                              0102 00108000
*   DO RI=255 TO 0 BY-1;            /* ACTUAL SVC NO IN SVC TABLE    */ 00109000
         LA    RI,255                                              0103 00110000
@DL00103 DS    0H                                                  0104 00111000
*     IF RI=LOCATE THEN             /* IS THIS LOCATE ENTRY          */ 00112000
         CH    RI,@CH01102                                         0104 00113000
         BNE   @RF00104                                            0104 00114000
*       SVCTPTR=ADDR(NVTLOCAT);     /* YES - ENTRY IS IN NVT         */ 00115000
         LA    SVCTPTR,NVTLOCAT(,RNVT)                             0105 00116000
*     ELSE                          /* NO                            */ 00117000
*       IF RI=ABEND THEN            /* IS THIS ABEND ENTRY           */ 00118000
         B     @RC00104                                            0106 00119000
@RF00104 CH    RI,@CH01100                                         0106 00120000
         BNE   @RF00106                                            0106 00121000
*         SVCTPTR=ADDR(NVTABSAV);   /* YES - ENTRY IS IN NVT         */ 00122000
         LA    SVCTPTR,NVTABSAV(,RNVT)                             0107 00123000
*       ELSE                        /* NO - NORMAL ENTRY             */ 00124000
*                                                                  0108 00125000
*         /***********************************************************/ 00126000
*         /*                                                         */ 00127000
*         /* IF NOT ABEND OR LOCATE, ENTRY IS IN SVC TABLE           */ 00128000
*         /*                                                         */ 00129000
*         /***********************************************************/ 00130000
*                                                                  0108 00131000
*         SVCTPTR=SCVTSVCT+(RI*LENGTH(SVCENTRY));                  0108 00132000
         B     @RC00106                                            0108 00133000
@RF00106 LR    SVCTPTR,RI                                          0108 00134000
         SLA   SVCTPTR,3                                           0108 00135000
         AL    SVCTPTR,SCVTSVCT(,SCVTSPTR)                         0108 00136000
*     IF SVCTYP34='11'B             /* THIS TYPE 3 - 4 ENTRY         */ 00137000
*       THEN                                                       0109 00138000
@RC00106 DS    0H                                                  0109 00139000
@RC00104 TM    SVCTYP34(SVCTPTR),B'11000000'                       0109 00140000
         BNO   @RF00109                                            0109 00141000
*       DO;                                                        0110 00142000
*                                                                  0110 00143000
*         /***********************************************************/ 00144000
*         /*                                                         */ 00145000
*         /* CONSTRUCT THE EBCDIC NAME                               */ 00146000
*         /*                                                         */ 00147000
*         /***********************************************************/ 00148000
*                                                                  0111 00149000
*         GENERATE REFS(RI) SETS(UNPACK,SVCNO);                    0111 00150000
                 CVD  RI,UNPACK      SVC NUMBER TO DECIMAL              00151000
                 UNPK SVCNO(4),UPKBUF+1(3) SVC NUMBER OF EBCDIC         00152000
*         CALL NPFNDSVC;            /* SEARCH LPA FOR MODULE NAME    */ 00153000
         BAL   @14,NPFNDSVC                                        0112 00154000
*         RESPECIFY                                                0113 00155000
*          (RCODE,                                                 0113 00156000
*           RPARM) RESTRICTED;                                     0113 00157000
*         IF RCODE=0                /* WAS NAME FOUND                */ 00158000
*           THEN                    /* YES                           */ 00159000
         LTR   RCODE,RCODE                                         0114 00160000
         BNZ   @RF00114                                            0114 00161000
*           DO;                     /* UPDATE TABLE ENTRY            */ 00162000
*             SVCEP=RPARM;          /* UPDATE SVC TABLE ENTRY        */ 00163000
         ST    RPARM,SVCEP(,SVCTPTR)                               0116 00164000
*             RESPECIFY                                            0117 00165000
*              (RCODE,                                             0117 00166000
*               RPARM) UNRESTRICTED;                               0117 00167000
*           END;                    /* END OF FOUND PROCESSING       */ 00168000
*         ELSE                      /* NOT FOUND                     */ 00169000
*           DO;                     /* ISSUE ERROR MESSAGE           */ 00170000
         B     @RC00114                                            0119 00171000
@RF00114 DS    0H                                                  0120 00172000
*             SVCEP=NVTIGCER;       /* MOVE IGCERROR @ TO SVC TABLE  */ 00173000
         MVC   SVCEP(4,SVCTPTR),NVTIGCER(RNVT)                     0120 00174000
*             SVCLOCKS=SVCLOCKS&&SVCLOCKS;/* CLEAR LOCK BYTES        */ 00175000
         XC    SVCLOCKS(2,SVCTPTR),SVCLOCKS(SVCTPTR)               0121 00176000
*             MSGFI1=SVCNAM;        /* MOVE SVC NAME TO MESSAGE      */ 00177000
*                                                                  0122 00178000
         MVI   MSGFI1+8,C' '                                       0122 00179000
         MVC   MSGFI1(8),SVCNAM                                    0122 00180000
*             /*******************************************************/ 00181000
*             /*                                                     */ 00182000
*             /* ISSUE IEA363I MOD NOT FOUND IN LPA                  */ 00183000
*             /*                                                     */ 00184000
*             /*******************************************************/ 00185000
*                                                                  0123 00186000
*             GENERATE REFS(RNVT,NVTWTO,MSGFS,MSGFI1) SETS(RPARM,REXIT, 00187000
*                 RENTRY);                                         0123 00188000
                    IEAPMNIP TYPE=WTO,PARAM=MSGFS                       00189000
*           END;                    /* END OF ERROR PROCESSING       */ 00190000
*       END;                        /* END TYPE 3-4 PROCESSING       */ 00191000
@RC00114 DS    0H                                                  0126 00192000
*   END;                            /* SVC ENTRY-CONTROL LOOP        */ 00193000
@RF00109 BCTR  RI,0                                                0126 00194000
         LTR   RI,RI                                               0126 00195000
         BNM   @DL00103                                            0126 00196000
*   RETURN;                         /* RETURN TO CALLER              */ 00197000
@EL00002 DS    0H                                                  0127 00198000
@EF00002 DS    0H                                                  0127 00199000
@ER00002 L     @14,@SA00002                                        0127 00200000
         BR    @14                                                 0127 00201000
*   END NPS5SVCT;                                                  0128 00202000
@PB00002 DS    0H                                                  0129 00203000
*   GENERATE NOREFS NOSETS(EJECT);                                 0129 00204000
*                                                                  0129 00205000
         EJECT                                                          00206000
*/********************************************************************/ 00207000
*/*                                                                  */ 00208000
*/* SEG NAME- NPS5ESRT                                               */ 00209000
*/*                                                                  */ 00210000
*/* FUNCTION- INITIALIZE THE ESR TABLE WITH ENTRY POINTS OF          */ 00211000
*/*           ESR SVC'S (TYPE 3 OR 4) RESIDING IN THE ACTIVE         */ 00212000
*/*           LPA OR PLPA                                            */ 00213000
*/*                                                                  */ 00214000
*/* INPUT-    1. RCVT     -  POINTER TO CVT                          */ 00215000
*/*           2. SCVTSPTR -  POINTER TO SECONDARY CVT                */ 00216000
*/*           3. SCVTSVCT - POINTER TO SVC TABLE                     */ 00217000
*/*                                                                  */ 00218000
*/* OUTPUT-   1. IF THE TYPE 3 - 4 ESR SVC WAS FOUND IN THE LPA      */ 00219000
*/*              ITS ENTRY POINT (SVCEP) IS INITIALIZED TO THIS      */ 00220000
*/*              VALUE                                               */ 00221000
*/*           2. IF THE TYPE 3 - 4 ESR SVC WAS NOT FOUND IN THE      */ 00222000
*/*              LPA, ITS ENTRY POINT IS INITIALIZED WITH THE        */ 00223000
*/*              ADDRESS OF IGXERROR                                 */ 00224000
*/*           3. IF THE TYPE 1 -2 ESR SVC ENTRY POINT HAS NOT BEEN   */ 00225000
*/*              INITIALIZED (SVCEP = 0) THE FIELD IS INITIALIZED    */ 00226000
*/*              WITH THE ADDRESS OF IGXERROR                        */ 00227000
*/*                                                                  */ 00228000
*/* NOTES-    1. THE ESR SVC TABLE CONSISTS OF 3 SECTIONS. EACH      */ 00229000
*/*              SECTION IS POINTED TO BY SVC'S IN THE SYSTEM        */ 00230000
*/*              SVC TABLE (SVC 109, 116, AND 112)                   */ 00231000
*/*           2. AN 8 BYTE PREFIX TO THE ESR TABLE INDICATES         */ 00232000
*/*              THE NUMBER OF ENTRIES IN EACH SECTION. EACH         */ 00233000
*/*              ENTRY IS IN THE SAME FORMAT AS THOSE IN THE         */ 00234000
*/*              SYSTEM SVC TABLE                                    */ 00235000
*/*                                                                  */ 00236000
*/* CALLER-   IEAVNPS5 MAINLINE                                      */ 00237000
*/*                                                                  */ 00238000
*/********************************************************************/ 00239000
*                                                                  0130 00240000
*   GENERATE NOREFS NOSETS(EJECT);                                 0130 00241000
*                                                                  0130 00242000
         EJECT                                                          00243000
*/*NPS5ESRT: CHART (AMODE, AREF)                                     */ 00244000
*/* HEADER                                                         0131 00245000
*/* 05/16/74                                                       0131 00246000
*/* NPS5ESRT    *** INIT THE ESR TABLE ***      PAGE #               */ 00247000
*/*       E  NPS5ESRT                                                */ 00248000
*/*       P  GET ADDRESS OF SVC TABLE (SVCTPTR = SCVTSVCT)           */ 00249000
*/*       P  SET PREFIX NAME TO 'IGX0'                               */ 00250000
*/*       P  LOCATE SVC 109 ENTRY IN SVC TABLE (SVCTPTR+872)         */ 00251000
*/*       COMMENT (1,1) SVC 109 EP IS                                */ 00252000
*/*       COMMENT (2,1) POINTER TO                                   */ 00253000
*/*       COMMENT (3,1) ESR TABLE                                    */ 00254000
*/*       P  COMPUTE OFFSET TO ESR PREFIX (SVCEP - 8)                */ 00255000
*/*       COMMENT (1,1) ESR PREFIX                                   */ 00256000
*/*       COMMENT (2,1) CONTAINS HIGHEST                             */ 00257000
*/*       COMMENT (3,1) SVC NO. IN EACH                              */ 00258000
*/*       COMMENT (4,1) ESR SECTION                                  */ 00259000
*/*       P  INIT SECTION COUNTER TO 1 (I = 1)                       */ 00260000
*/* %N1:  D  (YES,%N7,NO,) SECTION COUNTER AT MAX (I > 3)            */ 00261000
*/*       P  SET UP DIGIT TO BE USED AS FIFTH CHAR IN ESR NAME(Z=I)  */ 00262000
*/*       P  INDICATE CURRENT SVC ENTRY IS AN ESR SVC (SVCESR = 1)   */ 00263000
*/*       P  LOCATE THE START OF THE ESR SECTION: SVCTPTR=SVCEP      */ 00264000
*/*       COMMENT (1,1) EITHER SVC 109,                              */ 00265000
*/*       COMMENT (2,1) 116, OR 122                                  */ 00266000
*/* %N2:  D  (YES,%N3,NO,) THIS ENTRY TYPE 3 OR 4                    */ 00267000
*/*       D  (NO,%N5,YES,) IS EP ADDR UNRESOLVED (SVCEP = 0)         */ 00268000
*/*       P  MOVE ADDRESS OF IGXERROR TO SVCEP                       */ 00269000
*/*       P  (,%N5) CLEAR THE ESR ENTRY LOCK BYTES                   */ 00270000
*/* %N3:  P  ESTABLISH THE MODULE NAME (IGX0ZNNN)                    */ 00271000
*/*       S  NPFNDSVC: SEARCH ACTIVE THEN PLPA FOR MODULE NAME       */ 00272000
*/*       D  (YES,%N4,NO,) MODULE NAME FOUND (RCODE = 0)             */ 00273000
*/*       P  MOVE ADDRESS OF IGXERROR TO SVCEP                       */ 00274000
*/*       P  (,%N5) CLEAR THIS ENTRY LOCK BYTES                      */ 00275000
*/* %N4:  P  MOVE ENTRY POINT TO ESR TABLE (SVCEP = RPARM)           */ 00276000
*/* %N5:  D  (YES,%N6,NO,) ALL ENTRIES IN THIS SECTION PROCESSED     */ 00277000
*/*       P  (,%N2) POINT TO NEXT ESR ENTRY (SVCTPTR = SVCTPTR+8)    */ 00278000
*/* %N6:  P  (,%N1) INCREMENT ESR SECTION CONTROL COUNTER (I = I + 1)*/ 00279000
*/* %N7:  R  RETURN VIA REXIT                                        */ 00280000
*/*END NPS5ESRT                                                      */ 00281000
*                                                                  0131 00282000
*   GENERATE NOREFS NOSETS(EJECT);                                 0131 00283000
         EJECT                                                          00284000
*NPS5ESRT:                                                         0132 00285000
*   PROC OPTIONS(SAVE(REXIT));                                     0132 00286000
         B     @PB00003                                            0132 00287000
NPS5ESRT ST    @14,@SA00003                                        0132 00288000
*   SVCNAME=ESRNAME;                /* SET UP IGX0 PREFIX IN SVCNAME    00289000
*                                      AREA                          */ 00290000
         MVC   SVCNAME(4),ESRNAME                                  0133 00291000
*   SVCTPTR=SCVTSVCT+ESRSVC(1)*LENGTH(SVCENTRY);/* GET IGC109 ENTRY     00292000
*                                      IN THE SVCTABLE               */ 00293000
         LH    SVCTPTR,ESRSVC                                      0134 00294000
         SLA   SVCTPTR,3                                           0134 00295000
         AL    SVCTPTR,SCVTSVCT(,SCVTSPTR)                         0134 00296000
*   LIMITPTR=SVCEP-LENGTH(SVCENTRY);/* FIRST ENTRY IN ESRTABLE     0135 00297000
*                                      CONTAINS NUMBER OF EXSVC FOR     00298000
*                                      EACH ROUTER SVC               */ 00299000
         L     @12,SVCEP(,SVCTPTR)                                 0135 00300000
         SH    @12,@CH00052                                        0135 00301000
         ST    @12,LIMITPTR                                        0135 00302000
*   DO I=1 TO 3;                    /* INITIALIZE ALL THREE SECTIONS    00303000
*                                      OF THE ESRTABLE               */ 00304000
         LA    @12,1                                               0136 00305000
         STC   @12,I                                               0136 00306000
@DL00136 DS    0H                                                  0137 00307000
*     ESRDIGIT=I-1;                 /* SET UP UNIQUE DIGIT FOR THIS     00308000
*                                      SECTION OF THE TABLE          */ 00309000
         LR    @11,@12                                             0137 00310000
         BCTR  @11,0                                               0137 00311000
         STC   @11,ESRDIGIT                                        0137 00312000
*     SVCTPTR=SCVTSVCT+ESRSVC(I)*LENGTH(SVCENTRY);/* GET THE SVCTABLE   00313000
*                                      ENTRY FOR THIS ROUTER         */ 00314000
         ALR   @12,@12                                             0138 00315000
         LH    SVCTPTR,ESRSVC-2(@12)                               0138 00316000
         SLA   SVCTPTR,3                                           0138 00317000
         AL    SVCTPTR,SCVTSVCT(,SCVTSPTR)                         0138 00318000
*     SVCESR='1'B;                  /* TURN ON THE ESR BIT           */ 00319000
         OI    SVCESR(SVCTPTR),B'00000100'                         0139 00320000
*     SVCTPTR=SVCEP;                /* GET START OF THIS SECTION OF     00321000
*                                      THE ESRTABLE                  */ 00322000
         L     SVCTPTR,SVCEP(,SVCTPTR)                             0140 00323000
*     DO RI=0 TO LIMIT(I);          /* CHECK ALL ENTRIES THIS PART   */ 00324000
         SLR   RI,RI                                               0141 00325000
         B     @DE00141                                            0141 00326000
@DL00141 DS    0H                                                  0142 00327000
*       IF SVCTYP34='11'B           /* CHECK FOR TYPE 4 SVC          */ 00328000
*         THEN                      /* MUST LOCATE SVC               */ 00329000
         TM    SVCTYP34(SVCTPTR),B'11000000'                       0142 00330000
         BNO   @RF00142                                            0142 00331000
*         DO;                       /* BUILD NAME FOR SEARCH         */ 00332000
*           GENERATE;                                              0144 00333000
           CVD  RI,UNPACK                                               00334000
           UNPK SVCNO(4),UPKBUF+1(3)                                    00335000
           OI   SVCNO+3,X'F0'                                           00336000
           OC   SVCNO(1),ESRDIGIT                                       00337000
*           CALL NPFNDSVC;          /* SEARCH LPA FOR REQUESTED NAME */ 00338000
         BAL   @14,NPFNDSVC                                        0145 00339000
*           RESPECIFY                                              0146 00340000
*            (RCODE) RESTRICTED;                                   0146 00341000
*           IF RCODE=0              /* WAS MODULE FOUND              */ 00342000
*             THEN                  /* YES                           */ 00343000
         LTR   RCODE,RCODE                                         0147 00344000
         BNZ   @RF00147                                            0147 00345000
*             DO;                   /* UPDATE SVC TABLE              */ 00346000
*               RESPECIFY                                          0149 00347000
*                (RPARM) RESTRICTED;                               0149 00348000
*               SVCEP=RPARM;        /* STORE EP IN CURRENT SVC ENTRY */ 00349000
         ST    RPARM,SVCEP(,SVCTPTR)                               0150 00350000
*               RESPECIFY                                          0151 00351000
*                (RPARM,                                           0151 00352000
*                 RCODE) UNRESTRICTED;                             0151 00353000
*             END;                  /* END MODULE FOUND PROCESSING   */ 00354000
*           ELSE                    /* MODULE NOT FOUND              */ 00355000
*             DO;                   /* PROCESS AS ERROR              */ 00356000
         B     @RC00147                                            0153 00357000
@RF00147 DS    0H                                                  0154 00358000
*               SVCEP=NVTIGXER;     /* MOVE ADDR OF IGXERROR TO ENTRY*/ 00359000
         MVC   SVCEP(4,SVCTPTR),NVTIGXER(RNVT)                     0154 00360000
*               SVCLOCKS=SVCLOCKS&&SVCLOCKS;/* CLEAR LOCK BYTES      */ 00361000
         XC    SVCLOCKS(2,SVCTPTR),SVCLOCKS(SVCTPTR)               0155 00362000
*             END;                  /* END ERROR PROCESSING          */ 00363000
*         END;                      /* END TYPE 3-4 PROCESSING       */ 00364000
*       ELSE                        /* NOT TYPE 3-4                  */ 00365000
*         IF SVCEP=0                /* TYPE 1-2 EP RESOLVED          */ 00366000
*           THEN                    /* NO                            */ 00367000
         B     @RC00142                                            0158 00368000
@RF00142 ICM   @12,15,SVCEP(SVCTPTR)                               0158 00369000
         BNZ   @RF00158                                            0158 00370000
*           DO;                     /* GET ENTRY TO IGXERROR         */ 00371000
*             SVCEP=NVTIGXER;       /* MOVE ADDR OF IGXERROR TO ENTRY*/ 00372000
         MVC   SVCEP(4,SVCTPTR),NVTIGXER(RNVT)                     0160 00373000
*             SVCLOCKS=SVCLOCKS&&SVCLOCKS;/* CLEAR LOCK BYTES        */ 00374000
         XC    SVCLOCKS(2,SVCTPTR),SVCLOCKS(SVCTPTR)               0161 00375000
*           END;                    /* END TYPE 1-2 ERROR PROCESSING */ 00376000
*         ELSE                                                     0163 00377000
*           ;                       /* TYPE 1-2 EP RESOLVED          */ 00378000
@RF00158 DS    0H                                                  0164 00379000
*       SVCTPTR=SVCTPTR+LENGTH(SVCENTRY);/* GET NEXT ESR JOB ENTRY   */ 00380000
@RC00142 AH    SVCTPTR,@CH00052                                    0164 00381000
*     END;                          /* ENTRY CONTROL LOOP            */ 00382000
         AH    RI,@CH00059                                         0165 00383000
@DE00141 SLR   @12,@12                                             0165 00384000
         IC    @12,I                                               0165 00385000
         ALR   @12,@12                                             0165 00386000
         L     @11,LIMITPTR                                        0165 00387000
         AL    @11,@CF01160                                        0165 00388000
         CH    RI,LIMIT(@12,@11)                                   0165 00389000
         BNH   @DL00141                                            0165 00390000
*   END;                            /* SECTION CONTROL LOOP          */ 00391000
         LA    @12,1                                               0166 00392000
         SLR   @11,@11                                             0166 00393000
         IC    @11,I                                               0166 00394000
         ALR   @12,@11                                             0166 00395000
         STC   @12,I                                               0166 00396000
         CH    @12,@CH00197                                        0166 00397000
         BNH   @DL00136                                            0166 00398000
*   END NPS5ESRT;                   /* END OF ESR TABLE INIT.        */ 00399000
@EL00003 DS    0H                                                  0167 00400000
@EF00003 DS    0H                                                  0167 00401000
@ER00003 L     @14,@SA00003                                        0167 00402000
         BR    @14                                                 0167 00403000
@PB00003 DS    0H                                                  0168 00404000
*   GENERATE NOREFS NOSETS(EJECT);                                 0168 00405000
*                                                                  0168 00406000
         EJECT                                                          00407000
*/********************************************************************/ 00408000
*/*                                                                  */ 00409000
*/* SEG NAME- NPFNDSVC                                               */ 00410000
*/*                                                                  */ 00411000
*/* FUNCTION- SEARCH THE PROGRAM MANAGER ACTIVE LPA QUEUE            */ 00412000
*/*           OR PLPA DIRECTORY FOR A GIVEN MODULE NAME              */ 00413000
*/*                                                                  */ 00414000
*/* INPUT-    1. IEAQLPA  -  POINTER TO THE ACTIVE LPA CHAIN         */ 00415000
*/*           2. SVCNAM   -  THE INPUT SEARCH ARGUMENT               */ 00416000
*/*           3. CVTLPDSR -  POINTER TO THE PROGRAM MANAGER PLPA     */ 00417000
*/*                          SEARCH ROUTINE (IEAVVMSR)               */ 00418000
*/*                                                                  */ 00419000
*/* OUTPUT-   1. IF SUCCESSFUL:                                      */ 00420000
*/*                 RPARM = ENTRY POINT OF REQUESTED MODULE NAME     */ 00421000
*/*                 RCODE = 0                                        */ 00422000
*/*           2. IF UNSUCCESSFUL:                                    */ 00423000
*/*                 RCODE = 4                                        */ 00424000
*/*                                                                  */ 00425000
*/* NOTES-    NONE                                                   */ 00426000
*/*                                                                  */ 00427000
*/* CALLER-   NPS5SVCT,NPS5ESRT                                    0169 00428000
*                                                                    */ 00429000
*/*                                                                  */ 00430000
*/********************************************************************/ 00431000
*                                                                  0169 00432000
*   GENERATE NOREFS NOSETS(EJECT);                                 0169 00433000
*                                                                  0169 00434000
         EJECT                                                          00435000
*/*NPFNDSVC: CHART (AMODE, AREF)                                     */ 00436000
*/* HEADER                                                         0170 00437000
*/* 05/16/74                                                       0170 00438000
*/* NPFNDSVC    *** FIND CDE OR LPDE FOR GIVEN NAME ***   PAGE #     */ 00439000
*/*       E  NPFNDSVC                                                */ 00440000
*/*       P  GET PTR TO CDE CHAIN (CDENTPTR = IEAQLPA)               */ 00441000
*/* %N1:  D  (YES,%N3,NO,) EOQ (CDENTPTR = 0)                        */ 00442000
*/*       D  (YES,%N2,NO,) CDNAME = SVCNAME                          */ 00443000
*/*       P  (,%N1) GET PTR TO NEXT CDE (CDENTPTR = CDCHAIN)         */ 00444000
*/* %N2:  P  (,%N5) RETURN ENTRY POINT OF MODULE (RPARM=CDENTPT)     */ 00445000
*/* %N3:  P  SET UP R0 - R1 WITH SVC NAME FOR IEAVVMSR               */ 00446000
*/*       P  GET ADDRESS OF PROGRAM MANAGER SEARCH RTN (CVTLPDSR)    */ 00447000
*/*       S  IEAVVMSR: SEARCH THE PLPA DIRECTORY                     */ 00448000
*/*       D  (YES,%N4,NO,) MODULE NAME FOUND                         */ 00449000
*/*       P  (,%N6) INDICATE NAME NOT FOUND (RCODE = 4)              */ 00450000
*/* %N4:  P  RETURN ENTRY POINT OF MODULE (RPARM=LPDENTP)            */ 00451000
*/* %N5:  P  INDICATE NAME FOUND (RCODE = 0)                         */ 00452000
*/* %N6:  R  RETURN VIA REXIT                                        */ 00453000
*/*END NPFNDSVC                                                      */ 00454000
*                                                                  0170 00455000
*   GENERATE NOREFS NOSETS(EJECT);                                 0170 00456000
         EJECT                                                          00457000
*NPFNDSVC:                                                         0171 00458000
*   PROC OPTIONS(SAVE(REXIT));                                     0171 00459000
         B     @PB00004                                            0171 00460000
NPFNDSVC ST    @14,@SA00004                                        0171 00461000
*   RESPECIFY                                                      0172 00462000
*    (CDENTPTR,                                                    0172 00463000
*     RPARM,                                                       0172 00464000
*     RCODE) RESTRICTED;                                           0172 00465000
*   RCODE=0;                        /* INITIALIZE SUCCESSFUL RC      */ 00466000
         SLR   @12,@12                                             0173 00467000
         LR    RCODE,@12                                           0173 00468000
*   RPARM=0;                        /* FOR CDE LOOP CONTROL          */ 00469000
         LR    RPARM,@12                                           0174 00470000
*   CDENTPTR=IEAQLPA;               /* GET POINTER TO ACTIVE LPA     */ 00471000
         L     @14,CVTQLPAQ(,CVTPTR)                               0175 00472000
         L     CDENTPTR,IEAQLPA(,@14)                              0175 00473000
*   DO WHILE CDENTPTR^=0&RPARM=0;   /* SEARCH UNTIL EOQ OR FOUND     */ 00474000
         B     @DE00176                                            0176 00475000
@DL00176 DS    0H                                                  0177 00476000
*     IF CDNAME=SVCNAM              /* INPUT NAME EG CDE NAME        */ 00477000
*       THEN                        /* YES                           */ 00478000
         CLC   CDNAME(8,CDENTPTR),SVCNAM                           0177 00479000
         BNE   @RF00177                                            0177 00480000
*       DO;                         /* PROCESS FOUND SVC             */ 00481000
*         RPARM=CDENTPT;            /* RETURN EP OF MODULE           */ 00482000
         L     RPARM,CDENTPT(,CDENTPTR)                            0179 00483000
*       END;                        /* END OF FOUND PROCESSING       */ 00484000
*     ELSE                                                         0181 00485000
*       CDENTPTR=CDCHAIN;           /* GET PTR TO NEXT CDE           */ 00486000
         B     @RC00177                                            0181 00487000
@RF00177 L     CDENTPTR,CDCHAIN(,CDENTPTR)                         0181 00488000
*   END;                                                           0182 00489000
@RC00177 DS    0H                                                  0182 00490000
@DE00176 SLR   @14,@14                                             0182 00491000
         CR    CDENTPTR,@14                                        0182 00492000
         BE    @DC00176                                            0182 00493000
         CR    RPARM,@14                                           0182 00494000
         BE    @DL00176                                            0182 00495000
@DC00176 DS    0H                                                  0183 00496000
*   IF CDENTPTR=0                   /* MODUULE FOUND ON ACTIVE Q    */ 00497000
*     THEN                          /* NO                            */ 00498000
         LTR   CDENTPTR,CDENTPTR                                   0183 00499000
         BNZ   @RF00183                                            0183 00500000
*     DO;                           /* SEARCH PLPA DIRECTORY         */ 00501000
*       RESPECIFY                                                  0185 00502000
*        (CDENTPTR) UNRESTRICTED;                                  0185 00503000
*       RESPECIFY                                                  0186 00504000
*        (R0,                                                      0186 00505000
*         RENTRY,                                                  0186 00506000
*         REXIT) RESTRICTED;                                       0186 00507000
*       LM(R0,R1,SVCNAM);           /* LOAD INPUT MODULE NAME FOR  0187 00508000
*                                      IEAVVMSR                      */ 00509000
         LM    R0,R1,SVCNAM                                        0187 00510000
*       RENTRY=CVTLPDSR;            /* GET ADDR OF LPDE SEARCH RTN   */ 00511000
         L     RENTRY,CVTLPDSR(,CVTPTR)                            0188 00512000
*                                                                  0189 00513000
*/********************************************************************/ 00514000
*/* INVOKE THE PROGRAM MANAGER SEARCH ROUTINE                        */ 00515000
*/* INPUT  -  MODULE NAME IN R0 - R1                                 */ 00516000
*/*        -  CVT ADDRESS IN R3                                      */ 00517000
*/*        -  RETURN ADDRESS IN R14                                  */ 00518000
*/* OUTPUT -  R6, R8, R9 MODIFIED                                    */ 00519000
*/*        -  R0 SET TO LPDE ADDRESS IF FOUND                        */ 00520000
*/* RETURN -  ON R14 IF FOUND                                        */ 00521000
*/*        -  ON R14+4 IF NOT FOUND                                  */ 00522000
*/********************************************************************/ 00523000
*                                                                  0189 00524000
*       GENERATE REFS(R0,R1,RCVT,REXIT) SETS(R0,REXIT,R6,R8,R9,RCODE)   00525000
*           FLOWS(NP5SVCFD);                                       0189 00526000
        BALR  REXIT,RENTRY           BRANCH TO IEAVVMSR                 00527000
        B     NP5SVCFD                PROCESS FOUND SVC                 00528000
        LA    RCODE,4                 INDICATE NOT FOUND                00529000
*     END;                          /* END OF LPDE SEARCH            */ 00530000
*   RETURN;                         /* RETURN TO CALLER              */ 00531000
@EL00004 DS    0H                                                  0191 00532000
@EF00004 DS    0H                                                  0191 00533000
@ER00004 L     @14,@SA00004                                        0191 00534000
         BR    @14                                                 0191 00535000
*NP5SVCFD:                                                         0192 00536000
*   RESPECIFY                                                      0192 00537000
*    (LPDEPTR) RESTRICTED;                                         0192 00538000
NP5SVCFD DS    0H                                                  0193 00539000
*   LPDEPTR=R0;                     /* SET POINTER TO LPDE           */ 00540000
         LR    LPDEPTR,R0                                          0193 00541000
*   RPARM=LPDENTP;                  /* RETURN MODULE EP TO CALLER    */ 00542000
         L     RPARM,LPDENTP(,LPDEPTR)                             0194 00543000
*   RCODE=0;                        /* INITIALIZE SUCCESSFUL RC      */ 00544000
         SLR   RCODE,RCODE                                         0195 00545000
*   RETURN;                         /* RETURN TO CALLER              */ 00546000
         B     @EL00004                                            0196 00547000
*   END NPFNDSVC;                   /* END OF SUBROUTINE             */ 00548000
