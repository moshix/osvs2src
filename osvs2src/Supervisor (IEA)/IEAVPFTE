         L     @15,PVTPVRIN(,PVTPTR)                               0252 00672000
         BALR  @14,@15                                             0252 00673000
*       RCACSECT=SAVECSID;          /* RESTORE THE CALLER'S MODULE ID*/ 00674000
         MVC   RCACSECT(1,RCAPTR),SAVECSID(PVTPTR)                 0253 00675000
*       IF R1=0 THEN                /* TEST IF V=R PROCESSED THE PFTE*/ 00676000
         LTR   R1,R1                                               0254 00677000
