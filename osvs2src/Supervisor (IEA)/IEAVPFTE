*                                                            @Z40FPYL*/ 00349000
         BAL   @14,PGSECS                                          0157 00350000
*             RSMASCB->ASCBFMCT=RSMASCB->ASCBFMCT-1;/*       @Z40FPYL*/ 00351000
         L     @11,RSMASCB(,RSMHDPTR)                              0158 00352000
         LH    @07,ASCBFMCT(,@11)                                  0158 00353000
         BCTR  @07,0                                               0158 00354000
         STH   @07,ASCBFMCT(,@11)                                  0158 00355000
*           END;                    /*                       @Z40FPYL*/ 00356000
*         ELSE                                                     0160 00357000
*           ;                       /* NOT FROM CFQ OR LFQ   @Z40FPYL*/ 00358000
@RF00155 DS    0H                                                  0161 00359000
*       GOTO EXITPFTE;              /* NO MATTER WHERE IT CAME FROM  */ 00360000
         B     EXITPFTE                                            0161 00361000
*     END;                          /* MUST GO TO EXIT ROUTINE     0162 00362000
*                                                            @Z40FPYL*/ 00363000
*   ELSE                                                           0163 00364000
*     ;                             /* PFTE DOES NEED TO BE ENQD   0163 00365000
*                                                            @Z40FPYL*/ 00366000
@RF00151 DS    0H                                                  0164 00367000
*   IF SAVETQID=PFTAFQN THEN        /* TEST IF THE PFTE IS TO BE   0164 00368000
*                                      ENQUEUED TO THE AFQ           */ 00369000
