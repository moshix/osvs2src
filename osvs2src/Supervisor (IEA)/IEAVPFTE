*   SRBID='SRB ';                   /* SET SRB IDENTIFIER            */ 01119000
         MVC   SRBID(4,SRBPTR),@CC01799                            0374 01120000
*   SRBRMTR=PVTPSRBP;               /* GET RMTR ENTRY POINT          */ 01121000
