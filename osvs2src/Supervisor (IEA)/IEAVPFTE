*                                      BEEN NOTIFIED                 */ 00255000
         TM    PVTLSI(PVTPTR),B'00000100'                          0130 00256000
         BNZ   @RF00130                                            0130 00257000
*             DO;                   /* SRM NOT NOTIFIED: ISSUE AVQLOW   00258000
*                                      SYSEVENT                      */ 00259000
*               RESPECIFY                                          0132 00260000
*                (R1,                                              0132 00261000
*                 R13) RESTRICTED;                                 0132 00262000
*               R13=ADDR(PVTSAVE);  /* GET SAVE AREA ADDR FOR SRM TO    00263000
*                                      USE                           */ 00264000
         LA    R13,PVTSAVE(,PVTPTR)                                0133 00265000
*               R1=REGSYS;          /* SET CODE FOR REGULAR AVQLOW 0134 00266000
