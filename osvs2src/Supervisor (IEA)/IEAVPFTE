*                                      GET ADDRESS OF THESE POINTERS */ 00902000
         LA    QHEADEND,PVTAFQF(,PVTPTR)                           0315 00903000
*   IF SAVEFLAG='0'B THEN           /* TEST IF PFTE IS TO BE QUEUED     00904000
*                                      TO THE HEAD OR END OF THE AFQ */ 00905000
         TM    SAVEFLAG(PVTPTR),B'10000000'                        0316 00906000
         BZ    @RT00316                                            0316 00907000
*     GOTO PFTENQ;                  /* IS TO BE ENQUEUED TO THE END     00908000
*                                      OF THE AFQ. GO TO THE REGULAR    00909000
*                                      ENQ PROCESSOR.                */ 00910000
*   IF ENDPTR=0 THEN                /* TEST IF THE AFQ IS EMPTY      */ 00911000
         ICM   @11,3,ENDPTR(QHEADEND)                              0318 00912000
         BZ    @RT00318                                            0318 00913000
*     GOTO PFTENQ;                  /* AFQ IS EMPTY. GO TO THE     0319 00914000
*                                      REGULAR ENQ PROCESSOR.        */ 00915000
*   RESPECIFY                                                      0320 00916000
*    (FRSTPFTE) RESTRICTED;                                        0320 00917000
*   FRSTPFTE=PVTPFTP+HEADPTR;       /* GET THE ADDRESS OF THE FIRST     00918000
*                                      PFTE ON THE AFQ               */ 00919000
         SLR   FRSTPFTE,FRSTPFTE                                   0321 00920000
