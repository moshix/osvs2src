*               END;                /*                       @ZA16816*/ 01268000
@RC00409 DS    0H                                                  0416 01269000
*           END;                    /* END OF DO WHILE       @ZA16816*/ 01270000
@RC00405 DS    0H                                                  0416 01271000
@DE00404 C     PFTEPTR,PVTPFTP(,PVTPTR)                            0416 01272000
         BNE   @DL00404                                            0416 01273000
*           IF SAVERBN=0 THEN       /* SHOULD HAVE AN RBN.   @ZA16816*/ 01274000
         ICM   @11,3,SAVERBN(PVTPTR)                               0417 01275000
         BNZ   @RF00417                                            0417 01276000
*             DO;                   /* ERROR IF ITS ZERO -   @ZA16816*/ 01277000
*               PVTSQDC=0;          /* SHUT DOWN THE SQA     @ZA16816*/ 01278000
         MVI   PVTSQDC(PVTPTR),X'00'                               0419 01279000
*               PVTRSRVF=0;         /* RESERVE QUEUE AND     @ZA16816*/ 01280000
         SLR   @11,@11                                             0420 01281000
         STH   @11,PVTRSRVF(,PVTPTR)                               0420 01282000
*               PVTRSRVL=0;         /* ABEND C0D             @ZA16816*/ 01283000
         STH   @11,PVTRSRVL(,PVTPTR)                               0421 01284000
*               RCARCRD='21'X;      /* SET REASON CODE       @ZA16816*/ 01285000
