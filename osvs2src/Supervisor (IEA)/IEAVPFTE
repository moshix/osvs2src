         AL    FRSTPFTE,PVTPFTP(,PVTPTR)                           0321 00922000
*   FRSTPFTE->PFTBQPTR=PFTERBN;     /* MAKE THE FIRST PFTE POINT BACK   00923000
*                                      TO THE INPUT PFTE             */ 00924000
         STH   PFTERBN,PFTBQPTR(,FRSTPFTE)                         0322 00925000
*   PFTFQPTR=HEADPTR;               /* MAKE THE INPUT PFTE POINT   0323 00926000
