         STCM  XPTEPTR,7,PCBXPTA(PCBPTR)                           0141 00282000
*   PCBVBN=VBN;                     /* SET THE VBN FROM THE VSA      */ 00283000
         MVC   PCBVBN(2,PCBPTR),VBN(LCCAPTR)                       0142 00284000
*   VBNP=VBNP&'F0'X;                /* TURN OFF LOW-ORDER 4 BITS     */ 00285000
         NI    VBNP(PCBPTR),X'F0'                                  0143 00286000
*   PCBASCB=ASCBPTR;                /* SET THE ASCB ADDRESS          */ 00287000
         L     @12,ASCBPTR                                         0144 00288000
         ST    @12,PCBASCB(,PCBPTR)                                0144 00289000
*   IF PCBVBN<PVTLCSA THEN          /* IS PAGE IN PRIVATE AREA?    0145 00290000
*                                                            @Z40WPYD*/ 00291000
         CLC   PCBVBN(2,PCBPTR),PVTLCSA(PVTPTR)                    0145 00292000
         BNL   @RF00145                                            0145 00293000
*     AIAPRIV='1'B;                 /* YES, SET PRIVATE AREA FLAG ON    00294000
*                                                            @Z40WPYD*/ 00295000
         OI    AIAPRIV(PCBPTR),B'01000000'                         0146 00296000
*   RESPECIFY                                                      0147 00297000
*     XPTEPTR UNRESTRICTED;                                        0147 00298000
@RF00145 DS    0H                                                  0148 00299000
*                                                                  0148 00300000
*   /*****************************************************************/ 00301000
*   /*                                                               */ 00302000
*   /* PASS THE PCB TO GENERAL FRAME ALLOCATION TO HAVE A FRAME      */ 00303000
*   /* ASSIGNED AND PAGE-IN I/O SCHEDULED IF REQUIRED                */ 00304000
*   /*                                                               */ 00305000
*   /*****************************************************************/ 00306000
*                                                                  0148 00307000
*   CALL GFA;                                                      0148 00308000
         L     @15,PVTPGFA(,PVTPTR)                                0148 00309000
         BALR  @14,@15                                             0148 00310000
*   RCACSECT=RCACPIX;               /* RESET THE MODULE ID IN THE RCA*/ 00311000
         MVI   RCACSECT(RCAPTR),X'0D'                              0149 00312000
*/*                                                                  */ 00313000
*/*                                                                  */ 00314000
*                                                                  0150 00315000
*   /*****************************************************************/ 00316000
*   /*                                                               */ 00317000
*   /* CHECK THE RETURN CODE TO DETERMINE IF THE TASK HAS BEEN       */ 00318000
*   /* SUSPENDED OR PUT ON A DEFER QUEUE                             */ 00319000
*   /*                                                               */ 00320000
*   /*****************************************************************/ 00321000
*                                                                  0150 00322000
*   IF R15=4 THEN                   /* RETURN CODE 4 FROM GFA MEANS  */ 00323000
         C     R15,@CF00043                                        0150 00324000
         BNE   @RF00150                                            0150 00325000
*     DO;                           /* THE TASK MAY NOT BE DISPATCHED*/ 00326000
*       PVTREUS=0;                  /* ZERO THE REUSABLE PCB POINTER.   00327000
*                                      THE PCB WAS USED BY IEAVGFA.     00328000
*                                      THE PAGE WAS NOT RECLAIMED. 0152 00329000
*                                                            @Z40WPYD*/ 00330000
*                                                                  0152 00331000
         SLR   @12,@12                                             0152 00332000
         ST    @12,PVTREUS(,PVTPTR)                                0152 00333000
*       /*************************************************************/ 00334000
*       /*                                                           */ 00335000
*       /* IF PVTSIT IS ON, THEN NEED TO REPLENISH SSRB CHAINED OFF  */ 00336000
*       /* PVT. IF OFF, THEN SUSPEND DID NOT USE                     */ 00337000
*       /*                                                           */ 00338000
*       /*************************************************************/ 00339000
*                                                                  0153 00340000
*       IF PVTSIT='1'B              /* IF ON,THEN MUST GETMAIN EXTRA    00341000
*                                      SSRB                          */ 00342000
*         THEN                                                     0153 00343000
         TM    PVTSIT(PVTPTR),B'00000010'                          0153 00344000
         BNO   @RF00153                                            0153 00345000
*         DO;                                                      0154 00346000
*           RESPECIFY                                              0155 00347000
*            (R0,                                                  0155 00348000
*             R1,                                                  0155 00349000
*             R5,                                                  0155 00350000
*             R8,                                                  0155 00351000
*             R15) RSTD;                                           0155 00352000
*           R8=R3;                  /* SAVE ADDR PVT                 */ 00353000
         LR    R8,R3                                               0156 00354000
*           R5=PVTPRCA;             /* SAVE SINCE CLOBBERED ACCROSS     00355000
*                                      GETMAIN                       */ 00356000
         L     R5,PVTPRCA(,PVTPTR)                                 0157 00357000
*           R0=LENGTH(SSRBSECT);    /* LENGTH TO GETMAIN             */ 00358000
         LA    R0,753                                              0158 00359000
*           GENERATE REFS(WSAGGMFM,WSAG);                          0159 00360000
        GETMAIN RC,LV=(0),SP=245,BRANCH=(YES,GLOBAL)                    00361000
*           RESPECIFY                                              0160 00362000
*            (R0,                                                  0160 00363000
*             R5,                                                  0160 00364000
*             R8) UNRSTD;                                          0160 00365000
*           R3=R8;                  /* RESTORE PVT ADDR              */ 00366000
         LR    R3,R8                                               0161 00367000
*           PVTPRCA=R5;             /* RESTORE RCA ADDR              */ 00368000
*                                                                  0162 00369000
         ST    R5,PVTPRCA(,PVTPTR)                                 0162 00370000
*           /*********************************************************/ 00371000
*           /*                                                       */ 00372000
*           /* IF R15 = 0 THEN GETMAIN SUCCESSFUL, SO SET PVTSIT TO 0*/ 00373000
*           /* AND PUT GETMAINED ADDR IN PVTSSRB                     */ 00374000
*           /*                                                       */ 00375000
*           /*********************************************************/ 00376000
*                                                                  0163 00377000
*           IF R15=0 THEN                                          0163 00378000
         LTR   R15,R15                                             0163 00379000
         BNZ   @RF00163                                            0163 00380000
*             DO;                                                  0164 00381000
*               PVTSIT='0'B;        /* TURN OFF BIT                  */ 00382000
         NI    PVTSIT(PVTPTR),B'11111101'                          0165 00383000
*               PVTSSRB=R1;         /* NES SSRB ADDR                 */ 00384000
         ST    R1,PVTSSRB(,PVTPTR)                                 0166 00385000
*             END;                                                 0167 00386000
*         END;                                                     0168 00387000
@RF00163 DS    0H                                                  0169 00388000
*       R15=0;                      /* SET RETURN CODE FOR THIS      */ 00389000
@RF00153 SLR   R15,R15                                             0169 00390000
*       GOTO PIX0200;               /* CONDITION AND EXIT            */ 00391000
         B     PIX0200                                             0170 00392000
*       RESPECIFY                                                  0171 00393000
*        (R1,                                                      0171 00394000
*         R15) UNRSTD;                                             0171 00395000
*     END;                                                         0172 00396000
*   IF R15=0 THEN                   /* RETURN CODE 0 FROM GFA MEANS  */ 00397000
@RF00150 LTR   R15,R15                                             0173 00398000
         BNZ   @RF00173                                            0173 00399000
*     DO;                           /* THE TASK MAY BE DISPATCHED    */ 00400000
*       R15=4;                      /* SET RETURN CODE FOR THIS      */ 00401000
         LA    R15,4                                               0175 00402000
*       GOTO PIX0200;               /* CONDITION AND EXIT            */ 00403000
         B     PIX0200                                             0176 00404000
*     END;                                                         0177 00405000
*   RCARCRD='19'X;                  /* ANY OTHER RETURN CODE FROM GFA   00406000
*                                      IS INVALID FOR A PAGE FAULT SO   00407000
*                                      SET THE REASON CODE AND INVOKE   00408000
*                                      THE FRR ROUTINE               */ 00409000
@RF00173 MVI   RCARCRD(RCAPTR),X'19'                               0178 00410000
*                                                                  0179 00411000
*   /*****************************************************************/ 00412000
*   /*                                                               */ 00413000
*   /* THE FOLLOWING IS A COMMON ERROR EXIT ROUTINE USED TO INVOKE   */ 00414000
*   /* THE FRR VIA AN ABEND MACRO CALL AND TO SET THE RETURN CODE TO */ 00415000
*   /* INDICATE THAT AN INTERNAL ERROR HAS BEEN DETECTED SOMEWHERE IN*/ 00416000
*   /* THE PROCESSING.                                               */ 00417000
*   /*                                                               */ 00418000
*   /*****************************************************************/ 00419000
*                                                                  0179 00420000
*PIX0100:                                                          0179 00421000
*   GENERATE;                                                      0179 00422000
PIX0100  DS    0H                                                  0179 00423000
        ABEND X'C0D',,,SYSTEM    ISSUE ABEND TO INVOKE THE FRR          00424000
*   R15=12;                         /* SET THE RETURN CODE AND FALL     00425000
*                                      THRU TO EXIT                  */ 00426000
         LA    R15,12                                              0180 00427000
*/*                                                                  */ 00428000
*/*                                                                  */ 00429000
*                                                                  0181 00430000
*   /*****************************************************************/ 00431000
*   /*                                                               */ 00432000
*   /* THE FOLLOWING IS THE COMMON EXIT PATH FOR THIS ROUTINE. THE   */ 00433000
*   /* FRR ROUTINE IS DELETED, THE SALLOC LOCK IS RELEASED, AND      */ 00434000
*   /* RETURN IS MADE TO THE PROGRAM CHECK INTERRUPT HANDLER.        */ 00435000
*   /*                                                               */ 00436000
*   /*****************************************************************/ 00437000
*                                                                  0181 00438000
*PIX0200:                                                          0181 00439000
*   RESPECIFY                                                      0181 00440000
*     R8 RESTRICTED;                                               0181 00441000
PIX0200  DS    0H                                                  0182 00442000
*   RESPECIFY                                                      0182 00443000
*     R9 RESTRICTED;                                               0182 00444000
*   RESPECIFY                                                      0183 00445000
*     R11 RESTRICTED;                                              0183 00446000
*   RESPECIFY                                                      0184 00447000
*     R12 RESTRICTED;                                              0184 00448000
*   GENERATE;                                                      0185 00449000
         SETFRR D,WRKREGS=(R8,R9)       DELETE THE FRR ROUTINE          00450000
FREELOCK SETLOCK RELEASE,TYPE=SALLOC,DISABLED,                        **00451000
               RELATED=(RSM,IEAVPIX(GETLOCK))  RELEASE THE LOCK         00452000
