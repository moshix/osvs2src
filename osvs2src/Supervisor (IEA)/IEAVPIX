*                                                                  0137 00261000
*/*                                                                  */ 00262000
*/*                                                                  */ 00263000
*                                                                  0138 00264000
*   /*****************************************************************/ 00265000
*   /*                                                               */ 00266000
*   /* INITIALIZE THE PCB WHEN THE PCB TRACE IS ACTIVE, THE NOOP     */ 00267000
*   /* BELOW MUST BE ZAPPED TO X'920D1040'                   @Z40WPYD*/ 00268000
*   /*                                                               */ 00269000
*   /*****************************************************************/ 00270000
*                                                                  0138 00271000
*TRACEZX1:                                                         0138 00272000
*   GENERATE;                                                      0138 00273000
@RC00119 DS    0H                                                  0138 00274000
TRACEZX1 DS    0H                                                  0138 00275000
          BC    0,0                                                     00276000
*   PCBPEX='1'B;                    /* SET THE PAGE EXCEPTION FLAG   */ 00277000
         OI    PCBPEX(PCBPTR),B'01000000'                          0139 00278000
*   PCBPGTA=PTEPTR;                 /* SET THE PAGE TABLE ADDRESS    */ 00279000
         STCM  PTEPTR,7,PCBPGTA(PCBPTR)                            0140 00280000
