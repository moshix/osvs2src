*       RESPECIFY                                                  0124 00203000
*         R0 UNRESTRICTED;                                         0124 00204000
*                                                                  0124 00205000
*/*                                                                  */ 00206000
*/*                                                                  */ 00207000
*                                                                  0125 00208000
*       /*************************************************************/ 00209000
*       /*                                                           */ 00210000
*       /* CHECK THE RETURN CODE FROM PCB MANAGER                    */ 00211000
*       /*                                                           */ 00212000
*       /*************************************************************/ 00213000
*                                                                  0125 00214000
*       IF R15^=0 THEN              /* A NON-ZERO RETURN CODE        */ 00215000
         LTR   R15,R15                                             0125 00216000
         BZ    @RF00125                                            0125 00217000
*         DO;                       /* INDICATES THAT THE PCB        */ 00218000
*           RCARCRD='0E'X;          /* WAS NOT BUILT. SET THE REASON    00219000
*                                      CODE                          */ 00220000
         MVI   RCARCRD(RCAPTR),X'0E'                               0127 00221000
*           GOTO PIX0100;           /* AND GO INVOKE THE FRR ROUTINE */ 00222000
         B     PIX0100                                             0128 00223000
*         END;                                                     0129 00224000
*       ELSE                                                       0130 00225000
*         DO;                                                      0130 00226000
@RF00125 DS    0H                                                  0131 00227000
*           PVTREUS=PCBPTR;         /* SET REUSABLE PCB POINTER IN 0131 00228000
*                                      THE PVT               @Z40WPYD*/ 00229000
         ST    PCBPTR,PVTREUS(,PVTPTR)                             0131 00230000
*         END;                                                     0132 00231000
*     END;                                                         0133 00232000
*                                                                  0133 00233000
*   /*****************************************************************/ 00234000
*   /*                                                               */ 00235000
*   /* WHEN PCB TRACE IS ACTIVE, THE XC INSTRUCTION MUST BE ZAPPED TO*/ 00236000
*   /* HANDLE THE LARGER PCB                                 @Z40WPYD*/ 00237000
*   /*                                                               */ 00238000
*   /*****************************************************************/ 00239000
*                                                                  0134 00240000
*   ELSE                                                           0134 00241000
*     DO;                                                          0134 00242000
*                                                                  0134 00243000
         B     @RC00119                                            0134 00244000
@RF00119 DS    0H                                                  0135 00245000
*       /*************************************************************/ 00246000
*       /*                                                           */ 00247000
*       /* INITIALIZE THE PCB                                        */ 00248000
*       /*                                                           */ 00249000
*       /*************************************************************/ 00250000
*                                                                  0135 00251000
*       PCBPTR=PVTREUS;             /* SET PCBPTR EQUAL TO THE     0135 00252000
