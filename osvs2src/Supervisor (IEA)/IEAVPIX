         TITLE 'IEAVPIX  -  PROGRAM CHECK INTERRUPT EXTENSION          *00001000
                        '                                               00002000
IEAVPIX  CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVPIX   76.086'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @10,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@10                                         0001 00012000
*                                                                  0074 00013000
*   /*****************************************************************/ 00014000
*   /*                                                               */ 00015000
*   /* PRIME THE REGISTERS WHICH NEED TO HAVE POINTERS ESTABLISHED   */ 00016000
*   /*                                                               */ 00017000
*   /*****************************************************************/ 00018000
*                                                                  0074 00019000
*   PVTPTR=CVTPVTP;                                                0074 00020000
         L     @12,CVTPTR                                          0074 00021000
         L     PVTPTR,CVTPVTP(,@12)                                0074 00022000
*   RSMHDPTR=ASCBRSM;                                              0075 00023000
         L     @12,ASCBPTR                                         0075 00024000
         L     RSMHDPTR,ASCBRSM(,@12)                              0075 00025000
*   LCCAPTR=PSALCCAV;                                              0076 00026000
         L     LCCAPTR,PSALCCAV                                    0076 00027000
*   R7=R14;                         /* SAVE THE RETURN REGISTER      */ 00028000
         LR    R7,R14                                              0077 00029000
*/*                                                                  */ 00030000
*                                                                  0078 00031000
*   /*****************************************************************/ 00032000
*   /*                                                               */ 00033000
*   /* OBTAIN THE SALLOC LOCK                                        */ 00034000
*   /*                                                               */ 00035000
*   /*****************************************************************/ 00036000
*                                                                  0078 00037000
*   RESPECIFY                                                      0078 00038000
*     R11 RESTRICTED;                                              0078 00039000
*   RESPECIFY                                                      0079 00040000
*     R12 RESTRICTED;                                              0079 00041000
*   GENERATE;                                                      0080 00042000
GETLOCK  SETLOCK OBTAIN,TYPE=SALLOC,DISABLED,MODE=UNCOND,             **00043000
               RELATED=(RSM,IEAVPIX(FREELOCK))                          00044000
*   RESPECIFY                                                      0081 00045000
*     R11 UNRESTRICTED;                                            0081 00046000
*   RESPECIFY                                                      0082 00047000
*     R12 UNRESTRICTED;                                            0082 00048000
*                                                                  0082 00049000
*   /*****************************************************************/ 00050000
*   /*                                                               */ 00051000
*   /* CHECK THE RETURN CODE FROM LOCK MANAGER                       */ 00052000
*   /*                                                               */ 00053000
*   /*****************************************************************/ 00054000
*                                                                  0083 00055000
*   IF R13^=0 THEN                  /* A CODE GREATER THAN 0 IMPLIES    00056000
*                                      THE                           */ 00057000
         LTR   R13,R13                                             0083 00058000
         BZ    @RF00083                                            0083 00059000
*     DO;                           /* LOCK WAS NOT OBTAINED       0084 00060000
*                                      SUCCESSFULLY                  */ 00061000
*       R15=12;                     /* SET RETURN CODE FOR AN        */ 00062000
         LA    R15,12                                              0085 00063000
*       R14=R7;                     /* INTERNAL ERROR SITUATION AND     00064000
*                                      RETURN TO THE PGM CHECK     0086 00065000
*                                      INTERRUPT HANDLER.            */ 00066000
         LR    R14,R7                                              0086 00067000
*       RETURN CODE(R15);           /* EXIT                          */ 00068000
@EL00001 DS    0H                                                  0087 00069000
@EF00001 DS    0H                                                  0087 00070000
@ER00001 BR    @14                                                 0087 00071000
*     END;                                                         0088 00072000
*                                                                  0089 00073000
*   /*****************************************************************/ 00074000
*   /*                                                               */ 00075000
*   /* IF THE SALLOC LOCK IS OBTAINED SUCCESSFULLY, AN FRR           */ 00076000
*   /* ENVIRONMENT IS ESTABLISHED AND THE RECOVERY COMMUNICATIONS    */ 00077000
*   /* AREA IS INITIALIZED.                                          */ 00078000
*   /*                                                               */ 00079000
*   /*****************************************************************/ 00080000
*                                                                  0089 00081000
*   RESPECIFY                                                      0089 00082000
*     R8 RESTRICTED;                                               0089 00083000
@RF00083 DS    0H                                                  0090 00084000
*   RESPECIFY                                                      0090 00085000
*     R9 RESTRICTED;                                               0090 00086000
*   GENERATE;                                                      0091 00087000
       SETFRR A,FRRAD=PVTPRCV(R3),PARMAD=(RCAPTR),WRKREGS=(R8,R9)       00088000
*   RESPECIFY                                                      0092 00089000
*     R8 UNRESTRICTED;                                             0092 00090000
*   RESPECIFY                                                      0093 00091000
*     R9 UNRESTRICTED;                                             0093 00092000
*   PVTPRCA=RCAPTR;                 /* SAVE POINTER TO THE RCA       */ 00093000
         ST    RCAPTR,PVTPRCA(,PVTPTR)                             0094 00094000
*   RCACSECT=RCACPIX;               /* SET THE MODULE IN CONTROL FLAG*/ 00095000
         MVI   RCACSECT(RCAPTR),X'0D'                              0095 00096000
*   RCAPIX='1'B;                    /* SET THE ENTRY POINT ID        */ 00097000
         OI    RCAPIX(RCAPTR),B'10000000'                          0096 00098000
*   RCASAL='1'B;                    /* INDICATE SALLOC IS HELD       */ 00099000
         OI    RCASAL(RCAPTR),B'10000000'                          0097 00100000
*                                                                  0098 00101000
*   /*****************************************************************/ 00102000
*   /*                                                               */ 00103000
*   /* CALL 'FINDPAGE' TO LOCATE THE PAGE TABLE AND THE EXTERNAL PAGE*/ 00104000
*   /* TABLE ASSOCIATED WITH THE PAGE FAULT ADDRESS                  */ 00105000
*   /*                                                               */ 00106000
*   /*****************************************************************/ 00107000
*                                                                  0098 00108000
*   R1=LCCAPVAD;                    /* PUT THE VSA IN REG 1          */ 00109000
         L     R1,LCCAPVAD(,LCCAPTR)                               0098 00110000
*   CALL FINDPAGE;                  /* CALL IEAVFP2                  */ 00111000
         L     @15,PVTPFP2(,PVTPTR)                                0099 00112000
         BALR  @14,@15                                             0099 00113000
*/*                                                                  */ 00114000
*/*                                                                  */ 00115000
*                                                                  0100 00116000
*   /*****************************************************************/ 00117000
*   /*                                                               */ 00118000
*   /* CHECK THE RETURN CODE FROM FINDPAGE                           */ 00119000
*   /*                                                               */ 00120000
*   /*****************************************************************/ 00121000
*                                                                  0100 00122000
*   IF R15^=0 THEN                  /* IF REG 15 IS 0, THE TABLES    */ 00123000
         LTR   R15,R15                                             0100 00124000
         BZ    @RF00100                                            0100 00125000
*     DO;                           /* HAVE BEEN FOUND INT TACT      */ 00126000
*       RCARCRD='01'X;              /* OTHERWISE, SET THE REASON CODE*/ 00127000
         MVI   RCARCRD(RCAPTR),X'01'                               0102 00128000
*       GOTO PIX0100;               /* FOR THE FRR ROUTINE AND       */ 00129000
         B     PIX0100                                             0103 00130000
*     END;                          /* GO RECORD THE ERROR           */ 00131000
*                                                                  0105 00132000
*   /*****************************************************************/ 00133000
*   /*                                                               */ 00134000
*   /* IF IT IS FOUND THAT THE PAGE HAS ALREADY BEEN VALIDATED IN    */ 00135000
*   /* REAL STORAGE, RETURN IS MADE TO PCFLIH WITH A CODE 4 IN REG 15*/ 00136000
*   /* TO INDICATE THAT THE TASK MAY BE IMMEDIATELY REDISPATCHED     */ 00137000
*   /*                                                               */ 00138000
*   /*****************************************************************/ 00139000
*                                                                  0105 00140000
*   RESPECIFY                                                      0105 00141000
*     PTEPTR RESTRICTED;                                           0105 00142000
@RF00100 DS    0H                                                  0106 00143000
*   PTEPTR=R0;                      /* PUT THE PGTE ADDRESS IN REG 4 */ 00144000
         LR    PTEPTR,R0                                           0106 00145000
*   IF PGTPVM='0'B THEN             /* PGTPVM OFF MEANS THE PAGE IS  */ 00146000
         TM    PGTPVM(PTEPTR),B'00001000'                          0107 00147000
         BNZ   @RF00107                                            0107 00148000
*     DO;                           /* ALREADY VALID IN CORE         */ 00149000
*       R15=4;                      /* SET RETURN CODE 4             */ 00150000
         LA    R15,4                                               0109 00151000
*       GOTO PIX0200;               /* AND EXIT                      */ 00152000
         B     PIX0200                                             0110 00153000
*     END;                                                         0111 00154000
*                                                                  0111 00155000
*/*                                                                  */ 00156000
*/*                                                                  */ 00157000
*                                                                  0112 00158000
*   /*****************************************************************/ 00159000
*   /*                                                               */ 00160000
*   /* THE VALIDITY OF THE PAGE FAULT IS NEXT DETERMINED BY VERIFYING*/ 00161000
*   /* THAT THE 'GETMAIN ASSIGNED' FLAG IS ON IN THE PAGE TABLE      */ 00162000
*   /*                                                               */ 00163000
*   /*****************************************************************/ 00164000
*                                                                  0112 00165000
*   IF PGTPAM='0'B THEN             /* IF THE PAGE WAS NOT GETMAINED */ 00166000
@RF00107 TM    PGTPAM(PTEPTR),B'00000001'                          0112 00167000
         BNZ   @RF00112                                            0112 00168000
*     DO;                           /* TREAT THE PAGE FAULT AS A     */ 00169000
*       R15=8;                      /* LOGICAL PROTECTION EXCEPTION  */ 00170000
         LA    R15,8                                               0114 00171000
*       GOTO PIX0200;               /* RETURN TO PCFLIH              */ 00172000
         B     PIX0200                                             0115 00173000
*     END;                                                         0116 00174000
*   RESPECIFY                                                      0117 00175000
*     XPTEPTR RESTRICTED;                                          0117 00176000
@RF00112 DS    0H                                                  0118 00177000
*   XPTEPTR=R1;                     /* SAVE THE XPTE ADDR IN REG 5   */ 00178000
         LR    XPTEPTR,R1                                          0118 00179000
*                                                                  0119 00180000
*   /*****************************************************************/ 00181000
*   /*                                                               */ 00182000
*   /* A PCB MUST BE BUILT TO PROCESS THE PROBABLE PAGE-IN USE PCB   */ 00183000
*   /* ADDRESSED BY PVTREUS IF AVAILABLE                     @Z40WPYD*/ 00184000
*   /*                                                               */ 00185000
*   /*****************************************************************/ 00186000
*                                                                  0119 00187000
*   IF PVTREUS=0 THEN               /* IS THE REUSABLE PCB AVAILABLE    00188000
*                                                            @Z40WPYD*/ 00189000
         L     @12,PVTREUS(,PVTPTR)                                0119 00190000
         LTR   @12,@12                                             0119 00191000
         BNZ   @RF00119                                            0119 00192000
*     DO;                           /* NO, REPLENISH THE REUSABLE PCB   00193000
*                                      VIA CALL TO PCB MANAGER     0120 00194000
*                                                            @Z40WPYD*/ 00195000
*       R0=1;                       /* REQUEST ONE PCB FROM THE      */ 00196000
         LA    R0,1                                                0121 00197000
