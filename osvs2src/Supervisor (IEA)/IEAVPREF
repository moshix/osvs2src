         LR    RBN,@14                                             0279 00788000
*                     REG15=4;      /* INSURE RC STILL INITIALIZED 0280 00789000
*                                                            @ZA16816*/ 00790000
         LA    REG15,4                                             0280 00791000
*                     GOTO LOOPEND; /* CONTINUE SEARCH WITH THE SAME    00792000
*                                      PFTE                  @ZA16816*/ 00793000
         B     LOOPEND                                             0281 00794000
*STOLEN:                                                           0282 00795000
*                                                                  0282 00796000
*                     /***********************************************/ 00797000
*                     /*                                             */ 00798000
*                     /* NON-IRRG FRAME CAN BE STOLEN. IF FROM A     */ 00799000
*                     /* FOREIGN LOCAL FRAME QUEUE, DEQUEUE THE PFTE */ 00800000
*                     /* NOW.                                @ZA16816*/ 00801000
*                     /*                                             */ 00802000
*                     /***********************************************/ 00803000
*                                                                  0282 00804000
*                     IF ASCBPTR^=0 THEN/* IF ASCB NOT ZERO, @ZA16816*/ 00805000
STOLEN   LTR   ASCBPTR,ASCBPTR                                     0282 00806000
         BZ    @RF00282                                            0282 00807000
*                       DO;         /* TRAS BACK AND DEQ     @ZA16816*/ 00808000
*                         REG1=PSAAOLD;/* GET CUR ASCB       @ZA16816*/ 00809000
         L     REG1,PSAAOLD                                        0284 00810000
*                         GENERATE CODE REFS(ASCBSTOR);            0285 00811000
                    LCTL 1,1,ASCBSTOR(REG1) -----TRAS BACK-----         00812000
*                         LCCAASID=0;/* CLEAR RTM TRAS IND.  @ZA16816*/ 00813000
         SLR   @14,@14                                             0286 00814000
         L     @11,LCCAPTR                                         0286 00815000
         ST    @14,LCCAASID(,@11)                                  0286 00816000
*                         PVTTASID=0;/* CLEAR RCV TRAS IND.  @ZA16816*/ 00817000
         STH   @14,PVTTASID(,PVTPTR)                               0287 00818000
*                         REG0=FRAMEPTR-PVTPFTP;/* GET THE RBN     0288 00819000
*                                                            @ZA16816*/ 00820000
