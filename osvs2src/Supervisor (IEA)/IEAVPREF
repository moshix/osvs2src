         L     ASCBPTR,ASCBFWDP(,ASCBPTR)                          0105 00151000
*                   END;            /*                       @ZA16816*/ 00152000
*               END;                /*                       @ZA16816*/ 00153000
@RC00100 DS    0H                                                  0107 00154000
@DE00099 LTR   REG15,REG15                                         0107 00155000
         BNZ   @DL00099                                            0107 00156000
*               RESPECIFY                                          0108 00157000
*                (REG15) UNRSTD;    /*                       @ZA16816*/ 00158000
*               IF RCADISP='1'B THEN/* RELEASE DISP LOCK?    @ZA28519*/ 00159000
