         B     @RC00152                                            0164 00341000
@RF00152 DS    0H                                                  0165 00342000
*                 PCBRPTR=PCBRWRK3; /* GET THE NEXT PCBR     @ZA16816*/ 00343000
         L     PCBRPTR,PCBRWRK3(,PCBRPTR)                          0165 00344000
*                 GOTO PCBRLOOP;    /* GO CHECK ITS RANGE    @ZA16816*/ 00345000
         B     PCBRLOOP                                            0166 00346000
*               END;                /*                       @ZA16816*/ 00347000
*         END;                      /*                       @ZA16816*/ 00348000
*     END;                          /* END OF STEAL OPTION   @ZA16816*/ 00349000
*   ELSE                            /* WANT TO CONVERT A UNIT TO   0170 00350000
*                                      PREFERRED                     */ 00351000
*     DO;                           /* DO THE CONVERSION             */ 00352000
         B     @RC00069                                            0170 00353000
@RF00069 DS    0H                                                  0171 00354000
*       RESPECIFY                                                  0171 00355000
*        (MASK) RSTD;                                              0171 00356000
*       MASK=((PVTRSUS-1)&&ALLFOXES)*16;/* CONVERT THE # OF FRAMES IN   00357000
*                                      A STORAGE UNIT TO A MASK TO BE   00358000
*                                      USED TO ROUND A RBN TO A BOX     00359000
*                                      BOUNDARY                      */ 00360000
         LH    @06,PVTRSUS(,PVTPTR)                                0172 00361000
         N     @06,@CF01844                                        0172 00362000
