         L     REG6,SAVER0(,PVTPTR)                                0159 00330000
*                     CALL IEAVEQRI;/* CANCEL THE V=R JOB    @ZA16816*/ 00331000
         L     @15,PVTPVRIN(,PVTPTR)                               0160 00332000
         BALR  @14,@15                                             0160 00333000
*                     RESPECIFY                                    0161 00334000
*                      (REG1,                                      0161 00335000
*                       REG6) UNRSTD;/*                      @ZA16816*/ 00336000
*                   END;            /*                       @ZA16816*/ 00337000
*               END;                /*                       @ZA16816*/ 00338000
*             ELSE                  /* HERE IF WRONG PCBR    @ZA16816*/ 00339000
