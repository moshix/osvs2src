         SLL   MASK,4                                              0172 00366000
*       RBN=RBN&MASK;               /* ROUND THE INPUT RBN TO A LOWER   00367000
*                                      BOX BOUNDARY                  */ 00368000
         NR    RBN,MASK                                            0173 00369000
*       RFY                                                        0174 00370000
*        (MASK) UNRSTD;                                            0174 00371000
*       RFY                                                        0175 00372000
*        (ENDPFTE,                                                 0175 00373000
*         PFTEPTR) RSTD;                                           0175 00374000
*       ENDPFTE=PVTPFTP+(RBN+((PVTRSUS-1)*LENGTH(PFTE)));/* GET END     00375000
*                                      OF LOOP. IS THE HIGHEST PFTE     00376000
*                                      IN A STORAGE UNIT             */ 00377000
         L     @05,PVTPFTP(,PVTPTR)                                0176 00378000
         SLA   @06,4                                               0176 00379000
