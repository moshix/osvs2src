*                                                            @ZA16816*/ 00299000
*                                                                  0152 00300000
*             /*******************************************************/ 00301000
*             /*                                                     */ 00302000
*             /* DETERMINE IF THE STOLEN FRAME IS WITHIN THE REGION  */ 00303000
*             /* DEFINED BY THIS ROOT PCB. PCBRWRK1 AND PCBRWRK2     */ 00304000
*             /* CONTAIN THE START AND END RBNS OF THE V=R REGION    */ 00305000
*             /* REPRESENTED BY THIS ROOT PCB.               @ZA16816*/ 00306000
*             /*                                                     */ 00307000
*             /*******************************************************/ 00308000
*                                                                  0152 00309000
*             IF RBN^<PCBRWRK1&     /* IF THE CORRECT        @ZA16816*/ 00310000
*                 RBN^>PCBRWRK2 THEN/* ROOT PCBR IS FOUND    @ZA16816*/ 00311000
         B     @RC00146                                            0152 00312000
@RF00146 C     RBN,PCBRWRK1(,PCBRPTR)                              0152 00313000
         BL    @RF00152                                            0152 00314000
         C     RBN,PCBRWRK2(,PCBRPTR)                              0152 00315000
         BH    @RF00152                                            0152 00316000
*               DO;                 /* CANCEL THE V=R JOB    @ZA16816*/ 00317000
*                 PCBRFAIL='1'B;    /* SET CANCEL FLAG       @ZA16816*/ 00318000
         OI    PCBRFAIL(PCBRPTR),B'00001000'                       0154 00319000
*                 IF PCBRINT='0'B THEN/* IF NOT PREVIOUSLY   @ZA16816*/ 00320000
         TM    PCBRINT(PCBRPTR),B'10000000'                        0155 00321000
         BNZ   @RF00155                                            0155 00322000
*                   DO;             /* TERMINATED, CALL EQRI @ZA16816*/ 00323000
*                     RESPECIFY                                    0157 00324000
*                      (REG1,                                      0157 00325000
*                       REG6) RSTD; /*                       @ZA16816*/ 00326000
*                     REG1=RBN;     /* R1=RBN FOR IEAVEQRI   @ZA16816*/ 00327000
         LR    REG1,RBN                                            0158 00328000
