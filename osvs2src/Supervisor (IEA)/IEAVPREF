*       DO PFTEPTR=PVTPFTP+RBN TO ENDPFTE BY LENGTH(PFTE);/* PROCESS    00383000
*                                      EACH PFTE IN THE STORAGE BOX  */ 00384000
         ALR   @05,RBN                                             0177 00385000
         LR    PFTEPTR,@05                                         0177 00386000
         B     @DE00177                                            0177 00387000
@DL00177 DS    0H                                                  0178 00388000
