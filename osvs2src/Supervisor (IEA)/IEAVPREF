*                                                            @ZA16816*/ 00280000
         L     RBN,SAVER0(,PVTPTR)                                 0145 00281000
         SL    RBN,PVTPFTP(,PVTPTR)                                0145 00282000
*PCBRLOOP:                          /*                       @ZA16816*/ 00283000
*           IF PCBRPTR=0 THEN       /* IF ZERO, AN ERROR -   @ZA16816*/ 00284000
PCBRLOOP LTR   PCBRPTR,PCBRPTR                                     0146 00285000
         BNZ   @RF00146                                            0146 00286000
*             DO;                   /* RECORD ERROR          @ZA16816*/ 00287000
*               RCARCRD='08'X;      /* SET REASON CODE       @ZA16816*/ 00288000
         L     @06,RCAPTR(,PVTPTR)                                 0148 00289000
         MVI   RCARCRD(@06),X'08'                                  0148 00290000
*               GENERATE;                                          0149 00291000
              ABEND X'C0D',,,SYSTEM      ABEND CODE                     00292000
*               SAVER0->PFTVRINT='0'B;/* CLEAR INTERCEPT FLAG      0150 00293000
*                                                            @ZA16816*/ 00294000
         L     @06,SAVER0(,PVTPTR)                                 0150 00295000
         NI    PFTVRINT(@06),B'10111111'                           0150 00296000
