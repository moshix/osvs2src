*             IF PFTASID='FFFF'X    /* IF =, THEN SQA FRAME          */ 00811000
*               THEN                                               0322 00812000
         CLC   PFTASID(2,PFTEPTR),@CH02300                         0322 00813000
         BNE   @RF00322                                            0322 00814000
*               SQAFLG='1'B;                                       0323 00815000
         OI    SQAFLG(FLGPTR),B'00001000'                          0323 00816000
