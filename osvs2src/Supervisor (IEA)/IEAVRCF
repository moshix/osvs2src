*                                                            @G559P2L*/ 01075000
         OI    PWANRHED(@07),B'00100000'                           0392 01076000
*           SRBPARM=LRBMFSA;        /* INPUT IS THE ERROR ADDRESS  0393 01077000
*                                                            @G559P2L*/ 01078000
         L     @08,LRBPTR(,@08)                                    0393 01079000
