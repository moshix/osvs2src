         TITLE 'IEAVRELS -- PAGE RELEASE                               *00001000
                        '                                               00002000
IEAVRELS CSECT                                                          00003000
         USING *,15                                                     00004000
         MODID BR=NO                                                    00005000
IEAVREL2 DS    0H                                                       00006000
         ENTRY IEAVREL2                                                 00007000
         ENTRY IEAVRELV                                                 00008000
         ENTRY IEAVRELF                                                 00009000
LBSETBAS BALR  RLBASERG,0                                               00010000
         USING *,RLBASERG                                               00011000
*   CIWRETRN=RLRETREG;              /* SAVE PSI RETURN ADDRESS       */ 00012000
         ST    RLRETREG,CIWRETRN(,CIWAPTR)                         0109 00013000
*   RCACSECT=RCACPGRL;              /* SET CSECT ID IN RCA           */ 00014000
         L     @12,RCAPTR(,PVTPTR)                                 0110 00015000
         MVI   RCACSECT(@12),X'07'                                 0110 00016000
*   RCARELS='1'B;                   /* SET ENTRY POINT FLAG IN RCA   */ 00017000
         OI    RCARELS(@12),B'00000010'                            0111 00018000
*                                                                  0112 00019000
*   /*****************************************************************/ 00020000
*   /*                                                               */ 00021000
*   /* THE MAIN PART OF THE IEAVRELS ENTRY LOGIC CONSISTS OF TWO     */ 00022000
*   /* LOOPS. THE OUTER LOOP CONTROLS PROCESSING OF VSL ENTRIES. FOR */ 00023000
*   /* REGISTER FORM REQUESTS (CIWINVSL=0), ONLY ONE VSL ENTRY EXISTS*/ 00024000
*   /* TO BE PROCESSED. FOR LIST FORM REQUESTS MANY VSL ENTRIES MAY  */ 00025000
*   /* EXIST. EACH NEW VSL ENTRY IS OBTAINED BY CALLING THE NEXTVSL  */ 00026000
*   /* SUBROUTINE OF IEAVPSI.                                        */ 00027000
*   /*                                                               */ 00028000
*   /*****************************************************************/ 00029000
*                                                                  0112 00030000
*LBVSLOOP:                                                         0112 00031000
*   RLTESTAD=CIWSTRTA+MPSZMIN1;     /* REDUCE THE START ADDR IN THE     00032000
*                                      CIWA TO THE NEXT HIGHER PAGE     00033000
*                                      BOUNDARY                      */ 00034000
LBVSLOOP LA    RLTESTAD,4095                                       0112 00035000
         L     @12,CIWSTRTA-1(,CIWAPTR)                            0112 00036000
         LA    @12,0(,@12)                                         0112 00037000
         ALR   RLTESTAD,@12                                        0112 00038000
*   RLTESTAD=RLTESTAD&MPGBDYMK;                                    0113 00039000
         N     RLTESTAD,@CF02426                                   0113 00040000
*   RLNUMBYT=CIWEND1A-RLTESTAD;     /* SET UP DO WHILE CONTROL VALUE */ 00041000
*                                                                  0114 00042000
         L     RLNUMBYT,CIWEND1A-1(,CIWAPTR)                       0114 00043000
         LA    RLNUMBYT,0(,RLNUMBYT)                               0114 00044000
         SLR   RLNUMBYT,RLTESTAD                                   0114 00045000
*   /*****************************************************************/ 00046000
*   /*                                                               */ 00047000
*   /* THE INNER DO WHILE LOOP CHECKS EACH PAGE IN A VSL ENTRY FOR   */ 00048000
*   /* PGRLSE ELIGIBILITY AND THEN CALLS THE INTERNAL PAGE PROCESSOR */ 00049000
*   /* SUBROUTINE (SUBPAGE) TO PERFORM ACTUAL RELEASE PROCESSING. A  */ 00050000
*   /* PAGE IS ELIGIBLE FOR RELEASE PROCESSING IF IT IS 1) IN A VALID*/ 00051000
*   /* SEGMENT, 2) IS NOT IN THE QUICK START AREA (QSA), AND 3) IS IN*/ 00052000
*   /* THE KEY OF THE CALLER IF THE CALLER'S KEY IS NOT ZERO.        */ 00053000
*   /*                                                               */ 00054000
*   /*****************************************************************/ 00055000
*                                                                  0115 00056000
*   DO WHILE(RLNUMBYT>=MPGSIZE);    /* THIS LOOP IS EXITED WHEN THE     00057000
*                                      NUMBER OF BYTES TO RELEASE  0115 00058000
*                                      FALLS BELOW 4096              */ 00059000
         B     @DE00115                                            0115 00060000
@DL00115 DS    0H                                                  0116 00061000
*     RELSVSA=RLTESTAD;             /* STORE VSA IN CIWA             */ 00062000
         ST    RLTESTAD,RELSVSA(,CIWAPTR)                          0116 00063000
*     CIWWRK1=RLNUMBYT;             /* SAVE LOOP CONTROL VALUE       */ 00064000
         ST    RLNUMBYT,CIWWRK1(,CIWAPTR)                          0117 00065000
*     RLPARMRG=RELSVSA;             /* GET PGTE AND XPTE ADDRESSES   */ 00066000
         L     @12,RELSVSA(,CIWAPTR)                               0118 00067000
         LR    RLPARMRG,@12                                        0118 00068000
*     CALL EPFINDPG;                /* SEE ENTRY DCL FOR INTERFACE   */ 00069000
         L     @15,PVTPFP2(,PVTPTR)                                0119 00070000
         BALR  @14,@15                                             0119 00071000
*     IF RLRETCOD^=MZERO THEN       /* WHAT IS RETURN CODE           */ 00072000
         LTR   RLRETCOD,RLRETCOD                                   0120 00073000
         BZ    @RF00120                                            0120 00074000
*       DO;                         /* RETURN CODE IS NOT ZERO       */ 00075000
*         IF RLRETCOD=8 THEN        /* WHICH ERROR RETURN CODE       */ 00076000
         C     RLRETCOD,@CF00039                                   0122 00077000
         BNE   @RF00122                                            0122 00078000
*           DO;                     /* A RETURN CODE 8 MEANS AN    0123 00079000
*                                      INTERNAL RSM ERROR            */ 00080000
*             RCARCRD='01'X;        /* SET REASON CODE IN RCA        */ 00081000
         L     @12,RCAPTR(,PVTPTR)                                 0124 00082000
         MVI   RCARCRD(@12),X'01'                                  0124 00083000
*             GEN(ABEND X'C0D',,,SYSTEM) ;/* THE RSM FRR IS GIVEN  0125 00084000
*                                      CONTROL TO RECORD THE ERROR 0125 00085000
*                                      BEFORE CONTINUING             */ 00086000
         ABEND X'C0D',,,SYSTEM                                          00087000
*           END;                                                   0126 00088000
*         ELSE                                                     0127 00089000
*           ;                       /* RETURN CODE 4 MEANS USER ERROR*/ 00090000
@RF00122 DS    0H                                                  0128 00091000
*         GOTO LBERROR;             /* GO TO ERROR EXIT PROCESSING   */ 00092000
         B     LBERROR                                             0128 00093000
*       END;                                                       0129 00094000
*     ELSE                                                         0130 00095000
*       ;                           /* RETURN CODE WAS ZERO, CONTINUE*/ 00096000
@RF00120 DS    0H                                                  0131 00097000
*     PTEPTR=RLPARMR0;              /* PLACE PGTE AND                */ 00098000
         LR    PTEPTR,RLPARMR0                                     0131 00099000
*     XPTEPTR=RLPARMRG;             /* XPTE ADDRESSES IN BASE REGS   */ 00100000
         LR    XPTEPTR,RLPARMRG                                    0132 00101000
*     IF RELSVBN>=PVTLCSA THEN      /* IS PAGE IN COMMON AREA        */ 00102000
         CLC   RELSVBN(2,CIWAPTR),PVTLCSA(PVTPTR)                  0133 00103000
         BL    @RF00133                                            0133 00104000
*       DO;                         /* IF CURRENT PAGE IN COMMON   0134 00105000
*                                      AREA, CHECK FOR A QSA PAGE    */ 00106000
*         IF RELSVBN>=PVTLQSA&RELSVBN<PVTHQSA THEN/* IS PAGE A QSA 0135 00107000
*                                      PAGE                          */ 00108000
         CLC   RELSVBN(2,CIWAPTR),PVTLQSA(PVTPTR)                  0135 00109000
         BL    @RF00135                                            0135 00110000
         CLC   RELSVBN(2,CIWAPTR),PVTHQSA(PVTPTR)                  0135 00111000
         BNL   @RF00135                                            0135 00112000
*           DO;                                                    0136 00113000
*             RLNUMBYT=CIWWRK1;     /* IF PAGE A QSA PAGE, RESTORE 0137 00114000
*                                      LOOP CONTROL VALUE            */ 00115000
         L     RLNUMBYT,CIWWRK1(,CIWAPTR)                          0137 00116000
*             GOTO LBNXTPAG;        /* SKIP ALL PROCESSING FOR PAGE  */ 00117000
         B     LBNXTPAG                                            0138 00118000
*           END;                                                   0139 00119000
*         ELSE                                                     0140 00120000
*           ;                       /* PAGE NOT IN QSA, CONTINUE     */ 00121000
@RF00135 DS    0H                                                  0141 00122000
*       END;                        /* END COMMON AREA PROCESSING    */ 00123000
*     ELSE                          /* CURRENT PAGE IN PRIVATE AREA  */ 00124000
*       DO;                                                        0142 00125000
         B     @RC00133                                            0142 00126000
@RF00133 DS    0H                                                  0143 00127000
*         IF CIWKEY^=MZERO THEN     /* FOR PRIVATE AREA PAGES, IF THE   00128000
*                                      CALLERS KEY IS NOT ZERO, THE     00129000
*                                      KEY OF THE PAGE MUST EQUAL  0143 00130000
*                                      THAT OF THE REQUESTOR         */ 00131000
         CLI   CIWKEY(CIWAPTR),0                                   0143 00132000
         BE    @RF00143                                            0143 00133000
*           DO;                                                    0144 00134000
*             IF PGTPVM='0'B THEN   /* IF THE PAGE IS IN STORAGE, THE   00135000
*                                      KEY OF THE REAL FRAME IS    0145 00136000
*                                      TESTED. ONLY THE KEY OF THE 0145 00137000
*                                      FIRST 2K BLOCK OF THE FRAME IS   00138000
*                                      TESTED BECAUSE RSM ALWAYS SETS   00139000
*                                      THE KEYS FOR BOTH 2K PARTS OF    00140000
*                                      THE FRAME TO THE SAME VALUE.     00141000
*                                      NOTE THE REAL ADDRESS IN    0145 00142000
*                                      RLWRKRG7 WAS PLACED THERE BY     00143000
*                                      THE LRA INSTRUCTION ABOVE.    */ 00144000
         TM    PGTPVM(PTEPTR),B'00001000'                          0145 00145000
         BNZ   @RF00145                                            0145 00146000
*               DO;                                                0146 00147000
*                 RESPECIFY                                        0147 00148000
*                  (RLWRKRG7,                                      0147 00149000
*                   RLWRKRG8) RESTRICTED;                          0147 00150000
*                 RLWRKRG7=PGTRSA;  /* GET RBN TO FORM REAL ADDR     */ 00151000
         SLR   RLWRKRG7,RLWRKRG7                                   0148 00152000
         ICM   RLWRKRG7,3,PGTRSA(PTEPTR)                           0148 00153000
*                 GEN SETS(RLWRKRG8,RLWRKRG7);                     0149 00154000
          SRL   RLWRKRG7,4        REMOVE LOW 4 BITS                     00155000
          SLL   RLWRKRG7,12       CREATE 24 BIT REAL ADDR               00156000
          ISK   RLWRKRG8,RLWRKRG7 GET KEY OF REAL STORAGE               00157000
*                 RLWRKRG8=RLWRKRG8&RLISOKEY;/* ISOLATE KEY, ZEROING    00158000
*                                      FETCH PROTECT AND R/C BITS FOR   00159000
*                                      COMPARE                       */ 00160000
         N     RLWRKRG8,RLISOKEY                                   0150 00161000
*                 IF RLWRKRG8=CIWKEY THEN/* ARE KEYS EQUAL           */ 00162000
         SLR   @12,@12                                             0151 00163000
         IC    @12,CIWKEY(,CIWAPTR)                                0151 00164000
         CR    RLWRKRG8,@12                                        0151 00165000
         BE    @RT00151                                            0151 00166000
*                   GOTO LBCALPAG;  /* IF THE KEYS MATCH, CONTINUE   */ 00167000
*                 ELSE                                             0153 00168000
*                   ;               /* OTHERWISE, ERROR CONDITION    */ 00169000
*                 RESPECIFY                                        0154 00170000
*                  (RLWRKRG7,                                      0154 00171000
*                   RLWRKRG8) UNRESTRICTED;                        0154 00172000
*               END;                /* END ISK TEST                  */ 00173000
*             ELSE                  /* TEST KEY IN XPTE FOR PAGES NOT   00174000
*                                      IN REAL STORAGE               */ 00175000
*               DO;                                                0156 00176000
         B     @RC00145                                            0156 00177000
@RF00145 DS    0H                                                  0157 00178000
*                 IF CIWKEY=(XPTPROT&RLISOKY4) THEN/* ARE KEYS EQUAL */ 00179000
         SLR   @12,@12                                             0157 00180000
         IC    @12,XPTPROT(,XPTEPTR)                               0157 00181000
         SLR   @11,@11                                             0157 00182000
         IC    @11,RLISOKY4                                        0157 00183000
         NR    @12,@11                                             0157 00184000
         SLR   @11,@11                                             0157 00185000
         IC    @11,CIWKEY(,CIWAPTR)                                0157 00186000
         CR    @12,@11                                             0157 00187000
         BE    @RT00157                                            0157 00188000
*                   GOTO LBCALPAG;  /* IF THE KEYS MATCH, CONTINUE   */ 00189000
*                 ELSE                                             0159 00190000
*                   ;               /* OTHERWISE, ERROR CONDITION    */ 00191000
*               END;                /* END XPTE KEY TEST             */ 00192000
*LBERROR:                           /* ERROR CONDITION PROCESSING    */ 00193000
*             CIWRETC=MERRORCD;     /* ERROR CODE IN THE CIWA        */ 00194000
@RC00145 DS    0H                                                  0161 00195000
LBERROR  MVI   CIWRETC(CIWAPTR),X'04'                              0161 00196000
*             RLNUMBYT=MPGSIZE;     /* FORCE EXIT FROM THIS LOOP,  0162 00197000
*                                      PROCESSING WILL BE TERMINATED */ 00198000
         L     RLNUMBYT,@CF02411                                   0162 00199000
*             GOTO LBNXTPAG;                                       0163 00200000
         B     LBNXTPAG                                            0163 00201000
*           END;                    /* END NON KEY ZERO PROCESSING   */ 00202000
*         ELSE                                                     0165 00203000
*           ;                       /* CALLERS KEY IS ZERO, CONTINUE */ 00204000
@RF00143 DS    0H                                                  0166 00205000
*       END;                        /* END PRIVATE AREA PROCESSING   */ 00206000
*LBCALPAG:                                                         0167 00207000
*     CALL SUBPAGE;                 /* CALL THE PAGE PROCESSOR SUBRTN*/ 00208000
@RC00133 DS    0H                                                  0167 00209000
LBCALPAG BAL   @14,SUBPAGE                                         0167 00210000
*     RESPECIFY                                                    0168 00211000
*       RLRETCOD RESTRICTED;                                       0168 00212000
*     IF RLRETCOD^=MZERO THEN       /* CHECK SUBPAGE RETURN CODE     */ 00213000
         LTR   RLRETCOD,RLRETCOD                                   0169 00214000
         BZ    @RF00169                                            0169 00215000
*       DO;                         /* THE PAGE PROCESSOR HAS FOUND     00216000
*                                      AN ERROR, PROCESSING MUST BE     00217000
*                                      TERMINATED                    */ 00218000
*         CIWRETC=MERRORCD;         /* SET ERROR RETURN CODE         */ 00219000
         MVI   CIWRETC(CIWAPTR),X'04'                              0171 00220000
*         RLNUMBYT=MPGSIZE;         /* FORCE EXIT FROM LOOP          */ 00221000
         L     RLNUMBYT,@CF02411                                   0172 00222000
*       END;                                                       0173 00223000
*     ELSE                          /* SUBPAGE RETURN CODE IS ZERO   */ 00224000
*       DO;                         /* NO ERROR WAS FOUND            */ 00225000
         B     @RC00169                                            0174 00226000
@RF00169 DS    0H                                                  0175 00227000
*         RLNUMBYT=CIWWRK1;         /* RESTORE LOOP CONTROL VALUE    */ 00228000
         L     RLNUMBYT,CIWWRK1(,CIWAPTR)                          0175 00229000
*       END;                                                       0176 00230000
*LBNXTPAG:                                                         0177 00231000
*     RLTESTAD=RELSVSA+MPGSIZE;     /* ADJUST LOOP VALUES            */ 00232000
@RC00169 DS    0H                                                  0177 00233000
LBNXTPAG L     @14,@CF02411                                        0177 00234000
         L     RLTESTAD,RELSVSA(,CIWAPTR)                          0177 00235000
         ALR   RLTESTAD,@14                                        0177 00236000
*     RLNUMBYT=RLNUMBYT-MPGSIZE;    /* FOR NEXT PAGE                 */ 00237000
         SLR   RLNUMBYT,@14                                        0178 00238000
*   END;                            /* END RLNUMBYT DO WHILE         */ 00239000
@DE00115 C     RLNUMBYT,@CF02411                                   0179 00240000
         BNL   @DL00115                                            0179 00241000
*                                                                  0180 00242000
*   /*****************************************************************/ 00243000
*   /*                                                               */ 00244000
*   /* THIS IS THE MAIN PART OF THE OUTER LOOP WHERE ADDITIONAL VSL  */ 00245000
*   /* ENTRIES ARE SET UP TO BE PROCESSED IF THEY EXIST. ONCE ALL    */ 00246000
*   /* EXISTING VSL ENTRIES HAVE BEEN PROCESSED, OR IF AN ERROR      */ 00247000
*   /* EXISTS, EXIT IS MADE TO PSI.                                  */ 00248000
*   /*                                                               */ 00249000
*   /*****************************************************************/ 00250000
*                                                                  0180 00251000
*   IF CIWRETC=MZERO THEN                                          0180 00252000
         CLI   CIWRETC(CIWAPTR),0                                  0180 00253000
         BNE   @RF00180                                            0180 00254000
*     DO;                           /* IF AN ERROR WAS NOT DETECTED     00255000
*                                      IN THE ABOVE LOOP, GET THE  0181 00256000
*                                      NEXT VSL FOR LIST REQUESTS, 0181 00257000
*                                      EXIT FOR REGISTER REQUESTS    */ 00258000
*       IF CIWINVSL^=MZERO THEN     /* IS REQUEST LIST FORM          */ 00259000
         L     @14,CIWINVSL(,CIWAPTR)                              0182 00260000
         LTR   @14,@14                                             0182 00261000
         BZ    @RF00182                                            0182 00262000
*         DO;                       /* CALL PSI SUBROUTINE TO GET  0183 00263000
*                                      NEXT VSL ENTRY OF A LIST    0183 00264000
*                                      REQUEST                       */ 00265000
*           CALL NEXTVSL;           /* SEE ENTRY DCL FOR INTERFACE   */ 00266000
         L     @15,@CV02455                                        0184 00267000
         BALR  @14,@15                                             0184 00268000
*           IF RLRETCOD=MZERO THEN  /* MORE VSL ENTRIES TO PROCESS   */ 00269000
         LTR   RLRETCOD,RLRETCOD                                   0185 00270000
         BZ    @RT00185                                            0185 00271000
*             GOTO LBVSLOOP;        /* IF MORE VSL ENTRIES EXIST TO     00272000
*                                      BE PROCESSED, PROCESS THEM    */ 00273000
*           ELSE                    /* RETURN CODE NOT ZERO          */ 00274000
*             DO;                   /* OTHERWISE, CHECK FOR ERRORS   */ 00275000
*               IF RLRETCOD^=LISTDONE THEN/* WAS ERROR DETECTED      */ 00276000
         C     RLRETCOD,@CF00039                                   0188 00277000
         BE    @RF00188                                            0188 00278000
*                 DO;               /* AN ERROR WAS DETECTED         */ 00279000
*                   CIWRETC=MPARMERR;/* ERROR CODE IN CIWA           */ 00280000
         MVI   CIWRETC(CIWAPTR),X'10'                              0190 00281000
*                 END;                                             0191 00282000
*               ELSE                                               0192 00283000
*                 ;                 /* NO ERROR, REQUEST IS COMPLETE */ 00284000
@RF00188 DS    0H                                                  0193 00285000
*             END;                                                 0193 00286000
*         END;                                                     0194 00287000
*       ELSE                                                       0195 00288000
*         ;                         /* REQUEST IS REGISTER FORM      */ 00289000
@RF00182 DS    0H                                                  0196 00290000
*     END;                          /* END CONTINUATION PROCESSING   */ 00291000
*   ELSE                            /* AN ERROR WAS DETECTED         */ 00292000
*     DO;                           /* IF AN ERROR WAS DETECTED IN 0197 00293000
*                                      PAGE PROCESSING, THE ERROR  0197 00294000
*                                      FLAG MUST BE SET IN A VSL   0197 00295000
*                                      ENTRY FOR LIST FORM ENTRIES   */ 00296000
         B     @RC00180                                            0197 00297000
@RF00180 DS    0H                                                  0198 00298000
*       IF CIWINVSL^=MZERO THEN     /* IS REQUEST LIST FORM          */ 00299000
         L     @14,CIWINVSL(,CIWAPTR)                              0198 00300000
         LTR   @14,@14                                             0198 00301000
         BZ    @RF00198                                            0198 00302000
*         DO;                       /* YES                           */ 00303000
*           CIWIERR='1'B;           /* SET ERROR FLAG IN CIWA COPY   */ 00304000
         OI    CIWIERR(CIWAPTR),B'00010000'                        0200 00305000
*           VSL=CIWVSL;             /* STORE CIWA COPY OF VSL OVER 0201 00306000
*                                      USER SUPPLIED COPY            */ 00307000
         L     @14,VSLPTR(,CIWAPTR)                                0201 00308000
         MVC   VSL(8,@14),CIWVSL(CIWAPTR)                          0201 00309000
*         END;                                                     0202 00310000
*       ELSE                                                       0203 00311000
*         ;                         /* REQUEST IS REGISTER FORM      */ 00312000
@RF00198 DS    0H                                                  0204 00313000
*     END;                                                         0204 00314000
*   RCARELS='0'B;                   /* RESET THE RCA ENTRY POINT FLAG*/ 00315000
@RC00180 L     @14,RCAPTR(,PVTPTR)                                 0205 00316000
         NI    RCARELS(@14),B'11111101'                            0205 00317000
*   RLRETREG=CIWRETRN;              /* RESTORE RETURN ADDRESS TO PSI */ 00318000
         L     RLRETREG,CIWRETRN(,CIWAPTR)                         0206 00319000
*   RESPECIFY                                                      0207 00320000
*     RLRETCOD UNRESTRICTED;                                       0207 00321000
*   GEN(BR    RLRETREG) ;           /* RETURN TO PSI                 */ 00322000
         BR    RLRETREG                                                 00323000
*                                                                  0209 00324000
*   /*****************************************************************/ 00325000
*   /*                                                               */ 00326000
*   /* THIS IS THE SPECIAL FREEMAIN RELEASE ENTRY POINT. CONTROL IS  */ 00327000
*   /* RECEIVED ONLY FROM VSM WITH THE SALLOC LOCK HELD. THE         */ 00328000
*   /* OWNERSHIP OF THE LOCAL LOCK IS UNCERTAIN. THIS ENTRY SAVES    */ 00329000
*   /* REGISTERS, ESTABLISHES THE RECOVERY ENVIRONMENT, AND          */ 00330000
*   /* INITIALIZES THE STANDARD RSM REGISTERS. THIS SEGMENT ALSO     */ 00331000
*   /* PREPARES THE INPUT ADDRESSES IN REGISTERS 0 AND 1 FOR THE LOOP*/ 00332000
*   /* IN THE SECOND SEGMENT.                                        */ 00333000
*   /*                                                               */ 00334000
*   /*****************************************************************/ 00335000
*                                                                  0209 00336000
*IEAVRELV:                                                         0209 00337000
*   ENTRY;                                                         0209 00338000
*   GENERATE REFS(PVTPTR) SETS(PVTWSA5,RLBASERG,RLWRKRG7,PVTPRCA); 0210 00339000
IEAVRELV STM   RLOWADDR,RLRETREG,PVTWSA5(PVTPTR)                        00340000
*                                   SAVE CALLERS REGISTERS              00341000
LBRELVBS BALR  RLBASERG,0           SET UP THE BASE REG FOR ENTRY       00342000
         LA    RLWRKRG7,LBRELVBS-LBSETBAS  ADJUST BASE REG TO VALUE     00343000
         SR    RLBASERG,RLWRKRG7           USED THROUGHOUT MODULE       00344000
         SETFRR A,FRRAD=PVTPRCV(PVTPTR),PARMAD=PVTPRCA(,PVTPTR),      **00345000
               WRKREGS=(RLWRKRG7,RLWRKRG8) ESTABLISH RSM FRR            00346000
*   RCACSECT=RCACPGRL;              /* SET MODULE CSECT ID           */ 00347000
         L     @12,RCAPTR(,PVTPTR)                                 0211 00348000
         MVI   RCACSECT(@12),X'07'                                 0211 00349000
*   RCARELV='1'B;                   /* SET ENTRY POINT FLAG          */ 00350000
         OI    RCARELV(@12),B'01000000'                            0212 00351000
*   RSMHDPTR=ASCBRSM;               /* PLACE RSM HEADER ADDRESS IN 0213 00352000
*                                      STANDARD RSM REGISTER         */ 00353000
         L     RSMHDPTR,ASCBRSM(,ASCBPTR)                          0213 00354000
*   WSA5FREE=MZERO;                 /* INITIALIZE THE SAVE AREA FOR     00355000
*                                      FREE PGT/XPT SPACE            */ 00356000
         SLR   @12,@12                                             0214 00357000
         ST    @12,WSA5FREE(,PVTPTR)                               0214 00358000
*   RLTESTAD=RLOWADDR+MPSZMIN1;     /* REDUCE THE INPUT LOW ADDR TO     00359000
*                                      THE NEXT HIGHER PAGE BOUNDARY */ 00360000
         LA    RLTESTAD,4095                                       0215 00361000
         ALR   RLTESTAD,RLOWADDR                                   0215 00362000
*   RLTESTAD=RLTESTAD&MPGBDYMK;                                    0216 00363000
         N     RLTESTAD,@CF02426                                   0216 00364000
*   RLNUMBYT=RLHIADDR-RLTESTAD;     /* SET UP LOOP CONTROL VALUE     */ 00365000
         LR    RLNUMBYT,RLHIADDR                                   0217 00366000
         SLR   RLNUMBYT,RLTESTAD                                   0217 00367000
*   WSA5VSA=RLTESTAD;               /* STORE ADDR FIRST PAGE IN WSA  */ 00368000
         ST    RLTESTAD,WSA5VSA(,PVTPTR)                           0218 00369000
*   RESPECIFY                                                      0219 00370000
*     RLRETCOD RESTRICTED;          /* PROTECT REG 15 FROM COMPILER  */ 00371000
*   RLRETCOD=MZERO;                 /* INITIALIZE RETURN CODE TO ZERO   00372000
*                                      BEFORE BEGINNING DO WHILE LOOP*/ 00373000
         SLR   RLRETCOD,RLRETCOD                                   0220 00374000
*                                                                  0221 00375000
*   /*****************************************************************/ 00376000
*   /*                                                               */ 00377000
*   /* A LOOP IS NOW EXECUTED THAT IS CONTROLLED BY THE NUMBER OF    */ 00378000
*   /* BYTES THERE ARE TO RELEASE. THE FREEMAIN PAGE PROCESSOR IS    */ 00379000
*   /* CALLED AS A SUBROUTINE. THIS SEGMENT ALSO MAINTAINS A CHAIN OF*/ 00380000
*   /* AREAS TO BE FREEMAINED THAT WILL EVENTUALLY BE RETURNED TO    */ 00381000
*   /* VSM.                                                          */ 00382000
*   /*                                                               */ 00383000
*   /*****************************************************************/ 00384000
*                                                                  0221 00385000
*   DO WHILE(RLNUMBYT>=MPGSIZE);    /* THE LOOP IS EXITED WHEN LESS     00386000
*                                      THAN 4096 BYTES REMAIN TO BE     00387000
*                                      FREEMAINED.                   */ 00388000
         B     @DE00221                                            0221 00389000
@DL00221 DS    0H                                                  0222 00390000
*     IF(WSA5VBN>=PVTLQSA&WSA5VBN<PVTHQSA) THEN/* IF THE PAGE IS IN     00391000
*                                      THE QUICK START AREA, A     0222 00392000
*                                      FREEMAIN ERROR HAS OCCURRED.  */ 00393000
         CLC   WSA5VBN(2,PVTPTR),PVTLQSA(PVTPTR)                   0222 00394000
         BL    @RF00222                                            0222 00395000
         CLC   WSA5VBN(2,PVTPTR),PVTHQSA(PVTPTR)                   0222 00396000
         BNL   @RF00222                                            0222 00397000
*       DO;                                                        0223 00398000
*         RLNUMBYT=MPGSIZE;         /* FORCE EXIT FROM THE LOOP      */ 00399000
         L     RLNUMBYT,@CF02411                                   0224 00400000
*         RLRETCOD=MERRORCD;        /* SET THE RETURN CODE = 4       */ 00401000
         LA    RLRETCOD,4                                          0225 00402000
*       END;                                                       0226 00403000
*     ELSE                          /* OTHERWISE, CALL THE FREEMAIN     00404000
*                                      PAGE PROCESSOR                */ 00405000
*       DO;                                                        0227 00406000
         B     @RC00222                                            0227 00407000
@RF00222 DS    0H                                                  0228 00408000
*         RLPARMRG=WSA5VSA;         /* SET UP THE PARAMETER REG      */ 00409000
         L     RLPARMRG,WSA5VSA(,PVTPTR)                           0228 00410000
*         WSA5NUM=RLNUMBYT;         /* SAVE NUMBER OF BYTES TO RLSE  */ 00411000
         ST    RLNUMBYT,WSA5NUM(,PVTPTR)                           0229 00412000
*         CALL SUBFMPAG;            /* CALL FREEMAIN PAGE PROCESSOR  */ 00413000
         BAL   @14,SUBFMPAG                                        0230 00414000
*         IF RLRETCOD=MDFRLSCD THEN /* WAS RELEASE DEFERED           */ 00415000
         C     RLRETCOD,@CF00039                                   0231 00416000
         BNE   @RF00231                                            0231 00417000
*           DO;                     /* A DEFERED RELEASE CONDITION 0232 00418000
*                                      HAS BEEN DETECTED.            */ 00419000
*             WSA5DRLS=WSA5VSA;     /* SAVE THE ADDRESS OF THE     0233 00420000
*                                      DEFERED RELEASE PAGE          */ 00421000
         L     @14,WSA5VSA(,PVTPTR)                                0233 00422000
         ST    @14,WSA5DRLS(,PVTPTR)                               0233 00423000
*             RLNUMBYT=MPGSIZE;     /* FORCE EXIT FROM LOOP          */ 00424000
         L     RLNUMBYT,@CF02411                                   0234 00425000
*           END;                                                   0235 00426000
*         ELSE                      /* NO DEFERED RELEASE            */ 00427000
*           DO;                                                    0236 00428000
         B     @RC00231                                            0236 00429000
@RF00231 DS    0H                                                  0237 00430000
*             IF RLRETCOD^=MGOODCD THEN/* WAS ERROR DETECTED         */ 00431000
         LTR   RLRETCOD,RLRETCOD                                   0237 00432000
         BZ    @RF00237                                            0237 00433000
*               DO;                 /* AN ERROR WAS DETECTED         */ 00434000
*                 RLNUMBYT=MPGSIZE; /* FORCE EXIT FROM LOOP          */ 00435000
         L     RLNUMBYT,@CF02411                                   0239 00436000
*               END;                                               0240 00437000
*             ELSE                  /* OTHERWISE, A SUCCESSFUL     0241 00438000
*                                      RELEASE WAS EXECUTED          */ 00439000
*               DO;                                                0241 00440000
         B     @RC00237                                            0241 00441000
@RF00237 DS    0H                                                  0242 00442000
*                 IF RLRETSPC^=MZERO THEN/* WAS PGT/XPT SPACE      0242 00443000
*                                      RETURNED                      */ 00444000
         SLR   @14,@14                                             0242 00445000
         CR    RLRETSPC,@14                                        0242 00446000
         BE    @RF00242                                            0242 00447000
*                   DO;             /* PGT/XPT SPACE HAS BEEN      0243 00448000
*                                      RETURNED TO BE FREEMAINED. THE   00449000
*                                      ADDRESS OF THIS AREA IS SAVED    00450000
*                                      AS EITHER THE FIRST AREA IN 0243 00451000
*                                      THE REGISTER 2 SAVEAREA OF THE   00452000
*                                      WSA OR ADDED TO A CHAIN OF  0243 00453000
*                                      AREAS.                        */ 00454000
*                     IF WSA5FREE=MZERO THEN/* IS THIS FIRST AREA    */ 00455000
         C     @14,WSA5FREE(,PVTPTR)                               0244 00456000
         BNE   @RF00244                                            0244 00457000
*                       DO;         /* FIRST AREA TO BE SAVED        */ 00458000
*                         WSA5FREE=RLRETSPC;/* SAVE AREA ADDR        */ 00459000
         ST    RLRETSPC,WSA5FREE(,PVTPTR)                          0246 00460000
*                         FRECHAIN=MZERO;/* FIRST WORD OF AREA=0     */ 00461000
         L     @12,WSA5FREE(,PVTPTR)                               0247 00462000
         ST    @14,FRECHAIN(,@12)                                  0247 00463000
*                       END;                                       0248 00464000
*                     ELSE          /* NOT FIRST AREA RETURNED       */ 00465000
*                       DO;         /* ATTACH AREA TO END OF EXISTING   00466000
*                                      CHAIN                         */ 00467000
         B     @RC00244                                            0249 00468000
@RF00244 DS    0H                                                  0250 00469000
*                         RESPECIFY                                0250 00470000
*                           RLWRKRG7 RESTRICTED;                   0250 00471000
*                         RLWRKRG7=WSA5FREE;/* GET ADDR 1ST AREA     */ 00472000
         L     RLWRKRG7,WSA5FREE(,PVTPTR)                          0251 00473000
*                         RESPECIFY                                0252 00474000
*                           FREESPAC BASED(RLWRKRG7);              0252 00475000
*                         DO WHILE(FRECHAIN^=MZERO);/* THIS DO WHILE    00476000
*                                      LOOP GOES DOWN THE AREA CHAIN    00477000
*                                      UNTIL THE LAST AREA IS FOUND  */ 00478000
         B     @DE00253                                            0253 00479000
@DL00253 DS    0H                                                  0254 00480000
*                           RLWRKRG7=FRECHAIN;                     0254 00481000
         L     RLWRKRG7,FRECHAIN(,RLWRKRG7)                        0254 00482000
*                         END;      /* END FRECHAIN DO WHILE         */ 00483000
@DE00253 SLR   @14,@14                                             0255 00484000
         C     @14,FRECHAIN(,RLWRKRG7)                             0255 00485000
         BNE   @DL00253                                            0255 00486000
*                         FRECHAIN=RLRETSPC;/* PLACE NEW AREA ADDR IN   00487000
*                                      LAST AREA                     */ 00488000
         ST    RLRETSPC,FRECHAIN(,RLWRKRG7)                        0256 00489000
*                         RLWRKRG7=RLRETSPC;                       0257 00490000
         LR    RLWRKRG7,RLRETSPC                                   0257 00491000
*                         FRECHAIN=MZERO;/* FIRST WORD OF NEW AREA=0 */ 00492000
         ST    @14,FRECHAIN(,RLWRKRG7)                             0258 00493000
*                         RESPECIFY                                0259 00494000
*                           RLWRKRG7 UNRESTRICTED;                 0259 00495000
*                       END;        /* END ADD TO CHAIN GROUP        */ 00496000
*                   END;            /* END SAVE PGT/XPTSPACE GROUP   */ 00497000
*                 ELSE                                             0262 00498000
*                   ;               /* IT MUST BE ASSURED THAT REG 15   00499000
*                                      CONTAINS A ZERO AS EXIT FROM     00500000
*                                      THE LOOP MAY OCCUR            */ 00501000
@RF00242 DS    0H                                                  0263 00502000
*                 RLNUMBYT=WSA5NUM; /* RESTORE CORRECT NUMBER OF   0263 00503000
*                                      BYTES TO RELEASE.             */ 00504000
@RC00242 L     RLNUMBYT,WSA5NUM(,PVTPTR)                           0263 00505000
*               END;                /* END SUCCESSFUL PROCESSING     */ 00506000
*           END;                    /* END NON DEFERED RELEASE CODE  */ 00507000
@RC00237 DS    0H                                                  0266 00508000
*       END;                        /* END PAGE PROCESSOR GROUP      */ 00509000
@RC00231 DS    0H                                                  0267 00510000
*     WSA5VSA=WSA5VSA+MPGSIZE;      /* TESTADDR IS NEXT PAGE IN SEQ. */ 00511000
@RC00222 L     @14,@CF02411                                        0267 00512000
         L     @12,WSA5VSA(,PVTPTR)                                0267 00513000
         ALR   @12,@14                                             0267 00514000
         ST    @12,WSA5VSA(,PVTPTR)                                0267 00515000
*     RLNUMBYT=RLNUMBYT-MPGSIZE;    /* REDUCE NUMBER OF BYTES TO FREE*/ 00516000
         SLR   RLNUMBYT,@14                                        0268 00517000
*     RESPECIFY                                                    0269 00518000
*       RLRETCOD UNRESTRICTED;                                     0269 00519000
*   END;                            /* END THE DO WHILE LOOP         */ 00520000
@DE00221 C     RLNUMBYT,@CF02411                                   0270 00521000
         BNL   @DL00221                                            0270 00522000
*                                                                  0271 00523000
*   /*****************************************************************/ 00524000
*   /*                                                               */ 00525000
*   /* THIS SEGMENT GETS CONTROL UPON EXIT FROM THE DO WHILE LOOP.   */ 00526000
*   /* REGISTER 15 IS ALREADY SET WITH THE PROPER RETURN CODE. ALL   */ 00527000
*   /* THAT MUST BE DONE IS TO CANCEL THE RSM FRR AND RESTORE        */ 00528000
*   /* REGISTERS. IT SHOULD BE NOTED THAT WITH THE RESTORING OF      */ 00529000
*   /* REGISTERS, THE PROPER OUTPUT PARAMETERS ARE ALSO SET          */ 00530000
*   /* AUTOMATICALLY SINCE THE CORRECT VALUES HAVE BEEN PLACED IN THE*/ 00531000
*   /* REGISTER SAVE AREA BY THE LAST SEGMENT.                       */ 00532000
*   /*                                                               */ 00533000
*   /*****************************************************************/ 00534000
*                                                                  0271 00535000
*   GENERATE REFS(PVTWSA5);                                        0271 00536000
         SETFRR D,WRKREGS=(RLWRKRG7,RLWRKRG8)                           00537000
         LM    RLOWADDR,RLRETREG,PVTWSA5(PVTPTR)                        00538000
*   GOTO RLRETREG;                  /* RETURN TO THE CALLER          */ 00539000
         BR    RLRETREG                                            0272 00540000
*                                                                  0273 00541000
*   /*****************************************************************/ 00542000
*   /*                                                               */ 00543000
*   /* THE DEFERED RELEASE ENTRY POINT RECEIVES CONTROL FROM ANY RSM */ 00544000
*   /* MODULE WHO DETECTS THE DEFERED RELEASE CONDITION WHEN A PFTE  */ 00545000
*   /* FIX COUNT GOES TO ZERO. INPUT IS A VIRTUAL ADDRESS IN REGISTER*/ 00546000
*   /* 1 AND THE STANDARD RSM VALUES IN REGISTERS 2 AND 3. THE       */ 00547000
*   /* CALLERS REGISTERS ARE SAVED AND SUBFMPAG CALLED TO COMPLETE   */ 00548000
*   /* THE DEFERED RELEASE. UPON RETURN, IF A PGT/XPT IS TO BE FREED,*/ 00549000
*   /* IT IS FREEMAINED AND RETURN MADE TO THE CALLER WITH A ZERO    */ 00550000
*   /* ALWAYS IN REGISTER 0.                                         */ 00551000
*   /*                                                               */ 00552000
*   /*****************************************************************/ 00553000
*                                                                  0273 00554000
*IEAVRELF:                                                         0273 00555000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0273 00556000
IEAVRELF DS    0H                                                  0274 00557000
*   GENERATE SETS(PVTWSA6,RLBASERG,RLWRKRG7);                      0274 00558000
         STM   RLPARMRG,RLRETREG,PVTWSA6(PVTPTR) SAVE REGISTERS         00559000
LBRELFBS BALR  RLBASERG,0           SET UP THE BASE REG FOR ENTRY       00560000
         LA    RLWRKRG7,LBRELFBS-LBSETBAS ADJUST BASE REG TO VALUE      00561000
         SR    RLBASERG,RLWRKRG7          USED THROUGHOUT MODULE        00562000
*   RCACSECT=RCACPGRL;              /* INIT RCA WITH CSECT ID        */ 00563000
         L     @12,RCAPTR(,PVTPTR)                                 0275 00564000
         MVI   RCACSECT(@12),X'07'                                 0275 00565000
*   RCARELF='1'B;                   /* ENTRY POINT FLAG SET          */ 00566000
         OI    RCARELF(@12),B'10000000'                            0276 00567000
*   CALL SUBFMPAG;                  /* THE FREEMAIN PAGE PROCESSOR IS   00568000
*                                      CALLED TO COMPLETE THE DEFERED   00569000
*                                      RELEASE                       */ 00570000
         BAL   @14,SUBFMPAG                                        0277 00571000
*   RESPECIFY                                                      0278 00572000
*     RLRETCOD RESTRICTED;                                         0278 00573000
*   IF RLRETCOD=0 THEN              /* IF THE RETURN CODE IS ZERO, 0279 00574000
*                                      FREEMAIN ANY SPACE POINTED TO    00575000
*                                      BY REGISTER 0                 */ 00576000
         SLR   @14,@14                                             0279 00577000
         CR    RLRETCOD,@14                                        0279 00578000
         BNE   @RF00279                                            0279 00579000
*     DO;                                                          0280 00580000
*       IF RLRETSPC^=0 THEN         /* IS THERE SPACE TO FREE        */ 00581000
         CR    RLRETSPC,@14                                        0281 00582000
         BE    @RF00281                                            0281 00583000
*         DO;                                                      0282 00584000
*           RESPECIFY                                              0283 00585000
*            (RLFMRCAS,                                            0283 00586000
*             RLFMPVTS,                                            0283 00587000
*             RLFMTCB,                                             0283 00588000
*             RLWRKRG7) RESTRICTED;                                0283 00589000
*           RLPARMRG=RLRETSPC;      /* XFER REG 0 TO REG 1           */ 00590000
         LR    RLPARMRG,RLRETSPC                                   0284 00591000
*           RLPARMR0=(LENGTH(PGTPTE)+LENGTH(XPTE))*16;/* CALCULATE 0285 00592000
*                                      LENGTH OF PGT/XPT SPACE TO BE    00593000
*                                      FREEMAINED                    */ 00594000
         LA    RLPARMR0,224                                        0285 00595000
*           RLFMRCAS=PVTPRCA;       /* SAVE RCA ADDRESS AS FREEMAIN     00596000
*                                      MAY REENTER RSM               */ 00597000
         L     RLFMRCAS,PVTPRCA(,PVTPTR)                           0286 00598000
*           RLFMPVTS=PVTPTR;        /* SAVE PVT ADDR AS FREEMAIN USES   00599000
*                                      REG 3                         */ 00600000
         LR    RLFMPVTS,PVTPTR                                     0287 00601000
*           RLFMTCB=0;              /* SET FREEMAIN TCB ADDR TO ZERO */ 00602000
         SLR   RLFMTCB,RLFMTCB                                     0288 00603000
*           RLWRKRG7=PSAAOLD;       /* SET ASCB ADDR IN REG 7        */ 00604000
         L     RLWRKRG7,PSAAOLD                                    0289 00605000
*           GENERATE REFS(RLPARMR0,RLPARMRG,RLFMTCB,RLWRKRG7) SETS(     00606000
*               PVTPTR,RLRETCOD);                                  0290 00607000
         FREEMAIN RC,LV=(0),A=(1),BRANCH=YES,SP=255,KEY=0,            **00608000
               RELATED=(PGTXPT,IEAVGM00('GET PGT/XPT SPACE'))           00609000
*           PVTPTR=RLFMPVTS;        /* RESTORE PVT ADDR REG          */ 00610000
         LR    PVTPTR,RLFMPVTS                                     0291 00611000
*           PVTPRCA=RLFMRCAS;       /* RESTORE RCA ADDR IN PVT       */ 00612000
         ST    RLFMRCAS,PVTPRCA(,PVTPTR)                           0292 00613000
*           RESPECIFY                                              0293 00614000
*            (RLFMRCAS,                                            0293 00615000
*             RLFMPVTS,                                            0293 00616000
*             RLFMTCB,                                             0293 00617000
*             RLWRKRG7) UNRESTRICTED;                              0293 00618000
*           IF RLRETCOD^=0 THEN     /* WAS FREEMAIN SUCCESSFUL       */ 00619000
         LTR   RLRETCOD,RLRETCOD                                   0294 00620000
         BZ    @RF00294                                            0294 00621000
*             DO;                   /* IF FREEMAIN NOT SUCCESSFUL, 0295 00622000
*                                      RECORD ERROR AND CONTINUE     */ 00623000
*               RCARCRD='03'X;      /* SET REASON CODE IN RCA        */ 00624000
         L     @14,RCAPTR(,PVTPTR)                                 0296 00625000
         MVI   RCARCRD(@14),X'03'                                  0296 00626000
*               GEN(ABEND X'C0D',,,SYSTEM) ;                       0297 00627000
         ABEND X'C0D',,,SYSTEM                                          00628000
*               RLRETCOD=0;         /* FORCE RETURN CODE TO ZERO     */ 00629000
         SLR   RLRETCOD,RLRETCOD                                   0298 00630000
*               RESPECIFY                                          0299 00631000
*                 RLRETCOD UNRESTRICTED;                           0299 00632000
*             END;                                                 0300 00633000
*           ELSE                                                   0301 00634000
*             ;                     /* FREEMAIN WAS SUCCESSFUL       */ 00635000
@RF00294 DS    0H                                                  0302 00636000
*         END;                                                     0302 00637000
*       ELSE                                                       0303 00638000
*         ;                         /* NO SPACE TO FREEMAIN          */ 00639000
@RF00281 DS    0H                                                  0304 00640000
*     END;                          /* END RETURN CODE 0 PROCESSING  */ 00641000
*   ELSE                                                           0305 00642000
*     ;                             /* DEFERED RELEASE WAS NOT     0305 00643000
*                                      COMPLETED                     */ 00644000
@RF00279 DS    0H                                                  0306 00645000
*   RLPARMR0=0;                     /* REG 0 ALWAYS SET TO ZERO FOR     00646000
*                                      THE RETURN TO THE CALLER.     */ 00647000
         SLR   RLPARMR0,RLPARMR0                                   0306 00648000
*   RCARELF='0'B;                   /* TURN OFF ENTRY POINT FLAG     */ 00649000
         L     @12,RCAPTR(,PVTPTR)                                 0307 00650000
         NI    RCARELF(@12),B'01111111'                            0307 00651000
*   GENERATE REFS(PVTWSA6);                                        0308 00652000
         LM    RLPARMRG,RLRETREG,PVTWSA6(PVTPTR)   RESTORE REGS         00653000
*   END;                            /* END IEAVRELF SUBPROCEDURE     */ 00654000
@EL00002 DS    0H                                                  0309 00655000
@EF00002 DS    0H                                                  0309 00656000
@ER00002 BR    @14                                                 0309 00657000
*                                                                  0310 00658000
*   /*****************************************************************/ 00659000
*   /*                                                               */ 00660000
*   /* THE FREEMAIN PAGE PROCESSOR RECEIVES CONTROL FROM THE IEAVRELV*/ 00661000
*   /* OR IEAVRELF ENTRY POINTS WITH THE STANDARD RSM POINTERS IN    */ 00662000
*   /* REGISTERS 2 AND 3. THIS SEGMENT SAVES THE CALLERS RETURN      */ 00663000
*   /* ADDRESS AND DOES PRELIMINARY SET UP AND ERROR CHECKING BEFORE */ 00664000
*   /* CONTROL GOES TO THE IN-STORAGE OR OUT-OF-STORAGE SEGMENTS.    */ 00665000
*   /*                                                               */ 00666000
*   /*****************************************************************/ 00667000
*                                                                  0310 00668000
*SUBFMPAG:                                                         0310 00669000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0310 00670000
SUBFMPAG DS    0H                                                  0311 00671000
*   WSA6RETS=RLRETREG;              /* SAVE RETURN ADDRESS           */ 00672000
         ST    RLRETREG,WSA6RETS(,PVTPTR)                          0311 00673000
*   WSA6VSA=RLPARMRG;               /* STORE INPUT VSA IN WSA FOR USE*/ 00674000
         ST    RLPARMRG,WSA6VSA(,PVTPTR)                           0312 00675000
*   WSA6LOWB=WSA6LOWB&MVBNMASK;     /* ZERO LOW ORDER 4 BITS OF VBN     00676000
*                                      PORTION OF VSA TO MAKE A VBN  */ 00677000
         NI    WSA6LOWB(PVTPTR),X'F0'                              0313 00678000
*   IF WSA6VBN<PVTLPRIV THEN        /* IS PAGE IN NUCLEUS            */ 00679000
         CLC   WSA6VBN(2,PVTPTR),PVTLPRIV(PVTPTR)                  0314 00680000
         BNL   @RF00314                                            0314 00681000
*     DO;                                                          0315 00682000
*       GOTO LBEXIT0;               /* IGNORE RELEASE REQUESTS FOR 0316 00683000
*                                      NUCLEUS PAGES                 */ 00684000
         B     LBEXIT0                                             0316 00685000
*     END;                                                         0317 00686000
*   ELSE                                                           0318 00687000
*     ;                             /* PAGE NOT IN NUCLEUS, CONTINUE */ 00688000
@RF00314 DS    0H                                                  0319 00689000
*   CALL EPFINDPG;                  /* FINDPAGE IS CALLED TO OBTAIN     00690000
*                                      PGTE AND XPTE ADDRS -- SEE  0319 00691000
*                                      ENTRY DCL FOR INTERFACE       */ 00692000
         L     @15,PVTPFP2(,PVTPTR)                                0319 00693000
         BALR  @14,@15                                             0319 00694000
*   IF RLRETCOD^=MZERO THEN         /* IS RETURN CODE ZERO           */ 00695000
         LTR   RLRETCOD,RLRETCOD                                   0320 00696000
         BZ    @RF00320                                            0320 00697000
*     IF RLRETCOD=8 THEN            /* NO, IS ERROR USER OR RSM      */ 00698000
         C     RLRETCOD,@CF00039                                   0321 00699000
         BE    @RT00321                                            0321 00700000
*       GOTO LBRFRR01;              /* IF A RETURN CODE 8 IS ISSUED     00701000
*                                      BY FINDPAGE, AN INTERNAL ERROR   00702000
*                                      HAS OCCURRED IN RSM.          */ 00703000
*     ELSE                          /* ERROR IS USER ERROR           */ 00704000
*       GOTO LBEXIT4;               /* GO TO ERROR EXIT              */ 00705000
         B     LBEXIT4                                             0323 00706000
*   PTEPTR=RLPARMR0;                /* OTHERWISE, THE PGTE AND       */ 00707000
@RF00320 LR    PTEPTR,RLPARMR0                                     0324 00708000
*   XPTEPTR=RLPARMRG;               /* XPTE ADDRESSES ARE PLACED IN     00709000
*                                      THEIR CORRECT REGISTERS       */ 00710000
         LR    XPTEPTR,RLPARMRG                                    0325 00711000
*   RESPECIFY                                                      0326 00712000
*     PFTEPTR RESTRICTED;                                          0326 00713000
*   PFTEPTR=PGTRSA;                 /* BUILD PFTE ADDRESS USING THE     00714000
*                                      RSN IN THE PGTE.              */ 00715000
         SLR   PFTEPTR,PFTEPTR                                     0327 00716000
         ICM   PFTEPTR,3,PGTRSA(PTEPTR)                            0327 00717000
*   PFTEPTR=PFTEPTR&MREALMSK;       /* AND OFF PGTE FLAG BITS FOR A     00718000
*                                      PFTE INDEX.                   */ 00719000
         N     PFTEPTR,@CF02422                                    0328 00720000
*   WSA6RSN=PFTEPTR;                /* SAVE THE INDEX IN THE WSA     */ 00721000
         STH   PFTEPTR,WSA6RSN(,PVTPTR)                            0329 00722000
*   RLSAVPCB=0;                     /* CLEAR WORD TO BE USED AT LABEL   00723000
*                                      LBXPTPRC FOR XPTE PROCESSING.    00724000
*                                      WORD CONTAINS THE ADDRESS OF     00725000
*                                      THE PCB WITH VBN MATCHING THE    00726000
*                                      INPUT VBN             @Z40WPYD*/ 00727000
         SLR   @12,@12                                             0330 00728000
         ST    @12,RLSAVPCB(,PVTPTR)                               0330 00729000
*   PFTEPTR=PFTEPTR+PVTPFTP;        /* GET FULL PFTE ADDRESS         */ 00730000
         AL    PFTEPTR,PVTPFTP(,PVTPTR)                            0331 00731000
*   IF PGTPVM='0'B THEN             /* THE PGTE INVALID FLAG IS    0332 00732000
*                                      TESTED TO DETERMINE WHICH   0332 00733000
*                                      SEGMENT BELOW RECEIVES CNTRL  */ 00734000
         TM    PGTPVM(PTEPTR),B'00001000'                          0332 00735000
         BNZ   @RF00332                                            0332 00736000
*                                                                  0333 00737000
*     /***************************************************************/ 00738000
*     /*                                                             */ 00739000
*     /* THE IN-STORAGE SEGMENT PROCESSES ALL PAGES THAT ARE IN REAL */ 00740000
*     /* STORAGE. A NUMBER OF SPECIAL CONDITION CHECKS ARE MADE AND  */ 00741000
*     /* PROCESSED FIRST. IF NONE OF THE CONDITIONS ARISE PROCESSING */ 00742000
*     /* IS CONCLUDED WITH NORMAL FREEING OF THE REAL STORAGE AND    */ 00743000
*     /* INVALIDATION OF THE PAGE                                    */ 00744000
*     /*                                                             */ 00745000
*     /***************************************************************/ 00746000
*                                                                  0333 00747000
*     DO;                                                          0333 00748000
*                                                                  0333 00749000
*       /*************************************************************/ 00750000
*       /*                                                           */ 00751000
*       /* SPECIAL CONDITION PROCESSING                              */ 00752000
*       /*                                                           */ 00753000
*       /*************************************************************/ 00754000
*                                                                  0334 00755000
*       IF PFTDFRLS='1'B THEN       /* IS A DEFERED RELEASE PENDING  */ 00756000
         TM    PFTDFRLS(PFTEPTR),B'10000000'                       0334 00757000
         BNO   @RF00334                                            0334 00758000
*         DO;                       /* A DEFERED RELEASE IS TO BE  0335 00759000
*                                      COMPLETED.                    */ 00760000
*           IF PFTFXCT^=MZERO THEN  /* IS FIX COUNT ZERO             */ 00761000
         CH    @12,PFTFXCT(,PFTEPTR)                               0336 00762000
         BNE   @RT00336                                            0336 00763000
*             GOTO LBRFRR0A;        /* IF THE FIX COUNT IS NOT ZERO,    00764000
*                                      WE HAVE AN INTERNAL RSM ERROR */ 00765000
*           ELSE                    /* FIX COUNT IS ZERO             */ 00766000
*             DO;                   /* OTHERWISE, VSM IS NOTIFIED  0338 00767000
*                                      THAT THE VIRTUAL PAGE IS NOW     00768000
*                                      FREE                          */ 00769000
*               PFTDFRLS='0'B;      /* RESET THE DEFERED RELEASE FLAG*/ 00770000
         NI    PFTDFRLS(PFTEPTR),B'01111111'                       0339 00771000
*               RLPARMRG=WSA6VSA;   /* SET UP PARAMETER TO VSM       */ 00772000
         L     RLPARMRG,WSA6VSA(,PVTPTR)                           0340 00773000
*               IF WSA6VBN<PVTLCSA THEN/* IS PAGE IN PRIVATE AREA    */ 00774000
         CLC   WSA6VBN(2,PVTPTR),PVTLCSA(PVTPTR)                   0341 00775000
         BNL   @RF00341                                            0341 00776000
*                 DO;               /* FOR PRIVATE AREA PAGES, SET 0342 00777000
*                                      REG 0 TO ZERO                 */ 00778000
*                   RLPARMR0=0;                                    0343 00779000
         SLR   RLPARMR0,RLPARMR0                                   0343 00780000
*                 END;                                             0344 00781000
*               ELSE                /* PAGE IN COMMON AREA           */ 00782000
*                 DO;               /* FOR COMMON AREA PAGES, SET  0345 00783000
*                                      REGISTER 0 TO 1               */ 00784000
         B     @RC00341                                            0345 00785000
@RF00341 DS    0H                                                  0346 00786000
*                   RLPARMR0=1;                                    0346 00787000
         LA    RLPARMR0,1                                          0346 00788000
*                 END;                                             0347 00789000
*               CALL MRELEASR;      /* CALL VSM AT A SPECIAL ENTRY --   00790000
*                                      SEE ENTRY DCL FOR INTERFACE   */ 00791000
@RC00341 L     @15,@CV02454                                        0348 00792000
         BALR  @14,@15                                             0348 00793000
*             END;                  /* END VSM NOTIFICATION          */ 00794000
*         END;                      /* END INPUT DEFERED RELEASE COND*/ 00795000
*       ELSE                        /* NO PENDING DEFERED RELEASE    */ 00796000
*         DO;                       /* TEST FOR A V=R ALLOCATED PAGE */ 00797000
         B     @RC00334                                            0351 00798000
@RF00334 DS    0H                                                  0352 00799000
*           IF PFTVRALC='1'B THEN   /* IS PAGE V=R ALLOCATED         */ 00800000
         TM    PFTVRALC(PFTEPTR),B'00000010'                       0352 00801000
         BO    @RT00352                                            0352 00802000
*             GOTO LBEXIT4;         /* TRYING TO RELEASE A V=R PAGE     00803000
*                                      IS AN ERROR SINCE RSM OR VSM     00804000
*                                      ARE THE ONLY CALLERS          */ 00805000
*           ELSE                    /* PAGE NOT V=R ALLOCATED        */ 00806000
*             IF PFTFXCT^=MZERO THEN/* IS PAGE STILL FIXED           */ 00807000
         LH    @12,PFTFXCT(,PFTEPTR)                               0354 00808000
         LTR   @12,@12                                             0354 00809000
         BZ    @RF00354                                            0354 00810000
*               DO;                 /* A PAGE TO BE RELEASED IS STILL   00811000
*                                      PGFIXED, CREATING A DEFERED 0355 00812000
*                                      RELEASE CONDITION. IMMEDIATE     00813000
*                                      RETURN IS MADE TO THE CALLER  */ 00814000
*                 PFTDFRLS='1'B;    /* SET DEFERED RELEASE FLAG      */ 00815000
         OI    PFTDFRLS(PFTEPTR),B'10000000'                       0356 00816000
*                 RLRETCOD=MDFRLSCD;/* SET PROPER RETURN CODE        */ 00817000
         LA    RLRETCOD,8                                          0357 00818000
*                 GOTO LBEXIT;      /* GO TO EXIT CODE               */ 00819000
         B     LBEXIT                                              0358 00820000
*               END;                /* END DEFERED RELEASE CREATION  */ 00821000
*                                                                  0360 00822000
*           /*********************************************************/ 00823000
*           /*                                                       */ 00824000
*           /* NORMAL RELEASE PROCESSING IS PERFORMED AT THIS POINT. */ 00825000
*           /* THIS INCLUDES SQA, LSQA, PAGEABLE AND CREATE ADDRESS  */ 00826000
*           /* SPACE PAGES. SQA AND LSQA PAGES ARE SEPARATED BY THE  */ 00827000
*           /* ASID FIELD IN THE PFTE WHICH IS 'FFFF' FOR SQA. CREATE*/ 00828000
*           /* A.S. PAGES ARE LSQA PAGES WITH AN ASID NOT EQUAL TO   */ 00829000
*           /* THE CURRENT ASID AND A VBN NOT EQUAL TO THE VBN BEING */ 00830000
*           /* PROCESSED.                                            */ 00831000
*           /*                                                       */ 00832000
*           /*********************************************************/ 00833000
*                                                                  0360 00834000
*             ELSE                                                 0360 00835000
*               DO;                                                0360 00836000
@RF00354 DS    0H                                                  0361 00837000
*                 IF PFTLSQA='1'B THEN/* TEST FOR AN SQA OR LSQA PAGE*/ 00838000
         TM    PFTLSQA(PFTEPTR),B'00100000'                        0361 00839000
         BNO   @RF00361                                            0361 00840000
*                   DO;             /* SQA OR LSQA                   */ 00841000
*                     IF PFTASID=MCOMASID THEN/* TEST FOR SQA        */ 00842000
         CLC   PFTASID(2,PFTEPTR),@CH02428                         0363 00843000
         BNE   @RF00363                                            0363 00844000
*                       DO;                                        0364 00845000
*                                                                  0364 00846000
*                         /*******************************************/ 00847000
*                         /*                                         */ 00848000
*                         /* SQA RELEASE PROCESSING INCLUDES PAGE    */ 00849000
*                         /* INVALID ATION AND MOVING OF THE PFTE TO */ 00850000
*                         /* THE AFQ, BUT NO X PTE PROCESSING IS     */ 00851000
*                         /* NECESSARY AS THERE ARE NONE.            */ 00852000
*                         /*                                         */ 00853000
*                         /*******************************************/ 00854000
*                                                                  0365 00855000
*                         PFTLSQA='0'B;/* RESET THE SQA/LSQA FLAG    */ 00856000
         NI    PFTLSQA(PFTEPTR),B'11011111'                        0365 00857000
*                         RLPARMRG=PTEPTR;/* CALL PAGE INVALIDATION     00858000
*                                      TO INVALIDATE THE PGTE AND  0366 00859000
*                                      PURGE THE HARDWARE TRANSLATION   00860000
*                                      TABLE                         */ 00861000
         LR    RLPARMRG,PTEPTR                                     0366 00862000
*                         CALL EPPGINV;/* SEE ENTRY DCL FOR INTERFACE*/ 00863000
         L     @15,PVTPINV(,PVTPTR)                                0367 00864000
         BALR  @14,@15                                             0367 00865000
*                         PGTRSA=RLCLRPGT;/* SET THE PGTE FOR THE  0368 00866000
*                                      INPUT PAGE TO ZERO, PRESERVING   00867000
*                                      THE INVALID FLAG ALREADY SET     00868000
*                                      SET PARM REG 0 IN WSA AND CALL   00869000
*                                      PFTE ENQ/DEQ TO MOVE THE PFTE    00870000
*                                      FOR THE REAL STORAGE TO AFQ   */ 00871000
         MVC   PGTRSA(2,PTEPTR),RLCLRPGT                           0368 00872000
*                         WSA6HIGH='1'B;/* PFTE TO FRONT OF AFQ      */ 00873000
         OI    WSA6HIGH(PVTPTR),B'10000000'                        0369 00874000
*                         WSA6TQID=PFTAFQN;/* AFQ Q IDENTIFIER       */ 00875000
         MVI   WSA6TQID(PVTPTR),X'00'                              0370 00876000
*                         RLPARMR0=WSA6PARM;/* RSN ALREADY IN WSA    */ 00877000
         L     RLPARMR0,WSA6PARM(,PVTPTR)                          0371 00878000
*                         PVTSQAFX=PVTSQAFX-MONE;/* DECREMENT SQA  0372 00879000
*                                      COUNT                 @YA01572*/ 00880000
         LH    @12,PVTSQAFX(,PVTPTR)                               0372 00881000
         BCTR  @12,0                                               0372 00882000
         STH   @12,PVTSQAFX(,PVTPTR)                               0372 00883000
*                         PVTCNTFX=PVTCNTFX-MONE;/* DECREMENT SYSTEM    00884000
*                                      COUNT                 @YA01572*/ 00885000
         LH    @12,PVTCNTFX(,PVTPTR)                               0373 00886000
         BCTR  @12,0                                               0373 00887000
         STH   @12,PVTCNTFX(,PVTPTR)                               0373 00888000
*                         CALL EPENQDEQ;/* REG 1 NOT SET UP BECAUSE     00889000
*                                      PFTE ENQ/DEQ DOESN'T USE. --     00890000
*                                      SEE ENTRY DCL FOR INTERFACE   */ 00891000
         L     @15,PVTPFTE(,PVTPTR)                                0374 00892000
         BALR  @14,@15                                             0374 00893000
*                         GOTO LBEXIT0;/* EXIT TO THE CALLER         */ 00894000
         B     LBEXIT0                                             0375 00895000
*                       END;        /* END SQA PROCESSING            */ 00896000
*                     ELSE          /* PAGE NOT SQA                  */ 00897000
*                       DO;                                        0377 00898000
@RF00363 DS    0H                                                  0378 00899000
*                         IF PFTASID^=RSMASCB->ASCBASID&PFTVBN^=WSA6VBN 00900000
*                           THEN    /* TEST FOR A PAGE USED FOR    0378 00901000
*                                      CREATE ADDRESS SPACE          */ 00902000
         L     @12,RSMASCB(,RSMHDPTR)                              0378 00903000
         CLC   PFTASID(2,PFTEPTR),ASCBASID(@12)                    0378 00904000
         BE    @RF00378                                            0378 00905000
         CLC   PFTVBN(2,PFTEPTR),WSA6VBN(PVTPTR)                   0378 00906000
         BE    @RF00378                                            0378 00907000
*                           DO;     /* THE PAGE TO BE RELEASED WAS 0379 00908000
*                                      USED TO CREATE AN ADDRESS   0379 00909000
*                                      SPACE. THE ONLY PROCESSING  0379 00910000
*                                      THAT SHOULD BE DONE IS      0379 00911000
*                                      INVALIDATION & ZEROING OF PGTE*/ 00912000
*                             RLPARMRG=PTEPTR;                     0380 00913000
         LR    RLPARMRG,PTEPTR                                     0380 00914000
*                             CALL EPPGINV;/* SEE ENTRY DCL FOR    0381 00915000
*                                      INTERFACE                     */ 00916000
         L     @15,PVTPINV(,PVTPTR)                                0381 00917000
         BALR  @14,@15                                             0381 00918000
*                             PGTRSA=RLCLRPGT;/* SET PGTE TO ZERO  0382 00919000
*                                      CARE MUST BE TAKEN TO LEAVE 0382 00920000
*                                      THE INVALID FLAG SET          */ 00921000
         MVC   PGTRSA(2,PTEPTR),RLCLRPGT                           0382 00922000
*                             GOTO LBXPTPRC;/* PROCESSING CONTINUES     00923000
*                                      AT XPTE PROCESSING            */ 00924000
         B     LBXPTPRC                                            0383 00925000
*                           END;                                   0384 00926000
*                         ELSE      /* OTHERWISE, THE INPUT PAGE IS     00927000
*                                      AN LSQA PAGE                  */ 00928000
*                           DO;                                    0385 00929000
@RF00378 DS    0H                                                  0386 00930000
*                             PFTLSQA='0'B;/* RESET SQA/LSQA FLAG    */ 00931000
         NI    PFTLSQA(PFTEPTR),B'11011111'                        0386 00932000
*                             RSMCNTFX=RSMCNTFX-MONE;/* DECREMENT  0387 00933000
*                                      COUNT FOR THIS ADDRESS SPACE     00934000
*                                                            @YA01572*/ 00935000
         LH    @12,RSMCNTFX(,RSMHDPTR)                             0387 00936000
         BCTR  @12,0                                               0387 00937000
         STH   @12,RSMCNTFX(,RSMHDPTR)                             0387 00938000
*                             PVTCNTFX=PVTCNTFX-MONE;/* DECREMENT  0388 00939000
*                                      SYSTEM COUNT          @YA01572*/ 00940000
         LH    @12,PVTCNTFX(,PVTPTR)                               0388 00941000
         BCTR  @12,0                                               0388 00942000
         STH   @12,PVTCNTFX(,PVTPTR)                               0388 00943000
*                           END;                                   0389 00944000
*                       END;        /* END NON-SQA PROCESSING        */ 00945000
*                   END;            /* END SQA/LSQA PROCESSING       */ 00946000
*                 ELSE                                             0392 00947000
*                   ;               /* PAGE NOT SQA OR LSQA, CONTINUE*/ 00948000
@RF00361 DS    0H                                                  0393 00949000
*               END;                /* END NO CREATE RELEASE PROC    */ 00950000
*         END;                      /* END NON-INPUT DEFERED RELEASE */ 00951000
*                                                                  0395 00952000
*       /*************************************************************/ 00953000
*       /*                                                           */ 00954000
*       /* LSQA AND NORMAL PAGE FRAMES ARE FREED IN THIS SECTION AND */ 00955000
*       /* THE PGTE FOR THE PAGE SET TO 0. PROCESSING WILL THEN      */ 00956000
*       /* CONTINUE WITH XPTE PROCESSING.                            */ 00957000
*       /*                                                           */ 00958000
*       /*************************************************************/ 00959000
*                                                                  0395 00960000
*       RLPARMRG=PTEPTR;            /* SET UP PGTE ADDR PARAMETER    */ 00961000
@RC00334 LR    RLPARMRG,PTEPTR                                     0395 00962000
*       CALL EPPGINV;               /* CALL PAGE INVALIDATION -- SEE    00963000
*                                      ENTRY DCL FOR INTERFACE       */ 00964000
         L     @15,PVTPINV(,PVTPTR)                                0396 00965000
         BALR  @14,@15                                             0396 00966000
*       PGTRSA=RLCLRPGT;            /* SET PGTE TO ZERO, PRESERVING     00967000
*                                      THE INVALID FLAG ALREADY SET  */ 00968000
         MVC   PGTRSA(2,PTEPTR),RLCLRPGT                           0397 00969000
*       WSA6HIGH='1'B;              /* SET FLAG TO SEND PFTE TO THE     00970000
*                                      TOP OF THE AFQ                */ 00971000
         OI    WSA6HIGH(PVTPTR),B'10000000'                        0398 00972000
*       WSA6TQID=PFTAFQN;           /* SET TQID PARM TO AFQ Q ID     */ 00973000
         MVI   WSA6TQID(PVTPTR),X'00'                              0399 00974000
*       RLPARMR0=WSA6PARM;          /* LOAD PARM REG 0 WITH PRE-SET     00975000
*                                      WORD IN WSA. RSN HAS BEEN SET    00976000
*                                      PREVIOUSLY                    */ 00977000
         L     RLPARMR0,WSA6PARM(,PVTPTR)                          0400 00978000
*       CALL EPENQDEQ;              /* CALL PFTE ENQ/DEQ TO SEND THE    00979000
*                                      PFTE TO THE AFQ. REG 1 NOT SET   00980000
*                                      UP BECAUSE PFTE ENQ/DEQ     0401 00981000
*                                      DOESN'T USE. -- SEE ENTRY DCL    00982000
*                                      FOR INTERFACE                 */ 00983000
         L     @15,PVTPFTE(,PVTPTR)                                0401 00984000
         BALR  @14,@15                                             0401 00985000
*     END;                          /* END IN-STORAGE SEGMENT        */ 00986000
*                                                                  0403 00987000
*   /*****************************************************************/ 00988000
*   /*                                                               */ 00989000
*   /* THE OUT OF STORAGE SEGMENT PROCESSES A VIRTUAL PAGE IF IT IS  */ 00990000
*   /* NOT IN STORAGE UPON ENTRY AT IEAVRELF. IT FIRST CHECKS FOR AND*/ 00991000
*   /* PROCESSES THE SPECIAL OUT OF STORAGE DEFERED RELEASE ENTRY    */ 00992000
*   /* FROM PGFREE OR PAGE TERMINATION SERVICES IF IT EXISTS.        */ 00993000
*   /* OTHERWISE, NORMAL OUT OF STORAGE PROCESSING IS PERFORMED. THIS*/ 00994000
*   /* SEGMENT CALLS THE Q SEARCH SUBROUTINE AND POSSIBLY THE GFA    */ 00995000
*   /* DEFER Q OR I/O ACTIVE Q'S PROCESSOR SUBROUTINES IF A FIXED PCB*/ 00996000
*   /* IS FOUND BY THE Q SEARCH SUBROUTINE                           */ 00997000
*   /*                                                               */ 00998000
*   /*****************************************************************/ 00999000
*                                                                  0403 01000000
*   ELSE                                                           0403 01001000
*     DO;                                                          0403 01002000
*                                                                  0403 01003000
         B     @RC00332                                            0403 01004000
@RF00332 DS    0H                                                  0404 01005000
*       /*************************************************************/ 01006000
*       /*                                                           */ 01007000
*       /* CHECK FOR OUT-OF-STORAGE DEFERED RELEASE CONDITION THIS   */ 01008000
*       /* CONDITION EXISTS ONLY IF THE RSN (RBN) IN THE PGTE FOR THE*/ 01009000
*       /* INPUT PAGE IS NOT ZERO AND THE ASID, VBN DATA IN THE PFTE */ 01010000
*       /* INDEXED BY THE RSN MATCH THE INPUT ASID, VBN DATA.        */ 01011000
*       /*                                                           */ 01012000
*       /*************************************************************/ 01013000
*                                                                  0404 01014000
*       IF WSA6RSN^=MZERO THEN                                     0404 01015000
         LH    @12,WSA6RSN(,PVTPTR)                                0404 01016000
         N     @12,@CF02622                                        0404 01017000
         LTR   @12,@12                                             0404 01018000
         BZ    @RF00404                                            0404 01019000
*         DO;                       /* IF THE RSN OBTAINED FROM THE     01020000
*                                      PGTE IS NOT ZERO, THE PFTE  0405 01021000
*                                      MUST BE CHECKED               */ 01022000
*           IF PFTVBN=WSA6VBN THEN  /* DO VBNS MATCH                 */ 01023000
         CLC   PFTVBN(2,PFTEPTR),WSA6VBN(PVTPTR)                   0406 01024000
         BNE   @RF00406                                            0406 01025000
*             DO;                   /* YES                           */ 01026000
*               IF WSA6VBN<PVTLCSA THEN/* PRIVATE AREA VBN           */ 01027000
         CLC   WSA6VBN(2,PVTPTR),PVTLCSA(PVTPTR)                   0408 01028000
         BNL   @RF00408                                            0408 01029000
*                 DO;               /* YES                           */ 01030000
*                   IF PFTASID^=RSMASCB->ASCBASID THEN/* CHECK ASIDS    01031000
*                                      TO BE EQUAL                   */ 01032000
         L     @12,RSMASCB(,RSMHDPTR)                              0410 01033000
         CLC   PFTASID(2,PFTEPTR),ASCBASID(@12)                    0410 01034000
         BNE   @RT00410                                            0410 01035000
*                     GOTO LBNRMLCD;/* IF NOT SKIP PFTE PROCESSING   */ 01036000
*                   ELSE                                           0412 01037000
*                     ;             /* ASIDS ARE EQUAL, CONTINUE     */ 01038000
*                 END;                                             0413 01039000
*               ELSE                                               0414 01040000
*                 ;                 /* COMMON AREA VBN, NO ASID CHECK*/ 01041000
@RF00408 DS    0H                                                  0415 01042000
*               IF PFTDFRLS='1'B THEN/* IS DEFERED RELEASE PENDING   */ 01043000
*                                                                  0415 01044000
         TM    PFTDFRLS(PFTEPTR),B'10000000'                       0415 01045000
         BNO   @RF00415                                            0415 01046000
*                 /***************************************************/ 01047000
*                 /*                                                 */ 01048000
*                 /* THE REAL FRAME IS STILL ASSIGNED TO THE INPUT   */ 01049000
*                 /* PAGE AND AN OUT-OF-STORAGE DEFERED RELEASE      */ 01050000
*                 /* CONDITION IS TO BE PROCESSED                    */ 01051000
*                 /*                                                 */ 01052000
*                 /***************************************************/ 01053000
*                                                                  0416 01054000
*                 DO;                                              0416 01055000
*                   IF PFTFXCT=MZERO THEN/* IS FIX COUNT ZERO        */ 01056000
         LH    @12,PFTFXCT(,PFTEPTR)                               0417 01057000
         LTR   @12,@12                                             0417 01058000
         BNZ   @RF00417                                            0417 01059000
*                     DO;           /* THE FIX COUNT IS ZERO         */ 01060000
*                       PFTDFRLS='0'B;/* RESET THE DEFERED FLAG      */ 01061000
         NI    PFTDFRLS(PFTEPTR),B'01111111'                       0419 01062000
*                       RLPARMRG=WSA6VSA;/* SET UP THE CALL TO THE 0420 01063000
*                                      SPECIAL VSM ENTRY POINT TO  0420 01064000
*                                      NOTIFY VSM THAT THE PAGE IS 0420 01065000
*                                      AVAILABLE.                    */ 01066000
         L     RLPARMRG,WSA6VSA(,PVTPTR)                           0420 01067000
*                       IF WSA6VBN<PVTLCSA THEN/* IS PAGE PRIVATE OR    01068000
*                                      COMMON                        */ 01069000
         CLC   WSA6VBN(2,PVTPTR),PVTLCSA(PVTPTR)                   0421 01070000
         BNL   @RF00421                                            0421 01071000
*                         DO;       /* FOR PRIVATE AREA PAGES, SET 0422 01072000
*                                      REG 0 TO ZERO                 */ 01073000
*                           RLPARMR0=0;                            0423 01074000
         SLR   RLPARMR0,RLPARMR0                                   0423 01075000
*                         END;                                     0424 01076000
*                       ELSE        /* COMMON AREA PAGE              */ 01077000
*                         DO;       /* FOR COMMON AREA PAGES, SET  0425 01078000
*                                      REGISTER 0 TO 1               */ 01079000
         B     @RC00421                                            0425 01080000
@RF00421 DS    0H                                                  0426 01081000
*                           RLPARMR0=1;                            0426 01082000
         LA    RLPARMR0,1                                          0426 01083000
*                         END;                                     0427 01084000
*                       CALL MRELEASR;/* SEE ENTRY DCL FOR INTERFACE */ 01085000
@RC00421 L     @15,@CV02454                                        0428 01086000
         BALR  @14,@15                                             0428 01087000
*                       PGTRSA=RLCLRPGT;/* SET PGTE TO ZERO,       0429 01088000
*                                      PRESERVING THE INVALID FLAG 0429 01089000
*                                      ALREADY SET                   */ 01090000
         MVC   PGTRSA(2,PTEPTR),RLCLRPGT                           0429 01091000
*                       GOTO LBXPTPRC;/* SKIP PCB PROCESSING, DO NOT    01092000
*                                      FREE REAL FRAME FOR OUT OF  0430 01093000
*                                      STORAGE PROCESSING            */ 01094000
         B     LBXPTPRC                                            0430 01095000
*                     END;          /* END DEFERED RELEASE PROCESSING*/ 01096000
*                   ELSE            /* IF THE FIX COUNT IS NOT ZERO     01097000
*                                      WE HAVE AN INTERNAL ERROR.    */ 01098000
*                     GOTO LBRFRR0A;/* GO RECORD THE ERROR           */ 01099000
*                 END;              /* END DEFERED RELEASE PROCESSING*/ 01100000
*               ELSE                                               0434 01101000
*                 ;                 /* NO PENDING DEFERED RELEASE    */ 01102000
@RF00415 DS    0H                                                  0435 01103000
*             END;                  /* END VBNS EQUAL PROCESSING     */ 01104000
*           ELSE                                                   0436 01105000
*             ;                     /* VBNS WERE NOT EQUAL           */ 01106000
@RF00406 DS    0H                                                  0437 01107000
*         END;                      /* END NON-ZERO RSN PROCESSING   */ 01108000
*       ELSE                                                       0438 01109000
*         ;                         /* RSN IN PGTE WAS ZERO          */ 01110000
*                                                                  0438 01111000
@RF00404 DS    0H                                                  0439 01112000
*       /*************************************************************/ 01113000
*       /*                                                           */ 01114000
*       /* NORMAL OUT-OF-STORAGE PROCESSING CONSISTS OF PURGING ANY  */ 01115000
*       /* PCB'S ON ANY Q FOR THE PAGE AND SETTING THE PGTE FOR THE  */ 01116000
*       /* PAGE TO ZERO WITH THE GETMAIN FLAG OFF. A SPECIAL CASE    */ 01117000
*       /* ARISES IF A PCB FOR A FIX REQUEST IS FOUND ON AN I/O      */ 01118000
*       /* ACTIVE Q. IN THIS CASE, A DEFERRED RELEASE RESULTS.       */ 01119000
*       /*                                                           */ 01120000
*       /*************************************************************/ 01121000
*                                                                  0439 01122000
*LBNRMLCD:                                                         0439 01123000
*       RLPARMRG=WSA6VBN;           /* SET UP VBN FOR CALL           */ 01124000
LBNRMLCD SLR   RLPARMRG,RLPARMRG                                   0439 01125000
         ICM   RLPARMRG,3,WSA6VBN(PVTPTR)                          0439 01126000
*       CALL SUBQSRCH;              /* CALL THE Q SEARCH SUBROUTINE  */ 01127000
         BAL   @14,SUBQSRCH                                        0440 01128000
*       RESPECIFY                                                  0441 01129000
*         RLRETPCB RESTRICTED;                                     0441 01130000
*       IF RLRETPCB^=MZERO THEN     /* WAS A PCB ADDRESS RETURNED    */ 01131000
         LTR   RLRETPCB,RLRETPCB                                   0442 01132000
         BZ    @RF00442                                            0442 01133000
*         DO;                       /* THE Q SEARCH RTN FOUND A PCB     01134000
*                                      WITH A NON-ZERO FIX COUNT     */ 01135000
*           RESPECIFY                                              0444 01136000
*             PCBPTR RESTRICTED;                                   0444 01137000
*           PCBPTR=RLRETPCB;        /* INIT. PCB BASE PTR WITH     0445 01138000
*                                      RETURNED PCB ADDRESS          */ 01139000
         LR    PCBPTR,RLRETPCB                                     0445 01140000
*           IF PCBCQN^=PCBDEFRN THEN/* IS PCB ON AN I/O ACTIVE Q     */ 01141000
         CLI   PCBCQN(PCBPTR),24                                   0446 01142000
         BE    @RF00446                                            0446 01143000
*             DO;                                                  0447 01144000
*                                                                  0447 01145000
*               /*****************************************************/ 01146000
*               /*                                                   */ 01147000
*               /* PROCESS PCB ON AN I/O ACTIVE Q BY SETTING THE     */ 01148000
*               /* DEFERRED RELEASE FLAG IN THE PFTE ASSIGNED TO THE */ 01149000
*               /* PAGE AND CALLING THE SUBIOACT SUBROUTINE TO PURGE */ 01150000
*               /* ANY PCB'S NOT FOR A FIX REQUEST.                  */ 01151000
*               /*                                                   */ 01152000
*               /*****************************************************/ 01153000
*                                                                  0448 01154000
*               PFTEPTR=PCBRBN;     /* GET ADDR OF PFTE USING THE RBN   01155000
*                                      IN THE PCB                    */ 01156000
         LH    PFTEPTR,PCBRBN(,PCBPTR)                             0448 01157000
         N     PFTEPTR,@CF02622                                    0448 01158000
*               PFTEPTR=PFTEPTR+PVTPFTP;                           0449 01159000
         AL    PFTEPTR,PVTPFTP(,PVTPTR)                            0449 01160000
*               PFTDFRLS='1'B;      /* SET THE DEFERED RELEASE FLAG  */ 01161000
         OI    PFTDFRLS(PFTEPTR),B'10000000'                       0450 01162000
*               RESPECIFY                                          0451 01163000
*                 PFTEPTR UNRESTRICTED;                            0451 01164000
*               RLPARMRG=PCBPTR;                                   0452 01165000
         LR    RLPARMRG,PCBPTR                                     0452 01166000
*               CALL SUBIOACT;      /* CALL THE I/O ACTIVE Q       0453 01167000
*                                      PROCESSOR TO PURGE ALL      0453 01168000
*                                      NON-FIXED PCBS                */ 01169000
         BAL   @14,SUBIOACT                                        0453 01170000
*               PGTPAM='0'B;        /* TURN OFF THE GETMAIN ASSIGNED    01171000
*                                      FLAG TO PREVENT FURTHER RSM 0454 01172000
*                                      PROCESSING OF THIS PAGE       */ 01173000
         NI    PGTPAM(PTEPTR),B'11111110'                          0454 01174000
*               RLRETCOD=MDFRLSCD;  /* SET THE PROPER RETURN CODE    */ 01175000
         LA    RLRETCOD,8                                          0455 01176000
*               GOTO LBEXIT;        /* GO TO EXIT CODE               */ 01177000
         B     LBEXIT                                              0456 01178000
*             END;                  /* END I/O ACTIVE Q PROCESSING   */ 01179000
*           ELSE                    /* PCB ON GFA DEFER Q            */ 01180000
*             DO;                                                  0458 01181000
*                                                                  0458 01182000
@RF00446 DS    0H                                                  0459 01183000
*               /*****************************************************/ 01184000
*               /*                                                   */ 01185000
*               /* PROCESS PCB ON THE GFA DEFER Q BY DECREMENTING THE*/ 01186000
*               /* FOE TASK FIX COUNTS FOR PCB'S REPRESENTING FIX    */ 01187000
*               /* REQUESTS IF FOE'S EXIST AND THEN CALLING SUBGFADQ */ 01188000
*               /* TO PURGE ALL PCB'S.                               */ 01189000
*               /*                                                   */ 01190000
*               /*****************************************************/ 01191000
*                                                                  0459 01192000
*               DO WHILE(PCBPTR^=MZERO);/* PROCESS Q'ED PCB AND ALL     01193000
*                                      RELATED PCB'S                 */ 01194000
         B     @DE00459                                            0459 01195000
@DL00459 DS    0H                                                  0460 01196000
*                 IF PCBFXC^=MZERO THEN/* TEST PCB FIX COUNT         */ 01197000
         CLI   PCBFXC(PCBPTR),0                                    0460 01198000
         BE    @RF00460                                            0460 01199000
*                   DO;                                            0461 01200000
*                     IF PCBRTCBA^=MZERO THEN/* FOR PCB'S WITH     0462 01201000
*                                      NON-ZERO FIX CNT, TEST ROOT 0462 01202000
*                                      PCB TCB ADDR                  */ 01203000
         L     @14,PCBRPTR-1(,PCBPTR)                              0462 01204000
         LA    @14,0(,@14)                                         0462 01205000
         L     @12,PCBRTCBA-1(,@14)                                0462 01206000
         LA    @12,0(,@12)                                         0462 01207000
         LTR   @12,@12                                             0462 01208000
         BZ    @RF00462                                            0462 01209000
*                       DO;         /* DO FOE PROCESSING FOR PCBS  0463 01210000
*                                      WITH NON-ZERO FIX COUNTS AND     01211000
*                                      FOR WHICH FOES EXIST          */ 01212000
*                         RESPECIFY                                0464 01213000
*                           FOEPTR RESTRICTED;                     0464 01214000
*                         FOEPTR=TCBFOEA;/* GET ADDR OF FIRST FOE    */ 01215000
         L     @14,TCBPTR-1(,@14)                                  0465 01216000
         L     FOEPTR,TCBFOEA-1(,@14)                              0465 01217000
         LA    FOEPTR,0(,FOEPTR)                                   0465 01218000
*                         DO WHILE(FOEPTR^=MZERO);/* SEARCH ORDERED     01219000
*                                      FOE CHAIN                     */ 01220000
         B     @DE00466                                            0466 01221000
@DL00466 DS    0H                                                  0467 01222000
*                           IF FOEVINDX>WSA6VBN THEN/* TEST VBN      */ 01223000
         CLC   FOEVINDX(2,FOEPTR),WSA6VBN(PVTPTR)                  0467 01224000
         BNH   @RF00467                                            0467 01225000
*                             DO;   /* SEARCH TERMINATED WHEN A VBN     01226000
*                                      GREATER THAN INPUT IS FOUND IN   01227000
*                                      FOE                           */ 01228000
*                               FOEPTR=MZERO;                      0469 01229000
         SLR   FOEPTR,FOEPTR                                       0469 01230000
*                             END;                                 0470 01231000
*                           ELSE    /* VBN NOT GREATER THAN CURRENT  */ 01232000
*                             DO;   /* CHECK FOR EQUAL VBN'S         */ 01233000
         B     @RC00467                                            0471 01234000
@RF00467 DS    0H                                                  0472 01235000
*                               IF FOEVINDX=WSA6VBN THEN/* ARE VBNS     01236000
*                                      EQUAL                         */ 01237000
         CLC   FOEVINDX(2,FOEPTR),WSA6VBN(PVTPTR)                  0472 01238000
         BNE   @RF00472                                            0472 01239000
*                                 DO;/* IF EQUAL, DECR FOE FIX CNT   */ 01240000
*                                   FOEFXCT=FOEFXCT-PCBFXC;        0474 01241000
         LH    @14,FOEFXCT(,FOEPTR)                                0474 01242000
         SLR   @12,@12                                             0474 01243000
         IC    @12,PCBFXC(,PCBPTR)                                 0474 01244000
         SLR   @14,@12                                             0474 01245000
         STH   @14,FOEFXCT(,FOEPTR)                                0474 01246000
*                                   FOEPTR=MZERO;                  0475 01247000
         SLR   FOEPTR,FOEPTR                                       0475 01248000
*                                 END;                             0476 01249000
*                               ELSE/* VBNS NOT EQUAL                */ 01250000
*                                 DO;/* CONTINUE SEARCH              */ 01251000
         B     @RC00472                                            0477 01252000
@RF00472 DS    0H                                                  0478 01253000
*                                   FOEPTR=FOEFLINK;/* GET NEXT ADDR */ 01254000
         L     @14,FOEFLINK-1(,FOEPTR)                             0478 01255000
         LA    @14,0(,@14)                                         0478 01256000
         LR    FOEPTR,@14                                          0478 01257000
*                                 END;                             0479 01258000
*                             END;                                 0480 01259000
@RC00472 DS    0H                                                  0481 01260000
*                         END;      /* END FOEPTR DO WHILE           */ 01261000
@RC00467 DS    0H                                                  0481 01262000
@DE00466 LTR   FOEPTR,FOEPTR                                       0481 01263000
         BNZ   @DL00466                                            0481 01264000
*                         RESPECIFY                                0482 01265000
*                           FOEPTR UNRESTRICTED;                   0482 01266000
*                       END;        /* END FOE PROCESSING            */ 01267000
*                     ELSE                                         0484 01268000
*                       ;           /* ROOT TCB ADDR WAS ZERO        */ 01269000
@RF00462 DS    0H                                                  0485 01270000
*                   END;            /* END NON-ZERO FIX PROCESSING   */ 01271000
*                 ELSE                                             0486 01272000
*                   ;               /* PCB FIX COUNT WAS ZERO        */ 01273000
@RF00460 DS    0H                                                  0487 01274000
*                 PCBPTR=PCBRLPA;   /* GET RELATED PCB ADDRESS       */ 01275000
         L     @14,PCBRLPA-1(,PCBPTR)                              0487 01276000
         LA    @14,0(,@14)                                         0487 01277000
         LR    PCBPTR,@14                                          0487 01278000
*               END;                /* END PCBPTR DO WHILE           */ 01279000
@DE00459 LTR   PCBPTR,PCBPTR                                       0488 01280000
         BNZ   @DL00459                                            0488 01281000
*               RLPARMRG=RLRETPCB;  /* SET UP PCB PARM FOR SUBGFADQ  */ 01282000
         LR    RLPARMRG,RLRETPCB                                   0489 01283000
*               RESPECIFY                                          0490 01284000
*                (RLRETPCB,                                        0490 01285000
*                 PCBPTR) UNRESTRICTED;                            0490 01286000
*               CALL SUBGFADQ;      /* CALL TO THE GFA DEFER Q SUBRTN   01287000
*                                      TO PURGE ALL PCBS FOR THE VBN */ 01288000
         BAL   @14,SUBGFADQ                                        0491 01289000
*               RLSAVPCB=0;         /* SET TO ZERO SO XPTE PROCESSING   01290000
*                                      DOES NOT LOOK AT A PCB WHICH     01291000
*                                      MAY HAVE BEEN FREED   @Z40WPYD*/ 01292000
         SLR   @12,@12                                             0492 01293000
         ST    @12,RLSAVPCB(,PVTPTR)                               0492 01294000
*             END;                  /* END GFA DEFER Q PROCESSING    */ 01295000
*         END;                      /* END PCB PROCESSING            */ 01296000
*       ELSE                                                       0495 01297000
*         ;                         /* NO PCB WAS RETURNED FROM    0495 01298000
*                                      SUBRTN                        */ 01299000
@RF00442 DS    0H                                                  0496 01300000
*       PGTRSA=RLCLRPGT;            /* FOR ALL OUT-OF-STORAGE CASES     01301000
*                                      EXCEPT NEW DEFERED RELEASES,     01302000
*                                      SET THE PGTE TO ZERO,       0496 01303000
*                                      PRESERVING THE INVALID FLAG   */ 01304000
         MVC   PGTRSA(2,PTEPTR),RLCLRPGT                           0496 01305000
*     END;                          /* END OUT-OF-STORAGE SEGMENT    */ 01306000
*                                                                  0498 01307000
*   /*****************************************************************/ 01308000
*   /*                                                               */ 01309000
*   /* THE XPTE PROCESSING SEGMENT ZEROES THE LPID VALUE IN THE XPTE */ 01310000
*   /* FOR AN INPUT COMMON AREA PAGE, FREEING ANY AUXILIARY STORAGE  */ 01311000
*   /* ASSIGNED TO THE PAGE THAT CAN BE RELEASED. IT ALSO RESETS THE */ 01312000
*   /* PROTECT KEY FIELD TO 0, FETCH PROCTECT, AND RESETS THE FLAGS  */ 01313000
*   /* TO ALL OFF POSITIONS                                          */ 01314000
*   /*                                                               */ 01315000
*   /*****************************************************************/ 01316000
*                                                                  0498 01317000
*LBXPTPRC:                                                         0498 01318000
*   IF XPTPOINP='1'B THEN           /* IS I/O IN PROGRESS FOR THIS 0498 01319000
*                                      PAGE                          */ 01320000
@RC00332 DS    0H                                                  0498 01321000
LBXPTPRC TM    XPTPOINP(XPTEPTR),B'00100000'                       0498 01322000
         BNO   @RF00498                                            0498 01323000
*     DO;                           /* YES, I/O IS IN PROGRESS     0499 01324000
*                                                            @Z40WPYD*/ 01325000
*       XPTDEFER='0'B;              /* SET DEFER FLAG OFF SO QUEUE 0500 01326000
*                                      SEARCH ROUTINE WILL SCAN I/O     01327000
*                                      QUEUE                 @Z40WPYD*/ 01328000
         NI    XPTDEFER(XPTEPTR),B'11111101'                       0500 01329000
*       RLPARMRG=WSA6VBN;           /* SET UP VBN FOR CALL   @ZA13812*/ 01330000
         SLR   RLPARMRG,RLPARMRG                                   0501 01331000
         ICM   RLPARMRG,3,WSA6VBN(PVTPTR)                          0501 01332000
*       CALL SUBQSRCH;              /* CALL THE QUEUE SEARCH ROUTINE    01333000
*                                                            @Z40WPYD*/ 01334000
         BAL   @14,SUBQSRCH                                        0502 01335000
*     END;                                                         0503 01336000
*   ELSE                                                           0504 01337000
*     ;                                                            0504 01338000
@RF00498 DS    0H                                                  0505 01339000
*   IF XPTXAV='1'B THEN             /* IS AUX STORAGE ASSIGNED       */ 01340000
         TM    XPTXAV(XPTEPTR),B'01000000'                         0505 01341000
         BNO   @RF00505                                            0505 01342000
*     DO;                           /* AUXILIARY STORAGE IS ASSIGNED    01343000
*                                                            @Z40WPYD*/ 01344000
*       IF RLSAVPCB=0 THEN          /* WAS A PCB FOUND WITH A      0507 01345000
*                                      MATCHING VBN                  */ 01346000
         L     @12,RLSAVPCB(,PVTPTR)                               0507 01347000
         LTR   @12,@12                                             0507 01348000
         BNZ   @RF00507                                            0507 01349000
*         DO;                       /* YES                   @Z40WPYD*/ 01350000
*           IF XPTVIOLP='0'B THEN   /* MAY AUX STG BE FREED          */ 01351000
         TM    XPTVIOLP(XPTEPTR),B'10000000'                       0509 01352000
         BNZ   @RF00509                                            0509 01353000
*             CALL SUBFRAUX;        /* YES, GET STORAGE FREED      0510 01354000
*                                                            @Z40WPYD*/ 01355000
         BAL   @14,SUBFRAUX                                        0510 01356000
*           ELSE                                                   0511 01357000
*             XPTLPID=''B;          /* CLEAR THE LPID FIELD  @Z40WPYD*/ 01358000
         B     @RC00509                                            0511 01359000
@RF00509 XC    XPTLPID(8,XPTEPTR),XPTLPID(XPTEPTR)                 0511 01360000
*         END;                                                     0512 01361000
*       ELSE                                                       0513 01362000
*         DO;                                                      0513 01363000
         B     @RC00507                                            0513 01364000
@RF00507 DS    0H                                                  0514 01365000
*           PCBPTR=RLSAVPCB;        /* RESTORE PRIMARY PCB POINTER 0514 01366000
*                                                            @Z40WPYD*/ 01367000
         L     PCBPTR,RLSAVPCB(,PVTPTR)                            0514 01368000
*           IF PCBIOCMP='1'B THEN   /* IS PCB MARKED I/O COMPLETE    */ 01369000
         TM    PCBIOCMP(PCBPTR),B'00000001'                        0515 01370000
         BNO   @RF00515                                            0515 01371000
*             DO;                   /* YES, I/O IS COMPLETE  @Z40WPYD*/ 01372000
*               IF XPTVIOLP='0'B THEN/* MAY AUX STG BE FREED         */ 01373000
         TM    XPTVIOLP(XPTEPTR),B'10000000'                       0517 01374000
         BNZ   @RF00517                                            0517 01375000
*                 CALL SUBFRAUX;    /* YES, GO CALL ASM TO FREE SLOT    01376000
*                                                            @Z40WPYD*/ 01377000
         BAL   @14,SUBFRAUX                                        0518 01378000
*               ELSE                                               0519 01379000
*                 XPTLPID=''B;      /* CLEAR XPTLPID FIELD   @Z40WPYD*/ 01380000
         B     @RC00517                                            0519 01381000
@RF00517 XC    XPTLPID(8,XPTEPTR),XPTLPID(XPTEPTR)                 0519 01382000
*             END;                                                 0520 01383000
*           ELSE                    /* PCB IS NOT MARKED I/O COMPLETE*/ 01384000
*             DO;                                                  0521 01385000
         B     @RC00515                                            0521 01386000
@RF00515 DS    0H                                                  0522 01387000
*               RFY                                                0522 01388000
*                 AIAPTR RSTD;                                     0522 01389000
*               AIAPTR=ADDR(PCBAIA);/* GET ADDRESS OF AIA    @Z40WPYD*/ 01390000
         LA    AIAPTR,PCBAIA(,PCBPTR)                              0523 01391000
*               AIAFRAUX='1'B;      /* INDICATE TO ASM THAT SLOT MAY    01392000
*                                      BE FREED WHEN I/O COMPLETES 0524 01393000
*                                                            @Z40WPYD*/ 01394000
         OI    AIAFRAUX(AIAPTR),B'10000000'                        0524 01395000
*               AIALSID2=XPTLSID2;  /* COPY SECOND LSID INTO AIA.  0525 01396000
*                                      THIS IS NECESSARY BECAUSE,  0525 01397000
*                                      WHEN DUPLEXING, LSID2 MUST BE    01398000
*                                      FREED. NOTE THAT IF THE     0525 01399000
*                                      DUPLEXED COPY IS THE ONE BEING   01400000
*                                      USED, THEN LSID2 HAS        0525 01401000
*                                      PREVIOUSLY BEEN COPIED TO   0525 01402000
*                                      LSID1 AND LSID2 IS ZERO     0525 01403000
*                                                            @Z40WPYD*/ 01404000
         L     @11,XPTLSID2(,XPTEPTR)                              0525 01405000
         ST    @11,AIALSID2(,AIAPTR)                               0525 01406000
*               RFY                                                0526 01407000
*                 AIAPTR UNRSTD;                                   0526 01408000
*               XPTLPID=''B;        /* CLEAR XPTLPID FIELD   @Z40WPYD*/ 01409000
         XC    XPTLPID(8,XPTEPTR),XPTLPID(XPTEPTR)                 0527 01410000
*             END;                                                 0528 01411000
*         END;                                                     0529 01412000
*     END;                                                         0530 01413000
*   ELSE                                                           0531 01414000
*     ;                                                            0531 01415000
@RF00505 DS    0H                                                  0532 01416000
*   XPTFLAGS=MZERO;                 /* RESET 1ST BYTE OF XPTE FLAGS     01417000
*                                      TO ZERO                       */ 01418000
@RC00505 MVI   XPTFLAGS(XPTEPTR),X'00'                             0532 01419000
*   XPTFLAG2=MZERO;                 /* RESET 2ND BYTE OF XPTE FLAGS     01420000
*                                      TO ZERO               @Z40WPYD*/ 01421000
         MVI   XPTFLAG2(XPTEPTR),X'00'                             0533 01422000
*   XPTPROT=MKEY0FET;               /* RESET THE XPTE KEY TO ZERO, 0534 01423000
*                                      FETCH PROTECT                 */ 01424000
         MVI   XPTPROT(XPTEPTR),X'08'                              0534 01425000
*                                                                  0535 01426000
*   /*****************************************************************/ 01427000
*   /*                                                               */ 01428000
*   /* THE FREE SEGMENT PROCESSING SEGMENT CHECKS TO SEE IF THE ABOVE*/ 01429000
*   /* PROCESSING HAS FREED THE LAST PAGE IN A SEGMENT (16 VIRTUAL   */ 01430000
*   /* PAGES). IF SO, A PAGE TABLE/EXTERNAL PAGE TABLE COMBINATION   */ 01431000
*   /* CAN BE FREED. IT IS DISCONNECTED FROM THE SEGMENT TABLE BY    */ 01432000
*   /* CALLING THE DESTROY SEGMENT FUNCTION OF RSM. THE ADDRESS OF   */ 01433000
*   /* THIS TABLE IS THEN RETURNED TO THE CALLER TO BE FREEMAINED. IT*/ 01434000
*   /* CAN NOT BE FREEMAINED HERE BECAUSE FREEMAIN IS THE PRINCIPLE  */ 01435000
*   /* CALLER VIA THE IEAVRELV ENTRY POINT. THIS ACTION IS TAKEN ONLY*/ 01436000
*   /* FOR PRIVATE AREA SEGEMTS.                                     */ 01437000
*   /*                                                               */ 01438000
*   /*****************************************************************/ 01439000
*                                                                  0535 01440000
*   IF WSA6VBN<PVTLCSA THEN                                        0535 01441000
         CLC   WSA6VBN(2,PVTPTR),PVTLCSA(PVTPTR)                   0535 01442000
         BNL   @RF00535                                            0535 01443000
*     DO;                           /* GET THE ADDRESS OF THE PAGE BY   01444000
*                                      THE CALLING TRANSLATE REAL TO    01445000
*                                      VIRTUAL ROUTINE OF RSM WITH 0536 01446000
*                                      THE REAL ADDRESS OF THE PGT 0536 01447000
*                                      OBTAINED FROM THE SGTE        */ 01448000
*       RESPECIFY                                                  0537 01449000
*         STEPTR RESTRICTED;                                       0537 01450000
*       STEPTR=RSMVSTO+(WSA6SEGN*LENGTH(SGTSTE));                  0538 01451000
         SLR   STEPTR,STEPTR                                       0538 01452000
         IC    STEPTR,WSA6SEGN(,PVTPTR)                            0538 01453000
         SLA   STEPTR,2                                            0538 01454000
         AL    STEPTR,RSMVSTO(,RSMHDPTR)                           0538 01455000
*       RLPARMRG=SGTSTE&MSEGMASK;                                  0539 01456000
         ICM   RLPARMRG,15,SGTSTE(STEPTR)                          0539 01457000
         N     RLPARMRG,@CF02424                                   0539 01458000
*       CALL EPTRANS;               /* SEE ENTRY DCL FOR INTERFACE   */ 01459000
         L     @12,CVTPTR                                          0540 01460000
         L     @15,CVTPTRV(,@12)                                   0540 01461000
         BALR  @14,@15                                             0540 01462000
*       IF RLRETCOD^=MZERO THEN     /* IS RETURN CODE NON-ZERO       */ 01463000
         LTR   RLRETCOD,RLRETCOD                                   0541 01464000
         BNZ   @RT00541                                            0541 01465000
*         GOTO LBRFRR01;            /* IF A NON-ZERO RETURN CODE IS     01466000
*                                      RETURNED, AN INTERNAL ERROR 0542 01467000
*                                      HAS OCCURRED                  */ 01468000
*       ELSE                                                       0543 01469000
*         ;                         /* RETURN CODE WAS ZERO, CONTINUE*/ 01470000
*       DO PTEPTR=RLPARMRG BY LENGTH(PGTPTE) TO(RLPARMRG+LENGTH(PGTPTE) 01471000
*             *MFIFTEEN);           /* TEST THE GETMAIN ASSIGNED FLAG   01472000
*                                      IN EACH PGTE UNTIL ONE IS   0544 01473000
*                                      FOUND. THIS LOOP DEPENDS ON 0544 01474000
*                                      TRV RETURNING A ZERO IN THE 0544 01475000
*                                      HIGH ORDER BYTE OF REG 1.     */ 01476000
         LR    PTEPTR,RLPARMRG                                     0544 01477000
         B     @DE00544                                            0544 01478000
@DL00544 DS    0H                                                  0545 01479000
*         IF PGTRSA^=RLCLRPGT THEN  /* TEST FOR A VALUE OF X'0008'   */ 01480000
         CLC   PGTRSA(2,PTEPTR),RLCLRPGT                           0545 01481000
         BNE   @RT00545                                            0545 01482000
*           GOTO LBKEPSEG;          /* PAGE NOT FREEMAINED YET       */ 01483000
*         ELSE                                                     0547 01484000
*           ;                       /* PAGE FREEMAINED, CONTINUE LOOP*/ 01485000
*       END;                        /* END PGTE SEARCH               */ 01486000
         AL    PTEPTR,@CF00081                                     0548 01487000
@DE00544 LA    @12,30                                              0548 01488000
         ALR   @12,RLPARMRG                                        0548 01489000
         CR    PTEPTR,@12                                          0548 01490000
         BNH   @DL00544                                            0548 01491000
*       RLPARMR0=WSA6SEGN;          /* IF ALL PAGES HAVE BEEN      0549 01492000
*                                      FREEMAINED THEN CALL DESTROY     01493000
*                                      SEGMENT                       */ 01494000
         SLR   RLPARMR0,RLPARMR0                                   0549 01495000
         IC    RLPARMR0,WSA6SEGN(,PVTPTR)                          0549 01496000
*       CALL EPDSEG;                /* SEE ENTRY DCL FOR INTERFACE   */ 01497000
         L     @15,PVTPDSEG(,PVTPTR)                               0550 01498000
         BALR  @14,@15                                             0550 01499000
*       RCACSECT=RCACPGRL;          /* RESET CSECT ID IN RCA         */ 01500000
         L     @12,RCAPTR(,PVTPTR)                                 0551 01501000
         MVI   RCACSECT(@12),X'07'                                 0551 01502000
*       RLPARMR0=RLPARMRG;          /* PUT THE ADDRESS RETURNED BY 0552 01503000
*                                      DESTROY SEGMENT IN REG 0      */ 01504000
         LR    RLPARMR0,RLPARMRG                                   0552 01505000
*       GOTO LBEXIT0A;              /* SKIP SETTING OF REG 0         */ 01506000
         B     LBEXIT0A                                            0553 01507000
*       RESPECIFY                                                  0554 01508000
*         STEPTR UNRESTRICTED;                                     0554 01509000
*     END;                          /* END PRIVATE AREA PROCESSING   */ 01510000
*   ELSE                                                           0556 01511000
*     ;                             /* COMMON AREA PAGE, NO FREE   0556 01512000
*                                      SEGMENT PROCESSING TO BE DONE */ 01513000
@RF00535 DS    0H                                                  0557 01514000
*LBKEPSEG:                          /* ANY GETMAINED PAGE CAUSES EXIT   01515000
*                                      FROM ABOVE LOOP TO THIS POINT */ 01516000
*                                                                  0557 01517000
*   /*****************************************************************/ 01518000
*   /*                                                               */ 01519000
*   /* THIS SEGMENT CONTAINS THE EXIT LOGIC FOR SUBFMPAG. ALL RETURN */ 01520000
*   /* CODE 0 EXITS WHERE FREE SEGMENT PROCESSING WAS NOT PERFORMED  */ 01521000
*   /* RETURN A ZERO IN REGISTER 0.                                  */ 01522000
*   /*                                                               */ 01523000
*   /*****************************************************************/ 01524000
*                                                                  0557 01525000
*LBEXIT0:                           /* NORMAL EXIT POINT WHERE REG 0    01526000
*                                      MUST BE SET TO ZERO           */ 01527000
*   RLPARMR0=0;                     /* SET RETURN PARM TO ZERO       */ 01528000
LBKEPSEG DS    0H                                                  0557 01529000
LBEXIT0  SLR   RLPARMR0,RLPARMR0                                   0557 01530000
*LBEXIT0A:                          /* ALL NORMAL EXITS ARE MADE HERE*/ 01531000
*   RESPECIFY                                                      0558 01532000
*     RLRETCOD RESTRICTED;          /* PROTECT RETURN CODE IN R15    */ 01533000
LBEXIT0A DS    0H                                                  0559 01534000
*   RLRETCOD=MGOODCD;               /* SET UP NORMAL RETURN CODE     */ 01535000
         SLR   RLRETCOD,RLRETCOD                                   0559 01536000
*   GOTO LBEXIT;                    /* GO TO EXIT CODE               */ 01537000
*                                                                  0560 01538000
         B     LBEXIT                                              0560 01539000
*   /*****************************************************************/ 01540000
*   /*                                                               */ 01541000
*   /* WHEN INTERNAL RSM ERRORS ARE DETECTED, CONTROL COMES HERE     */ 01542000
*   /*                                                               */ 01543000
*   /*****************************************************************/ 01544000
*                                                                  0561 01545000
*LBRFRR01:                          /* FINDPAGE OR IEAVTRV RETURNED A   01546000
*                                      BAD RETURN CODE               */ 01547000
*   RCARCRD='01'X;                  /* SET REASON CODE               */ 01548000
LBRFRR01 L     @14,RCAPTR(,PVTPTR)                                 0561 01549000
         MVI   RCARCRD(@14),X'01'                                  0561 01550000
*   GOTO LBRSMFRR;                  /* RECORD THE ERROR              */ 01551000
         B     LBRSMFRR                                            0562 01552000
*LBRFRR0A:                          /* INCORRECT FIX COUNT WAS FOUND */ 01553000
