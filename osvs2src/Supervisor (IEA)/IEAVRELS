@EF00003 DS    0H                                                  0568 01585000
@ER00003 BR    @14                                                 0568 01586000
*                                                                  0569 01587000
*   /*****************************************************************/ 01588000
*   /*                                                               */ 01589000
*   /* THE PAGE PROCESSOR SUBROUTINE IS CALLED ONLY BY THE IEAVRELS  */ 01590000
*   /* ENTRY LOGIC TO PROCESS PAGES THAT LOGIC HAS FOUND ELIGIBLE FOR*/ 01591000
*   /* RELEASE PROCESSING. THIS FIRST SEGMENT PERFORMS IN-STORAGE    */ 01592000
*   /* PROCESSING PROVIDED THE PAGE IS NOT AN SQA/LSQA, V=R          */ 01593000
*   /* ALLOCATED, OR PGFIX'ED PAGE. THE PGTE FOR THE PAGE IS         */ 01594000
*   /* INVALIDATED AND SET TO ZERO PRESERVING THE GETMAIN FLAG AND   */ 01595000
*   /* THE PFTE FOR THE REAL FRAME ASSIGNED TO THE INPUT PAGE        */ 01596000
*   /* RETURNED TO THE AFQ.                                          */ 01597000
*   /*                                                               */ 01598000
*   /*****************************************************************/ 01599000
*                                                                  0569 01600000
*SUBPAGE:                                                          0569 01601000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0569 01602000
SUBPAGE  DS    0H                                                  0570 01603000
*   CIWWRK5=RLRETREG;               /* SAVE THE RETURN ADDRESS       */ 01604000
         ST    RLRETREG,CIWWRK5(,CIWAPTR)                          0570 01605000
*   IF RELSVBN<PVTLPRIV THEN        /* IS PAGE IN NUCLEUS            */ 01606000
         CLC   RELSVBN(2,CIWAPTR),PVTLPRIV(PVTPTR)                 0571 01607000
         BL    @RT00571                                            0571 01608000
*     GOTO LBRETRN0;                /* IF PAGE IS A NUCLEUS PAGE,  0572 01609000
*                                      RETURN IMMEDIATELY            */ 01610000
*   ELSE                                                           0573 01611000
*     ;                             /* PAGE NOT IN NUCLEUS           */ 01612000
*   IF PGTPVM='0'B THEN             /* IS PAGE VALID IN REAL STG     */ 01613000
*                                                                  0574 01614000
         TM    PGTPVM(PTEPTR),B'00001000'                          0574 01615000
         BNZ   @RF00574                                            0574 01616000
*     /***************************************************************/ 01617000
*     /*                                                             */ 01618000
*     /* IN-STORAGE PROCESSING                                       */ 01619000
*     /*                                                             */ 01620000
*     /***************************************************************/ 01621000
*                                                                  0575 01622000
*     DO;                                                          0575 01623000
*       RESPECIFY                                                  0576 01624000
*         PFTEPTR RESTRICTED;                                      0576 01625000
*       PFTEPTR=PGTRSA;             /* DEVELOP PFTE ADDRESS USING  0577 01626000
*                                      PGTE.                         */ 01627000
