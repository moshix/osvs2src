         B     @RC00668                                            0670 01917000
@RF00668 SLR   RLPARMR0,RLPARMR0                                   0670 01918000
*   R1=XPTEPTR;                     /* PUT POINTER TO XPTE IN REG 1     01919000
*                                      FOR FREE SLOT         @Z40WPYD*/ 01920000
@RC00668 LR    R1,XPTEPTR                                          0671 01921000
*   CALL ILRFRSLT;                  /* GO TO THE ASM FREE SLOT     0672 01922000
*                                      ROUTINE               @Z40WPYD*/ 01923000
         L     @15,PVTFRSLT(,PVTPTR)                               0672 01924000
         BALR  @14,@15                                             0672 01925000
*   RLRETREG=RLSAVRET;              /* RESTORE RETURN ADDR AND EXIT  */ 01926000
         LR    RLRETREG,RLSAVRET                                   0673 01927000
*   RESPECIFY                                                      0674 01928000
*    (RLSAVRET,                                                    0674 01929000
*     RLSAVREG) UNRESTRICTED;                                      0674 01930000
*   END;                            /* END SUBFRAUX SUBPROCEDURE     */ 01931000
@EL00005 DS    0H                                                  0675 01932000
@EF00005 DS    0H                                                  0675 01933000
@ER00005 BR    @14                                                 0675 01934000
*                                                                  0676 01935000
*   /*****************************************************************/ 01936000
*   /*                                                               */ 01937000
*   /* THE PCB Q SEARCH SUBROUTINE RECEIVES CONTROL FROM EITHER THE  */ 01938000
*   /* IEAVRELS OR IEAVRELF ENTRY POINTS AND HAS NO SAVE AREA. INPUT */ 01939000
*   /* IS A VBN IN REGISTER 1, AN XPTE ADDRESS IN REGISTER 6, AND THE*/ 01940000
*   /* STANDARD RSM REGISTERS 2 AND 3. THIS SUBROUTINE MUST NOT      */ 01941000
*   /* DESTROY REGISTERS 2, 3, 4, 5, 6, OR 10                        */ 01942000
*   /*                                                               */ 01943000
*   /*****************************************************************/ 01944000
*                                                                  0676 01945000
*SUBQSRCH:                                                         0676 01946000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0676 01947000
SUBQSRCH DS    0H                                                  0677 01948000
*   RESPECIFY                                                      0677 01949000
*    (RLSAVRET,                                                    0677 01950000
*     PCBPTR,                                                      0677 01951000
*     RLRETCOD) RESTRICTED;                                        0677 01952000
*   RLSAVRET=RLRETREG;              /* SAVE THE CALLERS RETURN ADDR  */ 01953000
         LR    RLSAVRET,RLRETREG                                   0678 01954000
*   RLSAVPCB=MZERO;                 /* INITIALIZE PCB ADDRESS SAVE 0679 01955000
*                                      AREA                  @ZA13812*/ 01956000
         SLR   @14,@14                                             0679 01957000
         ST    @14,RLSAVPCB(,PVTPTR)                               0679 01958000
*   IF XPTDEFER='1'B THEN           /* SEARCH THE GFA DEFER Q ONLY IF   01959000
*                                      THE DEFER FLAG IS SET IN THE     01960000
*                                      XPTE                  @Z40WPYD*/ 01961000
         TM    XPTDEFER(XPTEPTR),B'00000010'                       0680 01962000
         BNO   @RF00680                                            0680 01963000
*     DO;                                                          0681 01964000
*       RLQREG=MZERO;               /* SET Q INDICATOR REG TO VALUE     01965000
*                                      FOR GFA DEFER Q               */ 01966000
         SLR   RLQREG,RLQREG                                       0682 01967000
*       PCBPTR=PVTGFADF;            /* GET THE ADDR OF THE FIRST PCB    01968000
*                                      ON THE GFA DEFER Q            */ 01969000
         L     PCBPTR,PVTGFADF(,PVTPTR)                            0683 01970000
*     END;                                                         0684 01971000
*   ELSE                            /* GFA DEFER Q NOT TO BE SEARCHED*/ 01972000
*     DO;                                                          0685 01973000
         B     @RC00680                                            0685 01974000
@RF00680 DS    0H                                                  0686 01975000
*LBIOCHCK:                          /* IF THE GFA DEFER Q IS NOT TO     01976000
*                                      BE SEARCHED OR A PCB WAS NOT     01977000
*                                      FOUND ON THE GFA DEFER Q,   0686 01978000
*                                      CHECK FOR I/O Q SEARCH      0686 01979000
*                                      CRITERIA                      */ 01980000
*       IF XPTXAV='1'B THEN         /* AN I/O ACTIVE Q IS SEARCHED 0686 01981000
*                                      ONLY IF AUX STORAGE IS      0686 01982000
*                                      ASSIGNED TO THE INPUT PAGE    */ 01983000
LBIOCHCK TM    XPTXAV(XPTEPTR),B'01000000'                         0686 01984000
         BNO   @RF00686                                            0686 01985000
*         DO;                                                      0687 01986000
*           IF RLPARMRG<PVTLCSA THEN/* INPUT VBN IN PRIVATE OR COMMON*/ 01987000
         LH    @14,PVTLCSA(,PVTPTR)                                0688 01988000
         N     @14,@CF02622                                        0688 01989000
         CR    RLPARMRG,@14                                        0688 01990000
         BNL   @RF00688                                            0688 01991000
*             DO;                   /* GET ADDR OF FIRST PCB ON LOCAL   01992000
*                                      I/O ACTIVE Q                  */ 01993000
*               PCBPTR=RSMLIOQF;                                   0690 01994000
         L     PCBPTR,RSMLIOQF(,RSMHDPTR)                          0690 01995000
*             END;                                                 0691 01996000
*           ELSE                    /* COMMON AREA VBN               */ 01997000
*             DO;                   /* GET FIRST PCB ADDR ON COMMON     01998000
*                                      I/O ACTIVE Q                  */ 01999000
         B     @RC00688                                            0692 02000000
@RF00688 DS    0H                                                  0693 02001000
*               PCBPTR=PVTCIOQF;                                   0693 02002000
         L     PCBPTR,PVTCIOQF(,PVTPTR)                            0693 02003000
*             END;                                                 0694 02004000
*           RLQREG=MONE;            /* SET Q INDICATOR REG TO VALUE     02005000
*                                      FOR I/O ACTIVE Q              */ 02006000
@RC00688 LA    RLQREG,1                                            0695 02007000
*         END;                      /* END PREPARATION FOR I/O ACTIVE   02008000
*                                      Q SEARCH                      */ 02009000
*       ELSE                        /* NO AUX STORAGE WAS ASSIGNED   */ 02010000
*         DO;                       /* IF NEITHER Q IS TO BE SEARCHED   02011000
*                                      OR NO PCB WAS FOUND ON THE GFA   02012000
*                                      DEFER Q AND THE I/O ACITVE Q     02013000
*                                      IS NOT TO BE SEARCHED, RETURN    02014000
*                                      IS MADE TO THE CALLER WITH A     02015000
*                                      ZERO PCB ADDRESS              */ 02016000
         B     @RC00686                                            0697 02017000
@RF00686 DS    0H                                                  0698 02018000
*           RLRETPCB=MZERO;                                        0698 02019000
         SLR   RLRETPCB,RLRETPCB                                   0698 02020000
*           GOTO LBRETQS;           /* GO TO COMMON EXIT CODE        */ 02021000
         B     LBRETQS                                             0699 02022000
*         END;                      /* END ZERO RETURN LOGIC         */ 02023000
*     END;                          /* END I/O ACITVE Q CHECK        */ 02024000
@RC00686 DS    0H                                                  0702 02025000
*                                                                  0702 02026000
*   /*****************************************************************/ 02027000
*   /*                                                               */ 02028000
*   /* THIS SEGMENT SEARCHES THE PCB Q SET UP IN THE FIRST SEGMENT   */ 02029000
*   /* FOR PCBS WITH MATCHING VBNS AND ASCB ADDRESSES WHERE          */ 02030000
*   /* NECESSARY. THE VBN TEST IS ALWAYS PERFORMED FIRST BECAUSE THIS*/ 02031000
*   /* FIELD WILL BE ZERO IN ALREADY PURGED PCB'S AND BECAUSE IT IS  */ 02032000
*   /* THE ONLY TEST NECESSARY FOR INPUT PAGES IN THE COMMON AREA.   */ 02033000
*   /*                                                               */ 02034000
*   /*****************************************************************/ 02035000
*                                                                  0702 02036000
*LBPCBLOP:                          /* THIS IS THE BEGINNING OF LOOP */ 02037000
*   IF PCBPTR^=MZERO THEN                                          0702 02038000
@RC00680 DS    0H                                                  0702 02039000
LBPCBLOP LTR   PCBPTR,PCBPTR                                       0702 02040000
         BZ    @RF00702                                            0702 02041000
*     DO;                           /* IF THE LAST PCB HAS NOT BEEN     02042000
*                                      TESTED, THE NEXT PCB IS TESTED*/ 02043000
*       IF RLPARMRG=PCBVBN THEN     /* IS PCB FOR PAGE               */ 02044000
         LH    @14,PCBVBN(,PCBPTR)                                 0704 02045000
         N     @14,@CF02622                                        0704 02046000
         CR    RLPARMRG,@14                                        0704 02047000
         BNE   @RF00704                                            0704 02048000
*         DO;                       /* IF THE VBNS MATCH, THE ASCB 0705 02049000
*                                      ADDRESSES ARE TESTED FOR    0705 02050000
*                                      PRIVATE AREA VBNS ONLY        */ 02051000
*           IF RLPARMRG<PVTLCSA THEN/* IS PAGE IN PRIVATE AREA       */ 02052000
         LH    @14,PVTLCSA(,PVTPTR)                                0706 02053000
         N     @14,@CF02622                                        0706 02054000
         CR    RLPARMRG,@14                                        0706 02055000
         BNL   @RF00706                                            0706 02056000
*             DO;                   /* YES                           */ 02057000
*               IF RSMASCB^=PCBASCB THEN/* IS PCB FOR CURRENT ADDRESS   02058000
*                                      SPACE                         */ 02059000
         CLC   RSMASCB(4,RSMHDPTR),PCBASCB(PCBPTR)                 0708 02060000
         BNE   @RT00708                                            0708 02061000
*                 GOTO LBNXTPCB;    /* NO, TEST NEXT PCB             */ 02062000
*               ELSE                                               0710 02063000
*                 ;                 /* PCB IS FOR CURRENT A.S.       */ 02064000
*             END;                  /* END ASCB TEST PROCESSING      */ 02065000
*           ELSE                                                   0712 02066000
*             ;                     /* PAGE IS IN COMMON AREA        */ 02067000
@RF00706 DS    0H                                                  0713 02068000
*           RLSAVPCB=PCBPTR;        /* SAVE ADDR OF FOUND PCB        */ 02069000
         ST    PCBPTR,RLSAVPCB(,PVTPTR)                            0713 02070000
*           GOTO LBFOUND;           /* EXIT SEARCH LOOP              */ 02071000
         B     LBFOUND                                             0714 02072000
*         END;                      /* END VBN MATCH PROCESSING      */ 02073000
*       ELSE                        /* PCB NOT FOR CURRENT PAGE      */ 02074000
*         DO;                                                      0716 02075000
@RF00704 DS    0H                                                  0717 02076000
*LBNXTPCB:                          /* IF CURRENT PCB DOES NOT MATCH    02077000
*                                      CRITERIA, GET NEXT PCB ON Q   */ 02078000
*           PCBPTR=PCBFQPA;                                        0717 02079000
LBNXTPCB L     @14,PCBFQPA-1(,PCBPTR)                              0717 02080000
         LA    @14,0(,@14)                                         0717 02081000
         LR    PCBPTR,@14                                          0717 02082000
*           GOTO LBPCBLOP;          /* REPEAT ABOVE TESTS FOR NEW PCB*/ 02083000
         B     LBPCBLOP                                            0718 02084000
*         END;                      /* END NEXT PCB PROCESSING       */ 02085000
*     END;                          /* END NON-ZERO PCBPTR PROCESSING*/ 02086000
*   ELSE                            /* ALL PCBS ON A Q HAVE BEEN   0721 02087000
*                                      TESTED                        */ 02088000
*     DO;                           /* WHEN THE LAST PCB ON A Q HAS     02089000
*                                      BEEN PROCESSED, CHECK TO SEE     02090000
*                                      IF AN I/O Q SHOULD BE SEARCHED*/ 02091000
@RF00702 DS    0H                                                  0722 02092000
*       IF RLQREG=MZERO THEN        /* WHAT Q HAS BEEN SEARCHED      */ 02093000
         LTR   RLQREG,RLQREG                                       0722 02094000
         BZ    @RT00722                                            0722 02095000
*         GOTO LBIOCHCK;            /* ONLY THE GFA DEFER QUEUE HAS     02096000
*                                      BEEN SEARCHED, SEARCH I/O Q'S */ 02097000
*       ELSE                        /* NO PCBS HAVE BEEN FOUND ON ANY   02098000
*                                      Q.                            */ 02099000
*         DO;                                                      0724 02100000
*           RLRETPCB=MZERO;                                        0725 02101000
         SLR   RLRETPCB,RLRETPCB                                   0725 02102000
*           GOTO LBRETQS;           /* RETURN IS MADE TO THE CALLER     02103000
*                                      THRU COMMON EXIT CODE         */ 02104000
         B     LBRETQS                                             0726 02105000
*         END;                                                     0727 02106000
*     END;                          /* END LAST PCB PROCESSING       */ 02107000
*                                                                  0729 02108000
*   /*****************************************************************/ 02109000
*   /*                                                               */ 02110000
*   /* THIS SEGMENT PROCESSES A FOUND PCB. IF THE FOUND PCB OR A PCB */ 02111000
*   /* RELATED TO THE FOUND PCB HAS A NON-ZERO FIX COUNT, THE FOUND  */ 02112000
*   /* PCB ADDR IS RETURNED TO THE CALLER. OTHERWISE, IT IS PROCESSED*/ 02113000
*   /* BY CALLING THE CORRECT Q PROCESSOR SUBROUTINE.                */ 02114000
*   /*                                                               */ 02115000
*   /*****************************************************************/ 02116000
*                                                                  0729 02117000
*LBFOUND:                                                          0729 02118000
*   RLRETPCB=MZERO;                 /* SET UP RETURN PARM TO ZERO    */ 02119000
LBFOUND  SLR   RLRETPCB,RLRETPCB                                   0729 02120000
*   DO WHILE(PCBPTR^=MZERO);        /* LOOK FOR NON-ZERO FIX COUNTS  */ 02121000
         B     @DE00730                                            0730 02122000
@DL00730 DS    0H                                                  0731 02123000
*     IF PCBFXC=MZERO THEN          /* IS PCB FIX COUNT ZERO         */ 02124000
         CLI   PCBFXC(PCBPTR),0                                    0731 02125000
         BNE   @RF00731                                            0731 02126000
*       DO;                         /* IF A FIX COUNT IS ZERO, GET 0732 02127000
*                                      THE RELATED PCB PTR           */ 02128000
*         PCBPTR=PCBRLPA;                                          0733 02129000
         L     @14,PCBRLPA-1(,PCBPTR)                              0733 02130000
         LA    @14,0(,@14)                                         0733 02131000
         LR    PCBPTR,@14                                          0733 02132000
*       END;                                                       0734 02133000
*     ELSE                          /* A NON-ZERO FIX COUNT EXISTS   */ 02134000
*       DO;                                                        0735 02135000
         B     @RC00731                                            0735 02136000
@RF00731 DS    0H                                                  0736 02137000
*         RLRETPCB=RLSAVPCB;        /* PREPARE TO RETURN THE ADDR OF    02138000
*                                      THE PRIMARY PCB TO CALLER     */ 02139000
         L     RLRETPCB,RLSAVPCB(,PVTPTR)                          0736 02140000
*         PCBPTR=MZERO;             /* FORCE EXIT FROM LOOP          */ 02141000
         SLR   PCBPTR,PCBPTR                                       0737 02142000
*       END;                                                       0738 02143000
*   END;                            /* END DO WHILE                  */ 02144000
@RC00731 DS    0H                                                  0739 02145000
@DE00730 SLR   @14,@14                                             0739 02146000
         CR    PCBPTR,@14                                          0739 02147000
         BNE   @DL00730                                            0739 02148000
*   IF RLRETPCB=MZERO THEN          /* WAS A FIX PCB FOUND ABOVE     */ 02149000
         CR    RLRETPCB,@14                                        0740 02150000
         BNE   @RF00740                                            0740 02151000
*     DO;                           /* IF NO NON-ZERO FIX COUNTS WERE   02152000
*                                      FOUND, CALL THE CORRECT Q   0741 02153000
*                                      PROCESSOR SUBROUTINE          */ 02154000
*       RLPARMRG=RLSAVPCB;          /* SET UP INPUT PARAMETER        */ 02155000
         L     RLPARMRG,RLSAVPCB(,PVTPTR)                          0742 02156000
*       IF RLQREG=MZERO THEN        /* WHAT Q IS PCB ON              */ 02157000
         CR    RLQREG,@14                                          0743 02158000
         BNE   @RF00743                                            0743 02159000
*         DO;                       /* GFA DEFER Q                   */ 02160000
*           CALL SUBGFADQ;          /* CALL GFA DEFER Q PROCESSOR    */ 02161000
         BAL   @14,SUBGFADQ                                        0745 02162000
*           RLSAVPCB=0;             /* ZERO FOUND PCB ADDRESS SO XPTE   02163000
*                                      PROCESSING DOES NOT LOOK AT A    02164000
*                                      PCB WHICH MAY HAVE BEEN FREED    02165000
*                                                            @Z40WPYD*/ 02166000
         SLR   @14,@14                                             0746 02167000
         ST    @14,RLSAVPCB(,PVTPTR)                               0746 02168000
*         END;                                                     0747 02169000
*       ELSE                        /* PCB ON AN I/O ACTIVE Q        */ 02170000
*         DO;                                                      0748 02171000
         B     @RC00743                                            0748 02172000
@RF00743 DS    0H                                                  0749 02173000
*           CALL SUBIOACT;          /* CALL I/O ACTIVE Q PROCESSOR   */ 02174000
         BAL   @14,SUBIOACT                                        0749 02175000
*         END;                                                     0750 02176000
*       RLRETPCB=MZERO;             /* SET UP RETURN PCB PARM        */ 02177000
@RC00743 SLR   RLRETPCB,RLRETPCB                                   0751 02178000
*     END;                          /* END NON-FIX PROCESSING        */ 02179000
*   ELSE                                                           0753 02180000
*     ;                             /* A FIX PCB WAS FOUND, CONTINUE */ 02181000
@RF00740 DS    0H                                                  0754 02182000
*LBRETQS:                                                          0754 02183000
*   RLRETREG=RLSAVRET;              /* RESTORE RETURN ADDRESS        */ 02184000
LBRETQS  LR    RLRETREG,RLSAVRET                                   0754 02185000
*   RESPECIFY                                                      0755 02186000
*     RLRETPCB UNRESTRICTED;                                       0755 02187000
*   END;                            /* END SUBQSRCH PROC             */ 02188000
@EL00006 DS    0H                                                  0756 02189000
@EF00006 DS    0H                                                  0756 02190000
@ER00006 BR    @14                                                 0756 02191000
*                                                                  0757 02192000
*   /*****************************************************************/ 02193000
*   /*                                                               */ 02194000
*   /* THE GFA DEFER Q PROCESSOR SUBROUTINE IS CALLED FROM THE Q     */ 02195000
*   /* SEARCH SUBROUTINE OR THE IEAVRELF ENTRY POINT AND HAS NO SAVE */ 02196000
*   /* AREA. INPUT IS THE ADDRESS OF A QUEUED PCB IN REGISTER 1, THE */ 02197000
*   /* XPTE ADDRESS IN REGISTER 6, AND THE STANDARD RSM REGISTERS 2  */ 02198000
*   /* AND 3. IT MUST NOT DESTROY REGS 2, 3, 4, 5, 6, 10, AND 11. THE*/ 02199000
*   /* RETURN ADDRESS IS SAVED IN THE IEAVRELF WORK/SAVE AREA,       */ 02200000
*   /* PVTWSA6.                                                      */ 02201000
*   /*                                                               */ 02202000
*   /*****************************************************************/ 02203000
*                                                                  0757 02204000
*SUBGFADQ:                                                         0757 02205000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0757 02206000
SUBGFADQ DS    0H                                                  0758 02207000
*   WSA5GFAQ=RLRETREG;              /* SAVE CALLERS RETURN ADDRESS   */ 02208000
         ST    RLRETREG,WSA5GFAQ(,PVTPTR)                          0758 02209000
*   XPTDEFER='0'B;                  /* RESET THE DEFER FLAG IN XPTE  */ 02210000
         NI    XPTDEFER(XPTEPTR),B'11111101'                       0759 02211000
*   PCBPTR=RLPARMRG;                /* GET ADDR OF INPUT PCB         */ 02212000
         LR    PCBPTR,RLPARMRG                                     0760 02213000
*LBPROCES:                          /* PROCESSING FOR THE INPUT PCB     02214000
*                                      AND ALL RELATED PCBS STARTS   */ 02215000
*   IF PCBRESET='1'B THEN           /* DOES RESET HAVE TO BE CALLED  */ 02216000
*                                                                  0761 02217000
LBPROCES TM    PCBRESET(PCBPTR),B'00010000'                        0761 02218000
         BNO   @RF00761                                            0761 02219000
*     /***************************************************************/ 02220000
*     /*                                                             */ 02221000
*     /* THE RESET SUBROUTINE OF PCFLIH MUST BE CALLED FOR THIS PCB. */ 02222000
*     /* IT CAN BE CALLED ONLY FOR SRB MODE PCBS HERE BECAUSE        */ 02223000
*     /* OWNERSHIP OF THE LOCAL LOCK IS NOT KNOWN.                   */ 02224000
*     /*                                                             */ 02225000
*     /***************************************************************/ 02226000
*                                                                  0762 02227000
*     DO;                                                          0762 02228000
*       RESPECIFY                                                  0763 02229000
*        (RLGFAWK7,                                                0763 02230000
*         RLGFAWK8,                                                0763 02231000
*         RLGFAW12,                                                0763 02232000
*         RLSAVREG) RESTRICTED;                                    0763 02233000
*       IF PCBSRBMD='1'B THEN       /* IS PCB IN SRB MODE            */ 02234000
         TM    PCBSRBMD(PCBPTR),B'00100000'                        0764 02235000
         BNO   @RF00764                                            0764 02236000
*         DO;                       /* YES, PREPARE TO CALL RESET    */ 02237000
*           RLGFAWK7=RLRESET4;      /* SAVE THE CONTENTS             */ 02238000
         LR    RLGFAWK7,RLRESET4                                   0766 02239000
*           RLGFAWK8=RLRESET5;      /* OF THE INPUT                  */ 02240000
         LR    RLGFAWK8,RLRESET5                                   0767 02241000
*           RLGFAW12=RLRESET6;      /* REGISTERS TO RESET            */ 02242000
         LR    RLGFAW12,RLRESET6                                   0768 02243000
*           RLRESET4=PCBSRB;        /* SET UP INPUT REGS - SRB ADDR  */ 02244000
         L     RLRESET4,PCBSRB(,PCBPTR)                            0769 02245000
*           RLRESET5=MZERO;         /* INDICATE SRB MODE             */ 02246000
         SLR   RLRESET5,RLRESET5                                   0770 02247000
*           RLRESET6=MZERO;         /* NO PAGING I/O ERROR           */ 02248000
         SLR   RLRESET6,RLRESET6                                   0771 02249000
*           RLSAVREG=ADDR(PVTSAVE); /* GET ADDR GLOBAL SAVE AREA     */ 02250000
         LA    RLSAVREG,PVTSAVE(,PVTPTR)                           0772 02251000
*           CALL EPRESET;           /* CALL RESET -- SEE ENTRY DCL 0773 02252000
*                                      FOR INTERFACE                 */ 02253000
         L     @15,PVTPRSET(,PVTPTR)                               0773 02254000
         BALR  @14,@15                                             0773 02255000
*           RLRESET4=RLGFAWK7;      /* RESTORE RESET                 */ 02256000
         LR    RLRESET4,RLGFAWK7                                   0774 02257000
*           RLRESET5=RLGFAWK8;      /* PARAMETER                     */ 02258000
         LR    RLRESET5,RLGFAWK8                                   0775 02259000
*           RLRESET6=RLGFAW12;      /* REGISTERS                     */ 02260000
         LR    RLRESET6,RLGFAW12                                   0776 02261000
*           RESPECIFY                                              0777 02262000
*            (RLGFAWK7,                                            0777 02263000
*             RLGFAWK8,                                            0777 02264000
*             RLGFAW12,                                            0777 02265000
*             RLSAVREG) UNRESTRICTED;                              0777 02266000
*           RLPARMR0=PCBFREQN;      /* SET UP TQN PARM TO PCB MANAGER   02267000
*                                      TO SEND CURRENT PCB TO FREE Q */ 02268000
         LA    RLPARMR0,16                                         0778 02269000
*         END;                      /* END CALL RESET PROCESSING     */ 02270000
*       ELSE                        /* PCB IN TCB MODE               */ 02271000
*         DO;                                                      0780 02272000
*                                                                  0780 02273000
         B     @RC00764                                            0780 02274000
@RF00764 DS    0H                                                  0781 02275000
*           /*********************************************************/ 02276000
*           /*                                                       */ 02277000
*           /* IF THE PCB IS NOT IN SRB MODE, OR HAS A ROOT PCB WITH */ 02278000
*           /* A PCB COUNT OF 1, THEN IT MUST BE TRANSFERED TO THE   */ 02279000
*           /* CORRECT I/O ACTIVE Q AND PIOCP SCHEDULED TO PROCESS IT*/ 02280000
*           /*                                                       */ 02281000
*           /*********************************************************/ 02282000
*                                                                  0781 02283000
*LBTRANS:                                                          0781 02284000
*           PCBIOCMP='1'B;          /* SET I/O COMPLETE FLAG IN PCB  */ 02285000
LBTRANS  OI    PCBIOCMP(PCBPTR),B'00000001'                        0781 02286000
*           RLPARMRG=PCBPTR;        /* SET UP PARM REG FOR CALL      */ 02287000
         LR    RLPARMRG,PCBPTR                                     0782 02288000
*           CALL EPSCHED;           /* CALL A SUBROUTINE OF PIOP TO     02289000
*                                      SCHEDULE AN SRB FOR PIOCP --     02290000
*                                      SEE ENTRY DCL FOR INTERFACE   */ 02291000
         L     @15,PVTPOPBR(,PVTPTR)                               0783 02292000
         BALR  @14,@15                                             0783 02293000
*           IF PCBVBN<PVTLCSA THEN  /* IS PCB PRIVATE OR COMMON      */ 02294000
         CLC   PCBVBN(2,PCBPTR),PVTLCSA(PVTPTR)                    0784 02295000
         BNL   @RF00784                                            0784 02296000
*             DO;                                                  0785 02297000
*               RLPARMR0=PCBLIOQN;  /* SET TQN PARM FOR PCB MANAGER     02298000
*                                      TO LOCAL I/O ACTIVE Q         */ 02299000
         LA    RLPARMR0,136                                        0786 02300000
*             END;                                                 0787 02301000
*           ELSE                    /* PCB IS FOR COMMON AREA PAGE   */ 02302000
*             DO;                                                  0788 02303000
         B     @RC00784                                            0788 02304000
@RF00784 DS    0H                                                  0789 02305000
*               RLPARMR0=PCBCIOQN;  /* SET TQN PARM FOR PCB MANAGER     02306000
*                                      TO COMMON I/O ACTIVE Q        */ 02307000
         LA    RLPARMR0,32                                         0789 02308000
*             END;                                                 0790 02309000
*           PCBVBN=MZERO;           /* SET THE VBN FIELD TO ZERO     */ 02310000
@RC00784 SLR   @14,@14                                             0791 02311000
         STH   @14,PCBVBN(,PCBPTR)                                 0791 02312000
*         END;                      /* END NON-SRB MODE PROCESSING   */ 02313000
*     END;                          /* END ALL RESET PROCESSING      */ 02314000
*                                                                  0794 02315000
*   /*****************************************************************/ 02316000
*   /*                                                               */ 02317000
*   /* IF RESET DOES NOT HAVE TO BE CALLED, THE PCB EITHER HAS A ROOT*/ 02318000
*   /* PCB OR REQUIRES NO SPECIAL PROCESSING. NO ROOT EXIT RTN'S CAN */ 02319000
*   /* BE CALLED BECAUSE OWNERSHIP OF THE LOCAL LOCK IS NOT KNOWN    */ 02320000
*   /*                                                               */ 02321000
*   /*****************************************************************/ 02322000
*                                                                  0794 02323000
*   ELSE                                                           0794 02324000
*     DO;                                                          0794 02325000
         B     @RC00761                                            0794 02326000
@RF00761 DS    0H                                                  0795 02327000
*       IF PCBROOT='1'B THEN        /* DOES PCB HAVE A ROOT          */ 02328000
         TM    PCBROOT(PCBPTR),B'00000100'                         0795 02329000
         BNO   @RF00795                                            0795 02330000
*         DO;                       /* YES                           */ 02331000
*           IF PCBRLOAD='0'B THEN   /* IS ROOT FOR A PGFIX           */ 02332000
         L     @14,PCBRPTR-1(,PCBPTR)                              0797 02333000
         LA    @14,0(,@14)                                         0797 02334000
         TM    PCBRLOAD(@14),B'00000100'                           0797 02335000
         BNZ   @RF00797                                            0797 02336000
*             DO;                   /* YES                           */ 02337000
*               PCBRFAIL='1'B;      /* SET THE ERROR FLAG ONLY IN  0799 02338000
*                                      ROOT PCB FOR PGFIX            */ 02339000
         OI    PCBRFAIL(@14),B'00001000'                           0799 02340000
*             END;                                                 0800 02341000
*           ELSE                                                   0801 02342000
*             ;                     /* ROOT FOR PGLOAD, DO NOTHING   */ 02343000
@RF00797 DS    0H                                                  0802 02344000
*           IF PCBRCNT-MONE=MZERO THEN/* WILL PCB COUNT GO TO ZERO   */ 02345000
         L     @14,PCBRPTR-1(,PCBPTR)                              0802 02346000
         LA    @14,0(,@14)                                         0802 02347000
         LH    @12,PCBRCNT(,@14)                                   0802 02348000
         BCTR  @12,0                                               0802 02349000
         LTR   @12,@12                                             0802 02350000
         BZ    @RT00802                                            0802 02351000
*             GOTO LBTRANS;         /* YES, PCB MUST BE PROCESSED BY    02352000
*                                      PIOCP SO THAT THE ROOT EXIT 0803 02353000
*                                      ROUTINE MAY BE CALLED IN THE     02354000
*                                      RIGHT ADDRESS SPACE WITH THE     02355000
*                                      LOCAL LOCK HELD.              */ 02356000
*           ELSE                    /* NO, DECREMENT PCB COUNT       */ 02357000
*             PCBRCNT=PCBRCNT-MONE;                                0804 02358000
         STH   @12,PCBRCNT(,@14)                                   0804 02359000
*         END;                      /* END ROOT PROCESSING           */ 02360000
*       ELSE                                                       0806 02361000
*         ;                         /* NO SPECIAL PROCESSING IS    0806 02362000
*                                      REQUIRED IF NEITHER THE RESET    02363000
*                                      NOR ROOT FLAGS ARE SET IN THE    02364000
*                                      PCB                           */ 02365000
@RF00795 DS    0H                                                  0807 02366000
*       RLPARMR0=PCBFREQN;          /* SET TQN PARM FOR PCB MANAGER     02367000
*                                      TO THE FREE Q FOR ALL PCBS  0807 02368000
*                                      THAT DON'T HAVE TO BE       0807 02369000
*                                      PROCESSED BY PIOCP            */ 02370000
         LA    RLPARMR0,16                                         0807 02371000
*     END;                          /* END NON-RESET PROCESSING      */ 02372000
*                                                                  0809 02373000
*   /*****************************************************************/ 02374000
*   /*                                                               */ 02375000
*   /* FOR EACH PCB, THE PCB MANAGER IS CALLED TO MOVE THE PCB TO THE*/ 02376000
*   /* PCB Q DETERMINED ABOVE AND INDICATED BY THE TQN IN REG 0 THE  */ 02377000
*   /* FOLLOWING MOVES THE IEAVRELS CSECT ID INTO THE PCB. THE BRANCH*/ 02378000
*   /* MUST BE NOPED WHEN THE TRACE IS ACTIVE                @Z40WPYD*/ 02379000
*   /*                                                               */ 02380000
*   /*****************************************************************/ 02381000
*                                                                  0809 02382000
*TRACEZR1:                                                         0809 02383000
*   GENERATE REFS(TRACEZR1,RLWRKRG7,PCBPTR,MYID);                  0809 02384000
@RC00761 DS    0H                                                  0809 02385000
TRACEZR1 DS    0H                                                  0809 02386000
         B  TRACEZR1+16                                                 02387000
         L  RLWRKRG7,64(PCBPTR)                                         02388000
*   RLWRKRG7=RLWRKRG7+MYID;                                        0810 02389000
         AL    RLWRKRG7,@CF02446                                   0810 02390000
*   GENERATE;                                                      0811 02391000
         ST RLWRKRG7,64(PCBPTR)                                         02392000
*   PCBGFAD='0'B;                   /* FLAGS IN THE PCB              */ 02393000
         NI    PCBGFAD(PCBPTR),B'10111111'                         0812 02394000
*   RLPARMRG=PCBPTR;                /* SET UP INPUT PCB PARAMETER    */ 02395000
         LR    RLPARMRG,PCBPTR                                     0813 02396000
*   PCBPTR=PCBRLPA;                 /* SAVE THE RELATED PTR IN THE 0814 02397000
*                                      CURRENT PCB                   */ 02398000
         L     @14,PCBRLPA-1(,PCBPTR)                              0814 02399000
         LA    @14,0(,@14)                                         0814 02400000
         LR    PCBPTR,@14                                          0814 02401000
*   CALL EPPCBMGR;                  /* CALL THE PCB MANAGER. -- SEE     02402000
*                                      ENTRY DCL FOR INTERFACE       */ 02403000
         L     @15,PVTPCB(,PVTPTR)                                 0815 02404000
         BALR  @14,@15                                             0815 02405000
*   IF PCBPTR^=MZERO THEN           /* DID A RELATED PCB EXIST       */ 02406000
         LTR   PCBPTR,PCBPTR                                       0816 02407000
         BZ    @RF00816                                            0816 02408000
*     DO;                           /* A RELATED PCB EXISTS TO BE  0817 02409000
*                                      PROCESSED AND MUST BE SET TO     02410000
*                                      APPEAR AS A NORMAL PCB FOR THE   02411000
*                                      CALL TO THE PCB MANAGER       */ 02412000
*       PCBFQP=RLINITRL;            /* SET CURRENT Q NUMBER TO THE 0818 02413000
*                                      UNQUEUED VALUE AND THE FORWARD   02414000
*                                      Q PTR FIELD TO ZERO           */ 02415000
         L     @14,RLINITRL                                        0818 02416000
         ST    @14,PCBFQP(,PCBPTR)                                 0818 02417000
*       GOTO LBPROCES;              /* PROCESS RELATED PCB           */ 02418000
         B     LBPROCES                                            0819 02419000
*     END;                                                         0820 02420000
*   ELSE                                                           0821 02421000
*     ;                             /* ONCE NO RELATED PCBS EXIST TO    02422000
*                                      BE PROCESSED, RETURN IS MADE     02423000
*                                      TO THE CALLER                 */ 02424000
@RF00816 DS    0H                                                  0822 02425000
*   RLRETREG=WSA5GFAQ;                                             0822 02426000
         L     RLRETREG,WSA5GFAQ(,PVTPTR)                          0822 02427000
*   END;                            /* END SUBGFADQ PROC             */ 02428000
@EL00007 DS    0H                                                  0823 02429000
@EF00007 DS    0H                                                  0823 02430000
@ER00007 BR    @14                                                 0823 02431000
*                                                                  0824 02432000
*   /*****************************************************************/ 02433000
*   /*                                                               */ 02434000
*   /* THE I/O ACTIVE Q PROCESSOR SUBROUTINE IS CALLED FROM THE Q    */ 02435000
*   /* SEARCH SUBROUTINE OR THE IEAVRELF ENTRY POINT AND HAS NO SAVE */ 02436000
*   /* AREA. INPUT IS THE ADDRESS OF A QUEUED PCB IN REGISTER 1, THE */ 02437000
*   /* XPTE ADDRESS INREGISTER 6, AND THE STANDARD RSM REGISTERS 2   */ 02438000
*   /* AND 3. IT MUST NOT DESTROY REGISTERS 2, 3, 4, 5, 6, 10, AND   */ 02439000
*   /* 11.                                                           */ 02440000
*   /*                                                               */ 02441000
*   /*****************************************************************/ 02442000
*                                                                  0824 02443000
*SUBIOACT:                                                         0824 02444000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0824 02445000
SUBIOACT DS    0H                                                  0825 02446000
*   RESPECIFY                                                      0825 02447000
*     RLRETREG RESTRICTED;                                         0825 02448000
*   PCBPTR=RLPARMRG;                /* GET ADDR OF INPUT PCB         */ 02449000
         LR    PCBPTR,RLPARMRG                                     0826 02450000
*   XPTPOINP='0'B;                  /* SET PAGE-OUT IN PROGRESS FLAG    02451000
*                                      OFF                   @Z40WPYD*/ 02452000
         NI    XPTPOINP(XPTEPTR),B'11011111'                       0827 02453000
*   DO WHILE(PCBPTR^=MZERO);        /* THE FREE REAL FLAG IS SET AND    02454000
*                                      THE VBN SET TO ZERO IN ALL  0828 02455000
*                                      PCBS EXCEPT PCBS WITH NON ZERO   02456000
*                                      FIX COUNTS                    */ 02457000
         B     @DE00828                                            0828 02458000
@DL00828 DS    0H                                                  0829 02459000
*     IF PCBFXC=MZERO THEN          /* IS PCB FOR A PGFIX            */ 02460000
         CLI   PCBFXC(PCBPTR),0                                    0829 02461000
         BNE   @RF00829                                            0829 02462000
*       DO;                         /* NO                            */ 02463000
*         PCBVBN=MZERO;             /* THE VBN IS ZEROED TO PREVENT     02464000
*                                      DUPLICATE PCB'S FROM APPEARING   02465000
*                                      ON AN I/O ACTIVE QUEUE.       */ 02466000
         SLR   @12,@12                                             0831 02467000
         STH   @12,PCBVBN(,PCBPTR)                                 0831 02468000
*         PCBFREAL='1'B;            /* FRAME WILL BE FREED WHEN I/O     02469000
*                                      COMPLETES.                    */ 02470000
         OI    PCBFREAL(PCBPTR),B'10000000'                        0832 02471000
*       END;                                                       0833 02472000
*     PCBPTR=PCBRLPA;               /* GET RELATED PTR IN CURRENT PCB*/ 02473000
@RF00829 L     @12,PCBRLPA-1(,PCBPTR)                              0834 02474000
         LA    @12,0(,@12)                                         0834 02475000
         LR    PCBPTR,@12                                          0834 02476000
*   END;                            /* END DO WHILE                  */ 02477000
@DE00828 LTR   PCBPTR,PCBPTR                                       0835 02478000
         BNZ   @DL00828                                            0835 02479000
*   END;                            /* END SUBIOACT PROC             */ 02480000
@EL00008 DS    0H                                                  0836 02481000
@EF00008 DS    0H                                                  0836 02482000
@ER00008 BR    @14                                                 0836 02483000
*   END                             /* END IEAVRELS PROCEDURE        */ 02484000
*                                                                  0837 02485000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 02486000
*/*%INCLUDE SYSLIB  (ILRAIA  )                                       */ 02487000
*/*%INCLUDE SYSLIB  (IHAPSA  )                                       */ 02488000
*/*%INCLUDE SYSLIB  (CVT     )                                       */ 02489000
*/*%INCLUDE SYSLIB  (IHAASCB )                                       */ 02490000
*/*%INCLUDE SYSLIB  (IKJTCB  )                                       */ 02491000
*/*%INCLUDE SYSLIB  (ILRASMVT)                                       */ 02492000
*/*%INCLUDE SYSLIB  (IHAPVT  )                                       */ 02493000
*/*%INCLUDE SYSLIB  (IHARSMHD)                                       */ 02494000
*/*%INCLUDE SYSLIB  (IHAPFTE )                                       */ 02495000
*/*%INCLUDE SYSLIB  (IEACIWA )                                       */ 02496000
*/*%INCLUDE SYSLIB  (IHAVSL  )                                       */ 02497000
*/*%INCLUDE SYSLIB  (IHASGTE )                                       */ 02498000
*/*%INCLUDE SYSLIB  (IHAPGTE )                                       */ 02499000
*/*%INCLUDE SYSLIB  (IHAXPTE )                                       */ 02500000
*/*%INCLUDE SYSLIB  (IHAPCB  )                                       */ 02501000
*/*%INCLUDE SYSLIB  (IHAFOE  )                                       */ 02502000
*/*%INCLUDE SYSLIB  (IHAPCBR )                                       */ 02503000
*/*%INCLUDE SYSLIB  (IHARCA  )                                       */ 02504000
*/*%INCLUDE SYSLIB  (IHAFRRS )                                       */ 02505000
*                                                                  0837 02506000
*       ;                                                          0837 02507000
@DATA    DS    0H                                                       02508000
@CH02428 DC    XL2'FFFF'                                                02509000
@DATD    DSECT                                                          02510000
         DS    0F                                                       02511000
IEAVRELS CSECT                                                          02512000
         DS    0F                                                       02513000
@CF00081 DC    F'2'                                                     02514000
@CF00039 DC    F'8'                                                     02515000
@CF02411 DC    F'4096'                                                  02516000
@CF02422 DC    XL4'FFFFFFF0'                                            02517000
@CF02424 DC    XL4'00FFFFF8'                                            02518000
@CF02426 DC    XL4'FFFFF000'                                            02519000
@CF02446 DC    XL4'00000001'                                            02520000
@CF02622 DC    XL4'0000FFFF'                                            02521000
@DATD    DSECT                                                          02522000
         DS    0D                                                       02523000
@ENDDATD EQU   *                                                        02524000
IEAVRELS CSECT                                                          02525000
         NOPR  ((@ENDDATD-@DATD)*16)                                    02526000
         DS    0F                                                       02527000
@SIZDATD DC    AL1(0)                                                   02528000
         DC    AL3(@ENDDATD-@DATD)                                      02529000
@CV02454 DC    V(MRELEASR)                                              02530000
@CV02455 DC    V(NEXTVSL)                                               02531000
         DS    0D                                                       02532000
RLINITRL DS    FL4                                                      02533000
         ORG   RLINITRL                                                 02534000
RLNOQN   DC    X'FF'                                                    02535000
RLZERPTR DC    AL3(0)                                                   02536000
         ORG   RLINITRL+4                                               02537000
