*           R14=R3;                                                0549 01456000
         LR    R14,R3                                              0549 01457000
*           DO;                     /* FREEMAIN(RU) LV(R0)A(FREESTRG)   01458000
*                                      SP(R14) BRANCH                */ 01459000
*             RESPECIFY                                            0551 01460000
*              (GPR01F,                                            0551 01461000
*               GPR03F,                                            0551 01462000
*               GPR00F) RESTRICTED;                                0551 01463000
*             GPR01F=0;             /* REG 1 MUST BE ZERO            */ 01464000
         SLR   GPR01F,GPR01F                                       0552 01465000
*             GPR03F=3;             /* SET TYPE                      */ 01466000
         LA    GPR03F,3                                            0553 01467000
*             GPR00F=R0;            /* LENGTH REQUESTED              */ 01468000
*             GPR01F=ADDR(FREESTRG);/* SET REGISTER ONE              */ 01469000
         LR    GPR01F,R11                                          0555 01470000
