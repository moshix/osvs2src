*               TFRRTQEP=TQEPTR;    /* SAVE TQE PTR FOR FRR          */ 00233000
         ST    TQEPTR,TFRRTQEP(,TFRRPTR)                           0192 00234000
*               PCCATQEP=0;         /* INDICATE THAT THIS CPU'S CLK     00235000
*                                      COMP IS NOT TIMING A TQE.     */ 00236000
         L     @12,PCCAPTR                                         0193 00237000
