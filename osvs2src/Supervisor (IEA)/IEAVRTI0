*                (R2) UNRSTD;                                      0188 00222000
*CCRET1:                                                           0189 00223000
*             END;                                                 0189 00224000
*           ELSE                    /* IF CPU WAS TIMING A TQE,      */ 00225000
*             DO;                   /* PROCESS THE TQE.              */ 00226000
         B     @RC00178                                            0190 00227000
@RF00178 DS    0H                                                  0191 00228000
*               TQEPTR=TPCHDCCQ;    /* SET UP TQE POINTER            */ 00229000
         L     @12,CVTPTR                                          0191 00230000
