          STM   0,15,TPCSAVE1(13)                                       00026000
          LA    BASEREG,IEAVRMEP                                        00027000
          B     EXDEQTQE                                                00028000
          SPACE 2                                                       00029000
          ENTRY IEAVRQCK                                                00030000
IEAVRQCK  BALR  15,0              RQCHECK ENTRY POINT                   00031000
          USING *,15                                                    00032000
          LA    BASEREG,IEAVRMEP                                        00033000
          B     RQCHECK                                                 00034000
          SPACE 2                                                       00035000
          ENTRY IEAVRCKQ                                                00036000
IEAVRCKQ  BALR  15,0               SET CLOCK COMP ENTRY POINT           00037000
