         L     @12,CVTCSD(,@12)                                    0566 01576000
         L     @12,CSDGDCC(,@12)                                   0566 01577000
         LTR   @12,@12                                             0566 01578000
         BZ    @RF00566                                            0566 01579000
*             DO;                                                  0567 01580000
*               RESPECIFY                                          0568 01581000
*                (R2) RSTD;         /* CALL SET CC RTN               */ 01582000
*               R2=ADDR(CCRET2);    /* TO ENSURE TOP                 */ 01583000
         LA    R2,CCRET2                                           0569 01584000
*               CALL SETCC;         /* TQE IN QUE IS                 */ 01585000
         BAL   @14,SETCC                                           0570 01586000
*               RESPECIFY                                          0571 01587000
*                (R2) UNRSTD;       /* BEING TIMED.                  */ 01588000
*CCRET2:                                                           0572 01589000
*             END;                                                 0572 01590000
CCRET2   DS    0H                                                  0573 01591000
*         END;                                                     0573 01592000
@RF00566 DS    0H                                                  0574 01593000
*       TQEINCOM='0'B;              /* INDICATE INTV NOT COMPLETE    */ 01594000
@RC00526 NI    TQEINCOM(TQEPTR),B'11110111'                        0574 01595000
*       RESPECIFY                                                  0575 01596000
*        (FRRPTR,                                                  0575 01597000
*         FRRSPTR,                                                 0575 01598000
*         FRREPTR) RSTD;                                           0575 01599000
*       GENERATE;                                                  0576 01600000
*                                     /* DELETE FRR COVERAGE        */  01601000
              SETFRR  D,WRKREGS=(FRRSPTR,FRREPTR),                    CC01602000
               RELATED=('TQE ENQ',IEAVRTI0(NONE))                       01603000
*     END;                                                         0577 01604000
*   R13=CVTTPC;                     /* SET R13 TO PT TO TPC          */ 01605000
@RF00519 L     @15,CVTPTR                                          0578 01606000
         L     R13,CVTTPC(,@15)                                    0578 01607000
