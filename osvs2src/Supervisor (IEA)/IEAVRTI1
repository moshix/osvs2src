@RF00171 DS    0H                                                  0214 00216000
*       RESPECIFY                                                  0214 00217000
*        (R11,                                                     0214 00218000
*         R12,                                                     0214 00219000
*         R13,                                                     0214 00220000
*         R14) RSTD;                                               0214 00221000
*       GENERATE;                                                  0215 00222000
*                                     /* GET DISPATCHER LOCK        */  00223000
          SETLOCK OBTAIN,TYPE=DISP,MODE=UNCOND,                       CC00224000
               RELATED=('TQE QUEUE',IEAVRTI1(NONE))                     00225000
*       RESPECIFY                                                  0216 00226000
*        (R11,                                                     0216 00227000
*         R12,                                                     0216 00228000
*         R13,                                                     0216 00229000
*         R14) UNRSTD;                                             0216 00230000
*       IF TCBJSTCB=RMPLTCBA        /* IF TCBJST POINTS TO SELF      */ 00231000
*         THEN                      /* ITS JOB STEP TCB OR HIGHER    */ 00232000
         L     @12,RMPLPTR(,R10)                                   0217 00233000
         L     @11,RMPLTCBA(,@12)                                  0217 00234000
         C     @11,TCBJSTCB(,@11)                                  0217 00235000
         BNE   @RF00217                                            0217 00236000
*         DO;                       /* TURN OFF TIME LIMIT CHECKING     00237000
*                                      OF THIS MEMORY BY SETTING BIT    00238000
*                                      ASCBTOFF TO 1 AND BIT ASCBCEXT   00239000
*                                      TO 1.                         */ 00240000
*           RESPECIFY                                              0219 00241000
*            (R11,                                                 0219 00242000
*             R12,                                                 0219 00243000
