*       RESPECIFY                                                  0185 00133000
*        (TPCPTR) UNRSTD;                                          0185 00134000
*       RESPECIFY                                                  0186 00135000
*        (NEXTTQE) RSTD;                                           0186 00136000
*       DO WHILE TQEDUM='0'B;       /* SEARCH FOR MEMORY'S TQES      */ 00137000
         B     @DE00187                                            0187 00138000
@DL00187 DS    0H                                                  0188 00139000
*         NEXTTQE=TQEFLNK;          /* POINT TO NEXT TQE IN QUEUE    */ 00140000
         L     NEXTTQE,TQEFLNK(,TQEPTR)                            0188 00141000
*         IF TQEUSER='1'B           /* IF NOT A SYSTEM TQE           */ 00142000
*           THEN                    /* TQE CAN BELONG TO MEMORY      */ 00143000
         TM    TQEUSER(TQEPTR),B'01000000'                         0189 00144000
         BNO   @RF00189                                            0189 00145000
*           IF TQEAID=RMPLASID      /* IF TQE BELONGS TO MEMORY,     */ 00146000
*             THEN                  /* DEQUE IT AND FREE IT.         */ 00147000
