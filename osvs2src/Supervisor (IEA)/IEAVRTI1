         CLC   TQEAID(2,TQEPTR),RMPLASID(@12)                      0190 00149000
         BNE   @RF00190                                            0190 00150000
*             DO;                                                  0191 00151000
*               RESPECIFY                                          0192 00152000
*                (R2) RSTD;                                        0192 00153000
*               R2=ADDR(DEQRET1);                                  0193 00154000
         LA    R2,DEQRET1                                          0193 00155000
*               CALL DEQTQE;        /* FIRST, DEQUEUE THE TQE        */ 00156000
