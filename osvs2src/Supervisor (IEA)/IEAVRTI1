*             GENERATE              /* FREE IT FROM SQA (SP245)      */ 00325000
*                 (ICM  R0,8,SQA);                                 0240 00326000
         B     @RC00238                                            0240 00327000
@RF00238 DS    0H                                                  0240 00328000
         ICM  R0,8,SQA                                                  00329000
*           GENERATE                /* THIRD, FREE TQE,PURGE SRBS    */ 00330000
*               (FREEMAIN R,LV=(0),A=(1));                         0241 00331000
@RC00238 DS    0H                                                  0241 00332000
         FREEMAIN R,LV=(0),A=(1)                                        00333000
*         END;                                                     0242 00334000
*       ELSE                        /* HERE IF TCB HAS NO TQE        */ 00335000
*         DO;                       /* RELEASE THE DISP LOCK         */ 00336000
         B     @RC00226                                            0243 00337000
@RF00226 DS    0H                                                  0244 00338000
