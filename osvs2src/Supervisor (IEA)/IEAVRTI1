@RF00306 DS    0H                                                  0311 00552000
*               R10=1;              /* SET IEAVRCLS NOT NEEDED       */ 00553000
         LA    R10,1                                               0311 00554000
*             END;                                                 0312 00555000
*           ELSE                    /* HERE IF VALID MCH CHECK       */ 00556000
*             DO;                                                  0313 00557000
         B     @RC00298                                            0313 00558000
@RF00298 DS    0H                                                  0314 00559000
*               IF LRBMITOD='1'B    /* IF TOD CLOCK                  */ 00560000
*                 THEN              /* PROCESS BY...                 */ 00561000
