         L     R14,RMPLASCB(,@08)                                  0178 00103000
*       GENERATE REFS(ASCBAFFN,ASCBSWCT,ASCBFLG1,ASCBFLG2);        0179 00104000
      LA  11,X'80'                                                      00105000
      SLL 11,24-(8*((ASCBFLG1-ASCBAFFN)-(((ASCBFLG1-ASCBAFFN)/4)*4)))   00106000
      L   12,ASCBAFFN+(((ASCBFLG1-ASCBAFFN)/4)*4)(R14)                  00107000
TOFFM LR  13,12                                                         00108000
      OR  13,11                                                         00109000
      CS  12,13,ASCBAFFN+(((ASCBFLG1-ASCBAFFN)/4)*4)(R14)               00110000
      BNZ TOFFM                           *                             00111000
