*     GPR01P->SDWARCRD='1'B;        /* TURN ON RECORD INDICATOR      */ 02271000
         OI    SDWARCRD(GPR01P),B'10000000'                        0846 02272000
*     RESPECIFY                                                    0847 02273000
*      (GPR00P,                                                    0847 02274000
*       GPR01P,                                                    0847 02275000
*       GPR14P,                                                    0847 02276000
*       GPR15P) UNRSTD;                                            0847 02277000
*   END;                                                           0848 02278000
*   GENERATE;                                                      0849 02279000
       LM    14,12,12(R13)             RESTORE ALL REGISTERS            02280000
       BR    R14                       RETURN                           02281000
*   RESPECIFY                                                      0850 02282000
*    (R0,                                                          0850 02283000
*     R13) UNRSTD;                                                 0850 02284000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 02285000
*     GPR00P PTR(31) REG(0),                                       0851 02286000
*     GPR01P PTR(31) REG(1),                                       0851 02287000
*     GPR14P PTR(31) REG(14),                                      0851 02288000
