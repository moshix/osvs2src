*                     DO;           /* BEING TIMED.                  */ 00590000
*                       PCCATQEP->TQECOMP='0'B;                    0325 00591000
         NI    TQECOMP(@11),B'01111111'                            0325 00592000
*                       PCCATQEP=0;                                0326 00593000
         ST    @12,PCCATQEP(,PCCAPTR)                              0326 00594000
*                     END;                                         0327 00595000
*                 END;                                             0328 00596000
*               ELSE                /* HERE IF NOT TODC              */ 00597000
*                 DO;                                              0329 00598000
         B     @RC00314                                            0329 00599000
@RF00314 DS    0H                                                  0330 00600000
*                   IF LRBMICKC='1'B/* IF CLOCK COMP,                */ 00601000
*                     THEN          /* PROCESS BY...                 */ 00602000
         TM    LRBMICKC(LRBPTR),B'00000100'                        0330 00603000
         BNO   @RF00330                                            0330 00604000
*                     DO;                                          0331 00605000
*                       PCCANUCC='1'B;/* MARK CC UNUSABLE            */ 00606000
         OI    PCCANUCC(PCCAPTR),B'10000000'                       0332 00607000
*                       IF PCCATQEP^=0/* IF CPU TMG A TQE            */ 00608000
*                         THEN      /* CLEAR INDICATORS              */ 00609000
         SLR   @12,@12                                             0333 00610000
