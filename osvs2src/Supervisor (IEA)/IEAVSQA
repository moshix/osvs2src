*     PARM,                                                        0091 00118000
*     REG15);                       /*                       @ZA16816*/ 00119000
*   PASS=1;                         /* INDICATE PASS 1       @ZA16816*/ 00120000
         LA    PASS,1                                              0092 00121000
*   PARM=0;                         /* INDICATE STEAL OPTION @ZA16816*/ 00122000
         SLR   PARM,PARM                                           0093 00123000
*   CALL IEAVPREF;                  /* ATTEMPT TO GET A FRAME      0094 00124000
*                                                            @ZA16816*/ 00125000
         L     @15,PVTPPREF(,PVTPTR)                               0094 00126000
         BALR  @14,@15                                             0094 00127000
*   PFTEPTR=REG0;                   /* GET PFTE PTR          @ZA16816*/ 00128000
         LR    PFTEPTR,REG0                                        0095 00129000
