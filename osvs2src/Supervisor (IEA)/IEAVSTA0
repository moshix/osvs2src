*     DO;                                                          0440 01242000
*       SCBBRNTR='1'B;              /* RESTORE BRANCH AREA INDICATOR */ 01243000
         OI    SCBBRNTR(R2),B'00000100'                            0441 01244000
*     END;                                                         0442 01245000
*   R14=WORK1;                      /* RESTORE RETURN ADDRESS        */ 01246000
@RF00439 L     R14,WORK1(,R9)                                      0443 01247000
*   RETURN;                         /* RETURN                        */ 01248000
@EL00006 DS    0H                                                  0444 01249000
@EF00006 DS    0H                                                  0444 01250000
@ER00006 BR    @14                                                 0444 01251000
*   END OVERLAY;                                                   0445 01252000
*CREATE:                                                           0446 01253000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0446 01254000
*                                                                  0446 01255000
CREATE   DS    0H                                                  0447 01256000
*   /*****************************************************************/ 01257000
*   /*                                                               */ 01258000
*   /* THIS ROUTINE IS ENTERED TO CREATE AND ENQUEUE AN SCB OBTAINED */ 01259000
*   /* FROM THE FREESCB QUEUE IN SP 255. IF THE REQUEST IS FOR STAI  */ 01260000
*   /* OR ESTAI CONTROL IS THEN ROUTED TO THE PROPA ROUTINE REGISTERS*/ 01261000
*   /* UPON ENTRY 1- ADDRESS OF PARAMETER LIST 4- CURRENT TCB ADDRESS*/ 01262000
*   /* 5- CURRENT RB ADDRESS 14- RETURN ADDRESS                      */ 01263000
*   /*                                                               */ 01264000
*   /*****************************************************************/ 01265000
*                                                                  0447 01266000
*   WORK1=R14;                      /* SAVE RETURN ADDRESS           */ 01267000
         ST    R14,WORK1(,R9)                                      0447 01268000
*   DO;                                                            0448 01269000
*                                                                  0448 01270000
*     /***************************************************************/ 01271000
*     /*                                                             */ 01272000
*     /* CHECK FOR FREE SCB TO USE                                   */ 01273000
*     /*                                                             */ 01274000
*     /***************************************************************/ 01275000
*                                                                  0449 01276000
*TRYA:                                                             0449 01277000
*     R2=FREESCBQ;                                                 0449 01278000
TRYA     L     R2,FREESCBQ(,R7)                                    0449 01279000
*     IF R2=0 THEN                  /* FREE SCB?             @Z30EPXA*/ 01280000
         LTR   R2,R2                                               0450 01281000
         BNZ   @RF00450                                            0450 01282000
*       DO;                                                        0451 01283000
*         CALL GETSTOR;             /* GET STORAGE FOR SCB   @Z30EPXA*/ 01284000
         BAL   @14,GETSTOR                                         0452 01285000
*         GOTO TRYA;                /* RETRY CELL OBTAIN     @Z40FPXA*/ 01286000
         B     TRYA                                                0453 01287000
*       END;                                                       0454 01288000
*     R3=SCBCHAIN;                  /* PICK UP LINK FIELD    @Z40FPXA*/ 01289000
@RF00450 L     R3,SCBCHAIN(,R2)                                    0455 01290000
*     CS(R2,R3,FREESCBQ);           /* DEQUEUE FROM CHAIN    @Z40FPXA*/ 01291000
         CS    R2,@03,FREESCBQ(R7)                                 0456 01292000
*     BC(7,TRYA);                   /* GO TRY OBTAIN AGAIN   @Z40FPXA*/ 01293000
         BC    7,TRYA                                              0457 01294000
*   END;                                                           0458 01295000
*   DO;                                                            0459 01296000
*     SCBFLGS2=0;                   /* CLEAR FLAG BYTE IN SCB      0460 01297000
*                                                            @Z40FPXA*/ 01298000
         MVI   SCBFLGS2(R2),X'00'                                  0460 01299000
*     CALL COMPLSCB;                /* BUILD COMMON SECTION OF SCB   */ 01300000
         BAL   @14,COMPLSCB                                        0461 01301000
*     DO;                                                          0462 01302000
*       IF ESTASTAI='1'B            /* STAI OR ESTAI ?               */ 01303000
*         THEN                      /* YES                           */ 01304000
         TM    ESTASTAI(R1),B'10000000'                            0463 01305000
         BNO   @RF00463                                            0463 01306000
*         DO;                                                      0464 01307000
*           SCBCHAIN=0;             /* CLEAR LINK FIELD      @Z40FPXA*/ 01308000
         SLR   @12,@12                                             0465 01309000
         ST    @12,SCBCHAIN(,R2)                                   0465 01310000
*           SCBOWNRA=TCBPTR;        /* THE CURRENT TCB IS OWNER      */ 01311000
         STCM  TCBPTR,7,SCBOWNRA(R2)                               0466 01312000
*           ESTAOWNR->TCBSTABB=R2;  /* ENQUEUE ON NEW TCB            */ 01313000
         L     @12,ESTAOWNR(,R1)                                   0467 01314000
         STCM  R2,7,TCBSTABB(@12)                                  0467 01315000
*           CALL PROPA;             /* COPY MOMS SCB Q               */ 01316000
         BAL   @14,PROPA                                           0468 01317000
*         END;                                                     0469 01318000
*       ELSE                                                       0470 01319000
*         DO;                                                      0470 01320000
         B     @RC00463                                            0470 01321000
@RF00463 DS    0H                                                  0471 01322000
*           IF ESTASTAR='0'B        /* ESTAR REQUEST                 */ 01323000
*             THEN                  /* NO                            */ 01324000
         TM    ESTASTAR(R1),B'01000000'                            0471 01325000
         BNZ   @RF00471                                            0471 01326000
*             DO;                   /* SET OWNER ADDRESS             */ 01327000
*               RBSECPTR=RBLINK;                                   0473 01328000
         L     RBSECPTR,RBLINK(,RBSECPTR)                          0473 01329000
*               SCBOWNRA=RBSECPTR;                                 0474 01330000
         STCM  RBSECPTR,7,SCBOWNRA(R2)                             0474 01331000
*               RBSCB='1'B;                                        0475 01332000
         LR    @12,RBSECPTR                                        0475 01333000
         AL    @12,@CF02220                                        0475 01334000
         OI    RBSCB-56(@12),B'00000010'                           0475 01335000
*             END;                                                 0476 01336000
*           ELSE                    /* CLEAR SCBOWNRA FIELD FOR ESTAR   01337000
*                                                            @Z40FPXA*/ 01338000
*             SCBOWNRA=0;           /* CLEAR FIELD           @Z40FPXA*/ 01339000
         B     @RC00471                                            0477 01340000
@RF00471 SLR   @12,@12                                             0477 01341000
         STCM  @12,7,SCBOWNRA(R2)                                  0477 01342000
*           SCBCHAIN=TCBSTABB;      /* POINT TO NEXT SCB             */ 01343000
@RC00471 L     @12,TCBSTABB-1(,TCBPTR)                             0478 01344000
         LA    @12,0(,@12)                                         0478 01345000
         ST    @12,SCBCHAIN(,R2)                                   0478 01346000
*           TCBSTABB=R2;            /* PLACE SCB ON QUEUE            */ 01347000
         STCM  R2,7,TCBSTABB(TCBPTR)                               0479 01348000
*         END;                                                     0480 01349000
*     END;                                                         0481 01350000
@RC00463 DS    0H                                                  0482 01351000
*   END;                                                           0482 01352000
*CREATEXT:                                                         0483 01353000
*   R14=WORK1;                      /* RESTORE REG 14        @Z30EPXA*/ 01354000
CREATEXT L     R14,WORK1(,R9)                                      0483 01355000
*   RETURN;                                                        0484 01356000
@EL00007 DS    0H                                                  0484 01357000
@EF00007 DS    0H                                                  0484 01358000
@ER00007 BR    @14                                                 0484 01359000
*   END CREATE;                                                    0485 01360000
*COMPLSCB:                                                         0486 01361000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0486 01362000
*                                                                  0486 01363000
COMPLSCB DS    0H                                                  0487 01364000
*   /*****************************************************************/ 01365000
*   /*                                                               */ 01366000
*   /* THIS ROUTINE OBTAINS THE KEY AND STATE OF THE INVOKER FOR THIS*/ 01367000
*   /* REQUEST-IF SVC 60 IS BRANCH ENTERED THE INVOKER IS IN KEY 0   */ 01368000
*   /* SUPERVISOR STATE. FOR SVC ENTRY THE KEY AND STATE ARE OBTAINED*/ 01369000
*   /* FROM THE RBOPSW FIELD FROM THE INVOKING RB                    */ 01370000
*   /*                                                               */ 01371000
*   /*****************************************************************/ 01372000
*                                                                  0487 01373000
*   IF ESTAESTA='1'B THEN           /* IF ESTA REQUEST THEN  @ZA15766*/ 01374000
         TM    ESTAESTA(R1),B'00010000'                            0487 01375000
         BNO   @RF00487                                            0487 01376000
*     SCBFLGS3=ESTAFLG2;            /* COPY FLAGS FROM ESTA PARM LIST   01377000
*                                                            @Z40FPXA*/ 01378000
         MVC   SCBFLGS3(1,R2),ESTAFLG2(R1)                         0488 01379000
*   ELSE                            /* IF STA REQUEST THEN   @ZA15766*/ 01380000
*     SCBFLGS3=''B;                 /* SET ESTA FLAGS TO ZERO      0489 01381000
*                                                            @ZA15766*/ 01382000
         B     @RC00487                                            0489 01383000
@RF00487 MVI   SCBFLGS3(R2),X'00'                                  0489 01384000
*   IF BRENTFLG='1'B                /* BRANCH ENTERED?       @Z40FPXA*/ 01385000
*     THEN                          /* YES                           */ 01386000
@RC00487 TM    BRENTFLG(R9),B'00000100'                            0490 01387000
         BNO   @RF00490                                            0490 01388000
*     DO;                                                          0491 01389000
*       SCBKEY0='1'B;               /* INDICATE CONTROL PROGRAM KEY  */ 01390000
*       SCBSUPER='1'B;              /* INDICATE SUPERVISOR STATE     */ 01391000
         OI    SCBKEY0(R2),B'00000011'                             0493 01392000
*       SCBPKEY='00'X;              /* KEY VALUE IS ZERO             */ 01393000
         MVI   SCBPKEY(R2),X'00'                                   0494 01394000
*     END;                                                         0495 01395000
*   ELSE                            /* SVC ENTERED                   */ 01396000
*     DO;                                                          0496 01397000
         B     @RC00490                                            0496 01398000
@RF00490 DS    0H                                                  0497 01399000
*       IF ESTASTAI='1'B            /* FOR STAI OR ESTAI THE INVOKER    01400000
*                                      IS THE THIRD RB ON THE QUEUE  */ 01401000
*         THEN                                                     0497 01402000
         TM    ESTASTAI(R1),B'10000000'                            0497 01403000
         BNO   @RF00497                                            0497 01404000
*         DO;                                                      0498 01405000
*           R3=RBLINK->RBLINK;      /* OBTAIN INVOKERS RB ADD      0499 01406000
*                                                            @Z40FPXA*/ 01407000
