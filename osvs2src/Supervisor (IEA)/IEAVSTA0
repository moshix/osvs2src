@RF00137 DS    0H                                                  0146 00248000
*     END;                                                         0146 00249000
*                                                                  0146 00250000
@RC00103 DS    0H                                                  0147 00251000
*   /*****************************************************************/ 00252000
*   /*                                                               */ 00253000
*   /* THIS SECTION OF CODE DETERMINES IF AN ESTAE CANCEL/OVERLAY OR */ 00254000
*   /* STAE CANCEL/OV HAS BEEN REQUESTED AND ROUTES CONTROL TO       */ 00255000
*   /* SPECIFIC VALIDITY ROUTINES                                    */ 00256000
*   /*                                                               */ 00257000
*   /*****************************************************************/ 00258000
*                                                                  0147 00259000
*   IF CANCELFL='1'B|FC=8           /* ANY CANCEL OR OVERLAY       0147 00260000
*                                      REQUESTED ?           @Z30EPXA*/ 00261000
*     THEN                          /* YES                           */ 00262000
@RC00089 TM    CANCELFL(R9),B'00001000'                            0147 00263000
         BO    @RT00147                                            0147 00264000
         CLI   FC(R9),8                                            0147 00265000
         BNE   @RF00147                                            0147 00266000
@RT00147 DS    0H                                                  0148 00267000
*     DO;                                                          0148 00268000
*       CALL SCBSRCH;               /* FIND LAST VALID SCB   @Z40FPXA*/ 00269000
         BAL   @14,SCBSRCH                                         0149 00270000
*       IF FC='84'X THEN            /* ESTAE CANCEL?         @Z40FPXA*/ 00271000
         CLI   FC(R9),X'84'                                        0150 00272000
         BNE   @RF00150                                            0150 00273000
*         DO;                                                      0151 00274000
*           CALL ESTCOVAL;          /* VERIFY ESTAE CANCEL REQUEST 0152 00275000
*                                                            @Z40FPXA*/ 00276000
         BAL   @14,ESTCOVAL                                        0152 00277000
*           GOTO PERFCAN;           /* PROCESS CANCEL REQUEST      0153 00278000
*                                                            @Z40FPXA*/ 00279000
         B     PERFCAN                                             0153 00280000
*         END;                                                     0154 00281000
*       IF FC^=4&ESTAESTA='1'B      /* ESTA REQUEST?         @ZM41808*/ 00282000
*         THEN                      /* YES                           */ 00283000
@RF00150 CLI   FC(R9),4                                            0155 00284000
         BE    @RF00155                                            0155 00285000
         TM    ESTAESTA(R1),B'00010000'                            0155 00286000
         BNO   @RF00155                                            0155 00287000
*         DO;                                                      0156 00288000
*           IF ESTASTAI='0'B&ESTASTAR='0'B/* ESTAE REQUEST ?         */ 00289000
*             THEN                  /* YES                           */ 00290000
         TM    ESTASTAI(R1),B'11000000'                            0157 00291000
         BNZ   @RF00157                                            0157 00292000
*             DO;                                                  0158 00293000
*               CALL ESTCOVAL;      /* VERIFY ESTAE CAN/OV REQ       */ 00294000
         BAL   @14,ESTCOVAL                                        0159 00295000
*             END;                                                 0160 00296000
*         END;                                                     0161 00297000
*       ELSE                        /* STAE REQUESTED                */ 00298000
*         DO;                                                      0162 00299000
*                                                                  0162 00300000
         B     @RC00155                                            0162 00301000
@RF00155 DS    0H                                                  0163 00302000
*           /*********************************************************/ 00303000
*           /*                                                       */ 00304000
*           /* IT HAS BEEN DETERMINED THAT THIS REQUEST IS EITHER A  */ 00305000
*           /* STAE CANCEL OR OVERLAY TYPE OF REQUEST-IN ORDER FOR   */ 00306000
*           /* THIS REQUEST TO BE VALID THE LAST VALID SCB POINTED TO*/ 00307000
*           /* BY REGISTER 2 MUST BE A STAE SCB AND OWNED BY THE     */ 00308000
*           /* INVOKING RB                                           */ 00309000
*           /*                                                       */ 00310000
*           /*********************************************************/ 00311000
*                                                                  0163 00312000
*           IF R2=0                 /* ANY SCB Q AT ALL TO CHECK ?   */ 00313000
*             THEN                  /* NO-INVALID                    */ 00314000
         LTR   R2,R2                                               0163 00315000
         BZ    @RT00163                                            0163 00316000
*             GOTO RETCOD8;         /* STAE RETURN CODE IS 8 @Z40FPXA*/ 00317000
*           IF SCBESTAE='0'B&SCBSTAI='1'B/* IS THIS A STAI SCB?      */ 00318000
*             THEN                  /* YES-INVALID                   */ 00319000
         TM    SCBSTAI(R2),B'10000000'                             0165 00320000
         BNO   @RF00165                                            0165 00321000
         TM    SCBESTAE(R2),B'00010000'                            0165 00322000
         BZ    @RT00165                                            0165 00323000
*             GOTO RETCOD8;         /* STAE RETURN CODE IS 8 @Z40FPXA*/ 00324000
*           IF SCBESTAE='1'B        /* IS IT A ESTA TYPE             */ 00325000
*             THEN                  /* YES                           */ 00326000
@RF00165 TM    SCBESTAE(R2),B'00010000'                            0167 00327000
         BO    @RT00167                                            0167 00328000
*             GOTO RETCOD10;        /* RETN CD FOR STAE REQ WITH ESTA   00329000
*                                      SCB                   @Z40FPXA*/ 00330000
*           IF SCBOWNRA^=RBLINKB    /* DOES THE INVOKER OWN IT       */ 00331000
*             THEN                  /* NO                            */ 00332000
         CLC   SCBOWNRA(3,R2),RBLINKB(RBSECPTR)                    0169 00333000
         BNE   @RT00169                                            0169 00334000
*             GOTO RETCOD10;        /* SCB NOT OWNED BY INVOKER    0170 00335000
*                                                            @Z40FPXA*/ 00336000
*         END;                                                     0171 00337000
*     END;                                                         0172 00338000
@RC00155 DS    0H                                                  0173 00339000
*   DO;                                                            0173 00340000
@RF00147 DS    0H                                                  0174 00341000
*     IF FC=4 THEN                  /* IF STAE CANCEL        @ZM41806*/ 00342000
         CLI   FC(R9),4                                            0174 00343000
         BE    @RT00174                                            0174 00344000
*       GOTO PERFCAN;               /* THEN PROCESS          @ZM41806*/ 00345000
*     IF ESTASTAR='1'B              /* ESTAR REQUEST ?               */ 00346000
*       THEN                        /* YES                           */ 00347000
         TM    ESTASTAR(R1),B'01000000'                            0176 00348000
         BNO   @RF00176                                            0176 00349000
*       DO;                                                        0177 00350000
*                                                                  0177 00351000
*/*  ****************************************************************   00352000
*                                                                  0178 00353000
*     ESTAR REQUESTS ARE VERIFIED BY                               0178 00354000
*                1- INSURING THAT THE INVOKER IS AUTHORIZED        0178 00355000
*                   VIA APF OR IS IN KEY 0 OR IS IN SUPERVISOR     0178 00356000
*                   STATE                                          0178 00357000
*                2- IF CREATE IS REQUESTED THERE MUST NOT BE       0178 00358000
*                   ANY SCBS ON THE QUEUE                          0178 00359000
*                3- IF OVERLAY IS REQUESTED THE LAST VALID SCB     0178 00360000
*                   MUST BE AN ESTAR TYPE                          0178 00361000
*                    A-IF A LAST VALID SCB IS NON EXISTANT         0178 00362000
*                      A CHECK IS MADE TO DETERMINE IF AN          0178 00363000
*                      SCB QUEUE EXISTS-IF THE QUEUE IS NON-EXISTANT    00364000
*                      A CREATE ESTAR WILL BE PERFORMED            0178 00365000
*     ALL INVALID ESTAR REQUESTS RESULT IN ABEND 13C AND A REASON  0178 00366000
*     CODE OF 4 IN REGISTER 15                                     0178 00367000
*                                                                  0178 00368000
*  ***************************************************************** */ 00369000
*                                                                  0178 00370000
*         /***********************************************************/ 00371000
*         /*                                                         */ 00372000
*         /* ISSUE TESTAUTH TO VERIFY INVOKERS VALIDITY              */ 00373000
*         /*                                                         */ 00374000
*         /***********************************************************/ 00375000
*                                                                  0178 00376000
*         R6=R1;                    /* SAVE R1 ACROSS TESTAUTH     0178 00377000
*                                                            @Z40FPXA*/ 00378000
         LR    R6,R1                                               0178 00379000
*         GENERATE;                                                0179 00380000
         TESTAUTH FCTN=1,STATE=YES,KEY=YES,RBLEVEL=2,BRANCH=YES         00381000
*         R1=R6;                    /* RESTORE REG 1         @Z40FPXA*/ 00382000
         LR    R1,R6                                               0180 00383000
*         IF R15^=0|(FC^=0&FC^=8)|ESTAEXIT=0/* IS THE INVOKER VALID     00384000
*                                      AND IS THE ACTION REQUESTED 0181 00385000
*                                      CREATE OR OVERLAY             */ 00386000
*           THEN                    /* NO- THIS REQUEST IS INVALID   */ 00387000
         LTR   R15,R15                                             0181 00388000
         BNZ   @RT00181                                            0181 00389000
         CLI   FC(R9),0                                            0181 00390000
         BE    @GL00005                                            0181 00391000
         CLI   FC(R9),8                                            0181 00392000
         BNE   @RT00181                                            0181 00393000
@GL00005 L     @12,ESTAEXIT-1(,R1)                                 0181 00394000
         LA    @12,0(,@12)                                         0181 00395000
         LTR   @12,@12                                             0181 00396000
         BNZ   @RF00181                                            0181 00397000
@RT00181 DS    0H                                                  0182 00398000
*           DO;                                                    0182 00399000
*             REASON=4;             /* ABEND REASON CODE             */ 00400000
         MVI   REASON(R9),X'04'                                    0183 00401000
*             CALL INVALID;         /* ISSUE ABEND                   */ 00402000
         BAL   @14,INVALID                                         0184 00403000
*           END;                                                   0185 00404000
*                                                                  0185 00405000
*         /***********************************************************/ 00406000
*         /*                                                         */ 00407000
*         /* INVOKER IS VALID                                        */ 00408000
*         /*                                                         */ 00409000
*         /***********************************************************/ 00410000
*                                                                  0186 00411000
*         IF FC=8                   /* OVERLAY REQUEST ?             */ 00412000
*           THEN                    /* YES                           */ 00413000
@RF00181 CLI   FC(R9),8                                            0186 00414000
         BNE   @RF00186                                            0186 00415000
*           DO;                                                    0187 00416000
*             CALL SCBSRCH;         /* FIND LAST VALID SCB           */ 00417000
         BAL   @14,SCBSRCH                                         0188 00418000
*             IF R2=0               /* IS A LAST VALID SCB EXISTANT  */ 00419000
*               THEN                /* NO                            */ 00420000
         LTR   R2,R2                                               0189 00421000
         BNZ   @RF00189                                            0189 00422000
*               DO;                                                0190 00423000
*                 RETCOD=4;         /* SET RETURN CODE 4 FOR MODIFIED   00424000
*                                      CREATE                @Z40FPXA*/ 00425000
         MVI   RETCOD(R9),X'04'                                    0191 00426000
*                 FC='00'X;         /* CHANGE TO CREATE REQUEST    0192 00427000
*                                                            @Z40FPXA*/ 00428000
         MVI   FC(R9),X'00'                                        0192 00429000
*               END;                                               0193 00430000
*             ELSE                  /* A VALID SCB EXISTS            */ 00431000
*               DO;                                                0194 00432000
