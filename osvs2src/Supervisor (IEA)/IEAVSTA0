         NI    SCBBRNTR(R2),B'11111011'                            0528 01486000
*   RETURN;                                                        0529 01487000
@EL00008 DS    0H                                                  0529 01488000
@EF00008 DS    0H                                                  0529 01489000
@ER00008 BR    @14                                                 0529 01490000
*   END COMPLSCB;                                                  0530 01491000
*PROPA:                                                            0531 01492000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0531 01493000
*                                                                  0531 01494000
PROPA    DS    0H                                                  0532 01495000
*   /*****************************************************************/ 01496000
*   /*                                                               */ 01497000
*   /* PROPA IS ENTERED TO COPY ALL STAI AND ESTAI SCBS FROM THE TCB */ 01498000
*   /* THAT HAS REQUESTED ATTACH TO THE NEW TCB BEING CREATED BY THE */ 01499000
*   /* ATTACH FUNCTION                                               */ 01500000
*   /*                                                               */ 01501000
*   /*****************************************************************/ 01502000
*                                                                  0532 01503000
*   WORK2=R14;                      /* SAVE RETURN ADDRESS           */ 01504000
         ST    R14,WORK2(,R9)                                      0532 01505000
*   RESPECIFY                                                      0533 01506000
*    (R5) RSTD;                                                    0533 01507000
*   R2=TCBSTABB;                    /* START AT THE TOP OF THE SCB Q */ 01508000
         L     R2,TCBSTABB-1(,TCBPTR)                              0534 01509000
         LA    R2,0(,R2)                                           0534 01510000
*   R5=0;                           /*                       @Z30EPXA*/ 01511000
         SLR   R5,R5                                               0535 01512000
*   DO WHILE(R2^=0&R5=0);           /* FIND FIRST STAI SCB   @Z40FPXA*/ 01513000
         B     @DE00536                                            0536 01514000
@DL00536 DS    0H                                                  0537 01515000
*     IF SCBID^='DB'X               /* IS IT AN SCB ?                */ 01516000
*       THEN                        /* NO                            */ 01517000
