         L     @12,TCBSTAB(,@12)                                   0563 01607000
         ST    R2,SCBCHAIN(,@12)                                   0563 01608000
*       DO WHILE(R5^=0);            /* COPY ALL STAI AND ESTAI SCBS     01609000
*                                      FROM MOMS QUEUE       @Z30EPXA*/ 01610000
@RC00561 B     @DE00564                                            0564 01611000
@DL00564 DS    0H                                                  0565 01612000
*         SCB=R5->SCB;              /* COPY THE SCB          @Z30EPXA*/ 01613000
