*         IF SCBSTAI='1'B           /* IS THIS A STAI OR ESTAI TYPE     01531000
*                                      SCB                           */ 01532000
*           THEN                    /* YES-                          */ 01533000
         TM    SCBSTAI(R2),B'10000000'                             0544 01534000
         BNO   @RF00544                                            0544 01535000
*           R5=R2;                  /*                       @Z30EPXA*/ 01536000
         LR    R5,R2                                               0545 01537000
*         R2=SCBCHAIN;              /* ADDRESS OF NEXT SCB           */ 01538000
@RF00544 L     R2,SCBCHAIN(,R2)                                    0546 01539000
*       END;                                                       0547 01540000
*   END;                                                           0548 01541000
@DE00536 SLR   @12,@12                                             0548 01542000
         CR    R2,@12                                              0548 01543000
         BE    @DC00536                                            0548 01544000
         CR    R5,@12                                              0548 01545000
         BE    @DL00536                                            0548 01546000
@DC00536 DS    0H                                                  0549 01547000
*   IF R5=0                         /* ANY SCB TO COPY               */ 01548000
*     THEN                                                         0549 01549000
         LTR   R5,R5                                               0549 01550000
         BZ    @RT00549                                            0549 01551000
*     ;                             /* NO- BYPASS PROCESS            */ 01552000
*   ELSE                            /* YES- OBTAIN STORAGE           */ 01553000
*     DO;                                                          0551 01554000
*                                                                  0551 01555000
*       /*************************************************************/ 01556000
*       /*                                                           */ 01557000
*       /* CHECK IF FREE SCB CAN BE USED. IF NOT OBTAIN MORE SCB'S   */ 01558000
*       /*                                                           */ 01559000
*       /*************************************************************/ 01560000
*                                                                  0552 01561000
*TRYA1:                                                            0552 01562000
*       R2=FREESCBQ;                                               0552 01563000
TRYA1    L     R2,FREESCBQ(,R7)                                    0552 01564000
*       IF R2=0 THEN                /* FREE Q EMPTY?         @Z30EPXA*/ 01565000
         LTR   R2,R2                                               0553 01566000
         BNZ   @RF00553                                            0553 01567000
*         DO;                                                      0554 01568000
*           CALL GETSTOR;           /* OBTAIN SCB'S          @Z30EPXA*/ 01569000
         BAL   @14,GETSTOR                                         0555 01570000
*           GOTO TRYA1;             /* GO RETRY CELL OBTAIN  @Z40FPXA*/ 01571000
         B     TRYA1                                               0556 01572000
*         END;                                                     0557 01573000
*                                                                  0557 01574000
*       /*************************************************************/ 01575000
*       /*                                                           */ 01576000
*       /* PICK UP ADDRESS OF FREE SCB                               */ 01577000
*       /*                                                           */ 01578000
*       /*************************************************************/ 01579000
*                                                                  0558 01580000
*       R3=SCBCHAIN;                /* PICK UP CHAIN FIELD   @Z40FPXA*/ 01581000
