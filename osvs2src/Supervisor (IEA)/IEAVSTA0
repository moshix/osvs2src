         TITLE 'IGC00060 - STA/ESTA SERVICE ROUTINE (SVC 60)'           00001000
* SET UP THE SVC ENTRY POINT AND THE ADDRESS USED FOR BRANCH ENTRIES.   00002000
IGC00060 CSECT                                                          00003000
         USING *,R6          BASE REG PRE-SET BY SVC SLIH               00004000
SVCEPT   B     SVCENTRY      IEAVSTA0+0 ENTER HERE ON SVC ENTRY         00005000
         DC    A(ESTAEBRE)   IEAVSTA0+4 = ADDRESS OF BRANCH ENTRY--     00006000
*                            MOVED TO -CVTSV60- DURING OUR NIP          00007000
*                            INITIALIZATION BY -IEAVNPA6-               00008000
*                            IF THE DISPLACEMENT OF THIS                00009000
*                            ADDRESS CONSTANT IS CHANGED IEAVNPA6       00010000
*                            MUST BE CHANGED ACCORDINGLY                00011000
         MODID BR=NO                                                    00012000
         DROP  R6                                                       00013000
         USING *,R15                                                    00014000
         B     RETN               RETRY ENTRY POINT          @Z40FPXA   00015000
         DROP  R15                                                      00016000
ESTAEBRE ST    R14,12(R13)      SAVE RETURN ADDRESS                     00017000
         STM   R2,R12,28(R13)   SAVE REGS ON BRANCH ENTRY               00018000
PROLOG   BALR  R10,0         SET BASE FOR BOTH ENTRY TYPES              00019000
PSTART   DS    0H                                            @Z30EPXA   00020000
         USING PSTART,R10                                               00021000
         B     BRENTP                                        @Z30EPXA   00022000
SVCENTRY LA    R10,PSTART-SVCEPT(R6)                         @Z30EPXA   00023000
RBPRFXLN EQU 64                                                         00024000
RBPRFLNA EQU 32                                                         00025000
PRBLEN   EQU 136                                                        00026000
SIRBLEN  EQU 200                                                        00027000
TIRBLEN  EQU 136                                                        00028000
IRBLEN   EQU 128                                                        00029000
SVRBLEN  EQU 224                                                        00030000
*                                      SAVE AREA (40 BYTES)          */ 00031000
*SVCENTP:                                                          0078 00032000
*                                                                  0078 00033000
*   /*****************************************************************/ 00034000
*   /*                                                               */ 00035000
*   /* OBTAIN WORK AREA IN CURRENT SVRB                              */ 00036000
*   /*                                                               */ 00037000
*   /*****************************************************************/ 00038000
*                                                                  0078 00039000
*   R9=ADDR(RBFEPARM);              /* ADDRESS OF WA IN SVRB.. WORK     00040000
*                                      AREA                  @Z40FPXA*/ 00041000
SVCENTP  LA    R9,RBFEPARM(,RBSECPTR)                              0078 00042000
*   R7=R7->ASXBPTR->ASXBSPSA->WSALSTAE;/* FREE SCB QUEUE PTR @Z40FPXA*/ 00043000
*                                                                  0079 00044000
         L     @15,ASXBPTR(,R7)                                    0079 00045000
         L     @15,ASXBSPSA(,@15)                                  0079 00046000
         L     @15,WSALSTAE(,@15)                                  0079 00047000
         LR    R7,@15                                              0079 00048000
*   /*****************************************************************/ 00049000
*   /*                                                               */ 00050000
*   /* INDICATE TO RTM THAT ESTAE IS IN CONTROL, RTM WILL ROUTE      */ 00051000
*   /* CONTROL TO ESTAE'S RECOVERY ROUTINE IN EVENT OF ERROR         */ 00052000
*   /*                                                               */ 00053000
*   /*****************************************************************/ 00054000
*                                                                  0080 00055000
*   IF TCBFX^='1'B THEN             /* DISALLOW ASYNCRONOUS  @ZA24347*/ 00056000
         TM    TCBFX(TCBPTR),B'00000001'                           0080 00057000
         BO    @RF00080                                            0080 00058000
*     DO;                           /* INTERRUPTS.           @ZA24347*/ 00059000
*       TCBSTAFX='1'B;              /*                       @ZA24347*/ 00060000
         OI    TCBSTAFX(TCBPTR),B'01000000'                        0082 00061000
*       TCBFX='1'B;                 /*                       @ZA24347*/ 00062000
         OI    TCBFX(TCBPTR),B'00000001'                           0083 00063000
*     END;                          /*                       @ZA24347*/ 00064000
*   ELSE                                                           0085 00065000
*     ;                             /*                       @ZA24347*/ 00066000
@RF00080 DS    0H                                                  0086 00067000
*   TCBSTAWA=R9;                    /* SAVE WORK AREA POINTER      0086 00068000
*                                                            @Z40FPXA*/ 00069000
         ST    R9,TCBSTAWA(,TCBPTR)                                0086 00070000
*   R14SAVE=R14;                    /* SAVE RETURN ADDRESS   @Z40FPXA*/ 00071000
         ST    R14,R14SAVE(,R9)                                    0087 00072000
*   WKWORD=REG0;                    /* SAVE FUNCTION CODE AND CLEAR     00073000
*                                      WORK AREA             @Z40FPXA*/ 00074000
         ST    REG0,WKWORD(,R9)                                    0088 00075000
*   IF FC='84'X|FC=4                /* NO PARM LIST IF CANCEL        */ 00076000
*     THEN                          /* CANCEL IS REQUESTED           */ 00077000
         CLI   FC(R9),X'84'                                        0089 00078000
         BE    @RT00089                                            0089 00079000
         CLI   FC(R9),4                                            0089 00080000
         BNE   @RF00089                                            0089 00081000
@RT00089 DS    0H                                                  0090 00082000
*     DO;                                                          0090 00083000
*       CANCELFL='1'B;              /* SET CANCEL FLAG       @Z30EPXA*/ 00084000
         OI    CANCELFL(R9),B'00001000'                            0091 00085000
*     END;                                                         0092 00086000
*   ELSE                                                           0093 00087000
*     DO;                                                          0093 00088000
*                                                                  0093 00089000
         B     @RC00089                                            0093 00090000
@RF00089 DS    0H                                                  0094 00091000
*       /*************************************************************/ 00092000
*       /*                                                           */ 00093000
*       /* CHECK FOR PROP REQUEST                                    */ 00094000
*       /*                                                           */ 00095000
*       /*************************************************************/ 00096000
*                                                                  0094 00097000
*       IF FC=2 THEN                /* PROP REQUEST?         @Z40FPXA*/ 00098000
         CLI   FC(R9),2                                            0094 00099000
         BNE   @RF00094                                            0094 00100000
*         DO;                       /*                       @Z40FPXA*/ 00101000
*           IF(ESTASTAI='0'B|ESTASTAR='1'B|ESTAESTA='0'B)/* PROP ONLY   00102000
*                                      VALID WITH ESTAI      @Z30EPXA*/ 00103000
*             THEN                  /*                       @YM01507*/ 00104000
         TM    ESTASTAR(R1),B'01000000'                            0096 00105000
         BNZ   @RT00096                                            0096 00106000
         TM    ESTASTAI(R1),B'10010000'                            0096 00107000
         BO    @RF00096                                            0096 00108000
@RT00096 DS    0H                                                  0097 00109000
*             DO;                   /* INVALID REQUEST       @YM01507*/ 00110000
*               REASON=8;           /*                       @YM01507*/ 00111000
         MVI   REASON(R9),X'08'                                    0098 00112000
*               CALL INVALID;       /* ISSUE ABEND           @YM01507*/ 00113000
         BAL   @14,INVALID                                         0099 00114000
*             END;                  /*                       @YM01507*/ 00115000
*           ELSE                    /* VALID PROP REQUEST.. PROCESS     00116000
*                                      IT                    @Z40FPXA*/ 00117000
*             GOTO PROPROC;         /* PROCESS PROP          @Z40FPXA*/ 00118000
*         END;                      /* END OF PROP PROCESSING      0102 00119000
*                                                            @Z40FPXA*/ 00120000
*                                                                  0102 00121000
*       /*************************************************************/ 00122000
*       /*                                                           */ 00123000
*       /* THIS SECTION DETERMINES IF A STA RECURSION HAS TAKEN PLACE*/ 00124000
*       /* -VERIFIES STAI REQUESTS                                   */ 00125000
*       /*                                                           */ 00126000
*       /*************************************************************/ 00127000
*                                                                  0103 00128000
*       IF ESTAESTA='0'B            /* STA TYPE REQUEST ?    @Z30EPXA*/ 00129000
*         THEN                      /* YES- CHECK RECURSION          */ 00130000
@RF00094 TM    ESTAESTA(R1),B'00010000'                            0103 00131000
         BNZ   @RF00103                                            0103 00132000
*         DO;                                                      0104 00133000
*           IF TCBSTABE='1'B        /* IS A STA EXIT IN PROGRESS     */ 00134000
*             THEN                  /* YES- INVALID                  */ 00135000
         TM    TCBSTABE(TCBPTR),B'10000000'                        0105 00136000
         BNO   @RF00105                                            0105 00137000
*             DO;                                                  0106 00138000
*               GOTO RETCOD8;       /*                       @Z40FPXA*/ 00139000
         B     RETCOD8                                             0107 00140000
*             END;                                                 0108 00141000
*           ELSE                    /* NO- OKAY SO FAR               */ 00142000
*             DO;                                                  0109 00143000
@RF00105 DS    0H                                                  0110 00144000
*               IF ESTASTAI='1'B    /* STAI REQUEST ?                */ 00145000
*                 THEN              /* YES                           */ 00146000
         TM    ESTASTAI(R1),B'10000000'                            0110 00147000
         BNO   @RF00110                                            0110 00148000
*                 DO;                                              0111 00149000
*                                                                  0111 00150000
*                   /*************************************************/ 00151000
*                   /*                                               */ 00152000
*                   /* THIS SECTION OF CODE INSURES THAT A CREATE    */ 00153000
*                   /* REQUEST IS THE ONLY REQUEST THAT CAN BE       */ 00154000
*                   /* HONORED FOR STAI-CONTROL IS THEN GIVEN TO     */ 00155000
*                   /* VERATTCH TO INSURE ATTACH IS THE REQUESTOR    */ 00156000
*                   /*                                               */ 00157000
*                   /*************************************************/ 00158000
*                                                                  0112 00159000
*                   IF FC=0         /* CREATE REQUESTED ?            */ 00160000
*                     THEN          /* YES                           */ 00161000
         CLI   FC(R9),0                                            0112 00162000
         BNE   @RF00112                                            0112 00163000
*                     DO;                                          0113 00164000
*                       CALL VERATTCH;/* INSURE ATTACH CALLED        */ 00165000
         BAL   @14,VERATTCH                                        0114 00166000
*                     END;                                         0115 00167000
*                   ELSE            /* CREATE IS NOT THE REQUEST     */ 00168000
*                     DO;                                          0116 00169000
         B     @RC00112                                            0116 00170000
@RF00112 DS    0H                                                  0117 00171000
*                       GOTO RETCOD10;/* ONLY CREATE ALLOWED         */ 00172000
         B     RETCOD10                                            0117 00173000
*                     END;                                         0118 00174000
*                 END;                                             0119 00175000
*               ELSE                /* NOT STAI                      */ 00176000
*                 DO;               /* INSURE IT IS NOT A          0120 00177000
*                                      NON-SUPPORTED STAR REQUEST    */ 00178000
         B     @RC00110                                            0120 00179000
@RF00110 DS    0H                                                  0121 00180000
*                   IF ESTASTAR='1'B/* STAR!!!                       */ 00181000
*                     THEN          /* YES                           */ 00182000
         TM    ESTASTAR(R1),B'01000000'                            0121 00183000
         BNO   @RF00121                                            0121 00184000
*                     DO;                                          0122 00185000
*                       GOTO RETCOD10;/* STAR NOT SUPPORTED  @Z40FPXA*/ 00186000
         B     RETCOD10                                            0123 00187000
*                     END;                                         0124 00188000
*                 END;                                             0125 00189000
@RF00121 DS    0H                                                  0126 00190000
*             END;                                                 0126 00191000
*                                                                  0126 00192000
@RC00110 DS    0H                                                  0127 00193000
*           /*********************************************************/ 00194000
*           /*                                                       */ 00195000
*           /* THIS SECTION OF CODE INSURES THAT THE EXIT AND        */ 00196000
*           /* PARAMETER LIST ADDRESSES SPECIFIED ON A STA CREATE OR */ 00197000
*           /* OVERLAY ARE VALID                                     */ 00198000
*           /*                                                       */ 00199000
*           /*********************************************************/ 00200000
*                                                                  0127 00201000
*           IF FC=0|FC=8            /* STA CT OR OV ?        @Z40FPXA*/ 00202000
*             THEN                  /* YES                           */ 00203000
         CLI   FC(R9),0                                            0127 00204000
         BE    @RT00127                                            0127 00205000
         CLI   FC(R9),8                                            0127 00206000
         BNE   @RF00127                                            0127 00207000
@RT00127 DS    0H                                                  0128 00208000
*             DO;                                                  0128 00209000
*               IF ESTAEXIT=0       /* IMPLIED STAE CANCEL?          */ 00210000
*                 THEN              /* YES                           */ 00211000
         L     @15,ESTAEXIT-1(,R1)                                 0129 00212000
         LA    @15,0(,@15)                                         0129 00213000
         LTR   @15,@15                                             0129 00214000
         BNZ   @RF00129                                            0129 00215000
*                 DO;                                              0130 00216000
*                   FC=4;           /* INDICATE STAE CANCEL  @Z40FPXA*/ 00217000
         MVI   FC(R9),X'04'                                        0131 00218000
*                   CANCELFL='1'B;  /* SET CANCEL FLAG       @Z30EPXA*/ 00219000
         OI    CANCELFL(R9),B'00001000'                            0132 00220000
*                 END;                                             0133 00221000
*             END;                                                 0134 00222000
*         END;                                                     0135 00223000
*       ELSE                                                       0136 00224000
*         DO;                                                      0136 00225000
         B     @RC00103                                            0136 00226000
@RF00103 DS    0H                                                  0137 00227000
*           IF ESTASTAI='0'B&ESTASTAR='0'B/* ESTAE?          @Z40FPXA*/ 00228000
*             THEN                  /* YES                           */ 00229000
         TM    ESTASTAI(R1),B'11000000'                            0137 00230000
         BNZ   @RF00137                                            0137 00231000
*             DO;                                                  0138 00232000
*               IF ESTAEXIT=0       /* IMPLIED CANCEL?               */ 00233000
*                 THEN              /* YES                           */ 00234000
         L     @15,ESTAEXIT-1(,R1)                                 0139 00235000
         LA    @15,0(,@15)                                         0139 00236000
         LTR   @15,@15                                             0139 00237000
         BNZ   @RF00139                                            0139 00238000
*                 DO;                                              0140 00239000
