         OI    SCBSUPER(R2),B'00000001'                            0512 01438000
*         END;                                                     0513 01439000
*     END;                                                         0514 01440000
*                                                                  0514 01441000
*   /*****************************************************************/ 01442000
*   /*                                                               */ 01443000
*   /* THIS ROUTINE COMPLETES THE COMMON CREATE AND OVERLAY          */ 01444000
*   /* PROCESSING BY INITIALIZING THE REMAINING SCB FIELDS           */ 01445000
*   /*                                                               */ 01446000
*   /*****************************************************************/ 01447000
*                                                                  0515 01448000
*FINSCB:                                                           0515 01449000
*   ENTRY;                                                         0515 01450000
         B     @EC00515                                            0515 01451000
FINSCB   DS    0H                                                  0515 01452000
@EC00515 DS    0H                                                  0516 01453000
*   SCBEXIT=0;                      /* ZERO HIGH ORDER BYTE OF     0516 01454000
*                                      SCBEXIT               @ZA15390*/ 01455000
         SLR   @12,@12                                             0516 01456000
         ST    @12,SCBEXIT(,R2)                                    0516 01457000
*   SCBFLDS=ESTAFLDS;               /* MOVE PARMS INTO SCB   @Z40FPXA*/ 01458000
         MVC   SCBFLDS(7,R2),ESTAFLDS(R1)                          0517 01459000
*   SCBFLGS1=ESTAFLG1;              /* BUILD SCB FLAG 1              */ 01460000
         MVC   SCBFLGS1(1,R2),ESTAFLG1(R1)                         0518 01461000
*   IF XCTLIN<0                     /* WAS XCTL =YES SPECIFIED       */ 01462000
