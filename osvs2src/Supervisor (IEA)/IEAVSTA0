*       CS(R2,R3,FREESCBQ);         /* DEQUEUE ELEMENT       @Z40FPXA*/ 01583000
         CS    R2,@03,FREESCBQ(R7)                                 0559 01584000
*       BC(7,TRYA1);                /* RETRY IF UNSUCCESSFUL @Z40FPXA*/ 01585000
*                                                                  0560 01586000
         BC    7,TRYA1                                             0560 01587000
*       /*************************************************************/ 01588000
*       /*                                                           */ 01589000
*       /* NOW LOOK AT NEW TCB BEING PROP TO                         */ 01590000
*       /*                                                           */ 01591000
*       /*************************************************************/ 01592000
*                                                                  0561 01593000
*       IF ESTAOWNR->TCBSTABB=0     /* WAS AN SCB CREATED ON THE TCB */ 01594000
*         THEN                      /* NO                            */ 01595000
         L     @12,ESTAOWNR(,R1)                                   0561 01596000
         L     @11,TCBSTABB-1(,@12)                                0561 01597000
         LA    @11,0(,@11)                                         0561 01598000
