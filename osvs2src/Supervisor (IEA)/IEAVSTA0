*         DO;                                                      0507 01428000
*           SCBKEY0='1'B;           /* INDICATE CONTROL PROGRAM KEY  */ 01429000
         OI    SCBKEY0(R2),B'00000010'                             0508 01430000
*         END;                                                     0509 01431000
*       IF R3->PPMODE^='1'B         /* PROBLEM PROGRM MODE?  @Z40FPXA*/ 01432000
