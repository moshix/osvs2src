         BE    @RF00957                                            0957 02902000
*         TSLOTCBP=TCBNTC;          /* YES, SET TCB PTR TO SISTER OF */ 02903000
         LR    TSLOTCBP,@04                                        0958 02904000
*       ELSE                        /* INPUT TCB                     */ 02905000
*         DO;                                                      0959 02906000
         B     @RC00957                                            0959 02907000
@RF00957 DS    0H                                                  0960 02908000
*           TSLOMTH=TCBOTC;         /* NO, SET TCB PTR TO MOTHER OF     02909000
*                                      INPUT TCB PREVENT LOOP EXEC.  */ 02910000
         L     TSLOMTH,TCBOTC(,TSLOTCBP)                           0960 02911000
*           TSLORETR=TSLOMTH;       /* RETURN MOTHER                 */ 02912000
         LR    TSLORETR,TSLOMTH                                    0961 02913000
*         END;                                                     0962 02914000
*     END;                                                         0963 02915000
@RC00957 DS    0H                                                  0964 02916000
*   IF TSLOMTH^=TCBOTC THEN         /* HAS MOTHER TCBPTR BEEN      0964 02917000
*                                      OBTAINED                      */ 02918000
@RC00954 C     TSLOMTH,TCBOTC(,TSLOTCBP)                           0964 02919000
         BE    @RF00964                                            0964 02920000
*     DO;                                                          0965 02921000
*       DO WHILE(TCBLTC^=TSLOZERO); /* NO, LOOK FOR SUBTASKS         */ 02922000
         B     @DE00966                                            0966 02923000
@DL00966 DS    0H                                                  0967 02924000
*         TSLOTCBP=TCBLTC;          /* GET DAUGHTER OF TCB           */ 02925000
         L     TSLOTCBP,TCBLTC(,TSLOTCBP)                          0967 02926000
*       END;                                                       0968 02927000
@DE00966 C     TSLOZERO,TCBLTC(,TSLOTCBP)                          0968 02928000
         BNE   @DL00966                                            0968 02929000
*       TSLORETR=TSLOTCBP;          /* RETURN TCB ADDR TO CALLER     */ 02930000
