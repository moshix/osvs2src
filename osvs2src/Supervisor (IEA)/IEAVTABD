*                 RFY                                              0374 00864000
*                   R0 UNRSTD;                                     0374 00865000
*                 RTM2EGET='0'B;    /* GETMAIN NOT IN CONTROL      0375 00866000
*                                                            @G33SPHW*/ 00867000
         NI    RTM2EGET(RTM2PTR),B'10111111'                       0375 00868000
*                 IF RETCODE^=0 THEN/* GETMAIN UNSUCCESSFUL  @G33SPHW*/ 00869000
         LTR   RETCODE,RETCODE                                     0376 00870000
         BZ    @RF00376                                            0376 00871000
*                   DO;             /* YES - FAILED          @G33SPHW*/ 00872000
*                     RTM2NDMP=ON;  /* INDICATE NO DUMP      @G33SPHW*/ 00873000
         OI    RTM2NDMP(RTM2PTR),B'10000000'                       0378 00874000
*                     CALL ADSPFAIL;/* FAILED MESSAGE        @G33SPHW*/ 00875000
         BAL   @14,ADSPFAIL                                        0379 00876000
*                   END;            /* END GETMAIN FAILED    @G33SPHW*/ 00877000
