         TITLE 'IEAVTABD  - ABDUMP                                     *00001000
                        '                                               00002000
IEAVTABD CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVTABD  78.086'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @06,0                                               0001 00011000
@PSTART  LA    @05,4095(,@06)                                      0001 00012000
         USING @PSTART,@06                                         0001 00013000
         USING @PSTART+4095,@05                                    0001 00014000
*   RTM2PTR=PARMPTR;                /* GET ADDRESS OF RTM2 WORK AREA */ 00015000
         LR    RTM2PTR,PARMPTR                                     0148 00016000
*   RTM2SFWA=RTM2SFWA&&RTM2SFWA;    /* ZERO AREA                     */ 00017000
         XC    RTM2SFWA(16,RTM2PTR),RTM2SFWA(RTM2PTR)              0149 00018000
*   GEN(GETMAIN R,LV=@ENDDATD-@DATD,SP=253);/* GETMAIN DATA AREA   0150 00019000
*                                                            @G33SPHW*/ 00020000
         GETMAIN R,LV=@ENDDATD-@DATD,SP=253                             00021000
*   AUTOPTR=PARMPTR;                /* PUT GETMAINED ADDRESS IN R1 0151 00022000
*                                                            @G33SPHW*/ 00023000
         LR    AUTOPTR,PARMPTR                                     0151 00024000
*   GEN(XC  0(@ENDDATD-@DATD,AUTOPTR),0(AUTOPTR));/* ZERO STORAGE  0152 00025000
*                                                            @G33SPHW*/ 00026000
         XC  0(@ENDDATD-@DATD,AUTOPTR),0(AUTOPTR)                       00027000
*   GEN(USING @DATD,AUTOPTR);                                      0153 00028000
         USING @DATD,AUTOPTR                                            00029000
*   RTM2WARG=8;                     /* INDICATE REG7 CONTAINS ADDR OF   00030000
*                                      RTM2WA                        */ 00031000
         MVI   RTM2WARG(RTM2PTR),X'08'                             0154 00032000
*   TCBPTR=RTM2TCBC;                /* GET ADDR OF CURRENT TCB       */ 00033000
         MVC   TCBPTR(4),RTM2TCBC(RTM2PTR)                         0155 00034000
*   GENERATE REFS(RTM2RREG);                                       0156 00035000
         STM   0,15,RTM2RREG(RTM2PTR) SAVE REGS FOR USE BY RECOVERY RTN 00036000
*   RTM2TRRA=ADDR(ADRECOV);         /* GET ADDR OF ABDUMP RECOVERY 0157 00037000
*                                      RTN                           */ 00038000
         LA    @14,ADRECOV                                         0157 00039000
         ST    @14,RTM2TRRA(,RTM2PTR)                              0157 00040000
*ADDCHK:                                                           0158 00041000
*   IF(RTCTISAB='1'B&               /* ARE SYSABEND          @G33NPHE*/ 00042000
*       RTCTISYU='1'B&              /* SYSUDUMP AND                  */ 00043000
*       RTCTISYM='1'B)              /* SYMDUMP DUMPS BEING IGNORED 0158 00044000
*                                                            @G33SPHW*/ 00045000
*       |RTM2NODP=ON                /* OR HAS SLIP REQUESTED THAT ALL   00046000
*                                      DUMP REQUESTS FOR THIS CALL TO   00047000
*                                      RTM BE IGNORED?       @G33NPHE*/ 00048000
*     THEN                                                         0158 00049000
ADDCHK   LA    @14,236                                             0158 00050000
         L     @09,CVTPTR                                          0158 00051000
         AL    @14,CVTRTMCT(,@09)                                  0158 00052000
         TM    RTCTISAB(@14),B'00000001'                           0158 00053000
         BNO   @GL00001                                            0158 00054000
         TM    RTCTISYU(@14),B'00000001'                           0158 00055000
         BNO   @GL00001                                            0158 00056000
         TM    RTCTISYM(@14),B'00000001'                           0158 00057000
         BO    @RT00158                                            0158 00058000
@GL00001 TM    RTM2NODP(RTM2PTR),B'00010000'                       0158 00059000
         BNO   @RF00158                                            0158 00060000
@RT00158 DS    0H                                                  0159 00061000
*     RTM2SNCC=0;                   /* YES, RETURN TO RTM2           */ 00062000
         SLR   @14,@14                                             0159 00063000
         ST    @14,RTM2SNCC(,RTM2PTR)                              0159 00064000
*   ELSE                                                           0160 00065000
*     DO;                                                          0160 00066000
*                                                                  0160 00067000
         B     @RC00158                                            0160 00068000
@RF00158 DS    0H                                                  0161 00069000
*       /*************************************************************/ 00070000
*       /*                                                           */ 00071000
*       /* THIS SECTION OF CODE DETERMINES WHETHER THE TIOT CONTAINS */ 00072000
*       /* THE APPRORIATE INFORMATION TO PROVIDE A SYSABEND OR       */ 00073000
*       /* SYSUDUMP DUMP. THE FOLLOWING CONDITIONS MUST EXIST: 1. THE*/ 00074000
*       /* TCB MUST CONTAIN A TIOT POINTER 2. TIOT MUST CONTAIN A    */ 00075000
*       /* SYSABEND ,SYSUDUMP OR SYSMDUMP ENTRY 3. THE ADDR OF THE   */ 00076000
*       /* UCB IN THE TIOT ENTRY MUST BE NON-ZERO. (ZERO INDICATES A */ 00077000
*       /* DD DUMMY DATA SET. 4. THE TIOT ENTRY ADDR FOR THE DATA SET*/ 00078000
*       /* MUST BE NON-ZERO. NOTE: THE LAST OCCURANCE OF EITHER      */ 00079000
*       /* SYSABEND ,SYSUDUMP OR SYSMDUMP IS TAKEN AS THE DEFINITION */ 00080000
*       /* OF THE DUMP DATA SET                                      */ 00081000
*       /*                                                           */ 00082000
*       /*************************************************************/ 00083000
*                                                                  0161 00084000
*       IF TCBTIO=0 THEN            /* DOES TIOT EXIST               */ 00085000
         L     @14,TCBPTR                                          0161 00086000
         ICM   @09,15,TCBTIO(@14)                                  0161 00087000
         BNZ   @RF00161                                            0161 00088000
*         RTM2NDMP='1'B;            /* NO, INDICATE NO DUMP          */ 00089000
         OI    RTM2NDMP(RTM2PTR),B'10000000'                       0162 00090000
*       ELSE                                                       0163 00091000
*         DO;                                                      0163 00092000
         B     @RC00161                                            0163 00093000
@RF00161 DS    0H                                                  0164 00094000
*           TIOTPTR=TCBTIO;         /* GET ADDR. OF TIOT             */ 00095000
*                                                                  0164 00096000
         L     @14,TCBPTR                                          0164 00097000
         L     TIOTPTR,TCBTIO(,@14)                                0164 00098000
*           /*********************************************************/ 00099000
*           /*                                                       */ 00100000
*           /* LOOP THRU ALL ENTRIES OF TIOT USE LAST DUMP DD FOUND  */ 00101000
*           /* ON THE TIOT AS DUMP TYPE(SYSABEND,SYSUDUMP,SYSMDUMP). */ 00102000
*           /*                                               @G33SPHW*/ 00103000
*           /*                                                       */ 00104000
*           /*********************************************************/ 00105000
*                                                                  0165 00106000
*           DMPTIOTA=0;             /* INIT ADDR OF TIOT ENTRY TO 0  */ 00107000
         SLR   DMPTIOTA,DMPTIOTA                                   0165 00108000
*           DO WHILE(TIOELNGH^='00'X);/* LOOP THROUGH TIOT ENTRIES 0166 00109000
*                                      UNTIL SYSABEND , SYSUDUMP OR     00110000
*                                      SYSMDUMP DD IS FOUND  @G33SPHW*/ 00111000
         B     @DE00166                                            0166 00112000
@DL00166 DS    0H                                                  0167 00113000
*             IF TIOEDDNM='SYSABEND'|TIOEDDNM='SYSUDUMP'|/* IS ENTRY    00114000
*                                                            @G33SPHW*/ 00115000
*                 TIOEDDNM='SYSMDUMP'/* FOR                  @G33SPHW*/ 00116000
*               THEN                /* DUMP DATA SET                 */ 00117000
         CLC   TIOEDDNM(8,TIOTPTR),@CC03621                        0167 00118000
         BE    @RT00167                                            0167 00119000
         CLC   TIOEDDNM(8,TIOTPTR),@CC03622                        0167 00120000
         BE    @RT00167                                            0167 00121000
         CLC   TIOEDDNM(8,TIOTPTR),@CC03623                        0167 00122000
         BNE   @RF00167                                            0167 00123000
@RT00167 DS    0H                                                  0168 00124000
*               DMPTIOTA=TIOTPTR;   /* YES, SAVE ADDR OF TIOT ENTRY  */ 00125000
         LR    DMPTIOTA,TIOTPTR                                    0168 00126000
*             TIOTPTR=TIOTPTR+TIOELNGH;/* GET ADDR OF NEXT TIOT ENTRY*/ 00127000
@RF00167 SLR   @14,@14                                             0169 00128000
         IC    @14,TIOELNGH(,TIOTPTR)                              0169 00129000
         ALR   TIOTPTR,@14                                         0169 00130000
*           END;                                                   0170 00131000
@DE00166 CLI   TIOELNGH(TIOTPTR),0                                 0170 00132000
         BNE   @DL00166                                            0170 00133000
*           TIOTPTR=DMPTIOTA;       /* GET ADDR OF TIOT ENTRY FOR  0171 00134000
*                                      DUMP DATASET                  */ 00135000
         LR    TIOTPTR,DMPTIOTA                                    0171 00136000
*           IF TIOTPTR=0|TIOEFSRT=0&/* IF TIOT ENTRY ADDR IS ZERO OR    00137000
*                                      UCB ADDR IS ZERO AND          */ 00138000
*               TIOTTERM='0'B&      /* DEVICE IS TERMINAL            */ 00139000
*               TIOEDYNM='0'B&      /* DYNAMIC ALLOC ENTRY           */ 00140000
*               TIOESYIN='0'B&      /* SYSIN ENTRY                   */ 00141000
*               TIOESYOT='0'B       /* SYSOUT ENTRY                  */ 00142000
*             THEN                  /* ABSENCE OF INDICATOR INDICATES   00143000
*                                      DD DUMMY                      */ 00144000
         LTR   TIOTPTR,TIOTPTR                                     0172 00145000
         BZ    @RT00172                                            0172 00146000
         ICM   @14,7,TIOEFSRT(TIOTPTR)                             0172 00147000
         BNZ   @RF00172                                            0172 00148000
         TM    TIOTTERM(TIOTPTR),B'00110110'                       0172 00149000
         BNZ   @RF00172                                            0172 00150000
@RT00172 DS    0H                                                  0173 00151000
*             RTM2NDMP='1'B;        /* INDICATE NO DUMP TO BE TAKEN  */ 00152000
         OI    RTM2NDMP(RTM2PTR),B'10000000'                       0173 00153000
*           ELSE                                                   0174 00154000
*             DO;                                                  0174 00155000
         B     @RC00172                                            0174 00156000
@RF00172 DS    0H                                                  0175 00157000
*               RTM2DD=TIOEDDNM;    /* SAVE DDNAME IN RTM2WA         */ 00158000
         MVC   RTM2DD(8,RTM2PTR),TIOEDDNM(TIOTPTR)                 0175 00159000
*               IF RTM2DD='SYSABEND'&/* SYSABEND DD AND              */ 00160000
*                   RTCTISAB='1'B THEN/* IGNORE SYSABEND DUMPS       */ 00161000
         CLC   RTM2DD(8,RTM2PTR),@CC03621                          0176 00162000
         BNE   @RF00176                                            0176 00163000
         L     @14,CVTPTR                                          0176 00164000
         L     @14,CVTRTMCT(,@14)                                  0176 00165000
         TM    RTCTISAB+236(@14),B'00000001'                       0176 00166000
         BNO   @RF00176                                            0176 00167000
*                 RTM2NDMP='1'B;    /* INDICATE NO DUMP TO BE TAKEN  */ 00168000
         OI    RTM2NDMP(RTM2PTR),B'10000000'                       0177 00169000
*               ELSE                                               0178 00170000
*                 IF RTM2DD='SYSUDUMP'&/* SYSUDUMP DD AND            */ 00171000
*                     RTCTISYU='1'B THEN/* IGNORE SYSUDUMP DUMPS     */ 00172000
         B     @RC00176                                            0178 00173000
@RF00176 CLC   RTM2DD(8,RTM2PTR),@CC03622                          0178 00174000
         BNE   @RF00178                                            0178 00175000
         L     @14,CVTPTR                                          0178 00176000
         L     @14,CVTRTMCT(,@14)                                  0178 00177000
         TM    RTCTISYU+236(@14),B'00000001'                       0178 00178000
         BNO   @RF00178                                            0178 00179000
*                   RTM2NDMP='1'B;  /* INDICATE NO DUMP TO BE TAKEN  */ 00180000
         OI    RTM2NDMP(RTM2PTR),B'10000000'                       0179 00181000
*                 ELSE              /*                       @G33SPHW*/ 00182000
*                   IF RTM2DD='SYSMDUMP'&/* SYSMDUMP DD AND  @G33SPHW*/ 00183000
*                       RTCTISYM='1'B THEN/* IGNORE SYSMDUMP DUMPS 0180 00184000
*                                                            @G33SPHW*/ 00185000
