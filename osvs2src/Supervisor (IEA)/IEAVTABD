*                   RFY                                            0361 00784000
*                     RTM2SWA UNRSTD;/*                      @YM01200*/ 00785000
*                 END;                                             0362 00786000
*               RTM2SNCC=4;         /* INDICATE OPEN FAILURE TO RTM2 */ 00787000
@RF00336 MVC   RTM2SNCC(4,RTM2PTR),@CF00103                        0363 00788000
*               PARMPTR=ADDR(ENQPARMS);/* GET ADDRESS OF DEQ       0364 00789000
*                                      PARAMETER LIST                */ 00790000
