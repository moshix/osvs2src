@RF00178 CLC   RTM2DD(8,RTM2PTR),@CC03623                          0180 00187000
         BNE   @RF00180                                            0180 00188000
         L     @14,CVTPTR                                          0180 00189000
         L     @14,CVTRTMCT(,@14)                                  0180 00190000
         TM    RTCTISYM+236(@14),B'00000001'                       0180 00191000
         BNO   @RF00180                                            0180 00192000
*                     RTM2NDMP='1'B;/* INDICATE NO DUMP TAKEN      0181 00193000
*                                                            @G33SPHW*/ 00194000
         OI    RTM2NDMP(RTM2PTR),B'10000000'                       0181 00195000
*             END;                                                 0182 00196000
@RF00180 DS    0H                                                  0182 00197000
@RC00178 DS    0H                                                  0182 00198000
@RC00176 DS    0H                                                  0183 00199000
*         END;                                                     0183 00200000
@RC00172 DS    0H                                                  0184 00201000
*       IF RTM2NDMP='1'B THEN       /* SHOULD DUMP BE TAKEN          */ 00202000
@RC00161 TM    RTM2NDMP(RTM2PTR),B'10000000'                       0184 00203000
         BNO   @RF00184                                            0184 00204000
*         RTM2SNCC=0;               /* NO, EITHER NO SYSABEND      0185 00205000
*                                      ,SYSUDUMP OR SYSMDUMP DD FOUND   00206000
*                                                            @G33SPHW*/ 00207000
         SLR   @14,@14                                             0185 00208000
         ST    @14,RTM2SNCC(,RTM2PTR)                              0185 00209000
*       ELSE                                                       0186 00210000
*         DO;                                                      0186 00211000
*                                                                  0186 00212000
         B     @RC00184                                            0186 00213000
@RF00184 DS    0H                                                  0187 00214000
*           /*********************************************************/ 00215000
*           /*                                                       */ 00216000
*           /* THE DUMP DATA SET IS ENQUEUED UPON USING A MAJOR NAME */ 00217000
*           /* OF SYSIEA01 AND A MINOR NAME OF IEA. THE ENQ PARAMETER*/ 00218000
*           /* LIST IS BUILT IN THE RTM2 WORK AREA.                  */ 00219000
*           /*                                                       */ 00220000
*           /*********************************************************/ 00221000
*                                                                  0187 00222000
*           ENQFLAGS='FF030000'X;   /* INDICATE ONE ENTRY & MINOR  0187 00223000
*                                      NAME LENGTH OF 3              */ 00224000
         MVC   ENQFLAGS(4,RTM2PTR),@CB03624                        0187 00225000
*           MAJORAD=ADDR(MAJOR);    /* GET ADDRESS OF MAJOR NAME     */ 00226000
         LA    @14,MAJOR(,RTM2PTR)                                 0188 00227000
         ST    @14,MAJORAD(,RTM2PTR)                               0188 00228000
*           MINORAD=ADDR(MINOR);    /* GET ADDRESS OF MINOR NAME     */ 00229000
         LA    @14,MINOR(,RTM2PTR)                                 0189 00230000
         ST    @14,MINORAD(,RTM2PTR)                               0189 00231000
*           MAJOR='SYSIEA01';       /* MAJOR NAME                    */ 00232000
         MVC   MAJOR(8,RTM2PTR),@CC03625                           0190 00233000
*           MINOR='IEA';            /* MINOR NAME                    */ 00234000
         MVC   MINOR(3,RTM2PTR),@CC03626                           0191 00235000
*           PARMPTR=ADDR(ENQPARMS); /* PUT POINTER TO PARMETER LIST     00236000
*                                      IN R1                         */ 00237000
         LA    PARMPTR,ENQPARMS(,RTM2PTR)                          0192 00238000
*           RTM2EENQ='1'B;          /* INDICATE ENQ IN CNTRL         */ 00239000
*                                                                  0193 00240000
         OI    RTM2EENQ(RTM2PTR),B'10000000'                       0193 00241000
*           /*********************************************************/ 00242000
*           /*                                                       */ 00243000
*           /* IF THIS ENTRY IS FOR CANCEL, DO ENQUEUE TEST FIRST TO */ 00244000
*           /* ENSURE THAT DUMP DATASET IS REALLY FREE. NORMAL ENQ   */ 00245000
*           /* WILL CAUSE ENQ STEAL BECAUSE SUBTASKS ARE             */ 00246000
*           /* NON-DISPATCHABLE                              @ZA05491*/ 00247000
*           /*                                                       */ 00248000
*           /*********************************************************/ 00249000
*                                                                  0194 00250000
*           IF RTM2CNCL='1'B THEN   /* SUBTASKS NON-DISPATCHABLE?  0194 00251000
*                                                            @ZA05491*/ 00252000
         TM    RTM2CNCL(RTM2PTR),B'01000000'                       0194 00253000
         BNO   @RF00194                                            0194 00254000
*             DO;                   /*                       @ZA05491*/ 00255000
*               GEN(ENQ   (,,E,,STEP),RET=TEST,MF=(E,(PARMPTR)));/*     00256000
*                                                            @ZA05491*/ 00257000
         ENQ   (,,E,,STEP),RET=TEST,MF=(E,(PARMPTR))                    00258000
*               ENQFLAGS='FF030000'X;/* RESET PARM FLAGS     @ZA05491*/ 00259000
         MVC   ENQFLAGS(4,RTM2PTR),@CB03624                        0197 00260000
*               PARMPTR=ADDR(ENQPARMS);/* RESET PARAMETER REG      0198 00261000
*                                                            @ZA05491*/ 00262000
         LA    PARMPTR,ENQPARMS(,RTM2PTR)                          0198 00263000
*             END;                  /* END OF CANCEL PROCESSING    0199 00264000
*                                                            @ZA05491*/ 00265000
*           IF RETCODE=0            /* ENQUEUE TEST SUCCESSFUL     0200 00266000
*                                                            @ZA05491*/ 00267000
*               |RTM2CNCL='0'B THEN /* OR NOT NEEDED         @ZA05491*/ 00268000
@RF00194 LTR   RETCODE,RETCODE                                     0200 00269000
         BZ    @RT00200                                            0200 00270000
         TM    RTM2CNCL(RTM2PTR),B'01000000'                       0200 00271000
         BNZ   @RF00200                                            0200 00272000
@RT00200 DS    0H                                                  0201 00273000
*             GENERATE;                                            0201 00274000
         ENQ   (,,E,,STEP),RET=HAVE,MF=(E,(PARMPTR))                    00275000
*             RTM2SNCC=RETCODE;     /* SAVE RESTURN CODE             */ 00276000
@RF00200 ST    RETCODE,RTM2SNCC(,RTM2PTR)                          0202 00277000
*           IF RETCODE=0 THEN       /* WAS ENQ SUCCESSFUL            */ 00278000
         LTR   RETCODE,RETCODE                                     0203 00279000
         BNZ   @RF00203                                            0203 00280000
*             DO;                   /* YES                           */ 00281000
*               RTM2EENQ='0'B;      /* TURN OFF ENQ IN CNTRL BIT     */ 00282000
         NI    RTM2EENQ(RTM2PTR),B'01111111'                       0205 00283000
*               RTM2DENQ='1'B;      /* INDICATE ENQ COMPLETED        */ 00284000
         OI    RTM2DENQ(RTM2PTR),B'10000000'                       0206 00285000
*             END;                                                 0207 00286000
*           ELSE                                                   0208 00287000
*             DO;                                                  0208 00288000
         B     @RC00203                                            0208 00289000
@RF00203 DS    0H                                                  0209 00290000
*               PARMPTR=ADDR(SDUMPE);/* ENG FAILURE                  */ 00291000
         LA    PARMPTR,SDUMPE                                      0209 00292000
*               GEN(SDUMP MF=(E,(1)));                             0210 00293000
         SDUMP MF=(E,(1))                                               00294000
*             END;                                                 0211 00295000
*           IF RTM2SNCC=0 THEN      /* WAS ENQ WUCCESSFUL            */ 00296000
@RC00203 ICM   @14,15,RTM2SNCC(RTM2PTR)                            0212 00297000
         BNZ   @RF00212                                            0212 00298000
*             DO;                   /* YES                           */ 00299000
*                                                                  0213 00300000
*               /*****************************************************/ 00301000
*               /*                                                   */ 00302000
*               /* CORE FOR THE DUMP DATA SET DCB & PARMLIST IS      */ 00303000
*               /* OBTAINED FROM SP230. (88 BYTES FOR DCB, 8 BYTES   */ 00304000
*               /* FOR PARMLIST)                             @YM01200*/ 00305000
*               /* CORE IS ALSO NEEDED FOR THE QMNGRIO WORKAREA AND  */ 00306000
*               /* PARMLIST, AND AN AREA TO READ IN THE JFCB AND     */ 00307000
*               /* JFCBE                                     @Z40MPXH*/ 00308000
*               /* IF CORE IS NOT AVAILABLE, RETURN TO RTM2 WITH     */ 00309000
*               /* EIGHT RETURN CODE. OTHERWISE , FOR SYSABEND OR    */ 00310000
*               /* SYSUDUMPS THE DCB IS INITALIZED WITH DSORG=PS,    */ 00311000
*               /* MACRF=W, RECFM=VBA, BLKSIZE=1632, LRECL=125,      */ 00312000
*               /* DDNAME=SYSABEND/SYSUDUMP FOR SYSMDUMPS THE DCB IS */ 00313000
*               /* INITIALIZED FOR SDUMP LRECL=4104,                 */ 00314000
*               /* BLKSIZE=4104,RECFM=FB,MACRF=W,DDNAME=SYSMDUMP     */ 00315000
*               /*                                           @G33SPHW*/ 00316000
*               /*                                                   */ 00317000
*               /*****************************************************/ 00318000
*                                                                  0214 00319000
*               GENERATE REFS(RTM2RREG);                           0214 00320000
         STM   0,15,RTM2RREG(RTM2PTR) SAVE REGS FOR USE BY RECOVERY RTN 00321000
*ADGET:                                                            0215 00322000
*               RFY                                                0215 00323000
*                (RTM2SWA) RSTD;                                   0215 00324000
ADGET    DS    0H                                                  0216 00325000
*               RFY                                                0216 00326000
*                 R3 RSTD;                                         0216 00327000
*               R3=TCBPKF;          /* GET TCB KEY           @YM01200*/ 00328000
         L     @14,TCBPTR                                          0217 00329000
         SLR   R3,R3                                               0217 00330000
         IC    R3,TCBPKF(,@14)                                     0217 00331000
*               RFY                                                0218 00332000
*                 R0 RSTD;                                         0218 00333000
*               R0=LENGTH(COREAREA);/* AREAS MENTIONED ABOVE @Z40MPXH*/ 00334000
         LA    R0,503                                              0219 00335000
*               RTM2EGET='1'B;      /* INDICATE GETMAIN IN CNTRL     */ 00336000
         OI    RTM2EGET(RTM2PTR),B'01000000'                       0220 00337000
*               RTM2SWA=RTM2PTR;    /* SAVE WKAREA ADDR      @YM01200*/ 00338000
         LR    RTM2SWA,RTM2PTR                                     0221 00339000
*               RFY                                                0222 00340000
*                 ASCBPTR RSTD;     /*                       @G33SPHW*/ 00341000
*               ASCBPTR=PSAAOLD;    /* GET CURRENT ASCB PTR  @YM01200*/ 00342000
         L     ASCBPTR,PSAAOLD                                     0223 00343000
*               RFY                                                0224 00344000
*                 R4 RSTD;          /*                       @G33SPHW*/ 00345000
*               R4=TCBPTR;          /* PUT TCB ADDRESS IN R4 FOR   0225 00346000
*                                      GETMAIN               @G33SPHW*/ 00347000
         LR    R4,@14                                              0225 00348000
*               GENERATE REFS(CVTPTR,CVTCRMN,CVTMAP,RTM2RMSA);     0226 00349000
*                                                          /*@YM01200*/ 00350000
         STM R11,R14,RTM2RMSA(RTM2SWA)                                  00351000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=('IEAVTABD')     00352000
         GETMAIN RC,LV=(R0),SP=230,BRANCH=YES,KEY=(3)          @YM01200 00353000
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=('IEAVTABD')       @YM01200 00354000
         LM R11,R14,RTM2RMSA(RTM2SWA)                          @YM01200 00355000
*               RFY                                                0227 00356000
*                (ASCBPTR,                                         0227 00357000
*                 R4) UNRSTD;       /*                       @G33SPHW*/ 00358000
*               RFY                                                0228 00359000
*                (R3,                                              0228 00360000
*                 R0) UNRSTD;                                      0228 00361000
*               RTM2PTR=RTM2SWA;    /* RESTORE RTM2 WKAREA ADDR    0229 00362000
*                                                            @YM01200*/ 00363000
         LR    RTM2PTR,RTM2SWA                                     0229 00364000
*               RFY                                                0230 00365000
*                 RTM2SWA UNRSTD;   /*                       @YM01200*/ 00366000
*               RTM2EGET='0'B;      /* TURN OFF GETMAIN IN CNTRL BIT */ 00367000
         NI    RTM2EGET(RTM2PTR),B'10111111'                       0231 00368000
*               RTM2DGET='1'B;      /* INDICATE GETMAIN FOR DCB    0232 00369000
*                                      COMPLETED                     */ 00370000
         OI    RTM2DGET(RTM2PTR),B'01000000'                       0232 00371000
*               IF RETCODE^=0       /* WAS GETMAIN SUCCESSFUL        */ 00372000
*                 THEN              /* NO                            */ 00373000
         LTR   RETCODE,RETCODE                                     0233 00374000
         BZ    @RF00233                                            0233 00375000
*                 DO;                                              0234 00376000
