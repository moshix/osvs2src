         L     @14,TCBPTR                                          0316 00648000
         SLR   R9,R9                                               0316 00649000
         IC    R9,TCBPKF(,@14)                                     0316 00650000
*                 SPKA(R9);         /* SET PSW KEY                   */ 00651000
         SPKA  0(R9)                                               0317 00652000
*                 RESPECIFY                                        0318 00653000
*                  (GPR02F) UNRESTRICTED;                          0318 00654000
*               END;                /* MODESET SAVEKEY(RTM2PKEY)   0319 00655000
*                                      EXTKEY(TCB) WORKREG(R9)     0319 00656000
*                                                            @YM00100*/ 00657000
*               GEN(OPEN  MF=(E,(R11))  OPEN DUMP DATA SET);       0320 00658000
         OPEN  MF=(E,(R11))  OPEN DUMP DATA SET                         00659000
*               DO;                 /* MODESET KEYADDR(RTM2PKEY)   0321 00660000
*                                      WORKREG(R9)                   */ 00661000
*                 R9=RTM2PKEY;      /* GET KEY FORM SAVE LOCATION    */ 00662000
         SLR   R9,R9                                               0322 00663000
         IC    R9,RTM2PKEY(,RTM2PTR)                               0322 00664000
*                 SPKA(R9);         /* SET PSW KEY                   */ 00665000
         SPKA  0(R9)                                               0323 00666000
*               END;                /* MODESET KEYADDR(RTM2PKEY)   0324 00667000
*                                      WORKREG(R9) RETURN KEY0     0324 00668000
*                                                            @YM00100*/ 00669000
*               RFY                                                0325 00670000
*                (R9,                                              0325 00671000
*                 R11) UNRSTD;                                     0325 00672000
*               RTM2EOPN='0'B;      /* TURN OFF OPEN IN CNTRL BIT    */ 00673000
         NI    RTM2EOPN(RTM2PTR),B'11011111'                       0326 00674000
*               RTM2DOPN='1'B;      /* INDICATE OPEN COMPLETED       */ 00675000
         OI    RTM2DOPN(RTM2PTR),B'00100000'                       0327 00676000
