*TSLO:                                                             0944 02845000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0944 02846000
*                                                                  0944 02847000
TSLO     DS    0H                                                  0945 02848000
*   /*****************************************************************/ 02849000
*   /*                                                               */ 02850000
*   /* TSLO - SUBROUTINE OF ABDUMP TSLO SELECTS AND RETURNS TO THE   */ 02851000
*   /* CALLER THE ADDRESS OF THE LOWEST LEVEL TCB IN THE TREE TO BE  */ 02852000
*   /* SEARCHED. ON THE FIRST CALL TO TSLO, THE INPUT TCB ADDRESS    */ 02853000
*   /* MUST BE THE COMPLEMENT OF THE ADDRESS OF THE TOP TCB IN THE   */ 02854000
*   /* TREE BEING SEARCHED. TSLO RETURNS THE ADDRESS OF THE LOWEST   */ 02855000
*   /* LEVEL                                                  SUBTASK*/ 02856000
*   /* IN A DIRECT LINE FROM THE INPUT TCB. ON SUBSEQUENT CALLS TO   */ 02857000
*   /* TSLO, THE INPUT TCB ADDRESS SHOULD BE THE SAME AS THAT LAST   */ 02858000
*   /* RETURNED TO THE CALLER. IF THE TCB PASSED AS INPUT TO TSLO HAS*/ 02859000
*   /* A SISTER TASK, TSLO RETURNS THE ADDRESS OF THE LOWEST LEVEL   */ 02860000
*   /* SUBTASK OF THE SISTER (IF ANY). IF THE SISTER DOES NOT HAVE   */ 02861000
*   /* ANY SUBTASKS, TSLO RE- TURNS THE ADDRESS OF THE SISTER TCB. IF*/ 02862000
*   /* THE INPUT TCB DOES NOT HAVE A SISTER, TSLO RETURNS THE ADDRESS*/ 02863000
*   /* OF ITS MOTHER TCB. INPUT: R9 - ADDRESS OF TCB AT WHICH TO     */ 02864000
*   /* START SEARCH R13- ADDRESS OF REGISTER SAVE AREA R14- RETURN   */ 02865000
*   /* ADDRESS OUTPUT: R8 - ADDRESS OF SELECTED TCB                  */ 02866000
*   /*                                                               */ 02867000
*   /*****************************************************************/ 02868000
*                                                                  0945 02869000
*   RFY                                                            0945 02870000
*    (TSLORETR,                                                    0945 02871000
*     TSLOTCBP) RSTD;                                              0945 02872000
*   RFY                                                            0946 02873000
*     R0 RSTD;                                                     0946 02874000
*   R0=R13;                         /* SAVE SAVE AREA REG            */ 02875000
         LR    R0,R13                                              0947 02876000
*   GENERATE REFS(PSALITA);                                        0948 02877000
         STM  R14,R12,12(R13)     SAVE REGISTERS                        02878000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=('ABDUMP')       02879000
*   R13=R0;                         /* RESTORE R13                   */ 02880000
         LR    R13,R0                                              0949 02881000
*   RFY                                                            0950 02882000
*     R0 UNRSTD;                                                   0950 02883000
*   RESPECIFY                                                      0951 02884000
*     TCB BASED(TSLOTCBP);                                         0951 02885000
*   TSLOMTH=0;                      /* INIT REG TO HOLD TBCOTC TO 0  */ 02886000
         SLR   TSLOMTH,TSLOMTH                                     0952 02887000
*   TSLOZERO=0;                     /* INIT. TCB COMPARE REG TO 0    */ 02888000
         SLR   TSLOZERO,TSLOZERO                                   0953 02889000
*   IF TSLOTCBP<TSLOZERO THEN       /* IS THIS THE FIRST ENTRY       */ 02890000
         CR    TSLOTCBP,TSLOZERO                                   0954 02891000
         BNL   @RF00954                                            0954 02892000
*     TSLOTCBP=-TSLOTCBP;           /* MAKE INPUT TCB ADDR POSITIVE  */ 02893000
         LCR   TSLOTCBP,TSLOTCBP                                   0955 02894000
*   ELSE                            /* NOT FIRST ENTRY               */ 02895000
*     DO;                                                          0956 02896000
         B     @RC00954                                            0956 02897000
@RF00954 DS    0H                                                  0957 02898000
*       IF TCBNTC^=TSLOZERO THEN    /* DOES INPUT TCB HAVE A SISTER  */ 02899000
         L     @04,TCBNTC(,TSLOTCBP)                               0957 02900000
