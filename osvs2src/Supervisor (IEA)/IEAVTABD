         LA    PARMPTR,RTM2SPRM(,RTM2PTR)                          0997 03007000
*       RTM2PTR->RTM2DPLA=R1;       /*                       @Z40MPXH*/ 03008000
         ST    R1,RTM2DPLA(,RTM2PTR)                               0998 03009000
*       RTM2PTR->RTM2DD=R11->RTM2DD;/* DDNAME FOR DUMP DS    @Z40MPXH*/ 03010000
         MVC   RTM2DD(8,RTM2PTR),RTM2DD(R11)                       0999 03011000
*       RTM2PTR->RTM2ABDR=R11->RTM2ABDR;/* ABDUMP FLAGS      @Z40MPXH*/ 03012000
         MVC   RTM2ABDR(2,RTM2PTR),RTM2ABDR(R11)                   1000 03013000
*       RTM2PTR->RTM2DCTL=R11->RTM2DCTL;/* FUNCTIONS COMPLETED     1001 03014000
*                                                            @Z40MPXH*/ 03015000
         MVC   RTM2DCTL(1,RTM2PTR),RTM2DCTL(R11)                   1001 03016000
*       RTM2PTR->RTM2ECTL=R11->RTM2ECTL;/* EXTERNAL ROUTINES @Z40MPXH*/ 03017000
         MVC   RTM2ECTL(1,RTM2PTR),RTM2ECTL(R11)                   1002 03018000
*       RTM2PTR->RTM2RMWS(1)=R11->RTM2RMWS(1);/* ENQ,GETMAIN,OPEN  1003 03019000
*                                                            @Z40MPXH*/ 03020000
         MVC   RTM2RMWS(4,RTM2PTR),RTM2RMWS(R11)                   1003 03021000
*       RTM2PTR->RTM2SNCC=R11->RTM2SNCC;/* RETURN CODE       @Z40MPXH*/ 03022000
         MVC   RTM2SNCC(4,RTM2PTR),RTM2SNCC(R11)                   1004 03023000
*       RFY                                                        1005 03024000
*         R0 RSTD;                                                 1005 03025000
*       R0=80;                      /* MESSAGE BUFFER LENGTH @Z40MPXH*/ 03026000
         LA    R0,80                                               1006 03027000
*       GENERATE REFS(R0);                                         1007 03028000
*                                                          /*@Z40MPXH*/ 03029000
    GETMAIN  RC,LV=(R0),SP=253                               @Z40MPXH   03030000
*       RFY                                                        1008 03031000
*         R0 UNRSTD;                                               1008 03032000
*       IF RETCODE=0 THEN           /* SUCCESSFUL GETMAIN    @Z40MPXH*/ 03033000
         LTR   RETCODE,RETCODE                                     1009 03034000
         BNZ   @RF01009                                            1009 03035000
*         DO;                       /* ISSUE MESSAGE         @Z40MPXH*/ 03036000
*           RFY                                                    1011 03037000
*            (R10) RSTD;            /*                       @Z40MPXH*/ 03038000
*           R10=PARMPTR;            /* SAVE ADDR OF BUFFER   @Z40MPXH*/ 03039000
         LR    R10,PARMPTR                                         1012 03040000
*           BUFFER(1:72)=QMIOMSG;   /* MOVE MSG INTO BUFFER  @Z40MPXH*/ 03041000
*                                                                  1013 03042000
         MVC   BUFFER(72,PARMPTR),QMIOMSG                          1013 03043000
*           /*********************************************************/ 03044000
*           /*                                                       */ 03045000
*           /* ISSUE MESSAGE AND FREE BUFFER                 @Z40MPXH*/ 03046000
*           /*                                                       */ 03047000
*           /*********************************************************/ 03048000
*                                                                  1014 03049000
*           GENERATE;                                              1014 03050000
*                                                          /*@Z40MPXH*/ 03051000
     WTO   MF=(E,(PARMPTR)),          ROUTE MESSAGE TO PROG   Z40MPXH   03052000
     FREEMAIN  R,LV=80,SP=253,A=(10)   FREE MSG BUFFER        Z40MPXH   03053000
*         END;                      /*                       @Z40MPXH*/ 03054000
*       DCBPTR=ADDR(DCBAREA);       /*                       @Z40MPXH*/ 03055000
@RF01009 L     @04,COREPTR                                         1016 03056000
         ST    @04,DCBPTR                                          1016 03057000
*       DCBBUF=DCBIMAGE;            /* USE STANDARD FORMAT   @Z40MPXH*/ 03058000
         MVC   DCBBUF(88,@04),DCBIMAGE                             1017 03059000
*       DCBDDNAM=R7->RTM2DD;        /* MOVE DDNAME INTO DCB  @Z40MPXH*/ 03060000
         MVC   DCBDDNAM+40(8,@04),RTM2DD(R7)                       1018 03061000
*       RTM2WARG=8;                 /* WORKAREA REG          @G33SPHW*/ 03062000
         MVI   RTM2WARG(R11),X'08'                                 1019 03063000
*       GOTO ADQMEND;               /* RETRY TO TAKE DUMP    @Z40MPXH*/ 03064000
         B     ADQMEND                                             1020 03065000
*       RFY                                                        1021 03066000
*        (R10) UNRSTD;              /*                       @Z40MPXH*/ 03067000
*     END;                          /* END RETRY SETUP       @Z40MPXH*/ 03068000
*   END ADQMRCV;                    /* END QMNGRIO RECOVERY  @Z40MPXH*/ 03069000
         EJECT                                                          03070000
*ADRECOV:                                                          1024 03071000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               1024 03072000
*                                                                  1024 03073000
ADRECOV  DS    0H                                                  1025 03074000
*   /*****************************************************************/ 03075000
*   /*                                                               */ 03076000
*   /* THE ABDUMP RECOVERY ROUTINE IS CALLED BY THE RTM2 RECURSION   */ 03077000
*   /* HANDLER. ADRECOV DETERMINES FROM INDICATORS SET IN THE MAIN-  */ 03078000
*   /* LINE ROUTINE WHAT PROCESSING WAS IN PROGRESS OR HAD COMPLETED */ 03079000
*   /* SUCCESSFULLY WHEN ABDUMP GAVE UP CONTROL. ON A RETRY CONDITION*/ 03080000
*   /* CONTROL RETURNS TO MAINLINE ABDUMP. OTHERWISE, CONTROL RETURNS*/ 03081000
*   /* TO THE RTM2 RECURSION HANDLER. INPUT: R7 ADDR OF CURRENT      */ 03082000
*   /* RTM2WA R13 ADDR OF SAVE AREA(RTM2) PASSED TO MAINLINE ABDUMP  */ 03083000
*   /*                                                               */ 03084000
*   /*****************************************************************/ 03085000
*                                                                  1025 03086000
*   RFY                                                            1025 03087000
*     R11 RSTD;                                                    1025 03088000
*   R11=R7->RTM2PRWA;               /* GET PTR TO PREVIOUS WORK AREA    03089000
*                                                            @ZM40320*/ 03090000
         L     R11,RTM2PRWA(,R7)                                   1026 03091000
*   RTM2TRRA=0;                     /* PREVENT ABDUMP FROM BEING   1027 03092000
*                                      ENTERED RECURSIVELY           */ 03093000
         SLR   @10,@10                                             1027 03094000
         ST    @10,RTM2TRRA(,R11)                                  1027 03095000
*   RTM2WARG=0;                     /* ZERO WORK AREA ADDRESS        */ 03096000
         MVI   RTM2WARG(R11),X'00'                                 1028 03097000
*   IF RTM2MCHK='1'B|               /* DID MACHINE CHECK OR          */ 03098000
*       RTM2PCHK='1'B THEN          /* PROGRAM CHECK OCCUR           */ 03099000
         TM    RTM2MCHK(R11),B'11000000'                           1029 03100000
         BZ    @RF01029                                            1029 03101000
*     DO;                           /* YES                           */ 03102000
*       RESPECIFY                                                  1031 03103000
*         RTM2WA BASED(R11);        /* GET PREVIOUS WORK AREA PTR    */ 03104000
*       CALL ADRCLN;                /* DETERMINE CLEAN-UP TO BE DONE */ 03105000
         BAL   @14,ADRCLN                                          1032 03106000
*     END;                                                         1033 03107000
*   ELSE                            /* DETERMINE IF AN EXTERNAL    1034 03108000
*                                      ROUTINE CALLED BY ABDUMP    1034 03109000
*                                      SUFFERED AN ERROR. IF SO,   1034 03110000
*                                      CONTINUE WITH TERMINATION     */ 03111000
*AD1:                                                              1034 03112000
*     DO;                           /* PREV WORKAREA BEING EXAMINED  */ 03113000
         B     @RC01029                                            1034 03114000
@RF01029 DS    0H                                                  1034 03115000
AD1      DS    0H                                                  1035 03116000
*       IF RTM2EGET='1'B|           /* GETMAIN FOR DCB IN CONTROL?   */ 03117000
*           RTM2EOPN='1'B|          /* OPEN IN CONTROL?              */ 03118000
*           RTM2ESNP='1'B|          /* SNAP IN CONTROL?              */ 03119000
*           RTM2ECLS='1'B|          /* CLOSE IN CONTROL?             */ 03120000
*           RTM2EFRM='1'B|          /* FREEMAIN FOR DCB IN CONTROL?  */ 03121000
*           RTM2SMDP='1'B|          /* SYSMDUMP IN CONTROL?  @G33SPHW*/ 03122000
*           RTM2EDEQ='1'B THEN      /* DEQ IN CONTROL                */ 03123000
         TM    RTM2EGET(R11),B'01111100'                           1035 03124000
         BNZ   @RT01035                                            1035 03125000
         TM    RTM2SMDP(R11),B'01000000'                           1035 03126000
         BO    @RT01035                                            1035 03127000
         TM    RTM2EDEQ(R11),B'00000010'                           1035 03128000
         BNO   @RF01035                                            1035 03129000
@RT01035 DS    0H                                                  1036 03130000
*         CALL ADRCLN;              /* PERFORM CLEAN-UP IF NECESSARY */ 03131000
         BAL   @14,ADRCLN                                          1036 03132000
*       ELSE                                                       1037 03133000
*AD2:                                                              1037 03134000
*         DO;                                                      1037 03135000
         B     @RC01035                                            1037 03136000
@RF01035 DS    0H                                                  1037 03137000
AD2      DS    0H                                                  1038 03138000
*           IF RTM2DDEQ='1'B        /* DID DEQ COMPLETE?             */ 03139000
*             THEN                                                 1038 03140000
         TM    RTM2DDEQ(R11),B'00000010'                           1038 03141000
         BO    @RT01038                                            1038 03142000
*             ;                     /* YES, RETURN TO RTM2           */ 03143000
*           ELSE                                                   1040 03144000
*AD3:                                                              1040 03145000
*             DO;                                                  1040 03146000
*                                                                  1040 03147000
AD3      DS    0H                                                  1041 03148000
*               /*****************************************************/ 03149000
*               /*                                                   */ 03150000
*               /* REFRESH ALL FIELDS USED BY ABDUMP IN CURRENT      */ 03151000
*               /* WORKAREA                                          */ 03152000
*               /*                                                   */ 03153000
*               /*****************************************************/ 03154000
*                                                                  1041 03155000
*               RTM2PTR->RTM2DTCB=R11->RTM2DTCB;/* TASK BEING DMPED  */ 03156000
         MVC   RTM2DTCB(4,RTM2PTR),RTM2DTCB(R11)                   1041 03157000
*               RTM2PTR->RTM2SPRM=R11->RTM2SPRM;/* BASE OF SNAPPARM     03158000
*                                      LIST                  @YM04727*/ 03159000
         MVC   RTM2SPRM(20,RTM2PTR),RTM2SPRM(R11)                  1042 03160000
*               PARMPTR=ADDR(RTM2PTR->RTM2SPRM);/* PTR TO PARM LIST     03161000
*                                      FOR SNAP              @YM04727*/ 03162000
         LA    PARMPTR,RTM2SPRM(,RTM2PTR)                          1043 03163000
*               RTM2PTR->RTM2DPLA=PARMPTR;                         1044 03164000
         ST    PARMPTR,RTM2DPLA(,RTM2PTR)                          1044 03165000
*               RTM2PTR->RTM2DD=R11->RTM2DD;/* DDNAME FOR DUMP DATA     03166000
*                                      SET                           */ 03167000
         MVC   RTM2DD(8,RTM2PTR),RTM2DD(R11)                       1045 03168000
*               RTM2PTR->RTM2ABDR=R11->RTM2ABDR;/* ABDUMP FLAGS      */ 03169000
         MVC   RTM2ABDR(2,RTM2PTR),RTM2ABDR(R11)                   1046 03170000
*               RTM2PTR->RTM2DCTL=R11->RTM2DCTL;/* FUNCTIONS COMPLE-    03171000
*                                      TED IN ABDUMP                 */ 03172000
         MVC   RTM2DCTL(1,RTM2PTR),RTM2DCTL(R11)                   1047 03173000
*               RTM2PTR->RTM2ECTL=R11->RTM2ECTL;/* EXT. RTNS CALLED  */ 03174000
         MVC   RTM2ECTL(1,RTM2PTR),RTM2ECTL(R11)                   1048 03175000
*               RTM2PTR->RTM2RMWS(1)=R11->RTM2RMWS(1);/* ENQ,      1049 03176000
*                                      GETMAIN, OPEN PARMLISTS       */ 03177000
         MVC   RTM2RMWS(4,RTM2PTR),RTM2RMWS(R11)                   1049 03178000
*               RTM2PTR->RTM2SNCC=R11->RTM2SNCC;/* RETURN CODE       */ 03179000
         MVC   RTM2SNCC(4,RTM2PTR),RTM2SNCC(R11)                   1050 03180000
*               IF RTM2DFRM='1'B THEN/* DID FREEMAIN COMPLETE?       */ 03181000
         TM    RTM2DFRM(R11),B'00000100'                           1051 03182000
         BO    @RT01051                                            1051 03183000
*                 GOTO ADDEQ;       /* DEQ FROM DUMP DATA SET        */ 03184000
*               ELSE                                               1053 03185000
*                 IF RTM2DCLS='1'B THEN/* DID CLOSE COMPLETE?        */ 03186000
         TM    RTM2DCLS(R11),B'00001000'                           1053 03187000
         BO    @RT01053                                            1053 03188000
*                   GOTO ADFREE;    /* FREE DCB CORE & DEQ           */ 03189000
*                 ELSE                                             1055 03190000
*                   IF RTM2DSNP='1'B THEN/* DID SNAP COMPLETE?       */ 03191000
         TM    RTM2DSNP(R11),B'00010000'                           1055 03192000
         BO    @RT01055                                            1055 03193000
*                     GOTO ADCLOSE; /* CLOSE DUMP DATA SET, FREE CORE   03194000
*                                      AND DEQ                       */ 03195000
*                   ELSE                                           1057 03196000
*                     IF RTM2DFTK='1'B THEN/* WAS FIRST TCB DUMPED ON*/ 03197000
         TM    RTM2DFTK(R11),B'00000001'                           1057 03198000
         BO    @RT01057                                            1057 03199000
*                       GOTO ADRTCB;/* PROCESS REMAINING TCBS FOR  1058 03200000
*                                      ABEND IN PROGRESS             */ 03201000
*                     ELSE                                         1059 03202000
*                       IF RTM2DOPN='1'B THEN/* DID OPEN COMPLETE?   */ 03203000
         TM    RTM2DOPN(R11),B'00100000'                           1059 03204000
         BO    @RT01059                                            1059 03205000
*                         GOTO ADDOPT;/* GO DETERMINE DUMP OPTS      */ 03206000
*                       ELSE                                       1061 03207000
*                         IF RTM2DGET='1'B THEN/* DID GETMAIN FOR DCB   03208000
*                                      COMPLETE?                     */ 03209000
         TM    RTM2DGET(R11),B'01000000'                           1061 03210000
         BO    @RT01061                                            1061 03211000
*                           GOTO ADOPEND;/* OPEN DUMP DATA SET       */ 03212000
*                         ELSE                                     1063 03213000
*                           IF RTM2DENQ='1'B THEN/* DID ENQ COMPLETE */ 03214000
         TM    RTM2DENQ(R11),B'10000000'                           1063 03215000
         BO    @RT01063                                            1063 03216000
*                             GOTO ADGET;/* GET CORE FOR DCB         */ 03217000
*                           ELSE                                   1065 03218000
*                             GOTO ADDCHK;/* SCAN FOR DD             */ 03219000
         B     ADDCHK                                              1065 03220000
*             END AD3;                                             1066 03221000
*         END AD2;                                                 1067 03222000
@RT01038 DS    0H                                                  1068 03223000
*     END AD1;                                                     1068 03224000
@RC01035 DS    0H                                                  1069 03225000
*   GEN(LM 14,12,12(13));           /* RESTORE RTM2 REGS             */ 03226000
@RC01029 DS    0H                                                  1069 03227000
         LM 14,12,12(13)                                                03228000
*   RETURN;                         /* RETURN TO RTM2                */ 03229000
@EL00006 DS    0H                                                  1070 03230000
@EF00006 DS    0H                                                  1070 03231000
@ER00006 BR    @14                                                 1070 03232000
*   END ADRECOV;                                                   1071 03233000
         EJECT                                                          03234000
*                                                                  1072 03235000
*   /*****************************************************************/ 03236000
*   /*                                                               */ 03237000
*   /* BASED ON OPTIONS SET IN THE RTM2DCTL FIELD OF THE RTM2WA THIS */ 03238000
*   /* ROUTINE DETERMINES WHETHER 1) THE DUMP DATA SET HAS BEEN      */ 03239000
*   /* CLOSED. 2) THE DCB CORE HAS BEEN FREED. 3) THE DUMP DATA SET  */ 03240000
*   /* HAS BEEN DEQUEUED FROM. ADRCLN PERFORMS THE CLEAN-UP REQUIRED */ 03241000
*   /* TO CONTINUE WITH TERMINATION.                                 */ 03242000
*   /*                                                               */ 03243000
*   /*****************************************************************/ 03244000
*                                                                  1072 03245000
*ADRCLN:                                                           1072 03246000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1072 03247000
ADRCLN   DS    0H                                                  1073 03248000
*   RFY                                                            1073 03249000
*    (R3) RSTD;                                                    1073 03250000
*   RFY                                                            1074 03251000
*     RTM2WA BASED(R11);                                           1074 03252000
*   RFY                                                            1075 03253000
*     TCB BASED(TCBPTR);                                           1075 03254000
*   RFY                                                            1076 03255000
*     R14 RSTD;                                                    1076 03256000
*   SAVERTN=R14;                    /* SAVE RETURN ADDR      @YM04727*/ 03257000
         ST    R14,SAVERTN                                         1077 03258000
*   RFY                                                            1078 03259000
*     R14 UNRSTD;                                                  1078 03260000
*   TCBPTR=R11->RTM2TCBC;           /* GET CURRENT TCB       @YM01200*/ 03261000
         MVC   TCBPTR(4),RTM2TCBC(R11)                             1079 03262000
*   DCBPTR=DCBSAVE;                 /* RESTORE DCB ADDR      @YM01200*/ 03263000
         MVC   DCBPTR(4),DCBSAVE                                   1080 03264000
*   IF RTM2EOPN='1'B THEN           /* WAS OPEN IN CONTROL           */ 03265000
         TM    RTM2EOPN(R11),B'00100000'                           1081 03266000
         BNO   @RF01081                                            1081 03267000
*     DO;                           /* YES, ISSUE MSG IEA030I        */ 03268000
*       RFY                                                        1083 03269000
*         R2 RSTD;                                                 1083 03270000
*       DO;                         /* GETMAIN (RC) LV(80) SP(253)   */ 03271000
*         RESPECIFY                                                1085 03272000
*          (GPR01F,                                                1085 03273000
*           GPR15F,                                                1085 03274000
*           GPR00F) RESTRICTED;                                    1085 03275000
*         GPR01F=0;                 /* REG 1 MUST BE ZERO            */ 03276000
         SLR   GPR01F,GPR01F                                       1086 03277000
*         GPR15F=0;                 /* RC-TYPE GETMAIN               */ 03278000
         SLR   GPR15F,GPR15F                                       1087 03279000
*         GPR00F=80;                /* LENGTH REQUESTED              */ 03280000
         LA    GPR00F,80                                           1088 03281000
*         GPR15F=GPR15F|((253)*256);/* SP IN BYTE 2                  */ 03282000
         O     GPR15F,@CF03743                                     1089 03283000
*         SVC(120);                 /* RC/RU-FORM OF GETMAIN         */ 03284000
         SVC   120                                                 1090 03285000
*         RESPECIFY                                                1091 03286000
*          (GPR01F,                                                1091 03287000
*           GPR15F,                                                1091 03288000
*           GPR00F) UNRESTRICTED;                                  1091 03289000
*       END;                        /* GETMAIN (RC) LV(80) SP(253)   */ 03290000
*       IF RETCODE=0 THEN           /* WAS GETMAIN SUCCESSFUL        */ 03291000
         LTR   RETCODE,RETCODE                                     1093 03292000
         BNZ   @RF01093                                            1093 03293000
*         DO;                       /* YES                           */ 03294000
*           R2=PARMPTR;             /* SAVE PTR TO BUFFER            */ 03295000
         LR    R2,PARMPTR                                          1095 03296000
*           BUFFER(1:65)=OPENMSG;   /* MOVE MSG TO BUFFER    @YM07271*/ 03297000
         MVC   BUFFER(65,PARMPTR),OPENMSG                          1096 03298000
*           IF TCBPTR^=TCBJSTCA THEN/* DETERMINE WHETHER JS OR     1097 03299000
*                                                             SUBTASK*/ 03300000
         L     @14,TCBPTR                                          1097 03301000
         SLR   @10,@10                                             1097 03302000
         ICM   @10,7,TCBJSTCA(@14)                                 1097 03303000
         CR    @14,@10                                             1097 03304000
         BE    @RF01097                                            1097 03305000
*             OPMSGST='ST';         /* SUBTASK ABEND                 */ 03306000
         MVC   OPMSGST+46(2,PARMPTR),@CC03643                      1098 03307000
*           R3=RTM2PTR->RTM2CC;     /* GET COMPLETION CODE   @YM07271*/ 03308000
@RF01097 SLR   R3,R3                                               1099 03309000
         ICM   R3,7,RTM2CC(RTM2PTR)                                1099 03310000
*           IF R3=0 THEN            /* WAS THERE A COMP CODE @YM07271*/ 03311000
         LTR   R3,R3                                               1100 03312000
         BNZ   @RF01100                                            1100 03313000
*             BUFFER(50:61)=' ';    /* NO,BLANK DUE TO PORTION OF MSG   03314000
*                                                            @YM07271*/ 03315000
         MVI   BUFFER+50(PARMPTR),C' '                             1101 03316000
         MVC   BUFFER+51(10,PARMPTR),BUFFER+50(PARMPTR)            1101 03317000
         MVI   BUFFER+49(PARMPTR),C' '                             1101 03318000
*           ELSE                                                   1102 03319000
*             DO;                   /* PROCESS COMPLETION          1102 03320000
*                                      CODE@YM07271                  */ 03321000
         B     @RC01100                                            1102 03322000
@RF01100 DS    0H                                                  1103 03323000
*               RFY                                                1103 03324000
*                 R0 RSTD;                                         1103 03325000
*               R0=R3;              /* SAVE CC OVER SHIFT    @YM07271*/ 03326000
         LR    R0,R3                                               1104 03327000
*               GEN(SRL R3,12);     /* ISOLATE SYSTEM CODE   @YM07271*/ 03328000
         SRL R3,12                                                      03329000
*               IF R3=0 THEN        /* IS THERE A SYSTEM CODE      1106 03330000
*                                                            @YM07271*/ 03331000
         LTR   R3,R3                                               1106 03332000
         BNZ   @RF01106                                            1106 03333000
*                 DO;               /* NO, PROCESS USER CODE @YM07271*/ 03334000
*                   R3=R0;          /* RESTORE USER CODE             */ 03335000
         LR    R3,R0                                               1108 03336000
*                   CVD(R3,COMPCODE);/* CONVERT USER CC TO DECIMAL 1109 03337000
*                                                            @YM07271*/ 03338000
         CVD   R3,COMPCODE                                         1109 03339000
*                   UNPK(COMPCODE(1:5),COMPCODE(6:8));/* UNPACK IT 1110 03340000
*                                                            @YM07271*/ 03341000
         UNPK  COMPCODE(5),COMPCODE+5(3)                           1110 03342000
*                   COMPCODE(5:5)=COMPCODE(5:5)|'F0'X;/* FIX UP SIGN    03343000
*                                      BYTE                  @YM07271*/ 03344000
         OI    COMPCODE+4,X'F0'                                    1111 03345000
*                   BUFFER(57:57)='U';/* INDICATE USER CODE  @YM07271*/ 03346000
         MVI   BUFFER+56(PARMPTR),C'U'                             1112 03347000
*                   BUFFER(58:61)=COMPCODE(2:5);/* MOVE COMP CODE TO    03348000
*                                      BUFFER                @YM07271*/ 03349000
         MVC   BUFFER+57(4,PARMPTR),COMPCODE+1                     1113 03350000
*                 END;                                             1114 03351000
*               ELSE                                               1115 03352000
*                 DO;               /* PROCESS SYS CODE      @YM07271*/ 03353000
         B     @RC01106                                            1115 03354000
@RF01106 DS    0H                                                  1116 03355000
*                   R3=R3*256;      /* SHIFT CC TO NEXT BYTE FOR   1116 03356000
*                                      UNPACK                @YM07271*/ 03357000
         SLL   R3,8                                                1116 03358000
*                   COMPCODE(5:8)=R3;/* STORE IT TO WORK ON IT     1117 03359000
*                                                            @YM07271*/ 03360000
         ST    R3,COMPCODE+4                                       1117 03361000
*                   UNPK(COMPCODE(1:5),COMPCODE(6:8));/* UNPACK SYS     03362000
*                                      CODE                  @YM07271*/ 03363000
         UNPK  COMPCODE(5),COMPCODE+5(3)                           1118 03364000
*                   TR(COMPCODE(1:4),TRANSTBL);/* TRANS TO PRINT   1119 03365000
*                                                            @YM07271*/ 03366000
         LA    @14,TRANMASK                                        1119 03367000
         AL    @14,@CF03784                                        1119 03368000
         TR    COMPCODE(4),TRANSTBL(@14)                           1119 03369000
*                   BUFFER(57:57)='S';/* INDICATE SYSTEM CODE      1120 03370000
*                                                            @YM07271*/ 03371000
         MVI   BUFFER+56(PARMPTR),C'S'                             1120 03372000
*                   BUFFER(59:61)=COMPCODE(2:4);/* MOVE COMP CODE TO    03373000
*                                      BUFFER                @YM07271*/ 03374000
         MVC   BUFFER+58(3,PARMPTR),COMPCODE+1                     1121 03375000
*                   BUFFER(58:58)=' ';/* BLANK GARBAGE BYTE  @YM07271*/ 03376000
         MVI   BUFFER+57(PARMPTR),C' '                             1122 03377000
*                 END;                                             1123 03378000
*               RFY                                                1124 03379000
*                 R0 UNRSTD;                                       1124 03380000
@RC01106 DS    0H                                                  1125 03381000
*             END;                                                 1125 03382000
*           GEN(WTO MF=(E,(PARMPTR)));/* ISSUE MSG                   */ 03383000
@RC01100 DS    0H                                                  1126 03384000
         WTO MF=(E,(PARMPTR))                                           03385000
*           DO;                     /* FREEMAIN(R)                 1127 03386000
*                                      LV(80)SP(253)A(R2->BUFFER)    */ 03387000
*             RESPECIFY                                            1128 03388000
*              (GPR01F,                                            1128 03389000
*               GPR00F) RESTRICTED;                                1128 03390000
*             GPR00F=80;            /* LENGTH REQUESTED              */ 03391000
         LA    GPR00F,80                                           1129 03392000
*             GPR01F=253;           /* SP IN REG 1                   */ 03393000
         LA    GPR01F,253                                          1130 03394000
*             GEN(SLL   GPR01F,24)                                 1131 03395000
*             NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;         1131 03396000
         SLL   GPR01F,24                                                03397000
*             GPR00F=GPR00F|GPR01F; /* SP IN HI BYTE                 */ 03398000
         OR    GPR00F,GPR01F                                       1132 03399000
