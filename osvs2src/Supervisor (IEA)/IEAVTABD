         BNZ   @RF00329                                            0329 00682000
*             DO;                   /* NO, ISSUE OPEN FAIL MESSAGE   */ 00683000
*                                                                  0331 00684000
*               /*****************************************************/ 00685000
*               /*                                                   */ 00686000
*               /* IF OPEN IS NOT SUCCESSFUL, MESSAGE 'IEA030I OPEN  */ 00687000
*               /* FAILED FOR DUMP DATA SET FOR JS IS ROUTED TO      */ 00688000
*               /* SYSOUT AND CONTROL RETURNS TO RTM2 WITH A RETURN  */ 00689000
*               /* CODE OF FOUR. STORAGE FOR THE MESSAGE BUFFER IS   */ 00690000
*               /* OBTIANED FROM SUBPOOL 253                         */ 00691000
*               /*                                                   */ 00692000
*               /*****************************************************/ 00693000
*                                                                  0331 00694000
*               RTM2TRRA=0;         /* INDICATE ABDUMP NOT TO BE   0331 00695000
*                                      ENTERED DURING RECURSION      */ 00696000
         SLR   @14,@14                                             0331 00697000
         ST    @14,RTM2TRRA(,RTM2PTR)                              0331 00698000
*               RFY                                                0332 00699000
*                (R0) RSTD;                                        0332 00700000
*               R0=80;              /* INDICATE LENGTH OF MESSAGE  0333 00701000
*                                      BUFFER                        */ 00702000
         LA    R0,80                                               0333 00703000
*               GENERATE REFS(R0);                                 0334 00704000
         GETMAIN RC,LV=(R0),SP=253   GET CORE FOR MESSAGE BUFFER        00705000
*               RFY                                                0335 00706000
*                 R0 UNRSTD;                                       0335 00707000
*               IF RETCODE=0 THEN                                  0336 00708000
         LTR   RETCODE,RETCODE                                     0336 00709000
