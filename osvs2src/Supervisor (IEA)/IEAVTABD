         LA    @10,88                                              0306 00625000
         ALR   @10,@14                                             0306 00626000
         ST    @14,OCDCBAD(,@10)                                   0306 00627000
*               OCDCBFL='8F'X;      /* INDICATE ONE ENTRY IN LIST AND   00628000
*                                      OPEN DATA SET FOR OUTPUT      */ 00629000
         MVI   OCDCBFL(@10),X'8F'                                  0307 00630000
*               R11=ADDR(OCDCBAD);  /* GET ADDRESS OF OPEN PARMLIST  */ 00631000
         LR    R11,@10                                             0308 00632000
*               RTM2EOPN='1'B;      /* INDICATE OPEN IN CONTROL      */ 00633000
         OI    RTM2EOPN(RTM2PTR),B'00100000'                       0309 00634000
*               DO;                 /* MODESET SAVEKEY(RTM2PKEY)   0310 00635000
*                                      EXTKEY(TCB) WORKREG(R9)       */ 00636000
*                 RESPECIFY                                        0311 00637000
*                  (GPR02F) RESTRICTED;                            0311 00638000
*                 R9=GPR02F;        /* SAVE REGISTER 2               */ 00639000
         LR    R9,GPR02F                                           0312 00640000
*                 IPK;              /* SAVE CURRENT PSW KEY          */ 00641000
         IPK                                                       0313 00642000
*                 RTM2PKEY=GPR02F;                                 0314 00643000
