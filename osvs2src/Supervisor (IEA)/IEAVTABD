*                 DO;                                              0337 00711000
*                   RESPECIFY                                      0338 00712000
*                    (R11) RESTRICTED;                             0338 00713000
*                   R11=PARMPTR;    /* SAVE ADDR OF BUFFER           */ 00714000
         LR    R11,PARMPTR                                         0339 00715000
*                   BUFFER(1:65)=OPENMSG;/* MOVE MESSAGE TO BUFFER 0340 00716000
*                                                            @YM07271*/ 00717000
         MVC   BUFFER(65,PARMPTR),OPENMSG                          0340 00718000
*                   JSTCBPTR=TCBJSTCA;/* PICK UP JS-TCB POINTER      */ 00719000
         L     @14,TCBPTR                                          0341 00720000
