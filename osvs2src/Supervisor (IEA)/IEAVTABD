*               RESPECIFY                                          0366 00794000
*                (R11) UNRESTRICTED;                               0366 00795000
*             END;                                                 0367 00796000
         EJECT                                                          00797000
*           ELSE                                                   0368 00798000
*             IF RTM2DD='SYSMDUMP' THEN/* SYSMDUMP REQUEST   @G33SPHW*/ 00799000
         B     @RC00329                                            0368 00800000
@RF00329 CLC   RTM2DD(8,RTM2PTR),@CC03623                          0368 00801000
         BNE   @RF00368                                            0368 00802000
*               DO;                 /* YES...                @G33SPHW*/ 00803000
*                                                                  0369 00804000
*                 /***************************************************/ 00805000
*                 /*                                                 */ 00806000
*                 /* FUNCTION OF THIS ROUTINE IS TO BUILD THE        */ 00807000
*                 /* PARMLIST FOR SDUMP AND OBTAIN THE WORKAREAS     */ 00808000
*                 /* REQUIRED BY SDUMP TO TAKE A DUMP. A BUFFER THE  */ 00809000
*                 /* LENGTH OF SDCNTRL PORTION OF SDWORKAREA IS      */ 00810000
*                 /* OBTAINED FROM LSQA AND A BUFFER THE SIZE OF     */ 00811000
*                 /* SDBUFFER PORTION OF SDWORKAREA + THE HEADER     */ 00812000
*                 /* LENGTH(MRMQHEAD) IS OBTAINED FROM CSA IN THE KEY*/ 00813000
*                 /* OF THE TCB. THE ADDRESS OF THE CSA AREA IS PUT  */ 00814000
*                 /* IN THE SDCNTRL SECTION OF SDWORK. THE ADDRESS OF*/ 00815000
*                 /* THE CSA AREA IS CHAINED TO A FIELD IN THE RTCT  */ 00816000
*                 /* (RTCTMRMQ) THE ADDRESS IS CHAINED OFF THE RTCT  */ 00817000
*                 /* IN CASE OF MEMTERM HAPPENING WHEN SYSMDUMP IS   */ 00818000
*                 /* RUNNING. IF MEMTERM OCCURS IEAVTMRM(RTM RESOURCE*/ 00819000
*                 /* MGR.) WILL USE THIS QUEUE TO FREE THE CSA       */ 00820000
*                 /* STORAGE USING THE HEADER INFORMATION FOR KEY    */ 00821000
*                 /* LENGTH AND ADDRESS. THIS QUEUE WILL KEEP ITS    */ 00822000
*                 /* INTEGRITY BY HAVING USERS OF QUEUE TO OBTAIN CMS*/ 00823000
*                 /* LOCK BEFORE REF. UNDER NORMAL CLEANUP IEAVTABD  */ 00824000
*                 /* WILL FREE THE CSA STORAGE AND REMOVE ENTRY FROM */ 00825000
*                 /* QUEUE. ANOTHER AREA OF STORAGE WILL BE OBTAINED */ 00826000
*                 /* FROM LSQA A 4096 BYTE SDUMP BUFFER. A DECB WILL */ 00827000
*                 /* BE OBTAINED FROM SP250 UNPROTECTED STORAGE AND  */ 00828000
*                 /* PASSED TO SDUMP TO BE USED WHEN WRITING OUT THE */ 00829000
*                 /* SYSMDUMP. ALL STORAGE WILL BE FREED BY IEAVTABD.*/ 00830000
*                 /*                                         @G33SPHW*/ 00831000
*                 /*                                                 */ 00832000
*                 /***************************************************/ 00833000
*                                                                  0370 00834000
*/********************************************************************/ 00835000
*/*                    SYSMDUMP AQQUIRED STORAGE                     */ 00836000
*/*                    _________________________                     */ 00837000
*/*     SDUMP        SDCNTRL      SDBUFF      DECB        WORK       */ 00838000
*/*     PARMLIST     (SDWORK)     (SDWORK)                AREA       */ 00839000
*/*     --------     --------     --------    --------    --------   */ 00840000
*/*     |FROM  |     |FROM  |     | FROM |    |FROM  |    |FROM  |   */ 00841000
*/*     |LSQA  |     |LSQA  |     | CSA  |    |SP250 |    | LSQA |   */ 00842000
*/*     | 24   |     |      |     | USER |    |32    |    | 4096 |   */ 00843000
*/*     | BYTES|     |      |     | KEY  |    |BYTES |    | BYTES|   */ 00844000
*/*     |      |     |      |     | 4104 |    |      |    |      |   */ 00845000
*/*     |SDCNTR|->   |      |     | BYTES|    | DCB  |->  |      |   */ 00846000
*/*     |WORKA |->   |      |     |      |    |SDBUFF|->  |      |   */ 00847000
*/*     --------     |      |     |      |    |      |    |      |   */ 00848000
*/*                  |      |     |      |    --------    |      |   */ 00849000
*/*                  |SDBUFF|->   |      |                |      |   */ 00850000
*/*                  | DECB |->   |      |                |      |   */ 00851000
*/*                  --------     --------                --------   */ 00852000
*/*                                                                  */ 00853000
*/********************************************************************/ 00854000
*                                                                  0370 00855000
*                 RFY                                              0370 00856000
*                  (R0) RSTD;                                      0370 00857000
*                 R0=4096;          /* SIZE OF BUFFER IN R0  @G33SPHW*/ 00858000
         LH    R0,@CH03647                                         0371 00859000
*                 RTM2EGET='1'B;    /* GETMAIN IN CONTROL    @G33SPHW*/ 00860000
         OI    RTM2EGET(RTM2PTR),B'01000000'                       0372 00861000
*                 GENERATE REFS(R0);                               0373 00862000
