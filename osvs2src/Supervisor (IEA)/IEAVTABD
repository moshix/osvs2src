*                   IF TCBPTR^=JSTCBPTR/* DETERMINE WHICH TCB      0342 00723000
*                                      REQUESTED DUMP: SUBTASK OR  0342 00724000
*                                      JOBSTEP                       */ 00725000
*                     THEN                                         0342 00726000
         CR    @14,JSTCBPTR                                        0342 00727000
         BE    @RF00342                                            0342 00728000
*                     OPMSGST='ST'; /* INDICATE SUBTASK IN MESSAGE   */ 00729000
         MVC   OPMSGST+46(2,PARMPTR),@CC03643                      0343 00730000
*                   BUFFER(50:61)=' ';/* CLEAR DUE TO PORTION OF BUF    00731000
*                                                            @YM07271*/ 00732000
@RF00342 MVI   BUFFER+50(PARMPTR),C' '                             0344 00733000
         MVC   BUFFER+51(10,PARMPTR),BUFFER+50(PARMPTR)            0344 00734000
         MVI   BUFFER+49(PARMPTR),C' '                             0344 00735000
*                   GENERATE;                                      0345 00736000
         WTO   MF=(E,(PARMPTR))    ROUTE MESSAGE TO PROGRAMMER          00737000
         FREEMAIN R,LV=80,SP=253,A=(11)  FREE CORE                      00738000
*                   PARMPTR=OCDCBADR;/* GET ADDRESS OF DCB & PARMLIST*/ 00739000
         L     @14,DCBPTR                                          0346 00740000
         SLR   PARMPTR,PARMPTR                                     0346 00741000
         ICM   PARMPTR,7,OCDCBADR+88(@14)                          0346 00742000
*                   RFY                                            0347 00743000
*                     R3 RSTD;                                     0347 00744000
*                   R3=TCBPKF;      /* GET TCB KEY           @YM01200*/ 00745000
         L     @14,TCBPTR                                          0348 00746000
         SLR   R3,R3                                               0348 00747000
         IC    R3,TCBPKF(,@14)                                     0348 00748000
*                   RTM2EFRM='1'B;  /* INDICATE FREEMAIN IN CONTRL   */ 00749000
         OI    RTM2EFRM(RTM2PTR),B'00000100'                       0349 00750000
*                   RFY                                            0350 00751000
*                     RTM2SWA RSTD; /*                       @YM01200*/ 00752000
*/*   RELEASE THE CORE USED FOR DCB, OPEN PARM LIST, QMNGRIO         */ 00753000
*/*   WORKAREA AND PARM LIST, AND JFCB/JFCBE READ IN AREA    @Z40MPXH*/ 00754000
*                                                                  0351 00755000
*                   RFY                                            0351 00756000
*                     R0 RSTD;                                     0351 00757000
*                   R0=LENGTH(COREAREA);/* GETMAINED AREA OF CORE  0352 00758000
*                                                            @Z40MPXH*/ 00759000
         LA    R0,503                                              0352 00760000
*                   RTM2SWA=RTM2PTR;/* SAVE WKAREA ADDR      @YM01200*/ 00761000
         LR    RTM2SWA,RTM2PTR                                     0353 00762000
*                   ASCBPTR=PSAAOLD;/* GET CURRENT ASCB PTR  @YM01200*/ 00763000
         L     ASCBPTR,PSAAOLD                                     0354 00764000
*                   RFY                                            0355 00765000
*                     R4 RSTD;      /*                       @G33SPHW*/ 00766000
*                   R4=TCBPTR;      /* PUT TCB ADDRESS IN R4 FOR   0356 00767000
*                                      FREEMAIN              @G33SPHW*/ 00768000
         LR    R4,@14                                              0356 00769000
*                   GENERATE;                                      0357 00770000
         STM R11,R14,RTM2RMSA(RTM2SWA)                         @YM01200 00771000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=('IEAVTABD')     00772000
         FREEMAIN RC,LV=(R0),A=(1),SP=230,BRANCH=YES,KEY=(3)   @YM01200 00773000
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=('IEAVTABD')       @YM01200 00774000
         LM R11,R14,RTM2RMSA(RTM2SWA)                          @YM01200 00775000
*                   RFY                                            0358 00776000
*                     R4 UNRSTD;    /*                       @G33SPHW*/ 00777000
*                   RFY                                            0359 00778000
*                    (R3,                                          0359 00779000
*                     R0) UNRSTD;                                  0359 00780000
