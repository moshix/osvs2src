         ST    @10,RTM2TRRA(,RTM2PTR)                              0983 02972000
*   RTM2WARG=0;                     /* ZERO WORKAREA ADDR    @Z40MPXH*/ 02973000
         MVI   RTM2WARG(RTM2PTR),X'00'                             0984 02974000
*   RTM2EQMN='0'B;                  /* TURN OFF QMIO IN CNTRL@Z40MPXH*/ 02975000
         NI    RTM2EQMN(RTM2PTR),B'11111110'                       0985 02976000
*   IF RTM2MCHK='1'B THEN           /* MACHINE CHECK OCCUR?  @Z40MPXH*/ 02977000
         TM    RTM2MCHK(RTM2PTR),B'10000000'                       0986 02978000
         BNO   @RF00986                                            0986 02979000
*     DO;                           /* YES,                  @Z40MPXH*/ 02980000
*       RESPECIFY                                                  0988 02981000
*         RTM2WA BASED(R11);        /* GET PREVIOUS WORK AREA POINTER   02982000
*                                                            @Z40MPXH*/ 02983000
*       CALL ADRCLN;                /* CLEAN UP ONLY         @Z40MPXH*/ 02984000
         BAL   @14,ADRCLN                                          0989 02985000
*       GEN(LM 14,12,12(13));       /* RESTORE REGS          @Z40MPXH*/ 02986000
         LM 14,12,12(13)                                                02987000
*       RETURN;                     /* RETURN TO RTM2        @Z40MPXH*/ 02988000
@EL00005 DS    0H                                                  0991 02989000
@EF00005 DS    0H                                                  0991 02990000
@ER00005 BR    @14                                                 0991 02991000
*     END;                          /*                       @Z40MPXH*/ 02992000
*   ELSE                            /* OTHER QMNGRIO FAILURE @Z40MPXH*/ 02993000
*     DO;                           /* PREPARE TO RETRY      @Z40MPXH*/ 02994000
@RF00986 DS    0H                                                  0994 02995000
*/* REFRESH ALL FIELDS FOR ABDUMP IN CURRENT WORKAREA        @Z40MPXH*/ 02996000
*                                                                  0994 02997000
*       RFY                                                        0994 02998000
*         R1 RSTD;                                                 0994 02999000
*       RTM2PTR->RTM2DTCB=R11->RTM2DTCB;/* TASK BEING DUMPED @Z40MPXH*/ 03000000
         MVC   RTM2DTCB(4,RTM2PTR),RTM2DTCB(R11)                   0995 03001000
*       RTM2PTR->RTM2SPRM=R11->RTM2SPRM;/* BASE OF SNAP PARM LIST  0996 03002000
*                                                            @Z40MPXH*/ 03003000
         MVC   RTM2SPRM(20,RTM2PTR),RTM2SPRM(R11)                  0996 03004000
