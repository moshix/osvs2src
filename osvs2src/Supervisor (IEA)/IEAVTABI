*                   PDATASW='1'B;   /* -YES, BUT NEXT INVALID.       */ 00677000
         OI    PDSDSW,B'00000101'                                  0309 00678000
*                 END;                                             0310 00679000
*               ELSE                /* -NO.                          */ 00680000
*                 ERRSW='1'B;       /* SET TO FLUSH MEMBER.          */ 00681000
