@ER00006 BR    @14                                                 0267 00540000
*   RFY                                                            0268 00541000
*     R14 UNRSTD;                                                  0268 00542000
*   END BLANK;                                                     0269 00543000
         B     @EL00006                                            0269 00544000
*RPARAN:                                                           0270 00545000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0270 00546000
*                                                                  0270 00547000
RPARAN   DS    0H                                                  0271 00548000
*   /*****************************************************************/ 00549000
*   /*                                                               */ 00550000
*   /* A RIGHT PARANTHESES WAS FOUND ON THE CARD. THE PARAN COUNT    */ 00551000
*   /* MUST REDUCE TO ZERO. THE LENGTH OF THE SUBPARAMETER MUST BE   */ 00552000
*   /* COMPUTED, AND SCANNED. VALID AND INVALID CHARACTERS ARE       */ 00553000
*   /* UPDATED IN THE TABLE.                                         */ 00554000
*   /*                                                               */ 00555000
*   /*****************************************************************/ 00556000
*                                                                  0271 00557000
*   RFY                                                            0271 00558000
*     R14 RSTD;                                                    0271 00559000
*   PARANCNT=PARANCNT-1;            /* REDUCE PARAN COUNT.           */ 00560000
         SLR   @15,@15                                             0272 00561000
         IC    @15,PARANCNT                                        0272 00562000
         BCTR  @15,0                                               0272 00563000
         STC   @15,PARANCNT                                        0272 00564000
*   IF PARANCNT^=0 THEN             /* PARANS UNBALANCED...          */ 00565000
         CLI   PARANCNT,0                                          0273 00566000
         BE    @RF00273                                            0273 00567000
*     ERRSW='1'B;                   /* -YES, ERROR (FLUSH).          */ 00568000
         OI    ERRSW,B'00010000'                                   0274 00569000
*   ELSE                                                           0275 00570000
*     DO;                           /* PARANS BALANCED.              */ 00571000
*                                                                  0275 00572000
         B     @RC00273                                            0275 00573000
@RF00273 DS    0H                                                  0276 00574000
*       /*************************************************************/ 00575000
*       /*                                                           */ 00576000
*       /* ALLOW THESE CHARACTERS...                                 */ 00577000
*       /*                                                           */ 00578000
*       /*************************************************************/ 00579000
*                                                                  0276 00580000
*       TRANTBL(','+1)='02'X;       /* ALLOW COMMA.                  */ 00581000
         MVI   TRANTBL+107,X'02'                                   0276 00582000
*       TRANTBL(' '+1)='03'X;       /* BLANK ALLOWED.                */ 00583000
*                                                                  0277 00584000
         MVI   TRANTBL+64,X'03'                                    0277 00585000
*       /*************************************************************/ 00586000
*       /*                                                           */ 00587000
*       /* DISALLOW THESE CHARACTERS.                                */ 00588000
*       /*                                                           */ 00589000
*       /*************************************************************/ 00590000
*                                                                  0278 00591000
*       TRANTBL(')'+1)='06'X;       /* RIGHT PARAN INVALID           */ 00592000
         MVI   TRANTBL+93,X'06'                                    0278 00593000
*       TRANTBL('='+1)='06'X;       /* EQUAL SIGN INVALID            */ 00594000
         MVI   TRANTBL+126,X'06'                                   0279 00595000
*       TRANTBL('('+1)='06'X;       /* LEFT PARAN INVALID            */ 00596000
         MVI   TRANTBL+77,X'06'                                    0280 00597000
*     END;                                                         0281 00598000
*   RETURN;                         /* BACK TO MAINLINE.             */ 00599000
@EL00007 DS    0H                                                  0282 00600000
@EF00007 DS    0H                                                  0282 00601000
@ER00007 BR    @14                                                 0282 00602000
*   RFY                                                            0283 00603000
*     R14 UNRSTD;                                                  0283 00604000
*   END RPARAN;                                                    0284 00605000
         B     @EL00007                                            0284 00606000
*EQUAL:                                                            0285 00607000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0285 00608000
*                                                                  0285 00609000
EQUAL    DS    0H                                                  0286 00610000
*   /*****************************************************************/ 00611000
*   /*                                                               */ 00612000
*   /* AN EQUAL SIGN WAS FOUND. CHECK IF PDATA/SDATA PRECEDED IT. IF */ 00613000
*   /* SO, BUMP PAST THE EQUAL SIGN TO CHECK FOR SUBPARAMETERS. SET  */ 00614000
*   /* THE TRANSLATE TABLE FOR VALID AND INVALID CHARACTERS FOLLOWING*/ 00615000
*   /* AN EQUAL SIGN. PARANTHESES COUNT MUST ALSO BE ZERO AT THIS    */ 00616000
*   /* POINT.                                                        */ 00617000
*   /*                                                               */ 00618000
*   /*****************************************************************/ 00619000
*                                                                  0286 00620000
*   RFY                                                            0286 00621000
*     R14 RSTD;                                                    0286 00622000
*   IF PARANCNT^=0|                 /* PARAN COUNT ZERO...           */ 00623000
*       STOP-START^=5 THEN          /* ...AND LENGTH CORRECT...      */ 00624000
         CLI   PARANCNT,0                                          0287 00625000
         BNE   @RT00287                                            0287 00626000
         L     @15,STOP                                            0287 00627000
         SLR   @15,START                                           0287 00628000
         C     @15,@CF00856                                        0287 00629000
         BE    @RF00287                                            0287 00630000
@RT00287 DS    0H                                                  0288 00631000
*     DO;                           /* -NO, ERROR.                   */ 00632000
*       DOSCAN='0'B;                /* NO SCAN REQUIRED.             */ 00633000
*       ERRSW='1'B;                 /* SET TO FLUSH MEMBER.          */ 00634000
         OI    ERRSW,B'00010000'                                   0290 00635000
         NI    DOSCAN,B'11011111'                                  0290 00636000
*     END;                                                         0291 00637000
*   ELSE                            /* CHECK KEYWORD TYPE.           */ 00638000
*     DO;                                                          0292 00639000
         B     @RC00287                                            0292 00640000
@RF00287 DS    0H                                                  0293 00641000
*       R8=STOP-5;                  /* POINT TO START OF SDATA OR  0293 00642000
*                                      PDATA KEYWORD.                */ 00643000
         L     R8,STOP                                             0293 00644000
         SL    R8,@CF00856                                         0293 00645000
*       IF R8->PARM(1:5)='SDATA' THEN/* IS IT SDATA...               */ 00646000
         CLC   PARM(5,R8),@CC02252                                 0294 00647000
         BNE   @RF00294                                            0294 00648000
*         DO;                       /* -YES.                         */ 00649000
*           IF SDATASW='0'B THEN    /* FIRST TIME FOR SDATA...       */ 00650000
         TM    SDATASW,B'00000010'                                 0296 00651000
         BNZ   @RF00296                                            0296 00652000
*             DO;                   /* -YES.                         */ 00653000
*               PDSDSW='0'B;        /* SHOW SDATA SCAN.              */ 00654000
*               SDATASW='1'B;       /* -YES, BUT NEXT INVALID.       */ 00655000
         OI    SDATASW,B'00000010'                                 0299 00656000
         NI    PDSDSW,B'11111011'                                  0299 00657000
*             END;                                                 0300 00658000
*           ELSE                    /* -NO.                          */ 00659000
*             ERRSW='1'B;           /* SET TO FLUSH MEMBER.          */ 00660000
         B     @RC00296                                            0301 00661000
@RF00296 OI    ERRSW,B'00010000'                                   0301 00662000
*         END;                                                     0302 00663000
*       ELSE                        /* NOT SDATA.                    */ 00664000
*         DO;                                                      0303 00665000
         B     @RC00294                                            0303 00666000
@RF00294 DS    0H                                                  0304 00667000
*           IF R8->PARM(1:5)='PDATA' THEN/* IS IT PDATA...           */ 00668000
