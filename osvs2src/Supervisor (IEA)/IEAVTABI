*   NIPPARC=12;                     /* SET RETURN CODE TO 12   Y02752*/ 02489000
         L     @10,NVTPAREA(,RNVT)                                 0822 02490000
         MVC   NIPPARC(4,@10),@CF00033                             0822 02491000
*   IF NIPPMNAM=NIPPANAM            /* SEE IF MEMBER NAMES MATCH   0823 02492000
*                                                              Y02752*/ 02493000
*     THEN                                                         0823 02494000
         CLC   NIPPMNAM(8),NIPPANAM(@10)                           0823 02495000
         BE    @RT00823                                            0823 02496000
*     GO TO NIPPMPXT;               /* YES-TAKE NEW VERSION EXIT   0824 02497000
*                                                              Y02752*/ 02498000
*   GENERATE REFS(NIPPMREG) SETS(NIPPMR01,NIPPMR02);               0825 02499000
