*   DCL                                                            0686 01876000
*     NIPPMR09 REG(9) PTR(24);                                     0686 01877000
*   DCL                                                            0687 01878000
*     NIPPMR12 REG(12) PTR(24);                                    0687 01879000
*   DCL                                                            0688 01880000
*     NIPPMR13 REG(13) PTR(24);                                    0688 01881000
*   DCL                                                            0689 01882000
*     NIPPMR14 REG(14) PTR(24);                                    0689 01883000
*   DCL                                                            0690 01884000
*     NIPPMR15 REG(15) PTR(24);                                    0690 01885000
*                                                                  0690 01886000
*   /*****************************************************************/ 01887000
*   /*                                                               */ 01888000
*   /* SAVE AREA FOR CALLER'S REGISTERS                              */ 01889000
*   /*                                                               */ 01890000
*   /*****************************************************************/ 01891000
*                                                                  0691 01892000
*   DCL                                                            0691 01893000
*     NIPPMREG(16) FIXED(31);                                      0691 01894000
*                                                                  0691 01895000
*   /*****************************************************************/ 01896000
*   /*                                                               */ 01897000
*   /* SAVE AREA FOR CALLER'S RETURN ADDRESS                         */ 01898000
*   /*                                                               */ 01899000
*   /*****************************************************************/ 01900000
*                                                                  0692 01901000
*   DCL                                                            0692 01902000
*     NIPPMADR PTR(31);                                            0692 01903000
*                                                                  0692 01904000
*   /*****************************************************************/ 01905000
*   /*                                                               */ 01906000
*   /* POINTER TO DCB FOR BLDL                                       */ 01907000
*   /*                                                               */ 01908000
*   /*****************************************************************/ 01909000
*                                                                  0693 01910000
*   DCL                                                            0693 01911000
*     NIPPMDCB PTR(31);                                            0693 01912000
*                                                                  0693 01913000
*   /*****************************************************************/ 01914000
*   /*                                                               */ 01915000
*   /* POINTER TO BLDL HEADER FOR BLDL                               */ 01916000
*   /*                                                               */ 01917000
*   /*****************************************************************/ 01918000
*                                                                  0694 01919000
*   DCL                                                            0694 01920000
*     NIPPMBDL PTR(31);                                            0694 01921000
*                                                                  0694 01922000
*   /*****************************************************************/ 01923000
*   /*                                                               */ 01924000
*   /* NORMAL RETURN TO CALLER                                       */ 01925000
*   /*                                                               */ 01926000
*   /*****************************************************************/ 01927000
*                                                                  0695 01928000
*   DCL                                                            0695 01929000
*     NIPPMAD0 LABEL BASED(NIPPMADR);                              0695 01930000
*                                                                  0695 01931000
*   /*****************************************************************/ 01932000
*   /*                                                               */ 01933000
*   /* ERROR RETURN TO CALLER                                        */ 01934000
*   /*                                                               */ 01935000
*   /*****************************************************************/ 01936000
*                                                                  0696 01937000
*   DCL                                                            0696 01938000
*     NIPPMAD4 LABEL BASED(NIPPMADR+4);                            0696 01939000
*                                                                  0696 01940000
*   /*****************************************************************/ 01941000
*   /*                                                               */ 01942000
*   /* ESTABLISH BASE FOR NIP COMMUNICATION AREA               Y02752*/ 01943000
*   /*                                                               */ 01944000
*   /*****************************************************************/ 01945000
*                                                                  0697 01946000
*   RESPECIFY                                                      0697 01947000
*     NIPPAREA BASED(NVTPAREA);     /*                         Y02752*/ 01948000
*                                                                  0697 01949000
*   /*****************************************************************/ 01950000
*   /*                                                               */ 01951000
*   /* ONE BYTE SCANNING FIELD                                       */ 01952000
*   /*                                                               */ 01953000
*   /*****************************************************************/ 01954000
*                                                                  0698 01955000
*   DCL                                                            0698 01956000
*     NIPPMBYT CHAR(1) BASED;                                      0698 01957000
*                                                                  0698 01958000
*   /*****************************************************************/ 01959000
*   /*                                                               */ 01960000
*   /* TEMPORARY POINTER                                       Y02752*/ 01961000
*   /*                                                               */ 01962000
*   /*****************************************************************/ 01963000
*                                                                  0699 01964000
*   DCL                                                            0699 01965000
*     NIPPMKPR PTR(31);             /*                         Y02752*/ 01966000
*                                                                  0699 01967000
*   /*****************************************************************/ 01968000
*   /*                                                               */ 01969000
*   /* INPUT RECORD READ FROM PARMLIB                          Y02752*/ 01970000
*   /*                                                               */ 01971000
*   /*****************************************************************/ 01972000
*                                                                  0700 01973000
*   DCL                                                            0700 01974000
*     NIPPMBUF CHAR(80) BASED(NIPPMR01);/*                     Y02752*/ 01975000
*                                                                  0700 01976000
*   /*****************************************************************/ 01977000
*   /*                                                               */ 01978000
*   /* MBBCCHHR OF PARMLIB MEMBERS                                   */ 01979000
*   /*                                                               */ 01980000
*   /*****************************************************************/ 01981000
*                                                                  0701 01982000
*   DCL                                                            0701 01983000
*     1 NIPPMMBC CHAR(8),                                          0701 01984000
*      2 NIPPMMBB CHAR(3),                                         0701 01985000
*      2 NIPPMCCH CHAR(5);                                         0701 01986000
*                                                                  0701 01987000
*   /*****************************************************************/ 01988000
*   /*                                                               */ 01989000
*   /* CCHHR OF PARMLIB MEMBERS                              @ZA07472*/ 01990000
*   /*                                                               */ 01991000
*   /*****************************************************************/ 01992000
*                                                                  0702 01993000
*   DCL                                                            0702 01994000
*     1 NIPPMSID BDY(WORD) CHAR(5) BASED(ADDR(NIPPASID)),/*  @ZA07472*/ 01995000
*      2 NIPPMCC FIXED(15),         /* CYLINDER # OF SEARCH ID     0702 01996000
*                                                            @ZA07472*/ 01997000
*      2 NIPPMHH CHAR(2),           /* HEAD # OF SEARCH ID   @ZA07472*/ 01998000
*      2 NIPPMR CHAR(1);            /* RECORD # OF SEARCH ID @ZA07472*/ 01999000
*                                                                  0702 02000000
*   /*****************************************************************/ 02001000
*   /*                                                               */ 02002000
*   /* LOW ORDER 7 BYTES OF THE CSW STORED IN THE PARMLIB IOB BY EXCP*/ 02003000
*   /*                                                         Y02752*/ 02004000
*   /*                                                               */ 02005000
*   /*****************************************************************/ 02006000
*                                                                  0703 02007000
