         CLC   CNT(4),@CF00090                                     0638 01587000
         BNE   @RF00638                                            0638 01588000
*     DO;                           /* YES...                @G33SPHW*/ 01589000
*       R15=START;                  /* SET BASE FOR PARM FIELD     0640 01590000
*                                                            @G33SPHW*/ 01591000
         LR    R15,START                                           0640 01592000
*       ERRSW='1'B;                 /* PRESUPPOSE ERROR.     @G33SPHW*/ 01593000
         OI    ERRSW,B'00010000'                                   0641 01594000
*       IF R5=8 THEN                /* SUBPARM 8 BYTES       @G33SPHW*/ 01595000
         C     R5,@CF00038                                         0642 01596000
         BNE   @RF00642                                            0642 01597000
*         DO;                       /* YES...                @G33SPHW*/ 01598000
*           IF PARM(1:8)='ALLSDATA'&/* ALLSDATA OPTIONS      @G33SPHW*/ 01599000
*               RTCTSDA='00'X THEN  /* AND NO OTHERS SET     @G33SPHW*/ 01600000
         CLC   PARM(8,R15),@CC02295                                0644 01601000
         BNE   @RF00644                                            0644 01602000
         CLI   RTCTSDA(R6),X'00'                                   0644 01603000
         BNE   @RF00644                                            0644 01604000
*             DO;                   /* YES...                @G33SPHW*/ 01605000
*               RTCTSDA='FF'X;      /* SET ALL OPTIONS ON    @G33SPHW*/ 01606000
         MVI   RTCTSDA(R6),X'FF'                                   0646 01607000
*               ERRSW='0'B;         /* TURN ERROR SWITCH OFF @G33SPHW*/ 01608000
         NI    ERRSW,B'11101111'                                   0647 01609000
*             END;                  /* END... ALLSDATA       @G33SPHW*/ 01610000
*         END;                      /* END... LNTH = 8       @G33SPHW*/ 01611000
@RF00644 DS    0H                                                  0650 01612000
*       IF R5=4 THEN                /* SUBPARM 4 BYTES       @G33SPHW*/ 01613000
@RF00642 C     R5,@CF00035                                         0650 01614000
         BNE   @RF00650                                            0650 01615000
*         DO;                       /* YES...                @G33SPHW*/ 01616000
*           IF PARM(1:4)='LSQA'&    /* LSQA OPTION           @G33SPHW*/ 01617000
*               RTCTLSQA='0'B THEN  /* AND NOT ALREADY SET   @G33SPHW*/ 01618000
         CLC   PARM(4,R15),@CC02297                                0652 01619000
         BNE   @RF00652                                            0652 01620000
         TM    RTCTLSQA(R6),B'00100000'                            0652 01621000
         BNZ   @RF00652                                            0652 01622000
*             DO;                   /* YES...                @G33SPHW*/ 01623000
*               RTCTLSQA='1'B;      /* SET LSQA OPTION ON    @G33SPHW*/ 01624000
         OI    RTCTLSQA(R6),B'00100000'                            0654 01625000
*               ERRSW='0'B;         /* TURN ERROR SWITCH OFF @G33SPHW*/ 01626000
         NI    ERRSW,B'11101111'                                   0655 01627000
*             END;                  /* END... LSQA           @G33SPHW*/ 01628000
*         END;                      /* END LNTH 4            @G33SPHW*/ 01629000
@RF00652 DS    0H                                                  0658 01630000
*       IF R5=3 THEN                /* SUBPARM 3 BYTES       @G33SPHW*/ 01631000
@RF00650 C     R5,@CF00090                                         0658 01632000
         BNE   @RF00658                                            0658 01633000
*         DO;                       /* FIND OPTION MATCH     @G33SPHW*/ 01634000
*           ERRSW='0'B;             /* SHOW NO ERROR         @G33SPHW*/ 01635000
         NI    ERRSW,B'11101111'                                   0660 01636000
*           IF PARM(1:3)='NUC'&     /* VALID NUC SPELLING... @G33SPHW*/ 01637000
*               RTCTNUC='0'B THEN   /* ... AND 1ST TIME      @G33SPHW*/ 01638000
         CLC   PARM(3,R15),@CC02301                                0661 01639000
         BNE   @RF00661                                            0661 01640000
         TM    RTCTNUC(R6),B'10000000'                             0661 01641000
         BNZ   @RF00661                                            0661 01642000
*             RTCTNUC='1'B;         /* YES... ALLOW OPTION   @G33SPHW*/ 01643000
         OI    RTCTNUC(R6),B'10000000'                             0662 01644000
*           ELSE                    /* TES FOR SQA           @G33SPHW*/ 01645000
*             IF PARM(1:3)='SQA'&   /* VALID SQA SPELLING... @G33SPHW*/ 01646000
*                 RTCTSQA='0'B THEN /* ... AND 1ST TIME      @G33SPHW*/ 01647000
         B     @RC00661                                            0663 01648000
@RF00661 CLC   PARM(3,R15),@CC02302                                0663 01649000
         BNE   @RF00663                                            0663 01650000
         TM    RTCTSQA(R6),B'01000000'                             0663 01651000
         BNZ   @RF00663                                            0663 01652000
*               RTCTSQA='1'B;       /* YES... ALLOW OPTION   @G33SPHW*/ 01653000
         OI    RTCTSQA(R6),B'01000000'                             0664 01654000
*             ELSE                  /* TEST FOR SWA          @G33SPHW*/ 01655000
*               IF PARM(1:3)='SWA'& /* VALID SWA SPELLING... @G33SPHW*/ 01656000
*                   RTCTSWA='0'B THEN/* ... AND 1ST TIME     @G33SPHW*/ 01657000
         B     @RC00663                                            0665 01658000
@RF00663 CLC   PARM(3,R15),@CC02305                                0665 01659000
         BNE   @RF00665                                            0665 01660000
         TM    RTCTSWA(R6),B'00010000'                             0665 01661000
         BNZ   @RF00665                                            0665 01662000
*                 RTCTSWA='1'B;     /* YES... ALLOW OPTION   @G33SPHW*/ 01663000
         OI    RTCTSWA(R6),B'00010000'                             0666 01664000
*               ELSE                /* TEST FOR TRT          @G33SPHW*/ 01665000
*                 IF PARM(1:3)='TRT'&/* VALID TRT SPELLING...      0667 01666000
*                                                            @G33SPHW*/ 01667000
*                     RTCTTRT='0'B THEN/* ... AND 1ST TIME   @G33SPHW*/ 01668000
         B     @RC00665                                            0667 01669000
@RF00665 CLC   PARM(3,R15),@CC02303                                0667 01670000
         BNE   @RF00667                                            0667 01671000
         TM    RTCTTRT(R6),B'00001000'                             0667 01672000
         BNZ   @RF00667                                            0667 01673000
*                   RTCTTRT='1'B;   /* YES... ALLOW OPTION   @G33SPHW*/ 01674000
         OI    RTCTTRT(R6),B'00001000'                             0668 01675000
*                 ELSE              /* TEST FOR RGN          @G33SPHW*/ 01676000
*                   IF PARM(1:3)='RGN'&/* VALID RGN SPELLING...    0669 01677000
*                                                            @G33SPHW*/ 01678000
*                       RTCTRGN='0'B THEN/* ... AND 1ST TIME @G33SPHW*/ 01679000
         B     @RC00667                                            0669 01680000
@RF00667 CLC   PARM(3,R15),@CC02307                                0669 01681000
         BNE   @RF00669                                            0669 01682000
         TM    RTCTRGN(R6),B'00000100'                             0669 01683000
         BNZ   @RF00669                                            0669 01684000
*                     RTCTRGN='1'B; /* YES... ALLOW OPTION   @G33SPHW*/ 01685000
         OI    RTCTRGN(R6),B'00000100'                             0670 01686000
*                   ELSE            /* TEST FOR LPA          @G33SPHW*/ 01687000
*                     IF PARM(1:3)='LPA'&/* VALID LPA SPELLING...  0671 01688000
*                                                            @G33SPHW*/ 01689000
*                         RTCTLPA='0'B THEN/* AND 1ST TIME   @G33SPHW*/ 01690000
         B     @RC00669                                            0671 01691000
@RF00669 CLC   PARM(3,R15),@CC02289                                0671 01692000
         BNE   @RF00671                                            0671 01693000
         TM    RTCTLPA(R6),B'00000010'                             0671 01694000
         BNZ   @RF00671                                            0671 01695000
*                       RTCTLPA='1'B;/* ... YES ALLOW OPTION @G33SPHW*/ 01696000
         OI    RTCTLPA(R6),B'00000010'                             0672 01697000
*                     ELSE          /* TEST FOR CSA          @G33SPHW*/ 01698000
*                       IF PARM(1:3)='CSA'&/* VALID CSA SPELLING   0673 01699000
*                                                            @G33SPHW*/ 01700000
*                           RTCTCSA='0'B THEN/* ... AND 1ST TIME   0673 01701000
*                                                            @G33SPHW*/ 01702000
         B     @RC00671                                            0673 01703000
@RF00671 CLC   PARM(3,R15),@CC02308                                0673 01704000
         BNE   @RF00673                                            0673 01705000
         TM    RTCTCSA(R6),B'00000001'                             0673 01706000
         BNZ   @RF00673                                            0673 01707000
*                         RTCTCSA='1'B;/* ... YES ALLOW OPTION     0674 01708000
*                                                            @G33SPHW*/ 01709000
         OI    RTCTCSA(R6),B'00000001'                             0674 01710000
*                       ELSE        /* NO MATCH              @G33SPHW*/ 01711000
*                         ERRSW='1'B;/* INDICATE ERROR       @G33SPHW*/ 01712000
         B     @RC00673                                            0675 01713000
@RF00673 OI    ERRSW,B'00010000'                                   0675 01714000
*         END;                      /* END--- LNTH 3         @G33SPHW*/ 01715000
*     END;                          /* END -- SYSMDUMP       @G33SPHW*/ 01716000
*   RETURN;                                                        0678 01717000
@EL00011 DS    0H                                                  0678 01718000
@EF00011 DS    0H                                                  0678 01719000
@ER00011 BR    @14                                                 0678 01720000
*   RFY                                                            0679 01721000
*    (R5,                                                          0679 01722000
*     R14,                                                         0679 01723000
*     R15) UNRSTD;                                                 0679 01724000
*   END SDATPARM;                                                  0680 01725000
         B     @EL00011                                            0680 01726000
*                                                                  0681 01727000
*/* DEFINE NIP PARMLIB READ ROUTINE AS SUBROUTINE                    */ 01728000
*                                                                  0681 01729000
*GETCARD:                                                          0681 01730000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0681 01731000
*                                                                  0681 01732000
GETCARD  DS    0H                                                  0682 01733000
*/********************************************************************/ 01734000
*/*                                                                  */ 01735000
*/* MACRO-      IEAPSPIO                                             */ 01736000
*/*                                                                  */ 01737000
*/* STATUS-     VERSION DATE=10/24/73, VS/2 RELEASE 2                */ 01738000
*/*                                                                  */ 01739000
*/* FUNCTION-   IEAPSPIO IS PART OF THE  SUPERVISOR                  */ 01740000
*/*             INITIALIZATION PROCESS FOR VS2.                      */ 01741000
*/*                                                                  */ 01742000
*/*             THIS MACRO GENERATES INLINE PL/S STATEMENTS          */ 01743000
*/*             WHICH FORM A SUBROUTINE TO FIND AND READ TEXT        */ 01744000
*/*             RECORDS FROM SYS1.PARMLIB.                           */ 01745000
*/*                                                                  */ 01746000
*/*  REGISTER DEPENDENCIES-                                          */ 01747000
*/*                                                                  */ 01748000
*/*                 1. IEAPSPIO DEPENDS ON THE INCLUDING ROUTINES    */ 01749000
*/*                    BASE REGISTERS FOR ADDRESSABILITY             */ 01750000
*/*                                                                  */ 01751000
*/*                 2. REGISTER 3 MUST CONTAIN THE ADDRESS OF        */ 01752000
*/*                    THE CVT ON ENTRY                              */ 01753000
*/*                                                                  */ 01754000
*/*                 3. REGISTER 2 MUST CONTAIN THE ADDRESS OF        */ 01755000
*/*                    THE NVT ON ENTRY                              */ 01756000
*/*                                                                  */ 01757000
*/*                 4. REGISTER 14 MUST CONTAIN THE ADDRESS OF       */ 01758000
*/*                    THE CALLERS RETURN POINT ON ENTRY             */ 01759000
*/*                                                                  */ 01760000
*/* ENTRY POINTS-                                                    */ 01761000
*/*                                                                  */ 01762000
*/*    TWO CALLING SEQUENCRS ARE AVAILABLE.  ONE PROVIDES            */ 01763000
*/*    COMPATIBILITY WITH THE VS2-1 VERSION OF THIS MACRO,           */ 01764000
*/*    THE OTHER CALLING SEQUENCE WAS DEVELOPED EXPRESSLY            */ 01765000
*/*    FOR VS2-2.                                                    */ 01766000
*/*                                                                  */ 01767000
*/*        VS2-1 COMPATIBLE VERSION-                                 */ 01768000
*/*                                                                  */ 01769000
*/*            1. CALL NIPPMPDS TO LOCATE (VIA BLDL) THE MEMBER      */ 01770000
*/*               OF SYS1.PARMLIB                                    */ 01771000
*/*                                                                  */ 01772000
*/*            2. CALL NIPPMTXT TO READ A TEXT RECORD FROM           */ 01773000
*/*               SYS1.PARMLIB                                       */ 01774000
*/*                                                                  */ 01775000
*/*        VS2-2 VERSION-                                            */ 01776000
*/*                                                                  */ 01777000
*/*            1. CALL NIPPMMEM TO LOCATE THE MEMBER AND RETURN      */ 01778000
*/*               A TEXT RECORD                                      */ 01779000
*/*                                                                  */ 01780000
*/*            2. REPEAT CALL TO NIPPMMEM TO READ ADDITIONAL         */ 01781000
*/*               TEXT RECORDS                                       */ 01782000
*/*                                                                  */ 01783000
*/* OUTPUT (NORMAL) -                                                */ 01784000
*/*                                                                  */ 01785000
*/*     THE ROUTINE WILL SAVE AND RESTORE ALL GENERAL REGISTERS      */ 01786000
*/*     WITH THE EXCEPTION OF RPARM.  ON A SUCCESSFUL READ,          */ 01787000
*/*     RPARM (REGISTER 1) WILL BE SET TO POINT TO THE FIRST NON-    */ 01788000
*/*     BLANK CHARACTER IN THE TEXT RECORD.  IF THE RECORD           */ 01789000
*/*     COLUMNS 1 THRU 72 ALL CONTAIN BLANKS, RPARM WILL             */ 01790000
*/*     POINT TO A BLANK IN COLUMN 72.  IF COLUMNS 1 THRU 71         */ 01791000
*/*     ARE BLANK BUT COLUMN 72 IS NON-BLANK, THE ROUTINE            */ 01792000
*/*     WILL AUTOMATICALLY ATTEMPT TO READ THE NEXT RECORD.  IN      */ 01793000
*/*     ALL CASES, COLUMN 72 WILL BE SET TO BLANK AND COLUMN 73 WILL */ 01794000
*/*     BE SET TO X'FF'.                                             */ 01795000
*/*                                                                  */ 01796000
*/* EXIT (NORMAL)-                                                   */ 01797000
*/*                                                                  */ 01798000
*/*     VS2-1 AND VS2-2 VERSIONS RETURN TO THE ADDRESS FOUND         */ 01799000
*/*     IN REGISTER 14 ON ENTRY.                                     */ 01800000
*/*                                                                  */ 01801000
*/* EXIT (ERROR)-                                                    */ 01802000
*/*                                                                  */ 01803000
*/*     VS2-1 COMPATIBLE VERSION-                                    */ 01804000
*/*                                                                  */ 01805000
*/*         RETURN TO THE ADDRESS IN REGISTER 14 +4                  */ 01806000
*/*                                                                  */ 01807000
*/*         NOTE- WHEN AN ERROR OCCURS ON AN ATTEMP TO READ A RECORD */ 01808000
*/*               THAT DOES NOT EXIST (E-O-F),                       */ 01809000
*/*               IT IS CONSIDERED A PERMANENT I/O ERROR             */ 01810000
*/*                                                                  */ 01811000
*/*     VS2-2 VERSION-                                               */ 01812000
*/*                                                                  */ 01813000
*/*        RETURN TO THE ADDRESS FOUND IN REGISTER 14 ON ENTRY       */ 01814000
*/*        WITH REGISTER 1 (RPARM) SET TO A NEGATIVE VALUE.  IN THE  */ 01815000
*/*        CASE WHERE RPARM IS NEGATIVE ON RETURN, THE NIPPARC FIELD */ 01816000
*/*        OF THE NIP PATAMETER AREA WILL INDICATE THE REASON FOR    */ 01817000
*/*        FAILURE AS FOLLOWS-                                       */ 01818000
*/*                            4 - MEMBER NOT FOUND                  */ 01819000
*/*                            8 - I/O ERROR DURING BLDL             */ 01820000
*/*                            12- I/O ERROR DURING READ             */ 01821000
*/*                            16- END-OF-FILE                       */ 01822000
*/*                                                                  */ 01823000
*/*     COMMON TO BOTH VERSIONS-                                     */ 01824000
*/*                                                                  */ 01825000
*/*        AN INTERPRETIVE MESSAGE WILL BE WRITTEN TO THE OPERATOR   */ 01826000
*/*        FOR EACH OF THE FIRST THREE ERROR CONDITIONS LISTED       */ 01827000
*/*        ABOVE.  THE CALLER MAY SUPPRESS THE 'MEMBER NOT FOUND'    */ 01828000
*/*        MESSAGE BY SETTING THE NIPPAFL1 FLAG FIELD TO BINARY      */ 01829000
*/*        ONE IN THE NIP PARAMETER AREA BEFORE CALLING THIS         */ 01830000
*/*        ROUTINE.  NO MESSAGE WILL BE WRITTEN FOR THE E-O-F        */ 01831000
*/*        CONDITION IF THE VS2-2 VERSION IS USED.                   */ 01832000
*/*                                                                  */ 01833000
*/* MACROS-                                                          */ 01834000
*/*     THE FOLLOWING MAPPING MACROS MUST BE                         */ 01835000
*/*     INCLUDED IN THE MODULE WHICH INCLUDES                        */ 01836000
*/*     IEAPSPIO-                                                    */ 01837000
*/*                                       IEZIOB                     */ 01838000
*/*                                       IHANVT                     */ 01839000
*/*                                       IHAECB                     */ 01840000
*/*                                       IEAPPNIP                   */ 01841000
*/*                                       IEFUCBOB                   */ 01842000
*/*                                       CVT                        */ 01843000
*/*                                                                  */ 01844000
*/* NOTES-                                                           */ 01845000
*/*        THE FIRST FOUR LETTERS OF ALL IEAPSPIO SYMBOLES ARE:      */ 01846000
*/*        NIPP OR IPPM.                                             */ 01847000
*/*                                                                  */ 01848000
*/*        REGISTERS 0, 1, 14, AND 15 WILL BE UNRESTRICTED AT THE    */ 01849000
*/*        END OF THE SEGMENT GENERATED BY IEAPSPIO.                 */ 01850000
*/*                                                                  */ 01851000
*/*        NIPPAREA WILL BE BASED ON NVTPAREA AND IOBSTDRD WILL BE   */ 01852000
*/*        BASED ON THE ADDR OF NIPPAIOB AT THE END OF THE SEGMENT   */ 01853000
*/*        GENERATED BY IEAPSPIO                                     */ 01854000
*/*                                                                  */ 01855000
*/*        IEAPSPIO MUST BE INCLUDED IN A CODE SEGMENT,              */ 01856000
*/*        NOT AT THE TOP OF THE PROCEDURE WITH THE                  */ 01857000
*/*        MAPPING MACROS LISTED ABOVE.                              */ 01858000
*/*                                                            Y02752*/ 01859000
*/********************************************************************/ 01860000
*                                                                  0682 01861000
*   /*****************************************************************/ 01862000
*   /*                                                               */ 01863000
*   /* REGISTERS                                                     */ 01864000
*   /*                                                               */ 01865000
*   /*****************************************************************/ 01866000
*                                                                  0682 01867000
*   DCL                                                            0682 01868000
*     NIPPMR00 REG(0) PTR(24);                                     0682 01869000
*   DCL                                                            0683 01870000
*     NIPPMR01 REG(1) PTR(24);                                     0683 01871000
