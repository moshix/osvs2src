         TITLE 'IEAVTABI - ABDUMP INITIALIZATION                       *00001000
                        '                                               00002000
IEAVTABI CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVTABI  77.006'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @11,0                                               0001 00011000
@PSTART  LA    @12,4095(,@11)                                      0001 00012000
         USING @PSTART,@11                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
*                                                                       00015000
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF          00016000
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES      00017000
** WILL ASSEMBLE PROPERLY.                                              00018000
*   FLDRTCT(1)=ADDR(RTCTSAP);       /* SYSABEND PORTION OF RTCT.     */ 00019000
         L     @10,CVTRTMCT(,CVTPTR)                               0130 00020000
         LA    @07,RTCTSAP(,@10)                                   0130 00021000
         ST    @07,FLDRTCT                                         0130 00022000
*   FLDRTCT(2)=ADDR(RTCTSUP);       /* SYSUDUMP PORTION OF RTCT.   0131 00023000
*                                                            @G33SPHW*/ 00024000
         LA    @07,RTCTSUP(,@10)                                   0131 00025000
         ST    @07,FLDRTCT+4                                       0131 00026000
*   FLDRTCT(3)=ADDR(RTCTSYD);       /* SYSMDUMP PORTION OF RTCT.   0132 00027000
*                                                            @G33SPHW*/ 00028000
         LA    @10,RTCTSYD(,@10)                                   0132 00029000
         ST    @10,FLDRTCT+8                                       0132 00030000
*   NVTPTRSV=RNVT;                  /* PRESERVE NVT ADDR.            */ 00031000
         ST    RNVT,NVTPTRSV                                       0133 00032000
*   DO CNT=1 TO 3 BY 1;             /* PROCESS THREE PARMLIB MEMBERS    00033000
*                                                            @G33SPHW*/ 00034000
*                                                                  0134 00035000
         LA    @10,1                                               0134 00036000
         ST    @10,CNT                                             0134 00037000
@DL00134 DS    0H                                                  0135 00038000
*     /***************************************************************/ 00039000
*     /*                                                             */ 00040000
*     /* INITIALIZE TO READ A MEMBER FROM PARMLIB.                   */ 00041000
*     /*                                                             */ 00042000
*     /***************************************************************/ 00043000
*                                                                  0135 00044000
*     GEN REFS(TRANTBL);                                           0135 00045000
      MVI   TRANTBL,X'06'    SET TABLE TO ALL INVALID                   00046000
      MVC   TRANTBL+1(255),TRANTBL                                      00047000
*     TRANTBL('A'+1:'I'+1)=''B;     /* ALPHAS VALID.                 */ 00048000
         XC    TRANTBL+193(9),TRANTBL+193                          0136 00049000
*     TRANTBL('J'+1:'R'+1)=''B;     /* ALPHAS VALID.                 */ 00050000
         XC    TRANTBL+209(9),TRANTBL+209                          0137 00051000
*     TRANTBL('S'+1:'Z'+1)=''B;     /* ALPHAS VALID.                 */ 00052000
         XC    TRANTBL+226(8),TRANTBL+226                          0138 00053000
*     TRANTBL('='+1)='01'X;         /* EQUAL ONLY CHAR VALID.        */ 00054000
         MVI   TRANTBL+126,X'01'                                   0139 00055000
*     FLAG1='00'X;                  /* CLEAR CONTROL FLAGS.          */ 00056000
         MVI   FLAG1,X'00'                                         0140 00057000
*     FLAG2='00'X;                  /* CLEAR CONTROL FLAGS.          */ 00058000
         MVI   FLAG2,X'00'                                         0141 00059000
*     PARANCNT=0;                                                  0142 00060000
         MVI   PARANCNT,X'00'                                      0142 00061000
*     NEXTREAD='1'B;                /* FORCE FIRST READ.             */ 00062000
*     NEXTCARD='1'B;                /* FIRST CARD EXPECTED.          */ 00063000
         OI    NEXTREAD,B'11000000'                                0144 00064000
*     RNVT=NVTPTRSV;                /* ENSURE NVT ADDRESSABLE.       */ 00065000
         L     RNVT,NVTPTRSV                                       0145 00066000
*     NVTPAREA->NIPPANAM=NAME(CNT); /* MEMBER TO BE READ.            */ 00067000
         L     @10,NVTPAREA(,RNVT)                                 0146 00068000
         L     @07,CNT                                             0146 00069000
         LR    @15,@07                                             0146 00070000
         SLA   @15,3                                               0146 00071000
         LA    @14,NAME-8(@15)                                     0146 00072000
         MVC   NIPPANAM(8,@10),0(@14)                              0146 00073000
*     R6=FLDRTCT(CNT);              /* APPLICABLE PORTION OF RTCT.   */ 00074000
         SLA   @07,2                                               0147 00075000
         L     R6,FLDRTCT-4(@07)                                   0147 00076000
*     DO WHILE NEXTREAD='1'B;       /* READ TILL TOLD TO STOP.       */ 00077000
         B     @DE00148                                            0148 00078000
@DL00148 DS    0H                                                  0149 00079000
*       RNVT=NVTPTRSV;              /* SET NVT ADDR.                 */ 00080000
*                                                                  0149 00081000
         L     RNVT,NVTPTRSV                                       0149 00082000
*       /*************************************************************/ 00083000
*       /*                                                           */ 00084000
*       /* REQUEST CARD FROM PARMLIB                                 */ 00085000
*       /*                                                           */ 00086000
*       /*************************************************************/ 00087000
*                                                                  0150 00088000
*       CALL GETCARD;               /* READ CARD VIA NIPPMMEM        */ 00089000
*                                                                  0150 00090000
         BAL   @14,GETCARD                                         0150 00091000
*       /*************************************************************/ 00092000
*       /*                                                           */ 00093000
*       /* PROCESS CARD, IF PRESENT.                                 */ 00094000
*       /*                                                           */ 00095000
*       /*************************************************************/ 00096000
*                                                                  0151 00097000
*       IF RPARM>0 THEN             /* GOOD READ...                  */ 00098000
         LTR   RPARM,RPARM                                         0151 00099000
         BNP   @RF00151                                            0151 00100000
*         DO;                       /* -YES.                         */ 00101000
*           IF NEXTCARD='0'B THEN   /* SHOULD CARD BE PRESENT...     */ 00102000
         TM    NEXTCARD,B'01000000'                                0153 00103000
         BNZ   @RF00153                                            0153 00104000
*             ERRSW='1'B;           /* -NO, ERROR.                   */ 00105000
         OI    ERRSW,B'00010000'                                   0154 00106000
*           ELSE                                                   0155 00107000
*             CALL CARDSCAN;        /* CHECK ENTIRE CARD.            */ 00108000
         B     @RC00153                                            0155 00109000
@RF00153 BAL   @14,CARDSCAN                                        0155 00110000
*         END;                                                     0156 00111000
*                                                                  0156 00112000
*       /*************************************************************/ 00113000
*       /*                                                           */ 00114000
*       /* CARD WAS NOT BROUGHT IN FOR SOME REASON.                  */ 00115000
*       /*                                                           */ 00116000
*       /*************************************************************/ 00117000
*                                                                  0157 00118000
*       ELSE                        /* BAD READ (RC=4,8,12,16)       */ 00119000
*         DO;                                                      0157 00120000
         B     @RC00151                                            0157 00121000
@RF00151 DS    0H                                                  0158 00122000
*           NEXTREAD='0'B;          /* STOP FUTURE READS.            */ 00123000
         NI    NEXTREAD,B'01111111'                                0158 00124000
*           IF NVTPAREA->NIPPARC=16 THEN/* EOF (RC=16) ...           */ 00125000
         L     @10,NVTPAREA(,RNVT)                                 0159 00126000
         CLC   NIPPARC(4,@10),@CF00872                             0159 00127000
         BNE   @RF00159                                            0159 00128000
*             DO;                   /* -YES.                         */ 00129000
*               IF NEXTCARD='1'B THEN/* WAS NEW CARD EXPECTED...     */ 00130000
         TM    NEXTCARD,B'01000000'                                0161 00131000
         BNO   @RF00161                                            0161 00132000
*                 ERRSW='1'B;       /* -YES,ERROR-PREMATURE EOF      */ 00133000
         OI    ERRSW,B'00010000'                                   0162 00134000
*             END;                                                 0163 00135000
*           ELSE                    /* RC = 4, 8, OR 12.             */ 00136000
*             RTCTMULT=RTCTMULT&&RTCTMULT;/* NO OPTIONS ALLOWED      */ 00137000
         B     @RC00159                                            0164 00138000
@RF00159 XC    RTCTMULT(4,R6),RTCTMULT(R6)                         0164 00139000
*         END;                                                     0165 00140000
*                                                                  0165 00141000
@RC00159 DS    0H                                                  0166 00142000
*       /*************************************************************/ 00143000
*       /*                                                           */ 00144000
*       /* IF ERROR WAS FOUND, PRINT ERROR MESSAGE(S) AND ABORT THIS */ 00145000
*       /* PARMLIB MEMBER.                                           */ 00146000
*       /*                                                           */ 00147000
*       /*************************************************************/ 00148000
*                                                                  0166 00149000
*       IF ERRSW='1'B THEN          /* ERROR ENCOUNTERED...          */ 00150000
@RC00151 TM    ERRSW,B'00010000'                                   0166 00151000
         BNO   @RF00166                                            0166 00152000
*         DO;                       /* -YES.                         */ 00153000
*           NM884(1:8)=NAME(CNT);   /* SET NAME IN MSG               */ 00154000
         L     @10,CNT                                             0168 00155000
         SLA   @10,3                                               0168 00156000
         LA    @07,NAME-8(@10)                                     0168 00157000
         MVC   NM884(8),0(@07)                                     0168 00158000
*           NM885(1:8)=NAME(CNT);                                  0169 00159000
         LA    @07,NAME-8(@10)                                     0169 00160000
         MVC   NM885(8),0(@07)                                     0169 00161000
*           CALL ERRMSG;            /* PUT OUT MSGS.                 */ 00162000
         BAL   @14,ERRMSG                                          0170 00163000
*           NEXTREAD='0'B;          /* STOP READS.                   */ 00164000
         NI    NEXTREAD,B'01111111'                                0171 00165000
*         END;                                                     0172 00166000
*     END;                          /* END OF DO-WHILE               */ 00167000
@RF00166 DS    0H                                                  0173 00168000
@DE00148 TM    NEXTREAD,B'10000000'                                0173 00169000
         BO    @DL00148                                            0173 00170000
*   END;                            /* END OF DO-BY, ALL PROCESSED   */ 00171000
         LA    @10,1                                               0174 00172000
         AL    @10,CNT                                             0174 00173000
         ST    @10,CNT                                             0174 00174000
         C     @10,@CF00090                                        0174 00175000
         BNH   @DL00134                                            0174 00176000
*   RTCTSAO=RTCTSAP;                /* SET SYSABEND EFFECTIVE OPTIONS   00177000
*                                                            @G33SPHW*/ 00178000
         L     @10,CVTRTMCT(,CVTPTR)                               0175 00179000
         MVC   RTCTSAO(4,@10),RTCTSAP(@10)                         0175 00180000
*   RTCTSUO=RTCTSUP;                /* SET SYSUDUMP EFFECTIVE OPTIONS   00181000
*                                                            @G33SPHW*/ 00182000
         MVC   RTCTSUO(4,@10),RTCTSUP(@10)                         0176 00183000
*   RTCTSYO=RTCTSYD;                /* SET SYSMDUMP EFFECTIVE OPTIONS   00184000
*                                                            @G33SPHW*/ 00185000
         MVC   RTCTSYO(4,@10),RTCTSYD(@10)                         0177 00186000
*CARDSCAN:                                                         0178 00187000
*   PROC;                                                          0178 00188000
*                                                                  0178 00189000
@EL00001 DS    0H                                                  0178 00190000
@EF00001 DS    0H                                                  0178 00191000
@ER00001 LM    @14,@12,12(@13)                                     0178 00192000
         BR    @14                                                 0178 00193000
CARDSCAN STM   @14,@12,@SA00002                                    0178 00194000
*/* THIS ROUTINE WILL PROCESS THE ENTIRE CARD JUST READ IN.  IT      */ 00195000
*/* WILL RETURN TO THE MAINLINE ONLY IF A NEW CARD IS NEEDED, OR     */ 00196000
*/* IF AN ERROR IS DETECTED.                                         */ 00197000
*                                                                  0179 00198000
*   IF RPARM->PARM(1:1)=' '&        /* BLANK CARD...                 */ 00199000
*       RPARM->PARM(2:2)='FF'X THEN /* -YES.                         */ 00200000
         CLI   PARM(RPARM),C' '                                    0179 00201000
         BNE   @RF00179                                            0179 00202000
         CLI   PARM+1(RPARM),X'FF'                                 0179 00203000
         BNE   @RF00179                                            0179 00204000
*     ERRSW='1'B;                   /* BLANK CARD NOT PERMITTED.     */ 00205000
*                                                                  0180 00206000
         OI    ERRSW,B'00010000'                                   0180 00207000
*   /*****************************************************************/ 00208000
*   /*                                                               */ 00209000
*   /* CARD NOT BLANK SO SCAN IT.                                    */ 00210000
*   /*                                                               */ 00211000
*   /*****************************************************************/ 00212000
*                                                                  0181 00213000
*   ELSE                                                           0181 00214000
*     DO;                                                          0181 00215000
         B     @RC00179                                            0181 00216000
@RF00179 DS    0H                                                  0182 00217000
*       START=RPARM;                /* START OF SCAN                 */ 00218000
         LR    START,RPARM                                         0182 00219000
*       STOPSCAN='0'B;              /* DO-WHILE LOOP CONTROL         */ 00220000
*                                                                  0183 00221000
         NI    STOPSCAN,B'11110111'                                0183 00222000
*       /*************************************************************/ 00223000
*       /*                                                           */ 00224000
*       /* SCAN ACROSS THE CARD UNTIL IT IS COMPLETE (NORMALLY OR    */ 00225000
*       /* UNTIL AN ERROR IS FOUND).                                 */ 00226000
*       /*                                                           */ 00227000
*       /*************************************************************/ 00228000
*                                                                  0184 00229000
*       DO WHILE STOPSCAN='0'B;                                    0184 00230000
         B     @DE00184                                            0184 00231000
@DL00184 DS    0H                                                  0185 00232000
*         RFY                                                      0185 00233000
*          (R2,                                                    0185 00234000
*           R1) RSTD;                                              0185 00235000
*         R2=0;                     /* TRT INDEX VALUE.              */ 00236000
         SLR   R2,R2                                               0186 00237000
*         TRT(PARM(1:9),TRANTBL);   /* LOOK FOR STOPPER.             */ 00238000
         TRT   PARM(9,START),TRANTBL                               0187 00239000
*         IF R2=0 THEN              /* .GT. 8 CHARACTERS...          */ 00240000
         LTR   R2,R2                                               0188 00241000
         BNZ   @RF00188                                            0188 00242000
*           ERRSW='1'B;             /* -YES, SHOW ERROR FOUND.       */ 00243000
         OI    ERRSW,B'00010000'                                   0189 00244000
*         ELSE                      /* .LE. 8 CHARACTERS.            */ 00245000
*           DO;                                                    0190 00246000
         B     @RC00188                                            0190 00247000
@RF00188 DS    0H                                                  0191 00248000
*             ERRSW='0'B;           /* NO ERROR YET.                 */ 00249000
*             DOSCAN='1'B;          /* PRESUPPOSE SCAN NEEDED        */ 00250000
         OI    DOSCAN,B'00100000'                                  0192 00251000
         NI    ERRSW,B'11101111'                                   0192 00252000
*             STOP=R1;              /* POINT WHERE SCAN STOPPED.     */ 00253000
*                                                                  0193 00254000
         ST    R1,STOP                                             0193 00255000
*             /*******************************************************/ 00256000
*             /*                                                     */ 00257000
*             /* USE VALUE EXTRACTED FROM TRANSLATE TABLE TO GET TO  */ 00258000
*             /* PROPER CHARACTER HANDLING ROUTINE.                  */ 00259000
*             /*                                                     */ 00260000
*             /*******************************************************/ 00261000
*                                                                  0194 00262000
*             CALL BRTABLE(R2);     /* GO HANDLE STOPPER CHARACTER.  */ 00263000
         LR    @10,R2                                              0194 00264000
         SLA   @10,2                                               0194 00265000
         L     @15,BRTABLE-4(@10)                                  0194 00266000
         BALR  @14,@15                                             0194 00267000
*             RFY                                                  0195 00268000
*              (R2,                                                0195 00269000
*               R1) UNRSTD;                                        0195 00270000
*                                                                  0195 00271000
*             /*******************************************************/ 00272000
*             /*                                                     */ 00273000
*             /* SYNTAX CHECK THE ISOLATED SUB-PARM IF THE STOPPER   */ 00274000
*             /* CHARACTER WARRANTS IT.                              */ 00275000
*             /*                                                     */ 00276000
*             /*******************************************************/ 00277000
*                                                                  0196 00278000
*             IF DOSCAN='1'B THEN   /* SCAN NEEDED...                */ 00279000
         TM    DOSCAN,B'00100000'                                  0196 00280000
         BNO   @RF00196                                            0196 00281000
*               DO;                 /* -YES.                         */ 00282000
*                 R5=STOP-START;    /* LENGTH OF SUBPARAMETER.       */ 00283000
         L     R5,STOP                                             0198 00284000
         SLR   R5,START                                            0198 00285000
*                 IF R5<2 THEN      /* PARM TOO SHORT...             */ 00286000
         C     R5,@CF00166                                         0199 00287000
         BNL   @RF00199                                            0199 00288000
*                   ERRSW='1'B;     /* -YES, SHOW ERROR.             */ 00289000
         OI    ERRSW,B'00010000'                                   0200 00290000
*                 ELSE              /* PARM WITHIN LIMITS            */ 00291000
*                   DO;                                            0201 00292000
         B     @RC00199                                            0201 00293000
@RF00199 DS    0H                                                  0202 00294000
*                     IF PDSDSW='1'B THEN/* KEYWORD TYPE...          */ 00295000
         TM    PDSDSW,B'00000100'                                  0202 00296000
         BNO   @RF00202                                            0202 00297000
*                       CALL PDATPARM;/* PDATA = ...                 */ 00298000
         BAL   @14,PDATPARM                                        0203 00299000
*                     ELSE                                         0204 00300000
*                       CALL SDATPARM;/* SDATA = ...                 */ 00301000
         B     @RC00202                                            0204 00302000
@RF00202 BAL   @14,SDATPARM                                        0204 00303000
*                     IF STOP->PARM(1:1)=')' THEN/* SUBSTRING JUST 0205 00304000
*                                      END...                        */ 00305000
@RC00202 L     @10,STOP                                            0205 00306000
         CLI   PARM(@10),C')'                                      0205 00307000
         BNE   @RF00205                                            0205 00308000
*                       OPNPARAN='0'B;/* -YES, DONT ALLOW MORE     0206 00309000
*                                      SDATA/PDATA SUBPARMS.         */ 00310000
         NI    OPNPARAN,B'10111111'                                0206 00311000
*                   END;                                           0207 00312000
@RF00205 DS    0H                                                  0208 00313000
*               END;                                               0208 00314000
@RC00199 DS    0H                                                  0209 00315000
*           END;                                                   0209 00316000
@RF00196 DS    0H                                                  0210 00317000
*         IF ERRSW='1'B THEN        /* ANY ERRORS FOUND...           */ 00318000
@RC00188 TM    ERRSW,B'00010000'                                   0210 00319000
         BNO   @RF00210                                            0210 00320000
*           STOPSCAN='1'B;          /* -YES, KILL SCAN.              */ 00321000
         OI    STOPSCAN,B'00001000'                                0211 00322000
*         ELSE                      /* N0 ERRORS.                    */ 00323000
*           START=STOP+1;           /* BUMP FOR NEXT TRT SCAN        */ 00324000
         B     @RC00210                                            0212 00325000
@RF00210 LA    START,1                                             0212 00326000
         AL    START,STOP                                          0212 00327000
*       END;                        /* END STOPSCAN DO-WHILE.        */ 00328000
*                                                                  0213 00329000
@RC00210 DS    0H                                                  0213 00330000
@DE00184 TM    STOPSCAN,B'00001000'                                0213 00331000
         BZ    @DL00184                                            0213 00332000
*       /*************************************************************/ 00333000
*       /*                                                           */ 00334000
*       /* CARD HAS EITHER BEEN COMPLETED OR ABORTED.                */ 00335000
*       /*                                                           */ 00336000
*       /*************************************************************/ 00337000
*                                                                  0214 00338000
*       IF ERRSW='1'B THEN          /* STOP BECAUSE OF ERROR...      */ 00339000
         TM    ERRSW,B'00010000'                                   0214 00340000
         BNO   @RF00214                                            0214 00341000
*         NEXTREAD='0'B;            /* -YES, STOP THE READS.         */ 00342000
         NI    NEXTREAD,B'01111111'                                0215 00343000
*     END;                          /* END OF NON-BLANK CARD.        */ 00344000
*   RETURN;                         /* BACK TO MAINLINE.             */ 00345000
@EL00002 DS    0H                                                  0217 00346000
@EF00002 DS    0H                                                  0217 00347000
@ER00002 LM    @14,@12,@SA00002                                    0217 00348000
         BR    @14                                                 0217 00349000
*   END CARDSCAN;                                                  0218 00350000
         B     @EL00002                                            0218 00351000
*LPARAN:                                                           0219 00352000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0219 00353000
*                                                                  0219 00354000
LPARAN   DS    0H                                                  0220 00355000
*   /*****************************************************************/ 00356000
*   /*                                                               */ 00357000
*   /* A LEFT PARANTHESES WAS FOUND ON THE CARD. THE START-OF- SCAN  */ 00358000
*   /* POINTER FOR THE TRT WILL BE UPDATED AND THE PARAN COUNT WILL  */ 00359000
*   /* BE SET TO ONE. THE TRANSLATE TABLE WILL ALSO BE SET TO REFLECT*/ 00360000
*   /* VALID AND INVALID CHARACTERS FOLLOWING THE LEFT PARAN.        */ 00361000
*   /*                                                               */ 00362000
*   /*****************************************************************/ 00363000
*                                                                  0220 00364000
*   RFY                                                            0220 00365000
*     R14 RSTD;                                                    0220 00366000
*   PARANCNT=1;                     /* SET PARAN COUNT.              */ 00367000
         MVI   PARANCNT,X'01'                                      0221 00368000
*   OPNPARAN='1'B;                  /* SUBSTRINGING ALLOWED AFTER  0222 00369000
*                                      LEFT PARANTHESES.             */ 00370000
*                                                                  0222 00371000
         OI    OPNPARAN,B'01000000'                                0222 00372000
*   /*****************************************************************/ 00373000
*   /*                                                               */ 00374000
*   /* ALLOW CHARACTERS.                                             */ 00375000
*   /*                                                               */ 00376000
*   /*****************************************************************/ 00377000
*                                                                  0223 00378000
*   TRANTBL(')'+1)='05'X;           /* RIGHT PARAN VALID             */ 00379000
         MVI   TRANTBL+93,X'05'                                    0223 00380000
*   TRANTBL(','+1)='02'X;           /* COMMA VALID.                  */ 00381000
*                                                                  0224 00382000
         MVI   TRANTBL+107,X'02'                                   0224 00383000
*   /*****************************************************************/ 00384000
*   /*                                                               */ 00385000
*   /* DISALLOW CHARACTERS                                           */ 00386000
*   /*                                                               */ 00387000
*   /*****************************************************************/ 00388000
*                                                                  0225 00389000
*   TRANTBL('='+1)='06'X;           /* EQUAL SIGN INVALID            */ 00390000
         MVI   TRANTBL+126,X'06'                                   0225 00391000
*   TRANTBL(' '+1)='06'X;           /* BLANK INVALID.                */ 00392000
         MVI   TRANTBL+64,X'06'                                    0226 00393000
*   TRANTBL('('+1)='06'X;           /* LEFT PARAN INVALID            */ 00394000
*                                                                  0227 00395000
         MVI   TRANTBL+77,X'06'                                    0227 00396000
*   /*****************************************************************/ 00397000
*   /*                                                               */ 00398000
*   /* SHOW NO SCAN NECCESSARY                                       */ 00399000
*   /*                                                               */ 00400000
*   /*****************************************************************/ 00401000
*                                                                  0228 00402000
*   DOSCAN='0'B;                    /* NO SCAN NECCESSARY.           */ 00403000
         NI    DOSCAN,B'11011111'                                  0228 00404000
*   RETURN;                                                        0229 00405000
@EL00003 DS    0H                                                  0229 00406000
@EF00003 DS    0H                                                  0229 00407000
@ER00003 BR    @14                                                 0229 00408000
*   RFY                                                            0230 00409000
*     R14 UNRSTD;                                                  0230 00410000
*   END LPARAN;                                                    0231 00411000
*INVALID:                                                          0232 00412000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0232 00413000
*                                                                  0232 00414000
INVALID  DS    0H                                                  0233 00415000
*   /*****************************************************************/ 00416000
*   /*                                                               */ 00417000
*   /* AN INVALID CHARACTER WAS FOUND DURING THE SCAN, IE, IT WAS NOT*/ 00418000
*   /* PERMITTED AFTER A PREVIOUSLY VALID STOPPER CHARACTER. THE     */ 00419000
*   /* CURRENT PARMLIB MEMBER WILL BE FLUSHED.                       */ 00420000
*   /*                                                               */ 00421000
*   /*****************************************************************/ 00422000
*                                                                  0233 00423000
*   RFY                                                            0233 00424000
*     R14 RSTD;                                                    0233 00425000
*   ERRSW='1'B;                     /* SHOW MEMBER FLUSH             */ 00426000
*   DOSCAN='0'B;                    /* NO SCAN TO BE DONE            */ 00427000
         OI    ERRSW,B'00010000'                                   0235 00428000
         NI    DOSCAN,B'11011111'                                  0235 00429000
*   RETURN;                         /* BACK TO MAINLINE              */ 00430000
@EL00004 DS    0H                                                  0236 00431000
@EF00004 DS    0H                                                  0236 00432000
@ER00004 BR    @14                                                 0236 00433000
*   RFY                                                            0237 00434000
*     R14 UNRSTD;                                                  0237 00435000
*   END INVALID;                                                   0238 00436000
*COMMA:                                                            0239 00437000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0239 00438000
*                                                                  0239 00439000
COMMA    DS    0H                                                  0240 00440000
*   /*****************************************************************/ 00441000
*   /*                                                               */ 00442000
*   /* A COMMA WAS FOUND ON THE CARD. THE SYNTAX SCAN SWITCH WILL BE */ 00443000
*   /* SET. THE TRANSLATE TABLE WILL ALSO BE SET TO ALLOW VALID      */ 00444000
*   /* CHARACTERS AFTER A COMMA.                                     */ 00445000
*   /*                                                               */ 00446000
*   /*****************************************************************/ 00447000
*                                                                  0240 00448000
*   RFY                                                            0240 00449000
*     R14 RSTD;                                                    0240 00450000
*                                                                  0240 00451000
*   /*****************************************************************/ 00452000
*   /*                                                               */ 00453000
*   /* ALLOW...                                                      */ 00454000
*   /*                                                               */ 00455000
*   /*****************************************************************/ 00456000
*                                                                  0241 00457000
*   TRANTBL(' '+1)='03'X;           /* BLANK NEXT VALID STOPPER.     */ 00458000
         MVI   TRANTBL+64,X'03'                                    0241 00459000
*   TRANTBL(','+1)='02'X;           /* COMMA STILL VALID             */ 00460000
         MVI   TRANTBL+107,X'02'                                   0242 00461000
*   TRANTBL(')'+1)='05'X;           /* RIGHT PARAN VALID             */ 00462000
         MVI   TRANTBL+93,X'05'                                    0243 00463000
*   TRANTBL('='+1)='01'X;           /* EQUAL SIGN VALID              */ 00464000
*                                                                  0244 00465000
         MVI   TRANTBL+126,X'01'                                   0244 00466000
*   /*****************************************************************/ 00467000
*   /*                                                               */ 00468000
*   /* DISALLOW                                                      */ 00469000
*   /*                                                               */ 00470000
*   /*****************************************************************/ 00471000
*                                                                  0245 00472000
*   TRANTBL('('+1)='06'X;           /* LEFT PARAN NOT VALID.         */ 00473000
         MVI   TRANTBL+77,X'06'                                    0245 00474000
*   IF(STOP-1)->PARM(1:1)=')' THEN  /* FOLLOW PARAN...               */ 00475000
         L     @15,STOP                                            0246 00476000
         BCTR  @15,0                                               0246 00477000
         CLI   PARM(@15),C')'                                      0246 00478000
         BNE   @RF00246                                            0246 00479000
*     DOSCAN='0'B;                  /* -YES, NO SCAN NEEDED          */ 00480000
         NI    DOSCAN,B'11011111'                                  0247 00481000
*   RETURN;                         /* BACK TO MAINLINE.             */ 00482000
@EL00005 DS    0H                                                  0248 00483000
@EF00005 DS    0H                                                  0248 00484000
@ER00005 BR    @14                                                 0248 00485000
*   RFY                                                            0249 00486000
*     R14 UNRSTD;                                                  0249 00487000
*   END COMMA;                                                     0250 00488000
         B     @EL00005                                            0250 00489000
*BLANK:                                                            0251 00490000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0251 00491000
*                                                                  0251 00492000
BLANK    DS    0H                                                  0252 00493000
*   /*****************************************************************/ 00494000
*   /*                                                               */ 00495000
*   /* A BLANK WAS READ. IF PRECEDED BY COMMA IT SIGNALS A           */ 00496000
*   /* CONTINUATION CARD. IF PRECEDED BY ANYTHING ELSE IT SIGNALS END*/ 00497000
*   /* OF MEMBER - NO NEXT CARD WILL BE ALLOWED                      */ 00498000
*   /*                                                               */ 00499000
*   /*****************************************************************/ 00500000
*                                                                  0252 00501000
*   RFY                                                            0252 00502000
*     R14 RSTD;                                                    0252 00503000
*   NEXTREAD='1'B;                  /* TO TEST IF MORE CARDS.        */ 00504000
*   STOPSCAN='1'B;                  /* STOP DO-WHILE.                */ 00505000
         OI    NEXTREAD,B'10001000'                                0254 00506000
*   IF(STOP-1)->PARM(1:1)=',' THEN  /* IS IT CONTINUATION...         */ 00507000
         L     @15,STOP                                            0255 00508000
         BCTR  @15,0                                               0255 00509000
         CLI   PARM(@15),C','                                      0255 00510000
         BNE   @RF00255                                            0255 00511000
*     DO;                           /* -YES.                         */ 00512000
*       NEXTCARD='1'B;              /* SET TO EXPECT NEXT CARD.      */ 00513000
*       DOSCAN='0'B;                /* NO SUBPARM SCAN.              */ 00514000
         OI    NEXTCARD,B'01000000'                                0258 00515000
         NI    DOSCAN,B'11011111'                                  0258 00516000
*     END;                                                         0259 00517000
*   ELSE                            /* IT'S END OF MEMBER.           */ 00518000
*     DO;                                                          0260 00519000
         B     @RC00255                                            0260 00520000
@RF00255 DS    0H                                                  0261 00521000
*       NEXTCARD='0'B;              /* NEXT CARD NOT EXPECTED.       */ 00522000
         NI    NEXTCARD,B'10111111'                                0261 00523000
*       IF(STOP-1)->PARM(1:1)=')' THEN/* FOLLOW PARAN...             */ 00524000
         L     @15,STOP                                            0262 00525000
         BCTR  @15,0                                               0262 00526000
         CLI   PARM(@15),C')'                                      0262 00527000
         BNE   @RF00262                                            0262 00528000
*         DOSCAN='0'B;              /* NO SCAN AFTER PARAN.          */ 00529000
         NI    DOSCAN,B'11011111'                                  0263 00530000
*       IF PARANCNT^=0 THEN         /* BALANCED PARANTHESES...       */ 00531000
@RF00262 CLI   PARANCNT,0                                          0264 00532000
         BE    @RF00264                                            0264 00533000
*         ERRSW='1'B;               /* -NO, FLUSH MEMBER.            */ 00534000
         OI    ERRSW,B'00010000'                                   0265 00535000
*     END;                                                         0266 00536000
*   RETURN;                         /* BACK TO MAINLINE.             */ 00537000
