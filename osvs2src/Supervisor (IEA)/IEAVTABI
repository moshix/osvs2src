         XC    NIPPMHH(2,@10),NIPPMHH(@10)                         0790 02394000
*               NIPPMR='01'X;       /* AND RECORD 1          @ZA07472*/ 02395000
         MVI   NIPPMR(@10),X'01'                                   0791 02396000
*               GOTO NIPPMRCR;      /* CONTINUE TO PROCESS TEXT    0792 02397000
*                                                            @ZA07472*/ 02398000
         B     NIPPMRCR                                            0792 02399000
*             END;                  /*                       @ZA07472*/ 02400000
*         END;                      /*                         Y02752*/ 02401000
*       GOTO NIPPMTER;              /* INDICATE I/O ERROR      Y02752*/ 02402000
         B     NIPPMTER                                            0795 02403000
*     END;                          /*                         Y02752*/ 02404000
*   RESPECIFY                                                      0797 02405000
*     NIPPMBYT BASED(NIPPMR01);                                    0797 02406000
@RF00778 DS    0H                                                  0798 02407000
*NIPPMRCR:                          /*                       @ZA07472*/ 02408000
*   NIPPMR01=ADDR(NIPPARCD);        /* R1 = ADDR OF TEXT RECORD READ    02409000
*                                      IN                            */ 02410000
*                                                                  0798 02411000
NIPPMRCR L     NIPPMR01,NVTPAREA(,RNVT)                            0798 02412000
         LA    NIPPMR01,NIPPARCD(,NIPPMR01)                        0798 02413000
*   /*****************************************************************/ 02414000
*   /*                                                               */ 02415000
*   /* SCAN THROUGH LEADING BLANK CHARACTERS                         */ 02416000
*   /*                                                               */ 02417000
*   /*****************************************************************/ 02418000
*                                                                  0799 02419000
*   NIPPMBUF(73:73)='FF'X;          /* PLACE 'FF'X IN COL 73         */ 02420000
         MVI   NIPPMBUF+72(NIPPMR01),X'FF'                         0799 02421000
*   NIPPMKPR=1;                     /* COUNT STARTS AT ONE           */ 02422000
         LA    NIPPMKPR,1                                          0800 02423000
*NIPPMPBK:                                                         0801 02424000
*   IF NIPPMBYT=' '                 /* IS THIS CHAR BLANK            */ 02425000
*     THEN                          /* YES - CHECK NEXT CHAR         */ 02426000
NIPPMPBK CLI   NIPPMBYT(NIPPMR01),C' '                             0801 02427000
         BNE   @RF00801                                            0801 02428000
*     DO;                                                          0802 02429000
*       NIPPMR01=NIPPMR01+1;        /* POINT TO NEXT CHAR      Y02752*/ 02430000
         LA    @08,1                                               0803 02431000
         ALR   NIPPMR01,@08                                        0803 02432000
*       NIPPMKPR=NIPPMKPR+1;        /* INCREMENT COUNTER       Y02752*/ 02433000
         ALR   NIPPMKPR,@08                                        0804 02434000
*       IF NIPPMKPR=72              /* ARE FIRST 71 CHARS BLANK    0805 02435000
*                                                              Y02752*/ 02436000
*         THEN                      /* YES-CHECK FOR CONTINUATION    */ 02437000
         C     NIPPMKPR,@CF02389                                   0805 02438000
         BNE   @RF00805                                            0805 02439000
*         DO;                                                      0806 02440000
*           IF NIPPMBYT^=' '        /* IS 72'ND CHAR BLK             */ 02441000
*             THEN                  /* NO - INDICATES CONTINUATION   */ 02442000
         CLI   NIPPMBYT(NIPPMR01),C' '                             0807 02443000
         BNE   @RT00807                                            0807 02444000
*             GO TO NIPPMPRD;       /* READ NEXT RECORD              */ 02445000
*           ELSE                    /* 72ND IS BLANK - NO          0809 02446000
*                                      CONTINUATION                  */ 02447000
*             GO TO NIPPMTND;       /* REG 1 POINTS TO 72ND BLANK. 0809 02448000
*                                      NORMAL RETURN TO CALLER       */ 02449000
         B     NIPPMTND                                            0809 02450000
*         END;                                                     0810 02451000
*       ELSE                                                       0811 02452000
*         GO TO NIPPMPBK;           /* COUNT IS NOT 72 YET- GO CHECK    02453000
*                                      NEXT CHAR                     */ 02454000
*     END;                                                         0812 02455000
*   ELSE                            /* FIRST 71 CHARS ARE NOT        */ 02456000
*     DO;                           /* BLANK                         */ 02457000
@RF00801 DS    0H                                                  0814 02458000
*       NIPPMKPR=NIPPMR01-NIPPMKPR+72;/* POINT TO 72'ND BYTE   Y02752*/ 02459000
         LR    @08,NIPPMR01                                        0814 02460000
         SLR   @08,NIPPMKPR                                        0814 02461000
         AL    @08,@CF02389                                        0814 02462000
         LR    NIPPMKPR,@08                                        0814 02463000
*       RESPECIFY                                                  0815 02464000
*         NIPPMBYT BASED(NIPPMKPR); /*                         Y02752*/ 02465000
*       NIPPMBYT=' ';               /* INSURE BLANK AT END BY SETTING   02466000
*                                      72'ND BYTE TO BLK             */ 02467000
         MVI   NIPPMBYT(NIPPMKPR),C' '                             0816 02468000
*     END;                                                         0817 02469000
*   GENERATE REFS(NIPPMREG) SETS(NIPPMR02,NIPPMR00);               0818 02470000
*                                                                /*     02471000
*NIPPMTND  DS  0H                                               Y02752  02472000
NIPPMTND LM    NIPPMR02,NIPPMR00,NIPPMREG     RESTORE CALLERS REGS      02473000
*   GO TO NIPPMAD0;                 /* NORMAL RETURN TO CALLER       */ 02474000
         BR    NIPPMADR                                            0819 02475000
*NIPPMTER:                                                         0820 02476000
*   NIPPMR01=ADDR(NIPPAIOB);        /* R1 = ADDR OF PARMLIB IOB      */ 02477000
*                                                                  0820 02478000
NIPPMTER L     NIPPMR01,NVTPAREA(,RNVT)                            0820 02479000
         LA    NIPPMR01,NIPPAIOB(,NIPPMR01)                        0820 02480000
*   /*****************************************************************/ 02481000
*   /*                                                               */ 02482000
*   /* NIPSENSE - FORMAT AND WRITE I/O ERROR MESSAGE                 */ 02483000
*   /*                                                               */ 02484000
*   /*****************************************************************/ 02485000
*                                                                  0821 02486000
