         TITLE 'IEAVTACR                                               *00001000
                        '                                               00002000
IEAVTACR CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(15)                                             0001 00007000
         DC    C'IEAVTACR 77129 '                                  0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @05,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@05                                         0001 00012000
*   DO;                                                            0170 00013000
*                                                                  0170 00014000
*/* ******************************************************************/ 00015000
*/*                                                                  */ 00016000
*/* 01* SEGMENT NAME   ACRINIT1                                      */ 00017000
*/*                                                                  */ 00018000
*/* 02* FUNCTION                                                     */ 00019000
*/*     ACR INITIALIZATION SEGMENT FOR ALL ENTRIES VIA CALLRTM       */ 00020000
*/*                                                                  */ 00021000
*/* 02* INPUT= ENTCDREG(REG 0) - ENTRY TYPE - IDENTIFIER CODE        */ 00022000
*/*                            * TYPE=ACR           8                */ 00023000
*/*                            * TYPE=ACRLM         4                */ 00024000
*/*                            * TYPE=ACRDISP       2                */ 00025000
*/*            LRBPTR (REG 1) - ENTRY TYPE  -    CONTENTS            */ 00026000
*/*                           * TYPE=ACR       LOGREC BUFFER ADDRESS */ 00027000
*/*                           * TYPE=ACRLM     UNKNOWN               */ 00028000
*/*                           * TYPE=ACRDISP   UNKNOWN               */ 00029000
*/*            IHRETPTR(REG 2) - ENTRY TYPE  -    CONTENTS           */ 00030000
*/*                           * TYPE=ACR ONLY  RETURN PT             */ 00031000
*/*            R14 (REG 14) -  ENTRY TYPE                            */ 00032000
*/*                           * TYPE=ACRLM AND ACRDISP -RETURN PT    */ 00033000
*/*                                                                  */ 00034000
*/* 02* OUTPUT= PSAPTR (REG 4) - CURRENT PSA ADDRESS (BASED ZERO)    */ 00035000
*/*             RRPLPTR (REG 6) - RECOVERY RTN PARM LIST ADDRESS     */ 00036000
*/*             LCCAPTR (REG 10) - CURRENT LCCA ADDRESS              */ 00037000
*/*                                                                  */ 00038000
*/* ******************************************************************/ 00039000
*                                                                  0171 00040000
*ACRSTART:                                                         0171 00041000
*     R4=0;                         /* SET BASE FOR PSA              */ 00042000
ACRSTART SLR   R4,R4                                               0171 00043000
*     R10=PSALCCAV;                 /* INITIALIZE CURRENT LCCA BASE  */ 00044000
         L     R10,PSALCCAV(,R4)                                   0172 00045000
*     LCCACREX=R0;                  /* SET THE ACR ENTRY FLAGS       */ 00046000
         STC   R0,LCCACREX(,R10)                                   0173 00047000
*     RESPECIFY                                                    0174 00048000
*      (R6) RSTD;                                                  0174 00049000
*     R6=ADDR(ACRRRPL);             /* SET THE RECOVERY PARM ADDRESS */ 00050000
         L     @15,LCCACPUS(,R10)                                  0175 00051000
         L     @15,WSACACR(,@15)                                   0175 00052000
         LA    R6,ACRRRPL(,@15)                                    0175 00053000
*     IF LCCACRIN='1'B THEN         /* IS THIS PRE-PROCESSING        */ 00054000
         TM    LCCACRIN(R10),B'00001000'                           0176 00055000
         BNO   @RF00176                                            0176 00056000
*       DO;                         /* YES, PRE-PROCESSING           */ 00057000
*         ACRRRPLP=R6;              /* YES, SET RRPL ADDR IN CURRENT    00058000
*                                      CPUS ACRSA                    */ 00059000
         ST    R6,ACRRRPLP(,@15)                                   0178 00060000
*         ACRENTR1=R1;              /* TEMP SAVE AREA FOR LRB        */ 00061000
         ST    R1,ACRENTR1(,@15)                                   0179 00062000
*       END;                                                       0180 00063000
*     ELSE                          /* NOT PRE-PROCESSING PHASE      */ 00064000
*       LCCARCPU->LCCACPUS->WSACACR->ACRRRPLP=R6;/* SET RRPL ADDR IN    00065000
*                                      RECOVERY CPUS ACRSA           */ 00066000
         B     @RC00176                                            0181 00067000
@RF00176 L     @15,LCCARCPU(,R10)                                  0181 00068000
         L     @15,LCCACPUS(,@15)                                  0181 00069000
         L     @15,WSACACR(,@15)                                   0181 00070000
         ST    R6,ACRRRPLP(,@15)                                   0181 00071000
*     RRPL=''B;                     /* CLEAR THE RRPL                */ 00072000
@RC00176 XC    RRPL(64,R6),RRPL(R6)                                0182 00073000
*     GEN REFS(RRPLREGS)(STM 0,14,RRPLREGS(R6));/* SAVE REGS FOR   0183 00074000
*                                      RECURSION                     */ 00075000
         STM 0,14,RRPLREGS(R6)                                          00076000
*     PSAACR='1'B;                  /* SET ACR SUPER BIT TO CAUSE RTM   00077000
*                                      TO DIRECT BRANCH TO ACR     0184 00078000
*                                      RECOVERY ROUTINE              */ 00079000
         OI    PSAACR(R4),B'00000100'                              0184 00080000
*     RESPECIFY                                                    0185 00081000
*      (R0,                                                        0185 00082000
*       R1,                                                        0185 00083000
*       R2,                                                        0185 00084000
*       R14) UNRESTRICTED;                                         0185 00085000
*                                                                  0185 00086000
*/* ******************************************************************/ 00087000
*/*  END OF  INITIALIZATION SEGMENT - ACRINIT1                       */ 00088000
*/* ******************************************************************/ 00089000
*                                                                  0186 00090000
*   END;                                                           0186 00091000
*RSTRT00:                           /* RETRY ENTRY FOR LOGICAL PHASE    00092000
*                                      IDS (00,01,02,03,04)          */ 00093000
*   R10->LCCACPUS->WSACACR->ACRRTNPT=RRPLRRGS(14+1);/* SAVE LOCK MGR    00094000
*                                      AND SPIN RETURN POINT (REG 14    00095000
*                                      ON ENTRY)                     */ 00096000
RSTRT00  L     @12,LCCACPUS(,R10)                                  0187 00097000
         L     @12,WSACACR(,@12)                                   0187 00098000
         L     @08,RRPLRRGS+56(,R6)                                0187 00099000
         ST    @08,ACRRTNPT(,@12)                                  0187 00100000
*   IF LCCACRIN='1'B THEN           /* IS ENTRY FROM THE EMS OR MFA     00101000
*                                      SLIH?                         */ 00102000
         TM    LCCACRIN(R10),B'00001000'                           0188 00103000
         BNO   @RF00188                                            0188 00104000
*     DO;                                                          0189 00105000
*       R10->LCCACPUS->WSACACR->ACRRTNPT=RRPLRRGS(2+1);/* IN THE CASE   00106000
*                                      OF SLIH ENTRY SAVE REG 2 FOR     00107000
*                                      THE RETURN POINT              */ 00108000
         L     @08,RRPLRRGS+8(,R6)                                 0190 00109000
         ST    @08,ACRRTNPT(,@12)                                  0190 00110000
*/* ******************************************************************/ 00111000
*/* ******************************************************************/ 00112000
*/* *********************    ACR PRE-PROCESSING     ******************/ 00113000
*/* ******************************************************************/ 00114000
*/* ******************************************************************/ 00115000
*                                                                  0191 00116000
*       DO;                         /* EMS/MFA SLIH ENTRY            */ 00117000
*/* ******************************************************************/ 00118000
*/*                                                                  */ 00119000
*/* 01* SEGMENT-NAME  ACRPREP                                        */ 00120000
*/*                                                                  */ 00121000
*/* 02* FUNCTION                                                     */ 00122000
*/*     THE ACR PREPROCESSOR PHASE IS ENTERED VIA THE MFA OR EMS     */ 00123000
*/*     EXTERNAL SLIH AS A RESULT OF A CPU MALFUNCTION.  REGISTER 1  */ 00124000
*/*     CONTAINS THE LOGREC BUFFER ADDRESS ASSOCIATED WITH THE DEAD  */ 00125000
*/*     CPU'S FAILURE.  THE ACR PREPROCESSOR IS AN EXTERNAL INTERRUPT*/ 00126000
*/*     ROUTINE WHICH WILL MAKE THE ACR CONDITION KNOWN TO THE SYSTEM*/ 00127000
*/*     AND PRESERVE THE NECESSARY DATA FOR ACR RECOVERY DURING LATER*/ 00128000
*/*     PROCESSING.  THIS PHASE WILL RETURN CONTROL TO THE EMS SLIH, */ 00129000
*/*     OR TO THE EXTERNAL FLIH IN CASES OF AN MFA.  WHEN THE        */ 00130000
*/*     INTERRUPTED PROCESS RELINQUISHES CONTROL AGAIN, EITHER ON ITS*/ 00131000
*/*     OWN ACCORD, OR DUE TO SOME CONTROL PROGRAM ACTION, ACR'S     */ 00132000
*/*     INTERMEDIATE PROCESSING WILL GAIN CONTROL TO INITIATE SYSTEM */ 00133000
*/*     RECOVERY.                                                    */ 00134000
*/*                                                                  */ 00135000
*/* 02* INPUT= PSAPTR (REG 4) - ADDR OF CURR PSA BASED ZERO          */ 00136000
*/*            RRPLPTR (REG 6) - ADDR OF RECOVERY RTN PARAMETER LIST */ 00137000
*/*            LCCAPTR (REG 10) - ADDR OF THE CURRENT LCCA           */ 00138000
*/*                                                                  */ 00139000
*/* 02* OUTPUT= PSAPTR (REG 4) - ADDR OF CURR PSA BASED ZERO         */ 00140000
*/*            RRPLPTR (REG 6) - ADDR OF RECOVERY RTN PARAMETER LIST */ 00141000
*/*            PSAPTR2 (REG 9) -  ADDR OF THE DEAD PSA               */ 00142000
*/*            LCCAPTR (REG 10) - ADDR OF THE CURRENT LCCA           */ 00143000
*/*            LCCAPTR2 (REG 11) - ADDR OF THE DEAD LCCA             */ 00144000
*/*                                                                  */ 00145000
*/* ******************************************************************/ 00146000
*                                                                  0192 00147000
*         DO;                                                      0192 00148000
*                                                                  0192 00149000
*           /*********************************************************/ 00150000
*           /*                                                       */ 00151000
*           /* RECOVERY NOTE: POSSIBLE PHASE IDS AT THIS POINT ARE   */ 00152000
*           /* (00,01). AN ERROR IN THIS PHASE IS RETRIED AT LABEL   */ 00153000
*           /* RSTRT00.                                              */ 00154000
*           /*                                                       */ 00155000
*           /*********************************************************/ 00156000
*                                                                  0193 00157000
*           IF LOGPHID<'01'X THEN   /* IS ENTRY DUE TO A RETRY FOR AN   00158000
*                                      ERROR IN THIS PHASE           */ 00159000
         CLI   LOGPHID(R6),X'01'                                   0193 00160000
         BNL   @RF00193                                            0193 00161000
*             RRPLFLGS='0'B;        /* NO,RESET ANY PREVIOUS PHASE 0194 00162000
*                                      ERROR INDICATIONS             */ 00163000
         MVI   RRPLFLGS(R6),B'00000000'                            0194 00164000
*           LOGPHID='01'X;          /* SET LOGICAL PHASE ID 01       */ 00165000
@RF00193 MVI   LOGPHID(R6),X'01'                                   0195 00166000
*         END;                                                     0196 00167000
*                                                                  0196 00168000
*         /***********************************************************/ 00169000
*         /*                                                         */ 00170000
*         /* SET THE DEAD CPU OFFLINE TO THE SYSTEM                  */ 00171000
*         /*                                                         */ 00172000
*         /***********************************************************/ 00173000
*                                                                  0197 00174000
*         RESPECIFY                                                0197 00175000
*          (CSD) BASED(R7);         /* TEMP BASE FOR CSD             */ 00176000
*         RESPECIFY                                                0198 00177000
*          (R7) RSTD;                                              0198 00178000
*         R7=CVTCSD;                /* SET CSD ADDR IN CSDPTR        */ 00179000
         L     @12,CVTPTR(,R4)                                     0199 00180000
         L     R7,CVTCSD(,@12)                                     0199 00181000
*         IF RRPLOL='0'B            /* HAS THE CPU ALREADY BEEN    0200 00182000
*                                      MARKED NOT ALIVE BY ACR?      */ 00183000
*           THEN                                                   0200 00184000
         TM    RRPLOL(R6),B'01000000'                              0200 00185000
         BNZ   @RF00200                                            0200 00186000
*           DO;                     /* NO, MARK THE CPU DEAD         */ 00187000
*             RESPECIFY                                            0202 00188000
*              (R2,                                                0202 00189000
*               R3) RSTD;                                          0202 00190000
*                                                                  0202 00191000
*             /*******************************************************/ 00192000
*             /*                                                     */ 00193000
*             /* PREVENT FURTHER JOB SCHEDULING TO THE DEAD CPU      */ 00194000
*             /*                                                     */ 00195000
*             /*******************************************************/ 00196000
*                                                                  0203 00197000
*             R2=SKEDMSK;           /* PICK UP THE CURR JS MASK      */ 00198000
         L     R2,SKEDMSK(,R7)                                     0203 00199000
*SWAPSKED:                          /* RETRY PT IF CS FAILS          */ 00200000
*             SWAPWORD='FFFFFFFF'X; /* SET ALL BITS ON IN WORD       */ 00201000
SWAPSKED L     @12,LCCACPUS(,R10)                                  0204 00202000
         L     @12,WSACACR(,@12)                                   0204 00203000
         MVC   SWAPWORD(4,@12),@CF03111                            0204 00204000
*             SWAPJS=SWAPJS&&BITS(PSASPAD+1);/* TURN OFF THE DEAD CPU   00205000
*                                      BIT                           */ 00206000
         LH    @08,PSASPAD(,R4)                                    0205 00207000
         ALR   @08,@08                                             0205 00208000
         LA    @01,BITS(@08)                                       0205 00209000
         XC    SWAPJS(2,@12),0(@01)                                0205 00210000
*             R3=SWAPWORD&R2;       /* BLD NEW MSK LESS DEAD CPU     */ 00211000
         L     R3,SWAPWORD(,@12)                                   0206 00212000
         NR    R3,R2                                               0206 00213000
*             CS(R2,R3,SKEDMSK);    /* CHANGE THE CSD JS MASK        */ 00214000
         CS    R2,@03,SKEDMSK(R7)                                  0207 00215000
*             BC(7,SWAPSKED);       /* MASK CHANGED BEFORE SWAP      */ 00216000
*                                                                  0208 00217000
         BC    7,SWAPSKED                                          0208 00218000
*             /*******************************************************/ 00219000
*             /*                                                     */ 00220000
*             /* PREVENT FURTHER SYSTEM REFERENCE TO THE DEAD CPU    */ 00221000
*             /*                                                     */ 00222000
*             /*******************************************************/ 00223000
*                                                                  0209 00224000
*             R2=ALIVEMSK;          /* PICK UP CURR ALIVE MASK       */ 00225000
         L     R2,ALIVEMSK(,R7)                                    0209 00226000
*SWAPALIV:                          /* RETRY PT IF CS FAILS          */ 00227000
*             SWAPWORD='FFFFFFFF'X; /* SET ALL BITS ON IN SWAPWORD   */ 00228000
SWAPALIV L     @12,LCCACPUS(,R10)                                  0210 00229000
         L     @12,WSACACR(,@12)                                   0210 00230000
         MVC   SWAPWORD(4,@12),@CF03111                            0210 00231000
*             SWAPAL=SWAPAL&&BITS(PSASPAD+1);/* RESET THE DEAD CPU'S    00232000
*                                      ALIVE MASK INDICATOR          */ 00233000
         LH    @08,PSASPAD(,R4)                                    0211 00234000
         ALR   @08,@08                                             0211 00235000
         LA    @01,BITS(@08)                                       0211 00236000
         XC    SWAPAL(2,@12),0(@01)                                0211 00237000
*             SWAPWORD=SWAPWORD&R2; /* CHANGE MASK FOR COMP&SWAP     */ 00238000
         L     @08,SWAPWORD(,@12)                                  0212 00239000
         NR    @08,R2                                              0212 00240000
         ST    @08,SWAPWORD(,@12)                                  0212 00241000
*             SWAPOL=R2-1;          /* DECREMENT THE ONLINE CPU CT   */ 00242000
         LR    @08,R2                                              0213 00243000
         BCTR  @08,0                                               0213 00244000
         STH   @08,SWAPOL(,@12)                                    0213 00245000
*             R3=SWAPWORD;          /* LOAD SWAP REGISTER            */ 00246000
         L     R3,SWAPWORD(,@12)                                   0214 00247000
*             CS(R2,R3,ALIVEMSK);   /* CHANGE THE CSD ALIVE MSK      */ 00248000
         CS    R2,@03,ALIVEMSK(R7)                                 0215 00249000
*             BC(7,SWAPALIV);       /* MASK CHANGED BEFORE SWAP      */ 00250000
         BC    7,SWAPALIV                                          0216 00251000
*             RRPLOL='1'B;          /* SET CT UPDATED BIT FOR FRR    */ 00252000
         OI    RRPLOL(R6),B'01000000'                              0217 00253000
*             RESPECIFY                                            0218 00254000
*              (R2,                                                0218 00255000
*               R3) UNRSTD;                                        0218 00256000
*           END;                                                   0219 00257000
*                                                                  0219 00258000
*         /***********************************************************/ 00259000
*         /*                                                         */ 00260000
*         /* OBTAIN THE DEAD CPU'S SYSTEM CONTROL BLOCKS             */ 00261000
*         /*                                                         */ 00262000
*         /***********************************************************/ 00263000
*                                                                  0220 00264000
*         R9=PCCAT00P(PSASPAD+1)->PCCAPSAV;/* GET THE DEAD PSA ADDR  */ 00265000
@RF00200 LH    @12,PSASPAD(,R4)                                    0220 00266000
         SLA   @12,2                                               0220 00267000
         L     @01,CVTPTR(,R4)                                     0220 00268000
         L     @01,CVTPCCAT(,@01)                                  0220 00269000
         L     @12,PCCAT00P(@12,@01)                               0220 00270000
         L     R9,PCCAPSAV(,@12)                                   0220 00271000
*         R11=R9->PSALCCAV;         /* GET THE DEAD LCCA ADDRESS     */ 00272000
*                                                                  0221 00273000
         L     R11,PSALCCAV(,R9)                                   0221 00274000
*         /***********************************************************/ 00275000
*         /*                                                         */ 00276000
*         /* SET THE SYSTEM ACR IN PROGRESS INDICATOR IN THE CSD     */ 00277000
*         /*                                                         */ 00278000
*         /***********************************************************/ 00279000
*                                                                  0222 00280000
*         IF RRPLCSD='0'B THEN      /* HAS ACR ALREADY SET SYSTEM ACR   00281000
*                                      INDICATOR BYTE                */ 00282000
         TM    RRPLCSD(R6),B'00100000'                             0222 00283000
         BNZ   @RF00222                                            0222 00284000
*           DO;                     /* NO                            */ 00285000
*             R9->PSAPCCAV->PCCARMS='0'B;/* MARK RMS EMS RECEIVED    */ 00286000
         L     @12,PSAPCCAV(,R9)                                   0224 00287000
         NI    PCCARMS(@12),B'11111110'                            0224 00288000
*             TS(CSDACR);           /* TEST AND SET THE SYSTEM ACR 0225 00289000
*                                      INDICATION                    */ 00290000
         TS    CSDACR(R7)                                          0225 00291000
*             BC(7,STERM);          /* IF ACR IS ALREADY IN PROG-  0226 00292000
*                                      RESS WITHIN THE 'SYSTEM',   0226 00293000
*                                      TERMINATE THE 'SYSTEM'        */ 00294000
         BC    7,STERM                                             0226 00295000
*             RRPLCSD='1'B;         /* SET THE ACR SET BIT FOR FRR   */ 00296000
         OI    RRPLCSD(R6),B'00100000'                             0227 00297000
*           END;                                                   0228 00298000
*         RESPECIFY                                                0229 00299000
*          (CSD) BASED(CVTCSD);                                    0229 00300000
@RF00222 DS    0H                                                  0230 00301000
*         RESPECIFY                                                0230 00302000
*          (R7) UNRSTD;                                            0230 00303000
*                                                                  0230 00304000
*         /***********************************************************/ 00305000
*         /*                                                         */ 00306000
*         /* INITIALIZE THE LCCA ACR FIELDS OF THE RECOVERY CPU      */ 00307000
*         /*                                                         */ 00308000
*         /***********************************************************/ 00309000
*                                                                  0231 00310000
*         LCCARCPU=R10;             /* SET THE RECOVERING CPU FIELD     00311000
*                                      IN THE RECOVERY CPU'S LCCA    */ 00312000
         ST    R10,LCCARCPU(,R10)                                  0231 00313000
*         LCCADCPU=R11;             /* SET THE 'DEAD' CPU FIELD IN 0232 00314000
*                                      THE RECOVER CPU'S LCCA        */ 00315000
         ST    R11,LCCADCPU(,R10)                                  0232 00316000
*         LCCACRFL=LCCACRFL&'01'X;  /* CLEAR THE ACR FLAGS IN THE  0233 00317000
*                                      RECOVERY CPU LCCA             */ 00318000
         NI    LCCACRFL(R10),X'01'                                 0233 00319000
*         LCCAACR='1'B;             /* SET THE ACR IN PROGRESS FLAG     00320000
*                                      IN THE RECOVERY LCCA          */ 00321000
*                                                                  0234 00322000
         OI    LCCAACR(R10),B'10000000'                            0234 00323000
*         /***********************************************************/ 00324000
*         /*                                                         */ 00325000
*         /* INITIALIZE THE LCCA ACR FIELDS OF THE DEAD CPU'S LCCA   */ 00326000
*         /*                                                         */ 00327000
*         /***********************************************************/ 00328000
*                                                                  0235 00329000
*         R11->LCCARCPU=R10;        /* SET THE RECOVERING CPU FIELD     00330000
*                                      IN THE DEAD CPU'S LCCA        */ 00331000
         ST    R10,LCCARCPU(,R11)                                  0235 00332000
*         R11->LCCADCPU=R11;        /* SET THE 'DEAD' CPU FIELD IN 0236 00333000
*                                      THE DEAD CPU'S LCCA           */ 00334000
         ST    R11,LCCADCPU(,R11)                                  0236 00335000
*         R11->LCCACREX=LCCACREX;   /* PROPAGATE THE ENTRY FLAGS     */ 00336000
         MVC   LCCACREX(1,R11),LCCACREX(R10)                       0237 00337000
*         R11->LCCACRFL=LCCACRFL;   /* CLEAR THE ACR FLAGS IN THE  0238 00338000
*                                      DEAD CPU'S LCCA               */ 00339000
         MVC   LCCACRFL(1,R11),LCCACRFL(R10)                       0238 00340000
*         R11->LCCAACR='1'B;        /* SET ACR IN PROGRESS FLAG IN 0239 00341000
*                                      THE DEAD CPU'S LCCA           */ 00342000
*                                                                  0239 00343000
         OI    LCCAACR(R11),B'10000000'                            0239 00344000
*         /***********************************************************/ 00345000
*         /*                                                         */ 00346000
*         /* SAVE THE MACHINE CHECK LOGREC ERROR BUFFER PASSED AS    */ 00347000
*         /* INPUT                                                   */ 00348000
*         /*                                                         */ 00349000
*         /***********************************************************/ 00350000
*                                                                  0240 00351000
*         R11->LCCACPUS->WSACACR->ACRMCHEB=LCCACPUS->WSACACR->ACRENTR1; 00352000
*                                                                  0240 00353000
         L     @12,LCCACPUS(,R11)                                  0240 00354000
         L     @12,WSACACR(,@12)                                   0240 00355000
         L     @08,LCCACPUS(,R10)                                  0240 00356000
         L     @08,WSACACR(,@08)                                   0240 00357000
         L     @07,ACRENTR1(,@08)                                  0240 00358000
         ST    @07,ACRMCHEB(,@12)                                  0240 00359000
*         /***********************************************************/ 00360000
*         /*                                                         */ 00361000
*         /* OBTAIN THE DEAD CPU'S PSA ENVIRONMENT AT TIME OF ERROR  */ 00362000
*         /* AND SAVE IT IN THE CPU RELATED ACR WORK SAVE AREA OF THE*/ 00363000
*         /* DEAD CPU                                                */ 00364000
*         /*                                                         */ 00365000
*         /***********************************************************/ 00366000
*                                                                  0241 00367000
*         RESPECIFY                                                0241 00368000
*          (R13) RSTD;                                             0241 00369000
*         R13=WSACACR;              /* SET ACR SAVE AREA ADDRESS     */ 00370000
         LR    R13,@08                                             0242 00371000
*         CALL ACRPROSW;                                           0243 00372000
         BAL   @14,ACRPROSW                                        0243 00373000
*         RESPECIFY                                                0244 00374000
*          (R13) UNRSTD;                                           0244 00375000
*                                                                  0244 00376000
*         /***********************************************************/ 00377000
*         /*                                                         */ 00378000
*         /* SET THE RETURN PATH TO THE EXTERNAL FLIH OR SLIH        */ 00379000
*         /*                                                         */ 00380000
*         /***********************************************************/ 00381000
*                                                                  0245 00382000
*         LCCACREF='1'B;            /* SET THE EXTERNAL EXIT FLAG    */ 00383000
         OI    LCCACREF(R10),B'10000000'                           0245 00384000
*STERM:                                                            0246 00385000
*                                                                  0246 00386000
*         /***********************************************************/ 00387000
*         /*                                                         */ 00388000
*         /* THIS SECTION WILL ROUTE CONTROL TO THE SYSTEM           */ 00389000
*         /* TERMINATION ROUTINE IF ACR HAS DETECTED A CONDITION     */ 00390000
*         /* WHERE ANOTHER ACR PROCESS IS ALREADY IN PROGRESS WHEN AN*/ 00391000
*         /* MFA OR EMS WAS RECEIVED TO INITIATE ACR RECOVERY FOR A  */ 00392000
*         /* CPU.                                                    */ 00393000
*         /*                                                         */ 00394000
*         /***********************************************************/ 00395000
*                                                                  0246 00396000
*         IF LCCACREF='0'B THEN     /* IS THE EXTERNAL EXIT FLG SET  */ 00397000
STERM    TM    LCCACREF(R10),B'10000000'                           0246 00398000
         BNZ   @RF00246                                            0246 00399000
*           DO;                     /* EXTERNAL EXIT FLAG NOT SET    */ 00400000
*/* ******************************************************************/ 00401000
*/*                                                                  */ 00402000
*/* 01* SEGMENT-NAME  ACRSTERM                                       */ 00403000
*/*                                                                  */ 00404000
*/* 02*  FUNCTION:                                                   */ 00405000
*/*     ACR SYSTEM TERMINATION INTERFACE ROUTINE - PREPARES THE      */ 00406000
*/*     NECESSARY INTERFACE TO THE SYSTEM TERMINATION FACILITY.  IT  */ 00407000
*/*     BUILDS THE WTO MESSAGE AND A TWO WORD PARAMETER LIST.  WORD 1*/ 00408000
*/*     OF THE PARAMETER LIST CONTAINS THE REAL ADDRESS OF THE MSG   */ 00409000
*/*     AREA, WORD 2 CONTAINS THE REAL ADDRESS OF THE LOGREC BUFFER  */ 00410000
*/*     PASSED TO ACR BY RMS.  THIS ROUTINE WILL ALSO PLACE THE WAIT */ 00411000
*/*     STATE CODE OF 050 INTO THE LOGREC BUFFER TO BE USED BY THE   */ 00412000
*/*     SYSTEM TERMINATION FACILITY WAIT STATE.                      */ 00413000
*/*                                                                  */ 00414000
*/* 02* INPUT= RRPLPTR (REG 6) - RRPL PARAMETER LIST ADDRESS         */ 00415000
*/*            PSAPTR  (REG 4) - RECOVERY CPU PSA PTR                */ 00416000
*/*            PSAPTR2 (REG 9) - DEAD CPU PSA PTR                    */ 00417000
*/*            LCCAPTR (REG 10) - RECOVERY CPU LCCA ADDRESS          */ 00418000
*/*            LCCAPTR2 (REG 11) - DEAD CPU LCCA ADDRESS             */ 00419000
*/*                                                                  */ 00420000
*/* 02* USAGE= WPLPTR (REG 8),WRKPTR (REG 7)                         */ 00421000
*/*                                                                  */ 00422000
*/* 02* OUTPUT= SYSTEM TERMINATION PARAMETER LIST ADDRESS (REAL)HAS  */ 00423000
*/*             BEEN PLACED IN THE RRPL FOR THE EXIT PROCESSOR       */ 00424000
*/*                                                                  */ 00425000
*/* ******************************************************************/ 00426000
*                                                                  0248 00427000
*             RESPECIFY                                            0248 00428000
*              (R8,                                                0248 00429000
*               R7,                                                0248 00430000
*               R1,                                                0248 00431000
*               R2) RESTRICTED;                                    0248 00432000
*                                                                  0248 00433000
*             /*******************************************************/ 00434000
*             /*                                                     */ 00435000
*             /* SET WAIT STATE CODE 050 IN THE LOGREC BUFFER FOR    */ 00436000
*             /* SYSTERM                                             */ 00437000
*             /*                                                     */ 00438000
*             /*******************************************************/ 00439000
*                                                                  0249 00440000
*             R1=LCCACPUS->WSACACR->ACRENTR1;/* GET LOGREC BUFFER  0249 00441000
*                                      ADDR                          */ 00442000
         L     @12,LCCACPUS(,R10)                                  0249 00443000
         L     @12,WSACACR(,@12)                                   0249 00444000
         L     R1,ACRENTR1(,@12)                                   0249 00445000
*             LRBMWSC=COD050;       /* PLACE WAIT CODE IN LRB        */ 00446000
         MVC   LRBMWSC(4,R1),@CF02839                              0250 00447000
*             LRBHTYPE='81'X;       /* INDICATE SYSTEM TERMINATION 0251 00448000
*                                      RECORD TYPE           @YM04283*/ 00449000
*                                                                  0251 00450000
         MVI   LRBHTYPE(R1),X'81'                                  0251 00451000
*             /*******************************************************/ 00452000
*             /*                                                     */ 00453000
*             /* BUILD THE OUTPUT MESSAGE -- IEA856W                 */ 00454000
*             /*                                                     */ 00455000
*             /*******************************************************/ 00456000
*                                                                  0252 00457000
*             R8=R11->LCCACPUS->WSACACR;/* SET DEAD CPU ACRSA BASE   */ 00458000
         L     @12,LCCACPUS(,R11)                                  0252 00459000
         L     R8,WSACACR(,@12)                                    0252 00460000
*             R7=ADDR(R8->ACRACRWA);/* POINT TO PARM LIST AREA       */ 00461000
         LA    R7,ACRACRWA(,R8)                                    0253 00462000
*             R8->TXT856W(1:LENGTH(MSG856W))=MSG856W;/* MOVE MSG     */ 00463000
         MVC   TXT856W(49,R8),MSG856W                              0254 00464000
*             R2=PSASPAD;           /* GET DEAD CPU ADDRESS          */ 00465000
         LH    R2,PSASPAD(,R4)                                     0255 00466000
*             GENERATE REFS(R2,CVDDWD,R7);                         0256 00467000
    CVD R2,CVDDWD(R7)             CONVERT DEAD CPU ADDRESS              00468000
*             UNPK(R8->CPUADDRZ,CVDDWD(7:8));/* DEAD CPU ADDR TO MSG */ 00469000
         UNPK  CPUADDRZ(2,R8),CVDDWD+6(2,R7)                       0257 00470000
*             R8->CPUADDRZ(2)=R8->CPUADDRZ(2)|'F0'X;/* MAKE PRINTABLE*/ 00471000
*                                                                  0258 00472000
         OI    CPUADDRZ+1(R8),X'F0'                                0258 00473000
*             /*******************************************************/ 00474000
*             /*                                                     */ 00475000
*             /* BUILD SYSTERM PARAMETER LIST                        */ 00476000
*             /*                                                     */ 00477000
*             /*******************************************************/ 00478000
*                                                                  0259 00479000
*             GENERATE REFS(R8,TXT856W,R1);                        0259 00480000
        LRA R8,TXT856W(R8)       OBTAIN REAL ADDRESS OF MSG             00481000
        LRA R1,0(R1)             OBTAIN REAL LRB ADDRESS                00482000
*             STWPL=R8;             /* PLACE REAL MSG PTR IN LIST    */ 00483000
         ST    R8,STWPL(,R7)                                       0260 00484000
*             STLRB=R1;             /* PLACE REAL LRB PTR IN LIST    */ 00485000
         ST    R1,STLRB(,R7)                                       0261 00486000
*             RESPECIFY                                            0262 00487000
*              (R1) UNRSTD;                                        0262 00488000
*             GENERATE REFS(R7,STRMPARM);                          0263 00489000
        LRA R7,STRMPARM(R7)      OBTAIN REAL ADDRESS OF LIST            00490000
*             LCCACRST='1'B;        /* SET THE TERMINATION EXIT SW   */ 00491000
         OI    LCCACRST(R10),B'00000001'                           0264 00492000
*             LCCACPUS->WSACACR->ACRENTR1=R7;/* PLACE REAL PARAMETER    00493000
*                                      LIST ADDRESS IN RRPL FOR EXIT    00494000
*                                      PROCESSOR                     */ 00495000
         L     @12,LCCACPUS(,R10)                                  0265 00496000
         L     @12,WSACACR(,@12)                                   0265 00497000
         ST    R7,ACRENTR1(,@12)                                   0265 00498000
*             RESPECIFY                                            0266 00499000
*              (R8,                                                0266 00500000
*               R7,                                                0266 00501000
*               R2) UNRESTRICTED;                                  0266 00502000
*                                                                  0266 00503000
*/* ******************************************************************/ 00504000
*/*   END OF SEGMENT ACRSTERM                                        */ 00505000
*/* ******************************************************************/ 00506000
*                                                                  0267 00507000
*           END;                                                   0267 00508000
*                                                                  0267 00509000
*/* ******************************************************************/ 00510000
*/*   END OF SEGMENT ACRPREP                                         */ 00511000
*/* ******************************************************************/ 00512000
*                                                                  0268 00513000
*       END;                                                       0268 00514000
@RF00246 DS    0H                                                  0269 00515000
*     END;                                                         0269 00516000
*   ELSE                                                           0270 00517000
*                                                                  0270 00518000
*/* ******************************************************************/ 00519000
*/* ******************************************************************/ 00520000
*/* ****************    ACR INTERMEDIATE PROCESSING     **************/ 00521000
*/* ******************************************************************/ 00522000
*/* ******************************************************************/ 00523000
*                                                                  0270 00524000
*     DO;                           /* NOT SLIH ENTRY                */ 00525000
         B     @RC00188                                            0270 00526000
@RF00188 DS    0H                                                  0271 00527000
*       DO;                                                        0271 00528000
*                                                                  0271 00529000
*/* ******************************************************************/ 00530000
*/*                                                                  */ 00531000
*/* 01* SEGMENT NAME  ACRINIT2                                       */ 00532000
*/*                                                                  */ 00533000
*/* 02* FUNCTION                                                     */ 00534000
*/*                                                                  */ 00535000
*/*     ACR INITIALIZATION FOR ENTRIES TO THE INTERMEDIATE PHASE     */ 00536000
*/*                                                                  */ 00537000
*/* 02* INPUT= PSAPTR (REG 4) - CURRENT PSA ADDRESS (BASED ZERO)     */ 00538000
*/*            RRPLPTR (REG 6) - RECOVERY RTN PARM LIST ADDRESS      */ 00539000
*/*            LCCAPTR (REG 10) - CURRENT LCCA ADDRESS               */ 00540000
*/*                                                                  */ 00541000
*/* 02* OUTPUT= PSAPTR (REG 4) - CURRENT PSA ADDRESS (BASED ZERO)    */ 00542000
*/*            RRPLPTR (REG 6) - RECOVERY RTN PARM LIST ADDRESS      */ 00543000
*/*            PSAPTR2 (REG 9) - ADDR OF THE OTHER PSA               */ 00544000
*/*            LCCAPTR (REG 10) - CURRENT LCCA ADDRESS               */ 00545000
*/*            LCCAPTR2 (REG 11) - ADDR OF THE OTHER LCCA            */ 00546000
*/*                                                                  */ 00547000
*/* ******************************************************************/ 00548000
*                                                                  0272 00549000
*         IF R10=LCCADCPU THEN      /* IF THE DEAD CPU IN CONTROL    */ 00550000
         C     R10,LCCADCPU(,R10)                                  0272 00551000
         BNE   @RF00272                                            0272 00552000
*           R11=LCCARCPU;           /* SET OTHER LCCA BASE ADDRESS TO   00553000
*                                      THE RECOVERY LCCA ADDRESS     */ 00554000
         L     R11,LCCARCPU(,R10)                                  0273 00555000
*         ELSE                      /* RECOVERY CPU IS IN CONTROL    */ 00556000
*           R11=LCCADCPU;           /* SET OTHER LCCA BASE TO THE  0274 00557000
*                                      DEAD LCCA ADDRESS             */ 00558000
         B     @RC00272                                            0274 00559000
@RF00272 L     R11,LCCADCPU(,R10)                                  0274 00560000
*         R11->LCCACREX=LCCACREX;   /* PROPOGATE ENTRY FLAGS TO LCCA2*/ 00561000
@RC00272 MVC   LCCACREX(1,R11),LCCACREX(R10)                       0275 00562000
*         R9=                       /* OBTAIN OTHER CPU PHYSICAL ADDR*/ 00563000
*             R11->LCCACPUS->WSACACR->ACRCPUAD;                    0276 00564000
         L     @12,LCCACPUS(,R11)                                  0276 00565000
         L     @12,WSACACR(,@12)                                   0276 00566000
         LH    R9,ACRCPUAD(,@12)                                   0276 00567000
         N     R9,@CF03159                                         0276 00568000
*         R9=                       /* OBTAIN OTHER PSA VIRTUAL ADDR */ 00569000
*             PCCAT00P(R9+1)->PCCAPSAV;                            0277 00570000
*                                                                  0277 00571000
         LR    @12,R9                                              0277 00572000
         SLA   @12,2                                               0277 00573000
         L     @08,CVTPTR(,R4)                                     0277 00574000
         L     @08,CVTPCCAT(,@08)                                  0277 00575000
         L     @12,PCCAT00P(@12,@08)                               0277 00576000
         L     @12,PCCAPSAV(,@12)                                  0277 00577000
         LR    R9,@12                                              0277 00578000
*/* ******************************************************************/ 00579000
*/*  END OF INTERMEDIATE PHASE INITIALIZATION SEGMENT - ACRINIT2     */ 00580000
*/* ******************************************************************/ 00581000
*                                                                  0278 00582000
*       END;                                                       0278 00583000
*       IF LCCACRLM='1'B THEN       /* IS ENTRY FROM ,LCK MGR, OR A     00584000
*                                      GLOBAL SPIN ROUTINE?          */ 00585000
         TM    LCCACRLM(R10),B'00000100'                           0279 00586000
         BNO   @RF00279                                            0279 00587000
*         LCCACLMS='1'B;            /* SET THE CURRENT LCCA SUSPENDED*/ 00588000
         OI    LCCACLMS(R10),B'01000000'                           0280 00589000
*       IF LCCACRTM^='1'B THEN      /* IS ENTRY FIRST ENTRY TO THE 0281 00590000
*                                      ACR INTERMEDIATE PROCESSING 0281 00591000
*                                      PHASE.                        */ 00592000
@RF00279 TM    LCCACRTM(R10),B'10000000'                           0281 00593000
         BO    @RF00281                                            0281 00594000
*         DO;                       /* 1ST ENTRY TO INTERMEDIATE   0282 00595000
*                                      PROCESSING                    */ 00596000
*/* ******************************************************************/ 00597000
*/*                                                                  */ 00598000
*/* 01* SEGMENT-NAME  ACRRTM                                         */ 00599000
*/*                                                                  */ 00600000
*/* 02* FUNCTION                                                     */ 00601000
*/*     ACR RTM INTERFACE ROUTINE - ENTERED ONLY AFTER THE ACR       */ 00602000
*/*     PREPROCESSOR HAS RETURNED CONTROL TO THE RECOVERY CPU PROCESS*/ 00603000
*/*     INTERRUPTED BY THE MFA OR EMS, AND THAT PROCESS HAS REACHED A*/ 00604000
*/*     POINT WHERE IT IS RELINQUISHING CONTROL FOR THE FIRST TIME.  */ 00605000
*/*     THIS RELINQUISHING OF CONTROL, EITHER BY THE PROCESS ITSELF  */ 00606000
*/*     OR AS THE RESULT OF A SUPERVISORY ACTION, WILL CAUSE ACR TO  */ 00607000
*/*     BE RE-ENTERED IN ITS INTERMEDIATE PROCESSING PHASE TO INITIATE  00608000
*                                                                    */ 00609000
*/*     THE SOFTWARE RECOVERY PROCESS OF THE DEAD CPU WORK. THE FIRST*/ 00610000
*/*     ENTRY TO INTERMEDIATE ACR PROCESSING WILL INVOKE THIS ROUTINE*/ 00611000
*/*     WHICH WILL SWITCH THE DEAD CPU'S PROCESS UNDER CONTROL OF THE*/ 00612000
*/*     RECOVERY CPU, AND ROUTE THAT PROCESS TO RTM FOR RECOVERY.    */ 00613000
*/*                                                                  */ 00614000
*/* 02* INPUT= PSAPTR (REG 4) - ADDR OF CURR PSA BASED ZERO          */ 00615000
*/*            RRPLPTR (REG 6) - ADDR OF RECOVERY RTN PARAMETER LIST */ 00616000
*/*            PSAPTR2 (REG 9) - ADDR OF DEAD CPU PSA                */ 00617000
*/*            LCCAPTR (REG 10) - ADDR OF THE CURRENT LCCA           */ 00618000
*/*            LCCAPTR2 (REG 11) - ADDR OF THE DEAD LCCA             */ 00619000
*/*                                                                  */ 00620000
*/* 02* OUTPUT= LCCAPTR (REG 11) - CONTAINS THE ADDR OF THE NEW LCCA */ 00621000
*/*                                                                  */ 00622000
*/* ******************************************************************/ 00623000
*                                                                  0283 00624000
*           DO;                                                    0283 00625000
*                                                                  0283 00626000
*             /*******************************************************/ 00627000
*             /*                                                     */ 00628000
*             /* RECOVERY NOTE: POSSIBLE PHASE IDS AT THIS POINT ARE */ 00629000
*             /* (00,02). AN ERROR IN THIS PHASE IS RETRIED AT LABEL */ 00630000
*             /* RSTRT00.                                            */ 00631000
*             /*                                                     */ 00632000
*             /*******************************************************/ 00633000
*                                                                  0284 00634000
*             IF LOGPHID<'02'X THEN /* IS ENTRY DUE TO A RETRY FOR AN   00635000
*                                      ERROR IN THIS PHASE           */ 00636000
         CLI   LOGPHID(R6),X'02'                                   0284 00637000
         BNL   @RF00284                                            0284 00638000
*               RRPLFLGS='0'B;      /* NO, RESET ANY PREVIOUS PHASE     00639000
*                                      ERROR INDICATIONS             */ 00640000
         MVI   RRPLFLGS(R6),B'00000000'                            0285 00641000
*             LOGPHID='02'X;        /* SET THIS LOGICAL PHASE ID     */ 00642000
@RF00284 MVI   LOGPHID(R6),X'02'                                   0286 00643000
*           END;                                                   0287 00644000
*                                                                  0287 00645000
*           /*********************************************************/ 00646000
*           /*                                                       */ 00647000
*           /* SET THE RTM INVOKED INDICATION FOR LATER PROCESSING   */ 00648000
*           /*                                                       */ 00649000
*           /*********************************************************/ 00650000
*                                                                  0288 00651000
*           LCCACRTM='1'B;          /* SET THE RTM INVOKED SWITCH IN    00652000
*                                      BOTH THE RECOVERY,            */ 00653000
         OI    LCCACRTM(R10),B'10000000'                           0288 00654000
*           R11->LCCACRTM='1'B;     /* DEAD LCCAS.                   */ 00655000
*                                                                  0289 00656000
         OI    LCCACRTM(R11),B'10000000'                           0289 00657000
*           /*********************************************************/ 00658000
*           /*                                                       */ 00659000
*           /* PROCESS SWITCH TO THE DEAD CPU'S PROCESS              */ 00660000
*           /*                                                       */ 00661000
*           /*********************************************************/ 00662000
*                                                                  0290 00663000
*           RESPECIFY                                              0290 00664000
*            (R13) RSTD;                                           0290 00665000
*           R13=WSACACR;            /* SET BASE TO ACR WORK AREA     */ 00666000
         L     @08,LCCACPUS(,R10)                                  0291 00667000
         L     R13,WSACACR(,@08)                                   0291 00668000
*           CALL ACRPROSW;          /* PROCESS SWITCH SUBRTN         */ 00669000
         BAL   @14,ACRPROSW                                        0292 00670000
*           RESPECIFY                                              0293 00671000
*            (R13) UNRSTD;                                         0293 00672000
*                                                                  0293 00673000
*           /*********************************************************/ 00674000
*           /*                                                       */ 00675000
*           /* SEND THE DEAD PROCESS TO RTM FOR RECOVERY. THE        */ 00676000
*           /* RECOVERY ENVIRONMENT IS MADE TO LOOK AS IF A MACHINE  */ 00677000
*           /* CHECK HAS OCURRED BY ENTERING RTM WITH THE MCH STACK  */ 00678000
*           /* CURRENT, AND SET TO LOOK AS IF A MACHINE CHECK HAS    */ 00679000
*           /* OCURRED. MACHINE CHECKS ARE DISABLED TEMPORARILY      */ 00680000
*           /* ACROSS THE RTM HARDWARE RECOVERY PROCESS. RTM WILL    */ 00681000
*           /* RETURN TO THIS ROUTINE FOR MACHINE CHECK ENABLEMENT   */ 00682000
*           /* BEFORE STARTING SOFTWARE RECOVERY. DISABLE MACHINE    */ 00683000
*           /* CHECKS                                                */ 00684000
*           /*                                                       */ 00685000
*           /*********************************************************/ 00686000
*                                                                  0294 00687000
*           GENERATE REFS(MCDSPSW);                                0294 00688000
        LPSW  MCDSPSW                     DISABLE MACHINE CHECKS        00689000
*MCDSABL:                           /* LPSW RETURN POINT             */ 00690000
*           PSAACR='0'B;            /* REMOVE PROTECTION DURING RTM     00691000
*                                      MCHK PROCESSING               */ 00692000
MCDSABL  NI    PSAACR(R4),B'11111011'                              0295 00693000
*           RESPECIFY                                              0296 00694000
*            (R1,                                                  0296 00695000
*             R13) RSTD;                                           0296 00696000
*           R1=ACRMCHEB;            /* INSURE LOGREC BUFFER ADDRESS  */ 00697000
         L     @12,LCCACPUS(,R10)                                  0297 00698000
         L     @12,WSACACR(,@12)                                   0297 00699000
         L     R1,ACRMCHEB(,@12)                                   0297 00700000
*           R13=ADDR(ACRACRSV);     /* PASS SAVE AREA ADDRESS        */ 00701000
*                                                                  0298 00702000
         LA    R13,ACRACRSV(,@12)                                  0298 00703000
*           /*********************************************************/ 00704000
*           /*                                                       */ 00705000
*           /* ADJUST THE FRR STACK TO SIMULATE MCH IN CONTROL       */ 00706000
*           /*                                                       */ 00707000
*           /*********************************************************/ 00708000
*                                                                  0299 00709000
*           IF PSACSTK^=PSAMSTK     /* IF MCH STACK NOT ALREADY THE     00710000
*                                      CURRENT STACK         @ZA15778*/ 00711000
*             THEN                                                 0299 00712000
         L     @12,PSACSTK(,R4)                                    0299 00713000
         L     @08,PSAMSTK(,R4)                                    0299 00714000
         CR    @12,@08                                             0299 00715000
         BE    @RF00299                                            0299 00716000
*             DO;                   /*                       @ZA15778*/ 00717000
*               PSAMSAV=PSACSTK;    /* PLACE CURRENT STACK AT TIME OF   00718000
*                                      ERROR AS THE INTERRUPTED STACK   00719000
*                                      OF MCH.                       */ 00720000
         ST    @12,PSAMSAV(,R4)                                    0301 00721000
*               PSACSTK=PSAMSTK;    /* SET THE CURRENT STACK TO THE     00722000
*                                      MCH STACK                     */ 00723000
         ST    @08,PSACSTK(,R4)                                    0302 00724000
*             END;                  /*                       @ZA15778*/ 00725000
*           ELSE                                                   0304 00726000
*             ;                     /* IF MCH ALREADY CURRENT LEAVE     00727000
*                                      STACK PTRS ALONE      @ZA15778*/ 00728000
*                                                                  0304 00729000
@RF00299 DS    0H                                                  0305 00730000
*           /*********************************************************/ 00731000
*           /*                                                       */ 00732000
*           /* LOAD FLOATING POINT REGS PRIOR TO GOING TO RTM        */ 00733000
*           /*                                                       */ 00734000
*           /*********************************************************/ 00735000
*                                                                  0305 00736000
*           GEN REFS(LOGFLOAT);                                    0305 00737000
*                                                                  0305 00738000
        LD 0,LOGFLOAT(R1)                                               00739000
        LD 2,LOGFLOAT+8(R1)                                             00740000
        LD 4,LOGFLOAT+16(R1)                                            00741000
        LD 6,LOGFLOAT+24(R1)                                            00742000
*           /*********************************************************/ 00743000
*           /*                                                       */ 00744000
*           /* ROUTE CONTROL TO RTM FOR HARDWARE RECORDING AND       */ 00745000
*           /* RECOVERY                                              */ 00746000
*           /*                                                       */ 00747000
*           /*********************************************************/ 00748000
*                                                                  0306 00749000
*           GENERATE;                                              0306 00750000
        CALLRTM TYPE=MACHCK             INITIATE HARDWARE RECOVERY      00751000
*           RESPECIFY                                              0307 00752000
*            (R13) UNRESTRICTED;                                   0307 00753000
*                                                                  0307 00754000
*           /*********************************************************/ 00755000
*           /*                                                       */ 00756000
*           /* RTM WILL RETURN TO ACR AT THIS POINT TO ALLOW ENABLING*/ 00757000
*           /* OF MACHINE CHECKS, AND WILL PASS IN REG 1 A POINTER TO*/ 00758000
*           /* AN AREA WHICH CONTAINS THE REGISTERS AND PSW TO BE    */ 00759000
*           /* LOADED TO RE-ENTER RTM FOR IT TO CONTINUE FURTHER     */ 00760000
*           /* SOFTWARE RECOVERY (FRR PROCESSING). RE-ESTABLISH THE  */ 00761000
*           /* STACK AT TIME OF ERROR AS CURRENT                     */ 00762000
*           /*                                                       */ 00763000
*           /*********************************************************/ 00764000
*                                                                  0308 00765000
*           PSACSTK=PSAMSAV;        /* RESTORE CURRENT STACK         */ 00766000
         L     @12,PSAMSAV(,R4)                                    0308 00767000
         ST    @12,PSACSTK(,R4)                                    0308 00768000
*           IF RRPLRTRY='0'B        /* IS ENTRY DUE TO A RETRY?      */ 00769000
*             THEN                  /* NOT RETRY                     */ 00770000
         TM    RRPLRTRY(R6),B'10000000'                            0309 00771000
         BNZ   @RF00309                                            0309 00772000
*             RRPLFLGS=''B;         /* CLEAR ANY PREVIOUS ERROR    0310 00773000
*                                      INDICATIONS                   */ 00774000
         MVI   RRPLFLGS(R6),X'00'                                  0310 00775000
*           LOGPHID='06'X;          /* SET THIS LOGICAL PHASE ID     */ 00776000
@RF00309 MVI   LOGPHID(R6),X'06'                                   0311 00777000
*           ACRENTR1=R1;            /* SAVE REG 1 FOR RTM RETURN USED   00778000
*                                      IN EXIT SEG.                  */ 00779000
         L     @12,LCCACPUS(,R10)                                  0312 00780000
         L     @12,WSACACR(,@12)                                   0312 00781000
         ST    R1,ACRENTR1(,@12)                                   0312 00782000
*           GEN REFS(RRPLREGS)(STM 0,14,RRPLREGS(R6));/* SAVE REGS 0313 00783000
*                                      FOR RECURSION.                */ 00784000
         STM 0,14,RRPLREGS(R6)                                          00785000
*           PSAACR='1'B;            /* SET PROTECTION AGAIN BEFORE 0314 00786000
*                                      ENABLING FOR MACHINE CHECKS   */ 00787000
         OI    PSAACR(R4),B'00000100'                              0314 00788000
*RSTRT06:                           /* REENTRY POINT FROM MACH CHK 0315 00789000
*                                      ENABLEMENT OR FRR RETRY       */ 00790000
*                                                                  0315 00791000
*           /*********************************************************/ 00792000
*           /*                                                       */ 00793000
*           /* ENABLE MACHINE CHECKS                                 */ 00794000
*           /*                                                       */ 00795000
*           /*********************************************************/ 00796000
*                                                                  0315 00797000
*           LPSW(MCENPSW);                                         0315 00798000
RSTRT06  LPSW  MCENPSW                                             0315 00799000
*MCENABL:                           /* LPSW RETURN POINT             */ 00800000
*           LCCACRRT='1'B;          /* SET THE RTM EXIT INDICATOR    */ 00801000
MCENABL  OI    LCCACRRT(R10),B'00010000'                           0316 00802000
*           RESPECIFY                                              0317 00803000
*            (R1) UNRESTRICTED;                                    0317 00804000
*                                                                  0317 00805000
*/* ******************************************************************/ 00806000
*/*  END OF SEGMENT ACRRTM                                           */ 00807000
*/* ******************************************************************/ 00808000
*                                                                  0318 00809000
*         END;                                                     0318 00810000
*       ELSE                        /* NOT INITIAL INTERMEDIATE PHASE   00811000
*                                      ENTRY CHECK FOR SUSPENSION    */ 00812000
*         IF                        /* IS THE OTHER PROCESS SUSPENDED*/ 00813000
*             R11->LCCACLMS='1'B THEN/* YES, SETLOCK INTERFACE     0319 00814000
*                                      ROUTINE                       */ 00815000
         B     @RC00281                                            0319 00816000
@RF00281 TM    LCCACLMS(R11),B'01000000'                           0319 00817000
         BNO   @RF00319                                            0319 00818000
*           DO;                     /* OTHER PROCESS IS SUSPENDED    */ 00819000
*/* ******************************************************************/ 00820000
*/*                                                                  */ 00821000
*/* 01* SEGMENT-NAME  ACRLKSPI                                       */ 00822000
*/*                                                                  */ 00823000
*/* 02* FUNCTION                                                     */ 00824000
*/*     ACR LOCK MANAGER-GLOBAL SPIN INTERFACE ROUTINE-- THIS        */ 00825000
*/*     ROUTINE IS ENTERED WHEN THE LOCK MANAGER HAS ENCOUNTERED A   */ 00826000
*/*     LOCK CONFLICT, OR WHEN AUTHORIZED GLOBAL SPIN ROUTINES HAVE  */ 00827000
*/*     DISCOVERED A DEPENDENCY ON THE DEAD CPU.  THIS ROUTINE WILL  */ 00828000
*/*     CAUSE A SWITCH OF PROCESSES TO THE OTHER PROCESS TO REMOVE   */ 00829000
*/*     THE CONFLICT OR DEPENDENCY.  THIS ROUTINE IS ALSO ENTERED    */ 00830000
*/*     IF A PROCESS ENTERS ACR VIA THE DISPATCHER, AND THE OTHER    */ 00831000
*/*     PROCESS IS IN A SUSPENDED STATE AS DESCRIBED IN THE PREVIOUS */ 00832000
*/*     SENTENCE.  THIS ROUTINE CONTROLS THE PING-PONG ACTION OF ACR */ 00833000
*/*     IN RESOLVING CONFLICTS BETWEEN THE TWO PROCESSES.            */ 00834000
*/*                                                                  */ 00835000
*/* 02* INPUT= PSAPTR (REG 4) - ADDR OF CURR PSA BASED ZERO          */ 00836000
*/*            RRPLPTR (REG 6) - ADDR OF RECOVERY RTN PARAMETER LIST */ 00837000
*/*            PSAPTR2 (REG 9) - ADDR OF THE OTHER CPU PSA           */ 00838000
*/*            LCCAPTR (REG 10) - ADDR OF THE CURRENT LCCA           */ 00839000
*/*            LCCAPTR2 (REG 11) - ADDR OF THE OTHER LCCA            */ 00840000
*/*                                                                  */ 00841000
*/* 02* OUTPUT= LCCAPTR (REG 10) - CONTAINS THE ADDR OF THE NEW LCCA */ 00842000
*/*                                                                  */ 00843000
*/* ******************************************************************/ 00844000
*                                                                  0321 00845000
*             DO;                                                  0321 00846000
*                                                                  0321 00847000
*               /*****************************************************/ 00848000
*               /*                                                   */ 00849000
*               /* RECOVERY NOTE: POSSIBLE PHASE IDS AT THIS POINT   */ 00850000
*               /* ARE (00,03). AN ERROR IN THIS PHASE IS RETRIED AT */ 00851000
*               /* LABEL RSTRT00.                                    */ 00852000
*               /*                                                   */ 00853000
*               /*****************************************************/ 00854000
*                                                                  0322 00855000
*               IF LOGPHID<'03'X THEN/* IS ENTRY DUE TO A RETRY FOR     00856000
*                                      AN ERROR IN THIS PHASE        */ 00857000
         CLI   LOGPHID(R6),X'03'                                   0322 00858000
         BNL   @RF00322                                            0322 00859000
*                 RRPLFLGS='0'B;    /* NO, RESET ANY PREVIOUS PHASE     00860000
*                                      ERROR INDICATIONS             */ 00861000
         MVI   RRPLFLGS(R6),B'00000000'                            0323 00862000
*               LOGPHID='03'X;      /* SET THIS LOGICAL PHASE ID     */ 00863000
@RF00322 MVI   LOGPHID(R6),X'03'                                   0324 00864000
*             END;                                                 0325 00865000
*                                                                  0325 00866000
*             /*******************************************************/ 00867000
*             /*                                                     */ 00868000
*             /* SWITCH TO THE OTHER CPUS PROCESSING                 */ 00869000
*             /*                                                     */ 00870000
*             /*******************************************************/ 00871000
*                                                                  0326 00872000
*             RESPECIFY                                            0326 00873000
*              (R13) RSTD;                                         0326 00874000
*             R13=WSACACR;          /* SET BASE TO ACR WORK AREA     */ 00875000
         L     @08,LCCACPUS(,R10)                                  0327 00876000
         L     R13,WSACACR(,@08)                                   0327 00877000
*             CALL ACRPROSW;        /* SWITCH TO SUSPENDED PROCESS   */ 00878000
         BAL   @14,ACRPROSW                                        0328 00879000
*             RESPECIFY                                            0329 00880000
*              (R13) UNRSTD;                                       0329 00881000
*             GEN REFS(RRPLREGS)(STM 0,14,RRPLREGS(R6));/* SAVE REGS    00882000
*                                      FOR RECURSION                 */ 00883000
         STM 0,14,RRPLREGS(R6)                                          00884000
*RSTRT05:                           /* REENTRY POINT FOR RECURSION   */ 00885000
*             IF LOGPHID^='05'X THEN/* IS THIS A RETRY?              */ 00886000
RSTRT05  CLI   LOGPHID(R6),X'05'                                   0331 00887000
         BE    @RF00331                                            0331 00888000
*               RRPLFLGS='0'B;      /* RESET ANY FLGS FROM PREVIOUS     00889000
*                                      PHASE                         */ 00890000
         MVI   RRPLFLGS(R6),B'00000000'                            0332 00891000
*             LOGPHID='05'X;        /* SET LOGICAL PHASEID 05 FOR FRR*/ 00892000
@RF00331 MVI   LOGPHID(R6),X'05'                                   0333 00893000
*             LCCACLMS='0'B;        /* RESET THE NEW LCCA SUSPEND BIT*/ 00894000
         NI    LCCACLMS(R10),B'10111111'                           0334 00895000
*             LCCACRLE='1'B;        /* SET THE LOCK & SPIN EXIT FLAG */ 00896000
         OI    LCCACRLE(R10),B'00100000'                           0335 00897000
*/* ******************************************************************/ 00898000
*/*  END OF SEGMENT ACRLKSPI                                         */ 00899000
*/* ******************************************************************/ 00900000
*                                                                  0336 00901000
*           END;                                                   0336 00902000
*       IF LOGPHID='00'X THEN       /* IF THE LOGICAL PHASE ID IS  0337 00903000
*                                      STILL X'00' AT THIS POINT,  0337 00904000
*                                      THEN BOTH PROCESSES ARE AT A     00905000
*                                      LOGICALLY ENABLED STATE AND 0337 00906000
*                                      THE POST PROCESS PHASE IS   0337 00907000
*                                      ENTERED.                      */ 00908000
@RF00319 DS    0H                                                  0337 00909000
@RC00281 CLI   LOGPHID(R6),X'00'                                   0337 00910000
         BNE   @RF00337                                            0337 00911000
*         DO;                       /* BOTH CPUS ARE ENABLED         */ 00912000
*/* ******************************************************************/ 00913000
*/* ******************************************************************/ 00914000
*/* ****************       ACR POST PROCESSING        ****************/ 00915000
*/* ******************************************************************/ 00916000
*/* ******************************************************************/ 00917000
*                                                                  0339 00918000
*           DO;                                                    0339 00919000
*                                                                  0339 00920000
*             /*******************************************************/ 00921000
*             /*                                                     */ 00922000
*             /* INSURE THE RECOVERY CPU IS IN CONTROL BEFORE        */ 00923000
*             /* ENTERING THE POST PROCESSOR SEGMENT                 */ 00924000
*             /*                                                     */ 00925000
*             /*******************************************************/ 00926000
*                                                                  0340 00927000
*             IF R10^=LCCARCPU THEN /* IS THE REC CPU IN CONTROL?    */ 00928000
         C     R10,LCCARCPU(,R10)                                  0340 00929000
         BE    @RF00340                                            0340 00930000
*               DO;                 /* DEAD CPU IN CONTROL           */ 00931000
*                 GEN REFS(RRPLREGS,R6)(STM 0,14,RRPLREGS(R6));/*  0342 00932000
*                                      SAVE REGS FOR RECURSION       */ 00933000
         STM 0,14,RRPLREGS(R6)                                          00934000
*                 RRPLFLGS='0'B;    /* CLEAR ERROR FLAGS             */ 00935000
         MVI   RRPLFLGS(R6),B'00000000'                            0343 00936000
*RSTRT04:                                                          0344 00937000
*                 LOGPHID='04'X;    /* SET LOGICAL PHASE ID FOR    0344 00938000
*                                      RECOVERY RTN                  */ 00939000
RSTRT04  MVI   LOGPHID(R6),X'04'                                   0344 00940000
*                 RESPECIFY                                        0345 00941000
*                  (R13) RSTD;                                     0345 00942000
*                 R13=WSACACR;      /* SET BASE TO ACR WORK SAVE AREA*/ 00943000
         L     @08,LCCACPUS(,R10)                                  0346 00944000
         L     R13,WSACACR(,@08)                                   0346 00945000
*                 CALL ACRPROSW;    /* MAKE RECOVERY CPU CURRENT     */ 00946000
         BAL   @14,ACRPROSW                                        0347 00947000
*                 RESPECIFY                                        0348 00948000
*                  (R13) UNRSTD;                                   0348 00949000
*               END;                                               0349 00950000
*           END;                                                   0350 00951000
@RF00340 DS    0H                                                  0351 00952000
*           DO;                                                    0351 00953000
*                                                                  0351 00954000
*/* ******************************************************************/ 00955000
*/*                                                                  */ 00956000
*/* 01* SEGMENT NAME  ACRPOSTP                                       */ 00957000
*/*                                                                  */ 00958000
*/* 02* FUNCTION                                                     */ 00959000
*/*     THE ACR POST PROCESSOR IS ENTERED ONLY WHEN BOTH CPUS HAVE   */ 00960000
*/*     ATTAINED THE LOGICALLY ENABLED STATE,(BOTH DISPATCHABLE, NO  */ 00961000
*/*     GLOBAL LOCKS HELD,AND NON SRB MODE). THIS PHASE OF PROCESSING*/ 00962000
*/*     WILL ALLOW THE RECOVERY CPU TO CLEAN UP THE SYSTEM SO IT CAN */ 00963000
*/*     RESUME NORMAL DISPATCHING WITHOUT THE USE OF THE DEAD CPU.   */ 00964000
*/*                                                                  */ 00965000
*/* 02* INPUT= PSAPTR (REG 4) - ADDR OF CURR PSA BASED ZERO          */ 00966000
*/*            RRPLPTR (REG 6) - ADDR OF RECOVERY RTN PARAMETER LIST */ 00967000
*/*            LCCAPTR (REG 10) - ADDR OF THE CURRENT LCCA           */ 00968000
*/*                                                                  */ 00969000
*/* 02* OUTPUT= SAME AS INPUT                                        */ 00970000
*/*                                                                  */ 00971000
*/* ******************************************************************/ 00972000
*                                                                  0352 00973000
*             RFY                                                  0352 00974000
*              (R3,                                                0352 00975000
*               R2,                                                0352 00976000
*               R1) RSTD;                                          0352 00977000
*                                                                  0352 00978000
*             /*******************************************************/ 00979000
*             /*                                                     */ 00980000
*             /* SET UP FRR PROTECTION FOR THE POST-PROCESSING PHASE */ 00981000
*             /*                                                     */ 00982000
*             /*******************************************************/ 00983000
*                                                                  0353 00984000
*             R3=ADDR(IEAVTCR1);                                   0353 00985000
         L     R3,@CA02845                                         0353 00986000
*             GEN(SETFRR A,FRRAD=(R3),WRKREGS=(R1,R2));            0354 00987000
         SETFRR A,FRRAD=(R3),WRKREGS=(R1,R2)                            00988000
*             RFY                                                  0355 00989000
*              (R3,                                                0355 00990000
*               R2,                                                0355 00991000
*               R1) UNRSTD;                                        0355 00992000
*             PSAACR='0'B;          /* TURN OFF SUPER BIT AND BE   0356 00993000
*                                      PROTECTED BY CONVENTIONAL FRR */ 00994000
         NI    PSAACR(R4),B'11111011'                              0356 00995000
*             GEN REFS(RRPLREGS)(STM 0,14,RRPLREGS(R6));/* SAVE    0357 00996000
*                                                            @YM08185   00997000
*                                      REGS FOR RECURSION.   @YM08185*/ 00998000
         STM 0,14,RRPLREGS(R6)                                          00999000
*             RRPLFLGS='0'B;        /* RESET ANY PREVIOUS PHASE ERROR   01000000
*                                      INDICATORS                    */ 01001000
         MVI   RRPLFLGS(R6),B'00000000'                            0358 01002000
*RSTRT07:                           /* FRR RETRY ENTRY PT            */ 01003000
*             IF RRPLRTRY='0'B THEN /* IS ENTRY DUE TO A RETRY?      */ 01004000
RSTRT07  TM    RRPLRTRY(R6),B'10000000'                            0359 01005000
         BNZ   @RF00359                                            0359 01006000
*               LOGPHID='07'X;      /* YES, SET PHASE ID 07          */ 01007000
*                                                                  0360 01008000
         MVI   LOGPHID(R6),X'07'                                   0360 01009000
*             /*******************************************************/ 01010000
*             /*                                                     */ 01011000
*             /* SET REGISTER 13 TO THE ADDR OF AN 18 WORD SAVE AREA */ 01012000
*             /* FOR USE BY ROUTINES CALLED BY THE ACR POST PROCESSOR*/ 01013000
*             /*                                                     */ 01014000
*             /*******************************************************/ 01015000
*                                                                  0361 01016000
*             RESPECIFY                                            0361 01017000
*              (R13) RSTD;                                         0361 01018000
@RF00359 DS    0H                                                  0362 01019000
*             R13=ADDR(ACRACRSV);   /* SAVE AREA ADDR IN CURR ACRSA  */ 01020000
*                                                                  0362 01021000
         L     @08,LCCACPUS(,R10)                                  0362 01022000
         L     R13,WSACACR(,@08)                                   0362 01023000
         LA    R13,ACRACRSV(,R13)                                  0362 01024000
*             /*******************************************************/ 01025000
*             /*                                                     */ 01026000
*             /* SEE IF VARY CPU WAS TAKING CHANNELS OFF THE RECOVERY*/ 01027000
*             /* CPU                                                 */ 01028000
*             /*                                                     */ 01029000
*             /*******************************************************/ 01030000
*                                                                  0363 01031000
*             DO;                                                  0363 01032000
*                                                                  0363 01033000
*/* ******************************************************************/ 01034000
*/*                                                                  */ 01035000
*/* 01* SEGMENT NAME  ACRVARY                                        */ 01036000
*/*                                                                  */ 01037000
*/* 02* FUNCTION                                                     */ 01038000
*/*     THIS SEGMENT TESTS TO SEE IF VARY CPU OFFLINE                */ 01039000
*/*     WAS IN THE PROCESS OF MARKING CHANNELS OFFLINE ON THE        */ 01040000
*/*     RECOVERY CPU WHEN THE MALFUNCTION OCCURRED ON THE CPU        */ 01041000
*/*     WHICH IS NOW DEAD.  IF THIS WAS THE CASE, ACR WILL           */ 01042000
*/*     INTERFACE TO THE VARY WAKE UP ROUTINE TO INSURE THAT         */ 01043000
*/*     THE RECOVERY CPU HAS ITS CHANNELS ONLINE BEFORE GOING        */ 01044000
*/*     TO I/O RESTART.                                              */ 01045000
*/*                                                                  */ 01046000
*/* 02* INPUT= PSAPTR (REG 4) - ADDR OF CURR PSA BASED ZERO          */ 01047000
*/*            RRPLPTR (REG 6) - ADDR OF RECOVERY RTN PARAMETER LIST */ 01048000
*/*            LCCAPTR (REG 10) - ADDR OF THE CURRENT LCCA           */ 01049000
*/*            R13 (REG 13) - ADDR OF AN 18 WORD SAVE AREA           */ 01050000
*/*                                                                  */ 01051000
*/* 02* OUTPUT= SAME AS INPUT                                        */ 01052000
*/*                                                                  */ 01053000
*/* ******************************************************************/ 01054000
*                                                                  0364 01055000
*               IF LCCAVARY='1'B THEN/* WAS VARY MARKING CHANNELS  0364 01056000
*                                      OFFLINE ON THE RECOVERY CPU   */ 01057000
         TM    LCCAVARY(R10),B'00000001'                           0364 01058000
         BNO   @RF00364                                            0364 01059000
*                 DO;               /* YES                           */ 01060000
*                   RESPECIFY                                      0366 01061000
*                    (R15) RSTD;                                   0366 01062000
*                   R15=CVTVACR;    /* OBTAIN WAKE UP RTN ADDRESS    */ 01063000
         L     @12,CVTPTR(,R4)                                     0367 01064000
         L     R15,CVTVACR(,@12)                                   0367 01065000
*                   GENERATE REFS(R0,R14,R13,R15);                 0368 01066000
              STM R0,R15,0(R13)   SAVE REGS ACROSS WAKE UP ROUTINE      01067000
              BALR R14,R15        BRANCH TO WAKE UP RTN                 01068000
              LM R0,R15,0(R13)     RESTORE REGS                         01069000
*                   RESPECIFY                                      0369 01070000
*                    (R15) UNRSTD;                                 0369 01071000
*                 END;                                             0370 01072000
*                                                                  0370 01073000
