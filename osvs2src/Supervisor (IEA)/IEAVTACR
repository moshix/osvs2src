*/* ******************************************************************/ 01562000
*/*                                                                  */ 01563000
*/* 01* SEGMENT-NAME  ACRTRMEX                                       */ 01564000
*/*                                                                  */ 01565000
*/* 02* FUNCTION                                                     */ 01566000
*/*     THIS SEGMENT PASSES CONTROL TO THE SYSTEM TERMINATION        */ 01567000
*/*     FACILITY BY EXECUTING A LPSW WITH THE FOLLOWING ATTRIBUTES. -*/ 01568000
*/*     DISABLED, KEY 0, DAT OFF, EC MODE, SUPERVISOR STATE.         */ 01569000
*/*     REGISTER 1 CONTAINS THE REAL ADDRESS OF A 2 WORD PARAMETER   */ 01570000
*/*     LIST WHICH CONTAINS THE REAL ADDRESS OF A MESSAGE BUFFER IN  */ 01571000
*/*     THE FIRST WORD, AND THE REAL ADDRESS OF THE LOGREC BUFFER IN */ 01572000
*/*     THE SECOND WORD.                                             */ 01573000
*/*                                                                  */ 01574000
*/* 02* INPUT= REGISTER 1 - ADDR OF SYSTERM PARAMETER LIST (REAL)    */ 01575000
*/*                                                                  */ 01576000
*/* 02* OUTPUT= REGISTER 1 - PASSED TO SYSTEM TERMINATION (IGFPTERM) */ 01577000
*/*                                                                  */ 01578000
*/* ******************************************************************/ 01579000
*                                                                  0465 01580000
*                       GENERATE REFS(TRMPSW);                     0465 01581000
*                                                                  0465 01582000
        LPSW  TRMPSW                    GO TO SYSTEM TERMINATION        01583000
*/* ******************************************************************/ 01584000
*/*   END OF SEGMENT ACRTERMEX                                       */ 01585000
*/* ******************************************************************/ 01586000
*                                                                  0466 01587000
*                     END;                                         0466 01588000
*                   END;                                           0467 01589000
*                 ELSE              /* RTM EXIT SET RETURN TO RTM    */ 01590000
*                   DO;                                            0468 01591000
         B     @RC00460                                            0468 01592000
@RF00460 DS    0H                                                  0469 01593000
*                     R1=ACRENTR1;  /* GET ADDRESS OF RTM REENTRY PSW*/ 01594000
         L     @15,LCCACPUS(,R10)                                  0469 01595000
         L     @15,WSACACR(,@15)                                   0469 01596000
         L     R1,ACRENTR1(,@15)                                   0469 01597000
*                     PSAPSWSV=R1->RTPSW;/* PLACE PASSED PSW IN PSA     01598000
*                                      FOR LPSW                      */ 01599000
         MVC   PSAPSWSV(8,R4),RTPSW(R1)                            0470 01600000
*                     PSAACR='0'B;  /* TURN OFF SUPER BIT            */ 01601000
         NI    PSAACR(R4),B'11111011'                              0471 01602000
*                     GENERATE REFS(R1,R0,R15,PSAPSWSV);           0472 01603000
                           LM R0,R15,0(R1)  LOAD PASSED REGISTERS       01604000
                           LPSW PSAPSWSV    RETURN TO RTM               01605000
*                   END;                                           0473 01606000
*               END;                                               0474 01607000
*             ELSE                                                 0475 01608000
*               DO;                                                0475 01609000
         B     @RC00458                                            0475 01610000
