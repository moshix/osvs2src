*           DO;                                                    0481 01625000
         B     @RC00456                                            0481 01626000
@RF00456 DS    0H                                                  0482 01627000
*             R14=CVT0DS;           /* YES,SET UP DISPATCHER RETRN   */ 01628000
         L     @12,CVTPTR(,R4)                                     0482 01629000
         L     R14,CVT0DS(,@12)                                    0482 01630000
*             PSAANEW=0;            /* FORCE DISPATCHER TO NEW SEARCH*/ 01631000
         SLR   @15,@15                                             0483 01632000
         ST    @15,PSAANEW(,R4)                                    0483 01633000
*             GEN(SETFRR D,WRKREGS=(R1,R2));/* DELETE FRR            */ 01634000
         SETFRR D,WRKREGS=(R1,R2)                                       01635000
*             GOTO R14;             /* GO TO DISPATCHER              */ 01636000
         BR    R14                                                 0485 01637000
*           END;                                                   0486 01638000
*       END;                                                       0487 01639000
*     ELSE                          /* YES, LOCK MGR-SPIN EXIT       */ 01640000
*       DO;                                                        0488 01641000
         B     @RC00454                                            0488 01642000
@RF00454 DS    0H                                                  0489 01643000
*         R14=LCCACPUS->WSACACR->ACRRTNPT;/* PICK UP RESUME POINT  0489 01644000
*                                      FROM NOW CURRENT CPU'S ACRSA  */ 01645000
         L     @12,LCCACPUS(,R10)                                  0489 01646000
         L     @12,WSACACR(,@12)                                   0489 01647000
         L     R14,ACRRTNPT(,@12)                                  0489 01648000
*         PSAACR='0'B;              /* TURN OFF SUPER BIT            */ 01649000
         NI    PSAACR(R4),B'11111011'                              0490 01650000
*         RETURN;                   /* RETURN TO THE LOCK MGR OR   0491 01651000
*                                      GLOBAL SPIN ROUTINE, OR THE 0491 01652000
*                                      DISPATCHER                    */ 01653000
@EL00001 DS    0H                                                  0491 01654000
@EF00001 DS    0H                                                  0491 01655000
@ER00001 BR    @14                                                 0491 01656000
*       END;                                                       0492 01657000
*     RESPECIFY                                                    0493 01658000
*      (R1,                                                        0493 01659000
*       R2,                                                        0493 01660000
*       R14) UNRESTRICTED;                                         0493 01661000
*                                                                  0493 01662000
@RC00454 DS    0H                                                  0494 01663000
*/* ******************************************************************/ 01664000
*/*  END OF SEGMENT ACREXPR                                          */ 01665000
*/* ******************************************************************/ 01666000
*                                                                  0494 01667000
*   END;                                                           0494 01668000
*                                                                  0495 01669000
*/* ******************************************************************/ 01670000
*/* ******************************************************************/ 01671000
*/* ****************       ACR PROCESS SWITCHING      ****************/ 01672000
*/* ******************************************************************/ 01673000
*/* ******************************************************************/ 01674000
*                                                                  0495 01675000
*ACRPROSW:                                                         0495 01676000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0495 01677000
*                                                                  0495 01678000
         B     @EL00001                                            0495 01679000
ACRPROSW DS    0H                                                  0496 01680000
*/* ******************************************************************/ 01681000
*/*                                                                  */ 01682000
*/* 01* SEGMENT NAME  ACRPROSW                                       */ 01683000
*/*                                                                  */ 01684000
*/* 02* FUNCTION   ACR PROCESS SWITCH ROUTINE                        */ 01685000
*/*     THE ACR PROCESS SWITCH ROUTINE IS THE KEY FUNCTION WHICH     */ 01686000
*/*     ALLOWS THE SWITCHING OF PROCESSES RUNNING ON TWO CPUS TO BE  */ 01687000
*/*     SWITCHED INTO ONE CPU TO SHARE IT'S HARDWARE CHARACTERISTICS.*/ 01688000
*/*     THIS FUNCTION IS NECESSARY AND ACCOMPLISHED BY THIS ROUTINE, */ 01689000
*/*     TO REMOVE POSSIBLE INTERLOCKS BETWEEN SOFTWARE PROCESSES     */ 01690000
*/*     WHEN ONE CPU MALFUNCTIONS.  THE PROCESS SWITCH ROUTINE       */ 01691000
*/*     PROVIDES THE FOLLOWING FUNCTIONS TO THE CALLING ACR ROUTINES.*/ 01692000
*/*            PREPROCESSOR ENTRY: THE LOGICAL DATA OF THE DEAD CPU  */ 01693000
*/*                                IS SAVED IN ITS ASSOCIATED ACR    */ 01694000
*/*                                WORK SAVE AREA.                   */ 01695000
*/*            OTHER ENTRIES (RTM INTERFACE ROUTINE, LOCK-SPIN INTER-*/ 01696000
*/*                  FACE ROUTINE, POST PROCESSOR):                  */ 01697000
*/*                                THE LOGICAL DATA OF THE CURRENT   */ 01698000
*/*                                CPU PSA IS SAVED IN ITS ASSOCIATED*/ 01699000
*/*                                ACR WORK SAVE AREA, AND REPLACED  */ 01700000
*/*                                WITH THE LOGICAL DATA OF THE OTHER*/ 01701000
*/*                                CPU IN THE ACR UNION, FROM ITS    */ 01702000
*/*                                ASSOCIATED ACR WORK SAVE AREA.    */ 01703000
*/*                                                                  */ 01704000
*/* 02*  INPUT = RRPLPTR (REG 6) - RRPL ADDRESS                      */ 01705000
*/*              R13     - ADDRESS OF CURRENT ACR WORK SAVE AREA     */ 01706000
*/*              R14     - RETURN ADDRESS                            */ 01707000
*/*              PSAPTR2 (REG 9) - DEAD PSA ADDRESS (PREP. ONLY)     */ 01708000
*/*                      - OTHER ACR PSA ADDRESS ' CPU TO            */ 01709000
*/*                        BE GAINING CONTROL' (ALL OTHER ENTRIES)   */ 01710000
*/*              LCCAPTR (REG 10) - CURRENT LCCA ADDRESS(ALL ENTRIES)*/ 01711000
*/*              LCCAPTR2 (REG 11) - DEAD LCCA ADDRESS (PREP. ONLY)  */ 01712000
*/*                      - OTHER ACR LCCA ADDRESS 'CPU TO BE         */ 01713000
*/*                        GAINING CONTROL' (ALL OTHER ENTRIES)      */ 01714000
*/*                                                                  */ 01715000
*/* 02*  OUTPUT = RRPLPTR (REG 6) - RRPL ADDRESS  (ALL ENTRIES)      */ 01716000
*/*               PSAPTR2 (REG 9) - DEAD PSA ADDRESS (PREP. ONLY)    */ 01717000
*/*                      - OTHER ACR PSA ADDRESS 'NEW CPU NOW        */ 01718000
*/*                        GAINING CONTROL' (ALL OTHER ENTRIES)      */ 01719000
*/*              LCCAPTR (REG 10) - CURRENT LCCA ADDRESS(ALL ENTRIES)*/ 01720000
*/*              LCCAPTR2 (REG 11) - DEAD LCCA ADDRESS (PREP. ONLY)  */ 01721000
*/*                                                                  */ 01722000
*/* ******************************************************************/ 01723000
*                                                                  0496 01724000
*   DO;                                                            0496 01725000
*                                                                  0496 01726000
*     /***************************************************************/ 01727000
*     /*                                                             */ 01728000
*     /* SAVE THE ENTRY ENVIRONMENT                                  */ 01729000
*     /*                                                             */ 01730000
*     /***************************************************************/ 01731000
*                                                                  0497 01732000
*     RESPECIFY                                                    0497 01733000
*      (R13,                                                       0497 01734000
*       R8,                                                        0497 01735000
*       R7) RESTRICTED;                                            0497 01736000
*     GENERATE REFS(R0,R15,ACRACRSV,R13);                          0498 01737000
         STM R0,R15,ACRACRSV(R13)        SAVE ENTRY REGS            */  01738000
*     RRPLFLGS='0'B;                /* RESET ANY PREVIOUS PHASE ERROR   01739000
*                                      INDICATIONS.                  */ 01740000
         MVI   RRPLFLGS(R6),B'00000000'                            0499 01741000
*     GEN REFS(RRPLREGS,R6)(STM 0,14,RRPLREGS(R6));/* SAVE REGS FOR     01742000
*                                      RECURSION                     */ 01743000
         STM 0,14,RRPLREGS(R6)                                          01744000
*RSTRTX0:                           /* FRR RETRY ENTRY POINT         */ 01745000
*     IF RRPLRTRY='0'B THEN         /* IS ENTRY FROM THE FRR         */ 01746000
*                                                                  0501 01747000
RSTRTX0  TM    RRPLRTRY(R6),B'10000000'                            0501 01748000
         BNZ   @RF00501                                            0501 01749000
*       /*************************************************************/ 01750000
*       /*                                                           */ 01751000
*       /* BUILD THE PROCESS SWITCH LOGICAL PHASE ID                 */ 01752000
*       /*                                                           */ 01753000
*       /*************************************************************/ 01754000
*                                                                  0502 01755000
*       LOGPHID=LOGPHID*16;         /* NO, SHIFT THE PHASE ID TO   0502 01756000
*                                      PRODUCE A RESULTING PHASE ID     01757000
*                                      OF 10, 20, 30, OR 40,       0502 01758000
*                                      CORRESPONDING WITH ENTRY      */ 01759000
         SLR   @12,@12                                             0502 01760000
         IC    @12,LOGPHID(,R6)                                    0502 01761000
         SLA   @12,4                                               0502 01762000
         STC   @12,LOGPHID(,R6)                                    0502 01763000
*     IF LCCACRIN='1'B THEN         /* PREPROCESSOR ENTRY?           */ 01764000
@RF00501 TM    LCCACRIN(R10),B'00001000'                           0503 01765000
         BNO   @RF00503                                            0503 01766000
*       DO;                         /* PREPROCESSOR ENTRY            */ 01767000
*         R13=R11->LCCACPUS->WSACACR;/* SET BASE TO DEAD CPU ACR WORK   01768000
*                                      SAVE AREA                     */ 01769000
         L     @01,LCCACPUS(,R11)                                  0505 01770000
         L     R13,WSACACR(,@01)                                   0505 01771000
*         R7=R9;                    /* SET BASE TO DEAD PSA FOR THE     01772000
*                                      MOVE SECTION                  */ 01773000
*                                                                  0506 01774000
         LR    R7,R9                                               0506 01775000
*         /***********************************************************/ 01776000
*         /*                                                         */ 01777000
*         /* OBTAIN AND SAVE THE DEAD CPU'S SEGMENT TABLE ORIGIN     */ 01778000
*         /* REGISTER                                                */ 01779000
*         /*                                                         */ 01780000
*         /***********************************************************/ 01781000
*                                                                  0507 01782000
*         R13->ACRCTRG1=R9->PSAAOLD->ASCBSTOR;                     0507 01783000
         L     @12,PSAAOLD(,R9)                                    0507 01784000
         L     @12,ASCBSTOR(,@12)                                  0507 01785000
         ST    @12,ACRCTRG1(,R13)                                  0507 01786000
*       END;                                                       0508 01787000
*     ELSE                                                         0509 01788000
*       DO;                         /* NOT PREPROCESSOR              */ 01789000
         B     @RC00503                                            0509 01790000
@RF00503 DS    0H                                                  0510 01791000
*         R7=0;                     /* SET CURRENT PSA FOR MOVES     */ 01792000
*                                                                  0510 01793000
         SLR   R7,R7                                               0510 01794000
*         /***********************************************************/ 01795000
*         /*                                                         */ 01796000
*         /* THE SEGMENT TABLE ORIGIN REGISTER IS SAVED IN THE ACR   */ 01797000
*         /* WORK AREA                                               */ 01798000
*         /*                                                         */ 01799000
*         /***********************************************************/ 01800000
*                                                                  0511 01801000
*         GENERATE REFS(R1,R13,ACRCTRG1);                          0511 01802000
           STCTL R1,R1,ACRCTRG1(R13)    SAVE CONT REG 1                 01803000
*       END;                                                       0512 01804000
*     DO;                                                          0513 01805000
*                                                                  0513 01806000
@RC00503 DS    0H                                                  0514 01807000
*/* ******************************************************************/ 01808000
*/*                                                                  */ 01809000
*/* 01* SEGMENT-NAME  ACRMVPSA                                       */ 01810000
*/*                                                                  */ 01811000
*/*     FUNCTION                                                     */ 01812000
*/*     THIS SEGMENT MOVES LOGICAL DATA FROM A PSA (DESIGNATED BY    */ 01813000
*/*     AMVPTR) TO ITS ASSOCIATED ACR WORK SAVE AREA (DESIGNATED BY  */ 01814000
*/*     AWSAPTR).  THE MOVE IS BROKEN INTO THREE LOGICAL SECTIONS-   */ 01815000
*/*     1- MOVE OF LOGICAL DATA FROM HARDWARE FLC LOCATIONS TO THE   */ 01816000
*/*         ACR WORK SAVE AREA                                       */ 01817000
*/*      2- MOVE OF THE ENTIRE SOFTWARE SECTION OF THE PSA TO THE    */ 01818000
*/*         ACR WORK SAVE AREA.  (FOR THIS MOVE THE PSA MAPPING IS   */ 01819000
*/*        PLACED OVER THE WORK AREA SO THAT THE SOFTWARE PSA SECTION*/ 01820000
*/*        (PSAPSA) IS POSITIONED FOLLOWING THE PREVIOUSLY MOVED     */ 01821000
*/*         FLC LOCATIONS.                                           */ 01822000
*/*    3-MOVE OF THE NORMAL STACK FROM ITS PSA LOCATION(PSASTAK)     */ 01823000
*/*                                                                  */ 01824000
*/* 02* INPUT = AWSAPTR - PTR TO ACR WORK SAVE AREA TO WHICH         */ 01825000
*/*                       DATA IS TO BE MOVED                        */ 01826000
*/*             AMVPTR  - ADDRESS OF THE PSA TO MOVE FROM            */ 01827000
*/*             LCCAPTR2- ADDRESS OF THE LCCA ASSOCIATED             */ 01828000
*/*                       WITH THE ACR WORK SAVE AREA PASSED         */ 01829000
*/*                                                                  */ 01830000
*/* 02* OUTPUT=                                                      */ 01831000
*/*                                                                  */ 01832000
*/* ******************************************************************/ 01833000
*                                                                  0514 01834000
*       RESPECIFY                                                  0514 01835000
*        (R2,                                                      0514 01836000
*         R3,                                                      0514 01837000
*         R14,                                                     0514 01838000
*         R15) RESTRICTED;                                         0514 01839000
*                                                                  0514 01840000
*       /*************************************************************/ 01841000
*       /*                                                           */ 01842000
*       /* MOVE FLC HARDWARE LOCATIONS TO THE ACR WORK SAVE AREA     */ 01843000
*       /*                                                           */ 01844000
*       /*************************************************************/ 01845000
*                                                                  0515 01846000
*       R13->ACRRPSW=R7->FLCROPSW;  /* SAVE THE RESTART OPSW         */ 01847000
         MVC   ACRRPSW(8,R13),FLCROPSW(R7)                         0515 01848000
*       R13->ACROPSW=R7->APSWSTRT;  /* SAVE THE OLD PSWS             */ 01849000
         MVC   ACROPSW(64,R13),APSWSTRT(R7)                        0516 01850000
*       R13->ACREPSWS=R7->AECSTRT;  /* SAVE THE PSW EXTENSIONS       */ 01851000
*                                                                  0517 01852000
         MVC   ACREPSWS(28,R13),AECSTRT(R7)                        0517 01853000
*       /*************************************************************/ 01854000
*       /*                                                           */ 01855000
*       /* PLACE CPU MASK AND ADDRESS IN SAVE AREA FOR EASE OF ACCESS*/ 01856000
*       /*                                                           */ 01857000
*       /*************************************************************/ 01858000
*                                                                  0518 01859000
*       R13->ACRCPUAD=R7->PSACPUPA; /* OBTAIN AND SAVE CPU ADDR      */ 01860000
         LH    @12,PSACPUPA(,R7)                                   0518 01861000
         STH   @12,ACRCPUAD(,R13)                                  0518 01862000
*       R13->ACRCAFM=BITS(R7->PSACPUPA+1);/* DEVELOPE MASK & SAV     */ 01863000
*                                                                  0519 01864000
         ALR   @12,@12                                             0519 01865000
         LA    @01,BITS(@12)                                       0519 01866000
         MVC   ACRCAFM(2,R13),0(@01)                               0519 01867000
*       /*************************************************************/ 01868000
*       /*                                                           */ 01869000
*       /* SET BASE TO ALLOW PSA MAPPING FOR THE SOFTWARE SECTION OF */ 01870000
*       /* THE PSA IN THE ACR WORK SAVE AREA                         */ 01871000
*       /*                                                           */ 01872000
*       /*************************************************************/ 01873000
*                                                                  0520 01874000
*       R13=ADDR(R13->AWSAEND)-512;                                0520 01875000
*                                                                  0520 01876000
         LA    @12,AWSAEND(,R13)                                   0520 01877000
         SL    @12,@CF02412                                        0520 01878000
         LR    R13,@12                                             0520 01879000
*       /*************************************************************/ 01880000
*       /*                                                           */ 01881000
*       /* MOVE THE SOFTWARE SECTION OF THE PSA TO THE ACR WORK      */ 01882000
*       /* SAVEAREA                                                  */ 01883000
*       /*                                                           */ 01884000
*       /*************************************************************/ 01885000
*                                                                  0521 01886000
*       R3=ADDR(R13->ACRPSEND)-ADDR(R13->ACRPSA);/* DETERMINE THE  0521 01887000
*                                      LENGTH TO MOVE                */ 01888000
         LA    R3,536                                              0521 01889000
*       R15=R3;                     /* SET LENGTH FOR MOVE           */ 01890000
         LR    R15,R3                                              0522 01891000
*       R2=ADDR(R13->ACRPSA);       /* DEFINE TO ADDRESS             */ 01892000
         LA    R2,ACRPSA(,R13)                                     0523 01893000
*       R14=ADDR(R7->PSAPSA);       /* DEFINE FROM ADDRESS           */ 01894000
         LA    R14,PSAPSA(,R7)                                     0524 01895000
*       GENERATE REFS(R2,R14);                                     0525 01896000
        MVCL R2,R14                    MOVE ENTIRE PSA SOFTWARE AREA    01897000
*       R13->PSAACR='1'B;           /* SET ACR SUPER BIT ON IN     0526 01898000
*                                      'SAVED' PSA SO THAT IT WILL BE   01899000
*                                      ON WHEN THE SAVED PSA DATA IS    01900000
*                                      MOVED INTO THE PSA            */ 01901000
         OI    PSAACR(R13),B'00000100'                             0526 01902000
*       R3=LENGTH(PSASTAK);         /* GET LENGTH OF THE CURRENT   0527 01903000
*                                      STACK                         */ 01904000
         LA    R3,596                                              0527 01905000
*       R15=R3;                     /* SET LENGTH FOR MOVE           */ 01906000
         LR    R15,R3                                              0528 01907000
*       R2=ADDR(R13->PSAUSEND);     /* DEFINE TO ADDRESS             */ 01908000
         LA    R2,PSAUSEND(,R13)                                   0529 01909000
*       R14=ADDR(R7->PSASTAK);      /* DEFINE FROM ADDRESS           */ 01910000
         LA    R14,PSASTAK(,R7)                                    0530 01911000
*       GENERATE REFS(R2,R14);                                     0531 01912000
       MVCL R2,R14                MOVE STACK TO SAVE AREA               01913000
*       RESPECIFY                                                  0532 01914000
*        (R2,                                                      0532 01915000
*         R3,                                                      0532 01916000
*         R14,                                                     0532 01917000
*         R15) UNRESTRICTED;                                       0532 01918000
*                                                                  0532 01919000
*       /*************************************************************/ 01920000
*       /*                                                           */ 01921000
*       /* PLACE THE CPU HIGHEST LOCK HELD INDICATOR IN THE          */ 01922000
*       /* ASSOCIATED LCCA FOR THE SETLOCK MANAGER                   */ 01923000
*       /*                                                           */ 01924000
*       /*************************************************************/ 01925000
*                                                                  0533 01926000
*       R7->PSALCCAV->LCCACRLC=R7->PSAHLHI;                        0533 01927000
*                                                                  0533 01928000
         L     @12,PSALCCAV(,R7)                                   0533 01929000
         L     @15,PSAHLHI(,R7)                                    0533 01930000
         ST    @15,LCCACRLC(,@12)                                  0533 01931000
*/* ******************************************************************/ 01932000
*/*  END OF SEGMENT ACRMVPSA                                         */ 01933000
*/* ******************************************************************/ 01934000
*                                                                  0534 01935000
*     END;                                                         0534 01936000
*     IF LCCACRIN='0'B THEN         /* IS IT A PREPROCESSOR ENTRY?   */ 01937000
         TM    LCCACRIN(R10),B'00001000'                           0535 01938000
         BNZ   @RF00535                                            0535 01939000
*       DO;                         /* NOT PREPROCESSOR ENTRY        */ 01940000
*         GEN REFS(RRPLREGS,R6)(STM 0,14,RRPLREGS(R6));/* SAVE REGS     01941000
*                                      FOR RECURSION                 */ 01942000
         STM 0,14,RRPLREGS(R6)                                          01943000
*         RRPLFLGS='0'B;            /* CLEAR ERROR FLAGS             */ 01944000
         MVI   RRPLFLGS(R6),B'00000000'                            0538 01945000
*         LOGPHID=LOGPHID|'01'X;    /* SET PHASE ID TO X1            */ 01946000
         OI    LOGPHID(R6),X'01'                                   0539 01947000
*RSTRTX1:                           /* FRR RETRY ENTRY PT            */ 01948000
*         R7=WSACACR;               /* SET BASE TO CURRENT ACR WORK     01949000
*                                      AREA                          */ 01950000
RSTRTX1  L     @12,LCCACPUS(,R10)                                  0540 01951000
         L     R7,WSACACR(,@12)                                    0540 01952000
*         R13=R11->LCCACPUS->WSACACR;/* SET BASE TO OTHER CPU ACR  0541 01953000
