*               ELSE                                               0402 01252000
*                 CONSPARM=''B;     /* BUILD SINGLE MASK VALUE       */ 01253000
*                                                                  0402 01254000
         B     @RC00400                                            0402 01255000
@RF00400 L     @12,LCCACPUS(,R10)                                  0402 01256000
         L     @12,WSACACR(,@12)                                   0402 01257000
         SLR   @08,@08                                             0402 01258000
         ST    @08,CONSPARM(,@12)                                  0402 01259000
*               /*****************************************************/ 01260000
*               /*                                                   */ 01261000
*               /* OR THE DEAD CPU MASK INTO THE ECB VALUE           */ 01262000
*               /*                                                   */ 01263000
*               /*****************************************************/ 01264000
*                                                                  0403 01265000
*               SWMASK=SWMASK|R11->LCCACPUS->WSACACR->ACRCAFM;     0403 01266000
@RC00400 L     @12,LCCACPUS(,R10)                                  0403 01267000
         L     @12,WSACACR(,@12)                                   0403 01268000
         L     @08,LCCACPUS(,R11)                                  0403 01269000
         L     @08,WSACACR(,@08)                                   0403 01270000
         OC    SWMASK(2,@12),ACRCAFM(@08)                          0403 01271000
*               POSTCDE='40'X;      /* SET THE POST CODE ON          */ 01272000
         MVI   POSTCDE(@12),X'40'                                  0404 01273000
*               R3=CONSPARM;        /* SET THE SWAP REGISTER         */ 01274000
         L     R3,CONSPARM(,@12)                                   0405 01275000
*               CS(R2,R3,CONSWECB); /* POST THE UCMARECB             */ 01276000
