         L     @15,WSACACR(,@15)                                   0476 01614000
         L     R2,ACRRTNPT(,@15)                                   0476 01615000
*                 PSAACR='0'B;      /* TURN OFF SUPER BIT            */ 01616000
         NI    PSAACR(R4),B'11111011'                              0477 01617000
*                 GO TO R2;         /* RETURN TO EXT FLIH OR SLIH    */ 01618000
