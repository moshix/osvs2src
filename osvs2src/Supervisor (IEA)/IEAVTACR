         L     @01,LCCACPUS(,R11)                                  0541 01955000
         L     R13,WSACACR(,@01)                                   0541 01956000
*         R8=ADDR(R7->ACRACRWA);    /* SET BASE TO NEXT PHASES RRPL     01957000
*                                      BUILD AREA                    */ 01958000
         LA    R8,ACRACRWA(,R7)                                    0542 01959000
*         GENERATE REFS(ACRCTRG1,R13,R1);                          0543 01960000
              LCTL R1,R1,ACRCTRG1(R13)   SET CONTROL REGISTER 1         01961000
*         DO;                                                      0544 01962000
*                                                                  0544 01963000
*/* ******************************************************************/ 01964000
*/*                                                                  */ 01965000
*/* 01* SEGMENT-NAME  ACRMVSVA                                       */ 01966000
*/*                                                                  */ 01967000
*/* 02* FUNCTION                                                     */ 01968000
*/*     THIS SEGMENT MOVES LOGICAL DATA FROM AN ACR WORK SAVE AREA   */ 01969000
*/*    (DESIGNATED BY AWSAPTR) TO THE CURRENT PSA.  THE MOVE IS      */ 01970000
*/*     BROKEN INTO THREE LOGICAL SECTIONS.                          */ 01971000
*/*    1- MOVE OF THE HARDWARE FLC FIELDS SAVED IN THE ACR WORK SAVE */ 01972000
*/*       AREA TO THE CURRENT PSA FLC AREA OF THE PSA.               */ 01973000
*/*    2- MOVE OF ENTIRE PSA SOFTWARE SECTION OF THE PSA SAVED IN THE*/ 01974000
*/*        ACR WORK SAVE AREA, WITH THE EXCEPTION OF THE PSAPCCA REAL*/ 01975000
*/*        AND VIRTUAL ADRESSES(PSAPCCAR,PSAPCCAV).                  */ 01976000
*/*    3-MOVE OF THE NORMAL STACK TO ITS PROPER PSA LOCATION.        */ 01977000
*/*                                                                  */ 01978000
*/* 02* INPUT = AWSAPTR - ADDRESS OF THE ACR WORK SAVE TO MOVE       */ 01979000
*/*                                                                  */ 01980000
*/* 02* OUTPUT=                                                      */ 01981000
*/*                                                                  */ 01982000
*/* ******************************************************************/ 01983000
*                                                                  0545 01984000
*           RESPECIFY                                              0545 01985000
*            (R2,                                                  0545 01986000
*             R3,                                                  0545 01987000
*             R14,                                                 0545 01988000
*             R15) RESTRICTED;                                     0545 01989000
*                                                                  0545 01990000
*           /*********************************************************/ 01991000
*           /*                                                       */ 01992000
*           /* MOVE DATA TO THE PSA FLC FROM THE ACR WORK SAVE AREA  */ 01993000
*           /*                                                       */ 01994000
*           /*********************************************************/ 01995000
*                                                                  0546 01996000
*           FLCROPSW=R13->ACRRPSW;  /* RESTORE THE RESTART OLD PSW   */ 01997000
         MVC   FLCROPSW(8,R4),ACRRPSW(R13)                         0546 01998000
*           APSWSTRT=R13->ACROPSW;  /* RESTORE THE OLD PSWS          */ 01999000
         MVC   APSWSTRT(64,R4),ACROPSW(R13)                        0547 02000000
*           AECSTRT=R13->ACREPSWS;  /* RESTORE THE PSW EXTENSIONS    */ 02001000
*                                                                  0548 02002000
         MVC   AECSTRT(28,R4),ACREPSWS(R13)                        0548 02003000
*           /*********************************************************/ 02004000
*           /*                                                       */ 02005000
*           /* SET BASE TO ALLOW PSA MAPPING FOR THE SOFTWARE SECTION*/ 02006000
*           /* OF THE PSA IN THE ACR WORK SAVE AREA.                 */ 02007000
*           /*                                                       */ 02008000
*           /*********************************************************/ 02009000
*                                                                  0549 02010000
*           R13=ADDR(R13->AWSAEND)-512;                            0549 02011000
*                                                                  0549 02012000
         LA    @12,AWSAEND(,R13)                                   0549 02013000
         SL    @12,@CF02412                                        0549 02014000
         LR    R13,@12                                             0549 02015000
*           /*********************************************************/ 02016000
*           /*                                                       */ 02017000
*           /* MOVE FIRST AREA TO PSA (PSAPSA TO PSAPCCAV)           */ 02018000
*           /*                                                       */ 02019000
*           /*********************************************************/ 02020000
*                                                                  0550 02021000
*           R2=ADDR(PSAPCCAV);      /* DETERMINE END OF MOVE         */ 02022000
         LA    R2,PSAPCCAV(,R4)                                    0550 02023000
*           R3=ADDR(PSAPSA);        /* DETERMINE START OF MOVE       */ 02024000
         LA    R3,PSAPSA(,R4)                                      0551 02025000
*           R2=R2-R3;               /* DETERMINE LENGTH TO MOVE      */ 02026000
         SLR   R2,R3                                               0552 02027000
*           PSAPSA(1:R2)=R13->ACRPSA(1:R2);/* MOVE DATA TO PSA       */ 02028000
*                                                                  0553 02029000
         LR    @12,R2                                              0553 02030000
         BCTR  @12,0                                               0553 02031000
         EX    @12,@SM03164                                        0553 02032000
*           /*********************************************************/ 02033000
*           /*                                                       */ 02034000
*           /* MOVE SECOND AREA TO PSA (PSALCCAV TO PSAUSEND)        */ 02035000
*           /*                                                       */ 02036000
*           /*********************************************************/ 02037000
*                                                                  0554 02038000
*           R3=ADDR(PSAUSEND)-ADDR(PSALCCAV);/* DETERMINE LENGTH TO     02039000
*                                      MOVE                          */ 02040000
         LA    R3,520                                              0554 02041000
*           R15=R3;                 /* SET LENGTH TO MOVE            */ 02042000
         LR    R15,R3                                              0555 02043000
*           R2=ADDR(PSALCCAV);      /* SET START ADDRESS             */ 02044000
         LA    R2,PSALCCAV(,R4)                                    0556 02045000
*           R14=ADDR(R13->ACRLCCAV);/* SET FROM ADDRESS              */ 02046000
         LA    R14,ACRLCCAV(,R13)                                  0557 02047000
*           GENERATE REFS(R2,R14);                                 0558 02048000
*                                                                  0558 02049000
        MVCL R2,R14                 MOVE TO PSA                         02050000
*           /*********************************************************/ 02051000
*           /*                                                       */ 02052000
*           /* MOVE THIRD AREA TO PSA (SAVED CURRENT STACK)          */ 02053000
*           /*                                                       */ 02054000
*           /*********************************************************/ 02055000
*                                                                  0559 02056000
*           R3=LENGTH(PSASTAK);     /* DETERMINE LENGTH TO MOVE      */ 02057000
         LA    R3,596                                              0559 02058000
*           R15=R3;                 /* SET MOVE LENGTH               */ 02059000
         LR    R15,R3                                              0560 02060000
*           R2=ADDR(PSASTAK);       /* SET PSA START ADDRESS         */ 02061000
         LA    R2,PSASTAK(,R4)                                     0561 02062000
*           R14=ADDR(R13->PSAUSEND);/* SET FROM ADDRESS              */ 02063000
         LA    R14,PSAUSEND(,R13)                                  0562 02064000
*           GENERATE REFS(R2,R14);                                 0563 02065000
        MVCL R2,R14                    MOVE TO PSA                      02066000
*           PSACPUSA=PSAPCCAV->PCCACPUA;/* RE-SET CPU STATIC PHYSICAL   02067000
*                                      ADDRESS TO CORRESPOND TO THE     02068000
*                                      PCCA FOR THIS CPU     @YM03489*/ 02069000
         L     @12,PSAPCCAV(,R4)                                   0564 02070000
         LH    @12,PCCACPUA(,@12)                                  0564 02071000
         STH   @12,PSACPUSA(,R4)                                   0564 02072000
*           RESPECIFY                                              0565 02073000
*            (R2,                                                  0565 02074000
*             R3,                                                  0565 02075000
*             R14,                                                 0565 02076000
*             R15) UNRESTRICTED;                                   0565 02077000
*                                                                  0565 02078000
*/* ******************************************************************/ 02079000
*/*  END OF SEGMENT ACRMVSVA                                         */ 02080000
*/* ******************************************************************/ 02081000
*                                                                  0566 02082000
*         END;                                                     0566 02083000
*                                                                  0566 02084000
*         /***********************************************************/ 02085000
*         /*                                                         */ 02086000
*         /* SWAP LCCA POINTERS (LCCAPTR2,LCCAPTR)                   */ 02087000
*         /*                                                         */ 02088000
*         /***********************************************************/ 02089000
*                                                                  0567 02090000
*         RFY                                                      0567 02091000
*           R15 RSTD;                                              0567 02092000
*         R15=R11;                  /* TEMP SAVE OTHER LCCA PTR      */ 02093000
         LR    R15,R11                                             0568 02094000
*         R11=R10;                  /* SET OTHER LCCA PTR AS THE PTR    02095000
*                                      TO THE ORIGINAL CURRENT       */ 02096000
         LR    R11,R10                                             0569 02097000
*         R10=R15;                  /* SET CURRENT LCCA PTR TO THE 0570 02098000
*                                      ONE THAT WAS THE 'OTHER' LCCA */ 02099000
         LR    R10,R15                                             0570 02100000
*         RFY                                                      0571 02101000
*           R15 UNRSTD;                                            0571 02102000
*         RESPECIFY                                                0572 02103000
*          (R14) RSTD;                                             0572 02104000
*         R14=R7->ACRSVR14;         /* PICK UP RETAURN ADDRESS       */ 02105000
         L     R14,ACRSVR14(,R7)                                   0573 02106000
*       END;                                                       0574 02107000
*     ELSE                                                         0575 02108000
*       DO;                         /* PREPROCESSOR RETURN           */ 02109000
*                                                                  0575 02110000
         B     @RC00535                                            0575 02111000
@RF00535 DS    0H                                                  0576 02112000
*         /***********************************************************/ 02113000
*         /*                                                         */ 02114000
*         /* RESET MCH FLAGS IN THE PSA JUST SAVED(DEAD) SO THAT MCH */ 02115000
*         /* CODE CALLED BY RTM(DURING INTERMEDIATE PHASE) WORKS     */ 02116000
*         /*                                                         */ 02117000
*         /***********************************************************/ 02118000
*                                                                  0576 02119000
*         R13->PSAMCHFL='00'X;      /* RESET MCH RECURSION BYTE      */ 02120000
         MVI   PSAMCHFL(R13),X'00'                                 0576 02121000
*         R13->PSAMCHIC='00'X;      /* RESET MCH INIT. BYTE          */ 02122000
         MVI   PSAMCHIC(R13),X'00'                                 0577 02123000
*         R13=WSACACR;              /* RESET PTR TO ACR WORK AREA    */ 02124000
         L     @15,LCCACPUS(,R10)                                  0578 02125000
         L     R13,WSACACR(,@15)                                   0578 02126000
*         GENERATE REFS(R0,R15,ACRACRSV,R13);                      0579 02127000
           LM R0,R15,ACRACRSV(R13)      RESTORE ENTRY REGS          */  02128000
*       END;                                                       0580 02129000
*     RESPECIFY                                                    0581 02130000
*      (R13,                                                       0581 02131000
*       R14,                                                       0581 02132000
*       R13,                                                       0581 02133000
*       R7) UNRESTRICTED;                                          0581 02134000
*     RETURN;                                                      0582 02135000
@EL00002 DS    0H                                                  0582 02136000
@EF00002 DS    0H                                                  0582 02137000
@ER00002 BR    @14                                                 0582 02138000
*   END;                                                           0583 02139000
*                                                                  0583 02140000
*/* ******************************************************************/ 02141000
*/*  END OF SEGMENT ACRPROSW                                         */ 02142000
*/* ******************************************************************/ 02143000
*                                                                  0584 02144000
*   END ACRPROSW;                                                  0584 02145000
         B     @EL00002                                            0584 02146000
*   END IEAVTACR                                                   0585 02147000
*                                                                  0585 02148000
