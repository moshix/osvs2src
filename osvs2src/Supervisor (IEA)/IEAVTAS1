         ST    @12,RTM2TRRA(,RTM2PTR)                              0488 01485000
*                         RTM2IORS=OFF;/* ZERO SVC 17 RECURSION FLAG */ 01486000
         NI    RTM2IORS(RTM2PTR),B'11011111'                       0489 01487000
*                         RTM2FIOB=FOX;/* INDICATE NO RESTORABLE I/O */ 01488000
         MVC   RTM2FIOB(3,RTM2PTR),@CF00087+1                      0490 01489000
*                         RESPECIFY                                0491 01490000
*                          (R1) UNRESTRICTED;                      0491 01491000
