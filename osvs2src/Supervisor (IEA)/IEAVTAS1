*                     RBSECPTR=RTM2VRBC;/* MAKE TOP RB CURRENT       */ 01016000
*                                                                  0352 01017000
         L     RBSECPTR,RTM2VRBC(,RTM2PTR)                         0352 01018000
*                     /***********************************************/ 01019000
*                     /*                                             */ 01020000
*                     /* CHAIN THROUGH THE RB QUEUE TO FIND THE      */ 01021000
*                     /* REGISTERS OF THE RB ASSOCIATED WITH THE     */ 01022000
*                     /* CURRENT SCB                                 */ 01023000
*                     /*                                             */ 01024000
*                     /***********************************************/ 01025000
*                                                                  0353 01026000
*                     DO WHILE(RBLINKB^=SCBOWNRA&RBLINKB^=RTM2STAR);    01027000
         B     @DE00353                                            0353 01028000
@DL00353 DS    0H                                                  0354 01029000
*                       RBSECPTR=RBLINKB;/* UPDATE CURRENT RB POINTER*/ 01030000
