         LM GPR11P,GPR13P,576(GPR13P)                                   00516000
*                             GEN(BALR GPR14P,GPR13P);/* BALR TO   0216 00517000
*                                      SETLOCK RTN.                  */ 00518000
         BALR GPR14P,GPR13P                                             00519000
*                             RFY                                  0217 00520000
*                              (GPR11P,                            0217 00521000
*                               GPR12P,                            0217 00522000
