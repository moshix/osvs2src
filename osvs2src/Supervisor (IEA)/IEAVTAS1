         LA    @12,0(,@12)                                         0192 00432000
         LTR   @12,@12                                             0192 00433000
         BZ    @RF00192                                            0192 00434000
*                 DO;               /* A CDE EXISTS                  */ 00435000
*                   RTM2ABNM=RBCDE1->CDNAME;/* ACCESS PROGRAM NAME   */ 00436000
         MVC   RTM2ABNM(8,RTM2PTR),CDNAME(@12)                     0194 00437000
*                   RTM2ABEP=RBCDE1->CDENTPT;/* ACCESS ENTRY POINT   */ 00438000
         L     @12,CDENTPT(,@12)                                   0195 00439000
         ST    @12,RTM2ABEP(,RTM2PTR)                              0195 00440000
*                 END;                                             0196 00441000
*               ELSE                                               0197 00442000
*                 ;                                                0197 00443000
@RF00192 DS    0H                                                  0198 00444000
*             ELSE                                                 0198 00445000
*               DO;                                                0198 00446000
         B     @RC00191                                            0198 00447000
@RF00191 DS    0H                                                  0199 00448000
*                 IF RBFTP='010'B|RBFTP='100'B THEN/* TEST IF THIS IS   00449000
*                                      AN IRB OR SIRB                */ 00450000
         TM    RBFTP(RBSECPTR),B'01000000'                         0199 00451000
         BNO   @GL00005                                            0199 00452000
         TM    RBFTP(RBSECPTR),B'10100000'                         0199 00453000
         BZ    @RT00199                                            0199 00454000
@GL00005 TM    RBFTP(RBSECPTR),B'10000000'                         0199 00455000
         BNO   @RF00199                                            0199 00456000
         TM    RBFTP(RBSECPTR),B'01100000'                         0199 00457000
         BNZ   @RF00199                                            0199 00458000
@RT00199 DS    0H                                                  0200 00459000
*                   RTM2ABEP=RBEP;  /* ACCESS EP OF SIRB OR IRB      */ 00460000
*                                                                  0200 00461000
