*                           ESAGLREC=ON;/* SET UP RECURSION        0211 00498000
*                                      ENVIRONMENT           @YM01819*/ 00499000
         L     @12,RTM2VRBC(,RTM2PTR)                              0211 00500000
         OI    ESAGLREC+96(@12),B'00010000'                        0211 00501000
*                           DO;     /* SETLOCK (OBTAIN) MODE(UNCOND)    00502000
*                                      TYPE(LOCAL) RELATED(JPAQ,IEAVT   00503000
*                                      AS1(CDESRCH))                 */ 00504000
*                             RFY                                  0213 00505000
*                              (GPR11P,                            0213 00506000
*                               GPR12P,                            0213 00507000
*                               GPR13P,                            0213 00508000
*                               GPR14P) RSTD;                      0213 00509000
*                             GPR13P=PSALITA;/* ADDRESS OF LOCK    0214 00510000
