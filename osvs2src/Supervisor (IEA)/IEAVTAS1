         BO    @RF00239                                            0239 00620000
*                             IF XTLSTPTR^=ZERO THEN/* TEST IF XTENT    00621000
*                                      LIST EXISTS                   */ 00622000
         LTR   XTLSTPTR,XTLSTPTR                                   0240 00623000
         BZ    @RF00240                                            0240 00624000
*                               DO I=1 TO XTLNRFAC WHILE(RTM2TRSW^=ON); 00625000
         LA    I,1                                                 0241 00626000
         B     @DE00241                                            0241 00627000
@DL00241 TM    RTM2TRSW(RTM2PTR),B'00010000'                       0241 00628000
         BO    @DC00241                                            0241 00629000
*                                   /* THIS SEARCH WILL CONTINUE   0242 00630000
*                                      UNTIL A MATCH IS FOUND OR   0242 00631000
*                                      THERE IS NO MATCH FOR THIS CDE*/ 00632000
*                                 IF XTLMSBAA(I)+XTLMSBLN(I)>RBCDE1&    00633000
*                                   /* IF EP BELOW TOP       @ZA24479*/ 00634000
*                                     XTLMSBAA(I)^>RBCDE1 THEN/* AND    00635000
*                                      IF EP IS ABOVE THE BOTTOM OF     00636000
