*             TCBFX=OFF;            /* ALLOW ASYNCHRONOUS EXITS      */ 01360000
         B     @RC00449                                            0451 01361000
@RF00449 NI    TCBFX(TCBPTR),B'11111110'                           0451 01362000
*           IF RTM2TRME=ON THEN     /* IN TERM PORCESSING IO HAS BEEN   01363000
*                                      PREVIOUSLY HALTED             */ 01364000
@RC00449 TM    RTM2TRME(RTM2PTR),B'10000000'                       0452 01365000
         BNO   @RF00452                                            0452 01366000
*             DO;                   /* TERM PROCESSING               */ 01367000
*               RTM2IOFS=BYTE0;     /* ZERO IO FLAG BYTE             */ 01368000
