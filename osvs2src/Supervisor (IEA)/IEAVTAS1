@RF00555 LA    R0,16                                               0557 01682000
*               R1=RTM2RTCA;        /* ADDRESS OF SDWA(RTCA)         */ 01683000
@RC00555 DS    0H                                                  0558 01684000
@RC00553 DS    0H                                                  0558 01685000
@RC00551 L     @12,RTM2RTCA(,RTM2PTR)                              0558 01686000
         LR    R1,@12                                              0558 01687000
*               R13=RTM2RTCA+LENGTH(SDWA);/* USER EXIT SAVE AREA     */ 01688000
         AL    @12,@CF03281                                        0559 01689000
         LR    R13,@12                                             0559 01690000
*             END;                                                 0560 01691000
*           ELSE                    /* IF SDWA(RTCA) NOT AVAILABLE   */ 01692000
*             DO;                                                  0561 01693000
         B     @RC00549                                            0561 01694000
@RF00549 DS    0H                                                  0562 01695000
*               R0=CODE12;          /* INDICATE SDWA NOT AVAILABLE TO   01696000
*                                      FOR USER EXIT                 */ 01697000
         LA    R0,12                                               0562 01698000
*               R1=RTM2CC;          /* ABEND COMPLETION CODE         */ 01699000
         L     R1,RTM2CC-1(,RTM2PTR)                               0563 01700000
         LA    R1,0(,R1)                                           0563 01701000
*               R2=SCBPARM;         /* ADDRESS OF USER PARAMETER LIST*/ 01702000
         L     R2,SCBPARM(,SCBPTR)                                 0564 01703000
*               R13=SCBEXIT;        /* NO SAVE AREA FOR USER,      0565 01704000
*                                      INITIALIZE WITH USER EXIT ADDR*/ 01705000
         L     R13,SCBEXIT(,SCBPTR)                                0565 01706000
*             END;                                                 0566 01707000
*                                                                  0566 01708000
*           /*********************************************************/ 01709000
*           /*                                                       */ 01710000
*           /* SEGMENT PRESYNCH WILL SET UP THE INTERFACE FOR THE    */ 01711000
*           /* SYNCH MACRO IN THE TCB. IT WILL ALSO INDICATE IN THE  */ 01712000
*           /* RTM2 WORKAREA THAT AN EXIT IS IN PROGRESS AND MARK THE*/ 01713000
*           /* CURRENT SCB IN USE.                                   */ 01714000
*           /*                                                       */ 01715000
*           /*********************************************************/ 01716000
*                                                                  0567 01717000
*           DO;                     /* BEGIN PRESYNCH                */ 01718000
