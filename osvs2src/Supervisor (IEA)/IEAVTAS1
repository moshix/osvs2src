*                 GEN(STM  0,15,RTM2RREG(RTM2PTR));                0471 01433000
         STM  0,15,RTM2RREG(RTM2PTR)                                    01434000
*                 RTM2TRRA=ADDR(RECURSE);/* ADDRESS OF RECURSION   0472 01435000
*                                      HANDLRE                       */ 01436000
         LA    @12,RECURSE                                         0472 01437000
         ST    @12,RTM2TRRA(,RTM2PTR)                              0472 01438000
*                 CALL IEAPTERM;    /* PURGE PAGE-IN                 */ 01439000
         L     @12,CVTPTR                                          0473 01440000
         L     @12,SCVTSPTR(,@12)                                  0473 01441000
         L     @15,SCVTPTRM(,@12)                                  0473 01442000
         BALR  @14,@15                                             0473 01443000
*PAGERET:                                                          0474 01444000
*                 RESPECIFY                                        0474 01445000
*                   R1 UNRESTRICTED;                               0474 01446000
PAGERET  DS    0H                                                  0475 01447000
*                 RTM2TRRA=ZERO;    /* ZERO RECURSION ADDRESS        */ 01448000
         SLR   @12,@12                                             0475 01449000
         ST    @12,RTM2TRRA(,RTM2PTR)                              0475 01450000
*                 RTM2PPS=OFF;      /* TURN OFF RESOURCE MANAGER   0476 01451000
*                                      RECURSION FLAG                */ 01452000
         NI    RTM2PPS(RTM2PTR),B'11110111'                        0476 01453000
*                 RMPLIPUF=OFF;     /* RESET FLAG                    */ 01454000
*                 RMPLRCOV=OFF;     /* RESET FLAG                    */ 01455000
         NI    RMPLIPUF+712(RTM2PTR),B'11100111'                   0478 01456000
*                 IF RTM2IOQR=ON THEN/* TEST IF I/O PREVIOUSLY     0479 01457000
*                                      QUIESCED                      */ 01458000
         TM    RTM2IOQR(RTM2PTR),B'10000000'                       0479 01459000
         BNO   @RF00479                                            0479 01460000
*                   DO;             /* I/O PREVIOUSLY QUIESCED, IT 0480 01461000
*                                      MUST NOW BE RESTORED          */ 01462000
*                     IF RTM2FIOB^=FOX THEN/* TEST IF ANY RESTORABLE    01463000
*                                      I/O                           */ 01464000
         CLC   RTM2FIOB(3,RTM2PTR),@CF00087+1                      0481 01465000
         BE    @RF00481                                            0481 01466000
*                       DO;         /* RESTORABLE I/O                */ 01467000
*                         RESPECIFY                                0483 01468000
*                          (R1) RESTRICTED;                        0483 01469000
*                         R1=ADDR(RTM2IOBP);/* POINTER TO PURGED I/O    01470000
*                                      REQUEST LIST          @YM07293*/ 01471000
         LA    R1,RTM2IOBP(,RTM2PTR)                               0484 01472000
*                         RTM2IORS=ON;/* SVC 17 RECURSION FLAG       */ 01473000
         OI    RTM2IORS(RTM2PTR),B'00100000'                       0485 01474000
*                         RTM2TRRA=ADDR(RECURSE);/* ADDRESS OF     0486 01475000
*                                      RECURSION HANDLER             */ 01476000
         LA    @12,RECURSE                                         0486 01477000
         ST    @12,RTM2TRRA(,RTM2PTR)                              0486 01478000
*                         GENERATE;                                0487 01479000
         STM   0,15,RTM2RREG(RTM2PTR)                                   01480000
         SVC   17                                                       01481000
*RSTORET:                                                          0488 01482000
*                         RTM2TRRA=ZERO;/* ZERO RECURSION ADDRESS    */ 01483000
