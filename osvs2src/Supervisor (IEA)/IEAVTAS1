*             IF SCBPKEY<KEY8 THEN  /* TEST FOR KEY 0 - 7            */ 01754000
@RC00578 CLI   SCBPKEY(SCBPTR),X'80'                               0581 01755000
         BNL   @RF00581                                            0581 01756000
*               TCBSCBKY=SUPERKEY;  /* PUT KEY 0 IN TCB              */ 01757000
         MVI   TCBSCBKY(TCBPTR),X'00'                              0582 01758000
*             ELSE                  /* KEY 8 - 15                    */ 01759000
*               TCBSCBKY=SCBPKEY;   /* PUT KEY IN TCB                */ 01760000
         B     @RC00581                                            0583 01761000
@RF00581 MVC   TCBSCBKY(1,TCBPTR),SCBPKEY(SCBPTR)                  0583 01762000
*             RESPECIFY                                            0584 01763000
*              (TCBPTR) UNRESTRICTED;                              0584 01764000
@RC00581 DS    0H                                                  0585 01765000
*             RTM2TRRA=ADDR(RECURSE);/* ADDRESS OF POST EXIT       0585 01766000
*                                      PROCESSING ROUTINE            */ 01767000
         LA    @12,RECURSE                                         0585 01768000
         ST    @12,RTM2TRRA(,RTM2PTR)                              0585 01769000
*           END;                                                   0586 01770000
*           RESPECIFY                                              0587 01771000
*            (R14,                                                 0587 01772000
*             R15) RESTRICTED;                                     0587 01773000
*           R15=SCBEXIT;            /* USER EXIT ROUTINE ADDRESS     */ 01774000
         L     R15,SCBEXIT(,SCBPTR)                                0588 01775000
*           GENERATE;                                              0589 01776000
         SYNCH (15)                                                     01777000
*           RESPECIFY                                              0590 01778000
*            (R2,                                                  0590 01779000
*             R14) UNRESTRICTED;                                   0590 01780000
*                                                                  0590 01781000
*           /*********************************************************/ 01782000
*           /*                                                       */ 01783000
*           /* POSTYNC LOCATES THE CURRENT RTM2WA BY STARTING FROM   */ 01784000
*           /* THE PSA BECAUSE THE REGISTERS MAY HAVE BEEN DESTROYED */ 01785000
*           /* BY THE USER'S EXIT. IT THEN CLEARS THE RECURSION      */ 01786000
*           /* ENVIRONMENT AND SAVES THE VOLATILE REGISTERS RETURNED */ 01787000
*           /* FROM THE EXIT IN THE WORKAREA                         */ 01788000
*           /*                                                       */ 01789000
*           /*********************************************************/ 01790000
*                                                                  0591 01791000
*           DO;                     /* BEGIN SEGMENT POSTSYNC        */ 01792000
*             GENERATE REFS(FLCCVT,CVTTCBP,TCBRTWA,RTM2PTR,RTM2RREG);   01793000
         L     I,FLCCVT-PSA        ACCESS CVT POINTER                   01794000
         L     I,CVTTCBP(,I)       TCB DOUBLE WORD                      01795000
         L     I,4(I)              CURRENT TCB                          01796000
         L     RTM2PTR,TCBRTWA(,I) CURRENT RTM2WA                       01797000
         LM    1,13,RTM2RREG+4(RTM2PTR) RESTORE REGISTERS TO PRE-SYNCH  01798000
*             RTM2TRRA=ZERO;        /* CLEAR RECURSION ENVIRONMENT   */ 01799000
         SLR   @14,@14                                             0593 01800000
         ST    @14,RTM2TRRA(,RTM2PTR)                              0593 01801000
*             RTM2XIP=OFF;                                         0594 01802000
         NI    RTM2XIP(RTM2PTR),B'11011111'                        0594 01803000
*             RTM2TCBC->TCBSTABE=OFF;                              0595 01804000
         L     @14,RTM2TCBC(,RTM2PTR)                              0595 01805000
         NI    TCBSTABE(@14),B'01111111'                           0595 01806000
*             RTM2RCDE=R15;         /* SAVE REGISTER 15 WHICH MAY  0596 01807000
*                                      CONTAIN ACTION CODE           */ 01808000
         STC   R15,RTM2RCDE(,RTM2PTR)                              0596 01809000
*             RTM2RTYA=R0;          /* SAVE REGISTER 0 WHICH MAY   0597 01810000
*                                      CONTAIN RETRY ADDRESS         */ 01811000
         ST    R0,RTM2RTYA(,RTM2PTR)                               0597 01812000
*             IF SCBSTAR=ON&RTM2WANA=ON THEN                       0598 01813000
         TM    SCBSTAR(SCBPTR),B'01000000'                         0598 01814000
         BNO   @RF00598                                            0598 01815000
         TM    RTM2WANA(RTM2PTR),B'00100000'                       0598 01816000
         BNO   @RF00598                                            0598 01817000
*               IF RTM2RCDE=SDWACWT THEN                           0599 01818000
         CLI   RTM2RCDE(RTM2PTR),0                                 0599 01819000
         BNE   @RF00599                                            0599 01820000
*                 SCBINUSE=ON;      /* MARK ESTAR SCB INUSE          */ 01821000
         OI    SCBINUSE(SCBPTR),B'00010000'                        0600 01822000
*           END;                    /* END OF POSTSYNCH              */ 01823000
@RF00599 DS    0H                                                  0601 01824000
@RF00598 DS    0H                                                  0602 01825000
*           RESPECIFY                                              0602 01826000
*            (R0,                                                  0602 01827000
*             R1,                                                  0602 01828000
*             R15) UNRESTRICTED;                                   0602 01829000
*         END;                      /* END EXITINTR                  */ 01830000
*CALLAS2:                                                          0604 01831000
*         GEN(STM 0,15,RTM2RREG(RTM2PTR));                         0604 01832000
CALLAS2  STM 0,15,RTM2RREG(RTM2PTR)                                     01833000
*         R13=ADDR(RTM2TRSA);       /* SET UP SAVE AREA - REGISTERS     01834000
*                                      CONTAINING POINTERS TO THE  0605 01835000
*                                      RTM2 WORKAREA, SDWA AND SCB 0605 01836000
*                                      ARE GLOBALLY RESTRICTED       */ 01837000
         LA    R13,RTM2TRSA(,RTM2PTR)                              0605 01838000
*         CALL IEAVTAS2;            /* PERFORM RECOVERY REQUESTS     */ 01839000
