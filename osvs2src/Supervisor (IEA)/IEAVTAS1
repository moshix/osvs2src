         L     @15,RTM2PREV(,@15)                                  0715 02247000
         ST    @15,RTM2PRWA(,RTM2PTR)                              0715 02248000
*           END;                    /* END RTM2 WORKAREA SEARCH      */ 02249000
@DE00714 L     @15,RTM2PRWA(,RTM2PTR)                              0716 02250000
         TM    RTM2XFLG(@15),B'00001000'                           0716 02251000
         BZ    @DL00714                                            0716 02252000
*           IF RTM2PRWA->RTM2WAIN=OFF THEN/* SDWA NOT INVALID        */ 02253000
