*                             ELSE  /* THERE IS NO XTENT LIST        */ 00674000
*                               CDENTPTR=CDCHAIN;/* GET NEXT CDE     */ 00675000
         B     @RC00240                                            0249 00676000
@RF00240 L     CDENTPTR,CDCHAIN(,CDENTPTR)                         0249 00677000
*                           ELSE    /* CDE IS A MINOR CDE            */ 00678000
*                             CDENTPTR=CDCHAIN;/* GET NEXT CDE       */ 00679000
         B     @RC00239                                            0250 00680000
@RF00239 L     CDENTPTR,CDCHAIN(,CDENTPTR)                         0250 00681000
*                         ELSE      /* MATCH FOUND                   */ 00682000
*                           DO;     /* INITIALIZE RTM2 WORKAREA      */ 00683000
