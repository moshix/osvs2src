*                       ELSE        /* INITIALIZE SEARCH OF LPAQ     */ 00541000
*                         DO;                                      0222 00542000
         B     @RC00208                                            0222 00543000
@RF00208 DS    0H                                                  0223 00544000
*                           RTM2LPAQ=ON;/* SEARCH LPAQ INDICATOR     */ 00545000
         OI    RTM2LPAQ(RTM2PTR),B'00000100'                       0223 00546000
*                           ESAGLREC=ON;/* SET UP RECURSION        0224 00547000
*                                      ENVIRONMENT           @YM01819*/ 00548000
         L     @12,RTM2VRBC(,RTM2PTR)                              0224 00549000
         OI    ESAGLREC+96(@12),B'00010000'                        0224 00550000
*                           DO;     /* SETLOCK (OBTAIN) MODE(UNCOND)    00551000
*                                      TYPE(CMS) RELATED(LPAQ,IEAVTAS   00552000
*                                      1(CDESRCH))                   */ 00553000
*                             RFY                                  0226 00554000
*                              (GPR11P,                            0226 00555000
*                               GPR12P,                            0226 00556000
*                               GPR13P,                            0226 00557000
*                               GPR14P) RSTD;                      0226 00558000
