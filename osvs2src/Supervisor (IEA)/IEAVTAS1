         BO    @RF00574                                            0574 01735000
*               SCBINUSE=ON;        /* MARK CURRENT SCB IN USE       */ 01736000
         OI    SCBINUSE(SCBPTR),B'00010000'                        0575 01737000
*             GEN(STM 0,15,RTM2RREG(RTM2PTR));                     0576 01738000
@RF00574 DS    0H                                                  0576 01739000
         STM 0,15,RTM2RREG(RTM2PTR)                                     01740000
*             TCBSYNCH=ON;          /* INDICATE TO SYNCH THAT TASK 0577 01741000
*                                      RECOVERY IS CALLER            */ 01742000
         OI    TCBSYNCH(TCBPTR),B'00000100'                        0577 01743000
*             IF SCBSUPER=ON THEN   /* TEST IF SUPERVISOR MODE       */ 01744000
