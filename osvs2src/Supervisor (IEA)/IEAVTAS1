*             RESPECIFY                                            0568 01720000
*              (TCBPTR) RESTRICTED;                                0568 01721000
*             TCBPTR=RTM2TCBC;      /* INITIALIZE CURRENT TCB        */ 01722000
         L     TCBPTR,RTM2TCBC(,RTM2PTR)                           0569 01723000
*             RTM2XIP=ON;           /* INDICATE EXIT IN PROGRESS     */ 01724000
*             RTM2XFLG=ON;          /* EXIT SECTION FLAG FOR       0571 01725000
*                                      RECURSION                     */ 01726000
         OI    RTM2XIP(RTM2PTR),B'00101000'                        0571 01727000
*             IF SCBESTAE=OFF THEN  /* TEST IF STAE EXIT     @YM01804*/ 01728000
         TM    SCBESTAE(SCBPTR),B'00010000'                        0572 01729000
         BNZ   @RF00572                                            0572 01730000
*               TCBSTABE=ON;        /* INDICATE STAE EXIT    @YM01804*/ 01731000
         OI    TCBSTABE(TCBPTR),B'10000000'                        0573 01732000
*             IF SCBSTAR^=ON THEN   /* IF ESTAR, DO NOT SET INUSE    */ 01733000
