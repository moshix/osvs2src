         BALR GPR14P,GPR13P                                             00764000
*                       RFY                                        0276 00765000
*                        (GPR11P,                                  0276 00766000
*                         GPR12P,                                  0276 00767000
*                         GPR13P,                                  0276 00768000
*                         GPR14P) UNRSTD;                          0276 00769000
*                     END;          /* SETLOCK (RELEASE) TYPE(LOCAL)    00770000
*                                      RELATED(JPAQ,IEAVTAS1(CDESRCH)   00771000
*                                      )                     @YM01819*/ 00772000
*                     ESAGLREC=OFF; /* TURN OFF RECURSION FLAG     0278 00773000
*                                                            @YM01819*/ 00774000
         L     @12,RTM2VRBC(,RTM2PTR)                              0278 00775000
         NI    ESAGLREC+96(@12),B'11101111'                        0278 00776000
*                     RESPECIFY                                    0279 00777000
*                       CDENTPTR UNRESTRICTED;                     0279 00778000
*                   END;            /* END OF IRB PROCESSING         */ 00779000
*                                                                  0280 00780000
*                 /***************************************************/ 00781000
*                 /*                                                 */ 00782000
*                 /* FOR AN SVRB, PROGRAM NAME AND ENTRY POINT ARE   */ 00783000
*                 /* ZEROES                                          */ 00784000
*                 /*                                                 */ 00785000
*                 /***************************************************/ 00786000
*                                                                  0281 00787000
*               END;                /* END OF RB TYPE SEARCH         */ 00788000
@RF00201 DS    0H                                                  0282 00789000
*             ;                     /* END CDE SEARCH                */ 00790000
@RC00191 DS    0H                                                  0283 00791000
*           END;                                                   0283 00792000
*         RESPECIFY                                                0284 00793000
*           RBSECPTR UNRESTRICTED;                                 0284 00794000
@RF00185 DS    0H                                                  0285 00795000
*         IF SCBPRNTR=ON THEN                                      0285 00796000
         TM    SCBPRNTR(SCBPTR),B'00001000'                        0285 00797000
         BNO   @RF00285                                            0285 00798000
*           RTM2STAE=ON;                                           0286 00799000
         OI    RTM2STAE(RTM2PTR),B'00100000'                       0286 00800000
*         ELSE                                                     0287 00801000
*           RTM2STAE=OFF;                                          0287 00802000
         B     @RC00285                                            0287 00803000
@RF00285 NI    RTM2STAE(RTM2PTR),B'11011111'                       0287 00804000
*         ;                         /* END WKUPDAT                   */ 00805000
*                                                                  0288 00806000
@RC00285 DS    0H                                                  0289 00807000
*         /***********************************************************/ 00808000
*         /*                                                         */ 00809000
*         /* SEGMENT SDWAINIT WILL OBTAIN AND INITIALIZE THE         */ 00810000
*         /* SDWA(RTCA). IT FIRST ESTABLISHES RECURSION CONTROL BY   */ 00811000
*         /* STORING REGISTERS IN RTM2 WORKAREA, INITIALIZING A      */ 00812000
*         /* GETMAIN RECURSION FLAG, AND ESTABLISHING A RECURSION    */ 00813000
*         /* ADDRESS. IT THEN INCLUDES SEGMENTS GETSDWA TO OBTAIN THE*/ 00814000
*         /* WORKAREA, SDWADATA TO INITIALIZE SDWA WITH DATA FROM    */ 00815000
*         /* VARIOUS RESOURCES, CNVRTPSW TO CONVERT EC MODE PSW TO BC*/ 00816000
*         /* MODE AND FINALLY, SDWARTM2 TO INITIALIZE SDWA WITH      */ 00817000
*         /* APPLICABLE DATA FROM RTM2 WORKAREA ALSO, A POINTER TO   */ 00818000
*         /* THE VARIABLE RECORDING AREA WITHIN THE SDWA IS          */ 00819000
*         /* INITIALIZED.                                            */ 00820000
*         /*                                                         */ 00821000
*         /***********************************************************/ 00822000
*                                                                  0289 00823000
*         DO;                       /* BEGIN SDWAINIT                */ 00824000
*                                                                  0289 00825000
*           /*********************************************************/ 00826000
*           /*                                                       */ 00827000
*           /* SEGMENT GETSDWA WILL PERFORM THE GETMAIN FOR THE      */ 00828000
*           /* SDWA(RTCA). IF THE USER IS IN SUPERVISOR MODE, THE    */ 00829000
*           /* REQUEST WILL BE FROM SUBPOOL 230. IF THE USER IS IN   */ 00830000
*           /* P/P MODE THE REQUEST WILL BE FROM SUBPOOL 0. IF THE   */ 00831000
*           /* GETMAIN IS SUCCESSFUL THE SUBPOOL ID AND LENGTH WILL  */ 00832000
*           /* BE STORED IN RTM2 WORKAREA. IF CORE IS NOT AVAILABLE A*/ 00833000
*           /* FLAG IS SET IN RTM2 WORKAREA.                         */ 00834000
*           /*                                                       */ 00835000
*           /*********************************************************/ 00836000
*                                                                  0290 00837000
*           DO;                     /* BEGIN GETSDWA                 */ 00838000
*             R15=4;                /* SET RC IN CASE GETMAIN      0291 00839000
*                                      RECURSION, SO RTM2WANA IS SET    00840000
*                                      ON                    @ZA01940*/ 00841000
         LA    R15,4                                               0291 00842000
*             IF RTM2GMR=OFF THEN   /* NOT GETMAIN RECURSION @ZA01940*/ 00843000
         TM    RTM2GMR(RTM2PTR),B'00100000'                        0292 00844000
         BNZ   @RF00292                                            0292 00845000
*               DO;                 /* OK TO GET SDWA        @ZA01940*/ 00846000
*                 IF SCBESTAE=ON&SCBPKEY<KEY8 THEN/* DETERMINE USER     00847000
*                                      MODE                          */ 00848000
         TM    SCBESTAE(SCBPTR),B'00010000'                        0294 00849000
         BNO   @RF00294                                            0294 00850000
         CLI   SCBPKEY(SCBPTR),X'80'                               0294 00851000
         BNL   @RF00294                                            0294 00852000
*                   RTM2SUBP=SUPERSP;/* GETMAIN FROM SP 230          */ 00853000
         MVI   RTM2SUBP(RTM2PTR),X'E6'                             0295 00854000
*                 ELSE                                             0296 00855000
*                   RTM2SUBP=PPSP;                                 0296 00856000
         B     @RC00294                                            0296 00857000
@RF00294 MVI   RTM2SUBP(RTM2PTR),X'FA'                             0296 00858000
*                 RTM2SIZE=LENGTH(SDWA)+USAVE;/* GETMAIN FROM SP 0   */ 00859000
@RC00294 MVC   RTM2SIZE(3,RTM2PTR),@CF03307+1                      0297 00860000
*                 RESPECIFY                                        0298 00861000
*                  (R0,                                            0298 00862000
*                   R1,                                            0298 00863000
*                   R15) RESTRICTED;                               0298 00864000
*                 GEN(STM  0,15,RTM2RREG(RTM2PTR));                0299 00865000
         STM  0,15,RTM2RREG(RTM2PTR)                                    00866000
*                 RTM2GMS=ON;       /* TURN ON SECTION FLAG          */ 00867000
         OI    RTM2GMS(RTM2PTR),B'00010000'                        0300 00868000
*                 RTM2TRRA=ADDR(RECURSE);/* RECURSION ADDRESS        */ 00869000
         LA    @14,RECURSE                                         0301 00870000
         ST    @14,RTM2TRRA(,RTM2PTR)                              0301 00871000
*                 DO;               /* GETMAIN (RC) LV(RTM2SIZE)   0302 00872000
*                                      SP(RTM2SUBP)                  */ 00873000
*                   RESPECIFY                                      0303 00874000
*                    (GPR01F,                                      0303 00875000
*                     GPR15F,                                      0303 00876000
*                     GPR00F) RESTRICTED;                          0303 00877000
*                   GPR01F=0;       /* REG 1 MUST BE ZERO            */ 00878000
         SLR   GPR01F,GPR01F                                       0304 00879000
*                   GPR15F=0;       /* RC-TYPE GETMAIN               */ 00880000
         SLR   GPR15F,GPR15F                                       0305 00881000
*                   GPR00F=RTM2SIZE;/* LENGTH REQUESTED              */ 00882000
         L     GPR00F,RTM2SIZE-1(,RTM2PTR)                         0306 00883000
         ICM   GPR00F,8,@CB03336                                   0306 00884000
*                   GPR15F=GPR15F|((RTM2SUBP)*256);/* SP IN BYTE 2   */ 00885000
         SLR   @14,@14                                             0307 00886000
         IC    @14,RTM2SUBP(,RTM2PTR)                              0307 00887000
         SLA   @14,8                                               0307 00888000
         OR    GPR15F,@14                                          0307 00889000
*                   SVC(120);       /* RC/RU-FORM OF GETMAIN         */ 00890000
         SVC   120                                                 0308 00891000
*                   RESPECIFY                                      0309 00892000
*                    (GPR01F,                                      0309 00893000
*                     GPR15F,                                      0309 00894000
*                     GPR00F) UNRESTRICTED;                        0309 00895000
*                 END;              /* GETMAIN (RC) LV(RTM2SIZE)   0310 00896000
*                                      SP(RTM2SUBP)                  */ 00897000
*GMRETURN:                          /* RETURN ADDRESS IF RECURSION   */ 00898000
*                 RTM2TRRA=ZERO;    /* ZERO RECURSION ADDRESS        */ 00899000
GMRETURN SLR   @14,@14                                             0311 00900000
         ST    @14,RTM2TRRA(,RTM2PTR)                              0311 00901000
*                 RTM2GMS=OFF;      /* ZERO SECTION FLAG             */ 00902000
         NI    RTM2GMS(RTM2PTR),B'11101111'                        0312 00903000
*               END;                /* SDWA OBTAINED         @ZA01940*/ 00904000
*             IF R15^=ZERO THEN     /* TEST IF CORE AVAILABLE        */ 00905000
@RF00292 SLR   @14,@14                                             0314 00906000
         CR    R15,@14                                             0314 00907000
         BE    @RF00314                                            0314 00908000
*               DO;                 /* CORE NOT AVAILABLE            */ 00909000
*                 RTM2WANA=ON;      /* INDICATE CORE NOT AVAILABLE   */ 00910000
         OI    RTM2WANA(RTM2PTR),B'00100000'                       0316 00911000
*                 RTM2SPLL=ZERO;    /* ZERO SP ID AND LENGTH         */ 00912000
         ST    @14,RTM2SPLL(,RTM2PTR)                              0317 00913000
*                 RTM2RTCA=ZERO;    /* ZERO SDWA PTR IN RTM2WA       */ 00914000
         ST    @14,RTM2RTCA(,RTM2PTR)                              0318 00915000
*               END;                                               0319 00916000
*             ELSE                  /* IF CORE AVAILABLE             */ 00917000
*               DO;                                                0320 00918000
