         ALR   @11,@04                                             0242 00645000
         AL    @11,@CF03334                                        0242 00646000
         L     @11,XTLMSBAA(@12,@11)                               0242 00647000
         L     @12,XTLMSBLN-5(@12,XTLSTPTR)                        0242 00648000
         LA    @12,0(,@12)                                         0242 00649000
         ALR   @12,@11                                             0242 00650000
         L     @04,RBCDE1-1(,RBSECPTR)                             0242 00651000
         LA    @04,0(,@04)                                         0242 00652000
         CR    @12,@04                                             0242 00653000
         BNH   @RF00242                                            0242 00654000
         CR    @11,@04                                             0242 00655000
         BH    @RF00242                                            0242 00656000
*                                   DO;/* MATCH FOUND                */ 00657000
*                                    RTM2ABNM=CDNAME;/* ACCESS     0244 00658000
*                                      PROGRAM NAME                  */ 00659000
         MVC   RTM2ABNM(8,RTM2PTR),CDNAME(CDENTPTR)                0244 00660000
*                                    RTM2TRSW=ON;/* TRIGGER END OF 0245 00661000
*                                                              SEARCH*/ 00662000
         OI    RTM2TRSW(RTM2PTR),B'00010000'                       0245 00663000
*                                   END;/* END MATCH FOUND           */ 00664000
*                                 ELSE                             0247 00665000
*                                   CDENTPTR=CDCHAIN;/* GET NEXT CDE */ 00666000
         B     @RC00242                                            0247 00667000
@RF00242 L     CDENTPTR,CDCHAIN(,CDENTPTR)                         0247 00668000
*                               END;/* END XTENT LIST SEARCH         */ 00669000
@RC00242 AL    I,@CF00057                                          0248 00670000
@DE00241 C     I,XTLNRFAC(,XTLSTPTR)                               0248 00671000
