*                           ESAGLREC=OFF;/* TURN OFF RECURSION FLAG     00528000
*                                                            @YM01819*/ 00529000
         L     @12,RTM2VRBC(,RTM2PTR)                              0219 00530000
         NI    ESAGLREC+96(@12),B'11101111'                        0219 00531000
*                           CDENTPTR=RTM2TCBC->TCBJSTCB->TCBJPQB;/*     00532000
