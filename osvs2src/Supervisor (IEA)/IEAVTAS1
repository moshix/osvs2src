         L     CDENTPTR,PTRCDE(,@12)                               0233 00587000
*                         END;                                     0234 00588000
*                                                                  0234 00589000
*                       /*********************************************/ 00590000
*                       /*                                           */ 00591000
*                       /* SEGMENT JLPASRCH SEARCHES THE CDE CHAIN OF*/ 00592000
*                       /* THE JOB PACK AREA QUEUE (JPAQ) AND THE    */ 00593000
*                       /* LINK PACK AREA QUEUE IN AN ATTEMPT TO     */ 00594000
*                       /* RELATE AN ENTRY POINT ADDRESS IN THE IRB  */ 00595000
*                       /* TO THE STORAGE DESCRIBED BY A CDE.        */ 00596000
*                       /*                                           */ 00597000
*                       /*********************************************/ 00598000
*                                                                  0235 00599000
*                       DO WHILE(CDENTPTR^=NULL&RTM2TRSW^=ON);/* THIS   00600000
*                                      SEARCH WILL CONTINUE UNTIL A     00601000
*                                      CDE IS FOUND OR THE CDE QUEUE    00602000
*                                      IS EXHAUSTED                  */ 00603000
@RC00208 B     @DE00235                                            0235 00604000
@DL00235 DS    0H                                                  0236 00605000
*                         RESPECIFY                                0236 00606000
*                          (XTLSTPTR,                              0236 00607000
*                           I) RESTRICTED;                         0236 00608000
*                         XTLSTPTR=CDXLMJP;/* INITIALIZE XTENT LIST     00609000
*                                      POINTER                       */ 00610000
         L     XTLSTPTR,CDXLMJP(,CDENTPTR)                         0237 00611000
*                         IF CDENTPT^=RBEP THEN/* TEST IF PROGRAM-S     00612000
*                                      ENTRY POINT IS EQUAL TO IRB 0238 00613000
