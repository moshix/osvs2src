         TITLE 'IEAVTAS1 - PRE EXIT PROCESSING                         *00001000
                        '                                               00002000
IEAVTAS1 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVTAS1  77.327'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @10,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@10                                         0001 00013000
RBPRFXLN EQU 64                                                         00014000
RBPRFLNA EQU 32                                                         00015000
PRBLEN   EQU 136                                                        00016000
SIRBLEN  EQU 200                                                        00017000
TIRBLEN  EQU 136                                                        00018000
IRBLEN   EQU 128                                                        00019000
SVRBLEN  EQU 224                                                        00020000
*                                                                  0098 00021000
*   /*****************************************************************/ 00022000
*   /*                                                               */ 00023000
*   /* SEGMENT IEAVTAS1 IS THE TOP TASK RECOVERY SEGMENT. IT INCLUDES*/ 00024000
*   /* FINDSCB TO LOCATE THE SCB TO BE SCHEDULED, WKUPDAT TO         */ 00025000
*   /* INITIALIZE RTM2 WORKAREA WITH APPLICABLE DATA AND SDWAINIT TO */ 00026000
*   /* ACQUIRE AND INITIALIZE THE SDWA(RTCA) FOR THE USER EXIT. IT   */ 00027000
*   /* ALSO PERFORMS USER I/O OPTIONS (USEROPTS), HANDLES RECURSION  */ 00028000
*   /* (RECURSE) AND SETS UP THE INTERFACE TO THE USER EXIT ROUTINE  */ 00029000
*   /* (EXITINTR). WHEN CONTROL RETURNS TO IEAVTAS1 FROM THE USER    */ 00030000
*   /* EXIT IEAVTAS2 IS CALLED TO PERFORM USER RECOVERY REQUESTS, I.E*/ 00031000
*   /* RETRY OR CONTINUE WITH TERMINATION.                           */ 00032000
*   /*                                                               */ 00033000
*   /*****************************************************************/ 00034000
*                                                                  0099 00035000
*   RTM2PTR=INPUTREG;               /* SAVE INPUT PTR TO RTM2      0099 00036000
*                                      WORKAREA                      */ 00037000
         LR    RTM2PTR,INPUTREG                                    0099 00038000
*   IF RTM2RETR=ON THEN             /* THIS CALL TO SETUP RBS FOR  0100 00039000
*                                      RETRY                 @G33SPHW*/ 00040000
         TM    RTM2RETR(RTM2PTR),B'01000000'                       0100 00041000
         BNO   @RF00100                                            0100 00042000
*     DO;                           /* INVOKE TAS3 TO DO ACTUAL SETUP   00043000
*                                                            @G33SPHW*/ 00044000
*       SCBPTR=RTM2SCBC;            /* USE SCB FOUND IN LAST PASS  0102 00045000
*                                                            @G33SPHW*/ 00046000
         L     SCBPTR,RTM2SCBC(,RTM2PTR)                           0102 00047000
*       R13=ADDR(RTM2TRSA);         /* SAVEAREA FOR IEAVTAS3 @G33SPHW*/ 00048000
         LA    R13,RTM2TRSA(,RTM2PTR)                              0103 00049000
*       CALL IEAVTAS3;              /* INVOKE IEAVTAS3 FOR SETUP   0104 00050000
*                                                            @G33SPHW*/ 00051000
         L     @15,@CV03262                                        0104 00052000
         BALR  @14,@15                                             0104 00053000
*     END;                          /* END OF RETRY SETUP    @G33SPHW*/ 00054000
*   RESPECIFY                                                      0106 00055000
*     INPUTREG UNRESTRICTED;                                       0106 00056000
@RF00100 DS    0H                                                  0107 00057000
*   RTM2WARG=R3INDEX;               /* INDICATE REGISTER FOR RTM2PTR    00058000
*                                      DURING RECURSION              */ 00059000
         MVI   RTM2WARG(RTM2PTR),X'04'                             0107 00060000
*TOP:                                                              0108 00061000
*   DO WHILE(RTM2RETR=OFF&RTM2SCBC^=ZERO);                         0108 00062000
*                                                                  0108 00063000
TOP      B     @DE00108                                            0108 00064000
@DL00108 DS    0H                                                  0109 00065000
*     /***************************************************************/ 00066000
*     /*                                                             */ 00067000
*     /* SEGMENT FINDSCB WILL SEARCH THE SCB QUEUE WITHIN THE LIMITS */ 00068000
*     /* DEFINED IN THE RTM2 WORKAREA FIELDS RTM2SCBO AND RTM2SCBN   */ 00069000
*     /* UNITIL AN ELIGIBLE SCB ( SCHEDULEABLE USER EXIT) IS FOUND.  */ 00070000
*     /* THE SCB ADDRESS IS PUT IN RTM2SCBC UNLESS THERE IS NO       */ 00071000
*     /* ELIGIBLE SCB, IN WHICH CASE RTM2SCBC IS ZEROED              */ 00072000
*     /*                                                             */ 00073000
*     /***************************************************************/ 00074000
*                                                                  0109 00075000
*     DO;                           /* BEGIN SEGMENT FINSCB          */ 00076000
*       IF RTM2SCBC=SCBSTART THEN   /* TEST FOR INITIAL ENTRY        */ 00077000
         CLC   RTM2SCBC(4,RTM2PTR),@CF00057                        0110 00078000
         BNE   @RF00110                                            0110 00079000
*         RTM2SCBC=RTM2SCBN;        /* INITIALIZE RTM2SCBC           */ 00080000
         L     @12,RTM2SCBN(,RTM2PTR)                              0111 00081000
         ST    @12,RTM2SCBC(,RTM2PTR)                              0111 00082000
*       ELSE                                                       0112 00083000
*         RTM2SCBC=RTM2SCBC->SCBCHAIN;/* ACCESS CHAIN FIELD POINTER  */ 00084000
         B     @RC00110                                            0112 00085000
@RF00110 L     @12,RTM2SCBC(,RTM2PTR)                              0112 00086000
         L     @12,SCBCHAIN(,@12)                                  0112 00087000
         ST    @12,RTM2SCBC(,RTM2PTR)                              0112 00088000
*       RTM2TRSW=OFF;               /* INITIALIZE FOR LOOP CONTROL   */ 00089000
*                                                                  0113 00090000
@RC00110 NI    RTM2TRSW(RTM2PTR),B'11101111'                       0113 00091000
*       /*************************************************************/ 00092000
*       /*                                                           */ 00093000
*       /* SEARCH FOR AN ELIGIBLE SCB                                */ 00094000
*       /*                                                           */ 00095000
*       /*************************************************************/ 00096000
*                                                                  0114 00097000
*       DO WHILE(RTM2TRSW=OFF);     /* SWITCH WILL BE TURNED ON WHEN    00098000
*                                      AN ELIGIBLE SCB IS FOUND OR 0114 00099000
*                                      THE END OF THE SCB Q IS     0114 00100000
*                                      ENCOUNTERED                   */ 00101000
         B     @DE00114                                            0114 00102000
@DL00114 DS    0H                                                  0115 00103000
*         IF RTM2SCBO^=RTM2SCBC&RTM2SCBC->SCBID=SCBDB THEN/* TEST FOR   00104000
*                                      END OF QUEUE AND VALID SCB    */ 00105000
         L     @12,RTM2SCBC(,RTM2PTR)                              0115 00106000
         C     @12,RTM2SCBO(,RTM2PTR)                              0115 00107000
         BE    @RF00115                                            0115 00108000
         CLI   SCBID(@12),X'DB'                                    0115 00109000
         BNE   @RF00115                                            0115 00110000
*           IF RTM2SCBC->SCBINUSE^=ON THEN/* IF INUSE BIT IS ON, SCB    00111000
*                                      HAS BEEN PREVIOUSLY SCHEDULED */ 00112000
         TM    SCBINUSE(@12),B'00010000'                           0116 00113000
         BO    @RF00116                                            0116 00114000
*             IF RTM2TRME=ON THEN   /* TEST IF TERM PROCESSING IS IN    00115000
*                                      PROGRESS                      */ 00116000
         TM    RTM2TRME(RTM2PTR),B'10000000'                       0117 00117000
         BNO   @RF00117                                            0117 00118000
*               IF RTM2SCBC->SCBTERMI^=ON THEN/* TEST IF TERM SCB    */ 00119000
         TM    SCBTERMI(@12),B'01000000'                           0118 00120000
         BO    @RF00118                                            0118 00121000
*                 RTM2SCBC=RTM2SCBC->SCBCHAIN;/* NOT A TERM SCB - SO    00122000
*                                      GET NEXT SCB                  */ 00123000
         L     @12,SCBCHAIN(,@12)                                  0119 00124000
         ST    @12,RTM2SCBC(,RTM2PTR)                              0119 00125000
*               ELSE                                               0120 00126000
*                 RTM2TRSW=ON;      /* ELIGIBLE SCB FOUND            */ 00127000
         B     @RC00118                                            0120 00128000
@RF00118 OI    RTM2TRSW(RTM2PTR),B'00010000'                       0120 00129000
*             ELSE                                                 0121 00130000
*               IF RTM2STA2=ON THEN /* IF ON THEN ONLY AN ESTAE SCB     00131000
*                                      IS ELIGIBLE                   */ 00132000
         B     @RC00117                                            0121 00133000
@RF00117 TM    RTM2STA2(RTM2PTR),B'10000000'                       0121 00134000
         BNO   @RF00121                                            0121 00135000
*                 IF RTM2SCBC->SCBSTAI=OFF&RTM2SCBC->SCBESTAE=OFF THEN  00136000
         L     @12,RTM2SCBC(,RTM2PTR)                              0122 00137000
         TM    SCBSTAI(@12),B'10010000'                            0122 00138000
         BNZ   @RF00122                                            0122 00139000
*                                   /* INELIGIBLE IF THIS IS NOT AN     00140000
*                                      ESTAE                         */ 00141000
*                   RTM2SCBC=RTM2SCBC->SCBCHAIN;/* GET NEXT SCB      */ 00142000
         L     @12,SCBCHAIN(,@12)                                  0123 00143000
         ST    @12,RTM2SCBC(,RTM2PTR)                              0123 00144000
*                 ELSE              /* ESTAE TYPE SCB                */ 00145000
*                   RTM2TRSW=ON;    /* ELIGIBLE SCB FOUND            */ 00146000
         B     @RC00122                                            0124 00147000
@RF00122 OI    RTM2TRSW(RTM2PTR),B'00010000'                       0124 00148000
*               ELSE                                               0125 00149000
*                 RTM2TRSW=ON;      /* STAE SCB IS ELIGIBLE          */ 00150000
         B     @RC00121                                            0125 00151000
@RF00121 OI    RTM2TRSW(RTM2PTR),B'00010000'                       0125 00152000
*           ELSE                    /* IN USE FLAG ON                */ 00153000
*             RTM2SCBC=RTM2SCBC->SCBCHAIN;/* GET NEXT SCB            */ 00154000
         B     @RC00116                                            0126 00155000
@RF00116 L     @12,RTM2SCBC(,RTM2PTR)                              0126 00156000
         L     @12,SCBCHAIN(,@12)                                  0126 00157000
         ST    @12,RTM2SCBC(,RTM2PTR)                              0126 00158000
*         ELSE                      /* END OF SCB QUEUE              */ 00159000
*           DO;                                                    0127 00160000
         B     @RC00115                                            0127 00161000
@RF00115 DS    0H                                                  0128 00162000
*             RTM2SCBC=ZERO;        /* INDICATE END OF QUEUE         */ 00163000
         SLR   @12,@12                                             0128 00164000
         ST    @12,RTM2SCBC(,RTM2PTR)                              0128 00165000
*             RTM2TRSW=ON;          /* INDICATE END OF SEARCH        */ 00166000
         OI    RTM2TRSW(RTM2PTR),B'00010000'                       0129 00167000
*           END;                                                   0130 00168000
*       END;                                                       0131 00169000
@RC00115 DS    0H                                                  0131 00170000
@DE00114 TM    RTM2TRSW(RTM2PTR),B'00010000'                       0131 00171000
         BZ    @DL00114                                            0131 00172000
*       SCBPTR=RTM2SCBC;            /* INITIALIZE CURRENT SCB PTR    */ 00173000
         L     SCBPTR,RTM2SCBC(,RTM2PTR)                           0132 00174000
*       IF SCBPTR^=0 THEN                                          0133 00175000
         LTR   SCBPTR,SCBPTR                                       0133 00176000
         BZ    @RF00133                                            0133 00177000
*         IF SCBSTAI=OFF&SCBSTAR=OFF THEN/* TEST FOR (E)STAE SCB     */ 00178000
         TM    SCBSTAI(SCBPTR),B'11000000'                         0134 00179000
         BNZ   @RF00134                                            0134 00180000
*           DO;                     /* VALIDITY SCBOWNRA FIELD       */ 00181000
*             RFY                                                  0136 00182000
*               RBSECPTR RSTD;                                     0136 00183000
*             RBSECPTR=RTM2VRBC;    /* START RB QUEUE SEARCH WITH  0137 00184000
*                                      ABEND SVRB                    */ 00185000
         L     RBSECPTR,RTM2VRBC(,RTM2PTR)                         0137 00186000
*             DO WHILE(RBTCBNXT^=ON&SCBOWNRA^=RBSECPTR);/* SEARCH FOR   00187000
*                                      RB MATCH                      */ 00188000
         B     @DE00138                                            0138 00189000
@DL00138 DS    0H                                                  0139 00190000
*               RBSECPTR=RBLINKB;   /* UPDATE RB POINTER             */ 00191000
         L     @12,RBLINKB-1(,RBSECPTR)                            0139 00192000
         LA    @12,0(,@12)                                         0139 00193000
         LR    RBSECPTR,@12                                        0139 00194000
*             END;                  /* END RB SEARCH                 */ 00195000
@DE00138 TM    RBTCBNXT(RBSECPTR),B'10000000'                      0140 00196000
         BO    @DC00138                                            0140 00197000
         L     @12,SCBOWNRA-1(,SCBPTR)                             0140 00198000
         LA    @12,0(,@12)                                         0140 00199000
         CR    RBSECPTR,@12                                        0140 00200000
         BNE   @DL00138                                            0140 00201000
@DC00138 DS    0H                                                  0141 00202000
*             IF SCBOWNRA^=RBSECPTR THEN                           0141 00203000
         L     @12,SCBOWNRA-1(,SCBPTR)                             0141 00204000
         LA    @12,0(,@12)                                         0141 00205000
         CR    RBSECPTR,@12                                        0141 00206000
         BE    @RF00141                                            0141 00207000
*               RTM2SCBC=ZERO;      /* TEST IF RB MATCH FOUND        */ 00208000
         SLR   @12,@12                                             0142 00209000
         ST    @12,RTM2SCBC(,RTM2PTR)                              0142 00210000
*             RFY                                                  0143 00211000
*               RBSECPTR UNRSTD;                                   0143 00212000
@RF00141 DS    0H                                                  0144 00213000
*           END;                                                   0144 00214000
*     END;                          /* END FINDSCB                   */ 00215000
@RF00134 DS    0H                                                  0145 00216000
@RF00133 DS    0H                                                  0146 00217000
*     IF RTM2SCBC^=ZERO THEN        /* TEST IF ELIGIBLE SCB FOUND    */ 00218000
         SLR   @12,@12                                             0146 00219000
         C     @12,RTM2SCBC(,RTM2PTR)                              0146 00220000
         BE    @RF00146                                            0146 00221000
*       DO;                         /* ELIGIBLE SCB FOUND            */ 00222000
*                                                                  0147 00223000
*         /***********************************************************/ 00224000
*         /*                                                         */ 00225000
*         /* SEGMENT WKUPDAT WILL UPDATE RTM2 WORKAREA FOR THE USER  */ 00226000
*         /* EXIT. IT FIRST INCLUDES IRBSRCH WHICH WILL DETERMINE IF */ 00227000
*         /* THERE IS AN IRB PRECEDING THE RB WHOSE (E)STAE EXIT IS  */ 00228000
*         /* NOW IN CONTROL. FLAG RTM2IRB IS TURNED ON IF THERE IS   */ 00229000
*         /* ONE. NEXT IT IS DETERMINED IF THE RB ASSOCIATED WITH THE*/ 00230000
*         /* CURRENT SCB WAS IN CONTROL AT THE TIME OF ERROR. IF IT  */ 00231000
*         /* WAS NOT IN CONTROL FLAG RTM2NRBE IS SET. THE RB ADDRESS */ 00232000
*         /* OF THE FAILING PROGRAM IS PUT IN RTM2 WORKAREA IF IT IS */ 00233000
*         /* SUPERVISOR MODE. IF IT IS PP MODE THE NAME AND ENTRY    */ 00234000
*         /* POINT OF THE ABENDING PROGRAM IS ENTERED IN RTM2        */ 00235000
*         /* WORKAREA                                                */ 00236000
*         /*                                                         */ 00237000
*         /***********************************************************/ 00238000
*                                                                  0148 00239000
*         ;                         /* BEGIN WKUPDAT                 */ 00240000
*         IF RTM2IRB=OFF THEN       /* TEST IF INTERVENING IRB     0149 00241000
*                                      PREVIOUSLY ENCOUNTERD         */ 00242000
*                                                                  0149 00243000
         TM    RTM2IRB(RTM2PTR),B'00100000'                        0149 00244000
         BNZ   @RF00149                                            0149 00245000
*           /*********************************************************/ 00246000
*           /*                                                       */ 00247000
*           /* SEGMENT IRBSRCH DETERMINES IF THERE IS AN IRB         */ 00248000
*           /* PRECEDING THE RB WHOSE (E)STA EXIT IS NOW IN CONTROL. */ 00249000
*           /* IT ACCOMPLISHES THIS BY TESTING EACH RB UP TO THE     */ 00250000
*           /* CURRENT RB TO SEE IF IT IS AN IRB. FOR (E)STAE EXITS  */ 00251000
*           /* THE CURRENT RB IS THE RB ADDRESS IN THE CURRENT SCB.  */ 00252000
*           /* FOR (E)STAI/ESTAR EXITS THE CURRENT RB IS DEFINED AS  */ 00253000
*           /* THE ONE WITH THE RBTCBNXT FLAG ON.                    */ 00254000
*           /*                                                       */ 00255000
*           /*********************************************************/ 00256000
*                                                                  0150 00257000
*           DO;                     /* BEGIN IRBSRCH                 */ 00258000
*             IF RTM2RBST=ZERO THEN /* RTM2RBST IS THE STARTING    0151 00259000
*                                      ADDRESS FOR THE RB QUEUE    0151 00260000
*                                      SEARCH. IF IT IS ZERO,        */ 00261000
         C     @12,RTM2RBST(,RTM2PTR)                              0151 00262000
         BNE   @RF00151                                            0151 00263000
*               RTM2RBST=RTM2VRBC;  /* INITIALIZE IT WITH THE TOP RB    00264000
*                                      ON THE QUEUE RESUME SEARCH  0152 00265000
*                                      WITH NEXT RB ON THE QUEUE     */ 00266000
         L     @12,RTM2VRBC(,RTM2PTR)                              0152 00267000
         ST    @12,RTM2RBST(,RTM2PTR)                              0152 00268000
*             RESPECIFY                                            0153 00269000
*               RBSECPTR RESTRICTED;                               0153 00270000
@RF00151 DS    0H                                                  0154 00271000
*             RBSECPTR=RTM2RBST;    /* MAKE RTM2RBST CURRENT         */ 00272000
         L     RBSECPTR,RTM2RBST(,RTM2PTR)                         0154 00273000
*             IF(SCBSTAI=ON|SCBSTAR=ON) THEN/* TEST IF (E)STAI OR  0155 00274000
*                                      ESTAR                         */ 00275000
         TM    SCBSTAI(SCBPTR),B'11000000'                         0155 00276000
         BZ    @RF00155                                            0155 00277000
*               DO WHILE(RTM2IRB^=ON&/* SEARCH FOR AN IRB UNTIL THE  */ 00278000
*                     RBTCBNXT^=ON);/* END OF THE RB QUEUE IS REACHED*/ 00279000
         B     @DE00156                                            0156 00280000
@DL00156 DS    0H                                                  0157 00281000
*                 IF RBFTP='010'B THEN/* DETERMINE IF IRB            */ 00282000
         TM    RBFTP(RBSECPTR),B'01000000'                         0157 00283000
         BNO   @RF00157                                            0157 00284000
         TM    RBFTP(RBSECPTR),B'10100000'                         0157 00285000
         BNZ   @RF00157                                            0157 00286000
*                   RTM2IRB=ON;     /* INDICATE IRB IN RTM2 WORKAREA */ 00287000
         OI    RTM2IRB(RTM2PTR),B'00100000'                        0158 00288000
*                 ELSE                                             0159 00289000
*                   RBSECPTR=RBLINKB;/* UPDATE CURRENT RB POINTER    */ 00290000
         B     @RC00157                                            0159 00291000
@RF00157 L     @12,RBLINKB-1(,RBSECPTR)                            0159 00292000
         LA    @12,0(,@12)                                         0159 00293000
         LR    RBSECPTR,@12                                        0159 00294000
*               END;                /* END IRB SEARCH                */ 00295000
@RC00157 DS    0H                                                  0160 00296000
@DE00156 TM    RTM2IRB(RTM2PTR),B'00100000'                        0160 00297000
         BO    @DC00156                                            0160 00298000
         TM    RBTCBNXT(RBSECPTR),B'10000000'                      0160 00299000
         BNO   @DL00156                                            0160 00300000
@DC00156 DS    0H                                                  0161 00301000
*             ELSE                                                 0161 00302000
*               DO WHILE(RTM2IRB^=ON&/* SEARCH FOR AN IRB UNTIL THE  */ 00303000
*                     SCBOWNRA^=RBSECPTR);/* RB RELATED TO CURRENT 0161 00304000
*                                      SCB IS ENCOUNTERED            */ 00305000
         B     @RC00155                                            0161 00306000
@RF00155 B     @DE00161                                            0161 00307000
@DL00161 DS    0H                                                  0162 00308000
*                 IF RBFTP='010'B THEN/* DETERMINE IF IRB            */ 00309000
         TM    RBFTP(RBSECPTR),B'01000000'                         0162 00310000
         BNO   @RF00162                                            0162 00311000
         TM    RBFTP(RBSECPTR),B'10100000'                         0162 00312000
         BNZ   @RF00162                                            0162 00313000
*                   RTM2IRB=ON;     /* INDICATE IRB IN RTM2 WKAREA   */ 00314000
         OI    RTM2IRB(RTM2PTR),B'00100000'                        0163 00315000
*                 ELSE                                             0164 00316000
*                   RBSECPTR=RBLINKB;/* UPDATE CURRENT RB POINTER    */ 00317000
         B     @RC00162                                            0164 00318000
@RF00162 L     @12,RBLINKB-1(,RBSECPTR)                            0164 00319000
         LA    @12,0(,@12)                                         0164 00320000
         LR    RBSECPTR,@12                                        0164 00321000
*               END;                                               0165 00322000
@RC00162 DS    0H                                                  0165 00323000
@DE00161 TM    RTM2IRB(RTM2PTR),B'00100000'                        0165 00324000
         BO    @DC00161                                            0165 00325000
         L     @12,SCBOWNRA-1(,SCBPTR)                             0165 00326000
         LA    @12,0(,@12)                                         0165 00327000
         CR    RBSECPTR,@12                                        0165 00328000
         BNE   @DL00161                                            0165 00329000
@DC00161 DS    0H                                                  0166 00330000
*             RTM2RBST=RBSECPTR;                                   0166 00331000
@RC00155 ST    RBSECPTR,RTM2RBST(,RTM2PTR)                         0166 00332000
*             RESPECIFY                                            0167 00333000
*               RBSECPTR UNRESTRICTED;                             0167 00334000
*           END;                                                   0168 00335000
*                                                                  0168 00336000
*/*  DETERMINE IF CURRENT RB WAS IN CONTROL AT TIME OF ERROR         */ 00337000
*                                                                  0169 00338000
*         RESPECIFY                                                0169 00339000
*           RBSECPTR RESTRICTED;                                   0169 00340000
@RF00149 DS    0H                                                  0170 00341000
*         IF SCBSTAR=OFF&SCBSTAI=OFF THEN/* TEST FOR (E)STAE         */ 00342000
         TM    SCBSTAR(SCBPTR),B'11000000'                         0170 00343000
         BNZ   @RF00170                                            0170 00344000
*           IF SCBOWNRA^=RTM2VRBC->RBLINKB THEN                    0171 00345000
         L     @12,RTM2VRBC(,RTM2PTR)                              0171 00346000
         CLC   SCBOWNRA(3,SCBPTR),RBLINKB(@12)                     0171 00347000
         BE    @RF00171                                            0171 00348000
*             RTM2NRBE=ON;          /* CURRENT RB NOT IN CONTROL     */ 00349000
         OI    RTM2NRBE(RTM2PTR),B'01000000'                       0172 00350000
*           ELSE                                                   0173 00351000
*             ;                     /* TEST FOR ESTAR OR (E)STAI     */ 00352000
@RF00171 DS    0H                                                  0174 00353000
*         ELSE                                                     0174 00354000
*           IF SCBSTAR=ON THEN      /* TEST FOR ESTAR                */ 00355000
         B     @RC00170                                            0174 00356000
@RF00170 TM    SCBSTAR(SCBPTR),B'01000000'                         0174 00357000
         BNO   @RF00174                                            0174 00358000
*             DO;                                                  0175 00359000
*               RBSECPTR=RTM2VRBC;  /* INITIALIZE WITH CURRENT RB    */ 00360000
         L     RBSECPTR,RTM2VRBC(,RTM2PTR)                         0176 00361000
*               DO WHILE(RBTCBNXT^=ON);/* BEGIN SEARCH FOR OLDEST RB */ 00362000
         B     @DE00177                                            0177 00363000
@DL00177 DS    0H                                                  0178 00364000
*                 RBSECPTR=RBLINKB; /* GET NEXT RB                   */ 00365000
         L     @12,RBLINKB-1(,RBSECPTR)                            0178 00366000
         LA    @12,0(,@12)                                         0178 00367000
         LR    RBSECPTR,@12                                        0178 00368000
*               END;                /* END OF RB SEARCH              */ 00369000
@DE00177 TM    RBTCBNXT(RBSECPTR),B'10000000'                      0179 00370000
         BNO   @DL00177                                            0179 00371000
*               RTM2STAR=RBSECPTR;  /* PUT RB ADDRESS IN RTM2      0180 00372000
*                                      WORKAREA                      */ 00373000
         ST    RBSECPTR,RTM2STAR(,RTM2PTR)                         0180 00374000
*               IF RTM2STAR^=RTM2VRBC->RBLINKB THEN                0181 00375000
         L     @12,RTM2STAR(,RTM2PTR)                              0181 00376000
         L     @11,RTM2VRBC(,RTM2PTR)                              0181 00377000
         L     @11,RBLINKB-1(,@11)                                 0181 00378000
         LA    @11,0(,@11)                                         0181 00379000
         CR    @12,@11                                             0181 00380000
         BE    @RF00181                                            0181 00381000
*                 RTM2NRBE=ON;                                     0182 00382000
         OI    RTM2NRBE(RTM2PTR),B'01000000'                       0182 00383000
*             END;                                                 0183 00384000
*           ELSE                                                   0184 00385000
*             RTM2NRBE=ON;          /* (E)STAI IS TCB RELATED        */ 00386000
         B     @RC00174                                            0184 00387000
@RF00174 OI    RTM2NRBE(RTM2PTR),B'01000000'                       0184 00388000
*         IF RTM2BFTL=OFF THEN      /* TEST IF RTM2ABNM PREVIOUSLY 0185 00389000
*                                      INITIALIZED                   */ 00390000
@RC00174 DS    0H                                                  0185 00391000
@RC00170 TM    RTM2BFTL(RTM2PTR),B'00001000'                       0185 00392000
         BNZ   @RF00185                                            0185 00393000
*           DO;                     /* IF NOT BEGIN SEARCH           */ 00394000
*             RBSECPTR=RTM2VRBC->RBLINKB;                          0187 00395000
         L     @12,RTM2VRBC(,RTM2PTR)                              0187 00396000
         L     RBSECPTR,RBLINKB-1(,@12)                            0187 00397000
         LA    RBSECPTR,0(,RBSECPTR)                               0187 00398000
*             IF ADDR(RBOPSW)->PSWPPSUP^=ON THEN/* TEST FOR SUPRVSOR    00399000
*                                      MODE                          */ 00400000
         TM    PSWPPSUP+16(RBSECPTR),B'00000001'                   0188 00401000
         BO    @RF00188                                            0188 00402000
*               RTM2ABNM(1:4)=RBSECPTR;/* SUPERVISOR MODE - ACCESS 0189 00403000
*                                      ABENDING RB ADDRESS           */ 00404000
         STCM  RBSECPTR,15,RTM2ABNM(RTM2PTR)                       0189 00405000
*             ELSE                                                 0190 00406000
*                                                                  0190 00407000
*               /*****************************************************/ 00408000
*               /*                                                   */ 00409000
*               /* SEGMENT CDESRCH IS ENTERED IF THE USER IS IN PP   */ 00410000
*               /* MODE. IF THE ABENDING RB IS A PRB THE NAME AND    */ 00411000
*               /* ENTRY POINT OF THE PROGRAM ARE OBTAINED FROM THE  */ 00412000
*               /* RELATED CDE IF ONE EXISTS. IF THE ABENDING RB IS  */ 00413000
*               /* AN SIRB NO NAME IS PROVIDED AND THE ENTRY POINT IS*/ 00414000
*               /* OBTAINED FROM THE FAILING IQE. IF THE ABENDING RB */ 00415000
*               /* IS AN SVRB THE NAME FIELD IS SET TO ZEROES. IF THE*/ 00416000
*               /* ABENDING RB IS AN IRB THE CDE CHAIN OF THE JOB    */ 00417000
*               /* PACK AREA QUEUE AND THE LINK PACK AREA QUEUE ARE  */ 00418000
*               /* SEARCHED IN AN ATTEMPT TO RELATE AN ENTRY POINT   */ 00419000
*               /* ADDRESS IN THE IRB TO THE STORAGE DESCRIBED BY A  */ 00420000
*               /* CDE.                                              */ 00421000
*               /*                                                   */ 00422000
*               /*****************************************************/ 00423000
*                                                                  0190 00424000
*               ;                   /* BEGIN CDE SEARCH              */ 00425000
@RF00188 DS    0H                                                  0191 00426000
*             IF RBFTP='000'B THEN  /* TEST FOR PRB                  */ 00427000
         TM    RBFTP(RBSECPTR),B'11100000'                         0191 00428000
         BNZ   @RF00191                                            0191 00429000
*               IF RBCDE1^=NULL THEN/* TEST IF A CDE EXISTS FOR PRB  */ 00430000
