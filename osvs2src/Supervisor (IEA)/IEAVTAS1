*         RECRREG=ADDR(CALLAS2);    /* ENTRY POINT ADDRESS           */ 02202000
         LA    RECRREG,CALLAS2                                     0702 02203000
*         DO WHILE(RTM2PRWA->RTM2XFLG=OFF);/* LOCATE RTM2 WORKAREA IN   02204000
*                                      CONTROL WHEN USER EXIT INVOKED*/ 02205000
         B     @DE00703                                            0703 02206000
@DL00703 DS    0H                                                  0704 02207000
*           RTM2PRWA=RTM2PRWA->RTM2PREV;/* CHAIN THROUGH WORKAREAS   */ 02208000
         L     @15,RTM2PRWA(,RTM2PTR)                              0704 02209000
         L     @15,RTM2PREV(,@15)                                  0704 02210000
         ST    @15,RTM2PRWA(,RTM2PTR)                              0704 02211000
*         END;                      /* END RTM2 WORKAREA SEARCH      */ 02212000
@DE00703 L     @15,RTM2PRWA(,RTM2PTR)                              0705 02213000
         TM    RTM2XFLG(@15),B'00001000'                           0705 02214000
         BZ    @DL00703                                            0705 02215000
*         IF RTM2PRWA->RTM2WAIN=OFF THEN/* SDWA NOT INVALID          */ 02216000
         TM    RTM2WAIN(@15),B'01000000'                           0706 02217000
         BNZ   @RF00706                                            0706 02218000
*           SDWAPTR=RTM2PRWA->RTM2RTCA;/* INITIALIZE SDWAPTR         */ 02219000
         L     SDWAPTR,RTM2RTCA(,@15)                              0707 02220000
*         SCBPTR=RTM2PRWA->RTM2SCBC;/* INITIALIZE SCBPTR             */ 02221000
@RF00706 L     @15,RTM2PRWA(,RTM2PTR)                              0708 02222000
         L     SCBPTR,RTM2SCBC(,@15)                               0708 02223000
*       END;                        /* END AS2 RECURSION SET UP      */ 02224000
*     ELSE                          /* NOT AS2 RECURSION             */ 02225000
*       IF RTM2PRWA->RTM2FMS=ON|RTM2PRWA->RTM2RTYS=ON THEN/* TEST  0710 02226000
*                                      SECTION FLAGS FOR IEAVTAS3    */ 02227000
         B     @RC00699                                            0710 02228000
@RF00699 L     @15,RTM2PRWA(,RTM2PTR)                              0710 02229000
         TM    RTM2FMS(@15),B'00000001'                            0710 02230000
         BO    @RT00710                                            0710 02231000
         TM    RTM2RTYS(@15),B'01000000'                           0710 02232000
         BNO   @RF00710                                            0710 02233000
@RT00710 DS    0H                                                  0711 02234000
*         DO;                       /* AS3 RECURSION SET UP          */ 02235000
*           RTM2AS3R=ON;            /* SET AS3 RECURSION FLAG        */ 02236000
         OI    RTM2AS3R(RTM2PTR),B'00000001'                       0712 02237000
*           RECRREG=ADDR(CALLAS3);  /* ENTRY POINT ADDRESS           */ 02238000
         LA    RECRREG,CALLAS3                                     0713 02239000
*           DO WHILE(RTM2PRWA->RTM2XFLG=OFF);/* LOCATE RTM2 WORKAREA    02240000
*                                      IN CONTROL WHEN USER EXIT   0714 02241000
