*           IF SCBSTAR=ON THEN      /* TEST IF ESTAR EXIT            */ 01375000
*                                                                  0458 01376000
@RF00452 TM    SCBSTAR(SCBPTR),B'01000000'                         0458 01377000
         BNO   @RF00458                                            0458 01378000
*             /*******************************************************/ 01379000
*             /*                                                     */ 01380000
*             /* SEGMENT ESTRPROC HANDLES I/O PROCESSING FOR ESTAR   */ 01381000
*             /* EXITS. A RECURSION ENVIRONMENT IS ESTABLISHED BEFORE*/ 01382000
*             /* CALLING IEAPTERM TO PURGE PAGE IN. A TEST IS MADE TO*/ 01383000
*             /* DETERMINE WHETHER I/O HAS BEEN PREVIOUSLY QUIESCED. */ 01384000
*             /* IF IT HAS BEEN, A RECURSION ENVIRONMENT IS AGAIN    */ 01385000
*             /* ESTABLISHED AND SVC 17 IS ISSUED TO RESTORE THE     */ 01386000
*             /* RESTORABLE I/O. THE FIRST TIME LOGIC INDICATOR IS   */ 01387000
*             /* THEN ZEROED SO THAT ON RETURN TO SEGMENT USEROPTS   */ 01388000
*             /* HALT I/O (SVC 16) WILL BE PERFORMED.                */ 01389000
*             /*                                                     */ 01390000
*             /*******************************************************/ 01391000
*                                                                  0459 01392000
*             IF RTM2PPR=OFF THEN   /* IF NOT A PURGE PAGE-IN      0459 01393000
*                                      RECURSION CONTINUE WITH PURGE,   01394000
*                                      NO NEED TO TEST I/O         0459 01395000
*                                      RECURSION,A LATER TEST WILL 0459 01396000
*                                      PREVENT A RESTORE     @ZA01940*/ 01397000
         TM    RTM2PPR(RTM2PTR),B'01000000'                        0459 01398000
         BNZ   @RF00459                                            0459 01399000
*               DO;                 /* BEGIN ESTRPROC                */ 01400000
*                 RESPECIFY                                        0461 01401000
*                   R1 RESTRICTED;                                 0461 01402000
*                 R1=ADDR(RTM2RMPS);/* ADDRESS OF RESOURCE MANAGER 0462 01403000
*                                      PARAMETER LIST                */ 01404000
         LA    R1,RTM2RMPS(,RTM2PTR)                               0462 01405000
*                 R13=ADDR(RTM2RMSA);/* ADDRESS OF RESOURCE MANAGER     01406000
*                                      SAVE AREA                     */ 01407000
         LA    R13,RTM2RMSA(,RTM2PTR)                              0463 01408000
*                 RMPLIPUF=ON;      /* INHIBIT PAGE UNFIXES          */ 01409000
*                 RMPLRCOV=ON;      /* INDICATES TASK RECOVERY IS  0465 01410000
*                                      CALLER                        */ 01411000
         LA    @12,712                                             0465 01412000
         ALR   @12,RTM2PTR                                         0465 01413000
         OI    RMPLIPUF(@12),B'00011000'                           0465 01414000
*                 RMPLTCBA=RTM2TCBC;/* ADDRESS OF CURRENT TCB        */ 01415000
         L     @11,RTM2TCBC(,RTM2PTR)                              0466 01416000
         ST    @11,RMPLTCBA(,@12)                                  0466 01417000
*                 IF RTM2TCBC->TCBJSTCA=RTM2TCBC THEN/* DETERMINE IF    01418000
*                                      THIS IS A JOBSTEP TCB         */ 01419000
         L     @06,TCBJSTCA-1(,@11)                                0467 01420000
         LA    @06,0(,@06)                                         0467 01421000
         CR    @11,@06                                             0467 01422000
         BNE   @RF00467                                            0467 01423000
*                   RMPLJST=ON;     /* INDICATE JOBSTEP TCB          */ 01424000
         OI    RMPLJST(@12),B'00000100'                            0468 01425000
*                 ELSE              /* NOT JOBSTEP TCB               */ 01426000
*                   RMPLJST=OFF;    /* INDICATE NOT JOBSTEP TCB      */ 01427000
         B     @RC00467                                            0469 01428000
