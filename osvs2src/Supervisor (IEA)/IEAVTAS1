         ST    @12,RTM2ABEP(,RTM2PTR)                              0200 00463000
*                 /***************************************************/ 00464000
*                 /*                                                 */ 00465000
*                 /* FOR AN IRB, ATTEMPT TO RELATE THE ENTRY POINT TO*/ 00466000
*                 /* THE STORAGE DESCRIBED BY A CDE.                 */ 00467000
*                 /*                                                 */ 00468000
*                 /***************************************************/ 00469000
*                                                                  0201 00470000
*                 IF RBFTP='010'B THEN                             0201 00471000
@RF00199 TM    RBFTP(RBSECPTR),B'01000000'                         0201 00472000
         BNO   @RF00201                                            0201 00473000
         TM    RBFTP(RBSECPTR),B'10100000'                         0201 00474000
         BNZ   @RF00201                                            0201 00475000
*                   DO;                                            0202 00476000
*                     RTM2LPAQ=OFF; /* INITIALIZE LPAQ SEARCH SWITCH */ 00477000
*                     RTM2JPAQ=OFF; /* INITIALIZE JPAQ SEARCH SWITCH */ 00478000
*                     RTM2TRSW=OFF; /* INITIALIZE END OF SEARCH    0205 00479000
*                                                              SWITCH*/ 00480000
         NI    RTM2LPAQ(RTM2PTR),B'11101001'                       0205 00481000
*                     RESPECIFY                                    0206 00482000
*                       CDENTPTR RESTRICTED;                       0206 00483000
*                     DO WHILE(RTM2LPAQ^=ON/* SEARCH UNTIL BOTH LPAQ    00484000
*                                      AND                           */ 00485000
*                           &RTM2TRSW^=ON);/* JPAQ HAVE BEEN       0207 00486000
*                                      EXHAUSTED OR A RELATED CDE IS    00487000
