*                     DO;           /* QUIESCE REQUESTED             */ 01548000
*                       RTM2IOQS=ON;/* QUIESCE SECTION FLAG          */ 01549000
         OI    RTM2IOQS(RTM2PTR),B'10000000'                       0511 01550000
*                       GEN(STM 0,15,RTM2RREG(RTM2PTR));           0512 01551000
         STM 0,15,RTM2RREG(RTM2PTR)                                     01552000
*                       RTM2TRRA=ADDR(RECURSE);/* RECURSION ADDRESS  */ 01553000
