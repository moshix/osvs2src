         BNO   @RF00721                                            0721 02268000
*           DO;                     /* USER EXIT RECURSION SET UP    */ 02269000
*             RTM2XABD=ON;          /* INDICATE EXIT ABENDED         */ 02270000
         OI    RTM2XABD(RTM2PTR),B'00010000'                       0723 02271000
*             RECRREG=ADDR(CALLAS2);/* ENTRY POINT ADDRESS           */ 02272000
         LA    RECRREG,CALLAS2                                     0724 02273000
*             IF RTM2PRWA->RTM2WAIN=OFF THEN/* SDWA NOT INVALID      */ 02274000
