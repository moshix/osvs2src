*         END;                      /* END USEROPTS                  */ 01637000
*                                                                  0546 01638000
*         /***********************************************************/ 01639000
*         /*                                                         */ 01640000
*         /* SEGMENT EXITINTR WILL SET UP THE INTERFACE TO THE USER  */ 01641000
*         /* EXIT ROUTINE. IT FIRST INITIALIZES THE PARAMETER        */ 01642000
*         /* REGISTERS FOR THE EXIT AND THEN STORES ITS OWN REGISTERS*/ 01643000
*         /* IN THE RTM2 WORKAREA. SEGMENT PRESYNCH IS INCLUDED TO   */ 01644000
*         /* SET UP THE IEAVTAS1-SYNCH INTERFACE. FINALLY, SYNCH     */ 01645000
*         /* MACRO IS ISSUED TO GIVE THE USER EXIT CONTROL.          */ 01646000
*         /*                                                         */ 01647000
*         /***********************************************************/ 01648000
*                                                                  0547 01649000
*         DO;                       /* BEGIN EXITINTR                */ 01650000
*           RESPECIFY                                              0548 01651000
*            (R0,                                                  0548 01652000
*             R1,                                                  0548 01653000
*             R2) RESTRICTED;                                      0548 01654000
*           IF RTM2WANA^=ON THEN    /* TEST IF SDWA(RTCA) AVAILABLE  */ 01655000
         TM    RTM2WANA(RTM2PTR),B'00100000'                       0549 01656000
         BO    @RF00549                                            0549 01657000
*             DO;                                                  0550 01658000
*               IF RTM2IOQR=ON THEN /* IF I/O WAS QUIESCED ...       */ 01659000
         TM    RTM2IOQR(RTM2PTR),B'10000000'                       0551 01660000
         BNO   @RF00551                                            0551 01661000
*                 R0=CODE0;         /* REGISTER 0 = 0                */ 01662000
         SLR   R0,R0                                               0552 01663000
*               ELSE                                               0553 01664000
*                 IF RTM2IOHT=ON THEN/* IF I/O WAS HALTED ...        */ 01665000
         B     @RC00551                                            0553 01666000
@RF00551 TM    RTM2IOHT(RTM2PTR),B'01000000'                       0553 01667000
         BNO   @RF00553                                            0553 01668000
*                   R0=CODE4;       /* REGISTER 0 = 4                */ 01669000
         LA    R0,4                                                0554 01670000
*                 ELSE                                             0555 01671000
*                   IF RTM2NOIO=ON THEN/* IF NO ACTIVE I/O ...       */ 01672000
         B     @RC00553                                            0555 01673000
@RF00553 TM    RTM2NOIO(RTM2PTR),B'00100000'                       0555 01674000
         BNO   @RF00555                                            0555 01675000
*                     R0=CODE8;     /* REGISTER 0 = 8                */ 01676000
         LA    R0,8                                                0556 01677000
*                   ELSE            /* IF NONE OF THE AVOVE THEN NO     01678000
*                                      I/O PROCESSING PERFORMED      */ 01679000
*                     R0=CODE16;    /* REGISTER 0 = 16               */ 01680000
