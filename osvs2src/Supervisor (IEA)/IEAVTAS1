*                 RTM2RTCA->SDWA(257:512)=''B;                     0324 00929000
         XC    SDWA+256(256,@14),SDWA+256(@14)                     0324 00930000
*               END;                                               0325 00931000
*             RESPECIFY                                            0326 00932000
*              (R0,                                                0326 00933000
*               R1,                                                0326 00934000
*               R15) UNRESTRICTED;                                 0326 00935000
@RC00314 DS    0H                                                  0327 00936000
*           END;                    /* END GETSDWA                   */ 00937000
*           SDWAPTR=RTM2RTCA;       /* INITIALIZE SDWA PTR           */ 00938000
         L     SDWAPTR,RTM2RTCA(,RTM2PTR)                          0328 00939000
*           IF RTM2WANA=OFF THEN    /* TEST IF SDWA AVAILABLE        */ 00940000
         TM    RTM2WANA(RTM2PTR),B'00100000'                       0329 00941000
         BNZ   @RF00329                                            0329 00942000
*             DO;                   /* DO IF SDWA AVAILABLE          */ 00943000
*                                                                  0330 00944000
*               /*****************************************************/ 00945000
*               /*                                                   */ 00946000
*               /* SEGMENT SDWADATA INITIALIZES THE SDWA(RTCA) WITH  */ 00947000
*               /* APPLICABLE DATA FROM THE CURRENT SCB AND ITS      */ 00948000
*               /* RELATED RB.                                       */ 00949000
*               /*                                                   */ 00950000
*               /*****************************************************/ 00951000
*                                                                  0331 00952000
*               DO;                 /* BEGIN SDWADATA                */ 00953000
