*                     GPR01F=ADDR(RTM2RTCA->SDWA);/* SET REGISTER ONE*/ 01320000
         L     GPR01F,RTM2RTCA(,RTM2PTR)                           0414 01321000
*                     GPR15F=GPR15F+((RTM2SUBP)*256);/* SUBPOOL VALUE*/ 01322000
         SLR   @14,@14                                             0415 01323000
