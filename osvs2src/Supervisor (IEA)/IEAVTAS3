*                                                                  0278 00810000
*                           CALL IEECVPRG;                         0278 00811000
         L     @12,CVTPTR(,RTM2PTR)                                0278 00812000
         L     @12,SCVTSPTR(,@12)                                  0278 00813000
         L     @15,SCVTPGWR(,@12)                                  0278 00814000
         BALR  @14,@15                                             0278 00815000
*                       END;                                       0279 00816000
@RF00277 DS    0H                                                  0280 00817000
*                     CALL IEAVTPMT;/* PURGE TYPE 1 MESSAGE TABLE    */ 00818000
*                                                                  0280 00819000
@RC00150 L     @15,@CV00059                                        0280 00820000
         BALR  @14,@15                                             0280 00821000
*                     /***********************************************/ 00822000
*                     /*                                             */ 00823000
*                     /* RESET RMPL AFTER TASK RECOVERY PURGES       */ 00824000
*                     /*                                             */ 00825000
*                     /***********************************************/ 00826000
*                                                                  0281 00827000
*                     RMPLRCOV=OFF;                                0281 00828000
         NI    RMPLRCOV+712(RTM2PTR),B'11110111'                   0281 00829000
*                     RESPECIFY                                    0282 00830000
*                      (R1,                                        0282 00831000
*                       R13) UNRESTRICTED;                         0282 00832000
*                                                                  0282 00833000
*                     /***********************************************/ 00834000
*                     /*                                             */ 00835000
*                     /* THIS SEGMENT WILL SCAN THROUGH THE RB-S TO  */ 00836000
*                     /* BE PURGED INSERTING SVC 3 (EXIT) IN THEIR   */ 00837000
*                     /* PSWS, ZEROING THEIR WAIT COUNTS AND         */ 00838000
*                     /* INSERTING PROPER REGISTER VALUES WHEN       */ 00839000
*                     /* NECESSARY TO ENSURE THAT THE CORRECT        */ 00840000
*                     /* REGISTERS ARE PASSED TO THE RETRYING RB.    */ 00841000
*                     /* THIS SEGMENT WILL ALSO BE ENTERED IF AN     */ 00842000
*                     /* UNRECOVERABLE ERROR OCCURS IN PRE-EXIT      */ 00843000
*                     /* PROCESSING (IEAVTAS1). IN THIS CASE THE     */ 00844000
*                     /* ABEND SVRB, THE RB UNDER WHICH IEAVTAS1     */ 00845000
*                     /* EXECUTES, WILL BE THE RETRYING RB.          */ 00846000
*                     /*                                             */ 00847000
*                     /***********************************************/ 00848000
*                                                                  0283 00849000
*                     DO;           /* BEGIN RBTYPE                  */ 00850000
*                       IF(SDWAPTR^=NONE&SCBESTAE=ON&SDWAUPRG=ON)| 0284 00851000
*                           RTM2AS1R=ON THEN/* USER SPECIFIED      0284 00852000
*                                      REGISTER UPDATE OR THIS IS A     00853000
*                                      PSEUDO RETRY INTO IEAVTAS1    */ 00854000
         LTR   SDWAPTR,SDWAPTR                                     0284 00855000
         BZ    @GL00007                                            0284 00856000
         TM    SCBESTAE(SCBPTR),B'00010000'                        0284 00857000
         BNO   @GL00007                                            0284 00858000
         TM    SDWAUPRG(SDWAPTR),B'00001000'                       0284 00859000
         BO    @RT00284                                            0284 00860000
@GL00007 TM    RTM2AS1R(RTM2PTR),B'00000100'                       0284 00861000
         BNO   @RF00284                                            0284 00862000
@RT00284 DS    0H                                                  0285 00863000
*                         DO;                                      0285 00864000
*                           RESPECIFY                              0286 00865000
*                            (R9,                                  0286 00866000
*                             R4) RESTRICTED;                      0286 00867000
*                           IF RTM2AS1R=ON THEN/* TEST IF RETRY INTO    00868000
*                                      IEAVTAS1                      */ 00869000
         TM    RTM2AS1R(RTM2PTR),B'00000100'                       0287 00870000
         BNO   @RF00287                                            0287 00871000
*                             DO;   /* SETUP FOR RETRY       @G33SPHW*/ 00872000
*                               RTM2RETR=ON;/* INDICATE RETRY      0289 00873000
*                                                            @G33SPHW*/ 00874000
         OI    RTM2RETR(RTM2PTR),B'01000000'                       0289 00875000
*                               R9=ADDR(RTM2PRWA->RTM2RREG);/* RETRY    00876000
*                                      REGISTERS IN RTM2RREG         */ 00877000
         L     R9,RTM2PRWA(,RTM2PTR)                               0290 00878000
         LA    R9,RTM2RREG(,R9)                                    0290 00879000
*                             END;  /*                       @G33SPHW*/ 00880000
*                           ELSE    /* NORMAL RETRY                  */ 00881000
*                             R9=ADDR(SDWASRSV);/* UPDATED REGISTERS    00882000
*                                      ARE IN SDWA                   */ 00883000
         B     @RC00287                                            0292 00884000
@RF00287 LA    R9,SDWASRSV(,SDWAPTR)                               0292 00885000
*                           RTM2UPRG=ON;/* INDICATE ALL REGS ARE TO     00886000
*                                      BE PASSED TO RETRY ROUTINE  0293 00887000
*                                                            @G33SPHW*/ 00888000
@RC00287 OI    RTM2UPRG(RTM2PTR),B'01000000'                       0293 00889000
*                           RTM2RYRG=R9->RSAVE;/* COPY APPROIATE   0294 00890000
*                                      REGISTER TO RTM2WA FOR      0294 00891000
*                                      SAFEKEEPING           @G33SPHW*/ 00892000
         MVI   RTM2RYRG+64(RTM2PTR),C' '                           0294 00893000
         MVC   RTM2RYRG+65(3,RTM2PTR),RTM2RYRG+64(RTM2PTR)         0294 00894000
         MVC   RTM2RYRG(64,RTM2PTR),RSAVE(R9)                      0294 00895000
*                         END;      /* END RETRY SET UP WITH USER  0295 00896000
*                                      UPDATED REGISTERS             */ 00897000
*                       ELSE                                       0296 00898000
*                                                                  0296 00899000
*                         /*******************************************/ 00900000
*                         /*                                         */ 00901000
*                         /* THIS SEGMENT WILL INITIALIZE PARAMETER  */ 00902000
*                         /* REGISTERS TO BE SENT TO THE RETRY       */ 00903000
*                         /* ROUTINE. IT THEN SCANS THE RB-S TO BE   */ 00904000
*                         /* PURGED AND INSERTS THE REGISTER IN THE  */ 00905000
*                         /* RB-S REGISTER SAVE AREA IF NECESSARY.   */ 00906000
*                         /* THE WAIT COUNT OF THE RB-S TO BE PURGED */ 00907000
*                         /* AND OF THE RETRYING RB IS ZEROED. SVC 3 */ 00908000
*                         /* IS INSERTED IN THE PSW-S OF THE RB-S TO */ 00909000
*                         /* BE PURGED AND THE RETRY ADDRESS IS      */ 00910000
*                         /* INSERTED IN THE PSW OF THE RETRYING RB. */ 00911000
*                         /*                                         */ 00912000
*                         /*******************************************/ 00913000
*                                                                  0296 00914000
*                         DO;       /* BEGIN PARMREGS                */ 00915000
         B     @RC00284                                            0296 00916000
@RF00284 DS    0H                                                  0297 00917000
*                           RESPECIFY                              0297 00918000
*                            (R0,                                  0297 00919000
*                             R1,                                  0297 00920000
*                             R14,                                 0297 00921000
*                             R2) RESTRICTED;/* PARAMETER REGISTERS     00922000
*                                      TO RETRY RB                   */ 00923000
*                           IF SDWAPTR=NONE THEN/* TEST IF THERE IS     00924000
*                                      AN SDWA                       */ 00925000
         LTR   SDWAPTR,SDWAPTR                                     0298 00926000
         BNZ   @RF00298                                            0298 00927000
*                             R0=RC12;/* INDICATE SDWA NOT AVAILABLE */ 00928000
         LA    R0,12                                               0299 00929000
*                           ELSE                                   0300 00930000
*                             IF SCBESTAE=ON&SDWAFREE=ON THEN/* TEST    00931000
*                                      IF USER REQUESTED TO FREE SDWA   00932000
*                                      PRIOR TO RETRY                */ 00933000
         B     @RC00298                                            0300 00934000
@RF00298 TM    SCBESTAE(SCBPTR),B'00010000'                        0300 00935000
         BNO   @RF00300                                            0300 00936000
         TM    SDWAFREE(SDWAPTR),B'00000100'                       0300 00937000
         BNO   @RF00300                                            0300 00938000
*                               R0=RC20;/* SDWA IS NOT FREED AT THIS    00939000
*                                      POINT BUT WILL BE PRIOR TO  0301 00940000
*                                      RETRY                         */ 00941000
         LA    R0,20                                               0301 00942000
*                             ELSE                                 0302 00943000
*                               R0=ZERO;/* AN SDWA WAS OBTAINED      */ 00944000
         B     @RC00300                                            0302 00945000
@RF00300 SLR   R0,R0                                               0302 00946000
*                           IF R0=ZERO THEN/* IF AN SDWA WAS OBTAINED   00947000
*                                      AND WILL NOT BE FREED PRIOR TO   00948000
*                                      RETRY                         */ 00949000
@RC00300 DS    0H                                                  0303 00950000
@RC00298 LTR   R0,R0                                               0303 00951000
         BNZ   @RF00303                                            0303 00952000
*                             R1=SDWAPTR;/* R1 CONTAINS PTR TO SDWA  */ 00953000
         LR    R1,SDWAPTR                                          0304 00954000
*                           ELSE                                   0305 00955000
*                             DO;                                  0305 00956000
         B     @RC00303                                            0305 00957000
@RF00303 DS    0H                                                  0306 00958000
*                               IF SCBESTAE=ON THEN/* TEST IF ESTAE     00959000
*                                      EXIT                          */ 00960000
         TM    SCBESTAE(SCBPTR),B'00010000'                        0306 00961000
         BNO   @RF00306                                            0306 00962000
*                                 R1=SCBPARMA;/* R1 CONTAINS PTR TO     00963000
*                                      USER PARAMETER LIST           */ 00964000
         L     R1,SCBPARMA-1(,SCBPTR)                              0307 00965000
         LA    R1,0(,R1)                                           0307 00966000
*                               ELSE/* STAE EXIT                     */ 00967000
*                                 R1=RTM2CC;/* R1 CONTAINS ABEND   0308 00968000
*                                      COMPLETION CODE               */ 00969000
         B     @RC00306                                            0308 00970000
@RF00306 L     R1,RTM2CC-1(,RTM2PTR)                               0308 00971000
         LA    R1,0(,R1)                                           0308 00972000
*                               IF RTM2FIOB^=NOIOB THEN/* IF       0309 00973000
*                                      RESTOREABLE I/O               */ 00974000
@RC00306 L     @15,RTM2FIOB-1(,RTM2PTR)                            0309 00975000
         LA    @15,0(,@15)                                         0309 00976000
         C     @15,@CF00069                                        0309 00977000
         BE    @RF00309                                            0309 00978000
*                                 R2=RTM2FIOB;/* R2 CONTAINS POINTER    00979000
*                                      TO PIRL                       */ 00980000
         LR    R2,@15                                              0310 00981000
*                               ELSE/* IF NO SDWA AND NO RESTOREABLE    00982000
*                                      I/O                           */ 00983000
*                                 R2=NONE;/* R2 CONTAINS ZERO        */ 00984000
         B     @RC00309                                            0311 00985000
@RF00309 SLR   R2,R2                                               0311 00986000
*                             END;                                 0312 00987000
@RC00309 DS    0H                                                  0313 00988000
*                           IF RTM2RYRB->RBFTP='110'B THEN/* @YM01820*/ 00989000
@RC00303 L     @15,RTM2RYRB(,RTM2PTR)                              0313 00990000
         TM    RBFTP(@15),B'11000000'                              0313 00991000
         BNO   @RF00313                                            0313 00992000
         TM    RBFTP(@15),B'00100000'                              0313 00993000
         BNZ   @RF00313                                            0313 00994000
*                             R14=CVTEXPRO;/*                @YM01820*/ 00995000
         L     @12,CVTPTR(,RTM2PTR)                                0314 00996000
         L     R14,CVTEXPRO(,@12)                                  0314 00997000
*                           ELSE    /*                       @YM01820*/ 00998000
*                             R14=ADDR(CVTEXIT);/*           @YM01820*/ 00999000
         B     @RC00313                                            0315 01000000
@RF00313 L     R14,CVTPTR(,RTM2PTR)                                0315 01001000
         LA    R14,CVTEXIT(,R14)                                   0315 01002000
*                           RTM2RYRS(1)=R0;/* SAVE PARAMETER REG IN     01003000
*                                      RTM2WA                @G33SPHW*/ 01004000
@RC00313 ST    R0,RTM2RYRS(,RTM2PTR)                               0316 01005000
*                           RTM2RYRS(2)=R1;/* SAVE R1 IN RTM2WA    0317 01006000
*                                                            @G33SPHW*/ 01007000
         ST    R1,RTM2RYRS+4(,RTM2PTR)                             0317 01008000
*                           RTM2RYRS(3)=R2;/* SAVE R2 IN RTM2WA    0318 01009000
*                                                            @G33SPHW*/ 01010000
         ST    R2,RTM2RYRS+8(,RTM2PTR)                             0318 01011000
*                           RTM2RYRS(15)=R14;/* SAVE R14 IN RTM2WA 0319 01012000
*                                                            @G33SPHW*/ 01013000
         ST    R14,RTM2RYRS+56(,RTM2PTR)                           0319 01014000
*                           RTM2RYRS(16)=RTM2RTYA;/* RETRY ADDRESS IN   01015000
*                                      REG 15                @G33SPHW*/ 01016000
         L     @15,RTM2RTYA(,RTM2PTR)                              0320 01017000
         ST    @15,RTM2RYRS+60(,RTM2PTR)                           0320 01018000
*                           RTM2UPRG=OFF;/* INDICATE PARM REGS ONLY     01019000
*                                      SAVED                 @G33SPHW*/ 01020000
         NI    RTM2UPRG(RTM2PTR),B'10111111'                       0321 01021000
*                           RESPECIFY                              0322 01022000
*                            (R0,                                  0322 01023000
*                             R1,                                  0322 01024000
*                             R2,                                  0322 01025000
*                             R14) UNRSTD;                         0322 01026000
*                         END;      /* END PARMREGS                  */ 01027000
*                     END;          /* END RBTYPE                    */ 01028000
@RC00284 DS    0H                                                  0325 01029000
*                   END;            /* END OF RBPRGE                 */ 01030000
*                   IF RTM2WANA=OFF&RTM2WAIN=OFF THEN/* VALID SDWA 0326 01031000
*                                      AVAILABLE                     */ 01032000
*                                                                  0326 01033000
         TM    RTM2WANA(RTM2PTR),B'01100000'                       0326 01034000
         BNZ   @RF00326                                            0326 01035000
*                     /***********************************************/ 01036000
*                     /*                                             */ 01037000
*                     /* IF A VALID SDWA EXISTS, VARIOUS FLAGS ARE   */ 01038000
*                     /* CHECKED TO DETERMINE HOW MUCH SHOULD BE     */ 01039000
*                     /* FREED: IF STA EXIT, ALL BUT FIRST 104 BYTES.*/ 01040000
*                     /* IF ESTA SPECIFIED FRESDWA=YES, ENTIRE SDWA  */ 01041000
*                     /* IS FREED. IF ESTA DID NOT REQUEST FREEING OF*/ 01042000
*                     /* THE WORK AREA, ONLY THE SAVE AREA IS FREED. */ 01043000
*                     /* IF A WORK AREA EXISTS, THE RESTORE QUEUE    */ 01044000
*                     /* POINTERS ARE INITIALIZED.                   */ 01045000
*                     /*                                             */ 01046000
*                     /***********************************************/ 01047000
*                                                                  0327 01048000
*                     DO;           /* START OF RTRYSDWA             */ 01049000
*                       IF SDWAPTR^=NONE THEN/* VALID SDWA EXISTS    */ 01050000
