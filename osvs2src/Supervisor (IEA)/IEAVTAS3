         TM    SDWAFREE(SDWAPTR),B'00000100'                       0473 01541000
         BNO   @RF00473                                            0473 01542000
*                 R0=RC20;          /* SDWA IS NOT FREED AT THIS   0474 01543000
*                                      POINT BUT WILL BE PRIOR TO  0474 01544000
*                                      RETRY                         */ 01545000
         LA    R0,20                                               0474 01546000
*               ELSE                                               0475 01547000
*                 R0=ZERO;          /* AN SDWA WAS OBTAINED          */ 01548000
         B     @RC00473                                            0475 01549000
