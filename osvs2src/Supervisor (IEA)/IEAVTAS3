*           END;                    /* END SCAN FOR SCB              */ 01415000
@DE00442 LTR   SCBPTR,SCBPTR                                       0446 01416000
         BZ    @DL00442                                            0446 01417000
*           RESPECIFY                                              0447 01418000
*             R8 UNRESTRICTED;                                     0447 01419000
*           RESPECIFY                                              0448 01420000
*             R9 RESTRICTED;                                       0448 01421000
*           IF RTM2PREV->RTM2FMS=ON THEN/* TEST IF FREEMAIN FAILURE  */ 01422000
         L     @12,RTM2PREV(,RTM2PTR)                              0449 01423000
         TM    RTM2FMS(@12),B'00000001'                            0449 01424000
         BNO   @RF00449                                            0449 01425000
*             R9=ADDR(FMRET);       /* RESUME FOLLOWING FREEMAIN     */ 01426000
         LA    R9,FMRET                                            0450 01427000
*           ELSE                                                   0451 01428000
*             IF RTM2PREV->RTM2RTYS=ON THEN                        0451 01429000
         B     @RC00449                                            0451 01430000
@RF00449 L     @12,RTM2PREV(,RTM2PTR)                              0451 01431000
         TM    RTM2RTYS(@12),B'01000000'                           0451 01432000
         BNO   @RF00451                                            0451 01433000
*               R9=ADDR(NORETRY);   /* RETRY FAILURE - RESUME WITH 0452 01434000
*                                      CONTINUE WITH TERMINATION     */ 01435000
         LA    R9,NORETRY                                          0452 01436000
*           GOTO R9;                                               0453 01437000
@RF00451 DS    0H                                                  0453 01438000
@RC00449 BR    R9                                                  0453 01439000
*           RESPECIFY                                              0454 01440000
*             R9 UNRESTRICTED;                                     0454 01441000
*         END;                      /* END RECURSE3                  */ 01442000
*     ELSE                                                         0456 01443000
*                                                                  0456 01444000
*       /*************************************************************/ 01445000
*       /*                                                           */ 01446000
*       /* THIS SEGMENT WILL SCAN THROUGH THE RB-S TO BE PURGED      */ 01447000
*       /* INSERTING SVC 3 (EXIT) IN THEIR PSWS, ZEROING THEIR WAIT  */ 01448000
*       /* COUNTS AND INSERTING PROPER REGISTER VALUES WHEN NECESSARY*/ 01449000
*       /* TO ENSURE THAT THE CORRECT REGISTERS ARE PASSED TO THE    */ 01450000
*       /* RETRYING RB. THIS SEGMENT WILL ALSO BE ENTERED IF AN      */ 01451000
*       /* UNRECOVERABLE ERROR OCCURS IN PRE-EXIT PROCESSING         */ 01452000
*       /* (IEAVTAS1). IN THIS CASE THE ABEND SVRB, THE RB UNDER     */ 01453000
*       /* WHICH IEAVTAS1 EXECUTES, WILL BE THE RETRYING RB.         */ 01454000
*       /*                                                           */ 01455000
*       /*************************************************************/ 01456000
*                                                                  0456 01457000
*       DO;                         /* BEGIN RBTYPE                  */ 01458000
         B     @RC00100                                            0456 01459000
@RF00100 DS    0H                                                  0457 01460000
*         IF(SDWAPTR^=NONE&SCBESTAE=ON&SDWAUPRG=ON)|RTM2AS1R=ON THEN    01461000
         LTR   SDWAPTR,SDWAPTR                                     0457 01462000
         BZ    @GL00012                                            0457 01463000
