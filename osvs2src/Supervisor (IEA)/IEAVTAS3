@RF00359 TM    SCBESTAE(SCBPTR),B'00010000'                        0365 01160000
         BNZ   @RF00365                                            0365 01161000
*                         RTM2DREQ=OFF;/* IF STAE/STAI RETRY DO NOT     01162000
*                                      DUMP                          */ 01163000
         NI    RTM2DREQ(RTM2PTR),B'01111111'                       0366 01164000
*                     END;          /* END OF RTRYSDWA               */ 01165000
*                                                                  0367 01166000
@RF00365 DS    0H                                                  0368 01167000
*                   /*************************************************/ 01168000
*                   /*                                               */ 01169000
*                   /* THIS SEGMENT UPDATES THE CURRENT SCB PRIOR TO */ 01170000
*                   /* RETRY. IF THE SCB IS AN ESTAE SCB THE INUSE   */ 01171000
*                   /* FLAG IS ZEROED SO THAT IT CAN BE REUSED. IF IT*/ 01172000
*                   /* IS A STAE SCB. IT IS MARKED LOGICALLY CANCELED*/ 01173000
*                   /* SO THAT IT WILL NOT BE REUSED. FOR (E)STAI    */ 01174000
*                   /* RETRY THE CURRENT SCB INUSE FLAG IS ALWAYS    */ 01175000
*                   /* ZEROED. IN ADDITION, IF THERE IS NO PREVIOUS  */ 01176000
*                   /* EXIT PRB ON THE QUEUE, THE INUSE FLAG IS      */ 01177000
*                   /* ZEROED IN ALL (E)STAI SCB-S ON THE QUEUE.     */ 01178000
*                   /*                                               */ 01179000
*                   /*************************************************/ 01180000
*                                                                  0368 01181000
*                   DO;             /* BEGIN SCBUPDAT                */ 01182000
@RF00326 DS    0H                                                  0369 01183000
*                     IF SCBESTAE=ON THEN/* IF ESTAE SCB             */ 01184000
         TM    SCBESTAE(SCBPTR),B'00010000'                        0369 01185000
         BNO   @RF00369                                            0369 01186000
*                       SCBINUSE=OFF;/* TURN OFF IN USE BIT SO IT CAN   01187000
*                                      BE REUSED                     */ 01188000
*                                                                  0370 01189000
         NI    SCBINUSE(SCBPTR),B'11101111'                        0370 01190000
*                     /***********************************************/ 01191000
*                     /*                                             */ 01192000
*                     /* (E)STAI SCB PROCESSING                      */ 01193000
*                     /*                                             */ 01194000
*                     /***********************************************/ 01195000
*                                                                  0371 01196000
*                     IF SCBSTAI=ON THEN/* IF AN (E)STAI SCB         */ 01197000
@RF00369 TM    SCBSTAI(SCBPTR),B'10000000'                         0371 01198000
         BNO   @RF00371                                            0371 01199000
*                       DO;         /* (E)STAI SCB                   */ 01200000
*                         SCBINUSE=OFF;/* ALWAYS RESET INUSE BIT IN     01201000
*                                      CURRENT (E)STAI SCB           */ 01202000
         NI    SCBINUSE(SCBPTR),B'11101111'                        0373 01203000
*                         IF RTM2RYRB->RBTCBNXT^=ON THEN           0374 01204000
         L     @12,RTM2RYRB(,RTM2PTR)                              0374 01205000
         TM    RBTCBNXT(@12),B'10000000'                           0374 01206000
         BO    @RF00374                                            0374 01207000
*                           IF EVAL((RTM2RYRB->RBLINKB)-LENGTH(RBPREFIX 01208000
