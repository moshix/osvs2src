         CR    SDWAPTR,@12                                         0328 01052000
         BE    @RF00328                                            0328 01053000
*                         DO;                                      0329 01054000
*                           RESPECIFY                              0330 01055000
*                            (R12,                                 0330 01056000
*                             R11) RESTRICTED;/* RESTRICT LENGTH AND    01057000
*                                      START PTRS                    */ 01058000
*                           IF SCBESTAE=ON THEN/* ESTA EXIT          */ 01059000
         TM    SCBESTAE(SCBPTR),B'00010000'                        0331 01060000
         BNO   @RF00331                                            0331 01061000
*                             IF SDWAFREE=ON THEN/* EXIT REQUESTED 0332 01062000
*                                      FREEING OF SDWA               */ 01063000
         TM    SDWAFREE(SDWAPTR),B'00000100'                       0332 01064000
         BNO   @RF00332                                            0332 01065000
*                               DO;                                0333 01066000
*                                 R12=RTM2SIZE;/* WHOLE THING TO BE     01067000
*                                      FREED                         */ 01068000
         L     R12,RTM2SIZE-1(,RTM2PTR)                            0334 01069000
         LA    R12,0(,R12)                                         0334 01070000
*                                 R11=RTM2RTCA;                    0335 01071000
         L     R11,RTM2RTCA(,RTM2PTR)                              0335 01072000
*                                 SDWAPTR=NONE;                    0336 01073000
         SLR   SDWAPTR,SDWAPTR                                     0336 01074000
*                                 RTM2RTCA=NONE;                   0337 01075000
         SLR   @08,@08                                             0337 01076000
         ST    @08,RTM2RTCA(,RTM2PTR)                              0337 01077000
*                               END;                               0338 01078000
*                             ELSE  /* EXIT DID NOT REQUEST FREEING  */ 01079000
*                               R12=72;/* FREE ONLY SAVE AREA        */ 01080000
         B     @RC00332                                            0339 01081000
@RF00332 LA    R12,72                                              0339 01082000
*                           ELSE    /* STA (NOT ESTA) EXIT           */ 01083000
*                             R12=RTM2SIZE-104;/* LEAVE 104 BYTES    */ 01084000
         B     @RC00331                                            0340 01085000
@RF00331 L     R12,RTM2SIZE-1(,RTM2PTR)                            0340 01086000
         LA    R12,0(,R12)                                         0340 01087000
         SL    R12,@CF03678                                        0340 01088000
*                           IF SDWAPTR^=NONE THEN/* PART OF SDWA TO     01089000
*                                      BE LEFT                       */ 01090000
@RC00331 LTR   SDWAPTR,SDWAPTR                                     0341 01091000
         BZ    @RF00341                                            0341 01092000
*                             DO;                                  0342 01093000
*                               RTM2SIZE=RTM2SIZE-R12;/* NEW LENGTH  */ 01094000
         L     @08,RTM2SIZE-1(,RTM2PTR)                            0343 01095000
         LA    @08,0(,@08)                                         0343 01096000
         SLR   @08,R12                                             0343 01097000
         STCM  @08,7,RTM2SIZE(RTM2PTR)                             0343 01098000
*                               SDWALNTH=RTM2SIZE;                 0344 01099000
         STCM  @08,7,SDWALNTH(SDWAPTR)                             0344 01100000
*                               R11=SDWAPTR+RTM2SIZE;/* FIRST BYTE TO   01101000
*                                      BE FREED                      */ 01102000
         ALR   @08,SDWAPTR                                         0345 01103000
         LR    R11,@08                                             0345 01104000
*                             END;                                 0346 01105000
*                           DO;     /* FREEMAIN(RC)                0347 01106000
*                                      LV(R12)A(R11->SDWA)         0347 01107000
*                                      SP(RTM2SUBP)                  */ 01108000
@RF00341 DS    0H                                                  0348 01109000
*                             RESPECIFY                            0348 01110000
*                              (GPR01F,                            0348 01111000
*                               GPR15F,                            0348 01112000
*                               GPR00F) RESTRICTED;                0348 01113000
*                             GPR01F=0;/* REG 1 MUST BE ZERO         */ 01114000
         SLR   GPR01F,GPR01F                                       0349 01115000
*                             GPR15F=1;/* SET TYPE                   */ 01116000
         LA    GPR15F,1                                            0350 01117000
*                             GPR00F=R12;/* LENGTH REQUESTED         */ 01118000
         LR    GPR00F,R12                                          0351 01119000
*                             GPR01F=ADDR(R11->SDWA);/* SET REGISTER    01120000
*                                      ONE                           */ 01121000
         LR    GPR01F,R11                                          0352 01122000
*                             GPR15F=GPR15F+((RTM2SUBP)*256);/*    0353 01123000
*                                      SUBPOOL VALUE                 */ 01124000
         SLR   @14,@14                                             0353 01125000
         IC    @14,RTM2SUBP(,RTM2PTR)                              0353 01126000
         SLA   @14,8                                               0353 01127000
         ALR   GPR15F,@14                                          0353 01128000
*                             SVC(120);/* RC/RU-FORM OF GETMAIN      */ 01129000
         SVC   120                                                 0354 01130000
*                             RESPECIFY                            0355 01131000
*                              (GPR01F,                            0355 01132000
*                               GPR15F,                            0355 01133000
*                               GPR00F) UNRESTRICTED;              0355 01134000
*                           END;    /* FREEMAIN(RC)                0356 01135000
*                                      LV(R12)A(R11->SDWA)         0356 01136000
*                                      SP(RTM2SUBP)                  */ 01137000
