*                         IF RTM2IOQR=ON THEN/* I/O WAS QUIESCED     */ 01146000
         TM    RTM2IOQR(RTM2PTR),B'10000000'                       0360 01147000
         BNO   @RF00360                                            0360 01148000
*                           DO;                                    0361 01149000
*                             SDWAFIOB=RTM2FIOB;/* LOCATES PIRL      */ 01150000
         L     @12,RTM2FIOB-1(,RTM2PTR)                            0362 01151000
         LA    @12,0(,@12)                                         0362 01152000
