*                       RTM2COMP=SDWACOMP;/* CARRY FORWARD USER WORD */ 01296000
@RC00402 L     @12,SDWACOMP(,SDWAPTR)                              0405 01297000
         ST    @12,RTM2COMP(,RTM2PTR)                              0405 01298000
*                     END;                                         0406 01299000
*                   RTM2FMS=ON;     /* SET FREEMAIN SECTION FLAG     */ 01300000
@RF00399 OI    RTM2FMS(RTM2PTR),B'00000001'                        0407 01301000
*                   RTM2TRRA=ADDR(RECURSE);/* ESTABLISH RECURSION  0408 01302000
*                                      ADDRESS                       */ 01303000
