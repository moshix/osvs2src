*           RTM2RREG(8)=SDWAPTR;    /* SAVE SDWAPTR SO THAT IT CAN BE   01390000
*                                      CARRIED FORWARD IF FURTHER  0438 01391000
*                                      RECURSION                     */ 01392000
         ST    SDWAPTR,RTM2RREG+28(,RTM2PTR)                       0438 01393000
*           RESPECIFY                                              0439 01394000
*             R8 RESTRICTED;                                       0439 01395000
*           SCBPTR=NONE;            /* INITIALIZE SCBPTR             */ 01396000
         SLR   SCBPTR,SCBPTR                                       0440 01397000
*           R8=RTM2PREV;            /* INITIALIZE FOR BEGINNING OF 0441 01398000
*                                      SCAN THROUGH PREVIOUS       0441 01399000
