         TITLE 'IEAVTAS3 - CONTROL RECOVERY'                            00001000
IEAVTAS3 CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'IEAVTAS3  78.206'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @10,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@10                                         0001 00012000
RBPRFXLN EQU 64                                                         00013000
RBPRFLNA EQU 32                                                         00014000
PRBLEN   EQU 136                                                        00015000
SIRBLEN  EQU 200                                                        00016000
TIRBLEN  EQU 136                                                        00017000
IRBLEN   EQU 128                                                        00018000
SVRBLEN  EQU 224                                                        00019000
*                                                                  0098 00020000
*   /*****************************************************************/ 00021000
*   /*                                                               */ 00022000
*   /* FOR NON-RECURSIVE ENTRIES, THIS ROUTINES ATTEMPTS TO SET UP   */ 00023000
*   /* FOR RETRY IF RETRY IS REQUESTED. IF RETRY IS NOT POSSIBLE     */ 00024000
*   /* (RETRY RB CANNOT BE LOCATED, ENTRY FOR TERM EXITS ONLY),      */ 00025000
*   /* PERCOLATION IS FORCED. IF NO RETRY, THE SDWA IS FREED AND     */ 00026000
*   /* FLAGS ARE RESET TO CORRECTLY INDICATE PROCESSING COMPLETED.   */ 00027000
*   /*                                                               */ 00028000
*   /*****************************************************************/ 00029000
*                                                                  0099 00030000
*   IF RTM2RETR=OFF THEN            /* NOT ENTRY TO SET UP FOR RETRY    00031000
*                                                            @G33SPHW*/ 00032000
         TM    RTM2RETR(RTM2PTR),B'01000000'                       0099 00033000
         BNZ   @RF00099                                            0099 00034000
*     IF RTM2AS1R=OFF THEN          /* ENTRY FOR PSEUDO RETRY INTO 0100 00035000
*                                      AS1                           */ 00036000
         TM    RTM2AS1R(RTM2PTR),B'00000100'                       0100 00037000
         BNZ   @RF00100                                            0100 00038000
*       IF RTM2AS3R=OFF THEN        /* NOT RECURSION WHICH IS HANDLED   00039000
*                                      IN THIS ROUTINE               */ 00040000
         TM    RTM2AS3R(RTM2PTR),B'00000001'                       0101 00041000
         BNZ   @RF00101                                            0101 00042000
*         DO;                                                      0102 00043000
*           IF RTM2RCDE=SDWARETY&RTM2CLUP=OFF THEN/* RETRY REQUESTED    00044000
*                                      AND ALLOWED                   */ 00045000
         CLI   RTM2RCDE(RTM2PTR),4                                 0103 00046000
         BNE   @RF00103                                            0103 00047000
         TM    RTM2CLUP(RTM2PTR),B'10000000'                       0103 00048000
         BNZ   @RF00103                                            0103 00049000
*             DO;                                                  0104 00050000
*               RTM2RTYS=ON;        /* INDICATE RETRY PROCESSING     */ 00051000
         OI    RTM2RTYS(RTM2PTR),B'01000000'                       0105 00052000
*               RTM2TRRA=ADDR(RECURSE);/* INITIALIZE RECURSION     0106 00053000
*                                      ADDRESS                       */ 00054000
*                                                                  0106 00055000
         L     @12,@CV00066                                        0106 00056000
         ST    @12,RTM2TRRA(,RTM2PTR)                              0106 00057000
*               /*****************************************************/ 00058000
*               /*                                                   */ 00059000
*               /* AN ATTEMPT IS MADE TO LOCATE THE RETRY RB. IF NO  */ 00060000
*               /* RB IS LOCATED, NO FURTHER RETRY PROCESSING IS DONE*/ 00061000
*               /* AND CONTINUE WITH TERMINATION IS FORCED. FOR      */ 00062000
*               /* (E)STAE EXITS THE RETRY RB IS THE ONE POINTED TO  */ 00063000
*               /* FROM THE SCB. FOR ESTAR EXITS, THE RETRY RB IS THE*/ 00064000
*               /* OLDEST RB ON THE QUEUE. FOR (E)STAI EXITS THE     */ 00065000
*               /* RETRY RB MUST BE A PRB. IF THERE IS A PRB WHOSE   */ 00066000
*               /* RBLINK FIELD POINTS TO AN RTM2 SVRB, THAT IS THE  */ 00067000
*               /* FIRST CHOICE. IF THERE IS NO PREVIOUS RTM2 RB ON  */ 00068000
*               /* THE QUEUE, THE RETRY RB IS THE NEWEST PRB OLDER   */ 00069000
*               /* THAN THE OLDEST NON-PRB.                          */ 00070000
*               /*                                                   */ 00071000
*               /*****************************************************/ 00072000
*                                                                  0107 00073000
*               DO;                 /* START OF FINDRB               */ 00074000
*                 IF SCBSTAI=OFF&SCBSTAR=OFF THEN/* (E)STAE SCB      */ 00075000
         TM    SCBSTAI(SCBPTR),B'11000000'                         0108 00076000
         BNZ   @RF00108                                            0108 00077000
*                   RTM2RYRB=SCBOWNRA;/* RETRY RB IS RB WHICH ISSUED    00078000
*                                      STAE                          */ 00079000
         L     @12,SCBOWNRA-1(,SCBPTR)                             0109 00080000
         LA    @12,0(,@12)                                         0109 00081000
         ST    @12,RTM2RYRB(,RTM2PTR)                              0109 00082000
*                 ELSE              /* NOT (E)STAE SCB               */ 00083000
*                   IF SCBSTAR=ON THEN/* ESTAR SCB                   */ 00084000
         B     @RC00108                                            0110 00085000
@RF00108 TM    SCBSTAR(SCBPTR),B'01000000'                         0110 00086000
         BNO   @RF00110                                            0110 00087000
*                     RTM2RYRB=RTM2STAR;/* ACCESS ESTAR RELATED RB   */ 00088000
*                                                                  0111 00089000
         L     @12,RTM2STAR(,RTM2PTR)                              0111 00090000
         ST    @12,RTM2RYRB(,RTM2PTR)                              0111 00091000
*                 /***************************************************/ 00092000
*                 /*                                                 */ 00093000
*                 /* (E)STAI RETRY                                   */ 00094000
*                 /*                                                 */ 00095000
*                 /***************************************************/ 00096000
*                                                                  0112 00097000
*                   ELSE                                           0112 00098000
*                     DO;                                          0112 00099000
         B     @RC00110                                            0112 00100000
@RF00110 DS    0H                                                  0113 00101000
*                       RESPECIFY                                  0113 00102000
*                        (R4,                                      0113 00103000
*                         R2) RESTRICTED;                          0113 00104000
*                       R4=RTM2VRBC->RBLINKB;/* START WITH CURRENT RB*/ 00105000
         L     @12,RTM2VRBC(,RTM2PTR)                              0114 00106000
         L     R4,RBLINKB-1(,@12)                                  0114 00107000
         LA    R4,0(,R4)                                           0114 00108000
*                       R2=NONE;                                   0115 00109000
         SLR   R2,R2                                               0115 00110000
*                       RTM2RYRB=NONE;                             0116 00111000
*                                                                  0116 00112000
         SLR   @12,@12                                             0116 00113000
         ST    @12,RTM2RYRB(,RTM2PTR)                              0116 00114000
*                       /*********************************************/ 00115000
*                       /*                                           */ 00116000
*                       /* SCAN FOR ELIGIBLE PRB                     */ 00117000
*                       /*                                           */ 00118000
*                       /*********************************************/ 00119000
*                                                                  0117 00120000
*                       DO WHILE(R4^=RTM2TCBC&RTM2RYRB=NONE);/* SCAN    00121000
*                                      ENTIRE QUEUE UNLESS STA EXIT     00122000
*                                      RB FOUND                      */ 00123000
         B     @DE00117                                            0117 00124000
@DL00117 DS    0H                                                  0118 00125000
*                         IF RBFTP='000'B THEN/* RB IS PRB           */ 00126000
         TM    RBFTP(R4),B'11100000'                               0118 00127000
         BNZ   @RF00118                                            0118 00128000
*                           IF RBTCBNXT^=ON THEN                   0119 00129000
         TM    RBTCBNXT(R4),B'10000000'                            0119 00130000
         BO    @RF00119                                            0119 00131000
*                             IF EVAL(RBLINKB-LENGTH(RBPREFIX))->  0120 00132000
*                                 RBABEND=ON THEN/* PREVIOUS RB IS 0120 00133000
*                                      RTM2 SVRB                     */ 00134000
         L     @12,RBLINKB-1(,R4)                                  0120 00135000
         LA    @12,0(,@12)                                         0120 00136000
         SL    @12,@CF00472                                        0120 00137000
         TM    RBABEND(@12),B'00100000'                            0120 00138000
         BNO   @RF00120                                            0120 00139000
*                               RTM2RYRB=R4;/* RETRY RB FOUND        */ 00140000
         ST    R4,RTM2RYRB(,RTM2PTR)                               0121 00141000
*                             ELSE  /* PRB BUT NOT EXIT RB           */ 00142000
*                               IF R2=NONE THEN/* NEWEST RB IN THIS     00143000
*                                      GROUP                         */ 00144000
         B     @RC00120                                            0122 00145000
@RF00120 LTR   R2,R2                                               0122 00146000
         BNZ   @RF00122                                            0122 00147000
*                                 R2=R4;/* SAVE RB ADDRESS           */ 00148000
         LR    R2,R4                                               0123 00149000
*                               ELSE                               0124 00150000
*                                 ; /* PREVIOUS RB WAS PRB - IGNORE     00151000
*                                      THIS ONE                      */ 00152000
@RF00122 DS    0H                                                  0125 00153000
*                           ELSE                                   0125 00154000
*                             IF R2=NONE THEN/* TEST IF NEWEST PRB   */ 00155000
         B     @RC00119                                            0125 00156000
@RF00119 LTR   R2,R2                                               0125 00157000
         BNZ   @RF00125                                            0125 00158000
*                               R2=R4;/* SAVE PRB ADDRESS            */ 00159000
         LR    R2,R4                                               0126 00160000
*                             ELSE                                 0127 00161000
*                               ;                                  0127 00162000
@RF00125 DS    0H                                                  0128 00163000
*                         ELSE      /* NOT PRB                       */ 00164000
*                           R2=NONE;/* CLEAR PTR - OLDER NON-PRB     */ 00165000
         B     @RC00118                                            0128 00166000
@RF00118 SLR   R2,R2                                               0128 00167000
*                         R4=RBLINKB;/* UPDATE RB POINTER            */ 00168000
@RC00118 L     @12,RBLINKB-1(,R4)                                  0129 00169000
         LA    @12,0(,@12)                                         0129 00170000
         LR    R4,@12                                              0129 00171000
*                       END;        /* END OF RB SCAN                */ 00172000
@DE00117 C     R4,RTM2TCBC(,RTM2PTR)                               0130 00173000
         BE    @DC00117                                            0130 00174000
         L     @12,RTM2RYRB(,RTM2PTR)                              0130 00175000
         LTR   @12,@12                                             0130 00176000
         BZ    @DL00117                                            0130 00177000
@DC00117 DS    0H                                                  0131 00178000
*                       IF RTM2RYRB=NONE THEN/* STA EXIT RB NOT FOUND*/ 00179000
         SLR   @12,@12                                             0131 00180000
         C     @12,RTM2RYRB(,RTM2PTR)                              0131 00181000
         BNE   @RF00131                                            0131 00182000
*                         IF R2^=NONE THEN/* PRB OLDER THAN OLDEST 0132 00183000
*                                      NON-PRB FOUND                 */ 00184000
         CR    R2,@12                                              0132 00185000
         BE    @RF00132                                            0132 00186000
*                           RTM2RYRB=R2;/* THIS IS RETRY RB          */ 00187000
         ST    R2,RTM2RYRB(,RTM2PTR)                               0133 00188000
*                         ELSE      /* OLDEST RB IS NOT PRB          */ 00189000
*                           RTM2RCDE=SDWACWT;/* RETRY NOT POSSIBLE   */ 00190000
         B     @RC00132                                            0134 00191000
@RF00132 MVI   RTM2RCDE(RTM2PTR),X'00'                             0134 00192000
*                     END;                                         0135 00193000
@RC00132 DS    0H                                                  0135 00194000
@RF00131 DS    0H                                                  0136 00195000
*                 RESPECIFY                                        0136 00196000
*                  (R4,                                            0136 00197000
*                   R2) UNRESTRICTED;/* RELEASE PTRS USED FOR SCAN   */ 00198000
@RC00110 DS    0H                                                  0136 00199000
@RC00108 DS    0H                                                  0137 00200000
*               END;                /* END OF FINDRB                 */ 00201000
*               IF RTM2RCDE=SDWARETY THEN/* RETRY RB FOUND - RETRY 0138 00202000
*                                      CONTINUES                     */ 00203000
         CLI   RTM2RCDE(RTM2PTR),4                                 0138 00204000
         BNE   @RF00138                                            0138 00205000
*                 DO;                                              0139 00206000
*                                                                  0139 00207000
*                   /*************************************************/ 00208000
*                   /*                                               */ 00209000
*                   /* RBPRGE PERFORMS PURGES NEEDED BEFORE RETRY. IT*/ 00210000
*                   /* INCLUDES CLOSEDS TO CLOSE EMBEDDED DCBS. IT   */ 00211000
*                   /* THEN PURGES ASYNCHRONOUS EXITS, TQES, AND     */ 00212000
*                   /* ENQ'D RESOURCES FOR ESTAR RETRY. FOR          */ 00213000
*                   /* (E)STAE/STAE RETRY, PAGEPRGE IS INCLUDED TO   */ 00214000
*                   /* PURGE PAGING I/O FOR INTERVENING RBS. FOR STA */ 00215000
*                   /* EXITS, WTORS ARE PURGED IF ANY RBS ARE TO BE  */ 00216000
*                   /* PURGED. THE TYPE 1 MESSAGE TABLE IS PURGED.   */ 00217000
*                   /* THEN RBTYPE IS INCLUDED TO SET ALL INTERVENING*/ 00218000
*                   /* RBS UP FOR RETRY.                             */ 00219000
*                   /*                                               */ 00220000
*                   /*************************************************/ 00221000
*                                                                  0140 00222000
*                   DO;             /* START OF RBPRGE               */ 00223000
*                                                                  0140 00224000
*                     /***********************************************/ 00225000
*                     /*                                             */ 00226000
*                     /* SET UP RMPL FOR TASK RECOVERY PURGES        */ 00227000
*                     /*                                             */ 00228000
*                     /***********************************************/ 00229000
*                                                                  0141 00230000
*                     RMPLRCOV=ON;  /* TASK RECOVERY IS CALLER       */ 00231000
*                     RMPLTYPE=ON;  /* INDICATE ABNORMAL TERMINATION    00232000
*                                                            @YM04241*/ 00233000
         LA    @12,712                                             0142 00234000
         ALR   @12,RTM2PTR                                         0142 00235000
         OI    RMPLRCOV(@12),B'10001000'                           0142 00236000
*                     RMPLTCBA=RTM2TCBC;/* PURGES ARE FOR CURRENT TCB*/ 00237000
         L     @11,RTM2TCBC(,RTM2PTR)                              0143 00238000
         ST    @11,RMPLTCBA(,@12)                                  0143 00239000
*                     IF RMPLTCBA=RMPLTCBA->TCBJSTCA THEN/* CURRENT     00240000
*                                      TCB IS JOBSTEP                */ 00241000
         L     @06,TCBJSTCA-1(,@11)                                0144 00242000
         LA    @06,0(,@06)                                         0144 00243000
         CR    @11,@06                                             0144 00244000
         BNE   @RF00144                                            0144 00245000
*                       RMPLJST=ON;                                0145 00246000
         OI    RMPLJST(@12),B'00000100'                            0145 00247000
*                     ELSE          /* CURRENT TCB IS NOT JOBSTEP    */ 00248000
*                       RMPLJST=OFF;                               0146 00249000
         B     @RC00144                                            0146 00250000
@RF00144 NI    RMPLJST+712(RTM2PTR),B'11111011'                    0146 00251000
*                     RESPECIFY                                    0147 00252000
*                      (R1,                                        0147 00253000
*                       R13) RESTRICTED;                           0147 00254000
@RC00144 DS    0H                                                  0148 00255000
*                     R1=ADDR(RTM2RMPS);                           0148 00256000
         LA    R1,RTM2RMPS(,RTM2PTR)                               0148 00257000
*                     R13=ADDR(RTM2RMSA);                          0149 00258000
         LA    R13,RTM2RMSA(,RTM2PTR)                              0149 00259000
*                     IF SCBSTAR=ON THEN/* ESTAR RETRY               */ 00260000
         TM    SCBSTAR(SCBPTR),B'01000000'                         0150 00261000
         BNO   @RF00150                                            0150 00262000
*                       DO;         /* SPECIAL PURGES FOR ESTAR      */ 00263000
*                         CALL DATAMGR;/* CLOSE ALL DATASETS         */ 00264000
         L     @12,CVTPTR(,RTM2PTR)                                0152 00265000
         L     @15,CVT0TC0A(,@12)                                  0152 00266000
         BALR  @14,@15                                             0152 00267000
*                         CALL AEXITRM;/* PURGE ASYNCHRONOUS EXITS   */ 00268000
         L     @12,CVTPTR(,RTM2PTR)                                0153 00269000
         L     @15,CVTDQIQE(,@12)                                  0153 00270000
         BALR  @14,@15                                             0153 00271000
*                         CALL IEAQPGTM;/* PURGE TIMER QUEUE ELEMENTS*/ 00272000
         L     @12,CVTPTR(,RTM2PTR)                                0154 00273000
         L     @12,SCVTSPTR(,@12)                                  0154 00274000
         L     @15,SCVTPGTM(,@12)                                  0154 00275000
         BALR  @14,@15                                             0154 00276000
*                         CALL IEAVENQ2;/* PURGE ENQUEUED RESOURCES  */ 00277000
         L     @12,CVTPTR(,RTM2PTR)                                0155 00278000
         L     @15,CVTRENQ(,@12)                                   0155 00279000
         BALR  @14,@15                                             0155 00280000
*                       END;        /* END SPECIAL ESTAR PURGES      */ 00281000
*                     ELSE          /* NOT ESTAR                     */ 00282000
*                       DO;                                        0157 00283000
*                                                                  0157 00284000
         B     @RC00150                                            0157 00285000
@RF00150 DS    0H                                                  0158 00286000
*                         /*******************************************/ 00287000
*                         /*                                         */ 00288000
*                         /* CLOSEDS LOCATES AND CLOSES DATASETS     */ 00289000
*                         /* EMBEDDED IN CODE DESCRIBED BY A PRB. IT */ 00290000
*                         /* ALSO UPDATES THE IOB RESTORE CHAIN TO   */ 00291000
*                         /* REMOVE IOBS ASSOCIATED WITH CLOSED      */ 00292000
*                         /* DATASETS. RBS BETWEEN THE CURRENT RB AND*/ 00293000
*                         /* THE RETRY RB ARE CHECKED FOR PRBS. FOR  */ 00294000
*                         /* EACH PRB FOUND THE EXTENT LIST IS       */ 00295000
*                         /* LOCATED AND FINDDCBS IS INCLUDED TO FIND*/ 00296000
*                         /* AND CLOSE ALL DCBS ASSOCIATED WITH THIS */ 00297000
*                         /* PRB.                                    */ 00298000
*                         /*                                         */ 00299000
*                         /*******************************************/ 00300000
*                                                                  0158 00301000
*                         ;         /* START OF CLOSEDS              */ 00302000
*                         RESPECIFY                                0159 00303000
*                          (R4,     /* RB PTR                        */ 00304000
*                           R9) RESTRICTED;/* EXTENT LIST PTR        */ 00305000
*                         RMPLRBPP=ON;/* INDICATE DCB LIST PASSED    */ 00306000
         LA    @12,712                                             0160 00307000
         ALR   @12,RTM2PTR                                         0160 00308000
         OI    RMPLRBPP(@12),B'00100000'                           0160 00309000
*                         R4=RTM2VRBC->RBLINKB;/* SET UP POINTER TO     00310000
*                                      FIRST RB                      */ 00311000
         L     @11,RTM2VRBC(,RTM2PTR)                              0161 00312000
         L     R4,RBLINKB-1(,@11)                                  0161 00313000
         LA    R4,0(,R4)                                           0161 00314000
*                         RMPLDCBL=ADDR(RTM2DCBA);/* SET UP PTR TO 0162 00315000
*                                      DCB LIST                      */ 00316000
*                                                                  0162 00317000
         LA    @11,RTM2DCBA(,RTM2PTR)                              0162 00318000
         ST    @11,RMPLDCBL(,@12)                                  0162 00319000
*                         /*******************************************/ 00320000
*                         /*                                         */ 00321000
*                         /* SCAN RBS TO RETRY RB                    */ 00322000
*                         /*                                         */ 00323000
*                         /*******************************************/ 00324000
*                                                                  0163 00325000
*                         DO WHILE(R4^=RTM2RYRB);                  0163 00326000
         B     @DE00163                                            0163 00327000
@DL00163 DS    0H                                                  0164 00328000
*                           R9=NONE;/* ASSUME NO EXTENT LIST         */ 00329000
         SLR   R9,R9                                               0164 00330000
*                           IF RBFTP='000'B/* PRB                    */ 00331000
*                               &RBCDE1->CDNIC=OFF&RBCDE1^=NONE THEN    00332000
         TM    RBFTP(R4),B'11100000'                               0165 00333000
         BNZ   @RF00165                                            0165 00334000
         L     @12,RBCDE1-1(,R4)                                   0165 00335000
         LA    @12,0(,@12)                                         0165 00336000
         TM    CDNIC(@12),B'01000000'                              0165 00337000
         BNZ   @RF00165                                            0165 00338000
         LTR   @12,@12                                             0165 00339000
         BZ    @RF00165                                            0165 00340000
*                                   /* CDE ENTRY PRESENT             */ 00341000
*                             IF RBCDE1->CDMIN=OFF THEN/* MAJOR CDE  */ 00342000
         TM    CDMIN(@12),B'00000100'                              0166 00343000
         BNZ   @RF00166                                            0166 00344000
*                               IF RBCDE1->CDXLE=ON THEN/* EXTENT  0167 00345000
*                                      LIST BUILT                    */ 00346000
         TM    CDXLE(@12),B'00100000'                              0167 00347000
         BNO   @RF00167                                            0167 00348000
*                                 R9=RBCDE1->CDXLMJP;/* LOCATE EXTENT   00349000
*                                      LIST                          */ 00350000
         L     R9,CDXLMJP(,@12)                                    0168 00351000
*                               ELSE                               0169 00352000
*                                 ; /* NO EXTENT LIST                */ 00353000
@RF00167 DS    0H                                                  0170 00354000
*                             ELSE  /* MINOR CDE                     */ 00355000
*                               IF RBCDE1->CDXLMJP->CDXLE=ON THEN/*     00356000
*                                      MAJOR CDE HAS EXTENT LIST     */ 00357000
         B     @RC00166                                            0170 00358000
@RF00166 L     @12,RBCDE1-1(,R4)                                   0170 00359000
         L     @12,CDXLMJP(,@12)                                   0170 00360000
         TM    CDXLE(@12),B'00100000'                              0170 00361000
         BNO   @RF00170                                            0170 00362000
*                                 R9=RBCDE1->CDXLMJP->CDXLMJP;/*   0171 00363000
*                                      LOCATE EXTENT LIST FOR MAJOR     00364000
*                                      CDE                           */ 00365000
         L     R9,CDXLMJP(,@12)                                    0171 00366000
*                           IF R9^=NONE THEN/* EXTENT LIST FOUND     */ 00367000
*                                                                  0172 00368000
@RF00170 DS    0H                                                  0172 00369000
@RC00166 DS    0H                                                  0172 00370000
@RF00165 LTR   R9,R9                                               0172 00371000
         BZ    @RF00172                                            0172 00372000
*                             /***************************************/ 00373000
*                             /*                                     */ 00374000
*                             /* FINDDCBS LOCATES AND CLOSES ALL OPEN*/ 00375000
*                             /* DATASETS EMBEDDED IN THE CODE FOR A */ 00376000
*                             /* GIVEN PRB. IT EXAMINES EACH DEB ON  */ 00377000
*                             /* THE TCB DEB QUEUE TO DETERMINE      */ 00378000
*                             /* WHETHER ITS DCB FALLS WITHIN ANY    */ 00379000
*                             /* EXTENT FOR THIS RB. IF IT DOES,     */ 00380000
*                             /* FINDIOB IS CALLED TO REMOVE RELATED */ 00381000
*                             /* IOBS FROM THE RESTORE CHAIN. THE    */ 00382000
*                             /* DATA MANAGEMENT RESOURCE MANAGER IS */ 00383000
*                             /* INVOKED TO CLOSE THE DATASET.       */ 00384000
*                             /*                                     */ 00385000
*                             /***************************************/ 00386000
*                                                                  0173 00387000
*                             DO;   /* START OF FINDDCBS             */ 00388000
*                               RESPECIFY                          0174 00389000
*                                (R11,/* PTR TO DEB                  */ 00390000
*                                 R2) RESTRICTED;/* LOOP CONTROL   0174 00391000
*                                      VARIABLE                      */ 00392000
*                               ESAGLREC=ON;/* SET RECURSION CONTROL */ 00393000
         L     @12,RTM2VRBC(,RTM2PTR)                              0175 00394000
         OI    ESAGLREC+96(@12),B'00010000'                        0175 00395000
*                               DO; /* SETLOCK (OBTAIN) MODE(UNCOND)    00396000
*                                      TYPE(LOCAL) RELATED(DEB,IEAVTA   00397000
*                                      S3(FINDDCBS))                 */ 00398000
*                                 RFY                              0177 00399000
*                                  (GPR11P,                        0177 00400000
*                                   GPR12P,                        0177 00401000
*                                   GPR13P,                        0177 00402000
*                                   GPR14P) RSTD;                  0177 00403000
*                                 GPR13P=PSALITA;/* ADDRESS OF LOCK     00404000
*                                      INTERFACE TABLE.              */ 00405000
         L     GPR13P,PSALITA                                      0178 00406000
*                                 GEN(LM GPR11P,GPR13P,576(GPR13P));    00407000
         LM GPR11P,GPR13P,576(GPR13P)                                   00408000
*                                   /* LOAD LOCKWORD, CLHT AND ENTRY    00409000
*                                      ADRS.                         */ 00410000
*                                 GEN(BALR GPR14P,GPR13P);/* BALR TO    00411000
