@RC00471 LTR   R0,R0                                               0476 01555000
         BNZ   @RF00476                                            0476 01556000
*               R1=SDWAPTR;         /* R1 CONTAINS PTR TO SDWA       */ 01557000
         LR    R1,SDWAPTR                                          0477 01558000
*             ELSE                                                 0478 01559000
*               DO;                                                0478 01560000
         B     @RC00476                                            0478 01561000
@RF00476 DS    0H                                                  0479 01562000
*                 IF SCBESTAE=ON THEN/* TEST IF ESTAE EXIT           */ 01563000
         TM    SCBESTAE(SCBPTR),B'00010000'                        0479 01564000
         BNO   @RF00479                                            0479 01565000
*                   R1=SCBPARMA;    /* R1 CONTAINS PTR TO USER     0480 01566000
*                                      PARAMETER LIST                */ 01567000
         L     R1,SCBPARMA-1(,SCBPTR)                              0480 01568000
