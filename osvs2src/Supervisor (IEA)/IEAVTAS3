         LA    @12,0(,@12)                                         0375 01213000
         SL    @12,@CF00472                                        0375 01214000
         TM    RBABEND(@12),B'00100000'                            0375 01215000
         BO    @RF00375                                            0375 01216000
*                             DO;   /* RESET ALL PREVIOUSLY ENTERED     01217000
*                                      (E)STAI INUSE BITS            */ 01218000
*                               RESPECIFY                          0377 01219000
*                                 R8 RESTRICTED;                   0377 01220000
*                               R8=RTM2SCBN;/* TEMPORARY SCB PTR USED   01221000
*                                      TO SCAN SCB QUEUE             */ 01222000
         L     R8,RTM2SCBN(,RTM2PTR)                               0378 01223000
*                               DO WHILE(R8^=RTM2SCBO);/* SEARCH FOR    01224000
*                                      (E)STAI SCB-S                 */ 01225000
         B     @DE00379                                            0379 01226000
@DL00379 DS    0H                                                  0380 01227000
*                                 IF R8->SCBSTAI=ON THEN/* TEST IF 0380 01228000
*                                      (E)STAI SCB                   */ 01229000
         TM    SCBSTAI(R8),B'10000000'                             0380 01230000
         BNO   @RF00380                                            0380 01231000
*                                   R8->SCBINUSE=OFF;/* TURN OFF   0381 01232000
*                                      INUSE FLAG                    */ 01233000
         NI    SCBINUSE(R8),B'11101111'                            0381 01234000
*                                 R8=R8->SCBCHAIN;/* UPDATE SCB    0382 01235000
*                                      POINTER                       */ 01236000
@RF00380 L     R8,SCBCHAIN(,R8)                                    0382 01237000
*                               END;/* END (E)STAI SEARCH            */ 01238000
@DE00379 C     R8,RTM2SCBO(,RTM2PTR)                               0383 01239000
         BNE   @DL00379                                            0383 01240000
*                               RESPECIFY                          0384 01241000
*                                 R8 UNRESTRICTED;                 0384 01242000
*                             END;  /* END RESETTING OF (E)STAI INUSE   01243000
*                                      BITS                          */ 01244000
*                           ELSE                                   0386 01245000
*                             ;                                    0386 01246000
@RF00375 DS    0H                                                  0387 01247000
*                         ELSE                                     0387 01248000
*                           ;                                      0387 01249000
@RF00374 DS    0H                                                  0388 01250000
*                       END;        /* END (E)STAI SCB PROCESSING    */ 01251000
*                   END;            /* END SCBUPDAT                  */ 01252000
@RF00371 DS    0H                                                  0390 01253000
*                   RTM2RETR=ON;    /* INDICATE RETRY TO RTM2        */ 01254000
         OI    RTM2RETR(RTM2PTR),B'01000000'                       0390 01255000
*                 END;                                             0391 01256000
*               RTM2TRRA=NONE;      /* CLEAR RECURSION ENVIRONMENT   */ 01257000
@RF00138 SLR   @12,@12                                             0392 01258000
         ST    @12,RTM2TRRA(,RTM2PTR)                              0392 01259000
*               RTM2RTYS=OFF;                                      0393 01260000
         NI    RTM2RTYS(RTM2PTR),B'10111111'                       0393 01261000
*             END;                                                 0394 01262000
*           IF RTM2RETR^=ON THEN    /* RETRY NOT REQUESTED OR NOT  0395 01263000
