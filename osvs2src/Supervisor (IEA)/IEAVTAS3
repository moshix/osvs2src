*               R9=ADDR(SDWASRSV);  /* UPDATED REGISTERS ARE IN SDWA */ 01490000
         B     @RC00460                                            0465 01491000
@RF00460 LA    R9,SDWASRSV(,SDWAPTR)                               0465 01492000
*             RTM2UPRG=ON;          /* INDICATE ALL REGS ARE TO BE 0466 01493000
*                                      PASSED TO RETRY ROUTINE     0466 01494000
*                                                            @G33SPHW*/ 01495000
@RC00460 OI    RTM2UPRG(RTM2PTR),B'01000000'                       0466 01496000
*             RTM2RYRG=R9->RSAVE;   /* COPY APPROIATE REGISTER TO  0467 01497000
*                                      RTM2WA FOR SAFEKEEPING      0467 01498000
*                                                            @G33SPHW*/ 01499000
         MVI   RTM2RYRG+64(RTM2PTR),C' '                           0467 01500000
         MVC   RTM2RYRG+65(3,RTM2PTR),RTM2RYRG+64(RTM2PTR)         0467 01501000
         MVC   RTM2RYRG(64,RTM2PTR),RSAVE(R9)                      0467 01502000
*           END;                    /* END RETRY SET UP WITH USER  0468 01503000
*                                      UPDATED REGISTERS             */ 01504000
*         ELSE                                                     0469 01505000
*                                                                  0469 01506000
*           /*********************************************************/ 01507000
*           /*                                                       */ 01508000
*           /* THIS SEGMENT WILL INITIALIZE PARAMETER REGISTERS TO BE*/ 01509000
*           /* SENT TO THE RETRY ROUTINE. IT THEN SCANS THE RB-S TO  */ 01510000
*           /* BE PURGED AND INSERTS THE REGISTER IN THE RB-S        */ 01511000
*           /* REGISTER SAVE AREA IF NECESSARY. THE WAIT COUNT OF THE*/ 01512000
*           /* RB-S TO BE PURGED AND OF THE RETRYING RB IS ZEROED.   */ 01513000
*           /* SVC 3 IS INSERTED IN THE PSW-S OF THE RB-S TO BE      */ 01514000
*           /* PURGED AND THE RETRY ADDRESS IS INSERTED IN THE PSW OF*/ 01515000
*           /* THE RETRYING RB.                                      */ 01516000
*           /*                                                       */ 01517000
*           /*********************************************************/ 01518000
*                                                                  0469 01519000
*           DO;                     /* BEGIN PARMREGS                */ 01520000
         B     @RC00457                                            0469 01521000
@RF00457 DS    0H                                                  0470 01522000
*             RESPECIFY                                            0470 01523000
*              (R0,                                                0470 01524000
*               R1,                                                0470 01525000
*               R14,                                               0470 01526000
*               R2) RESTRICTED;     /* PARAMETER REGISTERS TO RETRY     01527000
*                                      RB                            */ 01528000
*             IF SDWAPTR=NONE THEN  /* TEST IF THERE IS AN SDWA      */ 01529000
         LTR   SDWAPTR,SDWAPTR                                     0471 01530000
         BNZ   @RF00471                                            0471 01531000
*               R0=RC12;            /* INDICATE SDWA NOT AVAILABLE   */ 01532000
         LA    R0,12                                               0472 01533000
*             ELSE                                                 0473 01534000
*               IF SCBESTAE=ON&SDWAFREE=ON THEN/* TEST IF USER     0473 01535000
*                                      REQUESTED TO FREE SDWA PRIOR     01536000
*                                      TO RETRY                      */ 01537000
         B     @RC00471                                            0473 01538000
@RF00471 TM    SCBESTAE(SCBPTR),B'00010000'                        0473 01539000
