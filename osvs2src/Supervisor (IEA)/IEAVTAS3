*           DO;                                                    0458 01474000
*             RESPECIFY                                            0459 01475000
*              (R9,                                                0459 01476000
*               R4) RESTRICTED;                                    0459 01477000
*             IF RTM2AS1R=ON THEN   /* TEST IF RETRY INTO IEAVTAS1   */ 01478000
         TM    RTM2AS1R(RTM2PTR),B'00000100'                       0460 01479000
         BNO   @RF00460                                            0460 01480000
*               DO;                 /* SETUP FOR RETRY       @G33SPHW*/ 01481000
