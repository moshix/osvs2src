         B     @RC00121                                            0130 00147000
@RF00121 L     @12,RSMSPCT(,RSMHDPTR)                              0130 00148000
         TM    SPCTSWIN(@12),B'10000000'                           0130 00149000
         BNO   @RF00130                                            0130 00150000
*           DO;                     /* IS A SWAP IN. CANCEL THE SWAP.*/ 00151000
*             RESPECIFY                                            0132 00152000
*               PCBR BASED(SPCTSWRT);                              0132 00153000
*             PCBRFAIL='0'B;        /* IGNORE ANY I/O FAILURES.      */ 00154000
*             PCBRPB='1'B;          /* HAVE THE SWAP IN ROOT FREE ANY   00155000
*                                      I/O COMPLETE PCBS AND UNDO  0134 00156000
*                                      INCORE COMMON AREA FIXES.     */ 00157000
         L     @12,SPCTSWRT(,@12)                                  0134 00158000
