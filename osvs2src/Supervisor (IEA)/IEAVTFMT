@RC00711 DS    0H                                                  0750 02218000
*                   END;            /* END LOOP                      */ 02219000
@RC00705 LA    @14,1                                               0750 02220000
         AL    @14,LOOPCNTR                                        0750 02221000
         ST    @14,LOOPCNTR                                        0750 02222000
         CH    @14,@CH04158                                        0750 02223000
         BNH   @DL00702                                            0750 02224000
@DC00702 DS    0H                                                  0751 02225000
*                 END;              /* END COULD ACCESS QUEUE ELEMENT*/ 02226000
*               RFY                                                0752 02227000
*                 ASCB BASED(ASCBPTR);/* REBASE BACK                 */ 02228000
@RC00698 DS    0H                                                  0753 02229000
*             END;                  /* END SU5 ON SYSTEM             */ 02230000
*                                                                  0753 02231000
*           /*********************************************************/ 02232000
*           /*                                                       */ 02233000
*           /* SU5 IS NOT ON THE SYSTEM, ONLY ASCBS ON THE QUEUE USE */ 02234000
