         BZ    @RF00698                                            0698 02055000
*                 BUFFER(1:LENGTH(NOIHSA))=NOIHSA;/* ACCESS ERROR    */ 02056000
         L     @14,ADPLBUF(,ABDPLPTR)                              0699 02057000
         MVC   BUFFER(95,@14),NOIHSA                               0699 02058000
*               ELSE                                               0700 02059000
*                 DO;               /* FOUND ADDR OF FIRST ELEMENT   */ 02060000
         B     @RC00698                                            0700 02061000
@RF00698 DS    0H                                                  0701 02062000
*                   CMSASCBP=DATAWORD;/* ADDRESS OF FIRST QUEUE    0701 02063000
*                                      ELEMENT                       */ 02064000
         L     CMSASCBP,DATAWORD(,DATAPTR)                         0701 02065000
*                   DO LOOPCNTR=1 TO 300 WHILE(CMSASCBP^=ZERO);/*  0702 02066000
*                                      LOOP TILL END OF QUEUE-300 MAX   02067000
*                                      TO AVOID INFINITE LOOP TROUBLE*/ 02068000
         LA    @14,1                                               0702 02069000
         ST    @14,LOOPCNTR                                        0702 02070000
@DL00702 LTR   CMSASCBP,CMSASCBP                                   0702 02071000
         BZ    @DC00702                                            0702 02072000
*                     VADR=ADDR(ASCBASCB);/* LOCATE 'ASCB' FIELD     */ 02073000
         LR    VADR,CMSASCBP                                       0703 02074000
*                     CALL ACCESRTN;/* ACCESS IT                     */ 02075000
         BAL   @14,ACCESRTN                                        0704 02076000
*                     IF RCREG^=ZERO THEN                          0705 02077000
         LTR   RCREG,RCREG                                         0705 02078000
         BZ    @RF00705                                            0705 02079000
*                       DO;         /* ACCESS ERROR                  */ 02080000
*                         BUFFER(1:LENGTH(NOIHSA))=NOIHSA;         0707 02081000
         L     @14,ADPLBUF(,ABDPLPTR)                              0707 02082000
         MVC   BUFFER(95,@14),NOIHSA                               0707 02083000
*                         CMSASCBP=ZERO;/* END LOOP                  */ 02084000
         SLR   CMSASCBP,CMSASCBP                                   0708 02085000
*                       END;        /* END ACCESS ERROR              */ 02086000
*                     ELSE                                         0710 02087000
*                       DO;         /* FOUND 'ASCB' ADDRESS          */ 02088000
*                                                                  0710 02089000
         B     @RC00705                                            0710 02090000
@RF00705 DS    0H                                                  0711 02091000
*                         /*******************************************/ 02092000
*                         /*                                         */ 02093000
*                         /* MAKE SURE THE ELEMENT IS AN ASCB. BOTH  */ 02094000
*                         /* ASCBS AND SRBS ARE ON THE SU5 TYPE      */ 02095000
*                         /* QUEUE.                                  */ 02096000
*                         /*                                         */ 02097000
*                         /*******************************************/ 02098000
*                                                                  0711 02099000
*                         IF DATAWORD='ASCB' THEN                  0711 02100000
         CLC   DATAWORD(4,DATAPTR),@CF04159                        0711 02101000
         BNE   @RF00711                                            0711 02102000
*                           DO;     /* THIS Q ELEMENT IS AN ASCB     */ 02103000
*                             IF CMSASCBP=ASCBPTR THEN             0713 02104000
