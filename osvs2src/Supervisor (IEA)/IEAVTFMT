*                         CALL PRINT;/* 3 BLANKS                     */ 01209000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0479 01210000
         BALR  @14,@15                                             0479 01211000
*                         CALL PRINT;/* BLANK                        */ 01212000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0480 01213000
         BALR  @14,@15                                             0480 01214000
*                         CALL PRINT;/* BLANK                        */ 01215000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0481 01216000
         BALR  @14,@15                                             0481 01217000
*                         CALL IEAFTSCB;/* SCB FORMAT ROUTINE        */ 01218000
         L     @15,@CV03875                                        0482 01219000
         BALR  @14,@15                                             0482 01220000
*                       END;        /* END LOOP                      */ 01221000
         BCTR  STKPTR,0                                            0483 01222000
@DE00475 LTR   STKPTR,STKPTR                                       0483 01223000
         BP    @DL00475                                            0483 01224000
*                     END;          /* END SCB FOUND TO BE FORMATTED */ 01225000
*                   ELSE                                           0485 01226000
*                     ;             /* NO SCBS TO BE FORMATTED       */ 01227000
@RF00456 DS    0H                                                  0486 01228000
*                 END;              /* END SCB FOUND                 */ 01229000
*             END;                  /* END THIS IS AN ABEND TCB      */ 01230000
*           ELSE                                                   0488 01231000
*             ;                     /* NOT AN ABEND TCB              */ 01232000
@RF00448 DS    0H                                                  0489 01233000
*         END;                      /* END FLGS1 FOUND               */ 01234000
@RC00448 DS    0H                                                  0490 01235000
*     END;                          /* END FOUND COMPLETION CODE     */ 01236000
@RC00445 DS    0H                                                  0491 01237000
*   CALL PRINT;                     /* MSG OR BLANKS                 */ 01238000
@RC00439 L     @15,ADPLPRNT(,ABDPLPTR)                             0491 01239000
         BALR  @14,@15                                             0491 01240000
*   END SCBRTN;                                                    0492 01241000
@EL00005 DS    0H                                                  0492 01242000
@EF00005 DS    0H                                                  0492 01243000
@ER00005 L     @14,@SA00005                                        0492 01244000
         BR    @14                                                 0492 01245000
*RT1MAIN:                                                          0493 01246000
*   PROC OPTIONS(SAVE(14));                                        0493 01247000
*                                                                  0493 01248000
RT1MAIN  ST    @14,@SA00006                                        0493 01249000
*/********************************************************************/ 01250000
*/* FUNCTION-                                                      0494 01251000
*   ROUTINE TO DETERMINE IF THERE IS ANY RTM1 INFORMATION.  IF     0494 01252000
*   THE ASCBLOCK HAS A ZERO IN IT THERE IS NO RELATED RTM1         0494 01253000
*   INFORMATION.  IF THE LOCKWORD HAS 'FFFFFFFF' OR '7FFFFFFF'     0494 01254000
*   THERE IS A POSSIBILY OF A RELATED IHSA.  IF IT CONTAINS        0494 01255000
*   '00000040' OR '00000041' THERE IS THE POSSIBILITY OF A RELATED 0494 01256000
*   FRR STACK.                                                     0494 01257000
*   IF THE ASCBLOCK IS OTHER THAN THE ABOVE, THE FOLLOWING MESSAGE 0494 01258000
*   WILL BE PRINTED:                                               0494 01259000
*             *** UNKNOWN VALUE IN ASCBLOCK-RTM1 FORMATTING HALTED ***  01260000
*                                                                  0494 01261000
* NOTE - THIS ROUTINE WILL HANDLE ONLY 2 CPUS                      0494 01262000
*                                                                  0494 01263000
*                                                                  0494 01264000
*    CALLS-                                                        0494 01265000
*         ACCESRTN,CURRENT,INTERRUP,SUSPEND, AND PRINT             0494 01266000
*                                                                  0494 01267000
*    CALLED BY -                                                   0494 01268000
*         MAIN PROCEDURE                                           0494 01269000
*                                                                  0494 01270000
*    RETURNS TO -                                                  0494 01271000
*         MAIN PROCEDURE                                           0494 01272000
*                                                                    */ 01273000
*/********************************************************************/ 01274000
*/*   ACCESS THE ASCBLOCK WHICH CONTAINS INFORMATION NEEDED TO     0494 01275000
*     DETERMINE IF RTM1 INFORMATION IS AVAILABLE                     */ 01276000
*                                                                  0494 01277000
*   IF ASCBPTR^=ZERO THEN                                          0494 01278000
         LTR   ASCBPTR,ASCBPTR                                     0494 01279000
         BZ    @RF00494                                            0494 01280000
*     DO;                           /* THE ASCB COULD BE ACCESSED  0495 01281000
*                                      DURING RTCT PROCESSING.       */ 01282000
*       IF SU5SAVE=ON THEN                                         0496 01283000
         TM    SU5SAVE,B'01000000'                                 0496 01284000
         BNO   @RF00496                                            0496 01285000
*         VADR=ADDR(ASCBLOCK);      /* SU5 ON SYSTEM                 */ 01286000
         LA    VADR,ASCBLOCK(,ASCBPTR)                             0497 01287000
*       ELSE                                                       0498 01288000
*         VADR=ADDR(ASCBLCKB);      /* SU5 NOT ON SYSTEM-ASCB CHANGED   01289000
*                                      FOR SU5                       */ 01290000
         B     @RC00496                                            0498 01291000
@RF00496 LA    VADR,ASCBLCKB(,ASCBPTR)                             0498 01292000
*       CALL ACCESRTN;              /* ACCESS IT                     */ 01293000
@RC00496 BAL   @14,ACCESRTN                                        0499 01294000
*       IF RCREG^=ZERO THEN                                        0500 01295000
         LTR   RCREG,RCREG                                         0500 01296000
         BZ    @RF00500                                            0500 01297000
*         BUFFER(1:LENGTH(UNKNOWN))=UNKNOWN;/* COULD NOT ACCESS    0501 01298000
*                                      ASCBLOCK                      */ 01299000
         L     @14,ADPLBUF(,ABDPLPTR)                              0501 01300000
         MVC   BUFFER(60,@14),UNKNOWN                              0501 01301000
*       ELSE                                                       0502 01302000
*         DO;                       /* FOUND ASCBLOCK                */ 01303000
         B     @RC00500                                            0502 01304000
@RF00500 DS    0H                                                  0503 01305000
*           IF DATAWORD^=ZERO THEN                                 0503 01306000
         L     @14,DATAWORD(,DATAPTR)                              0503 01307000
         LTR   @14,@14                                             0503 01308000
         BZ    @RF00503                                            0503 01309000
*             DO;                   /* LOCAL LOCK IS HELD-POSSIBLE 0504 01310000
*                                      INFO                          */ 01311000
*               IF DATAWORD=CPU0|DATAWORD=CPU1 THEN                0505 01312000
         C     @14,CPU0                                            0505 01313000
         BE    @RT00505                                            0505 01314000
         C     @14,CPU1                                            0505 01315000
         BNE   @RF00505                                            0505 01316000
@RT00505 DS    0H                                                  0506 01317000
*                 CALL CURRENT;     /* LOCAL LOCK HELD               */ 01318000
         BAL   @14,CURRENT                                         0506 01319000
*               ELSE                                               0507 01320000
*                 IF DATAWORD=INTER THEN                           0507 01321000
         B     @RC00505                                            0507 01322000
@RF00505 CLC   DATAWORD(4,DATAPTR),INTER                           0507 01323000
         BNE   @RF00507                                            0507 01324000
*                   CALL INTERRUP;  /* TASK INTERRUPTED WHILE HOLDING   01325000
*                                      LOCAL LOCK                    */ 01326000
         BAL   @14,INTERRUP                                        0508 01327000
*                 ELSE                                             0509 01328000
*                   IF DATAWORD=SUSP THEN                          0509 01329000
         B     @RC00507                                            0509 01330000
@RF00507 CLC   DATAWORD(4,DATAPTR),SUSP                            0509 01331000
         BNE   @RF00509                                            0509 01332000
*                     CALL SUSPEND; /* ADDRESS SPACE SUSPENDED       */ 01333000
         BAL   @14,SUSPEND                                         0510 01334000
*                   ELSE                                           0511 01335000
*                     BUFFER(1:LENGTH(UNKNOWN))=UNKNOWN;/* UNKNOWN 0511 01336000
*                                      LOCKWORD                      */ 01337000
         B     @RC00509                                            0511 01338000
@RF00509 L     @14,ADPLBUF(,ABDPLPTR)                              0511 01339000
         MVC   BUFFER(60,@14),UNKNOWN                              0511 01340000
*             END;                  /* END ASCBLOCK HELD             */ 01341000
*           ELSE                                                   0513 01342000
*             ;                     /* LOCAL LOCK NOT HELD-NO RTM1 0513 01343000
*                                      INFO                          */ 01344000
@RF00503 DS    0H                                                  0514 01345000
*         END;                      /* END COULD ACCESS ASCBLOCK     */ 01346000
*     END;                          /* END ASCB COULD BE ACCESSED    */ 01347000
*   ELSE                                                           0516 01348000
*     ;                             /* ASCB COULD NOT BE ACCESSED    */ 01349000
@RF00494 DS    0H                                                  0517 01350000
*   CALL PRINT;                     /* MSG OR A BLANK                */ 01351000
@RC00494 L     @15,ADPLPRNT(,ABDPLPTR)                             0517 01352000
         BALR  @14,@15                                             0517 01353000
*CURRENT:                                                          0518 01354000
*   PROC OPTIONS(SAVE(14));                                        0518 01355000
*                                                                  0518 01356000
         B     @PB00007                                            0518 01357000
CURRENT  ST    @14,@SA00007                                        0518 01358000
*/********************************************************************/ 01359000
*/* FUNCTION-                                                      0519 01360000
*   ROUTINE TO PROCESS FRRS IF THE TASK WAS THE CURRENT TASK HOLDING    01361000
*   THE LOCAL LOCK.  IF THE TASK WAS RUNNING ON A CPU OTHER THAN   0519 01362000
*   THE ONE WHICH TOOK THE DUMP, THE CORRECT PSA WILL BE LOCATED TO     01363000
*   OBTAIN THE CORRECT FFRS. IF THE ACCESS ROUTINE ENCOUNTERS AN   0519 01364000
*   ERROR LOCATING INFORMATION FROM OR IN FINDING THE PSA,         0519 01365000
*   THE FOLLOWING MESSAGE WILL BE PRINTED:                         0519 01366000
*                  *** CURRENT FRR STACK NOT AVAILABLE: UNABLE TO LOCAT 01367000
*                      PSA ***.                                    0519 01368000
*                                                                  0519 01369000
*                                                                  0519 01370000
*    CALLS-                                                        0519 01371000
*         ACCESRTN,PRINT                                           0519 01372000
*                                                                  0519 01373000
*    CALLED BY -                                                   0519 01374000
*         RT1MAIN                                                  0519 01375000
*                                                                  0519 01376000
*    RETURNS TO -                                                  0519 01377000
*         RT1MAIN                                                  0519 01378000
*                                                                    */ 01379000
*/********************************************************************/ 01380000
*/*     IF THE DUMP WAS TAKEN FOR AN MP SYSTEM, A TAKK COULD HAVE  0519 01381000
*   RUNNING ON EITHER CPU(THIS CODE IS FOR A 2 CPU SYSTEM). THE    0519 01382000
*   LOCKWORD IN THE ASCB IS BEING USED TO DETERMINE THE CORRECT PSA     01383000
*   THAT CORRESPONDS TO THE TCB IN QUESTION. FOR A LOCKWORD OF 40 OR    01384000
*   41, THE LOCKWORD MATCHES THE LOGICAL CPU ADDRESS IN THE PSA. IN     01385000
*   THIS WAY IT CAN BE DETERMINED WHICH PSA IS THE CORRECT ONE TO  0519 01386000
*   SEARCH FOR THE FRR STACK. THE POINTERS USED TO GET TO THE OTHER     01387000
*   PSA ARE AS FOLLOWS:                                            0519 01388000
*          CVT -> PCCAVT -> PCCA -> PSA                              */ 01389000
*                                                                  0519 01390000
*   ERRSW=OFF;                      /* INITIALIZE PSA PROBLEMS OFF   */ 01391000
         NI    ERRSW,B'01111111'                                   0519 01392000
*   PSAPTR=ZERO;                    /* VIRTUAL PSA ADDRESS           */ 01393000
         SLR   PSAPTR,PSAPTR                                       0520 01394000
*   LOCK=DATAWORD;                  /* ASCBLOCK-EITHER 40 OR 41      */ 01395000
         MVC   LOCK(4),DATAWORD(DATAPTR)                           0521 01396000
*   VADR=ADDR(PSACPULA);            /* CPU LOGICAL ADDRESS-EITHER 40    01397000
*                                      OR 41                         */ 01398000
         LA    VADR,PSACPULA(,PSAPTR)                              0522 01399000
*   CALL ACCESRTN;                  /* ACCESS IT                     */ 01400000
         BAL   @14,ACCESRTN                                        0523 01401000
*   IF RCREG^=ZERO THEN                                            0524 01402000
         LTR   RCREG,RCREG                                         0524 01403000
         BZ    @RF00524                                            0524 01404000
*     DO;                           /* COULD NOT ACCESS              */ 01405000
*       BUFFER(1:LENGTH(BADPSA))=BADPSA;                           0526 01406000
         L     @14,ADPLBUF(,ABDPLPTR)                              0526 01407000
         MVC   BUFFER(61,@14),BADPSA                               0526 01408000
*       ERRSW=ON;                   /* PROBLEMS GETTING PSA        0527 01409000
*                                      INFORMATION                   */ 01410000
         OI    ERRSW,B'10000000'                                   0527 01411000
*     END;                                                         0528 01412000
*   ELSE                                                           0529 01413000
*     DO;                           /* COULD ACCESS                  */ 01414000
         B     @RC00524                                            0529 01415000
@RF00524 DS    0H                                                  0530 01416000
*       IF DATAWORD^=LOCKHALF THEN                                 0530 01417000
         L     @14,DATAWORD(,DATAPTR)                              0530 01418000
         CH    @14,LOCKHALF                                        0530 01419000
         BE    @RF00530                                            0530 01420000
*         DO;                       /* THE TASK WAS RUNNING ON     0531 01421000
*                                      ALTERNATE CPU                 */ 01422000
*           VADR=ADDR(CVTPCCAT);    /* GET VIRTUAL ADDR OF OTHER CPU-   01423000
*                                      PTR TO PCCAVT                 */ 01424000
         L     @14,CVTPTR(,ABDPLPTR)                               0532 01425000
         LA    VADR,CVTPCCAT(,@14)                                 0532 01426000
*           CALL ACCESRTN;          /* ACCESS IT                     */ 01427000
         BAL   @14,ACCESRTN                                        0533 01428000
*           IF RCREG^=ZERO THEN                                    0534 01429000
         LTR   RCREG,RCREG                                         0534 01430000
         BZ    @RF00534                                            0534 01431000
*             DO;                                                  0535 01432000
*               BUFFER(1:LENGTH(BADPSA))=BADPSA;                   0536 01433000
         L     @14,ADPLBUF(,ABDPLPTR)                              0536 01434000
         MVC   BUFFER(61,@14),BADPSA                               0536 01435000
*               ERRSW=ON;                                          0537 01436000
         OI    ERRSW,B'10000000'                                   0537 01437000
*             END;                                                 0538 01438000
*           ELSE                                                   0539 01439000
*             DO;                   /* COULD ACCESS PCCAVT           */ 01440000
         B     @RC00534                                            0539 01441000
@RF00534 DS    0H                                                  0540 01442000
*               LOCK=LOCK&LOCKMASK; /* GET THE CPU NUMBER            */ 01443000
         L     @14,LOCK                                            0540 01444000
         N     @14,LOCKMASK                                        0540 01445000
         ST    @14,LOCK                                            0540 01446000
*               VADR=PAVTPTR+(4*LOCK);/* PTR TO PCCA                 */ 01447000
         SLA   @14,2                                               0541 01448000
         AL    @14,PAVTPTR(,DATAPTR)                               0541 01449000
         LR    VADR,@14                                            0541 01450000
*               CALL ACCESRTN;      /* ACCESS IT                     */ 01451000
         BAL   @14,ACCESRTN                                        0542 01452000
*               IF RCREG^=ZERO THEN                                0543 01453000
         LTR   RCREG,RCREG                                         0543 01454000
         BZ    @RF00543                                            0543 01455000
*                 DO;                                              0544 01456000
*                   BUFFER(1:LENGTH(BADPSA))=BADPSA;               0545 01457000
         L     @14,ADPLBUF(,ABDPLPTR)                              0545 01458000
         MVC   BUFFER(61,@14),BADPSA                               0545 01459000
*                   ERRSW=ON;                                      0546 01460000
         OI    ERRSW,B'10000000'                                   0546 01461000
*                 END;                                             0547 01462000
*               ELSE                                               0548 01463000
*                 DO;               /* COULD ACCESS PCCA             */ 01464000
         B     @RC00543                                            0548 01465000
@RF00543 DS    0H                                                  0549 01466000
*                   VADR=ADDR(PCCAPSAV);/* PSA VIRTUAL ADDRESS       */ 01467000
         L     @14,PCCAPTR(,DATAPTR)                               0549 01468000
         LA    VADR,PCCAPSAV(,@14)                                 0549 01469000
*                   CALL ACCESRTN;  /* ACCESS IT                     */ 01470000
         BAL   @14,ACCESRTN                                        0550 01471000
*                   IF RCREG^=ZERO THEN                            0551 01472000
         LTR   RCREG,RCREG                                         0551 01473000
         BZ    @RF00551                                            0551 01474000
*                     DO;                                          0552 01475000
*                       BUFFER(1:LENGTH(BADPSA))=BADPSA;           0553 01476000
         L     @14,ADPLBUF(,ABDPLPTR)                              0553 01477000
         MVC   BUFFER(61,@14),BADPSA                               0553 01478000
*                       ERRSW=ON;                                  0554 01479000
         OI    ERRSW,B'10000000'                                   0554 01480000
*                     END;                                         0555 01481000
*                   ELSE            /* COULD ACCESS PSA              */ 01482000
*                     PSAPTR=DATAWORD;/* VIRTUAL PSA ADDRESS         */ 01483000
         B     @RC00551                                            0556 01484000
@RF00551 L     PSAPTR,DATAWORD(,DATAPTR)                           0556 01485000
*                 END;              /* ERRORS IN ACCESS              */ 01486000
*             END;                                                 0558 01487000
*         END;                                                     0559 01488000
*       ELSE                                                       0560 01489000
*         ;                         /* TASK WAS RUNNING ON THIS CPU  */ 01490000
@RF00530 DS    0H                                                  0561 01491000
*     END;                                                         0561 01492000
@RC00530 DS    0H                                                  0562 01493000
*                                                                  0562 01494000
*/********************************************************************/ 01495000
*/* ONCE WE HAVE OUR CORRECT PSA WE MUST DETERMINE IF THE TASK PASSED   01496000
*   WAS THE CURRENT TASK.  IF IT WAS FORMAT, THE FRR STACK.  IF THERE   01497000
*   ARE ACCESS ROUTINE ERRORS IN THIS SECITON THE FOLLOWING MESSAGE     01498000
*   WILL BE PRINTED:                                               0562 01499000
*            *** FRRS NOT FORMATTED-DATA NECESSARY TO LOCATE OR    0562 01500000
*               DETERMINE IT VALIDITY NOT AVAILABLE ***.             */ 01501000
*/********************************************************************/ 01502000
*                                                                  0562 01503000
*   IF ERRSW=OFF THEN                                              0562 01504000
@RC00524 TM    ERRSW,B'10000000'                                   0562 01505000
         BNZ   @RF00562                                            0562 01506000
*     DO;                           /* FOUND THE CORRECT PSA         */ 01507000
*                                                                  0563 01508000
*       /*************************************************************/ 01509000
*       /*                                                           */ 01510000
*       /* ACCESS THE PSA NEEDED TO WORK WITH. THE PSA IS ON A 4K    */ 01511000
*       /* BOUNDRY SO ALL THE PSA WILL BE PLACED IN PRINTDUMPS BUFFER*/ 01512000
*       /* TO BE ACCESSED DIRECTLY WITHOUT THE ACCESS SERVICE        */ 01513000
*       /* ROUTINE.                                                  */ 01514000
*       /*                                                           */ 01515000
*       /*************************************************************/ 01516000
*                                                                  0564 01517000
*       VADR=PSAPTR;                /* PSA ADDRESS                   */ 01518000
         LR    VADR,PSAPTR                                         0564 01519000
*       CALL ACCESRTN;              /* ACCESS IT                     */ 01520000
         BAL   @14,ACCESRTN                                        0565 01521000
*       IF RCREG^=ZERO THEN                                        0566 01522000
         LTR   RCREG,RCREG                                         0566 01523000
         BZ    @RF00566                                            0566 01524000
*         BUFFER(1:LENGTH(NOFRRS))=NOFRRS;                         0567 01525000
         L     @14,ADPLBUF(,ABDPLPTR)                              0567 01526000
         MVC   BUFFER(95,@14),NOFRRS                               0567 01527000
*       ELSE                                                       0568 01528000
*         DO;                       /* FOUND PSA                     */ 01529000
         B     @RC00566                                            0568 01530000
@RF00566 DS    0H                                                  0569 01531000
*           RFY                                                    0569 01532000
*             PSA BASED(DATAPTR);   /* BASE PSA OFF OF CALLERS BUFFER*/ 01533000
*           IF PSAPSA^='PSA ' THEN  /* THIS IS NOT A PSA             */ 01534000
         CLC   PSAPSA(4,DATAPTR),@CC04144                          0570 01535000
         BE    @RF00570                                            0570 01536000
*             BUFFER(1:LENGTH(NOFRRS))=NOFRRS;/* ERROR FINDING FRRS  */ 01537000
         L     @14,ADPLBUF(,ABDPLPTR)                              0571 01538000
         MVC   BUFFER(95,@14),NOFRRS                               0571 01539000
*           ELSE                    /* THIS IS INDEED A PSA          */ 01540000
*             DO;                                                  0572 01541000
         B     @RC00570                                            0572 01542000
@RF00570 DS    0H                                                  0573 01543000
*               IF ADPLTCB=PSATOLD THEN                            0573 01544000
         CLC   ADPLTCB(4,ABDPLPTR),PSATOLD(DATAPTR)                0573 01545000
         BNE   @RF00573                                            0573 01546000
*                 DO;               /* TCB PASSED IS CURRENT TCB     */ 01547000
*/*  TO DOUBLE CHECK THAT THE CURRENT STACK IS VALID, CHECK THE LOCKS   01548000
*        HELD WORD IN THE PSA TO MAKE SURE A LOCK IS HELD            */ 01549000
*                                                                  0575 01550000
*                   IF PSAHLHI^=ZERO THEN                          0575 01551000
         ICM   @14,15,PSAHLHI(DATAPTR)                             0575 01552000
         BZ    @RF00575                                            0575 01553000
*                     DO;           /* FRR STACK VALID               */ 01554000
*                       ADPLCOM1=ADDR(ESTAPARM);/* PASS ESTAE PARM 0577 01555000
*                                      LIST TO INDIVIDUAL FORMAT   0577 01556000
*                                      ROUTINES TO USE               */ 01557000
*                                                                  0577 01558000
         LA    @14,ESTAPARM                                        0577 01559000
         ST    @14,ADPLCOM1(,ABDPLPTR)                             0577 01560000
*                       /*********************************************/ 01561000
*                       /*                                           */ 01562000
*                       /* TO GET THE CORRECT STACK ADDRESS FROM     */ 01563000
*                       /* EITHER CPU, WHEN THE STACK IS WITHIN THE  */ 01564000
*                       /* PSA, ADD THE VIRTUAL ADDRESS OF THE PSA TO*/ 01565000
*                       /* THE ADDRESS OF THE STACK. THIS WILL ENSURE*/ 01566000
*                       /* THE CORRECT ADDRESS EVEN FOR A PSA FOR THE*/ 01567000
*                       /* OTHER CPU                                 */ 01568000
*                       /*                                           */ 01569000
*                       /*********************************************/ 01570000
*                                                                  0578 01571000
*                       IF PSACSTK=NORMLSTK THEN/* STACK IS WITHIN 0578 01572000
*                                      THE PSA                       */ 01573000
         L     @14,PSACSTK(,DATAPTR)                               0578 01574000
         C     @14,NORMLSTK                                        0578 01575000
         BNE   @RF00578                                            0578 01576000
*                         ADPLFMT2=PSAPTR+PSACSTK;/* VIRTUAL ADDRESS    01577000
*                                      OF STACK WITHIN THE PSA       */ 01578000
         ALR   @14,PSAPTR                                          0579 01579000
         ST    @14,ADPLFMT2(,ABDPLPTR)                             0579 01580000
*                       ELSE        /* STACK IS NOT WITHIN THE PSA 0580 01581000
*                                      AND THE VIRTUAL ADDRESS IS  0580 01582000
*                                      CORRECT                       */ 01583000
*                         ADPLFMT2=PSACSTK;/* FILL IN PARMLIST WITH     01584000
*                                      FRR PTR                       */ 01585000
         B     @RC00578                                            0580 01586000
@RF00578 MVC   ADPLFMT2(4,ABDPLPTR),PSACSTK(DATAPTR)               0580 01587000
*                       OFFSET=ZERO;                               0581 01588000
@RC00578 SLR   @14,@14                                             0581 01589000
         ST    @14,OFFSET(,ABDPLPTR)                               0581 01590000
*                       CALL PRINT; /* BLANK                         */ 01591000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0582 01592000
         BALR  @14,@15                                             0582 01593000
*                       CALL PRINT; /* BLANK                         */ 01594000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0583 01595000
         BALR  @14,@15                                             0583 01596000
*                       CALL PRINT; /* BLANK                         */ 01597000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0584 01598000
         BALR  @14,@15                                             0584 01599000
*                       CALL IEAFTFRR;/* FRRS FORMATTER              */ 01600000
         L     @15,@CV03871                                        0585 01601000
         BALR  @14,@15                                             0585 01602000
*                       IF RCREG=ZERO&ADPLFMT2^=ZERO THEN          0586 01603000
         SLR   @14,@14                                             0586 01604000
         CR    RCREG,@14                                           0586 01605000
         BNE   @RF00586                                            0586 01606000
         C     @14,ADPLFMT2(,ABDPLPTR)                             0586 01607000
         BE    @RF00586                                            0586 01608000
*                         CALL RT1EED;/* EED POINTER RETURNED FROM 0587 01609000
*                                      FRRS FORMATTER IS VALID(FRRS     01610000
*                                      FORMATTER COMPLETED         0587 01611000
*                                      SUCESSFULLY &RETURNED A     0587 01612000
*                                      NONZERO EED PTR)              */ 01613000
         BAL   @14,RT1EED                                          0587 01614000
*                       ELSE                                       0588 01615000
*                         ;         /* NO NEED TO FORMAT EED         */ 01616000
