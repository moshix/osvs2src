*                               END;/* END THIS IS CORRECT ASCB      */ 02146000
*                             ELSE                                 0727 02147000
*                               DO; /* NOT ASCB WE ARE LOOKING FOR   */ 02148000
*                                                                  0727 02149000
         B     @RC00713                                            0727 02150000
@RF00713 DS    0H                                                  0728 02151000
*                                 /***********************************/ 02152000
*                                 /*                                 */ 02153000
*                                 /* CHAIN TO NEXT ELEMENT           */ 02154000
*                                 /*                                 */ 02155000
*                                 /***********************************/ 02156000
*                                                                  0728 02157000
*                                 VADR=ADDR(ASCBCMSF);/* LOCATE PTR     02158000
*                                      TO NEXT Q ELEMENT             */ 02159000
