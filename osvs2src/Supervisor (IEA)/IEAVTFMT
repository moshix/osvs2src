@RF00780 L     CMSASCBP,DATAWORD(,DATAPTR)                         0785 02335000
*                       END;        /* END NOT CORRECT ASCB          */ 02336000
@RC00780 DS    0H                                                  0787 02337000
*                   END;            /* END LOOP                      */ 02338000
@RC00763 LA    @14,1                                               0787 02339000
         AL    @14,LOOPCNTR                                        0787 02340000
         ST    @14,LOOPCNTR                                        0787 02341000
         CH    @14,@CH04158                                        0787 02342000
         BNH   @DL00762                                            0787 02343000
@DC00762 DS    0H                                                  0788 02344000
*                   RFY                                            0788 02345000
*                     ASCB BASED(ASCBPTR);                         0788 02346000
*                 END;              /* END COULD ACCESS FIRST ASCB ON   02347000
*                                      Q                             */ 02348000
*             END;                  /* END SU5 NOT ON SYSTEM         */ 02349000
@RC00758 DS    0H                                                  0791 02350000
*         END;                      /* END FOUND CMS Q PTR           */ 02351000
@RC00693 DS    0H                                                  0792 02352000
*     END;                          /* END FOUND LIT ADDRESS         */ 02353000
@RC00690 DS    0H                                                  0793 02354000
*   CALL PRINT;                     /* MSG OR A BLANK LINE           */ 02355000
@RC00685 L     @15,ADPLPRNT(,ABDPLPTR)                             0793 02356000
         BALR  @14,@15                                             0793 02357000
*   END CMSEARCH;                                                  0794 02358000
@EL00010 DS    0H                                                  0794 02359000
@EF00010 DS    0H                                                  0794 02360000
@ER00010 L     @14,@SA00010                                        0794 02361000
         BR    @14                                                 0794 02362000
*   END SUSPEND;                                                   0795 02363000
@EL00009 DS    0H                                                  0795 02364000
@EF00009 DS    0H                                                  0795 02365000
@ER00009 L     @14,@SA00009                                        0795 02366000
         BR    @14                                                 0795 02367000
*RT1EED:                                                           0796 02368000
*   PROC OPTIONS(SAVE(14));                                        0796 02369000
*                                                                  0796 02370000
RT1EED   ST    @14,@SA00011                                        0796 02371000
*/********************************************************************/ 02372000
*/* FUNCTION-                                                      0797 02373000
*   ROUTINE TO FORMAT THE EED'S RETURNED BY EITHER THE FRRS FORMATTER   02374000
*   OR THE IHSA FORMATTER.  A MAXIMUM OF 25 WILL BE FORMATTED.  THE     02375000
*   MESSAGES ARE THE SAME AS THOSE PRINTED BY EEDRTN.              0797 02376000
*                                                                  0797 02377000
*                                                                  0797 02378000
*    CALLS-                                                        0797 02379000
*         ACCESRTN,IEAFTEED, AND PRINT                             0797 02380000
*                                                                  0797 02381000
*    CALLED BY -                                                   0797 02382000
*         CURRENT, INTERRUP, SUSPEND, AND CMSEARCH                 0797 02383000
*                                                                  0797 02384000
*    RETURNS TO -                                                  0797 02385000
*         CALLER                                                   0797 02386000
*                                                                    */ 02387000
*/********************************************************************/ 02388000
*                                                                  0797 02389000
*   EEDPTR=ADPLFMT2;                                               0797 02390000
         L     EEDPTR,ADPLFMT2(,ABDPLPTR)                          0797 02391000
*   DO STKPTR=1 TO 25 WHILE(EEDPTR^=ZERO);/* LOOP FOR EEDS           */ 02392000
         LA    STKPTR,1                                            0798 02393000
@DL00798 LTR   EEDPTR,EEDPTR                                       0798 02394000
         BZ    @DC00798                                            0798 02395000
*     SAVESTK(STKPTR)=EEDPTR;       /* SAVE EED ADDRESS              */ 02396000
         LR    @14,STKPTR                                          0799 02397000
         SLA   @14,2                                               0799 02398000
         ST    EEDPTR,SAVESTK-4(@14)                               0799 02399000
*     VADR=ADDR(EEDFWRDP);          /* LOCATE NEXT EED PTR           */ 02400000
         LR    VADR,EEDPTR                                         0800 02401000
*     CALL ACCESRTN;                /* ACCESS IT                     */ 02402000
         BAL   @14,ACCESRTN                                        0801 02403000
*     IF RCREG^=ZERO THEN                                          0802 02404000
         LTR   RCREG,RCREG                                         0802 02405000
         BZ    @RF00802                                            0802 02406000
*       DO;                         /* ACCESS ERROR                  */ 02407000
*         BUFFER(1:LENGTH(SOMEEED))=SOMEEED;                       0804 02408000
         L     @14,ADPLBUF(,ABDPLPTR)                              0804 02409000
         MVC   BUFFER(49,@14),SOMEEED                              0804 02410000
*         EEDPTR=ZERO;              /* END LOOP                      */ 02411000
         SLR   EEDPTR,EEDPTR                                       0805 02412000
*       END;                        /* END ACCESS ERROR              */ 02413000
*     ELSE                          /* FOUND NEXT EED PTR            */ 02414000
*       EEDPTR=DATAWORD;            /* NEXT EED ADDRESS              */ 02415000
         B     @RC00802                                            0807 02416000
@RF00802 L     EEDPTR,DATAWORD(,DATAPTR)                           0807 02417000
*   END;                            /* END LOOP                      */ 02418000
@RC00802 AH    STKPTR,@CH00041                                     0808 02419000
         CH    STKPTR,@CH03950                                     0808 02420000
