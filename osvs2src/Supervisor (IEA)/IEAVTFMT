*                     IF CMSASCBP=ASCBPTR THEN                     0763 02272000
         CR    CMSASCBP,ASCBPTR                                    0763 02273000
         BNE   @RF00763                                            0763 02274000
*                       DO;         /* ASCB LOOKED FOR               */ 02275000
*                         ADPLCOM1=ADDR(ESTAPARM);/* PASS ESTAE PARM    02276000
*                                      LIST TO INDIVIDUAL FORMAT   0765 02277000
*                                      ROUTINES TO USE               */ 02278000
         LA    @09,ESTAPARM                                        0765 02279000
         ST    @09,ADPLCOM1(,ABDPLPTR)                             0765 02280000
*                         ADPLFMT2=IHSAPTR;/* FILL IN PARMLIST       */ 02281000
         ST    IHSAPTR,ADPLFMT2(,ABDPLPTR)                         0766 02282000
*                         OFFSET=ZERO;                             0767 02283000
         ST    @14,OFFSET(,ABDPLPTR)                               0767 02284000
*                         CALL PRINT;/* 3 BLANKS                     */ 02285000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0768 02286000
         BALR  @14,@15                                             0768 02287000
*                         CALL PRINT;/* BLANK                        */ 02288000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0769 02289000
         BALR  @14,@15                                             0769 02290000
*                         CALL PRINT;/* BLANK                        */ 02291000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0770 02292000
         BALR  @14,@15                                             0770 02293000
*                         CALL IEAFTIHS;/* IHSA FORMATTER            */ 02294000
         L     @15,@CV03872                                        0771 02295000
         BALR  @14,@15                                             0771 02296000
*                         IF RCREG=ZERO&ADPLFMT2^=ZERO THEN        0772 02297000
         SLR   @14,@14                                             0772 02298000
         CR    RCREG,@14                                           0772 02299000
         BNE   @RF00772                                            0772 02300000
         C     @14,ADPLFMT2(,ABDPLPTR)                             0772 02301000
         BE    @RF00772                                            0772 02302000
*                           CALL RT1EED;/* EED Q RETURNED TO BE    0773 02303000
*                                      FORMATTED                     */ 02304000
         BAL   @14,RT1EED                                          0773 02305000
*                         ELSE                                     0774 02306000
*                           ;       /* NO EEDS TO BE FORMATTED       */ 02307000
@RF00772 DS    0H                                                  0775 02308000
*                         CMSASCBP=ZERO;/* END LOOP                  */ 02309000
         SLR   CMSASCBP,CMSASCBP                                   0775 02310000
*                       END;        /* END ASCB CORRECT              */ 02311000
*                     ELSE                                         0777 02312000
*                       DO;         /* NOT CORRECT ASCB              */ 02313000
         B     @RC00763                                            0777 02314000
@RF00763 DS    0H                                                  0778 02315000
*                         VADR=ADDR(ASCBCMFB);/* LOCATE NEXT ASCB  0778 02316000
*                                      POINTER                       */ 02317000
         LA    VADR,ASCBCMFB(,CMSASCBP)                            0778 02318000
*                         CALL ACCESRTN;/* ACCESS IT                 */ 02319000
         BAL   @14,ACCESRTN                                        0779 02320000
*                         IF RCREG^=ZERO THEN                      0780 02321000
         LTR   RCREG,RCREG                                         0780 02322000
         BZ    @RF00780                                            0780 02323000
*                           DO;     /* ACCESS ERROR                  */ 02324000
*                             BUFFER(1:LENGTH(NOIHSA))=NOIHSA;     0782 02325000
         L     @14,ADPLBUF(,ABDPLPTR)                              0782 02326000
         MVC   BUFFER(95,@14),NOIHSA                               0782 02327000
*                             CMSASCBP=ZERO;/* END LOOP              */ 02328000
         SLR   CMSASCBP,CMSASCBP                                   0783 02329000
*                           END;    /* END ACCES ERROR               */ 02330000
*                         ELSE                                     0785 02331000
*                           CMSASCBP=DATAWORD;/* FOUND NEXT ASCB   0785 02332000
