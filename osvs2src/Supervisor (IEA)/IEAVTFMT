*         CALLER                                                   0828 02502000
*                                                                    */ 02503000
*/********************************************************************/ 02504000
*                                                                  0828 02505000
*   IF ADPLSNPR=OFF THEN            /* SNAP WAS THE CALLER           */ 02506000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0828 02507000
         BNZ   @RF00828                                            0828 02508000
*     DO;                                                          0829 02509000
*       RTYADR=ADDR(RETRY);         /* RETRY ADDR FOR ESTAE IN CASE     02510000
*                                      OF EXPECTED ERROR-SIMULATES 0830 02511000
*                                      PRINT DUMP                    */ 02512000
         LA    @14,RETRY                                           0830 02513000
         ST    @14,RTYADR                                          0830 02514000
*       DATAPTR=VADR;               /* PREPARE FOR A LOAD FROM THE 0831 02515000
*                                      DUMP INTO AN INTERNAL BUFFER  */ 02516000
         LR    DATAPTR,VADR                                        0831 02517000
*       RCREG=ZERO;                 /* SUCCESS UNLESS OTHERWISE    0832 02518000
*                                      CHANGED                       */ 02519000
         SLR   RCREG,RCREG                                         0832 02520000
*       ERREXP=ON;                  /* AN ERROR IS EXPECTED          */ 02521000
         OI    ERREXP,B'10000000'                                  0833 02522000
*       VERIFY=DATADWRD;            /* MOVE A DOUBLE WORD INTO THE 0834 02523000
*                                      BUFFER                        */ 02524000
*                                                                  0834 02525000
         MVC   VERIFY(8),DATADWRD(DATAPTR)                         0834 02526000
*       /*************************************************************/ 02527000
*       /*                                                           */ 02528000
*       /* THIS MOVE WILL CAUSE AN ABEND IF THE DUMP DOES NOT CONTAIN*/ 02529000
*       /* THIS AREA OR IF IT IS PROTECTED. IF AN ABEND OCCURS THE   */ 02530000
*       /* ESTAE WILL GET CONTROL AND RETRY AT THE POINT WHICH SETS A*/ 02531000
*       /* BAD RETURN CODE AND CONTINUES IN THE MAIN LINE. THIS THEN */ 02532000
*       /* SIMULATES THE PRINT DUMP ACCESS ROUTINE. IF THE AREA ASKED*/ 02533000
*       /* FOR IS IN THE DUMP. A DOUBLE WORD IS MOVED INTO AN        */ 02534000
*       /* INTERNAL BUFFER TO PREVENT AN UNEXPECTED ERROR WHEN       */ 02535000
*       /* REFERENCING THE AREA AGAIN                                */ 02536000
*       /*                                                           */ 02537000
*       /*************************************************************/ 02538000
*                                                                  0835 02539000
*       ERREXP=OFF;                 /* AN ERROR IS NO LONGER EXPECTED*/ 02540000
         NI    ERREXP,B'01111111'                                  0835 02541000
*       DATAPTR=ADDR(VERIFY);       /* BASE DATAWORD ON THE BUFFER   */ 02542000
         LA    DATAPTR,VERIFY                                      0836 02543000
*     END;                          /* END SNAP WAS CALLER           */ 02544000
*   ELSE                            /* CALLER HAS OWN ACCESS ROUTINE */ 02545000
*     DO;                                                          0838 02546000
         B     @RC00828                                            0838 02547000
@RF00828 DS    0H                                                  0839 02548000
*       CALL ACCESS;                /* ACCESS SERVICE ROUTINE PASSED    02549000
*                                      BY CALLER                     */ 02550000
