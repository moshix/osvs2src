*            (GPR01F,                                              0210 00664000
*             GPR15F,                                              0210 00665000
*             GPR00F) RESTRICTED;                                  0210 00666000
*           GPR01F=0;               /* REG 1 MUST BE ZERO            */ 00667000
         SLR   GPR01F,GPR01F                                       0211 00668000
*           GPR15F=3;               /* SET TYPE                      */ 00669000
