         L     @15,ADPLPRNT(,ABDPLPTR)                             0201 00623000
         BALR  @14,@15                                             0201 00624000
*         RFY                                                      0202 00625000
*           R1 UNRSTD;              /* NO RETURN CODE                */ 00626000
*       END;                                                       0203 00627000
         AL    INDEX,@CF00043                                      0203 00628000
         CH    INDEX,@CH00101                                      0203 00629000
         BNH   @DL00198                                            0203 00630000
*     END;                                                         0204 00631000
*   SA0=ADDR(RRA);                  /* SETUP SA TO PUT RRA ADDR IN R0*/ 00632000
@RF00194 LA    @08,4                                               0205 00633000
         LR    @15,R13                                             0205 00634000
         ALR   @15,@08                                             0205 00635000
