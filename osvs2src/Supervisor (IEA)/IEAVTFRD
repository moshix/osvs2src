         CLC   SMDLRID(2,@08),@CH00869                             0105 00248000
         BNE   @RF00105                                            0105 00249000
*       DO;                                                        0106 00250000
*         CONDCODE=EOSDCODE;        /* SET THE END OF SUMDUMP CODE   */ 00251000
         LA    CONDCODE,8                                          0107 00252000
*         GO TO EXITCODE;           /* RETURN                        */ 00253000
         B     EXITCODE                                            0108 00254000
*       END;                                                       0109 00255000
*                                                                  0110 00256000
*   /*****************************************************************/ 00257000
*   /*                                                               */ 00258000
*   /* ATTEMPT TO READ THE NEXT SUMMARY DUMP LOGICAL RECORD HEADER   */ 00259000
*   /*                                                               */ 00260000
*   /*****************************************************************/ 00261000
*                                                                  0110 00262000
*   TOPTR=ADDR(HEADER);             /* INDICATE WHERE THE NEXT HEADER   00263000
*                                      IS TO BE MOVED INTO           */ 00264000
@RF00105 DS    0H                                                  0110 00265000
@RC00043 LA    TOPTR,HEADER                                        0110 00266000
*   FROMLEN=LENGTH(SMDLRHDR);       /* LENGTH FOR NEXT READ          */ 00267000
         LA    FROMLEN,20                                          0111 00268000
*   PASSCODE=NONCODE;               /* ERROR RETURN CODE FOR NEXT  0112 00269000
*                                      READ                          */ 00270000
         MVC   PASSCODE(4),@CF00790                                0112 00271000
*   CALL PDREAD;                    /* READ THE NEXT SUMDUMP RECORD     00272000
*                                      HEADER INTO THE HOLDING AREA  */ 00273000
         BAL   @14,PDREAD                                          0113 00274000
*   IF CONDCODE=GOODCODE            /* CHECK THE CONDITION CODE      */ 00275000
*     THEN                          /* IF NOTHING HAPPENED PROCEED   */ 00276000
         LTR   CONDCODE,CONDCODE                                   0114 00277000
         BNZ   @RF00114                                            0114 00278000
*     DO;                                                          0115 00279000
*       CALL SDMSGRTN;              /* WRITE OUT MSG INDICATED BY  0116 00280000
*                                      SMDLRMSG IN THIS HEADER       */ 00281000
*                                                                  0116 00282000
         BAL   @14,SDMSGRTN                                        0116 00283000
*       /*************************************************************/ 00284000
*       /*                                                           */ 00285000
*       /* FREE LAST RECORD RECONSTRUCTION AREA IF THERE WAS ONE AND */ 00286000
*       /* GET A NEW RECORD RECONSTRUCTION AREA (RRA) LARGE ENOUGH   */ 00287000
*       /* FOR THE RECORD ABOUT TO BE FORMATTED              @ZA25122*/ 00288000
*       /*                                                           */ 00289000
*       /*************************************************************/ 00290000
*                                                                  0117 00291000
*       IF ADDR(RRA)^=0             /* CHECK TO BE SURE THERE IS AN     00292000
*                                      OLD RECORD RECONSTRUCTION AREA*/ 00293000
*         THEN                      /* IF THERE IS FREE IT           */ 00294000
