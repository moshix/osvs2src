         L     @15,ADPLMEMA(,ABDPLPTR)                             0082 00165000
         BALR  @14,@15                                             0082 00166000
*       RFY                                                        0083 00167000
*         R1 UNRSTD;                                               0083 00168000
*       IF RETCODE^=0               /* CHECK THE SUCCESS OF THE DUMP    00169000
*                                      ACCESS                        */ 00170000
*         THEN                      /* IF IT FAILED                  */ 00171000
         LTR   RETCODE,RETCODE                                     0084 00172000
         BZ    @RF00084                                            0084 00173000
*         DO;                                                      0085 00174000
*           CONDCODE=NGDACODE;      /* INDICATE RETURN CODE          */ 00175000
         LA    CONDCODE,24                                         0086 00176000
*           GO TO EXITCODE;         /* RETURN                        */ 00177000
         B     EXITCODE                                            0087 00178000
*           RFY                                                    0088 00179000
*             RETCODE UNRSTD;                                      0088 00180000
*         END;                                                     0089 00181000
*       CBNXTADR=DATAPTR;           /* USE THE PRIVATE AREA STARTING    00182000
*                                      ADDRESS FROM THE GDA IN THE 0090 00183000
*                                      DUMP AT THE BEGINNING OF THE     00184000
*                                      PSEUDO VIRTUAL ADDRESSES FOR     00185000
