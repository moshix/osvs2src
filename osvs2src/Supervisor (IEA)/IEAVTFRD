TFRD     TITLE 'IEAVTFRD READ ROUTINE FOR IEAVTFSD                     *00001000
                        '                                               00002000
IEAVTFRD CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(22)                                             0001 00007000
         DC    C'IEAVTFRD UZ14566 77280'                           0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @11,8(,@13)                                         0001 00020000
         LR    @13,@11                                             0001 00021000
         MVC   @PC00001(4),0(@01)                                  0001 00022000
*   CONDCODE=GOODCODE;              /* START OUT ASSUMMING SUCCESSFUL   00023000
*                                      COMPLETION                    */ 00024000
         SLR   CONDCODE,CONDCODE                                   0039 00025000
*   MIAINDEX=1;                     /* BEGIN SEARCH OF MIATABLE AT 0040 00026000
*                                      THE BEGINNING                 */ 00027000
         MVC   MIAINDEX(4),@CF00043                                0040 00028000
*   ABDPLPTR=R1;                    /* SAVE AMDPRDMP PARMLIST BASE   */ 00029000
         LR    ABDPLPTR,R1                                         0041 00030000
*   RFY                                                            0042 00031000
*     R1 UNRSTD;                                                   0042 00032000
*                                                                  0042 00033000
*   /*****************************************************************/ 00034000
*   /*                                                               */ 00035000
*   /* FOR THE FIRST CALL PERFORM INITIALIZATION                     */ 00036000
*   /*                                                               */ 00037000
*   /*****************************************************************/ 00038000
*                                                                  0043 00039000
*   IF ADDR(VTFRDCB)=0              /* IS THE CONTROL BLOCK BASE ZERO*/ 00040000
*     THEN                          /* PERFORM INITIALIZATION        */ 00041000
         ICM   @08,15,CBPTR(ABDPLPTR)                              0043 00042000
         BNZ   @RF00043                                            0043 00043000
*     DO;                                                          0044 00044000
*       RFY                                                        0045 00045000
*        (R1,                                                      0045 00046000
*         RETCODE) RSTD;                                           0045 00047000
*       DO;                         /* GETMAIN (RC)                0046 00048000
*                                      LV(LENGTH(VTFRDCB))         0046 00049000
*                                      SP(ADPLSBPL)                  */ 00050000
*         RESPECIFY                                                0047 00051000
*          (GPR01F,                                                0047 00052000
*           GPR15F,                                                0047 00053000
*           GPR00F) RESTRICTED;                                    0047 00054000
*         GPR01F=0;                 /* REG 1 MUST BE ZERO            */ 00055000
         SLR   GPR01F,GPR01F                                       0048 00056000
*         GPR15F=0;                 /* RC-TYPE GETMAIN               */ 00057000
         SLR   GPR15F,GPR15F                                       0049 00058000
*         GPR00F=LENGTH(VTFRDCB);   /* LENGTH REQUESTED              */ 00059000
         LA    GPR00F,12                                           0050 00060000
*         GPR15F=GPR15F|((ADPLSBPL)*256);/* SP IN BYTE 2             */ 00061000
         SLR   @14,@14                                             0051 00062000
         IC    @14,ADPLSBPL(,ABDPLPTR)                             0051 00063000
         SLA   @14,8                                               0051 00064000
         OR    GPR15F,@14                                          0051 00065000
*         SVC(120);                 /* RC/RU-FORM OF GETMAIN         */ 00066000
         SVC   120                                                 0052 00067000
*         RESPECIFY                                                0053 00068000
*          (GPR01F,                                                0053 00069000
*           GPR15F,                                                0053 00070000
*           GPR00F) UNRESTRICTED;                                  0053 00071000
*       END;                        /* GETMAIN (RC)                0054 00072000
*                                      LV(LENGTH(VTFRDCB))         0054 00073000
*                                      SP(ADPLSBPL)                  */ 00074000
*       IF RETCODE^=0               /* TEST THE GETMAIN RETURN CODE  */ 00075000
*         THEN                      /* IF THE GETMAIN FAILED         */ 00076000
         LTR   RETCODE,RETCODE                                     0055 00077000
         BZ    @RF00055                                            0055 00078000
*         DO;                       /* TERMINATE                     */ 00079000
*           CONDCODE=GETCODE;       /* INDICATE THE RETURN CODE TO BE   00080000
*                                      GIVEN AT THE END              */ 00081000
         LA    CONDCODE,28                                         0057 00082000
*           GO TO EXITCODE;                                        0058 00083000
         B     EXITCODE                                            0058 00084000
*         END;                                                     0059 00085000
*       CBPTR=R1;                   /* SAVE THE STORAGE ADDRESS      */ 00086000
@RF00055 ST    R1,CBPTR(,ABDPLPTR)                                 0060 00087000
*       VTFRDCB=''B;                /* INIT THE CONTROL BLOCK        */ 00088000
         L     @14,CBPTR(,ABDPLPTR)                                0061 00089000
