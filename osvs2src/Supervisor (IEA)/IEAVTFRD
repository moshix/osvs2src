         L     @14,12(,@13)                                        0234 00743000
         LM    @00,@12,20(@13)                                     0234 00744000
         BR    @14                                                 0234 00745000
*SDMSGRTN:                                                         0235 00746000
*   PROC;                                                          0235 00747000
*                                                                  0235 00748000
@EL00001 L     @13,4(,@13)                                         0235 00749000
@EF00001 L     @00,@SIZDATD                                        0235 00750000
         LR    @01,@11                                             0235 00751000
         FREEMAIN R,LV=(0),A=(1)                                        00752000
@ER00001 LM    @14,@12,12(@13)                                     0235 00753000
         BR    @14                                                 0235 00754000
SDMSGRTN STM   @14,@12,@SA00002                                    0235 00755000
*   /*****************************************************************/ 00756000
*   /*                                                               */ 00757000
*   /* THIS INTERNAL PROC CHECKS THE VALUE OF THE SMDLRMSG FIELD, AND*/ 00758000
*   /* WRITE THE CORRESPONDING MSG INTO THE DUMP OUTPUT ON BEHALF OF */ 00759000
*   /* SUMMARY DUMP.                                                 */ 00760000
*   /*                                                               */ 00761000
*   /*****************************************************************/ 00762000
*                                                                  0236 00763000
*   IF SMDLRMSG>0                   /* IS THERE A MSG ID             */ 00764000
*     THEN                          /* YES THERE IS                  */ 00765000
         LA    @08,HEADER                                          0236 00766000
         CLI   SMDLRMSG(@08),0                                     0236 00767000
         BNH   @RF00236                                            0236 00768000
*     DO;                                                          0237 00769000
*       IF SMDLRMSG>=SDMSGMAX       /* IS THE MSG ID BEYOND THE SCOPE   00770000
*                                      OF THE SDMSG TABLE?           */ 00771000
*         THEN                                                     0238 00772000
         L     @15,@CA00924                                        0238 00773000
         CLC   SMDLRMSG(1,@08),SDMSGMAX-SDMSGTAB(@15)              0238 00774000
         BL    @RF00238                                            0238 00775000
*         DO;                                                      0239 00776000
*           RFY                                                    0240 00777000
*            (R0,                                                  0240 00778000
*             R1) RSTD;                                            0240 00779000
*           R0=SDMSG(SDMSGMAX);     /* YES SO USE THE LAST ENTRY   0241 00780000
*                                      VALUE WHICH IS A FORMAT     0241 00781000
*                                      PATTERN THAT WILL PRODUCE AN     00782000
*                                      ERROR MSG                     */ 00783000
         SLR   @14,@14                                             0241 00784000
         IC    @14,SDMSGMAX-SDMSGTAB(,@15)                         0241 00785000
         SLA   @14,2                                               0241 00786000
         AL    @15,@CF00976                                        0241 00787000
         L     R0,SDMSG-SDMSGTAB-1(@14,@15)                        0241 00788000
*           ADPLFMT2=ADDR(SMDLRMSG);/* POINT TO THE MSG ID TO BE   0242 00789000
*                                      FORMATTED INTO THE MSG TEXT   */ 00790000
         LA    @08,SMDLRMSG(,@08)                                  0242 00791000
         ST    @08,ADPLFMT2(,ABDPLPTR)                             0242 00792000
*           R1=ADDR(ABDPL);         /* POINT TO PARMLIST             */ 00793000
         LR    R1,ABDPLPTR                                         0243 00794000
*           CALL FMTSERV;           /* HAVE THE AMDPRDMP FMT SERVICE    00795000
*                                      BUILD THE MSG                 */ 00796000
         L     @15,ADPLFRMT(,ABDPLPTR)                             0244 00797000
         BALR  @14,@15                                             0244 00798000
*           RFY                                                    0245 00799000
*            (R0,                                                  0245 00800000
*             R1) UNRSTD;                                          0245 00801000
*         END;                                                     0246 00802000
*       ELSE                                                       0247 00803000
*         DO;                                                      0247 00804000
