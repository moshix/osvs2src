*           GO TO EXITCODE;         /* RETURN                        */ 00115000
         B     EXITCODE                                            0066 00116000
*         END;                                                     0067 00117000
*       RFY                                                        0068 00118000
*        (R0,                                                      0068 00119000
*         R1,                                                      0068 00120000
*         RETCODE) RSTD;                                           0068 00121000
@RF00063 DS    0H                                                  0069 00122000
*       R0=ADDR(CVTGDA);            /* PUT THE VIRTUAL ADDRESS OF THE   00123000
*                                      DATA TO BE RETRIEVED IN REG 0 */ 00124000
