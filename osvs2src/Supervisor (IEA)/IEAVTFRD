         L     @14,CBSDRECP(,@15)                                  0205 00638000
         ST    @14,SA0+20(,@01)                                    0205 00639000
*                                                                  0206 00640000
*   /*****************************************************************/ 00641000
*   /*                                                               */ 00642000
*   /* IF DATA IS NOT TO BE RETURNED THE STORAGE GOTTEN MUST BE FREED*/ 00643000
*   /* BEFORE RETURNING FOR THE LAST TIME                            */ 00644000
*   /*                                                               */ 00645000
*   /*****************************************************************/ 00646000
*                                                                  0206 00647000
*   IF CONDCODE>DATARET             /* CHECK TO SEE IF DATA IS BEING    00648000
*                                      RETURNED                      */ 00649000
*     THEN                          /* IF NOT                        */ 00650000
         CR    CONDCODE,@08                                        0206 00651000
         BNH   @RF00206                                            0206 00652000
*     DO;                                                          0207 00653000
*       IF ADDR(VTFRDCB)^=0&ADDR(RRA)^=0/* CHECK FOR AN RRA          */ 00654000
*         THEN                      /* IF THERE IS ONE FREE IT       */ 00655000
         SLR   @08,@08                                             0208 00656000
         CR    @15,@08                                             0208 00657000
         BE    @RF00208                                            0208 00658000
         CR    @14,@08                                             0208 00659000
         BE    @RF00208                                            0208 00660000
*         DO;                       /* FREEMAIN(RU)                0209 00661000
*                                      LV(CBSDRRAL)A(RRA)            */ 00662000
