         L     @08,ENDCOND                                         0179 00528000
         CR    TOPTR,@08                                           0179 00529000
         BNL   @RF00179                                            0179 00530000
*         DO;                       /* SO PAD OUT TO THE PROPER    0180 00531000
*                                      LENGTH                        */ 00532000
*           FROMLEN=PAD;            /* SET UP THE PADDING CHAR       */ 00533000
         L     FROMLEN,PAD                                         0181 00534000
*           TOLEN=ENDCOND-TOPTR;    /* SET THE LENGTH TO WHATEVER IS    00535000
*                                      LEFT                          */ 00536000
