*         PASSCODE=PARTCODE;        /* SET UP ERROR CODE TO BE USED  */ 00419000
         MVC   PASSCODE(4),@CF00082                                0154 00420000
*         CALL PDREAD;              /* MOVE IN THE WHOLE NEXT GROUP     00421000
*                                      OF DATA UP TO THE END OF THE     00422000
*                                      RECORD OR A BREAK IN THE    0155 00423000
*                                      RECORD, AND INCREMENT TOPTR BY   00424000
*                                      THE NUMBER OF BYTES MOVED     */ 00425000
         BAL   @14,PDREAD                                          0155 00426000
*         IF CONDCODE=GOODCODE      /* IF THERE WAS NO TROUBLE AND 0156 00427000
*                                      THE                           */ 00428000
*             &TOPTR<ENDCOND        /* END OF THE TOTAL DATA RECORD     00429000
*                                      HAS NOT BEEN REACHED          */ 00430000
*           THEN                    /* THEN THERE SHOULD BE A      0156 00431000
*                                      CONTINUATION HEADER, AND THE     00432000
*                                      BYTES MISSSING WILL HAVE TO BE   00433000
*                                      REPLACED                      */ 00434000
         LTR   CONDCODE,CONDCODE                                   0156 00435000
         BNZ   @RF00156                                            0156 00436000
         C     TOPTR,ENDCOND                                       0156 00437000
         BNL   @RF00156                                            0156 00438000
*           DO;                                                    0157 00439000
*             SAVEPTR=TOPTR;        /* TEMPORARILY SAVE THE TOPTR  0158 00440000
*                                      VALUE                         */ 00441000
         ST    TOPTR,SAVEPTR                                       0158 00442000
*             TOPTR=ADDR(HEADER);   /* AND REPLACE IT WITH THE HEADER   00443000
*                                      HOLDING AREA ADDRESS.       0159 00444000
*                                      CONTINUATION RECORD HEADERS 0159 00445000
*                                      ARE NOT RETURNED AS PART OF 0159 00446000
*                                      THE RECONSTRUCTION            */ 00447000
         LA    TOPTR,HEADER                                        0159 00448000
*             FROMLEN=LENGTH(SMDLRHDR);/* SET UP LENGTH OF NEXT DATA */ 00449000
         LA    FROMLEN,20                                          0160 00450000
*             PASSCODE=PARTCODE;    /* SET UP ERROR CODE TO BE USED  */ 00451000
         MVC   PASSCODE(4),@CF00082                                0161 00452000
*             CALL PDREAD;          /* READ THE NEXT SUMDUMP RECORD     00453000
*                                      HEADER INTO THE HOLDING AREA  */ 00454000
         BAL   @14,PDREAD                                          0162 00455000
*             TOPTR=SAVEPTR;        /* RESTORE THE TOPTR             */ 00456000
         L     TOPTR,SAVEPTR                                       0163 00457000
*             IF CONDCODE=GOODCODE  /* CHECK THE RETURN CODE         */ 00458000
*               THEN                /* IF NOTHING HAPPENED PROCEED   */ 00459000
         LTR   CONDCODE,CONDCODE                                   0164 00460000
         BNZ   @RF00164                                            0164 00461000
*               DO;                                                0165 00462000
*                 CALL SDMSGRTN;    /* WRITE OUT MSG INDICATED BY  0166 00463000
*                                      SMDLRMSG IN THIS HEADER       */ 00464000
         BAL   @14,SDMSGRTN                                        0166 00465000
*                 FROMLEN=PAD;      /* SET UP THE PADDING CHAR       */ 00466000
