         L     @08,@CA00924                                        0248 00814000
         AL    @08,@CF00976                                        0248 00815000
         L     MSGPTR,SDMSG-SDMSGTAB-1(@15,@08)                    0248 00816000
*           OUTBUF(1:MSGLEN)=MSGTXT(1:MSGLEN);/* MOVE IN THE MSG     */ 00817000
         L     @08,ADPLBUF(,ABDPLPTR)                              0249 00818000
         SLR   @03,@03                                             0249 00819000
         IC    @03,MSGLEN(,MSGPTR)                                 0249 00820000
         BCTR  @03,0                                               0249 00821000
         EX    @03,@SM00974                                        0249 00822000
*         END;                                                     0250 00823000
*       DO INDEX=1 TO 2;            /* PRINT THE MSG AND 1 BLANK LINE*/ 00824000
@RC00238 LA    INDEX,1                                             0251 00825000
@DL00251 DS    0H                                                  0252 00826000
*         RFY                                                      0252 00827000
*           R1 RSTD;                                               0252 00828000
*         R1=ADDR(ABDPL);           /* RESTORE AMDPRDMP PARMLIST PTR */ 00829000
         LR    R1,ABDPLPTR                                         0253 00830000
*         CALL LINEPRNT;            /* PRINT THE OUTPUT BUFFER       */ 00831000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0254 00832000
         BALR  @14,@15                                             0254 00833000
*         RFY                                                      0255 00834000
*           R1 UNRSTD;              /* NO RETURN CODE                */ 00835000
*       END;                                                       0256 00836000
         AL    INDEX,@CF00043                                      0256 00837000
         CH    INDEX,@CH00101                                      0256 00838000
         BNH   @DL00251                                            0256 00839000
*     END;                                                         0257 00840000
*   END SDMSGRTN;                                                  0258 00841000
@EL00002 DS    0H                                                  0258 00842000
@EF00002 DS    0H                                                  0258 00843000
@ER00002 LM    @14,@12,@SA00002                                    0258 00844000
         BR    @14                                                 0258 00845000
*PDREAD:                                                           0259 00846000
*   PROC OPTIONS(SAVE(14));         /* OTHER REGS ARE PASED BOTH WAYS   00847000
*                                      AND MUST NOT BE ALTERED       */ 00848000
*                                                                  0259 00849000
PDREAD   ST    @14,@SA00003                                        0259 00850000
*   /*****************************************************************/ 00851000
*   /*                                                               */ 00852000
*   /* THIS INTERNAL PROC READS A VARIABLE LENGTH AMOUNT OF DATA FROM*/ 00853000
*   /* THE DUMP STARTING AT THE PSEUDO ADDRESS CBNXTADR AND FOR THE  */ 00854000
*   /* LENGTH IN FROMLEN INTO AN AREA POINTED TO BY TOPTR. CBNXTADR  */ 00855000
*   /* AND TOPTR WILL BE INCREMENTED BY FROMLEN AND FROMLEN & TOLEN  */ 00856000
*   /* WILL BE SET TO 0. THE FROMPTR VALUE WILL HAVE NO MEANING.     */ 00857000
*   /*                                                               */ 00858000
*   /*****************************************************************/ 00859000
*                                                                  0260 00860000
*   TOLEN=(ADDR(NEXTDATA)&RNDTOPG)+PAGESIZE-ADDR(NEXTDATA);/*      0260 00861000
*                                      CALCULATE THE MAXIMUM NUMBER     00862000
*                                      OF BYTES AVAILABLE IN THE   0260 00863000
*                                      AMDPRDMP BUFFER TO BE COPIED     00864000
*                                      BY SUBTRACTING THE PSEUDO   0260 00865000
*                                      VIRTUAL ADDR OF THE DATA FROM    00866000
*                                      THE ADDR OF THE FIRST BYTE OF    00867000
*                                      THE NEXT SUMDUMP PSEUDO PAGE  */ 00868000
         L     @08,CBPTR(,ABDPLPTR)                                0260 00869000
         L     @08,CBNXTADR(,@08)                                  0260 00870000
         LR    TOLEN,@08                                           0260 00871000
         N     TOLEN,@CF00912                                      0260 00872000
         AL    TOLEN,@CF00910                                      0260 00873000
         SLR   TOLEN,@08                                           0260 00874000
*   DO WHILE FROMLEN>0&CONDCODE=GOODCODE;/* MOVE DATA UNTIL IT IS ALL   00875000
*                                      THERE. NOTE HERE THAT THE MVCL   00876000
*                                      WILL DECREMENT FROMLEN AS DATA   00877000
*                                      IS MOVED AND LIKEWISE       0261 00878000
*                                      INCREMENT TOPTR               */ 00879000
         B     @DE00261                                            0261 00880000
@DL00261 DS    0H                                                  0262 00881000
*     RFY                                                          0262 00882000
*      (R0,                                                        0262 00883000
*       R1,                                                        0262 00884000
*       RETCODE) RSTD;                                             0262 00885000
*     R0=ADDR(NEXTDATA);            /* PUT THE VIRTUAL ADDRESS OF THE   00886000
*                                      DATA TO BE RETRIEVED IN REG 0 */ 00887000
         L     @14,CBPTR(,ABDPLPTR)                                0263 00888000
         L     R0,CBNXTADR(,@14)                                   0263 00889000
*     R1=ABDPLPTR;                  /* RESTORE THE PTR TO THE      0264 00890000
*                                      AMDPRDMP PARMLIST             */ 00891000
         LR    R1,ABDPLPTR                                         0264 00892000
*     ADPLASID=SMDPASID;            /* CHANGE THE AMDPRDMP READ ASID    00893000
*                                      TO THE PSEUDO ONE FOR SUM DUMP*/ 00894000
         MVC   ADPLASID(2,ABDPLPTR),@CH00758                       0265 00895000
*     CALL DUMPREAD;                /* CALL AMDPRDMP DUMP ACCESS   0266 00896000
*                                      SERVICE                       */ 00897000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0266 00898000
         BALR  @14,@15                                             0266 00899000
*     RFY                                                          0267 00900000
*       R1 UNRSTD;                                                 0267 00901000
*     IF RETCODE^=0                 /* CHECK THE SUCCESS OF THE DUMP    00902000
*                                      ACCESS                        */ 00903000
*       THEN                        /* IF IT FAILED                  */ 00904000
         LTR   RETCODE,RETCODE                                     0268 00905000
         BZ    @RF00268                                            0268 00906000
*       CONDCODE=PASSCODE;          /* INDICATE RETURN CODE          */ 00907000
         L     CONDCODE,PASSCODE                                   0269 00908000
*     ELSE                          /* IF THERE IS MORE DATA         */ 00909000
*       DO;                                                        0270 00910000
         B     @RC00268                                            0270 00911000
@RF00268 DS    0H                                                  0271 00912000
*         RFY                                                      0271 00913000
*           RETCODE UNRSTD;                                        0271 00914000
*         FROMPTR=ADDR(DATAREA);    /* POINT TO THE AMDPRDMP BUFFER     00915000
*                                      JUST READ IN                  */ 00916000
         LR    FROMPTR,R0                                          0272 00917000
*         RFY                                                      0273 00918000
*           R0 UNRSTD;                                             0273 00919000
*         IF TOLEN>FROMLEN          /* AT THIS POINT TOLEN IS SET TO    00920000
*                                      COPY TO THE END OF THE      0274 00921000
*                                      AMDPRDMP BUFFER (WHICH IS A 0274 00922000
*                                      PAGE BOUNDRY IN THE SUMDUMP 0274 00923000
*                                      PSEUDO ADDR             SPACE)*/ 00924000
*           THEN                    /* IF TOLEN IS GREATER THAN    0274 00925000
*                                      FROMLEN THE END OF THE DATA 0274 00926000
*                                      REQUESTED IS IN THIS AMDPRDMP    00927000
*                                      BUFFER (WHICH IS A PSEUDO PAGE   00928000
*                                      FROM THE DUMP)                */ 00929000
         CR    TOLEN,FROMLEN                                       0274 00930000
         BNH   @RF00274                                            0274 00931000
*           TOLEN=FROMLEN;          /* SO TOLEN IS SET EQUAL TO    0275 00932000
*                                      FROMLEN TO PREVENT THE MVCL 0275 00933000
*                                      FROM PADDING THE RRA AND TO 0275 00934000
*                                      SERVE AS A COUNT OF THE NUMBER   00935000
*                                      OF BYTES MOVED.               */ 00936000
         LR    TOLEN,FROMLEN                                       0275 00937000
*         CBNXTADR=CBNXTADR+TOLEN;  /* INCREMENT THE PSEUDO VIRTUAL     00938000
*                                      ADDRESS OF THE NEXT SUM DUMP     00939000
*                                      DATA IN THE DUMP BY THE NUMBER   00940000
*                                      OF BYTES COPIED               */ 00941000
@RF00274 L     @08,CBPTR(,ABDPLPTR)                                0276 00942000
         LR    @15,TOLEN                                           0276 00943000
         AL    @15,CBNXTADR(,@08)                                  0276 00944000
         ST    @15,CBNXTADR(,@08)                                  0276 00945000
*         MVCL(TOPTR,FROMPTR);                                     0277 00946000
         MVCL  TOPTR,FROMPTR                                       0277 00947000
*         TOLEN=PAGESIZE;           /* PREPARE TO COPY THE WHOLE NEXT   00948000
*                                      AMDPRDMP BUFFER LOAD          */ 00949000
         LH    TOLEN,@CH00910                                      0278 00950000
*       END;                        /* CHECK OF READ RETURN CODE     */ 00951000
*   END;                            /* BRANCH BACK TO CONTINUE     0280 00952000
*                                      RECONSTRUCTING THE DATA       */ 00953000
@RC00268 DS    0H                                                  0280 00954000
@DE00261 SLR   @08,@08                                             0280 00955000
         CR    FROMLEN,@08                                         0280 00956000
         BNH   @DC00261                                            0280 00957000
         CR    CONDCODE,@08                                        0280 00958000
         BE    @DL00261                                            0280 00959000
@DC00261 DS    0H                                                  0281 00960000
*   END PDREAD;                                                    0281 00961000
@EL00003 DS    0H                                                  0281 00962000
@EF00003 DS    0H                                                  0281 00963000
@ER00003 L     @14,@SA00003                                        0281 00964000
         BR    @14                                                 0281 00965000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 00966000
*     GPR00F FIXED(31) REG(0),                                     0282 00967000
*     GPR01F FIXED(31) REG(1),                                     0282 00968000
*     GPR15F FIXED(31) REG(15);                                    0282 00969000
*   DECLARE                         /* COMMON VARIABLES              */ 00970000
*     I256C CHAR(256) BASED,                                       0283 00971000
*     I031F FIXED(31) BASED,                                       0283 00972000
*     I031P PTR(31) BASED,                                         0283 00973000
*     I015F FIXED(15) BASED,                                       0283 00974000
*     I015P PTR(15) BASED,                                         0283 00975000
*     I008P PTR(8) BASED,                                          0283 00976000
*     I001C CHAR(1) BASED;                                         0283 00977000
*   END                                                            0284 00978000
