*           SVC(120);               /* RC/RU-FORM OF GETMAIN         */ 00703000
         SVC   120                                                 0225 00704000
*           RESPECIFY                                              0226 00705000
*            (GPR01F,                                              0226 00706000
*             GPR15F,                                              0226 00707000
*             GPR00F) UNRESTRICTED;                                0226 00708000
*         END;                      /* FREEMAIN(RU)                0227 00709000
*                                      LV(LENGTH(VTFRDCB))A(VTFRDCB) */ 00710000
*       CBPTR=0;                    /* ZERO THE CONTROL BLOCK BASE IN   00711000
*                                      THE AMDPRDMP PARMLIST TO SHOW    00712000
*                                      IT HAS BEEN FREED             */ 00713000
@RF00218 SLR   @08,@08                                             0228 00714000
         ST    @08,CBPTR(,ABDPLPTR)                                0228 00715000
*     END;                                                         0229 00716000
*   ELSE                            /* IF DATA IS BEING RETURNED     */ 00717000
*     DO;                                                          0230 00718000
         B     @RC00206                                            0230 00719000
