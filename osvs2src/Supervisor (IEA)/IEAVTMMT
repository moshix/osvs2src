         BNZ   @RF00247                                            0247 00851000
*           DO;                     /* NO RETRY ADDR                 */ 00852000
*             REG15=0;              /* INDICATE NO RETRY             */ 00853000
         SLR   REG15,REG15                                         0249 00854000
*             GOTO REG14;           /* RETRUN TO RTM2                */ 00855000
         BR    REG14                                               0250 00856000
*           END;                                                   0251 00857000
*         ELSE                      /* RETRY ADDR EXISTS             */ 00858000
*           DO;                                                    0252 00859000
@RF00247 DS    0H                                                  0253 00860000
*             REG2->RTM2RREG(14+1)=REG2->RTM2TRRA;/* SET UP REG 14 IN   00861000
*                                      REG SAVE AREA TO ADDR OF    0253 00862000
*                                      RESUME POINT OF MAINLINE.   0253 00863000
*                                      MMTESTAX WILL BRANCH ON REG 14   00864000
*                                      AFTER RELOADING REGS.         */ 00865000
         L     @03,RTM2TRRA(,REG2)                                 0253 00866000
         ST    @03,RTM2RREG+56(,REG2)                              0253 00867000
*             REG2->RTM2TRRA=0;     /* INDICATE NO RECURSION ADDR    */ 00868000
         SLR   @03,@03                                             0254 00869000
         ST    @03,RTM2TRRA(,REG2)                                 0254 00870000
*             REG0=ADDR(MMTESTAX);  /* RETRY AT MMTESTAE EXTENSION   */ 00871000
         LA    REG0,MMTESTAX                                       0255 00872000
*             REG15=4;              /* INDICATE RETRY                */ 00873000
         LA    REG15,4                                             0256 00874000
*             GOTO REG14;           /* RETURN TO RTM2                */ 00875000
         BR    REG14                                               0257 00876000
*           END;                                                   0258 00877000
*       END;                                                       0259 00878000
*     ELSE                          /* SDWA AND REG SAVE AREA EXIST  */ 00879000
*       DO;                                                        0260 00880000
@RF00245 DS    0H                                                  0261 00881000
*         GEN(STM 14,12,12(13));    /* SAVE RTM2 REGS                */ 00882000
         STM 14,12,12(13)                                               00883000
*         RTM2PTR=SDWAPARM;         /* GET RTM2WA PTR FROM SDWA      */ 00884000
         L     RTM2PTR,SDWAPARM(,SDWAPTR)                          0262 00885000
*         IF RTM2TRRA=0 THEN        /* CHECK FOR RESUME ADDR         */ 00886000
         L     @03,RTM2TRRA(,RTM2PTR)                              0263 00887000
         LTR   @03,@03                                             0263 00888000
         BNZ   @RF00263                                            0263 00889000
*           DO;                     /* NO RESUME ADDR                */ 00890000
*             DO;                   /* SETRP RC(0)REGS(14,12)        */ 00891000
*               RESPECIFY                                          0266 00892000
*                (GPR00P,                                          0266 00893000
*                 GPR01P,                                          0266 00894000
*                 GPR14P,                                          0266 00895000
*                 GPR15P) RSTD;                                    0266 00896000
*               GPR01P->SDWARCDE=0; /* SAVE RC VALUE                 */ 00897000
         MVI   SDWARCDE(GPR01P),X'00'                              0267 00898000
*               GEN(RETURN (14,12));                               0268 00899000
         RETURN (14,12)                                                 00900000
*               RESPECIFY                                          0269 00901000
*                (GPR00P,                                          0269 00902000
*                 GPR01P,                                          0269 00903000
*                 GPR14P,                                          0269 00904000
*                 GPR15P) UNRSTD;                                  0269 00905000
*             END;                  /* CONTINUE WITH TERMINATION     */ 00906000
*           END;                                                   0271 00907000
*         ELSE                      /* RESUME ADDR EXISTS            */ 00908000
*           DO;                                                    0272 00909000
         B     @RC00263                                            0272 00910000
@RF00263 DS    0H                                                  0273 00911000
*             SDWASRSV=RTM2RRG;     /* MOVE REGS STORED BY MAINLINE     00912000
*                                      INTO SDWA FOR LOADING BY RTM2 */ 00913000
         MVC   SDWASRSV(64,SDWAPTR),RTM2RRG(RTM2PTR)               0273 00914000
*             REG2=RTM2TRRA;        /* SAVE RESUME ADDR              */ 00915000
         L     REG2,RTM2TRRA(,RTM2PTR)                             0274 00916000
*             RTM2TRRA=0;           /* NO RECURSION PROTECTION       */ 00917000
         SLR   @03,@03                                             0275 00918000
         ST    @03,RTM2TRRA(,RTM2PTR)                              0275 00919000
*             DO;                   /* SETRP RC(4)RETADDR(REG2->MANAG   00920000
*                                      ER)RETREGS(YES)REGS(14,12)FRES   00921000
*                                      DWA(YES)                      */ 00922000
*               RESPECIFY                                          0277 00923000
*                (GPR00P,                                          0277 00924000
*                 GPR01P,                                          0277 00925000
*                 GPR14P,                                          0277 00926000
*                 GPR15P) RSTD;                                    0277 00927000
*               GPR01P->SDWARCDE=4; /* STORE RC INTO SDWA            */ 00928000
         MVI   SDWARCDE(GPR01P),X'04'                              0278 00929000
*               GPR01P->SDWARTYA=ADDR(REG2->MANAGER);/* SAVE RETRY 0279 00930000
*                                      ADDRESS                       */ 00931000
         ST    REG2,SDWARTYA(,GPR01P)                              0279 00932000
*               GPR01P->SDWAFREE='1'B;/* SET FLAG TO FREE SDWA       */ 00933000
*               GPR01P->SDWAUPRG='1'B;/* TURN ON RETREGS INDICATOR   */ 00934000
         OI    SDWAFREE(GPR01P),B'00001100'                        0281 00935000
*               GEN(RETURN (14,12));                               0282 00936000
         RETURN (14,12)                                                 00937000
*               RESPECIFY                                          0283 00938000
*                (GPR00P,                                          0283 00939000
*                 GPR01P,                                          0283 00940000
*                 GPR14P,                                          0283 00941000
*                 GPR15P) UNRSTD;                                  0283 00942000
*             END;                                                 0284 00943000
*           END;                                                   0285 00944000
*       END;                                                       0286 00945000
@RC00263 DS    0H                                                  0287 00946000
*     GEN(DROP REG15);                                             0287 00947000
         DROP REG15                                                     00948000
*MMTESTAX:                                                         0288 00949000
*                                                                  0288 00950000
*     /***************************************************************/ 00951000
*     /*                                                             */ 00952000
*     /* ESTAE EXTENSION HANDLES RETRY WHEN NO SDWA EXISTS. IN THIS  */ 00953000
*     /* CASE, RTM2 WILL NOT UPDATE REGS, SO REGS MUST BE UPDATED IN */ 00954000
*     /* THE RETRY ROUTINE. ON ENTRY, REG1 POINTS TO PARM(RTM2WA).   */ 00955000
*     /*                                                             */ 00956000
*     /***************************************************************/ 00957000
*                                                                  0288 00958000
*     GEN REFS(RTM2RREG)(LM 0,15,RTM2RREG(REG1));/* RESTORE REGS FROM   00959000
*                                      SAVE AREA                     */ 00960000
MMTESTAX LM 0,15,RTM2RREG(REG1)                                         00961000
*     GOTO REG14;                   /* REG 14 IN SAVE AREA IS FILLED    00962000
*                                      IN BY ESTAE EXIT TO POINT TO     00963000
*                                      RESUME POINT THAT WAS       0289 00964000
*                                      INDICATED IN RTM2TRRA         */ 00965000
         BR    REG14                                               0289 00966000
*     RFY                                                          0290 00967000
*      (REG0,                                                      0290 00968000
*       REG1,                                                      0290 00969000
*       REG2,                                                      0290 00970000
*       REG13,                                                     0290 00971000
*       REG14,                                                     0290 00972000
*       REG15) UNRSTD;                                             0290 00973000
*   END;                            /* END SEGMENT MMTESTAE          */ 00974000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 00975000
*     GPR00P PTR(31) REG(0),                                       0292 00976000
*     GPR01P PTR(31) REG(1),                                       0292 00977000
*     GPR14P PTR(31) REG(14),                                      0292 00978000
*     GPR15P PTR(31) REG(15);                                      0292 00979000
*   DECLARE                         /* COMMON VARIABLES              */ 00980000
*     I256C CHAR(256) BASED,                                       0293 00981000
*     I031F FIXED(31) BASED,                                       0293 00982000
*     I031P PTR(31) BASED,                                         0293 00983000
*     I015F FIXED(15) BASED,                                       0293 00984000
*     I015P PTR(15) BASED,                                         0293 00985000
