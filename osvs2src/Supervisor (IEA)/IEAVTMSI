*                     GEN SETS(R0,R14,R15) REFS(MEMTERM,MEMRYECB,R3);   00187000
       ATTACH EPLOC=MEMTERM,LPMOD=1,SM=SUPV,DPMOD=-1,SZERO=NO,KEY=ZERO,*00188000
               JSTCB=YES,ECB=MEMRYECB(R3),SF=(E,(3))                    00189000
*                   END;                                           0078 00190000
*               END;                                               0079 00191000
@RF00074 DS    0H                                                  0080 00192000
*           END;                                                   0080 00193000
@RF00070 DS    0H                                                  0081 00194000
*           IF R15=0                /* ATTACHES MUST BE SUCCESSFUL IN   00195000
*                                      ORDER TO CONTINUE             */ 00196000
*             THEN                                                 0081 00197000
         LTR   R15,R15                                             0081 00198000
         BNZ   @RF00081                                            0081 00199000
*             DO;                                                  0082 00200000
*               DO;                                                0083 00201000
*                                                                  0083 00202000
*                 /***************************************************/ 00203000
*                 /*                                                 */ 00204000
*                 /* WHEN COMPLETING THEIR INITIALIZATION EACH TASK  */ 00205000
*                 /* WILL POST ITS ECB.--(SINCE THE ECB'S ARE ALSO   */ 00206000
*                 /* THE END OF TASK ECB'S, THE CONTROLLER WILL BE   */ 00207000
*                 /* NOTIFIED EVEN IF THE ATTACHED TASK FAILED AND   */ 00208000
*                 /* ENTERED ABEND. A CODE OF ZERO IN THE ECB        */ 00209000
*                 /* INDICATES SUCCESSFUL INITIALIZATION. IF ANY OF  */ 00210000
*                 /* THE ECB'S CONTAIN NON- ZERO, THE RETURN CODE TO */ 00211000
*                 /* THE MASTER SCHEDULER WILL BE SET TO 4 TO        */ 00212000
*                 /* INDICATE THAT IPL SHOULD BE ABORTED. THE ECB'S  */ 00213000
*                 /* ARE LEFT ALLOCATED AND POSTED FOR THE LIFE OF   */ 00214000
*                 /* THE IPL, BUT THE LIST OF ECB POINTERS USED FOR  */ 00215000
*                 /* THE WAIT IS FREED, ALONG WITH THE ATTACH        */ 00216000
*                 /* PARAMETER LIST. THE ESTAE IS CANCELLED BEFORE   */ 00217000
*                 /* RETURN TO IEEMB860. WAIT FOR ATTACHED TASKS TO  */ 00218000
*                 /* COMPLETE INITIALIZATION                         */ 00219000
*                 /*                                                 */ 00220000
*                 /***************************************************/ 00221000
*                                                                  0084 00222000
*                 GEN SETS(RECECB,DUMPECB,MEMRYECB) REFS(PTRLIST,R3);   00223000
   WAIT 3,ECBLIST=PTRLIST(R3)                                           00224000
*                 IF ECBRECRD^=0|ECBSDUMP^=0|ECBMEMTM^=0 THEN      0085 00225000
         ICM   @14,7,ECBRECRD(R3)                                  0085 00226000
         BNZ   @RT00085                                            0085 00227000
         ICM   @14,7,ECBSDUMP(R3)                                  0085 00228000
         BNZ   @RT00085                                            0085 00229000
         ICM   @14,7,ECBMEMTM(R3)                                  0085 00230000
         BZ    @RF00085                                            0085 00231000
@RT00085 DS    0H                                                  0086 00232000
*                   R15=4;          /* IF ANY ATTACHED TASK HAS    0086 00233000
*                                      POSTED ITS ECB WITH A NON-ZERO   00234000
*                                      CODE (TO INDICATE FAILURE TO     00235000
*                                      INITIALIZE) THEN THE RETURN 0086 00236000
*                                      CODE FOR THE MASTER IS SET TO    00237000
*                                      4.                            */ 00238000
         LA    R15,4                                               0086 00239000
*                 ELSE              /* INITIALIZATION SUCCESSFUL     */ 00240000
*                   DO;                                            0087 00241000
*                                                                  0087 00242000
         B     @RC00085                                            0087 00243000
@RF00085 DS    0H                                                  0088 00244000
*                     /***********************************************/ 00245000
*                     /*                                             */ 00246000
*                     /* FREE WORKAREA EXCEPT ECB'S OF ATTACHED TASKS*/ 00247000
*                     /*                                             */ 00248000
*                     /***********************************************/ 00249000
*                                                                  0088 00250000
*                     RESPECIFY                                    0088 00251000
*                      (R1,                                        0088 00252000
*                       R2) RESTRICTED;                            0088 00253000
*                     R1=ADDR(WORKAREA);                           0089 00254000
         LR    R1,R3                                               0089 00255000
*                     R2=LENGTH(FREEPART);                         0090 00256000
         LA    R2,104                                              0090 00257000
*                     GEN SETS(R0,R1,R15) REFS(R2);                0091 00258000
     FREEMAIN R,LV=(2),A=(1),SP=253                                     00259000
*                     RESPECIFY                                    0092 00260000
*                      (R1,                                        0092 00261000
*                       R2) UNRESTRICTED;                          0092 00262000
*                     R15=0;                                       0093 00263000
         SLR   R15,R15                                             0093 00264000
*                   END;                                           0094 00265000
*               END;                                               0095 00266000
*                                                                  0095 00267000
@RC00085 DS    0H                                                  0096 00268000
*               /*****************************************************/ 00269000
*               /*                                                   */ 00270000
*               /* IF SUCCESSFUL - FREE WORKAREA EXCEPT ECB'S        */ 00271000
*               /*                                                   */ 00272000
*               /*****************************************************/ 00273000
*                                                                  0096 00274000
*               IF R15=0            /* ATTACHED TASKS MUST HAVE    0096 00275000
*                                      SUCCESSFULLY INITIALIZED IN 0096 00276000
*                                      ORDER TO CONTINUE             */ 00277000
*                 THEN                                             0096 00278000
         LTR   R15,R15                                             0096 00279000
         BNZ   @RF00096                                            0096 00280000
*                 DO;                                              0097 00281000
*                   GEN;                                           0098 00282000
                        ESTAE 0    /*  CANCEL ESTAE RECOVERY  */        00283000
*                   R15=0;          /* SET GOOD RETURN CODE          */ 00284000
         SLR   R15,R15                                             0099 00285000
*                 END;                                             0100 00286000
*               ELSE                                               0101 00287000
*                 R15=4;            /* SET FAILURE RETURN CODE     0101 00288000
*                                      BECAUSE AN ATTACHED TASK    0101 00289000
*                                      FAILED TO INITIALIZE          */ 00290000
         B     @RC00096                                            0101 00291000
@RF00096 LA    R15,4                                               0101 00292000
*             END;                                                 0102 00293000
*           ELSE                                                   0103 00294000
*             R15=4;                /* SET FAILURE RETURN CODE     0103 00295000
*                                      BECAUSE ATTACH FAILED         */ 00296000
         B     @RC00081                                            0103 00297000
@RF00081 LA    R15,4                                               0103 00298000
*         END;                                                     0104 00299000
*       ELSE                                                       0105 00300000
*         R15=4;                    /* SET FAILURE RETURN CODE     0105 00301000
*                                      BECAUSE ESTAE ENVIRONMENT   0105 00302000
*                                      FAILED TO INITIALIZE          */ 00303000
         B     @RC00065                                            0105 00304000
@RF00065 LA    R15,4                                               0105 00305000
*     END;                                                         0106 00306000
*   ELSE                            /* GETMAIN FAILED.               */ 00307000
*     R15=4;                        /* SET FAILURE RETURN CODE     0107 00308000
*                                      BECAUSE GETMAIN FAILED        */ 00309000
         B     @RC00055                                            0107 00310000
@RF00055 LA    R15,4                                               0107 00311000
*MSIRETRN:                                                         0108 00312000
*   RESPECIFY                                                      0108 00313000
*    (R1,                                                          0108 00314000
*     R13) RESTRICTED;                                             0108 00315000
@RC00055 DS    0H                                                  0108 00316000
MSIRETRN DS    0H                                                  0109 00317000
*   RETURN CODE(R15);               /* RETURN TO MASTER SCHED.       */ 00318000
         LR    @11,@15                                             0109 00319000
         L     @13,4(,@13)                                         0109 00320000
         L     @00,@SIZDATD                                        0109 00321000
         LR    @01,@12                                             0109 00322000
         FREEMAIN R,LV=(0),A=(1)                                        00323000
         LR    @15,@11                                             0109 00324000
         L     @14,12(,@13)                                        0109 00325000
         LM    @00,@12,20(@13)                                     0109 00326000
         BR    @14                                                 0109 00327000
*MSIENDED:                                                         0110 00328000
*   R10=R1->BASESAVE;               /* RESTORE BASEREG TO ENSURE   0110 00329000
*                                      PROPER ADDRESSIBILITY         */ 00330000
MSIENDED L     R10,BASESAVE(,R1)                                   0110 00331000
*   R13=R1->R13SAVE;                /* ENSURE PROPER SAVE AREA PTR   */ 00332000
         L     R13,R13SAVE(,R1)                                    0111 00333000
*   R15=4;                          /* SET FAILURE RETURN CODE       */ 00334000
         LA    R15,4                                               0112 00335000
*   GOTO MSIRETRN;                  /* BRANCH TO GET OUT             */ 00336000
*                                                                  0113 00337000
         B     MSIRETRN                                            0113 00338000
*   /*****************************************************************/ 00339000
*   /*                                                               */ 00340000
*   /* THIS ESTAE RECOVERY ENVIRONMENT IS TO PROTECT AGAINST ABENDS  */ 00341000
*   /* DURING THE INITIALIZATION PROCESS. UPON ENTRY IF REG 0        */ 00342000
*   /* CONTAINS A CODE 12 THEN NO RTCA IS AVAILABLE, AND R2 CONTAINS */ 00343000
*   /* THE ADDRESS OF THE USER PARM LIST. THE CODE REG IS RESTORED   */ 00344000
*   /* FROM THE FIRST WORD OF THE PARM LIST. REG 0 IS SET WITH THE   */ 00345000
*   /* RETRY ADDRESS (PTR TO LABEL -MSIENDED-) IN THE MAINLINE CODE  */ 00346000
*   /* AND REG 15 IS SET TO CODE 4 TO INDICATE RETRY. A BRANCH REG 14*/ 00347000
*   /* TRANSFERS CONTROL TO THE RETRY RTN VIA TASK RECOVERY. UPON    */ 00348000
*   /* ENTRY IF REG 0 DOES NOT CONTAIN CODE 12 THEN R1 CONTAINS A PTR*/ 00349000
*   /* TO THE RTCA(SDWA), THE FIRST WORD OF WHICH POINTS TO THE USER */ 00350000
*   /* PARM LIST. THE CODE REG IS RESTORED AS EXPLAINED ABOVE. SETRP */ 00351000
*   /* IS THEN ISSUED SPECIFYING RETRY, THE SAME RETRY ADDRESS AS    */ 00352000
*   /* MENTIONED ABOVE AND FREESDWA.                                 */ 00353000
*   /*                                                               */ 00354000
*   /*****************************************************************/ 00355000
*                                                                  0114 00356000
*MSIESTAE:                          /* START OF ESTAE EXIT.          */ 00357000
*   RESPECIFY                                                      0114 00358000
*    (R0,                                                          0114 00359000
*     R1,                                                          0114 00360000
*     R2,                                                          0114 00361000
*     R8,                                                          0114 00362000
*     R14) RESTRICTED;                                             0114 00363000
MSIESTAE DS    0H                                                  0115 00364000
*   GEN;                                                           0115 00365000
*                                                                  0115 00366000
      USING  *,15                                              @YM02744 00367000
      LA     R8,12                 SET UP CONSTANT FOR COMPARE @YM02744 00368000
      CR     R0,R8                  IS THERE AN RTCA           @YM02744 00369000
      BNE    RTCAAVAL               YES                        @YM02744 00370000
      DROP   15                                                @YM02744 00371000
*   /*****************************************************************/ 00372000
*   /*                                                               */ 00373000
*   /* RESTORE CODE REG FROM FIRST WORD OF USER PARAM LIST AS POINTED*/ 00374000
*   /* TO BY REG 2                                                   */ 00375000
*   /*                                                               */ 00376000
*   /*****************************************************************/ 00377000
*                                                                  0116 00378000
*   R10=R2->BASESAVE;               /* RESTORE CODEREG               */ 00379000
*                                                                  0116 00380000
         L     R10,BASESAVE(,R2)                                   0116 00381000
*   /*****************************************************************/ 00382000
*   /*                                                               */ 00383000
*   /* THE CODE REG WAS SAVED AT BEGINNING OF MAINLINE CODE AND      */ 00384000
*   /* RESTORED NOW TO ENSURE PROPER ADDRESSIBILITY TO RETRY ROUTINE */ 00385000
*   /*                                                               */ 00386000
*   /*****************************************************************/ 00387000
*                                                                  0117 00388000
*   R0=ADDR(MSIENDED);              /* SET RETRY ADDRESS             */ 00389000
         LA    R0,MSIENDED                                         0117 00390000
*   R15=4;                          /* INDICATE RETRY                */ 00391000
         LA    R15,4                                               0118 00392000
*   GOTO R14->OUT;                  /* RETURN TO TASK RECOVERY       */ 00393000
         BR    R14                                                 0119 00394000
*RTCAAVAL:                          /* AN RTCA IS AVAILIBLE          */ 00395000
*                                                                  0120 00396000
*   /*****************************************************************/ 00397000
*   /*                                                               */ 00398000
*   /* RESTORE CODEREG FROM USER PARAM LIST AS POINTED TO BY R1 ->   */ 00399000
*   /* SDWA(1ST WORD) -> PARM LIST                                   */ 00400000
*   /*                                                               */ 00401000
*   /*****************************************************************/ 00402000
*                                                                  0120 00403000
*   R2=R1->SDWAPARM;                /* SET PTR TO USER PARM LIST FROM   00404000
*                                      SDWA                          */ 00405000
RTCAAVAL L     R2,SDWAPARM(,R1)                                    0120 00406000
*   R1->SDWAMODN='IEAVTMSI';        /* MODULE INVOLVED IN ERROR      */ 00407000
         MVC   SDWAMODN(8,R1),@CC01596                             0121 00408000
*   R1->SDWACSCT='IEAVTMSI';        /* CSECT INVOLVED IN ERROR       */ 00409000
         MVC   SDWACSCT(8,R1),@CC01596                             0122 00410000
*   R1->SDWAREXN='IEAVTMSI';        /* MOD HANDLING RECOVERY         */ 00411000
         MVC   SDWAREXN(8,R1),@CC01596                             0123 00412000
*   R10=R2->BASESAVE;               /* RESTORE BASE REG TO ENSURE  0124 00413000
*                                      PROPER ADDRESSIBILITY TO RETRY   00414000
*                                      RTN                           */ 00415000
*                                                                  0124 00416000
         L     R10,BASESAVE(,R2)                                   0124 00417000
*   /*****************************************************************/ 00418000
*   /*                                                               */ 00419000
*   /* USE SETRP --- SPECIFY RETURN ADDRESS, RECORDING, RETRY AND    */ 00420000
*   /* FREEING THE SDWA.                                             */ 00421000
*   /*                                                               */ 00422000
*   /*****************************************************************/ 00423000
*                                                                  0125 00424000
