IEAVTMSI CSECT ,                                                   0001 00003000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00004000
         BALR  @10,0                                               0001 00005000
@PSTART  DS    0H                                                  0001 00006000
         USING @PSTART,@10                                         0001 00007000
         L     @00,@SIZDATD                                        0001 00008000
         GETMAIN R,LV=(0)                                               00009000
         LR    @12,@01                                             0001 00010000
         USING @DATD,@12                                           0001 00011000
         ST    @13,@SA00001+4                                      0001 00012000
         LM    @00,@01,20(@13)                                     0001 00013000
         ST    @12,8(,@13)                                         0001 00014000
         LR    @13,@12                                             0001 00015000
*                                      TO EFFECT A BRANCH REG 14   0038 00016000
*                                      WITHOUT GENERATING A WARNING     00017000
*                                      MSG.                          */ 00018000
*                                                                  0038 00019000
*   /*****************************************************************/ 00020000
*   /*                                                               */ 00021000
*   /* RTMTRACK PARM LIST                                            */ 00022000
*   /*                                                               */ 00023000
*   /*****************************************************************/ 00024000
*                                                                  0039 00025000
*   DO;                                                            0039 00026000
*                                                                  0039 00027000
*     /***************************************************************/ 00028000
*     /*                                                             */ 00029000
*     /* A CONDITIONAL GETMAIN (SP=253) IS DONE FOR A WORKAREA. IF   */ 00030000
*     /* UNSUCCESSFUL INITIALIZATION IS ABORTED. IF SUCCESSFUL THE   */ 00031000
*     /* CORE IS CLEARED AND ECB PTRS , USER PARM LIST INITIALIZED.  */ 00032000
*     /* THIS WORKAREA IS USED FOR THE ATTACH PARAMETER LIST, THE    */ 00033000
*     /* LIST OF ECB POINTERS, THE USER PARAMETER LIST FOR THE ESTAE */ 00034000
*     /* EXIT ROUTINE AND THE ESTAE PARAMETER LIST (ALL OF WHICH ARE */ 00035000
*     /* FREED IF INITIALIZATION IS SUCCESSFUL). THE END OF THE      */ 00036000
*     /* WORKAREA IS USED FOR THE ECB'S OF THE TASKS TO BE ATTACHED  */ 00037000
*     /* AND ARE NEVER FREED.                                        */ 00038000
*     /*                                                             */ 00039000
*     /***************************************************************/ 00040000
*                                                                  0040 00041000
*     RESPECIFY                                                    0040 00042000
*      (R1,                                                        0040 00043000
*       R2,                                                        0040 00044000
*       R15) RESTRICTED;                                           0040 00045000
*     R2=LENGTH(WORKAREA);                                         0041 00046000
         LA    R2,116                                              0041 00047000
*     GEN SETS(R0,R1,R15) REFS(R2);                                0042 00048000
  GETMAIN RC,LV=(2),SP=253                                              00049000
*     RESPECIFY                                                    0043 00050000
*       R2 UNRESTRICTED;                                           0043 00051000
*     IF R15=0 THEN                                                0044 00052000
