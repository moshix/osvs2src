         LTR   @12,@12                                             0195 00532000
         BZ    @RF00195                                            0195 00533000
*                 DO;                                              0196 00534000
*                   STOPFLAG=OFF;   /* INITIALIZE FOR DO WHILE       */ 00535000
         NI    STOPFLAG(GOTNPTR),B'01111111'                       0197 00536000
*                   DO WHILE(STOPFLAG=OFF);/* WHEN SRBS ARE KNOWN TO    00537000
*                                      BE STOPPED, DO ENDS           */ 00538000
         B     @DE00198                                            0198 00539000
@DL00198 DS    0H                                                  0199 00540000
*                     STOPFLAG=ON;  /* WILL BE SET TO OFF IN LOOP    */ 00541000
         OI    STOPFLAG(GOTNPTR),B'10000000'                       0199 00542000
*                     RFY                                          0200 00543000
*                      (CPUFLAGS,                                  0200 00544000
*                       CPUMASK) RSTD;                             0200 00545000
*                                                                  0200 00546000
*                     /***********************************************/ 00547000
*                     /*                                             */ 00548000
*                     /* DISPATCHER LOCK IS OBTAINED HERE AT         */ 00549000
*                     /* BEGINNING OF DO-WHILE AND RELEASED AT END OF*/ 00550000
*                     /* DO-WHILE. THIS MEANS THAT THE LOOP CONTAINS */ 00551000
*                     /* AN ENABLED WINDOW DURING WHICH ANOTHER CPU  */ 00552000
*                     /* CAN ACQUIRE THE DISP LOCK. THIS WILL PREVENT*/ 00553000
*                     /* DEADLOCKS FROM OTHER CPUS WAITING FOR THE   */ 00554000
*                     /* DISP LOCK WHICH IS HELD ON THIS CPU.        */ 00555000
*                     /*                                             */ 00556000
*                     /***********************************************/ 00557000
*                                                                  0201 00558000
*                     DO;           /* SETLOCK (OBTAIN) MODE(UNCOND)    00559000
