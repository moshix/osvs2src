@DL00212 TM    STOPFLAG(GOTNPTR),B'10000000'                       0212 00613000
         BNO   @DC00212                                            0212 00614000
*                           /*****************************************/ 00615000
*                           /*                                       */ 00616000
*                           /* UP TO 16 CPUS ALLOWED                 */ 00617000
*                           /*                                       */ 00618000
*                           /*****************************************/ 00619000
*                                                                  0213 00620000
*                           CPUMASK=CPUMASK/2;/* SHIFT MASK ONE BIT     00621000
*                                      TO RIGHT                      */ 00622000
         SRL   CPUMASK,1                                           0213 00623000
*                           IF(CPUFLAGS&CPUMASK)^=0 THEN/* CHECK MASK   00624000
*                                      AGAINST ALIVE FLAGS TO SEE IF    00625000
*                                      THIS CPU IS ALIVE             */ 00626000
         LR    @12,CPUFLAGS                                        0214 00627000
         NR    @12,CPUMASK                                         0214 00628000
