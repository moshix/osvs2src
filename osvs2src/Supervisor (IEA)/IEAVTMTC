MTCESTA2 DS    0H                                                  0403 01276000
*     GEN(BALR REG15,0);            /* ADDRESSIBILITY                */ 01277000
         BALR REG15,0                                                   01278000
*     GEN(USING *,REG15);                                          0404 01279000
         USING *,REG15                                                  01280000
*     PSAPTR=0;                     /* PSA LOC 0                     */ 01281000
         SLR   PSAPTR,PSAPTR                                       0405 01282000
*     RTCTFASB=0;                   /* ZERO THE MEMTERM QUEUE PTR    */ 01283000
         SLR   @12,@12                                             0406 01284000
         L     @09,CVTPTR(,PSAPTR)                                 0406 01285000
         L     @09,CVTRTMCT(,@09)                                  0406 01286000
         ST    @12,RTCTFASB(,@09)                                  0406 01287000
*     RTCTMECB=0;                   /* ZERO THE MEMTERM ECB          */ 01288000
         ST    @12,RTCTMECB(,@09)                                  0407 01289000
*     IF REG0=12 THEN               /* FIND TRACK AREA PTR           */ 01290000
         C     REG0,@CF01102                                       0408 01291000
         BNE   @RF00408                                            0408 01292000
*       GOTNPTR=REG2;               /* IN REG2 IF NO SDWA            */ 01293000
         LR    GOTNPTR,REG2                                        0409 01294000
