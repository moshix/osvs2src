*                 DO;               /* CONCURRENT ACTIVITY           */ 00471000
*                                                                  0180 00472000
*                   /*************************************************/ 00473000
*                   /*                                               */ 00474000
*                   /* CALL MEMORY SWITCH TO SWITCH OTER CPUS AWAY   */ 00475000
*                   /* FROM MEMORY                                   */ 00476000
*                   /*                                               */ 00477000
*                   /*************************************************/ 00478000
*                                                                  0181 00479000
*                   RFY                                            0181 00480000
*                    (REG1) RSTD;                                  0181 00481000
*                   REG1=0;         /* INDICATE SWITCH AWAY          */ 00482000
         SLR   REG1,REG1                                           0182 00483000
*                   CALL MEMSWTCH;  /* GO TO MEMORY SWITCH           */ 00484000
         L     @15,CVTVEMS0(,@12)                                  0183 00485000
         BALR  @14,@15                                             0183 00486000
*                   RFY                                            0184 00487000
*                    (REG1) UNRSTD;                                0184 00488000
*                                                                  0184 00489000
*                   /*************************************************/ 00490000
*                   /*                                               */ 00491000
*                   /* RELEASE DISP LOCK                             */ 00492000
*                   /*                                               */ 00493000
*                   /*************************************************/ 00494000
*                                                                  0185 00495000
*                   DO;             /* SETLOCK (RELEASE) TYPE(DISP)     00496000
*                                      RELATED(CSD,IEAVTMTC(STOPACT))*/ 00497000
*                     RFY                                          0186 00498000
*                      (GPR11P,                                    0186 00499000
*                       GPR12P,                                    0186 00500000
*                       GPR13P,                                    0186 00501000
*                       GPR14P) RSTD;                              0186 00502000
