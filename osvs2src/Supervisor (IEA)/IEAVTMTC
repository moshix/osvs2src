@DL00192 DS    0H                                                  0193 00521000
*                   END;            /* END OF SPIN                   */ 00522000
@DE00192 L     @12,ASCBCPUS(,ASCBREG)                              0193 00523000
         LTR   @12,@12                                             0193 00524000
         BNZ   @DL00192                                            0193 00525000
*                 END;                                             0194 00526000
*               IF ASCBSRBS^=0 THEN /* IF THERE ARE SRBS IN MEMORY 0195 00527000
*                                      SOME MAY BE ACTIVE (SOME MAY     00528000
*                                      BE SUSPENDED FOR PAGING,    0195 00529000
