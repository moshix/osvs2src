*                                      TYPE(DISP)                  0169 00432000
*                                      RELATED(CSD,IEAVTMTC(STOPACT))*/ 00433000
*             RFY                                                  0170 00434000
*              (GPR11P,                                            0170 00435000
*               GPR12P,                                            0170 00436000
*               GPR13P,                                            0170 00437000
*               GPR14P) RSTD;                                      0170 00438000
*             GPR13P=PSALITA;       /* ADDRESS OF LOCK INTERFACE   0171 00439000
