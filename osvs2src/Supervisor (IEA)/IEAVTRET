*   END RETSRBEP;                                                  0716 02256000
*                                                                  0716 02257000
*   /*****************************************************************/ 02258000
*   /*                                                               */ 02259000
*   /* THE RCDTSKR PROC WILL GAIN CONTROL FROM RTM WHEN AN ERROR IS  */ 02260000
*   /* SUFFERED. IF WILL DETERMINE TYPE OF ERROR AND TAKE NECESSARY  */ 02261000
*   /* ACTION. IF THE ERROR IS DEEMED RECOVERABLE RETRY IS DONE AT A */ 02262000
*   /* COMMON POINT IN THE MAINLINE MODULE AND IT DETERMINES WHERE   */ 02263000
*   /* RETRY IN THE MAINLINE                                         */ 02264000
*   /*                                                               */ 02265000
*   /*****************************************************************/ 02266000
*                                                                  0717 02267000
*RCDTSKR:                                                          0717 02268000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0717 02269000
RCDTSKR  DS    0H                                                  0718 02270000
*   RFY                                                            0718 02271000
*    (R14,                                                         0718 02272000
*     R7,                                                          0718 02273000
*     R2,                                                          0718 02274000
*     R1,                                                          0718 02275000
*     R1) RSTD;                                                    0718 02276000
*   DO;                             /* ENTESTAE (8,9)ERREXIT(NORTCA) */ 02277000
*/*   ENTRY CODE FOR ESTAE ROUTINE.                                  */ 02278000
*/*   IF RTCA ALLOCATED THEN RESTORE CODE AND                        */ 02279000
*/*   DATA REGS FROM START OF USERS PARM LIST,                       */ 02280000
*/*   AND ESTABLISH ADDR TO ESTAE SAVE AREA                          */ 02281000
*/*   IF REQUIRED.                                                   */ 02282000
*/*   IF NO RTCA AVAILABLE THEN BRANCH TO ERROR                      */ 02283000
*/*   CODE AFTER ESTABLISHING ADDRESSABILITY                         */ 02284000
*                                                                  0720 02285000
*     GEN(USING *,GPR15P);                                         0720 02286000
         USING *,GPR15P                                                 02287000
*     GEN(CH    GPR00F,*+22);       /* CHECK IF RTCA ALLOC FAILED    */ 02288000
         CH    GPR00F,*+22                                              02289000
*     GEN(BNE   *+20);              /* NO,SAVE RTM REGS              */ 02290000
         BNE   *+20                                                     02291000
*     GEN(DROP  GPR15P);            /* DROP LOCAL ADDR               */ 02292000
         DROP  GPR15P                                                   02293000
*     GEN(LR    GPR15P,GPR02P);     /* PTR TO USER PARM LIST         */ 02294000
         LR    GPR15P,GPR02P                                            02295000
*     GEN(L     8,00(GPR15P));                                     0725 02296000
         L     8,00(GPR15P)                                             02297000
*     GEN(L     9,04(GPR15P));                                     0726 02298000
