*               /*****************************************************/ 00779000
*               /*                                                   */ 00780000
*               /* CLEAR BUFFERING AREA TO ZERO.                     */ 00781000
*               /*                                                   */ 00782000
*               /*****************************************************/ 00783000
*                                                                  0327 00784000
*               RFY                                                0327 00785000
*                (R4,                                              0327 00786000
*                 R5,                                              0327 00787000
*                 R2,                                              0327 00788000
*                 R3) RSTD;         /* RESTRICT REGS FOR MVCL        */ 00789000
*               R4=ADDR(RCBBUFRS);  /* START ADDR (TO).              */ 00790000
         LA    R4,RCBBUFRS(,R7)                                    0328 00791000
*               R5=LENGTH(RCBBUFRS);/* LENGTH TO CLEAR.              */ 00792000
         LA    R5,4000                                             0329 00793000
*               R2=R4;              /* START ADDR (FROM).            */ 00794000
         LR    R2,R4                                               0330 00795000
*               R3=ZERO;            /* PADD AND COUNT IS ZERO        */ 00796000
         SLR   R3,R3                                               0331 00797000
*               GEN(MVCL R4,R2);    /* CLEAR OUT THE AREA.           */ 00798000
         MVCL R4,R2                                                     00799000
*               RFY                                                0333 00800000
*                (R4,                                              0333 00801000
*                 R5,                                              0333 00802000
*                 R2,                                              0333 00803000
*                 R3) UNRSTD;       /* RESTRICT REGS FOR MVCL        */ 00804000
*               R7=RTCTRCB;         /* BACK TO FIXED RCB             */ 00805000
         L     @14,CVTPTR                                          0334 00806000
         L     @14,CVTRTMCT(,@14)                                  0334 00807000
         L     R7,RTCTRCB(,@14)                                    0334 00808000
*               RESPECIFY           /* RELEASE GETMAINED BUFFER REG  */ 00809000
*                (R1) UNRESTRICTED;                                0335 00810000
*                                                                  0335 00811000
*               /*****************************************************/ 00812000
*               /*                                                   */ 00813000
*               /* NOW LOOP THROUGH RCB AND MOVE RECORDS TO GETMAINED*/ 00814000
*               /* BUFFER                                            */ 00815000
*               /*                                                   */ 00816000
*               /*****************************************************/ 00817000
*                                                                  0336 00818000
*               LOOP=YES;           /* INSURE FIRST TIME THROUGH LOOP*/ 00819000
         OI    LOOP,B'10000000'                                    0336 00820000
*               DO WHILE(LOOP=YES); /* MOVE RECORDS TIL ALL MOVED,NO    00821000
*                                      READY RECORD OR SPACE       0337 00822000
*                                      EXHAUSTED                     */ 00823000
         B     @DE00337                                            0337 00824000
@DL00337 DS    0H                                                  0338 00825000
*                 OLDEST=RCBFREE+RCBFLNG;/* FIND OLDEST              */ 00826000
         LH    @14,RCBFLNG(,R7)                                    0338 00827000
         N     @14,@CF02236                                        0338 00828000
         AL    @14,RCBFREE(,R7)                                    0338 00829000
         ST    @14,OLDEST                                          0338 00830000
*                 IF OLDEST>=RCBBUFE THEN/* OUTSIDE BUFFER ?         */ 00831000
         C     @14,RCBBUFE(,R7)                                    0339 00832000
         BL    @RF00339                                            0339 00833000
*                   OLDEST=OLDEST-RCBTLNG;/* WRAP TO BEGINNING OF  0340 00834000
*                                      BUFFER                        */ 00835000
         LH    @12,RCBTLNG(,R7)                                    0340 00836000
         N     @12,@CF02236                                        0340 00837000
         SLR   @14,@12                                             0340 00838000
         ST    @14,OLDEST                                          0340 00839000
*                 RFY                                              0341 00840000
*                   RCBBUF BASED(OLDEST);/* BASE THE BUFFER MAP ON 0341 00841000
*                                      THIS REC                      */ 00842000
@RF00339 DS    0H                                                  0342 00843000
*/* THE DUMMY ALLOCATION CHECK IS NOT NEEDED SINCE THE HEADERS CAN 0342 00844000
*    NOW WRAP THE BUFFER ALSO. THIS ELIMINATES THE DUMMY SPACE AT THE   00845000
*    BOTTOM OF THE BUFFER                                     @ZA19199  00846000
*                                                                    */ 00847000
*                                                                  0342 00848000
*                 IF RCBBRDY=NO THEN/* IF RECORD NOT READY           */ 00849000
         L     @14,OLDEST                                          0342 00850000
         TM    RCBBRDY(@14),B'00000001'                            0342 00851000
         BNZ   @RF00342                                            0342 00852000
*                   LOOP=NO;        /* STOP SEARCH                   */ 00853000
         NI    LOOP,B'01111111'                                    0343 00854000
*                 ELSE              /* RECORD IS READY       @ZA19199*/ 00855000
*                   DO;                                            0344 00856000
         B     @RC00342                                            0344 00857000
@RF00342 DS    0H                                                  0345 00858000
*                     FREESPSW=YES; /* THE SPACE WILL BE FREED IF  0345 00859000
*                                      RECORD IS MOVED               */ 00860000
         OI    FREESPSW,B'10000000'                                0345 00861000
*                     IF RCBBNBFR=YES THEN                         0346 00862000
         L     @14,OLDEST                                          0346 00863000
         TM    RCBBNBFR(@14),B'00001000'                           0346 00864000
         BNO   @RF00346                                            0346 00865000
*                       DO;         /* UNBUFFERED RECORD...          */ 00866000
*                         MOVELEN=NBFRLEN;/* LENGTH                  */ 00867000
         LA    @12,20                                              0348 00868000
         ST    @12,MOVELEN                                         0348 00869000
*                         IF RCBBERFG=YES THEN/* IS ERRORID APPENDED    00870000
*                                                            @G17EP5W*/ 00871000
         TM    RCBBERFG(@14),B'00000010'                           0349 00872000
         BNO   @RF00349                                            0349 00873000
*                           MOVELEN=MOVELEN+LENGTH(NBFRERID);/*    0350 00874000
*                                      INCLUDE ITS LENGTH    @G17EP5W*/ 00875000
         AL    @12,@CF00063                                        0350 00876000
         ST    @12,MOVELEN                                         0350 00877000
*                       END;        /* END UNBUFFERED RECORD @G17EP5W*/ 00878000
*                     ELSE          /* BUFFERED RECORD               */ 00879000
*                       MOVELEN=RCBBLEN+/* LENGTH                    */ 00880000
*                           CONTLEN;                               0352 00881000
         B     @RC00346                                            0352 00882000
@RF00346 LA    @14,16                                              0352 00883000
         L     @12,OLDEST                                          0352 00884000
         LH    @12,RCBBLEN(,@12)                                   0352 00885000
         N     @12,@CF02236                                        0352 00886000
         ALR   @14,@12                                             0352 00887000
         ST    @14,MOVELEN                                         0352 00888000
*                     IF REBUFADR->RCBFLNG>=/* SPACE IN GOTTEN BUFFER   00889000
*                                      ?                             */ 00890000
*                         MOVELEN THEN/* FOR THIS RECORD             */ 00891000
@RC00346 L     @14,MOVELEN                                         0353 00892000
         L     @12,REBUFADR                                        0353 00893000
         LH    @06,RCBFLNG(,@12)                                   0353 00894000
         N     @06,@CF02236                                        0353 00895000
         CLR   @14,@06                                             0353 00896000
         BH    @RF00353                                            0353 00897000
*/********************************************************************/ 00898000
*/*                                                                  */ 00899000
*/* THE MOVEREC SEGMENT MOVES THE DATA FROM THE RCB TO A GETMAINED   */ 00900000
*/* BUFFER. ALLOWING FOR WRAPPED RECORDS IN THE RCB                  */ 00901000
*/*                                                                  */ 00902000
*/********************************************************************/ 00903000
*                                                                  0354 00904000
*                       DO;                                        0354 00905000
*                         RESPECIFY /* RESTRICT REGISTERS FOR        */ 00906000
*                          (R2,     /* POINTER TO MOVE FROM          */ 00907000
*                           R3,     /* LENGTH TO MOVE FROM           */ 00908000
*                           R4,     /* POINTER TO MOVE TO            */ 00909000
*                           R5) RESTRICTED;/* LENGTH TO MOVE TO      */ 00910000
*                         R2=OLDEST;/* MOVE DATA FROM THE OLDEST   0356 00911000
*                                      RECORD IN THE FIXED BUFFER    */ 00912000
         L     @06,OLDEST                                          0356 00913000
         LR    R2,@06                                              0356 00914000
*                         R4=REBUFADR->RCBFREE;/* MOVE DATA TO NEXT     00915000
*                                      FREE SPACE IN GETMAINED BUFFER*/ 00916000
         L     R4,RCBFREE(,@12)                                    0357 00917000
*                         IF RCBBWRAP=YES THEN/* IF THE RECORD WRAPS    00918000
*                                      THE BUFFER                    */ 00919000
         TM    RCBBWRAP(@06),B'10000000'                           0358 00920000
         BNO   @RF00358                                            0358 00921000
*                           DO;     /* MOVE IN TWO STAGES            */ 00922000
*                             R5=RCBBUFE-R2;/* MOVE DATA FROM BEGINNG   00923000
*                                      OF RECORD             @YM08276*/ 00924000
         L     R5,RCBBUFE(,R7)                                     0360 00925000
         SLR   R5,R2                                               0360 00926000
*                             R3=MOVELEN;/* THRU END OF BUFFER     0361 00927000
*                                                            @YM08276*/ 00928000
         LR    R3,@14                                              0361 00929000
*                             GEN(MVCL R4,R2);/* MOVE THE DATA       */ 00930000
         MVCL R4,R2                                                     00931000
*                             R2=RCBBUFB;/* WRAP TO BEGINNING OF   0363 00932000
*                                      FIXED BUFFER                  */ 00933000
         L     R2,RCBBUFB(,R7)                                     0363 00934000
*                             R5=R3;/* PICK UP REMANING LENGTH     0364 00935000
*                                                            @YM08276*/ 00936000
         LR    R5,R3                                               0364 00937000
*                             GEN(MVCL R4,R2);/* MOVE THE DATA       */ 00938000
         MVCL R4,R2                                                     00939000
*                           END;                                   0366 00940000
*                         ELSE      /* IF NOT A WRAPPED RECORD       */ 00941000
*                           DO;                                    0367 00942000
         B     @RC00358                                            0367 00943000
@RF00358 DS    0H                                                  0368 00944000
*                             R3=MOVELEN;/* MOVE THE ENTIRE RECORD   */ 00945000
         L     @14,MOVELEN                                         0368 00946000
         LR    R3,@14                                              0368 00947000
*                             R5=MOVELEN;                          0369 00948000
         LR    R5,@14                                              0369 00949000
*                             GEN(MVCL R4,R2);/* MOVE THE DATA       */ 00950000
         MVCL R4,R2                                                     00951000
*                           END;                                   0371 00952000
*                         R7=REBUFADR;/* MAP THE GETMAINED BUFFER    */ 00953000
@RC00358 L     R7,REBUFADR                                         0372 00954000
*                         RCBFLNG=RCBFLNG-(MOVELEN+/* DEDUCT ROUNDED    00955000
*                                      RECORD LENGTH                 */ 00956000
*                             SEVEN&'FFF8'X);/* FROM GETMAINED BUFFER*/ 00957000
         LA    @14,7                                               0373 00958000
         AL    @14,MOVELEN                                         0373 00959000
         N     @14,@CF02103                                        0373 00960000
         LH    @12,RCBFLNG(,R7)                                    0373 00961000
         N     @12,@CF02236                                        0373 00962000
         SLR   @12,@14                                             0373 00963000
         STH   @12,RCBFLNG(,R7)                                    0373 00964000
*                         RCBFREE=RCBFREE+(MOVELEN+/* POINT TO     0374 00965000
*                                      FREESPACE IN GETMAINED        */ 00966000
*                             SEVEN&'FFF8'X);/* BUFFER               */ 00967000
         AL    @14,RCBFREE(,R7)                                    0374 00968000
         ST    @14,RCBFREE(,R7)                                    0374 00969000
*                         R7=RTCTRCB;/* MAP THE FIXED BUFFER         */ 00970000
         L     @14,CVTPTR                                          0375 00971000
         L     @14,CVTRTMCT(,@14)                                  0375 00972000
         L     R7,RTCTRCB(,@14)                                    0375 00973000
*                         RESPECIFY /* RELEASE REGISTERS FROM        */ 00974000
*                          (R2,     /* POINTER TO MOVE FROM          */ 00975000
*                           R3,     /* LENGTH TO MOVE FROM           */ 00976000
*                           R4,     /* POINTER TO MOVE TO            */ 00977000
*                           R5) UNRESTRICTED;/* LENGTH TO MOVE TO    */ 00978000
*                       END;                                       0377 00979000
*                     ELSE          /* NO - SPACE                    */ 00980000
*                       DO;         /* IN GETMAINED BUFFER           */ 00981000
         B     @RC00353                                            0378 00982000
@RF00353 DS    0H                                                  0379 00983000
*                         LOOP=NO;  /* STOP SEARCH                   */ 00984000
         NI    LOOP,B'01111111'                                    0379 00985000
*                         FREESPSW=NO;/* DO NOT FREE THIS RECORD     */ 00986000
         NI    FREESPSW,B'01111111'                                0380 00987000
*                       END;                                       0381 00988000
*                     IF FREESPSW=YES THEN/* IF RECORD TO BE FREED   */ 00989000
@RC00353 TM    FREESPSW,B'10000000'                                0382 00990000
         BNO   @RF00382                                            0382 00991000
*/********************************************************************/ 00992000
*/*                                                                  */ 00993000
*/* THE FREEREC SEGMENT IS USED TO FREE RECORDS EITHER IN THE RCB    */ 00994000
*/* WHEN THE RECORDS ARE MOVED TO THE GETMAINED BUFFER, OR THE RCB/  */ 00995000
*/* GETMAINED BUFFER AFTER WRITE TO OPERATOR OR LOGREC               */ 00996000
*/*                                                                  */ 00997000
*/********************************************************************/ 00998000
*                                                                  0383 00999000
*                       DO;                                        0383 01000000
*                         RESPECIFY /* RESTRICT REGISTERS FOR        */ 01001000
*                          (R2,     /* SAVING FREE POINTER           */ 01002000
*                           R3,     /* SAVING FREE LEN & DUMMY DISP  */ 01003000
*                           R4,     /* UPDATING FREE POINTER         */ 01004000
*                           R5,     /* UPDATING DUMMY DISPLACEMENT   */ 01005000
*                           R6) RESTRICTED;/* UPDATING FREE LENGTH   */ 01006000
*                         RESPECIFY /* RESTRICT REGISTER FOR         */ 01007000
*                          (R1) RESTRICTED;/* ADDRESS OF FIELD FOR 0385 01008000
*                                      LOAD                          */ 01009000
*                         R1=ADDR(RCBCDS);/* OBTAIN ADDRESS OF     0386 01010000
*                                      RCBFLNG,RCBFREE AND RCBDUM    */ 01011000
         LA    R1,RCBCDS(,R7)                                      0386 01012000
*                         GEN(LM R2,R3,ZIP(R1));/* SAVE FOR        0387 01013000
*                                      SUBSEQUENT CDS                */ 01014000
         LM R2,R3,ZIP(R1)                                               01015000
*                         RESPECIFY /* RELEASE ADDRESS OF FIELD FOR  */ 01016000
*                          (R1) UNRESTRICTED;/* LOAD                 */ 01017000
*TRYCDS2:                           /* RETRY POINT IF CDS FAILS      */ 01018000
*                         R4=R2;    /* FREE POINTER AVAILABLE TO   0389 01019000
*                                      UPDATE                        */ 01020000
TRYCDS2  LR    R4,R2                                               0389 01021000
*                         R5=R3;    /* FREE LENGTH & DUMMY DISP FOR  */ 01022000
         LR    R5,R3                                               0390 01023000
*                         R6=R5;    /* UPDATING IN UNIQUE REGISTERS  */ 01024000
         LR    R6,R5                                               0391 01025000
*                         GENERATE;                                0392 01026000
         SLL   R5,HALFREG         /* ZERO HIGH ORDER TWO BYTES OF    */ 01027000
         SRL   R5,HALFREG         /* DUMMY DISPLACEMENT              */ 01028000
         SRL   R6,HALFREG         /* MOVE FREE LEN TO LOW HALF OF REG*/ 01029000
*                         IF RCBBNBFR=NO THEN/* IF THE RECORD WAS  0393 01030000
*                                      BUFFERED                      */ 01031000
         L     @14,OLDEST                                          0393 01032000
         TM    RCBBNBFR(@14),B'00001000'                           0393 01033000
         BNZ   @RF00393                                            0393 01034000
*                           R6=R6+(RCBBLEN+/* FREE THE DATA          */ 01035000
*                               CONTLEN+/* THE CONTROL INFO          */ 01036000
*                               SEVEN&/* ROUNDED TO A DOUBLE WORD    */ 01037000
*                               'FFF8'X);                          0394 01038000
         LA    @12,23                                              0394 01039000
         LH    @14,RCBBLEN(,@14)                                   0394 01040000
         N     @14,@CF02236                                        0394 01041000
         ALR   @12,@14                                             0394 01042000
         N     @12,@CF02103                                        0394 01043000
         ALR   R6,@12                                              0394 01044000
*                         ELSE                                     0395 01045000
*                           DO;     /* IF RECORD WAS NOT BUFFERED    */ 01046000
         B     @RC00393                                            0395 01047000
@RF00393 DS    0H                                                  0396 01048000
*                             RFY                                  0396 01049000
*                               R1 RSTD;/* RESTRICT REGISTER FOR   0396 01050000
*                                      ERRORID LENGTH        @G17EP5W*/ 01051000
*                             IF RCBBERFG=YES THEN/* IF ERRORID    0397 01052000
*                                      APPENDED IN RCB       @G17EP5W*/ 01053000
         L     @14,OLDEST                                          0397 01054000
         TM    RCBBERFG(@14),B'00000010'                           0397 01055000
         BNO   @RF00397                                            0397 01056000
*                               R1=LENGTH(NBFRERID);/* THEN INCLUDE     01057000
*                                      ITS LENGTH FOR FREEING      0398 01058000
*                                                            @G17EP5W*/ 01059000
         LA    R1,10                                               0398 01060000
*                             ELSE                                 0399 01061000
*                               R1=ZERO;/* OTHERWISE FREE NO       0399 01062000
*                                      ADDITIONAL SPACE      @G17EP5W*/ 01063000
         B     @RC00397                                            0399 01064000
@RF00397 SLR   R1,R1                                               0399 01065000
*                             R6=R6+(NBFRLEN+/* FREE (16 CONTROL INFO   01066000
*                                      + 4 ADDR)                     */ 01067000
*                                 R1+/* LENGTH OF ERRORID IF PRESENT    01068000
*                                                            @G17EP5W*/ 01069000
*                                 SEVEN&/* ROUNDED TO DOUBLE WORD    */ 01070000
*                                 'FFF8'X);                        0400 01071000
@RC00397 LA    @14,27                                              0400 01072000
         ALR   @14,R1                                              0400 01073000
         N     @14,@CF02103                                        0400 01074000
         ALR   R6,@14                                              0400 01075000
*                             RFY                                  0401 01076000
*                               R1 UNRSTD;/* UNRESTRICT LENGTH     0401 01077000
*                                      REGISTER              @G17EP5W*/ 01078000
*                           END;    /*                       @G17EP5W*/ 01079000
*                         GEN(SLL R6,HALFREG);/* MOVE NEW LENGTH TO     01080000
*                                      HIGH HALF OF REGISTER         */ 01081000
@RC00393 DS    0H                                                  0403 01082000
         SLL R6,HALFREG                                                 01083000
*                         R5=R5|R6; /* PUT FREE LENGTH & DUMMY DISP     01084000
*                                      IN SAME REGISTER              */ 01085000
         OR    R5,R6                                               0404 01086000
*                         CDS(R2,R4,RCBCDS);/* PUT VALUES IN RCB     */ 01087000
         CDS   R2,R4,RCBCDS(R7)                                    0405 01088000
*                         BC(4,TRYCDS2);/* IF RCB NOT ALTERED        */ 01089000
         BC    4,TRYCDS2                                           0406 01090000
*                         RESPECIFY /* FREE REGISTERS FROM           */ 01091000
*                          (R2,     /* SAVING FREE POINTER           */ 01092000
*                           R3,     /* SAVING FREE LEN & DUMMY DISP  */ 01093000
*                           R4,     /* UPDATING FREE POINTER         */ 01094000
*                           R5,     /* UPDATING DUMMY DISPLACEMENT   */ 01095000
*                           R6) UNRESTRICTED;/* UPDATING FREE LENGTH */ 01096000
*                       END;                                       0408 01097000
*                     IF RCBFLNG=RCBTLNG THEN/* ALL RECORDS DONE     */ 01098000
@RF00382 CLC   RCBFLNG(2,R7),RCBTLNG(R7)                           0409 01099000
         BNE   @RF00409                                            0409 01100000
*                       LOOP=NO;    /* STOP MOVING RECORDS           */ 01101000
         NI    LOOP,B'01111111'                                    0410 01102000
*                   END;            /* END RECORD READY PROCESS      */ 01103000
@RF00409 DS    0H                                                  0412 01104000
*               END;                /* END MOVE RECORD LOOP          */ 01105000
@RC00342 DS    0H                                                  0412 01106000
@DE00337 TM    LOOP,B'10000000'                                    0412 01107000
         BO    @DL00337                                            0412 01108000
*             END;                                                 0413 01109000
*           ELSE                    /* GETMAIN NOT SUCESSFUL         */ 01110000
*             REBUFADR=ZERO;        /* INDICATE NO REBUFFERING       */ 01111000
         B     @RC00285                                            0414 01112000
@RF00285 SLR   @14,@14                                             0414 01113000
         ST    @14,REBUFADR                                        0414 01114000
*           IF REBUFADR^=ZERO THEN  /* IF GETMAIN OK POINT TO      0415 01115000
*                                      GETMAINED                     */ 01116000
@RC00285 L     @14,REBUFADR                                        0415 01117000
         LTR   @14,@14                                             0415 01118000
         BZ    @RF00415                                            0415 01119000
*             R7=REBUFADR;          /* BUFFER FOR WRITING RECORDS    */ 01120000
*                                                                  0416 01121000
         LR    R7,@14                                              0416 01122000
*           /*********************************************************/ 01123000
*           /*                                                       */ 01124000
*           /* THIS SEGMENT PERFORMS THE ACTUAL OUTPUTTING OF DATA AS*/ 01125000
*           /* REQUESTED BY THE USER. AT THIS POINT THE CODE IS      */ 01126000
*           /* COMMON FOR BOTH A FIXED OR GETMAINED BUFFER. IF       */ 01127000
*           /* WRAPAROUND (SPLIT) RECORDS ARE IN THE FIXED BUFFER    */ 01128000
*           /* THEY WILL NOT BE WRITTEN, AND IF THE RECORD WAS       */ 01129000
*           /* DESTINED FOR LOGREC THE LOST RECORD COUNT WILL BE     */ 01130000
*           /* UPDATED. POSTING WILL BE REQUESTED IF THE USER HAD    */ 01131000
*           /* SPECIFIED IT AND THE ASID IS VALID. RCBPTR CONTAINS   */ 01132000
*           /* ADDRESS OF CORRECT BUFFER AREA.                       */ 01133000
*           /*                                                       */ 01134000
*           /*********************************************************/ 01135000
*                                                                  0417 01136000
*           LOOP=YES;               /* SET DO-WHILE CONTROL.         */ 01137000
*                                                                  0417 01138000
@RF00415 OI    LOOP,B'10000000'                                    0417 01139000
*           /*********************************************************/ 01140000
*           /*                                                       */ 01141000
*           /* GO THROUGH ENTIRE FIXED/GETMAINED BUFFER.             */ 01142000
*           /*                                                       */ 01143000
*           /*********************************************************/ 01144000
*                                                                  0418 01145000
*           DO WHILE(LOOP=YES);                                    0418 01146000
         B     @DE00418                                            0418 01147000
@DL00418 DS    0H                                                  0419 01148000
*             OLDEST=RCBFREE+RCBFLNG;/* FIRST/NEXT RECORD ADDR.      */ 01149000
         LH    @14,RCBFLNG(,R7)                                    0419 01150000
         N     @14,@CF02236                                        0419 01151000
         AL    @14,RCBFREE(,R7)                                    0419 01152000
         ST    @14,OLDEST                                          0419 01153000
*             IF OLDEST>=RCBBUFE THEN/* OUTSIDE FIXED BUFFER...      */ 01154000
         C     @14,RCBBUFE(,R7)                                    0420 01155000
         BL    @RF00420                                            0420 01156000
*               OLDEST=OLDEST-RCBTLNG;/* -YES, WRAP BACK RCD ADDR.   */ 01157000
         LH    @12,RCBTLNG(,R7)                                    0421 01158000
         N     @12,@CF02236                                        0421 01159000
         SLR   @14,@12                                             0421 01160000
         ST    @14,OLDEST                                          0421 01161000
*/* THE DUMMY ALLOCATION CHECK IS NOT NEEDED SINCE THE HEADERS CAN 0422 01162000
*    NOW WRAP THE BUFFER ALSO. THIS ELIMINATES THE DUMMY SPACE AT THE   01163000
*    BOTTOM OF THE BUFFER                                     @ZA19199  01164000
*                                                                    */ 01165000
*                                                                  0422 01166000
*             /*******************************************************/ 01167000
*             /*                                                     */ 01168000
*             /* CHECK IF ALL RECORDS HAVE BEEN PROCESSED.           */ 01169000
*             /*                                                     */ 01170000
*             /*******************************************************/ 01171000
*                                                                  0422 01172000
*             IF RCBFLNG=RCBTLNG THEN/* ALL RECORDS GONE...          */ 01173000
@RF00420 CLC   RCBFLNG(2,R7),RCBTLNG(R7)                           0422 01174000
         BNE   @RF00422                                            0422 01175000
*               LOOP=NO;            /* -YES, GET OUT OF LOOP.        */ 01176000
         NI    LOOP,B'01111111'                                    0423 01177000
*             ELSE                  /* RECORDS STILL REMAIN.         */ 01178000
*               DO;                                                0424 01179000
         B     @RC00422                                            0424 01180000
@RF00422 DS    0H                                                  0425 01181000
*                 RFY                                              0425 01182000
*                   RCBBUF BASED(OLDEST);                          0425 01183000
*                                                                  0425 01184000
*                 /***************************************************/ 01185000
*                 /*                                                 */ 01186000
*                 /* CHECK IF NEXT RECORD IS READY TO OUTPUT.        */ 01187000
*                 /*                                                 */ 01188000
*                 /***************************************************/ 01189000
*                                                                  0426 01190000
*                 IF RCBBRDY=NO THEN/* RECORD IN SHAPE TO WRITE.     */ 01191000
         L     @14,OLDEST                                          0426 01192000
         TM    RCBBRDY(@14),B'00000001'                            0426 01193000
         BNZ   @RF00426                                            0426 01194000
*                   LOOP=NO;        /* -NO, LEAVE IT ALONE (ONLY   0427 01195000
*                                      APPLIES IF RECORD IS IN FIXED    01196000
*                                      BUFFER).                      */ 01197000
         NI    LOOP,B'01111111'                                    0427 01198000
*                 ELSE              /* RECORD IS READY.              */ 01199000
*                   DO;                                            0428 01200000
*                                                                  0428 01201000
         B     @RC00426                                            0428 01202000
@RF00426 DS    0H                                                  0429 01203000
*                     /***********************************************/ 01204000
*                     /*                                             */ 01205000
*                     /* OUTPUT THE RECORD IF IT IS CONTIGUOUS. NOTE-*/ 01206000
*                     /* IT WILL ALWAYS BE IF WE ARE IN THE GETMAINED*/ 01207000
*                     /* BUFFER. TEST IF RECORD IS ADDRESSABLE FORM  */ 01208000
*                     /* MASTER MEMORY IF NO, IT WILL NOT BE         */ 01209000
*                     /* WRITTEN,AND IF IT IS FOR LOGREC UPDATE      */ 01210000
*                     /* LOSTRECORD COUNT.                           */ 01211000
*                     /*                                             */ 01212000
*                     /***********************************************/ 01213000
*                                                                  0429 01214000
*                     NOTADDR=NO;   /* INIT SWITCH                   */ 01215000
         NI    NOTADDR,B'01111111'                                 0429 01216000
*                     IF RCBBNBFR=YES THEN/* UNBUFFERED RECORD...    */ 01217000
         L     @14,OLDEST                                          0430 01218000
         TM    RCBBNBFR(@14),B'00001000'                           0430 01219000
         BNO   @RF00430                                            0430 01220000
*                       DO;         /* YES - CHECK IT                */ 01221000
*                         RFY                                      0432 01222000
*                           R1 RSTD;                               0432 01223000
*                         R1=ADDR(NBFRDATA);/* ADDR OF DATA - TO   0433 01224000
*                                      HANDLE WRAP HEADERS   @ZA19199*/ 01225000
         AL    @14,@CF00054                                        0433 01226000
         LR    R1,@14                                              0433 01227000
*                         IF R1>=RCBBUFE THEN/* DOES THE HEADER WRAP    01228000
*                                                            @ZA19199*/ 01229000
         C     R1,RCBBUFE(,R7)                                     0434 01230000
         BL    @RF00434                                            0434 01231000
*                           R1=R1-RCBTLNG;/* YES - RESET PTR FOR WRAP   01232000
*                                      CASE                  @ZA19199*/ 01233000
         LH    @14,RCBTLNG(,R7)                                    0435 01234000
         N     @14,@CF02236                                        0435 01235000
         SLR   R1,@14                                              0435 01236000
*                         ELSE                                     0436 01237000
*                           ;       /* NO - POINTER OK       @ZA19199*/ 01238000
@RF00434 DS    0H                                                  0437 01239000
*                         RFY                                      0437 01240000
*                           NBFREC BASED(R1);/* REBASE PTR TO HANDLE    01241000
*                                      WRAP CASE             @ZA19199*/ 01242000
*                         R1=NBFRDATA;/* GET RECORD ADDR             */ 01243000
         L     R1,NBFRDATA(,R1)                                    0438 01244000
*                         RFY                                      0439 01245000
*                           NBFREC BASED(ADDR(RCBBDATA));/* REBASE 0439 01246000
*                                      BACK                  @ZA19199*/ 01247000
*                         GEN(LRA R1,ZIP(R1));/* TRY DAT             */ 01248000
         LRA R1,ZIP(R1)                                                 01249000
*                         BC(8,OKADDR);/* ADDRESS OK,BRANCH          */ 01250000
         BC    8,OKADDR                                            0441 01251000
*                         NOTADDR=YES;/* SHOW RECORD UNADDRESS ABLE  */ 01252000
         OI    NOTADDR,B'10000000'                                 0442 01253000
*OKADDR:                                                           0443 01254000
*                         ;         /* TARGET IF ADDR OK             */ 01255000
OKADDR   DS    0H                                                  0444 01256000
*                         RFY                                      0444 01257000
*                           R1 UNRSTD;                             0444 01258000
*                       END;                                       0445 01259000
*                     IF NOTADDR=YES|/* BAD RECORD ADDR - OR         */ 01260000
*                         (REBUFADR=ZERO&/* IN THE FIXED BUFFER...   */ 01261000
*                         RCBBWRAP=YES) THEN/* .AND NON-CONTIGUOUS...   01262000
*                                      -YES, WE WILL DROP IT.        */ 01263000
@RF00430 TM    NOTADDR,B'10000000'                                 0446 01264000
         BO    @RT00446                                            0446 01265000
         L     @14,REBUFADR                                        0446 01266000
         LTR   @14,@14                                             0446 01267000
         BNZ   @RF00446                                            0446 01268000
         L     @14,OLDEST                                          0446 01269000
         TM    RCBBWRAP(@14),B'10000000'                           0446 01270000
         BNO   @RF00446                                            0446 01271000
@RT00446 DS    0H                                                  0447 01272000
*                       DO;                                        0447 01273000
*                         IF RCBBLREC=YES THEN/* IF LOGREC RECORD    */ 01274000
         L     @14,OLDEST                                          0448 01275000
         TM    RCBBLREC(@14),B'01000000'                           0448 01276000
         BNO   @RF00448                                            0448 01277000
*/********************************************************************/ 01278000
*/* THE PURPOSE OF THE INCLCNT SEGMENT IS TO INCREMENT THE LOST      */ 01279000
*/* RECORD COUNT FOR NOT SCHEDULED LOGREC REQUESTS                   */ 01280000
*/********************************************************************/ 01281000
*                                                                  0449 01282000
*                           DO;                                    0449 01283000
*                             RESPECIFY/* RESTRICT REGISTERS FOR     */ 01284000
*                              (R4, /* SAVING LOST RECORD COUNT      */ 01285000
*                               R5) RESTRICTED;/* INCREMENTING LOST     01286000
*                                      REC CT                        */ 01287000
*                             R4=RCBCS1;/* SAVE CURRENT LOST RECORD     01288000
*                                      COUNT                         */ 01289000
         L     R4,RCBCS1(,R7)                                      0451 01290000
*TRYCS1:                            /* RCBLCNT WILL BE INCREMENTED 0452 01291000
*                                      VIA CS EFFECTING SERIALIZATION*/ 01292000
*                             R5=R4;/* MAKE COUNT AVAILABLE FOR    0452 01293000
*                                      INCREMENTING                  */ 01294000
TRYCS1   LR    R5,R4                                               0452 01295000
*                             R5=R5+HIBYTE1;/* INCREMENT BY ONE      */ 01296000
         AL    R5,HIBYTE1                                          0453 01297000
*                             CS(R4,R5,RCBCS1);/* PLACE UPDATED COUNT   01298000
*                                      IN THE RCB                    */ 01299000
         CS    R4,@05,RCBCS1(R7)                                   0454 01300000
*                             BC(4,TRYCS1);/* IF THE RCB NOT ALTERED */ 01301000
         BC    4,TRYCS1                                            0455 01302000
*                             RESPECIFY/* RELEASE REGISTERS FROM     */ 01303000
*                              (R4, /* SAVING LOST RECORD COUNT      */ 01304000
*                               R5) UNRESTRICTED;/* INCREMENTING LOST   01305000
*                                      RECORD COUNT                  */ 01306000
*                           END;    /* END INCLCNT                   */ 01307000
*                       END;                                       0458 01308000
*                     ELSE                                         0459 01309000
*                       DO;                                        0459 01310000
*                                                                  0459 01311000
         B     @RC00446                                            0459 01312000
@RF00446 DS    0H                                                  0460 01313000
*                         /*******************************************/ 01314000
*                         /*                                         */ 01315000
*                         /* THIS SEGMENT PERFORMS THE ACTUAL I/O    */ 01316000
*                         /* INVOLVED IN OUTPUTTING THE DATA (TO     */ 01317000
*                         /* LOGREC OR THE OPERATOR).                */ 01318000
*                         /*                                         */ 01319000
*                         /*******************************************/ 01320000
*                                                                  0460 01321000
*                         RFY                                      0460 01322000
*                          (R1,                                    0460 01323000
*                           R0) RSTD;                              0460 01324000
*                         IF RCBBNBFR=YES THEN/* IF RECORD NOT     0461 01325000
*                                      BUFFERED              @G17EP5W*/ 01326000
         L     @14,OLDEST                                          0461 01327000
         TM    RCBBNBFR(@14),B'00001000'                           0461 01328000
         BNO   @RF00461                                            0461 01329000
*                           DO;     /* PROCESS NON-BUFF      @G17EP5W*/ 01330000
*/****************************************************************** */ 01331000
*/*                                                                  */ 01332000
*/*  THIS SEGMENT ATTEMPTS TO BUILD A LOGREC RECORD WITH AN          */ 01333000
*/*  ERRORID APPENDED FOR A NON-BUFFERED RECORD WHICH CONTAINS       */ 01334000
*/*  AN ERRORID.  THE NEW RECORD IS BUILT IN A BUFFER WHICH          */ 01335000
*/*  MUST BE AT LEAST RCBBLEN + LENGTH(RCBBERID) IN LENGTH.          */ 01336000
*/*  IF AN ADEQUATE BUFFER ALREADY EXISTS, THEN IT IS USED.          */ 01337000
*/*  OTHERWISE, IF NO BUFFER EXISTS, OR THE BUFFER IS TOO            */ 01338000
*/*  SMALL, A GETMAIN IS ISSUED FOR THE REQUIRED LENGTH.  ANY        */ 01339000
*/*  SMALLER BUFFER IS FREED.  THE DATA AND ERRORID ARE MOVED        */ 01340000
*/*  TO THE BUFFER.  RCBBLEN IS UPDATED TO THE NEW LENGTH            */ 01341000
*/*  AND NBFRDATA POINTS TO THE NEW AREA.  IF THE GETMAIN            */ 01342000
*/*  FAILED, THEN THE RECORD WILL BE WRITTEN FROM THE ORIGINAL       */ 01343000
*/*  BUFFER WITHOUT AN ERRORID.                                      */ 01344000
*/*                                                                  */ 01345000
*/****************************************************************** */ 01346000
*                                                                  0463 01347000
*                             IF RCBBLREC=YES&/* LOGREC RECORD     0463 01348000
*                                                            @G17EP5W*/ 01349000
*                                 RCBBERFG=YES THEN/* WITH AN ERRORID   01350000
*                                      THEN                  @G17EP5W*/ 01351000
         TM    RCBBLREC(@14),B'01000000'                           0463 01352000
         BNO   @RF00463                                            0463 01353000
         TM    RCBBERFG(@14),B'00000010'                           0463 01354000
         BNO   @RF00463                                            0463 01355000
*                               DO; /* ATTEMPT TO REBUFFER   @G17EP5W*/ 01356000
*                                 RFY                              0465 01357000
*                                   R5 RSTD;/* RESTRICT LENGTH REG.     01358000
*                                                            @G17EP5W*/ 01359000
*                                 R5=RCBBLEN+/* CALCULATE BUFFER   0466 01360000
*                                      SPACE                 @G17EP5W*/ 01361000
*                                     LENGTH(NBFRERID);/* DATA +   0466 01362000
*                                      ERRORID LENGTH        @G17EP5W*/ 01363000
         LA    R5,10                                               0466 01364000
         LH    @14,RCBBLEN(,@14)                                   0466 01365000
         N     @14,@CF02236                                        0466 01366000
         ALR   R5,@14                                              0466 01367000
*                                 IF R5>BLENGTH THEN/* IF A BUFFER OF   01368000
*                                      SUFFICIENT SIZE DOES NOT    0467 01369000
*                                      ALREADY EXIST         @G17EP5W*/ 01370000
         L     @14,BLENGTH                                         0467 01371000
         CR    R5,@14                                              0467 01372000
         BNH   @RF00467                                            0467 01373000
*                                   DO;/* THEN ATTEMPT TO GET ONE  0468 01374000
*                                                            @G17EP5W*/ 01375000
*                                    EXTRTN=YES;/* IF A BUFFER EXISTS   01376000
*                                                            @G17EP5W*/ 01377000
         OI    EXTRTN,B'01000000'                                  0469 01378000
*                                    IF BLENGTH^=NOBUFFER THEN/* FREE   01379000
*                                      IT BEFORE             @G17EP5W*/ 01380000
         LTR   @14,@14                                             0470 01381000
         BZ    @RF00470                                            0470 01382000
*                                     DO;/* GETTING A LARGER ONE   0471 01383000
*                                                            @G17EP5W*/ 01384000
*                                                                  0471 01385000
*                                      /******************************/ 01386000
*                                      /*                            */ 01387000
*                                      /* SHOW EXTERNAL RTN  @G17EP5W*/ 01388000
*                                      /*                            */ 01389000
*                                      /******************************/ 01390000
*                                                                  0472 01391000
*                                      RFY                         0472 01392000
*                                       (R1,                       0472 01393000
*                                        R4) RSTD;/* RESTRICT REG 4     01394000
*                                                            @G17EP5W*/ 01395000
*                                      R1=BUFADR;/* ADDR. TO BE FREED   01396000
*                                                            @G17EP5W*/ 01397000
         L     R1,BUFADR                                           0473 01398000
*                                      R4=BLENGTH;/* LENGTH TO BE  0474 01399000
*                                      FREED                 @G17EP5W*/ 01400000
         LR    R4,@14                                              0474 01401000
*                                      RETRYADR=ADDR(AFTBFREE);/*  0475 01402000
*                                      CURRENT RETRY ADDR    @G17EP5W*/ 01403000
         LA    @14,AFTBFREE                                        0475 01404000
         ST    @14,RETRYADR                                        0475 01405000
*                                      GEN                         0476 01406000
*(FREEMAIN RC,A=(R1),LV=(R4),SP=252 );/* FREE SMALLER BUFFER @G17EP5W*/ 01407000
         FREEMAIN RC,A=(R1),LV=(R4),SP=252                              01408000
*AFTBFREE:                                                         0477 01409000
*                                      RFY                         0477 01410000
*                                       (R1,                       0477 01411000
*                                        R4) UNRSTD;/* FREE REG 4  0477 01412000
*                                                            @G17EP5W*/ 01413000
AFTBFREE DS    0H                                                  0478 01414000
*                                     END;/* END SMALL BUFFER      0478 01415000
*                                                            @G17EP5W*/ 01416000
*                                    RETRYADR=ADDR(AFTBGET);/* SET 0479 01417000
*                                      NEW RETRY ADDR        @G17EP5W*/ 01418000
@RF00470 LA    @14,AFTBGET                                         0479 01419000
         ST    @14,RETRYADR                                        0479 01420000
*                                    BLENGTH=R5;/* SAVE BUFFER LENGTH   01421000
*                                                            @G17EP5W*/ 01422000
         ST    R5,BLENGTH                                          0480 01423000
*                                    GEN REFS(PARMLIST)            0481 01424000
*                                        (STM R8,R7,PARMLIST);/* SAVE   01425000
*                                      REGS IN CASE OF ERROR @G17EP5W*/ 01426000
         STM R8,R7,PARMLIST                                             01427000
*                                    RFY                           0482 01428000
*                                     (R1,                         0482 01429000
*                                      R15) RSTD;/* RESTRICT GETMAIN    01430000
*                                      REGS                  @G17EP5W*/ 01431000
*                                    DO;/* GETMAIN (RC) LV(BLENGTH)     01432000
*                                      SP(252)                       */ 01433000
*                                     RESPECIFY                    0484 01434000
*                                      (GPR01F,                    0484 01435000
*                                       GPR15F,                    0484 01436000
*                                       GPR00F) RESTRICTED;        0484 01437000
*                                     GPR01F=0;/* REG 1 MUST BE ZERO */ 01438000
         SLR   GPR01F,GPR01F                                       0485 01439000
*                                     GPR15F=0;/* RC-TYPE GETMAIN    */ 01440000
         SLR   GPR15F,GPR15F                                       0486 01441000
*                                     GPR00F=BLENGTH;/* LENGTH     0487 01442000
*                                      REQUESTED                     */ 01443000
         L     GPR00F,BLENGTH                                      0487 01444000
*                                     GPR15F=GPR15F|((252)*256);/* SP   01445000
*                                      IN BYTE 2                     */ 01446000
         O     GPR15F,@CF02226                                     0488 01447000
*                                     SVC(120);/* RC/RU-FORM OF    0489 01448000
*                                      GETMAIN                       */ 01449000
         SVC   120                                                 0489 01450000
*                                     RESPECIFY                    0490 01451000
*                                      (GPR01F,                    0490 01452000
*                                       GPR15F,                    0490 01453000
*                                       GPR00F) UNRESTRICTED;      0490 01454000
*                                    END;/* GETMAIN (RC) LV(BLENGTH)    01455000
*                                      SP(252)               @G17EP5W*/ 01456000
*AFTBGET:                                                          0492 01457000
*                                    EXTRTN=NO;/* INDICATE NOT     0492 01458000
*                                      EXTERNAL              @G15EP5W*/ 01459000
AFTBGET  NI    EXTRTN,B'10111111'                                  0492 01460000
*                                    RETRYADR=ADDR(AWAKE);/* CHANGE     01461000
*                                      RETRY ADDR            @G17EP5W*/ 01462000
         LA    @14,AWAKE                                           0493 01463000
         ST    @14,RETRYADR                                        0493 01464000
*                                    GEN REFS(PARMLIST)            0494 01465000
*                                        (STM R8,R7,PARMLIST);/* SAVE   01466000
*                                      REGS IN CASE OF ERROR @G17EP5W*/ 01467000
         STM R8,R7,PARMLIST                                             01468000
*                                    IF R15=ZERO THEN/* IF GETMAIN 0495 01469000
*                                      SUCCESSFUL            @G17EP5W*/ 01470000
         LTR   R15,R15                                             0495 01471000
         BNZ   @RF00495                                            0495 01472000
*                                     BUFADR=R1;/* SAVE BUFFER     0496 01473000
*                                      ADDRESS               @G17EP5W*/ 01474000
         ST    R1,BUFADR                                           0496 01475000
*                                    ELSE/* IF GETMAIN FAILED      0497 01476000
*                                                            @G17EP5W*/ 01477000
*                                     BLENGTH=NOBUFFER;/* SET BUFFER    01478000
*                                      LENGTH TO 0           @G17EP5W*/ 01479000
         B     @RC00495                                            0497 01480000
@RF00495 SLR   @14,@14                                             0497 01481000
         ST    @14,BLENGTH                                         0497 01482000
*                                    RFY                           0498 01483000
*                                     (R1,                         0498 01484000
*                                      R15) UNRSTD;/* FREE UP GETMAIN   01485000
*                                      REGS                  @G17EP5W*/ 01486000
@RC00495 DS    0H                                                  0499 01487000
*                                   END;/* END OF BLENGTH TOO SMALL     01488000
*                                                            @G17EP5W*/ 01489000
*                                 RFY                              0500 01490000
*                                   ERRORID BASED(R4);/* REFERENCE 0500 01491000
*                                      FOR ERRORID           @G17EP5W*/ 01492000
@RF00467 DS    0H                                                  0501 01493000
*                                 IF BLENGTH>=R5 THEN/* IF BUFFER  0501 01494000
*                                      EXISTS                @G17EP5W*/ 01495000
         C     R5,BLENGTH                                          0501 01496000
         BH    @RF00501                                            0501 01497000
*                                   DO;/* MOVE DATA INTO IT  @G17EP5W*/ 01498000
*                                    RFY                           0503 01499000
*                                     (R1,/* RESTRICT        @G17EP5W*/ 01500000
*                                      R4,/* REGISTERS       @G17EP5W*/ 01501000
*                                      R5,/* TO BE           @G17EP5W*/ 01502000
*                                      R6,/* USED IN         @G17EP5W*/ 01503000
*                                      R7) RSTD;/* MOVE      @G17EP5W*/ 01504000
*                                    R1=R7;/* SAVE ADDR. OF 4K BUF 0504 01505000
*                                                            @G17EP5W*/ 01506000
         LR    R1,R7                                               0504 01507000
*                                    R7=RCBBLEN;/* LENGTH TO BE MOVED   01508000
*                                                            @G17EP5W*/ 01509000
         L     @12,OLDEST                                          0505 01510000
         LH    R7,RCBBLEN(,@12)                                    0505 01511000
         N     R7,@CF02236                                         0505 01512000
*                                    R4=BUFADR;/* NEW BUFFER ADDRESS    01513000
*                                                            @G17EP5W*/ 01514000
         L     R4,BUFADR                                           0506 01515000
*                                    R6=NBFRDATA;/* OLD BUFFER     0507 01516000
*                                      ADDRESS               @G17EP5W*/ 01517000
         L     R6,NBFRDATA+16(,@12)                                0507 01518000
*                                    RCBBLEN=R5;/* UPDATE WRITE    0508 01519000
*                                      LENGTH                @G17EP5W*/ 01520000
         STH   R5,RCBBLEN(,@12)                                    0508 01521000
*                                    R5=R7;/* LENGTH TO MOVE IS    0509 01522000
*                                      LENGTH OF ORIGINAL BUFFER   0509 01523000
*                                                            @G17EP5W*/ 01524000
         LR    R5,R7                                               0509 01525000
*                                    MVCL(R4,R6);/* MOVE DATA      0510 01526000
*                                                            @G17EP5W*/ 01527000
         MVCL  R4,R6                                               0510 01528000
*                                    ERRORID=NBFRERID;/* ADD ERRORID    01529000
*                                                            @G17EP5W*/ 01530000
         LA    @12,16                                              0511 01531000
         AL    @12,OLDEST                                          0511 01532000
         MVC   ERRORID(10,R4),NBFRERID(@12)                        0511 01533000
*                                    IF NBFRDATA->LRBHTYPE=/* IF THIS   01534000
*                                      IS LRB                @G17EP5W*/ 01535000
*                                        LRBHMCH THEN              0512 01536000
         L     @12,NBFRDATA(,@12)                                  0512 01537000
         CLI   LRBHTYPE(@12),X'13'                                 0512 01538000
         BNE   @RF00512                                            0512 01539000
*                                     DO;/* THEN FREE IT     @G17EP5W*/ 01540000
*                                      NBFRDATA->LRBMACT=ZERO;/* FOR    01541000
*                                      SUBSEQUENT USE        @G17EP5W*/ 01542000
         MVI   LRBMACT(@12),X'00'                                  0514 01543000
*                                      BUFADR->HDRDS13=YES;/*      0515 01544000
*                                      INDICATE ERRORID APPENDED   0515 01545000
*                                                            @G17EP5W*/ 01546000
         L     @12,BUFADR                                          0515 01547000
         OI    HDRDS13(@12),B'00010000'                            0515 01548000
*                                     END;/* END LRB RECORD  @G17EP5W*/ 01549000
*                                    NBFRDATA=BUFADR;/* UPDATE WRITE    01550000
*                                      ADDRESS               @G17EP5W*/ 01551000
@RF00512 L     @12,BUFADR                                          0517 01552000
         L     @15,OLDEST                                          0517 01553000
         ST    @12,NBFRDATA+16(,@15)                               0517 01554000
*                                    R7=R1;/* RESTORE 4K BUF ADDR  0518 01555000
*                                                            @G17EP5W*/ 01556000
         LR    R7,R1                                               0518 01557000
*                                    RFY                           0519 01558000
*                                     (R1,/* UNRESTRICT      @G17EP5W*/ 01559000
*                                      R4,/* REGISTERS       @G17EP5W*/ 01560000
*                                      R5,/* USED IN         @G17EP5W*/ 01561000
*                                      R6);/* MOVE           @G17EP5W*/ 01562000
*                                   END;/* END MOVE DATA     @G17EP5W*/ 01563000
*                               END;/* END REBUFFER ATTEMPT  @G17EP5W*/ 01564000
@RF00501 DS    0H                                                  0522 01565000
*                             R1=NBFRDATA;/* ... TAKE USERS ADDRESS  */ 01566000
@RF00463 L     @12,OLDEST                                          0522 01567000
         L     R1,NBFRDATA+16(,@12)                                0522 01568000
*                           END;    /* END NONBUFFERED RECORDS     0523 01569000
*                                                            @G17EP5W*/ 01570000
*                         ELSE      /* IF DATA BUFFERED              */ 01571000
*                           R1=ADDR(RCBBDATA);/* USE OUR BUFFERED  0524 01572000
*                                      ADDRESS.                      */ 01573000
*                                                                  0524 01574000
         B     @RC00461                                            0524 01575000
@RF00461 L     R1,OLDEST                                           0524 01576000
         LA    R1,RCBBDATA(,R1)                                    0524 01577000
*                         /*******************************************/ 01578000
*                         /*                                         */ 01579000
*                         /* CHECK FOR OUTPUT DESTINATION.           */ 01580000
*                         /*                                         */ 01581000
*                         /*******************************************/ 01582000
*                                                                  0525 01583000
*                         IF RCBBLREC=NO&/* WTO OR LOGREC...         */ 01584000
*                             RCBBWTO=YES THEN                     0525 01585000
@RC00461 L     @12,OLDEST                                          0525 01586000
         TM    RCBBWTO(@12),B'00100000'                            0525 01587000
         BNO   @RF00525                                            0525 01588000
         TM    RCBBLREC(@12),B'01000000'                           0525 01589000
         BNZ   @RF00525                                            0525 01590000
*                           DO;     /* -WTO.                         */ 01591000
*                             EXTRTN=YES;/* SHOW IN EXTERNAL ROUTINE    01592000
*                                      FOR RECOVERY                  */ 01593000
         OI    EXTRTN,B'01000000'                                  0527 01594000
*                             RETRYADR=ADDR(AFTWTO);/* PRESET RETRY     01595000
*                                      ADDR                          */ 01596000
         LA    @12,AFTWTO                                          0528 01597000
         ST    @12,RETRYADR                                        0528 01598000
*                             GEN SETS(PARMLIST)(STM R8,R7,PARMLIST);   01599000
         STM R8,R7,PARMLIST                                             01600000
*                                   /*                       @YM02750*/ 01601000
*                             GEN(WTO  MF=(E,(1)));/* PUT DATA TO  0530 01602000
*                                      OPERATOR.                     */ 01603000
         WTO  MF=(E,(1))                                                01604000
*AFTWTO:                                                           0531 01605000
*                             ;     /* RETRY IF WTO FAILURE          */ 01606000
AFTWTO   DS    0H                                                  0532 01607000
*                             EXTRTN=NO;/* BACK TO RECORD            */ 01608000
         NI    EXTRTN,B'10111111'                                  0532 01609000
*                             RETRYADR=ADDR(AWAKE);/* RESET RETRY  0533 01610000
*                                      ADDR                          */ 01611000
         LA    @12,AWAKE                                           0533 01612000
         ST    @12,RETRYADR                                        0533 01613000
*                           END;                                   0534 01614000
*                         ELSE      /* -LOGREC.                      */ 01615000
*                           DO;                                    0535 01616000
         B     @RC00525                                            0535 01617000
@RF00525 DS    0H                                                  0536 01618000
*                             R0=-RCBBLEN;/* TWOS COMPLEMENT LENGTH  */ 01619000
         L     @12,OLDEST                                          0536 01620000
         LH    R0,RCBBLEN(,@12)                                    0536 01621000
         N     R0,@CF02236                                         0536 01622000
         LCR   R0,R0                                               0536 01623000
*                             RFY                                  0537 01624000
*                               LRB BASED(R1);                     0537 01625000
*                             IF LRBHTYPE=LRBHMCH&/* MCH RECORD    0538 01626000
*                                      OVERLAID...                   */ 01627000
*                                 LRBMCLB='FF'X THEN/* -YES, UP LOST    01628000
*                                      RCD COUNT AND DONT OUTPUT RCD.*/ 01629000
         CLI   LRBHTYPE(R1),X'13'                                  0538 01630000
         BNE   @RF00538                                            0538 01631000
         CLI   LRBMCLB(R1),X'FF'                                   0538 01632000
         BNE   @RF00538                                            0538 01633000
*/********************************************************************/ 01634000
*/* THE PURPOSE OF THE INCLCNT SEGMENT IS TO INCREMENT THE LOST      */ 01635000
*/* RECORD COUNT FOR NOT SCHEDULED LOGREC REQUESTS                   */ 01636000
*/********************************************************************/ 01637000
*                                                                  0539 01638000
*                               DO;                                0539 01639000
*                                 RESPECIFY/* RESTRICT REGISTERS FOR */ 01640000
*                                  (R4,/* SAVING LOST RECORD COUNT   */ 01641000
*                                   R5) RESTRICTED;/* INCREMENTING 0540 01642000
*                                      LOST REC CT                   */ 01643000
*                                 R4=RCBCS1;/* SAVE CURRENT LOST   0541 01644000
*                                      RECORD COUNT                  */ 01645000
         L     R4,RCBCS1(,R7)                                      0541 01646000
*TRYCS2:                            /* RCBLCNT WILL BE INCREMENTED 0542 01647000
*                                      VIA CS EFFECTING SERIALIZATION*/ 01648000
*                                 R5=R4;/* MAKE COUNT AVAILABLE FOR     01649000
*                                      INCREMENTING                  */ 01650000
TRYCS2   LR    R5,R4                                               0542 01651000
*                                 R5=R5+HIBYTE1;/* INCREMENT BY ONE  */ 01652000
         AL    R5,HIBYTE1                                          0543 01653000
*                                 CS(R4,R5,RCBCS1);/* PLACE UPDATED     01654000
*                                      COUNT IN THE RCB              */ 01655000
         CS    R4,@05,RCBCS1(R7)                                   0544 01656000
*                                 BC(4,TRYCS2);/* IF THE RCB NOT   0545 01657000
*                                      ALTERED                       */ 01658000
         BC    4,TRYCS2                                            0545 01659000
*                                 RESPECIFY/* RELEASE REGISTERS FROM */ 01660000
*                                  (R4,/* SAVING LOST RECORD COUNT   */ 01661000
*                                   R5) UNRESTRICTED;/* INCREMENTING    01662000
*                                      LOST RECORD COUNT             */ 01663000
*                               END;/* END INCLCNT                   */ 01664000
*                             ELSE  /* IF NOT MCH & OVERLAID         */ 01665000
*                               DO; /* PUT RECORD TO SYS1.LOGREC.    */ 01666000
         B     @RC00538                                            0548 01667000
@RF00538 DS    0H                                                  0549 01668000
*                                 DATAPTRS=R1;/* PRESERVE DATA     0549 01669000
*                                      ADDRESS.                      */ 01670000
         ST    R1,DATAPTRS                                         0549 01671000
*                                 EXTRTN=YES;/* SHOW IN EXTERNAL   0550 01672000
*                                      ROUTINE FOR RECOVERY          */ 01673000
         OI    EXTRTN,B'01000000'                                  0550 01674000
*                                 RETRYADR=ADDR(AFT76A);/* PRESET  0551 01675000
*                                      RETRY ADDR                    */ 01676000
         LA    @12,AFT76A                                          0551 01677000
         ST    @12,RETRYADR                                        0551 01678000
*                                 GEN SETS(PARMLIST)               0552 01679000
*                                     (STM R8,R7,PARMLIST);/*      0552 01680000
*                                                            @YM02750*/ 01681000
         STM R8,R7,PARMLIST                                             01682000
*                                 SVC(76);/* WRITE TO SYS1.LOGREC    */ 01683000
         SVC   76                                                  0553 01684000
*AFT76A:                                                           0554 01685000
*                                 ; /* RETRY IF SVC76 FAILURE        */ 01686000
AFT76A   DS    0H                                                  0555 01687000
*                                 EXTRTN=NO;/* BACK TO RECORD        */ 01688000
         NI    EXTRTN,B'10111111'                                  0555 01689000
*                                 RETRYADR=ADDR(AWAKE);/* RESET RETRY   01690000
*                                      ADDR                          */ 01691000
         LA    @12,AWAKE                                           0556 01692000
         ST    @12,RETRYADR                                        0556 01693000
*                                 R1=DATAPTRS;/* REGAIN DATA ADDRESS.*/ 01694000
         L     R1,DATAPTRS                                         0557 01695000
*                               END;                               0558 01696000
*                             IF LRBHTYPE=LRBHMCH THEN/* MCH       0559 01697000
*                                      RECORD...                     */ 01698000
@RC00538 CLI   LRBHTYPE(R1),X'13'                                  0559 01699000
         BNE   @RF00559                                            0559 01700000
*                               LRBMACT=ZERO;/* -YES, SHOW BUFFER  0560 01701000
*                                      FREE.                         */ 01702000
         MVI   LRBMACT(R1),X'00'                                   0560 01703000
*                             RFY                                  0561 01704000
*                               LRB BASED;                         0561 01705000
@RF00559 DS    0H                                                  0562 01706000
*                             RFY                                  0562 01707000
*                              (R1,                                0562 01708000
*                               R0) UNRSTD;                        0562 01709000
*                           END;    /* END OF LOGREC OUTPUT.         */ 01710000
*                       END;                                       0564 01711000
*                                                                  0564 01712000
@RC00525 DS    0H                                                  0565 01713000
*                     /***********************************************/ 01714000
*                     /*                                             */ 01715000
*                     /* THIS SEGMENT WILL POST THE USERS ECB TO     */ 01716000
*                     /* INDICATE THAT THE DATA HAS BEEN WRITTEN.    */ 01717000
*                     /* POSTING WILL NOT BE DONE IF IT WAS NOT      */ 01718000
*                     /* REQUESTED OR IF THE ASID IS INVALID.        */ 01719000
*                     /*                                             */ 01720000
*                     /***********************************************/ 01721000
*                                                                  0565 01722000
*                     RFY                                          0565 01723000
*                       ASVT BASED(CVTASVT);                       0565 01724000
@RC00446 DS    0H                                                  0566 01725000
*                     IF RCBBPOST=YES&/* WANT POSTING...             */ 01726000
*                         RCBBNBFR=YES THEN/* RECORD UNBUFFED..    0566 01727000
*                                                            @YM02617*/ 01728000
         L     @12,OLDEST                                          0566 01729000
         TM    RCBBPOST(@12),B'00001100'                           0566 01730000
         BNO   @RF00566                                            0566 01731000
*                       DO;         /* -YES.                         */ 01732000
*                         IF RCBBASID>ZERO&/* VALID NUMERICAL ASID...*/ 01733000
*                             RCBBASID<=ASVTMAXU&ASVTAVAL(RCBBASID)=NO  01734000
*                           THEN    /* ...AND ASID IN USE...         */ 01735000
         LH    @06,RCBBASID(,@12)                                  0568 01736000
         N     @06,@CF02236                                        0568 01737000
         LTR   @06,@06                                             0568 01738000
         BNP   @RF00568                                            0568 01739000
         L     @01,CVTPTR                                          0568 01740000
         L     @15,CVTASVT(,@01)                                   0568 01741000
         C     @06,ASVTMAXU(,@15)                                  0568 01742000
         BH    @RF00568                                            0568 01743000
         SLA   @06,2                                               0568 01744000
         ALR   @15,@06                                             0568 01745000
         TM    ASVTAVAL-4(@15),B'10000000'                         0568 01746000
         BNZ   @RF00568                                            0568 01747000
*                           DO;     /* YES,POST THE ECB.             */ 01748000
*                             RFY                                  0570 01749000
*                               R5 RSTD;/* PTR TO THE ECB TO POST TO    01750000
*                                      HANDLE WRAP HEADERS   @ZA19199*/ 01751000
*                             R5=ADDR(RCBBECB);/* ADDR OF ECB TO POST   01752000
*                                                            @ZA19199*/ 01753000
         LA    R5,RCBBECB(,@12)                                    0571 01754000
*                             RFY                                  0572 01755000
*                               USERECB BASED(ECBADR);/* TO INSURE 0572 01756000
*                                      PROPER ADDRESS OF ECB @ZA19199*/ 01757000
*                             IF R5>=RCBBUFE THEN/* DOES THE HDR WRAP   01758000
*                                                            @ZA19199*/ 01759000
         C     R5,RCBBUFE(,R7)                                     0573 01760000
         BL    @RF00573                                            0573 01761000
*                               R5=R5-RCBTLNG;/* YES - RESET POINTER    01762000
*                                                            @ZA19199*/ 01763000
         LH    @12,RCBTLNG(,R7)                                    0574 01764000
         N     @12,@CF02236                                        0574 01765000
         SLR   R5,@12                                              0574 01766000
*                             ELSE                                 0575 01767000
*                               ;   /* HDR DOES NOT WRAP     @ZA19199*/ 01768000
@RF00573 DS    0H                                                  0576 01769000
*                             EXTRTN=YES;/* SHOW IN EXTERNAL ROUTINE    01770000
*                                      FOR RECOVERY                  */ 01771000
         OI    EXTRTN,B'01000000'                                  0576 01772000
*                             RETRYADR=ADDR(AFTPSTB);/* PRESET RETRY    01773000
*                                      ADDR                          */ 01774000
         LA    @12,AFTPSTB                                         0577 01775000
         ST    @12,RETRYADR                                        0577 01776000
*                             GEN SETS(PARMLIST)(STM R8,R7,PARMLIST);   01777000
         STM R8,R7,PARMLIST                                             01778000
*                                   /*                       @YM02750*/ 01779000
*                             DO;   /* POST (USERECB,0) ASCB(ASVTENTY   01780000
*                                      (RCBBASID)->UASCBADR)       0579 01781000
*                                      ERRET(CVTBRET)                */ 01782000
*                               I002=I002&&I002;/* INITIALIZE LIST   */ 01783000
         XC    I002(12),I002                                       0580 01784000
*                               I00201=ADDR(USERECB);/* ECB ADDRESS  */ 01785000
         L     @12,ECBADR(,R5)                                     0581 01786000
         ST    @12,I00201                                          0581 01787000
*                               I00202=ADDR(ASVTENTY(RCBBASID)->   0582 01788000
*                                   UASCBADR);/* ASCB ADDRESS        */ 01789000
         L     @12,OLDEST                                          0582 01790000
         LH    @12,RCBBASID(,@12)                                  0582 01791000
         N     @12,@CF02236                                        0582 01792000
         SLA   @12,2                                               0582 01793000
         L     @06,CVTPTR                                          0582 01794000
         L     @04,CVTASVT(,@06)                                   0582 01795000
         L     @12,ASVTENTY-4(@12,@04)                             0582 01796000
         ST    @12,I00202                                          0582 01797000
*                               I00203=ADDR(CVTBRET);/* ERRET ADDRESS*/ 01798000
         LA    @12,CVTBRET(,@06)                                   0583 01799000
         ST    @12,I00203                                          0583 01800000
*                               RESPECIFY                          0584 01801000
*                                (GPR00F,                          0584 01802000
*                                 GPR01P) RESTRICTED;              0584 01803000
*                               GPR00F=0;/* POST CODE IN R0          */ 01804000
         SLR   GPR00F,GPR00F                                       0585 01805000
*                               GPR01P=ADDR(I002);/* PARMLIST ADDR   */ 01806000
         LA    GPR01P,I002                                         0586 01807000
*                               GPR01P=GPR01P|'80000000'X;/* SET LIST   01808000
*                                      FORM IND                      */ 01809000
         O     GPR01P,@CF00108                                     0587 01810000
*                               SVC(2);/* ISSUE POST SVC             */ 01811000
         SVC   2                                                   0588 01812000
*                               RESPECIFY                          0589 01813000
*                                (GPR00F,                          0589 01814000
*                                 GPR01P) UNRESTRICTED;            0589 01815000
*                             END;  /* POST (USERECB,0) ASCB(ASVTENTY   01816000
*                                      (RCBBASID)->UASCBADR)       0590 01817000
*                                      ERRET(CVTBRET)                */ 01818000
*AFTPSTB:                                                          0591 01819000
*                             ;     /* RETRY IF POST FAILURE         */ 01820000
AFTPSTB  DS    0H                                                  0592 01821000
*                             RFY                                  0592 01822000
*                               USERECB BASED(RCBBECB);/* REBASE BACK   01823000
*                                                            @ZA19199*/ 01824000
*                             RFY                                  0593 01825000
*                               R5 UNRSTD;/* FREE THE ECB POINTER  0593 01826000
*                                                            @ZA19199*/ 01827000
*                             EXTRTN=NO;/* BACK TO RECORD            */ 01828000
         NI    EXTRTN,B'10111111'                                  0594 01829000
*                             RETRYADR=ADDR(AWAKE);/* RESET RETRY  0595 01830000
*                                      ADDR                          */ 01831000
         LA    @12,AWAKE                                           0595 01832000
         ST    @12,RETRYADR                                        0595 01833000
*                           END;                                   0596 01834000
*                       END;                                       0597 01835000
@RF00568 DS    0H                                                  0598 01836000
*                     RFY                                          0598 01837000
*                       ASVT BASED;                                0598 01838000
*                                                                  0598 01839000
@RF00566 DS    0H                                                  0599 01840000
*/********************************************************************/ 01841000
*/*                                                                  */ 01842000
*/* THE FREEREC SEGMENT IS USED TO FREE RECORDS EITHER IN THE RCB    */ 01843000
*/* WHEN THE RECORDS ARE MOVED TO THE GETMAINED BUFFER, OR THE RCB/  */ 01844000
*/* GETMAINED BUFFER AFTER WRITE TO OPERATOR OR LOGREC               */ 01845000
*/*                                                                  */ 01846000
*/********************************************************************/ 01847000
*                                                                  0599 01848000
*                     DO;                                          0599 01849000
*                       RESPECIFY   /* RESTRICT REGISTERS FOR        */ 01850000
*                        (R2,       /* SAVING FREE POINTER           */ 01851000
*                         R3,       /* SAVING FREE LEN & DUMMY DISP  */ 01852000
*                         R4,       /* UPDATING FREE POINTER         */ 01853000
*                         R5,       /* UPDATING DUMMY DISPLACEMENT   */ 01854000
*                         R6) RESTRICTED;/* UPDATING FREE LENGTH     */ 01855000
*                       RESPECIFY   /* RESTRICT REGISTER FOR         */ 01856000
*                        (R1) RESTRICTED;/* ADDRESS OF FIELD FOR LOAD*/ 01857000
*                       R1=ADDR(RCBCDS);/* OBTAIN ADDRESS OF       0602 01858000
*                                      RCBFLNG,RCBFREE AND RCBDUM    */ 01859000
         LA    R1,RCBCDS(,R7)                                      0602 01860000
*                       GEN(LM R2,R3,ZIP(R1));/* SAVE FOR SUBSEQUENT    01861000
*                                      CDS                           */ 01862000
         LM R2,R3,ZIP(R1)                                               01863000
*                       RESPECIFY   /* RELEASE ADDRESS OF FIELD FOR  */ 01864000
*                        (R1) UNRESTRICTED;/* LOAD                   */ 01865000
*TRYCDS1:                           /* RETRY POINT IF CDS FAILS      */ 01866000
*                       R4=R2;      /* FREE POINTER AVAILABLE TO   0605 01867000
*                                      UPDATE                        */ 01868000
TRYCDS1  LR    R4,R2                                               0605 01869000
*                       R5=R3;      /* FREE LENGTH & DUMMY DISP FOR  */ 01870000
         LR    R5,R3                                               0606 01871000
*                       R6=R5;      /* UPDATING IN UNIQUE REGISTERS  */ 01872000
         LR    R6,R5                                               0607 01873000
*                       GENERATE;                                  0608 01874000
         SLL   R5,HALFREG         /* ZERO HIGH ORDER TWO BYTES OF    */ 01875000
         SRL   R5,HALFREG         /* DUMMY DISPLACEMENT              */ 01876000
         SRL   R6,HALFREG         /* MOVE FREE LEN TO LOW HALF OF REG*/ 01877000
*                       IF RCBBNBFR=NO THEN/* IF THE RECORD WAS    0609 01878000
*                                      BUFFERED                      */ 01879000
         L     @12,OLDEST                                          0609 01880000
         TM    RCBBNBFR(@12),B'00001000'                           0609 01881000
         BNZ   @RF00609                                            0609 01882000
*                         R6=R6+(RCBBLEN+/* FREE THE DATA            */ 01883000
*                             CONTLEN+/* THE CONTROL INFO            */ 01884000
*                             SEVEN&/* ROUNDED TO A DOUBLE WORD      */ 01885000
*                             'FFF8'X);                            0610 01886000
         LA    @15,23                                              0610 01887000
         LH    @12,RCBBLEN(,@12)                                   0610 01888000
         N     @12,@CF02236                                        0610 01889000
         ALR   @15,@12                                             0610 01890000
         N     @15,@CF02103                                        0610 01891000
         ALR   R6,@15                                              0610 01892000
*                       ELSE                                       0611 01893000
*                         DO;       /* IF RECORD WAS NOT BUFFERED    */ 01894000
         B     @RC00609                                            0611 01895000
@RF00609 DS    0H                                                  0612 01896000
*                           RFY                                    0612 01897000
*                             R1 RSTD;/* RESTRICT REGISTER FOR     0612 01898000
*                                      ERRORID LENGTH        @G17EP5W*/ 01899000
*                           IF RCBBERFG=YES THEN/* IF ERRORID      0613 01900000
*                                      APPENDED IN RCB       @G17EP5W*/ 01901000
         L     @12,OLDEST                                          0613 01902000
         TM    RCBBERFG(@12),B'00000010'                           0613 01903000
         BNO   @RF00613                                            0613 01904000
*                             R1=LENGTH(NBFRERID);/* THEN INCLUDE ITS   01905000
*                                      LENGTH FOR FREEING    @G17EP5W*/ 01906000
         LA    R1,10                                               0614 01907000
*                           ELSE                                   0615 01908000
*                             R1=ZERO;/* OTHERWISE FREE NO ADDITIONAL   01909000
*                                      SPACE                 @G17EP5W*/ 01910000
         B     @RC00613                                            0615 01911000
@RF00613 SLR   R1,R1                                               0615 01912000
*                           R6=R6+(NBFRLEN+/* FREE (16 CONTROL INFO +   01913000
*                                      4 ADDR)                       */ 01914000
*                               R1+ /* LENGTH OF ERRORID IF PRESENT     01915000
*                                                            @G17EP5W*/ 01916000
*                               SEVEN&/* ROUNDED TO DOUBLE WORD      */ 01917000
*                               'FFF8'X);                          0616 01918000
@RC00613 LA    @12,27                                              0616 01919000
         ALR   @12,R1                                              0616 01920000
         N     @12,@CF02103                                        0616 01921000
         ALR   R6,@12                                              0616 01922000
*                           RFY                                    0617 01923000
*                             R1 UNRSTD;/* UNRESTRICT LENGTH REGISTER   01924000
*                                                            @G17EP5W*/ 01925000
*                         END;      /*                       @G17EP5W*/ 01926000
*                       GEN(SLL R6,HALFREG);/* MOVE NEW LENGTH TO  0619 01927000
*                                      HIGH HALF OF REGISTER         */ 01928000
@RC00609 DS    0H                                                  0619 01929000
         SLL R6,HALFREG                                                 01930000
*                       R5=R5|R6;   /* PUT FREE LENGTH & DUMMY DISP     01931000
*                                      IN SAME REGISTER              */ 01932000
         OR    R5,R6                                               0620 01933000
*                       CDS(R2,R4,RCBCDS);/* PUT VALUES IN RCB       */ 01934000
         CDS   R2,R4,RCBCDS(R7)                                    0621 01935000
*                       BC(4,TRYCDS1);/* IF RCB NOT ALTERED          */ 01936000
         BC    4,TRYCDS1                                           0622 01937000
*                       RESPECIFY   /* FREE REGISTERS FROM           */ 01938000
*                        (R2,       /* SAVING FREE POINTER           */ 01939000
*                         R3,       /* SAVING FREE LEN & DUMMY DISP  */ 01940000
*                         R4,       /* UPDATING FREE POINTER         */ 01941000
*                         R5,       /* UPDATING DUMMY DISPLACEMENT   */ 01942000
*                         R6) UNRESTRICTED;/* UPDATING FREE LENGTH   */ 01943000
*                     END;                                         0624 01944000
*                   END;            /* END OF RECORD-IS-READY.       */ 01945000
*               END;                /* END OF RECORDS-STILL- REMAIN. */ 01946000
@RC00426 DS    0H                                                  0627 01947000
*           END;                    /* END OF DO-WHILE LOOP.         */ 01948000
@RC00422 DS    0H                                                  0627 01949000
@DE00418 TM    LOOP,B'10000000'                                    0627 01950000
         BO    @DL00418                                            0627 01951000
*           IF REBUFADR^=ZERO THEN  /* FIRST BUFFER EXIST?   @ZA25858*/ 01952000
         L     @12,REBUFADR                                        0628 01953000
         LTR   @12,@12                                             0628 01954000
         BZ    @RF00628                                            0628 01955000
*             DO;                   /* YES, FREE IT          @ZA25858*/ 01956000
*               R0=SP252|(REBLEN&HIBYTOFF);/* LENGTH AND SP  @ZA25858*/ 01957000
         L     R0,REBLEN                                           0630 01958000
         ICM   @06,15,HIBYTOFF                                     0630 01959000
         NR    R0,@06                                              0630 01960000
         ICM   @06,15,SP252                                        0630 01961000
         OR    R0,@06                                              0630 01962000
*               R1=REBUFADR;        /* ADDRESS               @ZA25858*/ 01963000
         LR    R1,@12                                              0631 01964000
*               GEN REFS(PARMLIST)(STM R8,R7,PARMLIST);/*    @ZA25858*/ 01965000
         STM R8,R7,PARMLIST                                             01966000
*               EXTRTN=YES;         /* RECURSION             @ZA25858*/ 01967000
         OI    EXTRTN,B'01000000'                                  0633 01968000
*               RETRYADR=ADDR(AFTFREE1);/* CONTROLS          @ZA25858*/ 01969000
         LA    @12,AFTFREE1                                        0634 01970000
         ST    @12,RETRYADR                                        0634 01971000
*               GEN(FREEMAIN R,LV=(0),A=(1));/*              @ZA25858*/ 01972000
         FREEMAIN R,LV=(0),A=(1)                                        01973000
*AFTFREE1:                          /*                       @ZA25858*/ 01974000
*               EXTRTN=NO;          /* RECURSION /*          @ZA25858*/ 01975000
AFTFREE1 NI    EXTRTN,B'10111111'                                  0636 01976000
*               RETRYADR=ADDR(AWAKE);/* CONTROLS /*          @ZA25858*/ 01977000
         LA    @12,AWAKE                                           0637 01978000
         ST    @12,RETRYADR                                        0637 01979000
*             END;                  /* 1ST BUFFER FREE       @ZA25858*/ 01980000
*           ELSE                                                   0639 01981000
*             ;                     /*                       @ZA25858*/ 01982000
@RF00628 DS    0H                                                  0640 01983000
*           IF BUFADR^=ZERO THEN    /* SECOND BUFFER EXIST?  @ZA25858*/ 01984000
         L     @12,BUFADR                                          0640 01985000
         LTR   @12,@12                                             0640 01986000
         BZ    @RF00640                                            0640 01987000
*             DO;                   /* YES, FREE IT          @ZA25858*/ 01988000
*               R0=SP252|(BLENGTH&HIBYTOFF);/* LENGTH AND SP @ZA25858*/ 01989000
         L     R0,BLENGTH                                          0642 01990000
         ICM   @06,15,HIBYTOFF                                     0642 01991000
         NR    R0,@06                                              0642 01992000
         ICM   @06,15,SP252                                        0642 01993000
