         TITLE '          IEAVTRET RECORDING TASK                      *00001000
                        '                                               00002000
IEAVTRET CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVTRET  77.334'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @08,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@08                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @09,@01                                             0001 00016000
         USING @DATD,@09                                           0001 00017000
         LM    @00,@01,20(@13)                                     0001 00018000
HALFREG EQU   16                   /* BITS TO SHIFT FOR HALFREG      */ 00019000
ZIP     EQU   0                    /* ZERO USED IN GEND CODE         */ 00020000
*                                                                  0104 00021000
*   /*****************************************************************/ 00022000
*   /*                                                               */ 00023000
*   /* THIS SECTION OF CODE IS EXECUTED ONLY ONCE DURING THE LIFE OF */ 00024000
*   /* THE RECORDING TASK. IT S PURPOSE IS TO INITIALIZE THE SRB USED*/ 00025000
*   /* BY THE NUCLEUS ROUTINE AND TO ESTABLISH ESTAE PROTECTION FOR  */ 00026000
*   /* THE RECORDING TASK. IF INITIALIZATUIN IS SUCCESSFUL, THE ECB  */ 00027000
*   /* PASSED BY IEAVTMSI ON THE ATTACH IS POSTED WITH A CODE OF     */ 00028000
*   /* ZERO. ANY FAILURE TO INITIALIZE (BAD ESTAE RETURN CODE, ENTRY */ 00029000
*   /* TO ESTAE EXIT DURING INITIALIZATION, ABEND BEFORE ESTAE       */ 00030000
*   /* ESTABLISHED) WILL CAUSE THE ECB TO BE POSTED WITH A NON-ZERO  */ 00031000
*   /* RETURN CODE.                                                  */ 00032000
*   /*                                                               */ 00033000
*   /*****************************************************************/ 00034000
*                                                                  0105 00035000
*   PARMLIST=PARMLIST&&PARMLIST;    /* INIT RECOVERY LIST            */ 00036000
         XC    PARMLIST(72),PARMLIST                               0105 00037000
*   RFY                                                            0106 00038000
*    (R1,                                                          0106 00039000
*     R5) RSTD;                                                    0106 00040000
*   R5=R1;                          /* SAVE INPUT ECB ADDR.          */ 00041000
         LR    R5,R1                                               0107 00042000
*   RFY                                                            0108 00043000
*    (R1) UNRESTRICTED;                                            0108 00044000
*   SVC60LST=MFLESTAE;              /* LIST FORM OF ESTAE            */ 00045000
         MVC   SVC60LST(16),MFLESTAE                               0109 00046000
*   RFY                                                            0110 00047000
*    (R1) RSTD;                     /* RESTRICT REG FOR ESTAE PTR    */ 00048000
*   R1=ADDR(SVC60LST);              /* SET FOR MF = E FORM OF ESTAE  */ 00049000
         LA    R1,SVC60LST                                         0111 00050000
*   GEN(ESTAE PARAM=PARMLIST,MF=(E,(1)));/* GET ESTAE EXIT           */ 00051000
         ESTAE PARAM=PARMLIST,MF=(E,(1))                                00052000
*   RFY                                                            0113 00053000
*    (R1) UNRSTD;                                                  0113 00054000
*                                                                  0113 00055000
*   /*****************************************************************/ 00056000
*   /*                                                               */ 00057000
*   /* IF ESTAE REQUEST FAILED INFORM IEAVTMSI                       */ 00058000
*   /*                                                               */ 00059000
*   /*****************************************************************/ 00060000
*                                                                  0114 00061000
*   GEN SETS(PARMLIST)(STM R8,R7,PARMLIST);/* SAVE FOR EXIT USE      */ 00062000
         STM R8,R7,PARMLIST                                             00063000
*   RETRYADR=ADDR(BADINIT);         /* SET FIRST RETRY POINT         */ 00064000
         LA    @14,BADINIT                                         0115 00065000
         ST    @14,RETRYADR                                        0115 00066000
*   IF R15^=ZERO THEN               /* ESTAE ESTABLISHED...          */ 00067000
         LTR   R15,R15                                             0116 00068000
         BZ    @RF00116                                            0116 00069000
*     DO;                           /* NO,ABORT INITIALIZATION       */ 00070000
*BADINIT:                           /* RETRY LABEL FOR ESTAE         */ 00071000
*       CVTRTMCT->RTCTRPER=YES;                                    0118 00072000
BADINIT  L     @14,CVTPTR                                          0118 00073000
         L     @14,CVTRTMCT(,@14)                                  0118 00074000
         OI    RTCTRPER(@14),B'01000000'                           0118 00075000
*       GEN(POST (R5),4);           /* SHOW INIT FAILURE.            */ 00076000
         POST (R5),4                                                    00077000
*     END;                                                         0120 00078000
*   SVC51LST=MFLSDUMP;              /* MOVE SDUMP PARM LIST TO     0121 00079000
*                                      AUTOMATIC STORAGE     @ZA13432*/ 00080000
*                                                                  0121 00081000
@RF00116 MVC   SVC51LST(24),MFLSDUMP                               0121 00082000
*   /*****************************************************************/ 00083000
*   /*                                                               */ 00084000
*   /* INITIALIZE THE SRB USED BY NUCLEUS RECORDING ROUTINE          */ 00085000
*   /*                                                               */ 00086000
*   /*****************************************************************/ 00087000
*                                                                  0122 00088000
*   RFY                                                            0122 00089000
*     R2 RSTD;                                                     0122 00090000
*   R2=ADDR(RCBSRB);                                               0123 00091000
         L     @14,CVTPTR                                          0123 00092000
         L     @14,CVTRTMCT(,@14)                                  0123 00093000
         L     @14,RTCTRCB(,@14)                                   0123 00094000
         LA    R2,RCBSRB(,@14)                                     0123 00095000
*   SRBPKF=ZERO;                    /* KEY 0 SRB                     */ 00096000
         MVI   SRBPKF(R2),X'00'                                    0124 00097000
*   SRBRMTR=ZERO;                   /* NO RESOURCE MANAGER.          */ 00098000
         SLR   @12,@12                                             0125 00099000
         ST    @12,SRBRMTR(,R2)                                    0125 00100000
*   SRBSAVE=ZERO;                   /* NO SAVE AREA.                 */ 00101000
         ST    @12,SRBSAVE(,R2)                                    0126 00102000
*   SRBCPAFF=ZERO;                  /* RUN ON ANY CPU.               */ 00103000
         STH   @12,SRBCPAFF(,R2)                                   0127 00104000
*   SRBFLNK=ZERO;                   /* NO FORWARD CHAIN PTR.         */ 00105000
         ST    @12,SRBFLNK(,R2)                                    0128 00106000
*   SRBPRIOR=ZERO;                  /* PRIORITY ZERO.                */ 00107000
         MVI   SRBPRIOR(R2),X'00'                                  0129 00108000
*   SRBPARM=R9;                     /* PASS DATA REG TO SRB RTN      */ 00109000
         ST    R9,SRBPARM(,R2)                                     0130 00110000
*   SRBID='SRB ';                   /* EBCDIC ID.                    */ 00111000
         MVC   SRBID(4,R2),@CC02080                                0131 00112000
*   SRBASCB=PSAAOLD;                /* MASTERS ASCB ADDRESS          */ 00113000
         L     @06,PSAAOLD                                         0132 00114000
         ST    @06,SRBASCB(,R2)                                    0132 00115000
*   SRBPASID=ZERO;                  /* NO TASK AFFINITY              */ 00116000
         STH   @12,SRBPASID(,R2)                                   0133 00117000
*   SRBPTCB=ZERO;                   /* NO TASK AFFINITY              */ 00118000
         ST    @12,SRBPTCB(,R2)                                    0134 00119000
*   SRBEP=ADDR(RETSRBEP);           /* SRBS ENTRY POINT.             */ 00120000
         LA    @12,RETSRBEP                                        0135 00121000
         ST    @12,SRBEP(,R2)                                      0135 00122000
*   RFY                                                            0136 00123000
*     R2 UNRSTD;                                                   0136 00124000
*   RCBSRBIU=NO;                    /* SHOW SRB CAN BE USED.         */ 00125000
*                                                                  0137 00126000
         NI    RCBSRBIU(@14),B'01111111'                           0137 00127000
*   /*****************************************************************/ 00128000
*   /*                                                               */ 00129000
*   /* SHOW GOOD INITIALIZATION TO IEAVTMSI                          */ 00130000
*   /*                                                               */ 00131000
*   /*****************************************************************/ 00132000
*                                                                  0138 00133000
*   GEN( POST (R5),ZIP);            /* CLEAN POST CODE               */ 00134000
          POST (R5),ZIP                                                 00135000
*   RFY                                                            0139 00136000
*     R5 UNRSTD;                                                   0139 00137000
*   RETINIT=YES;                    /* SHOW TASK IS INITIALIZED IN 0140 00138000
*                                      ESTAE PARMLIST                */ 00139000
         OI    RETINIT,B'10000000'                                 0140 00140000
*   RETRYADR=ADDR(AWAKE);           /* NEXT RETRY OUT OF ESTAE       */ 00141000
         LA    @14,AWAKE                                           0141 00142000
         ST    @14,RETRYADR                                        0141 00143000
*   GEN SETS(PARMLIST)(STM R8,R7,PARMLIST);/* SAVE FOR EXIT USE      */ 00144000
         STM R8,R7,PARMLIST                                             00145000
*SLEEP:                             /* ENTER WAIT                    */ 00146000
*   DO;                             /* WAIT ECB(RTCTRECB)            */ 00147000
SLEEP    DS    0H                                                  0144 00148000
*     RESPECIFY                                                    0144 00149000
*      (GPR00P,                                                    0144 00150000
*       GPR01P) RESTRICTED;                                        0144 00151000
*     GPR00P=1;                     /* LOAD WAIT COUNT               */ 00152000
         LA    GPR00P,1                                            0145 00153000
*     GPR01P=ADDR(RTCTRECB);        /* ADDRESS OF ECB                */ 00154000
         L     @14,CVTPTR                                          0146 00155000
         L     GPR01P,CVTRTMCT(,@14)                               0146 00156000
         LA    GPR01P,RTCTRECB(,GPR01P)                            0146 00157000
*     SVC(1);                       /* WAIT SVC                      */ 00158000
         SVC   1                                                   0147 00159000
*     RESPECIFY                                                    0148 00160000
*      (GPR00P,                                                    0148 00161000
*       GPR01P) UNRESTRICTED;                                      0148 00162000
*   END;                            /* WAIT ECB(RTCTRECB) WAIT FOR 0149 00163000
*                                      RER TO POST                   */ 00164000
*AWAKE:                             /* FIRST INSTRUCTION AFTER WAIT  */ 00165000
*   EXTRTN=NO;                      /* SHOW RECORD MAINLINE IN     0150 00166000
*                                      CONTROL               @YM02750*/ 00167000
AWAKE    NI    EXTRTN,B'10111111'                                  0150 00168000
*   RETRYADR=ADDR(AWAKE);           /* SET UP CORRECT RETRY ADDRESS     00169000
*                                                            @ZA20615*/ 00170000
         LA    @14,AWAKE                                           0151 00171000
         ST    @14,RETRYADR                                        0151 00172000
*   RTCTRECB=ZERO;                  /* RESET THE RECORDING ECB       */ 00173000
         SLR   @14,@14                                             0152 00174000
         L     @12,CVTPTR                                          0152 00175000
         L     @12,CVTRTMCT(,@12)                                  0152 00176000
         ST    @14,RTCTRECB(,@12)                                  0152 00177000
*   BUFLENS=BUFLENS&&BUFLENS;       /* ZERO BUFFER LENGTHS   @G17EP5W*/ 00178000
         XC    BUFLENS(8),BUFLENS                                  0153 00179000
*   BUFS=BUFS&&BUFS;                /* ZERO BUFFER ADDRESSES @G17EP5W*/ 00180000
         XC    BUFS(8),BUFS                                        0154 00181000
*   R7=RTCTRCB;                                                    0155 00182000
         L     R7,RTCTRCB(,@12)                                    0155 00183000
*   RESPECIFY                       /* BASE FOR RCB                  */ 00184000
*     RCB BASED(R7);                                               0156 00185000
*   IF RTCTRTER=YES THEN            /* IF TEMP ERROR SUFFERED        */ 00186000
         TM    RTCTRTER(@12),B'10000000'                           0157 00187000
         BNO   @RF00157                                            0157 00188000
*/********************************************************************/ 00189000
*/*                                                                  */ 00190000
*/* THE PURPOSE OF THE TEMPERR SEGMENT IS TO CLEANUP & REINITIALIZE  */ 00191000
*/* THE RCB IF A TEMPORARY ERROR HAS BEEN SUFFERED. IT FIRST CHECKS  */ 00192000
*/* TO SEE IF BUFFER MANIPULATION IS COMPLETE (RCBACNT = 0).IF SO IT */ 00193000
*/* WILL POST REQUESTORS WHO MAY BE WAITING AND REINITIALIZE THE RCB */ 00194000
*/*                                                                  */ 00195000
*/********************************************************************/ 00196000
*                                                                  0158 00197000
*     DO;                                                          0158 00198000
*       IF RCBACNT<=ZERO THEN       /* IF RCB BUFFER AREA IS NOT   0159 00199000
*                                      BEING                         */ 00200000
         CH    @14,RCBACNT(,R7)                                    0159 00201000
         BL    @RF00159                                            0159 00202000
*         DO;                       /* MANIPULATED BY REQUEST ROUTINE*/ 00203000
*                                                                  0160 00204000
*           /*********************************************************/ 00205000
*           /*                                                       */ 00206000
*           /* NOTE - SERIALIZATION OF THE RCB NOT NESESSARY AS AT   */ 00207000
*           /* THIS POINT NOBODY ELSE IS WORKING WITH IT             */ 00208000
*           /*                                                       */ 00209000
*           /*********************************************************/ 00210000
*                                                                  0161 00211000
*           RFY                                                    0161 00212000
*            (GPR01P) RSTD;         /*                       @ZA13432*/ 00213000
*           GPR01P=ADDR(SVC51LST);  /* SDUMP PARAMETER LIST  @ZA13432*/ 00214000
         LA    GPR01P,SVC51LST                                     0162 00215000
*           GEN(SDUMP MF=(E,(1)),HDR='RECORD TEMPORARY ERROR');/*  0163 00216000
*                                                            @ZA13432*/ 00217000
         SDUMP MF=(E,(1)),HDR='RECORD TEMPORARY ERROR'                  00218000
*           RFY                                                    0164 00219000
*            (GPR01P) UNRSTD;       /*                       @ZA13432*/ 00220000
*           RCBTLNG=LENGTH(RCBBUFRS);/* CONTROL POSSIBLE BAD LOOP    */ 00221000
         MVC   RCBTLNG(2,R7),@CH02223                              0165 00222000
*           TRYPOST=YES;            /* INDICATE TRYING TO POST       */ 00223000
         OI    TRYPOST,B'00100000'                                 0166 00224000
*           RETRYADR=ADDR(SKIPPOST);/* RETRY AFTER POST              */ 00225000
         LA    @14,SKIPPOST                                        0167 00226000
         ST    @14,RETRYADR                                        0167 00227000
*           DO WHILE(RCBFLNG<RCBTLNG);/* LOOP THRU BUFFER ENTRIES TO    00228000
*                                      POSTWAITING REQUESTORS        */ 00229000
         B     @DE00168                                            0168 00230000
@DL00168 DS    0H                                                  0169 00231000
*             OLDEST=RCBFREE+RCBFLNG;/* FIND THE OLDEST BUFFER ENTRY */ 00232000
         LH    @14,RCBFLNG(,R7)                                    0169 00233000
         N     @14,@CF02236                                        0169 00234000
         AL    @14,RCBFREE(,R7)                                    0169 00235000
         ST    @14,OLDEST                                          0169 00236000
*             IF OLDEST>=RCBBUFE THEN/* IF BUFFER WAS WRAPPED        */ 00237000
         C     @14,RCBBUFE(,R7)                                    0170 00238000
         BL    @RF00170                                            0170 00239000
*               OLDEST=OLDEST-RCBTLNG;/* UPDATE THE REC POINTER      */ 00240000
         LH    @12,RCBTLNG(,R7)                                    0171 00241000
         N     @12,@CF02236                                        0171 00242000
         SLR   @14,@12                                             0171 00243000
         ST    @14,OLDEST                                          0171 00244000
*             RESPECIFY             /* BASE BUFFER ENTRY MAP ON ADDR */ 00245000
*              (RCBBUF) BASED(OLDEST);/* OF OLDEST BUFFER ENTRY      */ 00246000
*                                                                  0172 00247000
@RF00170 DS    0H                                                  0173 00248000
*             /*******************************************************/ 00249000
*             /*                                                     */ 00250000
*             /* THIS SEGMENT WILL POST THE USERS ECB TO INDICATE    */ 00251000
*             /* THAT THE DATA HAS BEEN WRITTEN. POSTING WILL NOT BE */ 00252000
*             /* DONE IF IT WAS NOT REQUESTED OR IF THE ASID IS      */ 00253000
*             /* INVALID.                                            */ 00254000
*             /*                                                     */ 00255000
*             /*******************************************************/ 00256000
*                                                                  0173 00257000
*             RFY                                                  0173 00258000
*               ASVT BASED(CVTASVT);                               0173 00259000
*             IF RCBBPOST=YES&      /* WANT POSTING...               */ 00260000
*                 RCBBNBFR=YES THEN /* RECORD UNBUFFED..     @YM02617*/ 00261000
         L     @14,OLDEST                                          0174 00262000
         TM    RCBBPOST(@14),B'00001100'                           0174 00263000
         BNO   @RF00174                                            0174 00264000
*               DO;                 /* -YES.                         */ 00265000
*                 IF RCBBASID>ZERO& /* VALID NUMERICAL ASID...       */ 00266000
*                     RCBBASID<=ASVTMAXU&ASVTAVAL(RCBBASID)=NO THEN/*   00267000
*                                      ...AND ASID IN USE...         */ 00268000
         LH    @12,RCBBASID(,@14)                                  0176 00269000
         N     @12,@CF02236                                        0176 00270000
         LTR   @12,@12                                             0176 00271000
         BNP   @RF00176                                            0176 00272000
         L     @06,CVTPTR                                          0176 00273000
         L     @06,CVTASVT(,@06)                                   0176 00274000
         C     @12,ASVTMAXU(,@06)                                  0176 00275000
         BH    @RF00176                                            0176 00276000
         SLA   @12,2                                               0176 00277000
         ALR   @06,@12                                             0176 00278000
         TM    ASVTAVAL-4(@06),B'10000000'                         0176 00279000
         BNZ   @RF00176                                            0176 00280000
*                   DO;             /* YES,POST THE ECB.             */ 00281000
*                     RFY                                          0178 00282000
*                       R5 RSTD;    /* PTR TO THE ECB TO POST TO   0178 00283000
*                                      HANDLE WRAP HEADERS   @ZA19199*/ 00284000
*                     R5=ADDR(RCBBECB);/* ADDR OF ECB TO POST      0179 00285000
*                                                            @ZA19199*/ 00286000
         LA    R5,RCBBECB(,@14)                                    0179 00287000
*                     RFY                                          0180 00288000
*                       USERECB BASED(ECBADR);/* TO INSURE PROPER  0180 00289000
*                                      ADDRESS OF ECB        @ZA19199*/ 00290000
*                     IF R5>=RCBBUFE THEN/* DOES THE HDR WRAP      0181 00291000
*                                                            @ZA19199*/ 00292000
         C     R5,RCBBUFE(,R7)                                     0181 00293000
         BL    @RF00181                                            0181 00294000
*                       R5=R5-RCBTLNG;/* YES - RESET POINTER @ZA19199*/ 00295000
         LH    @14,RCBTLNG(,R7)                                    0182 00296000
         N     @14,@CF02236                                        0182 00297000
         SLR   R5,@14                                              0182 00298000
*                     ELSE                                         0183 00299000
*                       ;           /* HDR DOES NOT WRAP     @ZA19199*/ 00300000
@RF00181 DS    0H                                                  0184 00301000
*                     EXTRTN=YES;   /* SHOW IN EXTERNAL ROUTINE FOR     00302000
*                                      RECOVERY                      */ 00303000
         OI    EXTRTN,B'01000000'                                  0184 00304000
*                     RETRYADR=ADDR(AFTPSTA);/* PRESET RETRY ADDR    */ 00305000
         LA    @14,AFTPSTA                                         0185 00306000
         ST    @14,RETRYADR                                        0185 00307000
*                     GEN SETS(PARMLIST)(STM R8,R7,PARMLIST);/*    0186 00308000
*                                                            @YM02750*/ 00309000
         STM R8,R7,PARMLIST                                             00310000
*                     DO;           /* POST (USERECB,0) ASCB(ASVTENTY   00311000
*                                      (RCBBASID)->UASCBADR)       0187 00312000
*                                      ERRET(CVTBRET)                */ 00313000
*                       I002=I002&&I002;/* INITIALIZE LIST           */ 00314000
         XC    I002(12),I002                                       0188 00315000
*                       I00201=ADDR(USERECB);/* ECB ADDRESS          */ 00316000
         L     @14,ECBADR(,R5)                                     0189 00317000
         ST    @14,I00201                                          0189 00318000
*                       I00202=ADDR(ASVTENTY(RCBBASID)->UASCBADR);/*    00319000
*                                      ASCB ADDRESS                  */ 00320000
         L     @14,OLDEST                                          0190 00321000
         LH    @14,RCBBASID(,@14)                                  0190 00322000
         N     @14,@CF02236                                        0190 00323000
         SLA   @14,2                                               0190 00324000
         L     @12,CVTPTR                                          0190 00325000
         L     @06,CVTASVT(,@12)                                   0190 00326000
         L     @14,ASVTENTY-4(@14,@06)                             0190 00327000
         ST    @14,I00202                                          0190 00328000
*                       I00203=ADDR(CVTBRET);/* ERRET ADDRESS        */ 00329000
         LA    @14,CVTBRET(,@12)                                   0191 00330000
         ST    @14,I00203                                          0191 00331000
*                       RESPECIFY                                  0192 00332000
*                        (GPR00F,                                  0192 00333000
*                         GPR01P) RESTRICTED;                      0192 00334000
*                       GPR00F=0;   /* POST CODE IN R0               */ 00335000
         SLR   GPR00F,GPR00F                                       0193 00336000
*                       GPR01P=ADDR(I002);/* PARMLIST ADDR           */ 00337000
         LA    GPR01P,I002                                         0194 00338000
*                       GPR01P=GPR01P|'80000000'X;/* SET LIST FORM 0195 00339000
*                                      IND                           */ 00340000
         O     GPR01P,@CF00108                                     0195 00341000
*                       SVC(2);     /* ISSUE POST SVC                */ 00342000
         SVC   2                                                   0196 00343000
*                       RESPECIFY                                  0197 00344000
*                        (GPR00F,                                  0197 00345000
*                         GPR01P) UNRESTRICTED;                    0197 00346000
*                     END;          /* POST (USERECB,0) ASCB(ASVTENTY   00347000
*                                      (RCBBASID)->UASCBADR)       0198 00348000
*                                      ERRET(CVTBRET)                */ 00349000
*AFTPSTA:                                                          0199 00350000
*                     ;             /* RETRY IF POST FAILURE         */ 00351000
AFTPSTA  DS    0H                                                  0200 00352000
*                     RFY                                          0200 00353000
*                       USERECB BASED(RCBBECB);/* REBASE BACK      0200 00354000
*                                                            @ZA19199*/ 00355000
*                     RFY                                          0201 00356000
*                       R5 UNRSTD;  /* FREE THE ECB POINTER  @ZA19199*/ 00357000
*                     EXTRTN=NO;    /* BACK TO RECORD                */ 00358000
         NI    EXTRTN,B'10111111'                                  0202 00359000
*                     RETRYADR=ADDR(AWAKE);/* RESET RETRY ADDR       */ 00360000
         LA    @14,AWAKE                                           0203 00361000
         ST    @14,RETRYADR                                        0203 00362000
*                   END;                                           0204 00363000
*               END;                                               0205 00364000
@RF00176 DS    0H                                                  0206 00365000
*             RFY                                                  0206 00366000
*               ASVT BASED;                                        0206 00367000
@RF00174 DS    0H                                                  0207 00368000
*             IF RCBBNBFR=NO THEN   /* RECORD BUFFERED...            */ 00369000
         L     @14,OLDEST                                          0207 00370000
         TM    RCBBNBFR(@14),B'00001000'                           0207 00371000
         BNZ   @RF00207                                            0207 00372000
*               RCBFLNG=RCBFLNG+    /* YES - RELEASE IT              */ 00373000
*                   (RCBBLEN+       /* DATA PLUS                     */ 00374000
*                   CONTLEN+        /* CONTROL INFORMATION           */ 00375000
*                   SEVEN&          /* ROUNDED TO A DOUBLE WORD      */ 00376000
*                   'FFF8'X);                                      0208 00377000
         LA    @12,23                                              0208 00378000
         LH    @14,RCBBLEN(,@14)                                   0208 00379000
         N     @14,@CF02236                                        0208 00380000
         ALR   @12,@14                                             0208 00381000
         N     @12,@CF02103                                        0208 00382000
         LH    @14,RCBFLNG(,R7)                                    0208 00383000
         N     @14,@CF02236                                        0208 00384000
         ALR   @12,@14                                             0208 00385000
         STH   @12,RCBFLNG(,R7)                                    0208 00386000
*             ELSE                  /* RECORD IS NOT BUFFERED        */ 00387000
*               DO;                                                0209 00388000
         B     @RC00207                                            0209 00389000
@RF00207 DS    0H                                                  0210 00390000
*                 RFY                                              0210 00391000
*                   R1 RSTD;        /* RESTRICT REGISTER FOR ERRORID    00392000
*                                      LENGTH                @G17EP5W*/ 00393000
*                 IF RCBBERFG=YES THEN/* IF ERRORID APPENDED IN RCB     00394000
*                                                            @G17EP5W*/ 00395000
         L     @14,OLDEST                                          0211 00396000
         TM    RCBBERFG(@14),B'00000010'                           0211 00397000
         BNO   @RF00211                                            0211 00398000
*                   R1=LENGTH(NBFRERID);/* THEN INCLUDE ITS LENGTH 0212 00399000
*                                      FOR FREEING           @G17EP5W*/ 00400000
         LA    R1,10                                               0212 00401000
*                 ELSE              /* OTHERWISE             @G17EP5W*/ 00402000
*                   R1=ZERO;                                       0213 00403000
