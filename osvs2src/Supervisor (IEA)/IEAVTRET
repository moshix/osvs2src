*          2 I0351603 CHAR(45) INIT(                               0755 02382000
*                'IEA896I - RECORDING FUNCTION NO LONGER ACTIVE'),/*    02383000
*                                      MESSAGE TEXT                  */ 02384000
*          2 I0351604 BIT(16) INIT('0001000000000000'B),/* DESCRIPTOR   02385000
*                                      CODES                         */ 02386000
*          2 I0351605 BIT(16) INIT('0100000000000000'B);/* ROUTING 0755 02387000
*                                      CODES                         */ 02388000
*     END;                                                         0756 02389000
*   ELSE                                                           0757 02390000
*     DO;                           /* RETRY AT COMMON RETRY ROUTINE */ 02391000
         B     @RC00736                                            0757 02392000
@RF00736 DS    0H                                                  0758 02393000
*       DO;                         /* SETRP RETADDR(RETRYRTN)FRESDWA   02394000
*                                      (YES)RC(4)RECORD(NO)          */ 02395000
*         RESPECIFY                                                0759 02396000
*          (GPR00P,                                                0759 02397000
*           GPR01P,                                                0759 02398000
*           GPR14P,                                                0759 02399000
*           GPR15P) RSTD;                                          0759 02400000
*         GPR01P->SDWARCDE=4;       /* STORE RC INTO SDWA            */ 02401000
         MVI   SDWARCDE(GPR01P),X'04'                              0760 02402000
*         GPR01P->SDWARTYA=ADDR(RETRYRTN);/* SAVE RETRY ADDRESS      */ 02403000
         LA    @12,RETRYRTN                                        0761 02404000
         ST    @12,SDWARTYA(,GPR01P)                               0761 02405000
*         GPR01P->SDWAFREE='1'B;    /* SET FLAG TO FREE SDWA         */ 02406000
*         GPR01P->SDWARCRD='0'B;    /* TURN OFF RECORD INDICATOR     */ 02407000
         OI    SDWAFREE(GPR01P),B'00000100'                        0763 02408000
         NI    SDWARCRD(GPR01P),B'01111111'                        0763 02409000
*         RESPECIFY                                                0764 02410000
*          (GPR00P,                                                0764 02411000
*           GPR01P,                                                0764 02412000
*           GPR14P,                                                0764 02413000
*           GPR15P) UNRSTD;                                        0764 02414000
*       END;                                                       0765 02415000
*     END;                                                         0766 02416000
*   R14=R7;                         /* RESTORE RTM RETURN ADDRESS    */ 02417000
@RC00736 LR    R14,R7                                              0767 02418000
*   GOTO R14;                       /* RETURN TO RTM                 */ 02419000
         BR    R14                                                 0768 02420000
*NORTCA:                            /* IF SDWA NOT AVAILABLE         */ 02421000
*   R7=R14;                         /* SAVE RTM RETURN POINT         */ 02422000
NORTCA   LR    R7,R14                                              0769 02423000
*   IF(R1=CHARLIE1&EXTRTN=NO)|      /* IF ERROR WAS AN OPER EXCP   0770 02424000
*                                                            @YM02750*/ 02425000
*       (RTCTRTER=YES&TRYPOST=NO) THEN/* A TEMP ERROR ALREADY      0770 02426000
*                                      SUFFERED              @YM02750*/ 02427000
         SLR   @15,@15                                             0770 02428000
         ICM   @15,7,CHARLIE1                                      0770 02429000
         CR    R1,@15                                              0770 02430000
         BNE   @GL00012                                            0770 02431000
         TM    EXTRTN,B'01000000'                                  0770 02432000
         BZ    @RT00770                                            0770 02433000
@GL00012 L     @15,CVTPTR                                          0770 02434000
         L     @15,CVTRTMCT(,@15)                                  0770 02435000
         TM    RTCTRTER(@15),B'10000000'                           0770 02436000
         BNO   @RF00770                                            0770 02437000
         TM    TRYPOST,B'00100000'                                 0770 02438000
         BNZ   @RF00770                                            0770 02439000
@RT00770 DS    0H                                                  0771 02440000
*     DO;                           /* CONTINUE WITH TERMINATION     */ 02441000
*       RTCTRPER=YES;               /* IND RECORDING NO LONGER ACTIVE*/ 02442000
         L     @15,CVTPTR                                          0772 02443000
         L     @15,CVTRTMCT(,@15)                                  0772 02444000
         OI    RTCTRPER(@15),B'01000000'                           0772 02445000
*       RFY                                                        0773 02446000
*        (GPR01P) RSTD;             /*                       @ZA13432*/ 02447000
*       GPR01P=ADDR(SVC51LST);      /* PARM LIST IN REG 1    @ZA13432*/ 02448000
         LA    GPR01P,SVC51LST                                     0774 02449000
*       GEN(SDUMP MF=(E,(1)),HDR='RECORD PERMANENT ERROR');/*      0775 02450000
*                                                            @ZA13432*/ 02451000
         SDUMP MF=(E,(1)),HDR='RECORD PERMANENT ERROR'                  02452000
*       RFY                                                        0776 02453000
*        (GPR01P) UNRSTD;           /*                       @ZA13432*/ 02454000
*       DO;                         /* WTO('IEA896I - RECORDING    0777 02455000
*                                      FUNCTION NO LONGER ACTIVE') 0777 02456000
*                                      DESC(4) ROUTCDE(2)            */ 02457000
*         RFY                                                      0778 02458000
*           GPR01P RSTD;            /* RESTRICT REG 1                */ 02459000
*         GPR01P=ADDR(I03520);      /* ADDRESS CONTROL PROGRAM LIST  */ 02460000
         LA    GPR01P,I03520                                       0779 02461000
*         SVC(35);                  /* ISSUE WTO SVC                 */ 02462000
         SVC   35                                                  0780 02463000
*         RFY                                                      0781 02464000
*           GPR01P UNRSTD;          /* RELEASE REG 1                 */ 02465000
*       END;                        /* WTO('IEA896I - RECORDING    0782 02466000
*                                      FUNCTION NO LONGER ACTIVE') 0782 02467000
*                                      DESC(4) ROUTCDE(2)            */ 02468000
*       DECLARE                     /* WTO PARAMETER LIST            */ 02469000
*         1 I03520 STATIC BDY(WORD),                               0783 02470000
*          2 I0352001 FIXED(15) INIT(49),/* TEXT LENGTH              */ 02471000
*          2 I0352002 BIT(16) INIT('1000000000000000'B),/* MCS FLAG  */ 02472000
*          2 I0352003 CHAR(45) INIT(                               0783 02473000
*                'IEA896I - RECORDING FUNCTION NO LONGER ACTIVE'),/*    02474000
*                                      MESSAGE TEXT                  */ 02475000
*          2 I0352004 BIT(16) INIT('0001000000000000'B),/* DESCRIPTOR   02476000
*                                      CODES                         */ 02477000
*          2 I0352005 BIT(16) INIT('0100000000000000'B);/* ROUTING 0783 02478000
*                                      CODES                         */ 02479000
*       R15=RCZERO;                 /* INDICATE CONT W/TERMINATION   */ 02480000
         SLR   R15,R15                                             0784 02481000
*     END;                                                         0785 02482000
*   ELSE                                                           0786 02483000
*     DO;                           /* RETRY AT COMMON RETRY CODE    */ 02484000
         B     @RC00770                                            0786 02485000
@RF00770 DS    0H                                                  0787 02486000
*       R15=RCFOUR;                 /* INDICATE RETRY                */ 02487000
         LA    R15,4                                               0787 02488000
*       R0=ADDR(RETRYRTN);          /* RETRY ADDRESS                 */ 02489000
         LA    R0,RETRYRTN                                         0788 02490000
*     END;                                                         0789 02491000
*   R14=R7;                         /* RESTORE RTM RETURN ADDRESS    */ 02492000
@RC00770 LR    R14,R7                                              0790 02493000
*   GOTO R14;                       /* RETURN TO RTM TO RETRY        */ 02494000
         BR    R14                                                 0791 02495000
*   END RCDTSKR;                                                   0792 02496000
*                                                                  0792 02497000
*   /*****************************************************************/ 02498000
*   /*                                                               */ 02499000
*   /* THIS PROCEDURE IS USED TO RESET UP REGISTERS AND RETRY OR TO  */ 02500000
*   /* SET UP FOR CLEANUP BY THE RECORDING TASK                      */ 02501000
*   /*                                                               */ 02502000
