         DC    H'12'                                                    02303000
*     GEN(STM   GPR14P,GPR12F,12(GPR13P));/* GOOD R13, SAVE RTM'S  0729 02304000
*                                      REGS                          */ 02305000
         STM   GPR14P,GPR12F,12(GPR13P)                                 02306000
*     GEN(L     GPR15P,0(GPR01P));  /* PTR TO USER PARM LIST         */ 02307000
         L     GPR15P,0(GPR01P)                                         02308000
*     GEN(L     8,00(GPR15P));                                     0731 02309000
         L     8,00(GPR15P)                                             02310000
*     GEN(L     9,04(GPR15P));                                     0732 02311000
         L     9,04(GPR15P)                                             02312000
*   END;                            /* ENTESTAE (8,9)ERREXIT(NORTCA) */ 02313000
*   RFY                                                            0734 02314000
*     SDWA BASED(R1);                                              0734 02315000
*   R7=R14;                         /* SAVE RETURN ADDRESS TO RTM    */ 02316000
         LR    R7,R14                                              0735 02317000
*   IF(SDWACMPC=CHARLIE1&EXTRTN=NO)|/* IF ERROR IS OPER EXCP @YM02750*/ 02318000
*       (RTCTRTER=YES&              /* IF A TEMP ERROR ALREADY     0736 02319000
*                                      SUFFERED                      */ 02320000
*       TRYPOST=NO) THEN            /* AND NOT TRYING POST BEFORE  0736 02321000
*                                      REINIT                        */ 02322000
         CLC   SDWACMPC(3,R1),CHARLIE1                             0736 02323000
