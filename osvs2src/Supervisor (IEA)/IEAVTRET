         BNZ   @RF00797                                            0797 02516000
*       RTCTRTER=YES;               /* INDICATE OUR ERROR            */ 02517000
         L     @15,CVTPTR                                          0798 02518000
         L     @15,CVTRTMCT(,@15)                                  0798 02519000
         OI    RTCTRTER(@15),B'10000000'                           0798 02520000
*     R14=RETRYADR;                 /* RESUME AT                     */ 02521000
@RF00797 L     R14,RETRYADR                                        0799 02522000
*     GOTO R14;                     /* PRE-SET RETRY ADDR            */ 02523000
         BR    R14                                                 0800 02524000
*   END;                                                           0801 02525000
*   RFY                                                            0802 02526000
*    (R14,                                                         0802 02527000
*     R1) UNRSTD;                                                  0802 02528000
