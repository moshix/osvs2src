*     DO;                           /* THE ERROR IS DEEMED PERMANENT */ 02334000
*       DO;                         /* SETRP RECORD(NO)RC(0)         */ 02335000
*         RESPECIFY                                                0739 02336000
*          (GPR00P,                                                0739 02337000
*           GPR01P,                                                0739 02338000
*           GPR14P,                                                0739 02339000
*           GPR15P) RSTD;                                          0739 02340000
*         GPR01P->SDWARCDE=0;       /* SAVE RC VALUE                 */ 02341000
         MVI   SDWARCDE(GPR01P),X'00'                              0740 02342000
