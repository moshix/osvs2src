*           R2=R4;                  /* START ADDR (FROM).            */ 00571000
         LR    R2,R4                                               0259 00572000
*           R3=ZERO;                /* PADD AND COUNT IS ZERO        */ 00573000
         SLR   R3,R3                                               0260 00574000
*           GEN(MVCL R4,R2);        /* CLEAR OUT THE AREA.           */ 00575000
