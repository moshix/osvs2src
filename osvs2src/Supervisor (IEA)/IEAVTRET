*           IF R15=ZERO THEN        /* IF GETMAIN WAS SUCESSFUL      */ 00637000
         SLR   @14,@14                                             0285 00638000
         CR    R15,@14                                             0285 00639000
         BNE   @RF00285                                            0285 00640000
*/********************************************************************/ 00641000
*/*                                                                  */ 00642000
*/* THE PURPOSE OF THE REBUF SEGMENT IS TO MOVE ALL READY RECORDS TO */ 00643000
*/* THE GETMAINED BUFFER. THIS IS DONE TO AVOID LOST RECORDS DUE TO  */ 00644000
*/* LACK OF SPACE IN THE RCB. THE SPACE IN THE RCB IS FREED AS RECORD*/ 00645000
*/* MOVED RATHER THAN WAITING FOR I/O THAT WOULD BE NECESSARY IF     */ 00646000
*/* WRITTEN DIRECTLY.                                                */ 00647000
*/*                                                                  */ 00648000
*/********************************************************************/ 00649000
*                                                                  0286 00650000
*             DO;                                                  0286 00651000
*               REBUFADR=R1;        /* SAVE ADDRESS OF GOTTEN BUFFER */ 00652000
         ST    R1,REBUFADR                                         0287 00653000
*               REBLEN=FOURK;       /* SAVE LEN OF BUFFER    @G17EP5W*/ 00654000
         L     @12,FOURK                                           0288 00655000
