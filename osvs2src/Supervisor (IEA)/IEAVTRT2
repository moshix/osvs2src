*     R7=ADDR(RBEXSAVE);            /* SET REG WITH ADDR OF RTM ESA  */ 03011000
         LA    R7,RBEXSAVE(,R5)                                    0773 03012000
*     ESAR7=PSAAOLD;                /* SET CURRENT ASCB ADDRESS IN 0774 03013000
*                                      ESA                           */ 03014000
         L     @10,PSAAOLD                                         0774 03015000
         ST    @10,ESAR7(,R7)                                      0774 03016000
*     IF ESASDREC=OFF THEN          /* THERE HAS NOT BEEN A RECURSION   03017000
*                                      IN SVC DUMP                   */ 03018000
         TM    ESASDREC(R7),B'01000000'                            0775 03019000
