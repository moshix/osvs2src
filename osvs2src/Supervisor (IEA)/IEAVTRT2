         NI    TCBABTRM(R4),B'11011111'                            0175 00501000
*                       END;                                       0176 00502000
*                     ELSE                                         0177 00503000
*                       ;           /* ENTRY NOT DUE TO RTM1         */ 00504000
@RF00165 DS    0H                                                  0178 00505000
*                     TCBFX=ON;     /* PROHIBIT ASYNCHRONOUS EXITS   */ 00506000
