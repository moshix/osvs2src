               RELATED=(WSALSTAE,IEAVTRT2(RT2CRERR))                    03132000
*           RESPECIFY                                              0804 03133000
*            (GPR11P,                                              0804 03134000
*             GPR12P,                                              0804 03135000
*             GPR13P,                                              0804 03136000
*             GPR14P) UNRESTRICTED; /* RELEASE REGS USED BY SETLOCK  */ 03137000
*         END;                      /* SETLOCK RELEASE TYPE(LOCAL)REL   03138000
*                                      ATED(WSALSTAE,IEAVTRT2(RT2CRER   03139000
*                                      R)) RELEASE LOCAL LOCK        */ 03140000
*       ELSE                                                       0806 03141000
*         ;                         /* IF RECURSION IN SETLOCK THEN     03142000
*                                      THE LOCK WAS NOT ACQUIRED     */ 03143000
@RF00800 DS    0H                                                  0807 03144000
*       GENERATE REFS(R4) SETS(R0,R1,R14,R15);                     0807 03145000
*                                                                  0807 03146000
          STATUS SET,SD,(R4),(10)                                       03147000
*       /*************************************************************/ 03148000
*       /*                                                           */ 03149000
*       /* THE FOLLOWING CODE SETS UP THE BRANCH ENTRY TO STATUS AND */ 03150000
*       /* PASSES CONTROL TO IT. AS THE CURRENT TASK IS BEING SET    */ 03151000
*       /* NON-DISPATCHABLE, STATUS WILL FREE THE LOCAL LOCK AND EXIT*/ 03152000
*       /* TO THE DISPATCHER                                         */ 03153000
*       /*                                                           */ 03154000
*       /*************************************************************/ 03155000
*                                                                  0808 03156000
*       DO;                         /* SETLOCK OBTAIN TYPE(LOCAL)MODE   03157000
*                                      (UNCOND)RELATED('FREED BY   0808 03158000
*                                      STATUS')                      */ 03159000
