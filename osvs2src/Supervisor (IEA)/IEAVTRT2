*       DO;                                                        0776 03021000
*         ESASDREC=ON;              /* SET FOR RECURSION IN SVCDUMP  */ 03022000
         OI    ESASDREC(R7),B'01000000'                            0777 03023000
*         TCBFX=OFF;                /* PERMIT ASYNCHRONOUS EXITS     */ 03024000
         NI    TCBFX(R4),B'11111110'                               0778 03025000
*         GENERATE SETS(R0,R1,R14,R15) REFS(SDUMPLST);             0779 03026000
*                                                         /* @ZA04528*/ 03027000
          SDUMP MF=(E,SDUMPLST)                                @ZA04528 03028000
*         TCBFX=ON;                 /* PROHIBIT FURTHER ASYNCHRONOUS    03029000
*                                      EXITS                         */ 03030000
         OI    TCBFX(R4),B'00000001'                               0780 03031000
*         ESASDREC=OFF;             /* RESET SDUMP RECURSION FLAG    */ 03032000
         NI    ESASDREC(R7),B'10111111'                            0781 03033000
*       END;                                                       0782 03034000
*     ELSE                                                         0783 03035000
*       ;                           /* IF SVCDUMP HAS SUFFERED A   0783 03036000
*                                      PRIOR ERROR DO NOT TAKE ONE   */ 03037000
*                                                                  0783 03038000
@RF00775 DS    0H                                                  0784 03039000
*     /***************************************************************/ 03040000
*     /*                                                             */ 03041000
*     /* SEGMENT RT2TMRY WILL REQUEST TERMINATION OF THE MEMORY AND  */ 03042000
*     /* THEN SET THE CURRENT TASK TREE PERMANENTLY NONDISPATCHABLE. */ 03043000
*     /* IT FIRST REQUESTS THE LOCAL LOCK TO SERIALIZE THE WORK SAVE */ 03044000
*     /* AREA PASSED TO RTM1, UNLESS THE LOCK MANAGER IS SUFFERING AN*/ 03045000
*     /* ERROR. IT THEN CALLS RTM1 TO ABNORMALLY TERMINATE THE       */ 03046000
*     /* MEMORY. ON RETURN, IT RELEASES THE LOCAL LOCK IF IT WAS     */ 03047000
*     /* ACQUIRED AND ISSUES A STATUS SVC TO SET THE SUBTASK TREE    */ 03048000
*     /* NONDISPATCHABLE. ON RETURN FROM STATUS IT AGAIN ACQUIRES THE*/ 03049000
*     /* LOCAL LOCK AND RE-ENTERS STATUS VIA A BRANCH ENTRY TO SET   */ 03050000
*     /* THE CURRENT TASK NONDISPATCHABLE. STATUS WILL RELEASE THE   */ 03051000
*     /* LOCAL LOCK. ENQ/LOCK CONDITIONS ON ENTRY ARE UNKNOWN.       */ 03052000
*     /*                                                             */ 03053000
*     /***************************************************************/ 03054000
*                                                                  0784 03055000
*     DO;                           /* BEGIN SEGMENT RT2TMRY         */ 03056000
*       IF ESAGLREC=OFF THEN        /* THERE HAS NOT BEEN A SETLOCK     03057000
*                                      RECURSION                     */ 03058000
         TM    ESAGLREC(R7),B'00010000'                            0785 03059000
         BNZ   @RF00785                                            0785 03060000
*         DO;                                                      0786 03061000
*           ESAGLREC=ON;            /* SET FOR RECURSION IN SETLOCK  */ 03062000
         OI    ESAGLREC(R7),B'00010000'                            0787 03063000
*           DO;                     /* SETLOCK OBTAIN TYPE(LOCAL)MODE   03064000
*                                      (UNCOND)RELATED(WSALSTAE,IEAVT   03065000
*                                      RT2(RT2CRERR))                */ 03066000
*             RESPECIFY                                            0789 03067000
*              (GPR11P,                                            0789 03068000
*               GPR12P,                                            0789 03069000
*               GPR13P,                                            0789 03070000
*               GPR14P) RESTRICTED; /* RESTRICT REGS USED BY SETLOCK */ 03071000
*             GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P,GPR13P,     03072000
*                 GPR14P);                                         0790 03073000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        C03074000
               RELATED=(WSALSTAE,IEAVTRT2(RT2CRERR))                    03075000
*             RESPECIFY                                            0791 03076000
*              (GPR11P,                                            0791 03077000
*               GPR12P,                                            0791 03078000
*               GPR13P,                                            0791 03079000
*               GPR14P) UNRESTRICTED;/* RELEASE REGS USED BY SETLOCK */ 03080000
