*                                                                  0195 00561000
         TM    ESACTS(R7),B'00010000'                              0195 00562000
         BNZ   @RF00195                                            0195 00563000
*                 /***************************************************/ 00564000
*                 /*                                                 */ 00565000
*                 /* SEGMENT RT2GETWA ACQUIRES THE RTM2 WORK AREA.   */ 00566000
*                 /* THE TCBABGM FLAG IS SET SO THAT GETMAIN WILL    */ 00567000
*                 /* OBTAIN STORAGE FROM SQA IF NONE IS AVAILABLE IN */ 00568000
*                 /* LSQA. A CONDITIONAL GETMAIN IS THEN ISSUED. IF  */ 00569000
*                 /* IT IS SUCCESSFUL, THE ADDRESS OF THE ACQUIRED   */ 00570000
*                 /* CORE IS PLACED IN THE ESA AND THE CORE IS       */ 00571000
*                 /* ZEROED. IT IS CHAINED FROM THE TCB AND, IF IT   */ 00572000
*                 /* WAS OBTAINED FROM SQA, TO THE ASCB. IF CORE IS  */ 00573000
*                 /* NOT OBTAINED A RETURN CODE OF 4 IS RETURNED.    */ 00574000
*                 /* OTHERWISE, A RETURN CODE OF 0 IS RETURNED. ON   */ 00575000
*                 /* ENTRY REG 4 LOCATES THE TCB, REG 5, THE SVRB AND*/ 00576000
*                 /* REG 7, THE ASCB. NO LOCK IS HELD ON ENTRY OR    */ 00577000
*                 /* EXIT. THIS ROUTINE MUST NOT BE RE-ENTERED IF IT */ 00578000
*                 /* HAS SUFFERED AN ERROR.                          */ 00579000
*                 /*                                                 */ 00580000
*                 /***************************************************/ 00581000
*                                                                  0196 00582000
*                 DO;               /* BEGIN SEGMENT RT2GETWA        */ 00583000
*                   TCBABGM=ON;     /* FORCE GETMAIN TO USE SQA IF NO   00584000
*                                      LSQA AVAILABLE                */ 00585000
