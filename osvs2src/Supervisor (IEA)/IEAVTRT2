*                                      SECTION SHOULD RECURSE        */ 00783000
*                                                                  0254 00784000
         OI    ESAINREC(R7),B'00100000'                            0254 00785000
*             /*******************************************************/ 00786000
*             /*                                                     */ 00787000
*             /* SEGMENT RT2INWA CONTROLS THE INITIALIZATION OF THE  */ 00788000
*             /* RTM2WA. IT INCLUDES SEGMENTS RT2INCNV, RT2INCM,     */ 00789000
*             /* RT2INPG, RT2INRCR, RT2INRT1, RT2INMT, RT2INEOT AND  */ 00790000
*             /* RT2INABD TO INITIALIZE FIELDS IN THE RTM2WA BASED ON*/ 00791000
*             /* THE TYPE OF ENTRY. ON ENTRY REG 5 LOCATES THE SVRB  */ 00792000
*             /* AND REG 8, THE RTM2WA. NO LOCK IS HELD ON ENTRY OR  */ 00793000
*             /* EXIT. THERE ARE NO RETURN CODES. THIS ROUTINE MUST  */ 00794000
*             /* NOT BE RE-ENTERED IF IT HAS SUFFERED AN ERROR.      */ 00795000
*             /*                                                     */ 00796000
*             /*******************************************************/ 00797000
*                                                                  0255 00798000
*             DO;                   /* BEGIN SEGMENT RT2INWA         */ 00799000
*               IF ESACTS=ON THEN   /* ENTRY IS DUE TO A CONVERSION     00800000
*                                      TO STEP                       */ 00801000
*                                                                  0256 00802000
         TM    ESACTS(R7),B'00010000'                              0256 00803000
         BNO   @RF00256                                            0256 00804000
*                 /***************************************************/ 00805000
*                 /*                                                 */ 00806000
*                 /* SEGMENT RT2INCNV INITIALIZES THE WORK AREA FOR  */ 00807000
*                 /* ABENDS THAT HAVE BEEN CONVERTED TO STEP DUE TO  */ 00808000
*                 /* THE STEP OPTION BEING SPECIFIED BY THE REQUESTOR*/ 00809000
*                 /* OF ABEND OR BECAUSE THE ABEND OCCURRED ON A TASK*/ 00810000
*                 /* WHICH HELD STEP MUST COMPLETE RESOURCES. THE    */ 00811000
*                 /* WORK AREA BEING USED BY RTM2 ON THE TASK THAT   */ 00812000
*                 /* ORIGINALLY SUFFERED THE ERROR WAS QUEUED TO THE */ 00813000
*                 /* JOBSTEP TCB BEFORE CAUSING THE JOBSTEP TO BE    */ 00814000
*                 /* ABTERMED SO,AT THIS ENTRY, A WORK AREA ALREADY  */ 00815000
*                 /* EXISTS. THE WORK AREA IS ADJUSTED TO REFLECT THE*/ 00816000
*                 /* JOBSTEP LEVEL. FIELDS SET BY THE ORIGINAL RTM2  */ 00817000
*                 /* PROCESSING THAT ARE NO LONGER APPROPRIATE ARE   */ 00818000
*                 /* ZEROED. THEY WILL BE SET AS NEEDED DURING       */ 00819000
*                 /* CURRENT RTM2 PROCESSING. ON ENTRY REG 8 LOCATES */ 00820000
*                 /* THE RTM2WA AND REG 5 LOCATES THE SVRB.          */ 00821000
*                 /*                                                 */ 00822000
*                 /***************************************************/ 00823000
*                                                                  0257 00824000
*                 DO;               /* BEGIN SEGMENT RT2INCNV        */ 00825000
*                                                                  0257 00826000
*                   /*************************************************/ 00827000
*                   /*                                               */ 00828000
*                   /* ZERO APPROPRIATE FIELDS. THEY WILL BE SET AS  */ 00829000
*                   /* NEEDED DURING SUBSEQUENT PROCESSING           */ 00830000
*                   /*                                               */ 00831000
*                   /*************************************************/ 00832000
*                                                                  0258 00833000
*                   RTM2CVER(1:256)=''B;                           0258 00834000
         XC    RTM2CVER(256,R8),RTM2CVER(R8)                       0258 00835000
*                   RTM2CVER(257:512)=''B;                         0259 00836000
         XC    RTM2CVER+256(256,R8),RTM2CVER+256(R8)               0259 00837000
*                   RTM2CVER(513:LENGTH(RTM2CVER))=''B;            0260 00838000
         XC    RTM2CVER+512(10,R8),RTM2CVER+512(R8)                0260 00839000
*                   RTM2ERRC=''B;                                  0261 00840000
         MVI   RTM2ERRC(R8),X'00'                                  0261 00841000
*                   RTM2ERRD=''B;                                  0262 00842000
*                                                                  0262 00843000
         MVI   RTM2ERRD(R8),X'00'                                  0262 00844000
*                   /*************************************************/ 00845000
*                   /*                                               */ 00846000
*                   /* ADJUST WORK AREA TO REFLECT JOBSTEP LEVEL     */ 00847000
*                   /*                                               */ 00848000
*                   /*************************************************/ 00849000
*                                                                  0263 00850000
*                   RTM2TCBC=ESAR4; /* SET ADDRESS OF CURRENT TCB IN    00851000
*                                      RTM2WA                        */ 00852000
         L     @12,ESAR4(,R7)                                      0263 00853000
         ST    @12,RTM2TCBC(,R8)                                   0263 00854000
*                   RTM2TCBT=ESAR4; /* SET TOP TCB ADDRESS IN RTM2WA    00855000
*                                      THE SAME AS CURRENT           */ 00856000
         ST    @12,RTM2TCBT(,R8)                                   0264 00857000
*                   RTM2VRBC=ESAR5; /* SET CURRENT SVRB ADDRESS IN 0265 00858000
*                                      RTM2WA                        */ 00859000
         L     @09,ESAR5(,R7)                                      0265 00860000
         ST    @09,RTM2VRBC(,R8)                                   0265 00861000
*                   RTM2VRBT=ESAR5; /* SET TOP SVRB ADDRESS IN RTM2WA   00862000
*                                      THE SAME AS CURRENT           */ 00863000
         ST    @09,RTM2VRBT(,R8)                                   0266 00864000
*                   RMPLTCBA=ESAR4; /* SET TCB ADDRESS IN RMPL EQUAL    00865000
*                                      TO CURRENT                    */ 00866000
         ST    @12,RMPLTCBA+712(,R8)                               0267 00867000
*                   RTM2SCBN=ESAR4->TCBSTABB;/* PUT ADDRESS OF LAST     00868000
*                                      ESTABLISHED STAE CONTROL BLOCK   00869000
*                                      IN RTM2WA                     */ 00870000
         L     @12,TCBSTABB-1(,@12)                                0268 00871000
         LA    @12,0(,@12)                                         0268 00872000
         ST    @12,RTM2SCBN(,R8)                                   0268 00873000
*                   RTM2SCBO=0;     /* SET END OF SCB QUEUE INDICATOR   00874000
*                                      FOR TASK RECOVERY             */ 00875000
         SLR   @12,@12                                             0269 00876000
         ST    @12,RTM2SCBO(,R8)                                   0269 00877000
*                   RTM2SCBC=SCBSTART;/* INDICATE INITIAL ENTRY FOR     00878000
*                                      TASK RECOVERY                 */ 00879000
*                                                                  0270 00880000
         LA    @12,1                                               0270 00881000
         ST    @12,RTM2SCBC(,R8)                                   0270 00882000
*                   /*************************************************/ 00883000
*                   /*                                               */ 00884000
*                   /* RESET ECB PTRS                                */ 00885000
*                   /*                                               */ 00886000
*                   /*************************************************/ 00887000
*                                                                  0271 00888000
*                   RESPECIFY                                      0271 00889000
*                     R9 RESTRICTED;                               0271 00890000
*                   DO R9=1 TO MAXECBS;                            0272 00891000
         LR    R9,@12                                              0272 00892000
@DL00272 DS    0H                                                  0273 00893000
*                     RTM2ECBA(R9)=ADDR(RTM2ECBS(R9));/* SET ECB   0273 00894000
*                                      POINTER TO ECB ADDRESS        */ 00895000
         LR    @12,R9                                              0273 00896000
         SLA   @12,2                                               0273 00897000
         LA    @02,RTM2ECBS-4(@12,R8)                              0273 00898000
         ST    @02,RTM2ECBA-4(@12,R8)                              0273 00899000
*                   END;                                           0274 00900000
         AL    R9,@CF00067                                         0274 00901000
         C     R9,@CF00104                                         0274 00902000
         BNH   @DL00272                                            0274 00903000
*                   RESPECIFY                                      0275 00904000
*                     R9 UNRESTRICTED;                             0275 00905000
*                                                                  0275 00906000
*                   /*************************************************/ 00907000
*                   /*                                               */ 00908000
*                   /* INDICATE CONVERTED TO STEP ABEND              */ 00909000
*                   /*                                               */ 00910000
*                   /*************************************************/ 00911000
*                                                                  0276 00912000
*                   RTM2CTS=ON;                                    0276 00913000
*                                                                  0276 00914000
         OI    RTM2CTS(R8),B'00010000'                             0276 00915000
*                   /*************************************************/ 00916000
*                   /*                                               */ 00917000
*                   /* ADJUST TCB FIELDS                             */ 00918000
*                   /*                                               */ 00919000
*                   /*************************************************/ 00920000
*                                                                  0277 00921000
*                   ESAGLREC=ON;    /* SET RECURSION FLAG IN ESA TO     00922000
*                                      COVER LOCK MANAGER FAILURE    */ 00923000
         OI    ESAGLREC(R7),B'00010000'                            0277 00924000
*                   DO;             /* SETLOCK OBTAIN TYPE(LOCAL)MODE   00925000
*                                      (UNCOND)RELATED(TCBFDW,IEAVTRT   00926000
*                                      2(RT2INCNV))                  */ 00927000
*                     RESPECIFY                                    0279 00928000
*                      (GPR11P,                                    0279 00929000
*                       GPR12P,                                    0279 00930000
*                       GPR13P,                                    0279 00931000
*                       GPR14P) RESTRICTED;/* RESTRICT REGS USED BY     00932000
*                                      SETLOCK                       */ 00933000
*                     GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P,    00934000
*                         GPR13P,GPR14P);                          0280 00935000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        C00936000
               RELATED=(TCBFDW,IEAVTRT2(RT2INCNV))                      00937000
*                     RESPECIFY                                    0281 00938000
*                      (GPR11P,                                    0281 00939000
*                       GPR12P,                                    0281 00940000
*                       GPR13P,                                    0281 00941000
*                       GPR14P) UNRESTRICTED;/* RELEASE REGS USED BY    00942000
*                                      SETLOCK                       */ 00943000
*                   END;            /* SETLOCK OBTAIN TYPE(LOCAL)MODE   00944000
*                                      (UNCOND)RELATED(TCBFDW,IEAVTRT   00945000
*                                      2(RT2INCNV)) OBTAIN LOCAL LOCK*/ 00946000
*                   TCBFDW=OFF;     /* ZERO FDW SO THAT LATER PROCESS   00947000
*                                      DOES NOT ASSUME A SUBTASK IS     00948000
*                                      WAITING                       */ 00949000
         NI    TCBFDW(R4),B'11011111'                              0283 00950000
*                   TCBFA=ON;       /* INDICATE ERROR UNRECOVERABLE     00951000
*                                      AND PREVENT SUBTASKS FROM   0284 00952000
*                                      CONVERTING TO STEP            */ 00953000
*                   TCBFT=ON;       /* TOP TASK IN TREE              */ 00954000
         OI    TCBFA(R4),B'10000100'                               0285 00955000
*                   DO;             /* SETLOCK RELEASE TYPE(LOCAL)REL   00956000
*                                      ATED(TCBFDW,IEAVTRT2(RT2INCNV)   00957000
*                                      )                             */ 00958000
*                     RESPECIFY                                    0287 00959000
*                      (GPR11P,                                    0287 00960000
*                       GPR12P,                                    0287 00961000
*                       GPR13P,                                    0287 00962000
*                       GPR14P) RESTRICTED;/* RESTRICT REGS USED BY     00963000
*                                      SETLOCK                       */ 00964000
*                     GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P,    00965000
*                         GPR13P,GPR14P);                          0288 00966000
         SETLOCK RELEASE,TYPE=LOCAL,                                   C00967000
               RELATED=(TCBFDW,IEAVTRT2(RT2INCNV))                      00968000
*                     RESPECIFY                                    0289 00969000
*                      (GPR11P,                                    0289 00970000
*                       GPR12P,                                    0289 00971000
*                       GPR13P,                                    0289 00972000
*                       GPR14P) UNRESTRICTED;/* RELEASE REGS USED BY    00973000
*                                      SETLOCK                       */ 00974000
*                   END;            /* SETLOCK RELEASE TYPE(LOCAL)REL   00975000
*                                      ATED(TCBFDW,IEAVTRT2(RT2INCNV)   00976000
*                                      ) RELEASE LOCAL LOCK          */ 00977000
*                   ESAGLREC=OFF;   /* RESET RECURSION FLAG          */ 00978000
         NI    ESAGLREC(R7),B'11101111'                            0291 00979000
*                 END;              /* END SEGMENT RT2INCNV          */ 00980000
*               ELSE                /* NOT CONVERT TO STEP ENTRY     */ 00981000
*                 DO;                                              0293 00982000
*                                                                  0293 00983000
         B     @RC00256                                            0293 00984000
@RF00256 DS    0H                                                  0294 00985000
*                   /*************************************************/ 00986000
*                   /*                                               */ 00987000
*                   /* SEGMENT RT2INCM INITIALIZES THE FIELDS IN THE */ 00988000
*                   /* RTM2 WORK AREA THAT ARE NEEDED FOR ALL TYPES  */ 00989000
*                   /* OF ENTRIES EXCEPT CONVERTED TO STEP ENTRIES.  */ 00990000
*                   /* THE DATA TEMPORARILY STORED IN THE ESA IS     */ 00991000
*                   /* COPIED INTO THE WORK AREA. POINTERS WITHIN THE*/ 00992000
*                   /* WORK AREA TO DATA WITHIN THE WORK AREA ARE    */ 00993000
*                   /* SET. ON ENTRY, REG 5 LOCATES THE SVRB AND REG */ 00994000
*                   /* 8, THE RTM2WA.                                */ 00995000
*                   /*                                               */ 00996000
*                   /*************************************************/ 00997000
*                                                                  0294 00998000
*                   DO;             /* BEGIN SEGMENT RT2INCM         */ 00999000
*                     RTM2CVT=ESAR3;/* PUT ADDR OF CVT IN RTM2WA     */ 01000000
         L     @12,ESAR3(,R7)                                      0295 01001000
         ST    @12,RTM2CVT(,R8)                                    0295 01002000
*                     RTM2TCBC=ESAR4;/* PUT ADDR OF CURRENT TCB IN 0296 01003000
*                                      RTM2WA                        */ 01004000
         L     @09,ESAR4(,R7)                                      0296 01005000
         ST    @09,RTM2TCBC(,R8)                                   0296 01006000
*                     RTM2TCBT=ESAR4;/* PUT ADDR OF TOP TCB IN     0297 01007000
*                                      FAILING TASK IN RTM2WA -    0297 01008000
*                                      ASSUMED TO BE CURRENT AT THIS    01009000
*                                      TIME                          */ 01010000
         ST    @09,RTM2TCBT(,R8)                                   0297 01011000
*                     RTM2VRBC=ESAR5;/* PUT ADDRESS OF CURRENT SVRB     01012000
*                                      IN RTM2WA                     */ 01013000
         L     @02,ESAR5(,R7)                                      0298 01014000
         ST    @02,RTM2VRBC(,R8)                                   0298 01015000
*                     RTM2VRBT=ESAR5;/* PUT ADDR OF SVRB ON TOP TCB     01016000
*                                      IN RTM2WA - ASSUMED TO BE   0299 01017000
*                                      CURRENT AT THIS TIME.         */ 01018000
         ST    @02,RTM2VRBT(,R8)                                   0299 01019000
*                     RTM2ADDR=ESART2WA;/* PUT ADDR OF BEGINNING OF     01020000
*                                      RTM2WA IN RTM2WA              */ 01021000
         L     @02,ESART2WA(,R7)                                   0300 01022000
         ST    @02,RTM2ADDR(,R8)                                   0300 01023000
*                     RTM2RT2D=ESART2D;/* PUT SUBPOOL ID AND LENGTH     01024000
*                                      OF RTM2WA IN RTM2WA           */ 01025000
         MVC   RTM2RT2D(4,R8),ESART2D(R7)                          0301 01026000
*                     RTM2ID='RTM2';/* PUT SELF-IDENTIFIER IN RTM2WA */ 01027000
         MVC   RTM2ID(4,R8),@CC03137                               0302 01028000
*                     RTM2ASC=ESAR7;/* PUT ADDR OF ASCB IN RTM2WA    */ 01029000
         L     @02,ESAR7(,R7)                                      0303 01030000
         ST    @02,RTM2ASC(,R8)                                    0303 01031000
*                     RTM2CT=ESAR3->CVTRTMCT;/* PUT ADDR OF RTMCT IN    01032000
*                                      RTM2WA                        */ 01033000
         L     @12,CVTRTMCT(,@12)                                  0304 01034000
         ST    @12,RTM2CT(,R8)                                     0304 01035000
*                     RTM2SCBN=ESAR4->TCBSTABB;/* PUT ADDR OF LAST 0305 01036000
*                                      ESTABLISHED STAE CONTROL BLOCK   01037000
*                                      IN RTM2WA                     */ 01038000
         L     @12,TCBSTABB-1(,@09)                                0305 01039000
         LA    @12,0(,@12)                                         0305 01040000
         ST    @12,RTM2SCBN(,R8)                                   0305 01041000
*                     RTM2SCBC=SCBSTART;/* INDICATE INITIAL ENTRY FOR   01042000
*                                      TASK RECOVERY                 */ 01043000
         LA    @12,1                                               0306 01044000
         ST    @12,RTM2SCBC(,R8)                                   0306 01045000
*                     RTM2SCBO=0;   /* SET END OF SCB QUEUE INDICATOR   01046000
*                                      FOR TASK RECOVERY             */ 01047000
*                                                                  0307 01048000
         SLR   @09,@09                                             0307 01049000
         ST    @09,RTM2SCBO(,R8)                                   0307 01050000
*                     /***********************************************/ 01051000
*                     /*                                             */ 01052000
*                     /* SET ADDRESSES TO AREAS WITHIN THE WORK AREA */ 01053000
*                     /*                                             */ 01054000
*                     /***********************************************/ 01055000
*                                                                  0308 01056000
*                     RMPLRMWA=ADDR(RTM2RMWS);/* SET ADDR OF RESOURCE   01057000
*                                      MGR WORK SPACE IN RMPL        */ 01058000
         LA    @09,712                                             0308 01059000
         ALR   @09,R8                                              0308 01060000
         LA    @02,RTM2RMWS(,R8)                                   0308 01061000
         ST    @02,RMPLRMWA(,@09)                                  0308 01062000
*                     RTM2RMPS=ADDR(RMPL);/* SET ADDR OF RMPL IN   0309 01063000
*                                      RTM2WA                        */ 01064000
         ST    @09,RTM2RMPS(,R8)                                   0309 01065000
*                     RESPECIFY                                    0310 01066000
*                       R9 RESTRICTED;/* RESTRICT CONTROL REG        */ 01067000
*                     DO R9=1 TO MAXECBS;                          0311 01068000
         LR    R9,@12                                              0311 01069000
@DL00311 DS    0H                                                  0312 01070000
*                       RTM2ECBA(R9)=ADDR(RTM2ECBS(R9));/* SET ECB 0312 01071000
*                                      POINTER TO ECB ADDRESS        */ 01072000
         LR    @12,R9                                              0312 01073000
         SLA   @12,2                                               0312 01074000
         LA    @02,RTM2ECBS-4(@12,R8)                              0312 01075000
         ST    @02,RTM2ECBA-4(@12,R8)                              0312 01076000
*                     END;                                         0313 01077000
         AL    R9,@CF00067                                         0313 01078000
         C     R9,@CF00104                                         0313 01079000
         BNH   @DL00311                                            0313 01080000
*                     RESPECIFY                                    0314 01081000
*                       R9 UNRESTRICTED;/* RELEASE CONTROL REG       */ 01082000
*                   END;            /* END SEGMENT RT2INCM           */ 01083000
*                   IF ESANEOT=ON THEN/* NORMAL END OF TASK          */ 01084000
*                                                                  0316 01085000
         TM    ESANEOT(R7),B'00001000'                             0316 01086000
         BNO   @RF00316                                            0316 01087000
*                     /***********************************************/ 01088000
*                     /*                                             */ 01089000
*                     /* RT2INEOT INITIALIZES THE RESOURCE MANAGER   */ 01090000
*                     /* PARAMETER LIST AND SETS UP FOR NORMAL END OF*/ 01091000
*                     /* TASK PROCESSING. THE COMPLETION CODE IS     */ 01092000
*                     /* STORED IN THE TCB.                          */ 01093000
*                     /*                                             */ 01094000
*                     /***********************************************/ 01095000
*                                                                  0317 01096000
*                     DO;           /* START OF RT2INEOT             */ 01097000
*                                                                  0317 01098000
*                       /*********************************************/ 01099000
*                       /*                                           */ 01100000
*                       /* INITIALIZE RESOURCE MANAGER PARM LIST FOR */ 01101000
*                       /* NORMAL END OF TASK                        */ 01102000
*                       /*                                           */ 01103000
*                       /*********************************************/ 01104000
*                                                                  0318 01105000
*                       RMPLTERM=OFF;/* TASK TERMINATING             */ 01106000
*                       RMPLTYPE=OFF;/* NORMAL TERMINATION           */ 01107000
         LA    @12,712                                             0319 01108000
         ALR   @12,R8                                              0319 01109000
         NI    RMPLTERM(@12),B'00111111'                           0319 01110000
*                       RMPLASCB=ESAR7;/* SAVE ADDRESS OF ASCB OF  0320 01111000
*                                      ENDING TASK                   */ 01112000
         L     @09,ESAR7(,R7)                                      0320 01113000
         ST    @09,RMPLASCB(,@12)                                  0320 01114000
*                       RMPLASID=ESAR7->ASCBASID;/* SAVE ASID IN   0321 01115000
*                                      RESOURCE MGR PARM LIST        */ 01116000
         MVC   RMPLASID(2,@12),ASCBASID(@09)                       0321 01117000
*                       RTM2CODE=ESAR1;/* COMPLETION CODE WAS PASSED    01118000
*                                      IN REG. 1                     */ 01119000
*                                                                  0322 01120000
         L     @12,ESAR1(,R7)                                      0322 01121000
         ST    @12,RTM2CODE(,R8)                                   0322 01122000
*                       /*********************************************/ 01123000
*                       /*                                           */ 01124000
*                       /* SAVE COMPLETION CODE IN TCB               */ 01125000
*                       /*                                           */ 01126000
*                       /*********************************************/ 01127000
*                                                                  0323 01128000
*                       TCBCMPC=RTM2CC;/* SAVE COMPLETION CODE       */ 01129000
         MVC   TCBCMPC(3,R4),RTM2CC(R8)                            0323 01130000
*                       TCBCMPF=0;  /* CLEAR FLAGS FIELD             */ 01131000
         MVI   TCBCMPF(R4),X'00'                                   0324 01132000
*                     END;          /* END OF RT2INEOT               */ 01133000
*                   ELSE            /* NOT NORMAL END OF TASK        */ 01134000
*                     DO;                                          0326 01135000
         B     @RC00316                                            0326 01136000
@RF00316 DS    0H                                                  0327 01137000
*                       IF ESAPGNLY=ON THEN/* ENTRY IS FOR PURGE ONLY*/ 01138000
*                                                                  0327 01139000
         TM    ESAPGNLY(R7),B'00100000'                            0327 01140000
         BNO   @RF00327                                            0327 01141000
*                         /*******************************************/ 01142000
*                         /*                                         */ 01143000
*                         /* SEGMENT RT2INPG INITIALIZES THE WORK    */ 01144000
*                         /* AREA WHEN RTM2 IS ENTERED FOR           */ 01145000
*                         /* TERMINATION PURGES ONLY. THE CONTROLLING*/ 01146000
*                         /* WORK AREA IS FOUND BY SEARCHING THE     */ 01147000
*                         /* ANCESTOR TREE FOR THE TOP TCB IN THIS   */ 01148000
*                         /* FAILING TREE AND THE APPROPRIATE FIELDS */ 01149000
*                         /* WITHIN IT ARE COPIED. THESE FIELDS ARE  */ 01150000
*                         /* BASICALLY THOSE WHICH CONTAIN THE       */ 01151000
*                         /* ORIGINAL ERROR DATA. THE REMAINING      */ 01152000
*                         /* FIELDS ARE THEN INITIALIZED IF REQUIRED.*/ 01153000
*                         /* ON ENTRY REG 8 LOCATES THE RTM2WA AND   */ 01154000
*                         /* REG 5, THE SVRB.                        */ 01155000
*                         /*                                         */ 01156000
*                         /*******************************************/ 01157000
*                                                                  0328 01158000
*                         DO;       /* BEGIN SEGMENT RT2INPG         */ 01159000
*                                                                  0328 01160000
*                           /*****************************************/ 01161000
*                           /*                                       */ 01162000
*                           /* FIND CONTROLLING WORKAREA             */ 01163000
*                           /*                                       */ 01164000
*                           /*****************************************/ 01165000
*                                                                  0329 01166000
*                           RESPECIFY                              0329 01167000
*                             R9 RESTRICTED;/* RESTRICT ALT. TCB PTR */ 01168000
*                           R9=R4;  /* SET ANCESTOR TCB ADDR EQUAL TO   01169000
*                                      CURRENT                       */ 01170000
         LR    R9,R4                                               0330 01171000
*                           RESPECIFY                              0331 01172000
*                             TCB BASED(R9);/* BASE TCB ON ANCESTOR     01173000
*                                      ADDRESS                       */ 01174000
*                           DO WHILE(TCBFT=OFF);/* SEARCH FOR TOP  0332 01175000
*                                      TASK                          */ 01176000
         B     @DE00332                                            0332 01177000
@DL00332 DS    0H                                                  0333 01178000
*                             R9=TCBOTC;/* FIND ANCESTOR             */ 01179000
         L     R9,TCBOTC(,R9)                                      0333 01180000
*                           END;    /* ATCBPTR NOW CONTAINS ADDR OF     01181000
*                                      TOP TASK IN FAILING TREE      */ 01182000
@DE00332 TM    TCBFT(R9),B'00000100'                               0334 01183000
         BZ    @DL00332                                            0334 01184000
*                           DO;                                    0335 01185000
*                             RESPECIFY                            0336 01186000
*                               R2 RESTRICTED;                     0336 01187000
*                             R2=TCBRBP;/* LOCATE STARTING RB        */ 01188000
*                                                                  0337 01189000
         L     R2,TCBRBP(,R9)                                      0337 01190000
*                             /***************************************/ 01191000
*                             /*                                     */ 01192000
*                             /* SCAN FOR RB WITH RBABEND ON. SCAN   */ 01193000
*                             /* TERMINATED WHEN RBTCBNXT ON         */ 01194000
*                             /*                                     */ 01195000
*                             /***************************************/ 01196000
*                                                                  0338 01197000
*                             DO WHILE(EVAL(R2-LENGTH(RBPRFX))->RBABEND 01198000
*                                   ='0'B&R2->RBTCBNXT='0'B);      0338 01199000
         B     @DE00338                                            0338 01200000
@DL00338 DS    0H                                                  0339 01201000
*                               R2=R2->RBLINKB;/* LOCATE NEXT RB     */ 01202000
         L     @12,RBLINKB-1(,R2)                                  0339 01203000
         LA    @12,0(,@12)                                         0339 01204000
         LR    R2,@12                                              0339 01205000
*                             END;  /* END SCAN OF RBS               */ 01206000
@DE00338 LR    @12,R2                                              0340 01207000
         SL    @12,@CF00482                                        0340 01208000
         TM    RBABEND(@12),B'00100000'                            0340 01209000
         BNZ   @DC00338                                            0340 01210000
