@RC00408 DS    0H                                                  0491 01950000
*                 END;                                             0491 01951000
*                                                                  0491 01952000
@RC00316 DS    0H                                                  0492 01953000
*               /*****************************************************/ 01954000
*               /*                                                   */ 01955000
*               /* SEGMENT GETERRID PUTS THE ERRORID INTO RTM2ERID IN*/ 01956000
*               /* RTM2WA. GETERRID DETERMINES IF A PREVIOUS ERRORID */ 01957000
*               /* SHOULD BE PROPAGATED OR A NEW ERRORID GENERATED.  */ 01958000
*               /* IF EED'S WERE PASSED TO RTM2, THE EEDCHAIN IS     */ 01959000
*               /* SEARCHED FOR AN ERRORID. IF FOUND, THE ERRORID IS */ 01960000
*               /* MOVED TO THE RTM2 WORKAREA AND RTM2ERFL IS SET ON */ 01961000
*               /* TO INDICATE AN ERRORID. OTHERWISE,IF RTM2PREV     */ 01962000
*               /* CONTAINS AN ADDRESS, THEN THE PREVIOUS RTM2WA IS  */ 01963000
*               /* CHECKED. IF AN ERRORID IS PRESENT, THE ASID, CPUID*/ 01964000
*               /* AND SEQUENCE NUMBER ARE MOVED TO THE CURRENT      */ 01965000
*               /* WORKAREA, AND RTM2ERFL IS SET ON. THE CURRENT TIME*/ 01966000
*               /* IS GOTTEN FROM THE TOD CLOCK. IF NO ERRORID COULD */ 01967000
*               /* BE PROPAGATED, A NEW ERRORID IS GENERATED.        */ 01968000
*               /*                                                   */ 01969000
*               /*****************************************************/ 01970000
*                                                                  0492 01971000
*               DO;                 /* BEGIN ERRORID PROCESS @G17EP5W*/ 01972000
@RC00256 DS    0H                                                  0493 01973000
*                 RFY                                              0493 01974000
*                   R10 RSTD;       /* RESTRICT REG FOR PTR  @G17EP5W*/ 01975000
*                 R10=ZERO;         /* ZERO ITS VALUE SO THAT A VALID   01976000
*                                      ADDR. CAN BE DETERMINED     0494 01977000
*                                                            @G17EP5W*/ 01978000
         SLR   R10,R10                                             0494 01979000
*                 IF ESAEEDQ>EEDNULL THEN/* IF EEDS WERE PASSED    0495 01980000
*                                                            @G17EP5W*/ 01981000
         L     @12,ESAEEDQ(,R7)                                    0495 01982000
         C     @12,@CF00067                                        0495 01983000
         BNH   @RF00495                                            0495 01984000
*                   DO;             /* CHECK FOR ERRORID     @G17EP5W*/ 01985000
*                     R10=ESAEEDQ;  /* ADDR. OF 1ST EED      @G17EP5W*/ 01986000
         LR    R10,@12                                             0497 01987000
*                     DO WHILE(R10>EEDNULL);/* PROCESS EED CHAIN   0498 01988000
*                                      UNTIL END OR ERRORID FOUND  0498 01989000
*                                                            @G17EP5W*/ 01990000
         B     @DE00498                                            0498 01991000
@DL00498 DS    0H                                                  0499 01992000
*                       IF R10->EEDERFL=ON THEN/* IF ERRORID IN EED     01993000
