         NI    ESASLIPR(R7),B'11111101'                            0752 02939000
*         END;                      /*                       @G17SP2I*/ 02940000
*       R13=ADDR(RTM2SFSA);         /* RESTORE THE SAVEAREA PTR TO 0754 02941000
*                                      THE SAME AS ON ENTRY  @G17SP2I*/ 02942000
@RF00725 LA    R13,RTM2SFSA(,R8)                                   0754 02943000
*       RTM2SCTC(RTM2SLPS)=OFF;     /* CLEAR THE SLIP SECTION FLAG TO   02944000
*                                      SHOW SLIP PROCESSING DONE   0755 02945000
*                                                            @G17SP2I*/ 02946000
         NI    RTM2SCTC+1(R8),B'11101111'                          0755 02947000
*     END;                          /*                       @G17SP2I*/ 02948000
*   RETURN;                         /*                       @G17SP2I*/ 02949000
@EL00003 DS    0H                                                  0757 02950000
@EF00003 DS    0H                                                  0757 02951000
@ER00003 LM    @14,@12,12(@13)                                     0757 02952000
         BR    @14                                                 0757 02953000
*   RFY                                                            0758 02954000
*     R13 UNRSTD;                   /*                       @G17SP2I*/ 02955000
*   END;                            /* END OF SLIP PROC      @G17SP2I*/ 02956000
*                                                                  0759 02957000
         B     @EL00003                                            0759 02958000
@PB00003 DS    0H                                                  0759 02959000
*   /*****************************************************************/ 02960000
*   /*                                                               */ 02961000
*   /* SEGMENT RT2CRERR CONTROLS TERMINATION AFTER AN UNRECOVERABLE  */ 02962000
*   /* ERROR OCCURS. IT RESTRICTS THE REGISTERS REQUIRED BY THE      */ 02963000
*   /* SYSTEM ROUTINES IT CALLS. IT THEN REQUESTS AN SVC DUMP UNLESS */ 02964000
*   /* SVC DUMP HAS ITSELF SUFFERED AN ERROR. SEGMENT RT2TMRY IS THEN*/ 02965000
*   /* INCLUDED TO REQUEST A MEMORY TERMINATION. THE REGISTERS NOT   */ 02966000
*   /* RELEASED BY RT2TMRY ARE THEN RELEASED. THE ENQ/LOCK CONDITIONS*/ 02967000
*   /* ON ENTRY ARE UNKNOWN. IT DOES NOT ASSUME ANY INPUT. ON        */ 02968000
*   /* COMPLETION OF ITS PROCESSING THE CURRENT TASK TREE IS NON     */ 02969000
*   /* DISPATCHABLE AND THE MEMORY IS TERMINATING                    */ 02970000
*   /*                                                               */ 02971000
*   /*****************************************************************/ 02972000
*                                                                  0760 02973000
*RT2CRERR:                                                         0760 02974000
*   DO;                             /* BEGIN SEGMENT RT2CRERR        */ 02975000
RT2CRERR DS    0H                                                  0761 02976000
*     RESPECIFY                                                    0761 02977000
*       R0 RESTRICTED;              /* USED AS A PARAMETER REGISTER     02978000
*                                      BY SERVICES CALLED            */ 02979000
*     RESPECIFY                                                    0762 02980000
*       R1 RESTRICTED;              /* USED AS A PARAMETER REGISTER  */ 02981000
*     RESPECIFY                                                    0763 02982000
*       R2 RESTRICTED;              /* USED BY CALLRTM AS A PARMETER    02983000
*                                      REGISTER                      */ 02984000
*     RESPECIFY                                                    0764 02985000
*       R11 RESTRICTED;             /* USED BY SETLOCK AS A PARAMETER   02986000
*                                      REGISTER                      */ 02987000
*     RESPECIFY                                                    0765 02988000
*       R12 RESTRICTED;             /* USED BY SETLOCK AS A PARAMETER   02989000
*                                      REGISTER                      */ 02990000
*     RESPECIFY                                                    0766 02991000
*       R13 RESTRICTED;             /* USED AS SAVEAREA REG FOR    0766 02992000
*                                      CALLRTM AND FOR PARAMETERS BY    02993000
*                                      SETLOCK                       */ 02994000
*     RESPECIFY                                                    0767 02995000
*       R14 RESTRICTED;             /* USED AS LINKAGE REGISTER BY 0767 02996000
*                                      SERVICES CALLED               */ 02997000
*     RESPECIFY                                                    0768 02998000
*       R15 RESTRICTED;             /* USED AS LINKAGE REGISTER BY 0768 02999000
*                                      SERVICES CALLED               */ 03000000
*     GENERATE(BALR 6,0);           /* ESTABLISH ADDRESSIBILITY      */ 03001000
         BALR 6,0                                                       03002000
*     GENERATE(USING *,6);                                         0770 03003000
         USING *,6                                                      03004000
*     R4=PSATOLD;                   /* SET CURRENT TCB ADDRESS IN  0771 03005000
*                                      TCBPTR                        */ 03006000
         L     R4,PSATOLD                                          0771 03007000
