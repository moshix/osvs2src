*         END;                                                     0794 03087000
*       ELSE                                                       0795 03088000
*         ;                         /* IF RECURSION IS IN SETLOCK, 0795 03089000
*                                      THE RESOURCE WILL NOT BE    0795 03090000
*                                      SERIALIZED THIS IS NOT AN   0795 03091000
*                                      EXPOSURE AS THE MEMORY WILL BE   03092000
*                                      TERMINATED                    */ 03093000
*                                                                  0795 03094000
@RF00785 DS    0H                                                  0796 03095000
*       /*************************************************************/ 03096000
*       /*                                                           */ 03097000
*       /* ESTABLISH INTERFACE FOR CALL TO RTM1                      */ 03098000
*       /*                                                           */ 03099000
*       /*************************************************************/ 03100000
*                                                                  0796 03101000
*       R2=ASCBASID;                /* INITIALIZE R2 WITH ASID TO  0796 03102000
*                                      INTERFACE WITH RTM1           */ 03103000
         L     @10,ASCBPTR(,R7)                                    0796 03104000
         LH    R2,ASCBASID(,@10)                                   0796 03105000
         N     R2,@CF03215                                         0796 03106000
*       R1=MTCC;                    /* INITIALIZE R1 WITH THE COMP.     03107000
*                                      CODE TO INTERFACE WITH RTM1   */ 03108000
         LA    R1,1037                                             0797 03109000
*       R13=WSALSTAE;               /* SET SAVE AREA ADDRESS IN R13  */ 03110000
         L     @10,ASXBPTR(,@10)                                   0798 03111000
         L     @10,ASXBSPSA(,@10)                                  0798 03112000
         L     R13,WSALSTAE(,@10)                                  0798 03113000
*       GENERATE REFS(R1,R2,R13,CVTBTERM,CVTMAP,CVTUSER) SETS(R0,R1,R2, 03114000
*           R13,R14,R15);                                          0799 03115000
          CALLRTM TYPE=MEMTERM,COMPCOD=(1),ASID=(2)                     03116000
*       IF ESAGLREC=OFF THEN        /* IF NO RECURSION IN SETLOCK  0800 03117000
*                                      RTM2 HOLDS THE LOCAL LOCK     */ 03118000
         TM    ESAGLREC(R7),B'00010000'                            0800 03119000
         BNZ   @RF00800                                            0800 03120000
*         DO;                       /* SETLOCK RELEASE TYPE(LOCAL)REL   03121000
*                                      ATED(WSALSTAE,IEAVTRT2(RT2CRER   03122000
*                                      R))                           */ 03123000
*           RESPECIFY                                              0802 03124000
*            (GPR11P,                                              0802 03125000
*             GPR12P,                                              0802 03126000
*             GPR13P,                                              0802 03127000
*             GPR14P) RESTRICTED;   /* RESTRICT REGS USED BY SETLOCK */ 03128000
