*                     TCBFPRAP=ON;  /* PROHIBIT PERCOLATION OF     0179 00508000
*                                      ASYNCHRONOUS ERRORS           */ 00509000
         OI    TCBFPRAP(R4),B'00010000'                            0179 00510000
*                     ESAGLREC=ON;                                 0180 00511000
         OI    ESAGLREC(R7),B'00010000'                            0180 00512000
*                     DO;           /* SETLOCK RELEASE TYPE(LOCAL)REL   00513000
*                                      ATED(TCB,IEAVTRT2(RT2INESA))  */ 00514000
*                       RESPECIFY                                  0182 00515000
*                        (GPR11P,                                  0182 00516000
*                         GPR12P,                                  0182 00517000
*                         GPR13P,                                  0182 00518000
*                         GPR14P) RESTRICTED;/* RESTRICT REGS USED BY   00519000
*                                      SETLOCK                       */ 00520000
