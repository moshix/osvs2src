         OI    RTM2RCRX(R8),B'10000000'                            0174 00535000
*                         RTM2SCTX(RTM2TRS)=ON;/* SET SECTION FLAG 0175 00536000
*                                      FOR EXIT                      */ 00537000
*                                                                  0175 00538000
         OI    RTM2SCTX+1(R8),B'00100000'                          0175 00539000
*                         /*******************************************/ 00540000
*                         /*                                         */ 00541000
*                         /* LOCATE PREVIOUS TASK RECOVERY WORKAREA  */ 00542000
*                         /* AND UPDATE RETURN ADDRESS AND RECURSION */ 00543000
*                         /* ADDRESS SO THAT EXECUTION WITH RESUME ON*/ 00544000
*                         /* CORRECT PATH (TASK RECOVERY IS CALLED   */ 00545000
*                         /* FROM BOTH RECOVERABLE AND UNRECOVERABLE */ 00546000
*                         /* PATHS.)                                 */ 00547000
