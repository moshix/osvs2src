*               RTM2SKRA=ADDR(NORETR);/* SETUP RECURSION ADDR      0580 02133000
*                                                            @G33SPHW*/ 02134000
         LA    @12,NORETR                                          0580 02135000
         ST    @12,RTM2SKRA(,R8)                                   0580 02136000
*               RFY                                                0581 02137000
*                 R1 RSTD;          /* PARM REGISTER         @G33SPHW*/ 02138000
*               R1=ADDR(RTM2WA);    /* PASS WORKAREA ADDRESS @G33SPHW*/ 02139000
         LR    R1,R8                                               0582 02140000
*               R13=ADDR(RTM2SFSA); /* SAVEAREA POINTER      @G33SPHW*/ 02141000
         LA    R13,RTM2SFSA(,R8)                                   0583 02142000
*               CALL IEAVTAS1;      /* INVOKE ASIR TO SETUP RETRY  0584 02143000
*                                                            @G33SPHW*/ 02144000
         L     @15,@CV00099                                        0584 02145000
         BALR  @14,@15                                             0584 02146000
*               RFY                                                0585 02147000
*                 R1 UNRSTD;        /* FREE PARM REG         @G33SPHW*/ 02148000
*               RTM2SKRA=0;         /* CLEAR RECURSION ADDR  @G33SPHW*/ 02149000
         SLR   @12,@12                                             0586 02150000
         ST    @12,RTM2SKRA(,R8)                                   0586 02151000
*               RTM2SCTC(RTM2SRBS)='0'B;/* CLEAR SECTION FLAG      0587 02152000
*                                                            @G33SPHW*/ 02153000
         NI    RTM2SCTC+1(R8),B'11111101'                          0587 02154000
*             END;                  /*                       @G33SPHW*/ 02155000
*         END;                      /* END SETUP OF RBS      @G33SPHW*/ 02156000
*       ELSE                        /*                       @G33SPHW*/ 02157000
*NORETR:                            /* RECURSION LABEL USED WHEN   0590 02158000
*                                      UNABLE TO SETUP RBS FOR RETRY.   02159000
*                                                            @G33SPHW*/ 02160000
*         RTM2RETR=OFF;             /* CLEAR RETRY FLAG TO PREVENT 0590 02161000
*                                      CONFUSION IN SUBSEQUENT     0590 02162000
*                                      SECTIONS (NEEDED ONLY ON    0590 02163000
*                                      RECURSION).           @G33SPHW*/ 02164000
         B     @RC00570                                            0590 02165000
@RF00570 DS    0H                                                  0590 02166000
NORETR   NI    RTM2RETR(R8),B'10111111'                            0590 02167000
*       IF RTM2FLX=NONE THEN        /* NO EXIT REQUESTED YET @G33SPHW*/ 02168000
@RC00570 CLC   RTM2FLX(2,R8),@CB00069                              0591 02169000
         BNE   @RF00591                                            0591 02170000
*         DO;                       /* CHECK IF RETRY ALLOWED      0592 02171000
*                                                            @G33SPHW*/ 02172000
*           IF TCBFA=OFF THEN       /* NOT ALREADY IN ABEND          */ 02173000
         TM    TCBFA(R4),B'10000000'                               0593 02174000
         BNZ   @RF00593                                            0593 02175000
*             DO;                                                  0594 02176000
*               ESAGLREC=ON;        /* SET RECURSION FLAG FOR GETLOCK*/ 02177000
         OI    ESAGLREC+96(R5),B'00010000'                         0595 02178000
*               DO;                 /* SETLOCK OBTAIN TYPE(LOCAL)MODE   02179000
*                                      (UNCOND)RELATED(TCB,IEAVTRTC(R   02180000
*                                      TCABCTL),IEAVTRTE(RTECMEX))   */ 02181000
*                 RESPECIFY                                        0597 02182000
*                  (GPR11P,                                        0597 02183000
*                   GPR12P,                                        0597 02184000
*                   GPR13P,                                        0597 02185000
*                   GPR14P) RESTRICTED;/* RESTRICT REGS USED BY    0597 02186000
*                                      SETLOCK                       */ 02187000
*                 GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P,GPR13P, 02188000
*                     GPR14P);                                     0598 02189000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        C02190000
               RELATED=(TCB,IEAVTRTC(RTCABCTL),IEAVTRTE(RTECMEX))       02191000
*                 RESPECIFY                                        0599 02192000
*                  (GPR11P,                                        0599 02193000
