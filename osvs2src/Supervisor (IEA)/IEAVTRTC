*                                      POST'S REGISTERS              */ 02919000
*                                     END;                         0756 02920000
*                                    ELSE                          0757 02921000
*                                     ;/* NO OTHER TASK STACKED    0757 02922000
*                                      BEHIND THIS ONE               */ 02923000
@RF00749 DS    0H                                                  0758 02924000
*                                    R9=R9+1;/* INCREMENT COUNT OF 0758 02925000
*                                      ECBS                          */ 02926000
         AL    R9,@CF00071                                         0758 02927000
*                                    RTM2ECBS(R9)=0;/* CLEAR ECB     */ 02928000
         LR    @12,R9                                              0759 02929000
         SLA   @12,2                                               0759 02930000
         SLR   @02,@02                                             0759 02931000
         ST    @02,RTM2ECBS-4(@12,R8)                              0759 02932000
*PTCBL1:                                                           0760 02933000
*                                    ;/* THE STATEMENT IS LABELLED TO   02934000
*                                      PREVENT THE USE OF A COMPILER    02935000
*                                      TEMPORARY                     */ 02936000
PTCBL1   DS    0H                                                  0761 02937000
*                                    RTM2LECB(R9)=OFF;/* CLEAR FLAG     02938000
