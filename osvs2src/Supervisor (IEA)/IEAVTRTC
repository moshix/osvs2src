               RELATED=(TCB,IEAVTRTC(RTCAOR))                           02318000
*                 RESPECIFY                                        0628 02319000
*                  (GPR11P,                                        0628 02320000
*                   GPR12P,                                        0628 02321000
*                   GPR13P,                                        0628 02322000
*                   GPR14P) UNRESTRICTED;/* RELEASE REGS USED BY   0628 02323000
*                                      SETLOCK                       */ 02324000
*               END;                /* SETLOCK RELEASE TYPE(LOCAL)REL   02325000
*                                      ATED(TCB,IEAVTRTC(RTCAOR))  0629 02326000
*                                      RELEASE LOCAL LOCK ACQUIRED TO   02327000
*                                      SERIALIZE TCB FLAGS           */ 02328000
*               ESAGLREC=OFF;       /* CLEAR RECURSION FLAG          */ 02329000
         NI    ESAGLREC+96(R5),B'11101111'                         0630 02330000
*             END;                                                 0631 02331000
*           ELSE                    /* LOCK NOT ACQUIRED IF TCBFA  0632 02332000
*                                      ALREADY ON                    */ 02333000
*             IF RTM2PURG=OFF THEN  /* NO ENTRY HAS BEEN FOR PURGE 0632 02334000
*                                      ONLY                          */ 02335000
         B     @RC00618                                            0632 02336000
@RF00618 TM    RTM2PURG(R8),B'10000000'                            0632 02337000
         BNZ   @RF00632                                            0632 02338000
*               TCBFT=ON;           /* INDICATE THIS IS TOP TASK IN     02339000
*                                      FAILING TREE                  */ 02340000
         OI    TCBFT(R4),B'00000100'                               0633 02341000
*             ELSE                                                 0634 02342000
*               ;                   /* IF PURGE ONLY,CURRENT IS NEVER   02343000
*                                      TOP TASK                      */ 02344000
*                                                                  0634 02345000
@RF00632 DS    0H                                                  0635 02346000
*           /*********************************************************/ 02347000
*           /*                                                       */ 02348000
*           /* SEGMENT RTCTEINT INTERFACES WITH TASK RECOVERY TERM   */ 02349000
*           /* EXIT PROCESSING. IT ESTABLISHES RECURSION CONTROL AND */ 02350000
*           /* THEN, IF ANY EXITS EXIST IT ENSURES THAT THE FULL     */ 02351000
*           /* RANGE OF EXITS WILL GET CONTROL. (EARLIER RECURSIONS  */ 02352000
*           /* MAY HAVE CAUSED RTM2 TO PERMIT ONLY EXITS ESTABLISHED */ 02353000
*           /* SINCE THE LAST ABEND TO GET CONTROL.) IT THEN SETS    */ 02354000
*           /* FLAGS FOR TERM EXIT PROCESSING AND INVOKES TASK       */ 02355000
*           /* RECOVERY. ON RETURN IT ENSURES THAT TCB FLAGS WHICH   */ 02356000
*           /* MAY HAVE BEEN ALTERED BY THIS PROCESSING ARE PROPERLY */ 02357000
*           /* SET                                                   */ 02358000
*           /*                                                       */ 02359000
*           /*********************************************************/ 02360000
*                                                                  0635 02361000
*           DO;                     /* BEGIN SEGMENT RTCTEINT        */ 02362000
@RC00618 DS    0H                                                  0636 02363000
*             IF TCBSTABB^=0 THEN   /* (E)STA EXITS EXIST            */ 02364000
         L     @12,TCBSTABB-1(,R4)                                 0636 02365000
         LA    @12,0(,@12)                                         0636 02366000
         LTR   @12,@12                                             0636 02367000
         BZ    @RF00636                                            0636 02368000
*               DO;                                                0637 02369000
*                                                                  0637 02370000
*                 /***************************************************/ 02371000
*                 /*                                                 */ 02372000
*                 /* PERMIT FULL RANGE OF TERM EXITS                 */ 02373000
*                 /*                                                 */ 02374000
*                 /***************************************************/ 02375000
*                                                                  0638 02376000
*                 RTM2SCBO=0;                                      0638 02377000
         SLR   @12,@12                                             0638 02378000
         ST    @12,RTM2SCBO(,R8)                                   0638 02379000
*                 RTM2SCBC=SCBSTART;                               0639 02380000
         MVC   RTM2SCBC(4,R8),@CF00071                             0639 02381000
*                 TCBETERM=ON;      /* INDICATE IN TERM EXITS VIA TCB   02382000
*                                      FOR SVC 60                    */ 02383000
         OI    TCBETERM(R4),B'10000000'                            0640 02384000
*                 RTM2CLUP=ON;      /* INDICATE EXITS MAY CLEAN UP 0641 02385000
*                                      ONLY                          */ 02386000
         OI    RTM2CLUP(R8),B'10000000'                            0641 02387000
*                 RTM2TRME=ON;      /* INDICATE IN TERM EXIT       0642 02388000
*                                      PROCESSING                    */ 02389000
         OI    RTM2TRME(R8),B'10000000'                            0642 02390000
*                 TCBFPRAP=OFF;     /* PERMIT ASYNCHRONOUS ABENDS TO    02391000
*                                      PERCOLATE TO THE TASK         */ 02392000
         NI    TCBFPRAP(R4),B'11101111'                            0643 02393000
*                 IF RTM2RECR=ON&RTM2SCTR(RTM2TRS)=ON THEN/* TERM  0644 02394000
*                                      EXIT PROCESSING HAS RECURSED  */ 02395000
         TM    RTM2RECR(R8),B'10000000'                            0644 02396000
         BNO   @RF00644                                            0644 02397000
         TM    RTM2SCTR+1(R8),B'00100000'                          0644 02398000
         BNO   @RF00644                                            0644 02399000
*                   DO;                                            0645 02400000
*                     RTM2RCRX=ON;  /* SET RECURSION EXIT FLAG       */ 02401000
         OI    RTM2RCRX(R8),B'10000000'                            0646 02402000
*                     RTM2SCTX(RTM2TRS)=ON;/* SET SECTION FLAG FOR 0647 02403000
*                                      EXIT                          */ 02404000
*                                                                  0647 02405000
         OI    RTM2SCTX+1(R8),B'00100000'                          0647 02406000
*                     /***********************************************/ 02407000
*                     /*                                             */ 02408000
*                     /* LOCATE PREVIOUS TASK RECOVERY WORKAREA AND  */ 02409000
*                     /* UPDATE RETURN ADDRESS AND RECURSION ADDRESS */ 02410000
*                     /* SO THAT EXECUTION WILL RESUME ON CORRECT    */ 02411000
*                     /* PATH (TASK RECOVERY IS CALLED FROM BOTH     */ 02412000
*                     /* RECOVERABLE AND UNRECOVERABLE PATHS.)       */ 02413000
*                     /*                                             */ 02414000
*                     /***********************************************/ 02415000
*                                                                  0648 02416000
*                     RESPECIFY                                    0648 02417000
*                       R9 RESTRICTED;                             0648 02418000
*                     R9=RTM2PREV;  /* START WITH IMMEDIATELY      0649 02419000
*                                      PREVIOUS WORK AREA            */ 02420000
         L     R9,RTM2PREV(,R8)                                    0649 02421000
*                     RTM2PRWA=0;   /* CLEAR FIELD FROM PREVIOUS   0650 02422000
*                                      RECUR                         */ 02423000
         ST    @12,RTM2PRWA(,R8)                                   0650 02424000
*                     DO WHILE(RTM2PRWA=0);                        0651 02425000
         B     @DE00651                                            0651 02426000
@DL00651 DS    0H                                                  0652 02427000
*                       IF RTM2SCTX=R9->RTM2SCTC THEN/* WORK AREA  0652 02428000
*                                      BELONGED TO TASK RECOVERY     */ 02429000
         CLC   RTM2SCTX(4,R8),RTM2SCTC(R9)                         0652 02430000
         BNE   @RF00652                                            0652 02431000
*                         RTM2PRWA=R9;/* SAVE ADDR OF WORK AREA      */ 02432000
         ST    R9,RTM2PRWA(,R8)                                    0653 02433000
*                       ELSE        /* WORK AREA DID NOT BELONG TO 0654 02434000
*                                      TASK RECOVERY                 */ 02435000
*                         R9=R9->RTM2PREV;/* LOCATE PREVIOUS WORKAREA*/ 02436000
         B     @RC00652                                            0654 02437000
@RF00652 L     R9,RTM2PREV(,R9)                                    0654 02438000
*                     END;          /* END OF SCAN FOR PREVIOUS ASIR    02439000
*                                      WORK AREA                     */ 02440000
@RC00652 DS    0H                                                  0655 02441000
@DE00651 L     @12,RTM2PRWA(,R8)                                   0655 02442000
         LTR   @12,@12                                             0655 02443000
         BZ    @DL00651                                            0655 02444000
*                     RESPECIFY                                    0656 02445000
*                       R9 UNRESTRICTED;                           0656 02446000
*                     RTM2PRWA->RTM2SKRA=ADDR(RTCTEREC);/* UPDATE  0657 02447000
*                                      RECURSION ADDR                */ 02448000
         LA    @09,RTCTEREC                                        0657 02449000
         ST    @09,RTM2SKRA(,@12)                                  0657 02450000
*                     RTM2PRWA->RTM2SFSA(4)=ADDR(LTERMEX);/* UPDATE     02451000
*                                      RETURN ADDR IN R14 SLOT IN  0658 02452000
*                                      SAVE AREA                     */ 02453000
         LA    @09,LTERMEX                                         0658 02454000
         ST    @09,RTM2SFSA+12(,@12)                               0658 02455000
*                   END;                                           0659 02456000
*                 ELSE                                             0660 02457000
*                   DO;                                            0660 02458000
         B     @RC00644                                            0660 02459000
@RF00644 DS    0H                                                  0661 02460000
*                     RTM2SCTC(RTM2TRS)=ON;/* SET SECTION FLAG FOR 0661 02461000
*                                      RECURSION CONTROL             */ 02462000
         OI    RTM2SCTC+1(R8),B'00100000'                          0661 02463000
*                     GENERATE REFS(RTM2SFSA,R8);                  0662 02464000
          STM   14,12,RTM2SFSA+12(R8) - SAVE REGISTERS                  02465000
*                     RTM2SKRA=ADDR(RTCTEREC);/* SET RECURSION     0663 02466000
*                                      ADDRESS                       */ 02467000
         LA    @12,RTCTEREC                                        0663 02468000
         ST    @12,RTM2SKRA(,R8)                                   0663 02469000
*                     RESPECIFY                                    0664 02470000
*                      (R1) RESTRICTED;/* RESTRICT REGS USED IN    0664 02471000
*                                      INTERFACE TO TERM EXIT      0664 02472000
*                                      PROCESSING                    */ 02473000
*                     R1=ADDR(RTM2WA);/* SET R1 WITH ADDRESS OF    0665 02474000
*                                      RTM2WA                        */ 02475000
         LR    R1,R8                                               0665 02476000
*                     R13=ADDR(RTM2SFSA);/* SET R13 WITH SAVE AREA 0666 02477000
*                                      ADDRESS                       */ 02478000
         LA    R13,RTM2SFSA(,R8)                                   0666 02479000
*                     CALL IEAVTAS1;/* INVOKE TERM EXIT PROCESSOR    */ 02480000
         L     @15,@CV00099                                        0667 02481000
         BALR  @14,@15                                             0667 02482000
*LTERMEX:                                                          0668 02483000
*                     ;             /* RETURN PT FOR IEAVTAS1 ON   0668 02484000
*                                      UNRECOVERABLE PATH            */ 02485000
LTERMEX  DS    0H                                                  0669 02486000
*                     RESPECIFY                                    0669 02487000
*                      (R1) UNRESTRICTED;/* RELEASE REGS USED IN TERM   02488000
*                                      EXIT PROCESSOR INTERFACE      */ 02489000
*                     TCBCMPC=RTM2CC;/* COPY COMPLETION CODE WHICH 0670 02490000
*                                      MAY HAVE BEEN UPDATED BY EXIT    02491000
*                                                             @YM4164*/ 02492000
         MVC   TCBCMPC(3,R4),RTM2CC(R8)                            0670 02493000
*                   END;                                           0671 02494000
*               END;                                               0672 02495000
*             ELSE                                                 0673 02496000
*               ;                   /* NO EXITS EXIST                */ 02497000
@RF00636 DS    0H                                                  0674 02498000
*             RTM2SCTC(RTM2TRS)=OFF;/* CLEAR SECTION FLAG            */ 02499000
@RC00636 NI    RTM2SCTC+1(R8),B'11011111'                          0674 02500000
*             RTM2SKRA=0;           /* CLEAR RECURSION ADDRESS       */ 02501000
         SLR   @12,@12                                             0675 02502000
         ST    @12,RTM2SKRA(,R8)                                   0675 02503000
*RTCTEREC:                                                         0676 02504000
*             ;                                                    0676 02505000
RTCTEREC DS    0H                                                  0677 02506000
*             IF RTM2FLX=NONE THEN                                 0677 02507000
         CLC   RTM2FLX(2,R8),@CB00069                              0677 02508000
         BNE   @RF00677                                            0677 02509000
*               DO;                                                0678 02510000
*                 TCBFPRAP=ON;      /* PREVENT PERCOLATION OF      0679 02511000
*                                      ASYNCHRONOUS ABENDS           */ 02512000
         OI    TCBFPRAP(R4),B'00010000'                            0679 02513000
*                 TCBFX=ON;         /* PREVENT ASYNCHRONOUS EXITS    */ 02514000
         OI    TCBFX(R4),B'00000001'                               0680 02515000
*                 TCBATT=ON;                                       0681 02516000
         OI    TCBATT(R4),B'00100000'                              0681 02517000
*                 TCBETERM=OFF;     /* PERMIT RESOURCE MGRS TO ISSUE    02518000
*                                      ESTAE WITH TERM OPTION        */ 02519000
         NI    TCBETERM(R4),B'01111111'                            0682 02520000
*               END;                                               0683 02521000
*           END;                    /* END SEGMENT RTCTEINT          */ 02522000
@RF00677 DS    0H                                                  0685 02523000
*           IF RTM2PURG=OFF         /* NO ENTRY HAS BEEN FOR PURGE 0685 02524000
*                                      ONLY                          */ 02525000
*               &TCBCNCB=OFF THEN   /* NO ENTRY HAS BEEN EXPRESS     */ 02526000
         TM    RTM2PURG(R8),B'10000000'                            0685 02527000
         BNZ   @RF00685                                            0685 02528000
         TM    TCBCNCB(R4),B'10000000'                             0685 02529000
         BNZ   @RF00685                                            0685 02530000
*             IF RTM2FLX=NONE THEN  /* EXIT HAS NOT BEEN REQUESTED   */ 02531000
         CLC   RTM2FLX(2,R8),@CB00069                              0686 02532000
         BNE   @RF00686                                            0686 02533000
*               IF TCBLTC^=0 THEN   /* SUBTASKS EXIST                */ 02534000
         L     @12,TCBLTC(,R4)                                     0687 02535000
         LTR   @12,@12                                             0687 02536000
         BZ    @RF00687                                            0687 02537000
*                 DO;                                              0688 02538000
*                   IF RTM2RECR='1'B&RTM2SCTR(RTM2STKS)='1'B THEN/*     02539000
*                                      RECURSION IN THIS SECTION     */ 02540000
         TM    RTM2RECR(R8),B'10000000'                            0689 02541000
         BNO   @RF00689                                            0689 02542000
         TM    RTM2SCTR+1(R8),B'10000000'                          0689 02543000
         BNO   @RF00689                                            0689 02544000
*                     DO;                                          0690 02545000
*                       RTM2RCRX='1'B;/* SET RECURSION EXIT FLAG     */ 02546000
         OI    RTM2RCRX(R8),B'10000000'                            0691 02547000
*                       RTM2SCTX(RTM2STKS)='1'B;/* SET SECTION FLAG     02548000
*                                      FOR EXIT                      */ 02549000
         OI    RTM2SCTX+1(R8),B'10000000'                          0692 02550000
*                     END;                                         0693 02551000
*                   ELSE            /* NO RECURSION IN THIS SECTION  */ 02552000
*                     DO;                                          0694 02553000
         B     @RC00689                                            0694 02554000
@RF00689 DS    0H                                                  0695 02555000
*                       RTM2SCTC(RTM2STKS)='1'B;/* SET CURRENT     0695 02556000
*                                      SECTION FLAG                  */ 02557000
         OI    RTM2SCTC+1(R8),B'10000000'                          0695 02558000
*                       GEN REFS(R8) SETS(RTM2SFSA)                0696 02559000
*                           (STM R14,R12,RTM2SFSA+12(R8));/* SAVE  0696 02560000
*                                      REGISTERS                     */ 02561000
         STM R14,R12,RTM2SFSA+12(R8)                                    02562000
*                       RTM2SKRA=ADDR(RCR0031);/* SET UP RECURSION 0697 02563000
*                                      ADDRESS ESTABLISH RECURSION 0697 02564000
*                                      CONTROL                       */ 02565000
         LA    @12,RCR0031                                         0697 02566000
         ST    @12,RTM2SKRA(,R8)                                   0697 02567000
*/********************************************************************/ 02568000
*/*                                                                  */ 02569000
*/* PRELIMINARY                                                      */ 02570000
*/*    THE LOCAL LOCK IS OBTAINED TO PREVENT ANY TASK FROM CHANGING  */ 02571000
*/*    ITS STATUS WITH RESPECT TO RTM2.  (THE TCB FLAGS CONTROLLING  */ 02572000
*/*    STATUS IN RTM2 ARE ALL SERIALIZED BY THE LOCAL LOCK.)  THE    */ 02573000
*/*    COUNT OF TASKS ELIGIBLE FOR STACKING IS INITIALIZED TO 0.     */ 02574000
*/*                                                                  */ 02575000
*/* TREE SCAN                                                        */ 02576000
*/*    THE SUBTASK TREE IS SCANNED FROM TOP TO BOTTOM.  WHEN A TASK  */ 02577000
*/*    IN RTM2 IS FOUND(TCBRTM2=1), PROCESSING IS PERFORMED AS       */ 02578000
*/*    DESCRIBED BELOW AND ITS SUBTASK TREE IS IGNORED.  A TASK IS   */ 02579000
*/*    FORCED INTO RTM2 IF IT IS IN STEP-MUST-COMPLETE OR IS         */ 02580000
*/*    NONDISPATCHABLE DUE TO STEP-MUST-COMPLETE.  THE SCAN STOPS    */ 02581000
*/*    WHEN ALL TASKS HAVE BEEN SCANNED OR WHEN THE COUNT OF TASKS   */ 02582000
*/*    ELIGIBLE FOR STACKING REACHES THE NUMBER OF ECBS AVAILABLE    */ 02583000
*/*    FOR STACKING(FOUR).                                           */ 02584000
*/*                                                                  */ 02585000
*/* PROCESSING FOR TASK IN SMC                                       */ 02586000
*/*    TASKS IN STEP-MUST-COMPLETE MUST BE FORCED TO TERMINATE TO    */ 02587000
*/*    PREVENT THE CURRENT TASK FROM CAUSING AN INTERLOCK BY STOPPING*/ 02588000
*/*    THE TASK TREE AND THEN DETACHING ITS LOWEST SUBTASK, WHICH    */ 02589000
*/*    WOULD BE NONDISPATCHABLE.  THE JOBSTEP TASK WHICH IS          */ 02590000
*/*    NONDISPATCHABLE BECAUSE A SUBTASK IS IN SMC IS TERMINATED     */ 02591000
*/*    FOR A SIMILAR REASON.  THE TASK WILL END WHEN ITS SUBTASK     */ 02592000
*/*    RELEASES ITS STEP-MUST-COMPLETE.  SUCH TASKS ARE ABTERMED IF  */ 02593000
*/*    THEY ARE NOT ALREADY IN RTM2.  THEY ARE TREATED AS IF THEY    */ 02594000
*/*    WERE IN RTM2.                                                 */ 02595000
*/*                                                                  */ 02596000
*/* PROCESSING FOR TASK IN RTM2                                      */ 02597000
*/*    IF ANOTHER TASK IS STACKED BEHIND THIS TASK, THE ECB LOCATED  */ 02598000
*/*    BY THE TCBECB FIELD IS POSTED.  THE ANCESTOR TASK WILL SCAN   */ 02599000
*/*    ITS SUBTASK TREE AGAIN AND THIS TIME WILL STACK BEHIND THE    */ 02600000
*/*    CURRENT TASK.  THE TCBECB FIELD OF THE SUBTASK IS THUS FREED  */ 02601000
*/*    FOR THE CURRENT TASK'S USE.                                   */ 02602000
*/*                                                                  */ 02603000
*/*    STACKING CONSISTS OF INCREMENTING THE COUNT OF TASKS          */ 02604000
*/*    ELIGIBLE FOR STACKING(ECB'S USED), CLEARING THE END-OF-LIST   */ 02605000
*/*    FLAG FOR THE NEXT ECB, CLEARING THE NEXT ECB, CHANGING THE    */ 02606000
*/*    TCBECB FIELD IN THE SUBTASK TO POINT TO THE NEXT ECB IN THE   */ 02607000
*/*    LIST, AND TURNING ON THE TCBFMW FLAG IN THE SUBTASK'S TCB.    */ 02608000
*/*    THEN THE SCAN OF THE SUBTASK TREE CONTINUES, IGNORING         */ 02609000
*/*    SUBTASKS OF THE TASK JUST FOUND.                              */ 02610000
*/*                                                                  */ 02611000
*/* WAIT PROCESSING                                                  */ 02612000
*/*    AFTER ALL SUBTASKS HAVE BEEN EXAMINED (OR ALL ECBS HAVE BEEN  */ 02613000
*/*    USED), THE COUNT OF TASKS PREPARED FOR STACKING IS EXAMINED.  */ 02614000
*/*    IF THE COUNT IF NOT ZERO, THE END-OF-LIST FLAG IS TURNED ON   */ 02615000
*/*    FOR THE LAST ECB, THE LOCAL LOCK IS RELEASED, AND A WAIT IS   */ 02616000
*/*    ISSUED ON THE ECB LIST FOR THE COUNT OF ECBS.  WHEN           */ 02617000
*/*    PROCESSING IS RESUMED, CONTROL IS RETURNED TO THE TOP OF      */ 02618000
*/*    THIS SEGMENT TO CHECK FOR MORE SUBTASKS IN RTM2.              */ 02619000
*/*                                                                  */ 02620000
*/* CONCLUSION                                                       */ 02621000
*/*    WHEN THE COUNT IS ZERO, THERE ARE NO TASKS IN RTM2.  THE      */ 02622000
*/*    LOCAL LOCK IS NOT RELEASED:  IT IS HELD UNTIL SUBTASKS HAVE   */ 02623000
*/*    BEEN SET NONDISPATCHABLE.                                     */ 02624000
*/*                                                                  */ 02625000
*/********************************************************************/ 02626000
*                                                                  0698 02627000
*                       /*********************************************/ 02628000
*                       /*                                           */ 02629000
*                       /* A SUBTASK CURRENTLY IN RTM2 CANNOT BE     */ 02630000
*                       /* INTERRUPTED BECAUSE IT COULD BE IN THE    */ 02631000
*                       /* MIDDLE OF A TERM EXIT OR A RESOURCE       */ 02632000
*                       /* MANAGER. SUCH SUBTASKS ARE INFORMED BY A  */ 02633000
*                       /* BIT IN THEIR TCB THAT AN ANCESTOR IS      */ 02634000
*                       /* WAITING THEM. THE ANCESTOR TASK WAITS ON  */ 02635000
*                       /* THE SUBTASK'S ATTACH ECB FOR THE SUBTASK  */ 02636000
*                       /* TO COMPLETE. WHEN THE BIT IS ON IN ITS    */ 02637000
*                       /* TCB, THE SUBTASK IS NOT ALLOWED TO RETRY: */ 02638000
*                       /* THE SUBTASK MUST TERMINATE. EACH TASK IN  */ 02639000
*                       /* RTM2 HANDLES ITS OWN SUBTASKS. THEREFORE, */ 02640000
*                       /* NO SUBTASK OF A TASK IN RTM2 IS EXAMINED. */ 02641000
*                       /*                                           */ 02642000
*                       /*********************************************/ 02643000
*                                                                  0698 02644000
*                       DO;         /* START OF SEGMENT RTCSTACK     */ 02645000
*                                                                  0698 02646000
*                         /*******************************************/ 02647000
*                         /*                                         */ 02648000
*                         /* LOCATE TASKS IN RTM2 OR                 */ 02649000
*                         /* STEP-MUST-COMPLETE PROCESSING           */ 02650000
*                         /*                                         */ 02651000
*                         /*******************************************/ 02652000
*                                                                  0699 02653000
*                         RESPECIFY                                0699 02654000
*                           R9 RESTRICTED;/* RESTRICT REGISTER FOR 0699 02655000
*                                      COUNT                         */ 02656000
*                         R9=NONZERO;/* SET CONDITION TO FORCE LOOP     02657000
*                                      TO BE ENTERED                 */ 02658000
*                                                                  0700 02659000
         LA    R9,1                                                0700 02660000
*                         /*******************************************/ 02661000
*                         /*                                         */ 02662000
*                         /* SCAN TREE UNTIL NO TASKS FOUND IN RTM2  */ 02663000
*                         /*                                         */ 02664000
*                         /*******************************************/ 02665000
*                                                                  0701 02666000
*                         DO WHILE(R9^=0);                         0701 02667000
         B     @DE00701                                            0701 02668000
@DL00701 DS    0H                                                  0702 02669000
*                           R9=0;   /* SET COUNT TO ZERO. IF TASK  0702 02670000
*                                      FOUND TO STACK BEHIND IT WILL    02671000
*                                      BE INCREMENTED                */ 02672000
*                                                                  0702 02673000
         SLR   R9,R9                                               0702 02674000
*                           /*****************************************/ 02675000
*                           /*                                       */ 02676000
*                           /* OBTAIN LOCAL LOCK TO PREVENT SUBTASKS */ 02677000
*                           /* FROM ENTERING OR LEAVING RTM2 WHILE   */ 02678000
*                           /* TREE SCAN IS TAKING PLACE             */ 02679000
*                           /*                                       */ 02680000
*                           /*****************************************/ 02681000
*                                                                  0703 02682000
*                           ESAGLREC=ON;/* INDICATE SETLOCK IN     0703 02683000
*                                      CONTROL                       */ 02684000
         OI    ESAGLREC+96(R5),B'00010000'                         0703 02685000
*                           DO;     /* SETLOCK OBTAIN TYPE(LOCAL)MODE   02686000
*                                      (UNCOND)RELATED(TCB,IEAVTRTC(R   02687000
*                                      TCSTACK,RTCCSUB))             */ 02688000
*                             RESPECIFY                            0705 02689000
*                              (GPR11P,                            0705 02690000
*                               GPR12P,                            0705 02691000
*                               GPR13P,                            0705 02692000
*                               GPR14P) RESTRICTED;/* RESTRICT REGS     02693000
*                                      USED BY SETLOCK               */ 02694000
*                             GENERATE REFS(PSALITA,FLC) SETS(GPR11P,   02695000
*                                 GPR12P,GPR13P,GPR14P);           0706 02696000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        C02697000
               RELATED=(TCB,IEAVTRTC(RTCSTACK,RTCCSUB))                 02698000
*                             RESPECIFY                            0707 02699000
*                              (GPR11P,                            0707 02700000
*                               GPR12P,                            0707 02701000
*                               GPR13P,                            0707 02702000
*                               GPR14P) UNRESTRICTED;/* RELEASE REGS    02703000
*                                      USED BY SETLOCK               */ 02704000
*                           END;    /* SETLOCK OBTAIN TYPE(LOCAL)MODE   02705000
*                                      (UNCOND)RELATED(TCB,IEAVTRTC(R   02706000
*                                      TCSTACK,RTCCSUB))             */ 02707000
*                           ESAGLREC=OFF;/* SETLOCK COMPLETED      0709 02708000
*                                      SUCCESSFULLY                  */ 02709000
         NI    ESAGLREC+96(R5),B'11101111'                         0709 02710000
*                           RESPECIFY                              0710 02711000
*                             R7 RESTRICTED;/* RESTRICT REGISTER USED   02712000
*                                      FOR TCB TREE SCAN             */ 02713000
*                           R7=R4;  /* INITIALIZE SCAN REGISTER TO 0711 02714000
*                                      CURRENT TCB                   */ 02715000
*                                                                  0711 02716000
         LR    R7,R4                                               0711 02717000
*                           /*****************************************/ 02718000
*                           /*                                       */ 02719000
*                           /* SCAN TREE UNTIL IT IS EXHAUSTED OR ALL*/ 02720000
*                           /* STACKING ECBS HAVE BEEN USED          */ 02721000
*                           /*                                       */ 02722000
*                           /*****************************************/ 02723000
*                                                                  0712 02724000
*                           RTM2RSCN=OFF;/* RTM2 TCB NOT YET REACHED */ 02725000
         NI    RTM2RSCN(R8),B'01111111'                            0712 02726000
*                           DO WHILE(R9<MAXECBS&R7^=0);            0713 02727000
         B     @DE00713                                            0713 02728000
@DL00713 DS    0H                                                  0714 02729000
*                             RESPECIFY                            0714 02730000
*                               TCB BASED(R7);                     0714 02731000
*                                                                  0714 02732000
*                             /***************************************/ 02733000
*                             /*                                     */ 02734000
*                             /* SEGMENT RTCFTCB SCANS A TASK TREE   */ 02735000
*                             /* AND RETURNS THE ADDRESS OF A SINGLE */ 02736000
*                             /* SUBTASK IF ANY EXIST. MULTIPLE CALLS*/ 02737000
*                             /* WILL CAUSE EVERY SUBTASK OF A TREE  */ 02738000
*                             /* TO BE RETURNED. HOWEVER, IF RTM2RSCN*/ 02739000
*                             /* IS ON THEN THE SUBTASK TREE DEFINED */ 02740000
*                             /* BY THE INPUT TCB IS NOT SEARCHED.   */ 02741000
*                             /*                                     */ 02742000
*                             /***************************************/ 02743000
*                                                                  0715 02744000
*                             DO;   /* BEGIN SEGMENT RTCFTCB         */ 02745000
*                               RTM2CONT=ON;/* INITIALIZE CONTROL  0716 02746000
*                                      FLAG                          */ 02747000
         OI    RTM2CONT(R8),B'00000001'                            0716 02748000
*                               RESPECIFY                          0717 02749000
*                                 R11 RESTRICTED;/* RESTRICT REG USED   02750000
*                                      BY FIND TCB                   */ 02751000
*                               R11=R7;/* SET WORKTCB REG EQUAL TO 0718 02752000
*                                      THE INPUT TCB                 */ 02753000
         LR    R11,R7                                              0718 02754000
*                               IF RTM2RSCN=OFF THEN/* CONTINUE SCAN,   02755000
*                                      INPUT TCB NOT IN RTM2 NOR STEP   02756000
*                                      MUST COMPLETE                 */ 02757000
         TM    RTM2RSCN(R8),B'10000000'                            0719 02758000
         BNZ   @RF00719                                            0719 02759000
*                                 R7=R11->TCBLTC;/* SET INPUT TCB REG   02760000
*                                      EQUAL TO DAUGHTER OF INPUT TCB*/ 02761000
         L     R7,TCBLTC(,R11)                                     0720 02762000
*                               ELSE/* INPUT TCB IN RTM2 OR STEP MUST   02763000
*                                      COMPLETE. FORCE SUBSEQUENT  0721 02764000
*                                      CODE TO BELIEVE IT HAS NO   0721 02765000
*                                      DAUGHTERS                     */ 02766000
*                                 R7=0;                            0721 02767000
         B     @RC00719                                            0721 02768000
@RF00719 SLR   R7,R7                                               0721 02769000
*                               DO WHILE(RTM2CONT=ON);/* LOOP UNTIL     02770000
*                                      NEXT TCB IN THE TREE FOUND OR    02771000
*                                      THE TREE IS EXHAUSTED         */ 02772000
@RC00719 B     @DE00722                                            0722 02773000
@DL00722 DS    0H                                                  0723 02774000
*                                 IF R7=0 THEN/* NEXT TCB NOT FOUND  */ 02775000
         LTR   R7,R7                                               0723 02776000
         BNZ   @RF00723                                            0723 02777000
*                                   DO;                            0724 02778000
*                                    R7=R11->TCBNTC;/* SET EQUAL TO     02779000
*                                      SISTER                        */ 02780000
         L     R7,TCBNTC(,R11)                                     0725 02781000
*                                    IF R11^=RTM2TCBT THEN/* SCAN NOT   02782000
*                                      YET DONE                      */ 02783000
         C     R11,RTM2TCBT(,R8)                                   0726 02784000
