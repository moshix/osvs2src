*                         RTM2SCTC(RTM2EPS)='1'B;/* SET CURRENT    0359 01257000
*                                      SECTION FLAG                  */ 01258000
         OI    RTM2SCTC(R8),B'00000010'                            0359 01259000
*                         GEN REFS(R8) SETS(RTM2SFSA)              0360 01260000
*                             (STM R14,R12,RTM2SFSA+12(R8));/* SAVE     01261000
*                                      REGISTERS                     */ 01262000
         STM R14,R12,RTM2SFSA+12(R8)                                    01263000
*                         RTM2SKRA=ADDR(RCR0018);/* SET UP RECURSION    01264000
*                                      ADDRESS                       */ 01265000
