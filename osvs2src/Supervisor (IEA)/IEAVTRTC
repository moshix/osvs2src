*                             RTM2DEND=ON;/* FORCE EXIT FROM LOOP  0146 00427000
*                                                            @YM03570*/ 00428000
         OI    RTM2DEND(R8),B'01000000'                            0146 00429000
*                           END;    /*                       @YM03570*/ 00430000
