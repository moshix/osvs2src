*                                                                  0265 00873000
         NI    ESAGLREC+96(R5),B'11101111'                         0265 00874000
*                           /*****************************************/ 00875000
*                           /*                                       */ 00876000
*                           /* SEGMENT RTCFIQE FLAGS ALL IQES RELATED*/ 00877000
*                           /* TO THE CURRENT TASK TO PREVENT THEIR  */ 00878000
*                           /* BEING SCHEDULED. THE CHAIN OF IQES    */ 00879000
*                           /* FROM THE ASXB IS SCANNED, AND EACH    */ 00880000
*                           /* IQE'S TCB POINTER IS COMPARED TO THE  */ 00881000
*                           /* ADDRESS OF THE CURRENT TCB. IF IT     */ 00882000
*                           /* MATCHES, THE IQE IS FLAGGED. THE LOCAL*/ 00883000
*                           /* LOCK MUST BE HELD TO PREVENT IQES FROM*/ 00884000
*                           /* BEING ADDED TO THE QUEUE              */ 00885000
*                           /*                                       */ 00886000
*                           /*****************************************/ 00887000
*                                                                  0266 00888000
*                           DO;     /* BEGIN SEGMENT RTCFIQE         */ 00889000
*                             RESPECIFY                            0267 00890000
*                               R10 RESTRICTED;/* RESTRICT IQE     0267 00891000
*                                      REGISTER                      */ 00892000
*                             R10=ASXBFIQE;/* SET REG TO ADDR OF   0268 00893000
*                                      FIRST IQE                     */ 00894000
