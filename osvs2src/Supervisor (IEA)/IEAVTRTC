         B     @DE00823                                            0823 03166000
@DL00823 DS    0H                                                  0824 03167000
*                           CALL IEATRSCN;/* LOCATE NEXT TCB ON TREE */ 03168000
         L     @12,CVTPTR                                          0824 03169000
         L     @15,CVTRSCN(,@12)                                   0824 03170000
         BALR  @14,@15                                             0824 03171000
*                           IF R10->TCBFC^=ON THEN                 0825 03172000
         TM    TCBFC(R10),B'10000000'                              0825 03173000
         BO    @RF00825                                            0825 03174000
*                             DO;                                  0826 03175000
*                               R10->TCBFA=ON;/* PREVENT TASK FROM 0827 03176000
*                                      EVER BEING RECOVERED          */ 03177000
         OI    TCBFA(R10),B'10000000'                              0827 03178000
*                               R10->TCBEOTFM=ON;/* INDICATE       0828 03179000
*                                      END-OF-TASK FOR FREEMAIN    0828 03180000
*                                                            @YM01942*/ 03181000
