         BNO   @RF00320                                            0320 01119000
         TM    RTM2SCTR(R8),B'00000100'                            0320 01120000
         BNO   @RF00320                                            0320 01121000
*                 DO;                                              0321 01122000
*                   RTM2RCRX='1'B;  /* SET RECURSION EXIT FLAG       */ 01123000
         OI    RTM2RCRX(R8),B'10000000'                            0322 01124000
