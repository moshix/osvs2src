         BE    @RF00163                                            0163 00501000
         CLC   RTM2CC(3,R8),@CB00059                               0163 00502000
         BE    @RF00163                                            0163 00503000
*                   DO;                                            0164 00504000
*                     IF RTM2CC=DET33E THEN/* COMP CODE IS SYSTEM  0165 00505000
*                                      33E, ONLY CLEAN UP IS ALLOWED */ 00506000
         CLC   RTM2CC(3,R8),@CB00061                               0165 00507000
         BNE   @RF00165                                            0165 00508000
*                       TCB33E=ON;  /* SET TCB FLAG TO REMEMBER    0166 00509000
*                                      CONDITION ON RECURSION        */ 00510000
         OI    TCB33E(R4),B'00100000'                              0166 00511000
*                     ELSE                                         0167 00512000
*                       ;           /* THIS ENTRY NOT 33E            */ 00513000
@RF00165 DS    0H                                                  0168 00514000
*                     IF TCB33E=ON THEN/* SOME ENTRY WAS A 33E ABEND */ 00515000
