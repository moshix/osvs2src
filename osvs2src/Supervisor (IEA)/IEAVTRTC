*                           R8=R4;  /* ADDR OF TOP TCB IN TREE       */ 01325000
         LR    R8,R4                                               0371 01326000
*                           R10=R4; /* ADDR OF FIRST TCB             */ 01327000
         LR    R10,R4                                              0372 01328000
*                           R11=ADDR(LCNCL1);/* RETURN ADDRESS TO BE    01329000
*                                      USED AT END OF SCAN           */ 01330000
         LA    R11,LCNCL1                                          0373 01331000
*                           DO WHILE(R10^=0);/* WILL LOOP UNTIL TRSCN   01332000
*                                      BRANCHES OUT                  */ 01333000
         B     @DE00374                                            0374 01334000
@DL00374 DS    0H                                                  0375 01335000
*                             CALL IEATRSCN;/* LOCATE NEXT TCB ON  0375 01336000
*                                      TREE                          */ 01337000
