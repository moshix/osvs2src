@RF00602 TM    TCBFDW(R4),B'00100000'                              0604 02221000
         BNO   @RF00604                                            0604 02222000
         TM    RTM2AS1R(R8),B'00000100'                            0604 02223000
         BNZ   @RF00604                                            0604 02224000
*                   DO;                                            0605 02225000
*                     RTM2RETR=OFF; /* PROHIBIT RETRY                */ 02226000
         NI    RTM2RETR(R8),B'10111111'                            0606 02227000
*                     RTM2DWX=ON;   /* TAKE DAUGHTER WAITING EXIT    */ 02228000
         OI    RTM2DWX(R8),B'00010000'                             0607 02229000
*                   END;                                           0608 02230000
*                 ELSE                                             0609 02231000
*                   IF RTM2RETR=ON THEN/* RETRY HAS BEEN REQUESTED 0609 02232000
*                                      AND IS PERMITTED              */ 02233000
