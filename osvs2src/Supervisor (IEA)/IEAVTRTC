*                                    ELSE/* SCAN COMPLETE            */ 02789000
*                                     DO;                          0728 02790000
         B     @RC00726                                            0728 02791000
@RF00726 DS    0H                                                  0729 02792000
*                                      RTM2CONT=OFF;/* RESET CONTROL    02793000
*                                      VARIABLE                      */ 02794000
         NI    RTM2CONT(R8),B'11111110'                            0729 02795000
*                                      R7=0;/* INDICATE SCAN DONE    */ 02796000
         SLR   R7,R7                                               0730 02797000
*                                     END;                         0731 02798000
*                                   END;                           0732 02799000
*                                 ELSE/* NEXT TCB FOUND              */ 02800000
*                                   RTM2CONT=OFF;/* RESET CONTROL  0733 02801000
*                                      VARIABLE                      */ 02802000
         B     @RC00723                                            0733 02803000
@RF00723 NI    RTM2CONT(R8),B'11111110'                            0733 02804000
*                               END;                               0734 02805000
@RC00723 DS    0H                                                  0734 02806000
@DE00722 TM    RTM2CONT(R8),B'00000001'                            0734 02807000
         BO    @DL00722                                            0734 02808000
*                               RESPECIFY                          0735 02809000
*                                 R11 UNRESTRICTED;/* RELEASE REG  0735 02810000
*                                      USED IN SCAN                  */ 02811000
*                             END;  /* END SEGMENT RTCFTCB           */ 02812000
*                             RTM2RSCN=OFF;/* ASSUME NOT RTM2 TCB    */ 02813000
         NI    RTM2RSCN(R8),B'01111111'                            0737 02814000
*                             IF R7^=0 THEN/* SUBTASK TCB WAS FOUND  */ 02815000
*                                                                  0738 02816000
         LTR   R7,R7                                               0738 02817000
         BZ    @RF00738                                            0738 02818000
*                               /*************************************/ 02819000
*                               /*                                   */ 02820000
*                               /* SEGMENT RTCPTCB DETERMINES WHETHER*/ 02821000
*                               /* A TASK IS ELIGIBLE FOR STACKING   */ 02822000
*                               /* AND PREPARES IT FOR STACKING IF IT*/ 02823000
*                               /* IS. IF ANOTHER TASK IS ALREADY    */ 02824000
*                               /* STACKED BEHIND THE SUBTASK, IT IS */ 02825000
*                               /* POSTED TO RESUME AND RE-STACK     */ 02826000
*                               /* BEHIND THE CURRENT TASK.          */ 02827000
*                               /*                                   */ 02828000
*                               /*************************************/ 02829000
*                                                                  0739 02830000
*                               DO; /* START OF RTCPTCB              */ 02831000
*                                                                  0739 02832000
*                                 /***********************************/ 02833000
*                                 /*                                 */ 02834000
*                                 /* ABTERM TASK IF IT IS IN         */ 02835000
*                                 /* STEP-MUST-COMPLETE OR           */ 02836000
*                                 /* NONDISPATCHABLE DUE TO SUBTASK  */ 02837000
*                                 /* BEING IN SMC                    */ 02838000
*                                 /*                                 */ 02839000
*                                 /***********************************/ 02840000
*                                                                  0740 02841000
*                                 IF TCBRTM2=OFF/* DON'T ABTERM TASK    02842000
*                                      ALREADY IN RTM2               */ 02843000
*                                     &(TCBSTP=ON|TCBFJMC=ON) THEN 0740 02844000
