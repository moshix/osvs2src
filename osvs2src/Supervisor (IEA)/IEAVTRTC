*             /*******************************************************/ 01768000
*             /*                                                     */ 01769000
*             /* RESET FLAGS IN STORAGE LIST IF NECESSARY            */ 01770000
*             /*                                                     */ 01771000
*             /*******************************************************/ 01772000
*                                                                  0490 01773000
*             IF RTM2WRAP=ON THEN   /* RANGES HAVE WRAPPED AROUND    */ 01774000
         TM    RTM2WRAP(R8),B'00010000'                            0490 01775000
         BNO   @RF00490                                            0490 01776000
*               DO;                                                0491 01777000
*                 RESPECIFY                                        0492 01778000
*                   R9 RESTRICTED;                                 0492 01779000
*                 DO R9=1 TO RGENO WHILE(STORBTO(R9)=OFF);/* LOOP TO    01780000
*                                      FIND RANGE WITH END INDICATOR */ 01781000
         LA    R9,1                                                0493 01782000
@DL00493 LR    @12,R9                                              0493 01783000
         SLA   @12,3                                               0493 01784000
         ALR   @12,R8                                              0493 01785000
         TM    STORBTO+268(@12),B'10000000'                        0493 01786000
         BNZ   @DC00493                                            0493 01787000
*                 END;                                             0494 01788000
         AL    R9,@CF00071                                         0494 01789000
         C     R9,@CF00102                                         0494 01790000
         BNH   @DL00493                                            0494 01791000
@DC00493 DS    0H                                                  0495 01792000
*                 STORBTO(R9)=OFF;  /* TURN OFF END INDICATOR        */ 01793000
