*                                                                  0811 03114000
*                         /*******************************************/ 03115000
*                         /*                                         */ 03116000
*                         /* STOP ALL SUBTASKS                       */ 03117000
*                         /*                                         */ 03118000
*                         /*******************************************/ 03119000
*                                                                  0812 03120000
*                         RESPECIFY                                0812 03121000
*                          (R0,                                    0812 03122000
*                           R1) RESTRICTED;/* RESTRICT STATUS      0812 03123000
*                                      INTERFACE REGS                */ 03124000
*                         R0=STOPTREE;/* MASK AND ENTRY CODE FOR   0813 03125000
*                                      STATUS                        */ 03126000
         L     R0,@CF00085                                         0813 03127000
*                         R1=R4;    /* TOP TCB OF TREE               */ 03128000
         LR    R1,R4                                               0814 03129000
*                         R13=0;    /* INDICATE CURRENT MEMORY       */ 03130000
         SLR   R13,R13                                             0815 03131000
*                         CALL STATUS;/* STOP SUBTASK TREE           */ 03132000
         L     @12,CVTPTR                                          0816 03133000
         L     @12,SCVTSPTR(,@12)                                  0816 03134000
         L     @15,SCVTSTAT(,@12)                                  0816 03135000
         BALR  @14,@15                                             0816 03136000
*                         RESPECIFY                                0817 03137000
*                          (R0,                                    0817 03138000
*                           R1) UNRESTRICTED;                      0817 03139000
*                                                                  0817 03140000
*                         /*******************************************/ 03141000
*                         /*                                         */ 03142000
*                         /* SET ABEND FLAGS IN ALL SUBTASKS         */ 03143000
*                         /*                                         */ 03144000
*                         /*******************************************/ 03145000
*                                                                  0818 03146000
*                         RESPECIFY                                0818 03147000
*                          (R10,                                   0818 03148000
