*                       RTM2CLUP=ON;/* INDICATE CLEAN UP ONLY        */ 00518000
         OI    RTM2CLUP(R8),B'10000000'                            0169 00519000
*                     ELSE                                         0170 00520000
*                       ;           /* NO ENTRY WAS FOR 33E          */ 00521000
@RF00168 DS    0H                                                  0171 00522000
*                     TCBFPRAP=OFF; /* PERMIT ASYNCHRONOUS ABENDS TO    00523000
*                                      PERCOLATE TO THE TASK         */ 00524000
         NI    TCBFPRAP(R4),B'11101111'                            0171 00525000
*                     IF RTM2RECR=ON&RTM2SCTR(RTM2TRS)=ON THEN/*   0172 00526000
*                                      RECOVERY EXIT PROCESSING HAS     00527000
*                                      RECURSED                      */ 00528000
