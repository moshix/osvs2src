         BNO   @GL00041                                            0746 02883000
@GL00042 TM    TCBFC(R7),B'10000000'                               0746 02884000
         BZ    @RT00746                                            0746 02885000
@GL00041 TM    RTM2RSCN(R8),B'10000000'                            0746 02886000
         BNO   @RF00746                                            0746 02887000
@RT00746 DS    0H                                                  0747 02888000
*                                   /* TASK IN RTM2 OR JUST ABTERMED    02889000
*                                                            @ZA07154*/ 02890000
*                                   DO;                            0747 02891000
*                                    RTM2RSCN=ON;/* PREVENT TREE SCAN   02892000
*                                      FROM GOING BELOW THIS TASK    */ 02893000
         OI    RTM2RSCN(R8),B'10000000'                            0748 02894000
*                                    IF TCBFMW=ON THEN/* ANOTHER TASK   02895000
*                                      IS ALREADY STACKED BEHIND THIS   02896000
