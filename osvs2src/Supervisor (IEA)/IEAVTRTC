         L     @15,CVTRSCN(,@12)                                   0375 01339000
         BALR  @14,@15                                             0375 01340000
*                             IF R10->TCBFC^=ON THEN               0376 01341000
         TM    TCBFC(R10),B'10000000'                              0376 01342000
         BO    @RF00376                                            0376 01343000
*                               DO;                                0377 01344000
*                                 R10->TCBFA=ON;/* PREVENT TASK FROM    01345000
*                                      EVER BEING RECOVERED          */ 01346000
         OI    TCBFA(R10),B'10000000'                              0378 01347000
