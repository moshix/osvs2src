         BNO   @RF00450                                            0450 01621000
*                     DO;                                          0451 01622000
*                       RTM2RCRX='1'B;/* SET RECURSION EXIT FLAG     */ 01623000
         OI    RTM2RCRX(R8),B'10000000'                            0452 01624000
*                       RTM2SCTX(RTM2T1MS)='1'B;/* SET SECTION FLAG     01625000
*                                      FOR EXIT                      */ 01626000
         OI    RTM2SCTX(R8),B'01000000'                            0453 01627000
*                     END;                                         0454 01628000
