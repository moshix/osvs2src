*                                      (UNCOND)RELATED(TCB,IEAVTRTC(R   01078000
*                                      TCCSUB)) OBTAIN LOCAL LOCK    */ 01079000
*               ESAGLREC=OFF;       /* SETLOCK COMPLETED WITHOUT   0312 01080000
*                                      ERROR                         */ 01081000
*                                                                  0312 01082000
         NI    ESAGLREC+96(R5),B'11101111'                         0312 01083000
*               /*****************************************************/ 01084000
*               /*                                                   */ 01085000
*               /* SET ABEND FLAGS IN CURRENT TASK                   */ 01086000
*               /*                                                   */ 01087000
*               /*****************************************************/ 01088000
*                                                                  0313 01089000
*               TCBFA=ON;           /* INDICATE TASK CANNOT BE     0313 01090000
*                                      RECOVERED                     */ 01091000
         OI    TCBFA(R4),B'10000000'                               0313 01092000
*               TCBEOTFM=ON;        /* PREVENT CONDITONAL GETMAIN  0314 01093000
*                                      ABENDS FOR THIS TASK  @YM01942*/ 01094000
         OI    TCBEOTFM(R4),B'10000000'                            0314 01095000
*               IF RTM2CNCL=ON THEN /* CANCEL ENTRY                  */ 01096000
         TM    RTM2CNCL(R8),B'01000000'                            0315 01097000
         BNO   @RF00315                                            0315 01098000
*                 DO;                                              0316 01099000
*                   TCBFT=ON;       /* INDICATE CURRENT TASK IS TOP     01100000
*                                      OF TREE                       */ 01101000
         OI    TCBFT(R4),B'00000100'                               0317 01102000
*                 END;                                             0318 01103000
*               ELSE                                               0319 01104000
*                 ;                 /* PURGE-ONLY: CURRENT TASK IS 0319 01105000
*                                      NOT TOP OF TREE               */ 01106000
*                                                                  0319 01107000
@RF00315 DS    0H                                                  0320 01108000
*               /*****************************************************/ 01109000
*               /*                                                   */ 01110000
*               /* PREVENT EXISTING ASYNCHRONOUS ROUTINES FROM BEING */ 01111000
*               /* SCHEDULED                                         */ 01112000
*               /*                                                   */ 01113000
*               /*****************************************************/ 01114000
*                                                                  0320 01115000
*               IF RTM2RECR='1'B&RTM2SCTR(RTM2IQES)='1'B THEN/*    0320 01116000
*                                      RECURSION IN THIS SECTION     */ 01117000
