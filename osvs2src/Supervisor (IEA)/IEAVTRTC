*                 /***************************************************/ 01999000
*                 /*                                                 */ 02000000
*                 /* DETERMINE IF THE RB WHICH IS RETRYING IS PART OF*/ 02001000
*                 /* THE DUMP PROCESS OR NOT. IF IT IS PART OF THE   */ 02002000
*                 /* DUMP PROCESS THEN THE DUMP IS SKIPPED AND THE   */ 02003000
*                 /* RETRY SHOULD COMPLETE THE PREVIOUS DUMP. IF IT  */ 02004000
*                 /* IS OUTSIDE THE DUMP PROCESS THE RECURSION EXIT  */ 02005000
*                 /* IS TAKEN SO THAT DUMP CAN RELEASE ITS RESOURCES.*/ 02006000
*                 /*                                                 */ 02007000
*                 /***************************************************/ 02008000
*                                                                  0548 02009000
*                 DO;                                              0548 02010000
*                   RESPECIFY                                      0549 02011000
*                     R11 RESTRICTED;                              0549 02012000
*                   R11=RTM2RYRB;   /* LOCATE STARTING RB            */ 02013000
*                                                                  0550 02014000
         L     R11,RTM2RYRB(,R8)                                   0550 02015000
*                   /*************************************************/ 02016000
*                   /*                                               */ 02017000
*                   /* SCAN FOR RB WITH RBABEND ON. SCAN TERMINATED  */ 02018000
*                   /* WHEN RBTCBNXT ON                              */ 02019000
*                   /*                                               */ 02020000
*                   /*************************************************/ 02021000
*                                                                  0551 02022000
*                   DO WHILE(EVAL(R11-LENGTH(RBPRFX))->RBABEND='0'B&R11 02023000
*                         ->RBTCBNXT='0'B);                        0551 02024000
         B     @DE00551                                            0551 02025000
@DL00551 DS    0H                                                  0552 02026000
*                     R11=R11->RBLINKB;/* LOCATE NEXT RB             */ 02027000
         L     @12,RBLINKB-1(,R11)                                 0552 02028000
         LA    @12,0(,@12)                                         0552 02029000
         LR    R11,@12                                             0552 02030000
*                   END;            /* END SCAN OF RBS               */ 02031000
@DE00551 LR    @12,R11                                             0553 02032000
         SL    @12,@CF00470                                        0553 02033000
         TM    RBABEND(@12),B'00100000'                            0553 02034000
         BNZ   @DC00551                                            0553 02035000
         TM    RBTCBNXT(R11),B'10000000'                           0553 02036000
         BZ    @DL00551                                            0553 02037000
@DC00551 DS    0H                                                  0554 02038000
*                   IF EVAL(R11-LENGTH(RBPRFX))->RBABEND='0'B THEN/*    02039000
*                                      CHECK WHETHER ONE WAS FOUND   */ 02040000
         LR    @12,R11                                             0554 02041000
         SL    @12,@CF00470                                        0554 02042000
         TM    RBABEND(@12),B'00100000'                            0554 02043000
         BNZ   @RF00554                                            0554 02044000
*                     R11=0;        /* NO RTM2 RB FOUND              */ 02045000
         SLR   R11,R11                                             0555 02046000
*                 END;              /* DETERMINE IF RETRY RB IS    0556 02047000
*                                      PRECEDED BY RTM2SVRB          */ 02048000
@RF00554 DS    0H                                                  0557 02049000
*                 IF R11=0|(ADDR(R11->RBEXSAVE)->ESART2WA->RTM2SCTR(    02050000
*                     RTM2DPS)=OFF&ADDR(R11->RBEXSAVE)->ESART2WA-> 0557 02051000
*                     RTM2SCTC(RTM2DPS)=OFF) THEN/* RETRY NOT WITHIN    02052000
*                                      DUMP PROCESS          @YM04486*/ 02053000
