         BNO   @RF00612                                            0612 02246000
*               RTM2RTRX=ON;        /* TAKE RETRY EXIT               */ 02247000
         OI    RTM2RTRX(R8),B'00000001'                            0613 02248000
*             ELSE                                                 0614 02249000
*               ;                   /* RETRY NOT REQUESTED, CONTINUE    02250000
*                                      ABEND                         */ 02251000
@RF00612 DS    0H                                                  0615 02252000
*         END;                      /* END NO RECUSION             0615 02253000
*                                      PROCESS@G33SPHW               */ 02254000
@RC00593 DS    0H                                                  0616 02255000
*       IF RTM2FLX=NONE THEN        /* NO EXIT REQUESTED             */ 02256000
*                                                                  0616 02257000
@RF00591 CLC   RTM2FLX(2,R8),@CB00069                              0616 02258000
         BNE   @RF00616                                            0616 02259000
*         /***********************************************************/ 02260000
*         /*                                                         */ 02261000
*         /* SEGMENT RTCABCTL CONTROLS THE ABEND PROCESSING FOR A    */ 02262000
*         /* TASK OR TASK TREE. IT INDICATES THE TASK IS IN ABEND AND*/ 02263000
*         /* SETS A FLAG INDICATING IT IS THE TOP TASK IN THE FAILING*/ 02264000
*         /* TREE (TCBFT) UNLESS THIS ENTRY IS FOR PURGE ONLY. IT    */ 02265000
*         /* THEN INCLUDES SEGMENT RTCTEINT TO INTERFACE WITH TERM   */ 02266000
*         /* EXIT PROCESSING. AFTER TERM EXITS HAVE BEEN EXECUTED IT */ 02267000
*         /* STACKS BEHIND OTHER TASKS IN RTM2 UNLESS THE REASON FOR */ 02268000
*         /* ENTRY AT THIS TASK LEVEL HAS EVER BEEN AN EXPRESS ENTRY */ 02269000
*         /* OR A PURGE-ONLY ENTRY. RTM2 DOES NOT STACK IF ENTRY HAS */ 02270000
*         /* EVER BEEN EXPRESS BECAUSE THE SUBTASKS HAVE BEEN SET    */ 02271000
*         /* NON-DISPATCHABLE AND WILL NOT COMPLETE. IT DOES NOT     */ 02272000
*         /* STACK IF ENTRY WAS FOR PURGE ONLY BECAUSE ANY SUBTASKS  */ 02273000
*         /* THAT EXIST WERE CREATED BY A TERM EXIT. WHEN ALL        */ 02274000
*         /* SUBTASKS IN RTM2 HAVE COMPLETED IF STACKING WAS DONE    */ 02275000
*         /* SEGMENT RTCCSUB IS INCLUDED TO DO INITIAL SUBTASK       */ 02276000
*         /* PROCESSING. THIS PROCESSING IS NOT REPEATED IF IT WERE  */ 02277000
*         /* DONE DUE TO AN EARLIER PATH THROUGH RTM2. AT COMPLETION */ 02278000
*         /* OF THIS PROCESSING SEGMENT RTCTTINT IS INCLUDED TO      */ 02279000
*         /* INTERFACE WITH TASK TERMINATION PURGES. AFTER THE       */ 02280000
*         /* CURRENT TASK AND ALL ITS SUBTASKS HAVE BEEN PURGED      */ 02281000
*         /* SEGMENT RTCCONV CONTINUES THE ABEND AT THE JOBSTEP LEVEL*/ 02282000
*         /* IF STEP WAS REQUESTED ON THE ABEND MACRO OR THE TASK WAS*/ 02283000
*         /* IN STEP-MUST-COMPLETE AT THE TIME IT SUFFERED THE ERROR */ 02284000
*         /*                                                         */ 02285000
*         /***********************************************************/ 02286000
*                                                                  0617 02287000
*         DO;                       /* BEGIN SEGMENT RTCABCTL        */ 02288000
*           IF TCBFA=OFF THEN       /* TASK WAS NOT PREVIOUSLY IN  0618 02289000
*                                      ABEND PROCESSING              */ 02290000
         TM    TCBFA(R4),B'10000000'                               0618 02291000
         BNZ   @RF00618                                            0618 02292000
*             DO;                                                  0619 02293000
*               TCBFA=ON;           /* INDICATE TASK IN ABEND      0620 02294000
*                                      PROCESSING                    */ 02295000
         OI    TCBFA(R4),B'10000000'                               0620 02296000
*               TCBEOTFM=ON;        /* INDICATE END OF TASK TO     0621 02297000
*                                      FREEMAIN              @YM01942*/ 02298000
         OI    TCBEOTFM(R4),B'10000000'                            0621 02299000
*               IF RTM2PURG=OFF THEN/* NO ENTRY FOR PURGE ONLY       */ 02300000
         TM    RTM2PURG(R8),B'10000000'                            0622 02301000
         BNZ   @RF00622                                            0622 02302000
*                 TCBFT=ON;         /* INDICATE TOP TASK IN TREE     */ 02303000
         OI    TCBFT(R4),B'00000100'                               0623 02304000
*               ESAGLREC=ON;        /* SET FLAG FOR GETLOCK RECURSION*/ 02305000
@RF00622 OI    ESAGLREC+96(R5),B'00010000'                         0624 02306000
*               DO;                 /* SETLOCK RELEASE TYPE(LOCAL)REL   02307000
*                                      ATED(TCB,IEAVTRTC(RTCAOR))    */ 02308000
*                 RESPECIFY                                        0626 02309000
*                  (GPR11P,                                        0626 02310000
*                   GPR12P,                                        0626 02311000
*                   GPR13P,                                        0626 02312000
*                   GPR14P) RESTRICTED;/* RESTRICT REGS USED BY    0626 02313000
*                                      SETLOCK                       */ 02314000
*                 GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P,GPR13P, 02315000
*                     GPR14P);                                     0627 02316000
