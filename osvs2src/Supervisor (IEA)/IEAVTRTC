         LA    @12,0(,@12)                                         0335 01176000
         CR    R4,@12                                              0335 01177000
         BNE   @RF00335                                            0335 01178000
*                             IQEPURGE=ON;/* SET PURGE FLAG TO     0336 01179000
*                                      PREVENT IT FROM BEING       0336 01180000
*                                      SCHEDULED                     */ 01181000
         OI    IQEPURGE(R10),B'10000000'                           0336 01182000
*                           ELSE                                   0337 01183000
*                             ;     /* IQE NOT RELATED TO CURRENT TCB*/ 01184000
@RF00335 DS    0H                                                  0338 01185000
*                           R10=IQELNKA;/* GET NEXT IQE ON CHAIN     */ 01186000
         L     @12,IQELNKA-1(,R10)                                 0338 01187000
         LA    @12,0(,@12)                                         0338 01188000
         LR    R10,@12                                             0338 01189000
*                         END;                                     0339 01190000
@DE00334 L     @12,ASCBPTR(,R8)                                    0339 01191000
         L     @12,ASXBPTR(,@12)                                   0339 01192000
         C     R10,ASXBLIQE(,@12)                                  0339 01193000
         BNE   @DL00334                                            0339 01194000
*                         IF IQETCBA=R4 THEN/* LAST IQE IS RELATED TO   01195000
*                                      THE CURRENT TCB               */ 01196000
         L     @12,IQETCBA-1(,R10)                                 0340 01197000
         LA    @12,0(,@12)                                         0340 01198000
         CR    R4,@12                                              0340 01199000
         BNE   @RF00340                                            0340 01200000
*                           IQEPURGE=ON;/* SET PURGE FLAG TO PREVENT    01201000
*                                      SCHEDULING                    */ 01202000
         OI    IQEPURGE(R10),B'10000000'                           0341 01203000
*                         ELSE                                     0342 01204000
*                           ;       /* IQE NOT FOR CURRENT TCB       */ 01205000
@RF00340 DS    0H                                                  0343 01206000
*                       END;                                       0343 01207000
*                     ELSE                                         0344 01208000
*                       ;           /* NO IQE ON CHAIN               */ 01209000
@RF00332 DS    0H                                                  0345 01210000
*                     RESPECIFY                                    0345 01211000
*                       R10 UNRESTRICTED;                          0345 01212000
*                   END;            /* END SEGMENT RTCFIQE           */ 01213000
*                   RTM2SKRA=0;     /* CLEAR RECURSION ADDRESS       */ 01214000
         SLR   @12,@12                                             0347 01215000
         ST    @12,RTM2SKRA(,R8)                                   0347 01216000
