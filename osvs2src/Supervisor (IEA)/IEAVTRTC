         NI    RTM2SCTC(R8),B'11011111'                            0158 00465000
*             END;                                                 0159 00466000
*RCR0004:                           /* RESET RECURSION FLAGS         */ 00467000
*           IF RTM2FLX=NONE THEN    /* NO EXIT HAS BEEN REQUESTED    */ 00468000
@RC00087 DS    0H                                                  0160 00469000
RCR0004  CLC   RTM2FLX(2,R8),@CB00069                              0160 00470000
         BNE   @RF00160                                            0160 00471000
*             DO;                                                  0161 00472000
*                                                                  0161 00473000
*               /*****************************************************/ 00474000
*               /*                                                   */ 00475000
*               /* SEGMENT RTCTRINT INTERFACES WITH TASK RECOVERY. IT*/ 00476000
*               /* ESTABLISHES RECURSION CONTROL AND THEN DETERMINES */ 00477000
*               /* IF EXITS EXIST AND SHOULD BE ENTERED. (EXITS WILL */ 00478000
*               /* NOT BE ENTERED IF THE COMPLETION CODE WAS 13E OR  */ 00479000
*               /* 20D AS THESE ERRORS ARE NOT RECOVERABLE). IF THE  */ 00480000
*               /* COMPLETION CODE IS 33E IT INDICATES THAT CLEAN-UP */ 00481000
*               /* ONLY IS PERMITTED. IT THEN INVOKES TASK RECOVERY. */ 00482000
*               /* ON RETURN IT ENSURES THAT TCB FLAGS WHICH MAY HAVE*/ 00483000
*               /* BEEN ALTERED BY TASK RECOVERY PROCESSING ARE      */ 00484000
*               /* PROPERLY SET                                      */ 00485000
*               /*                                                   */ 00486000
*               /*****************************************************/ 00487000
*                                                                  0162 00488000
*               DO;                 /* BEGIN SEGMENT RTCTRINT        */ 00489000
*                 IF TCBSTABB^=0    /* (E)STA EXITS EXIST            */ 00490000
*                     &ESACC^=CNVCODE&RTM2CC^=SMCCODE&RTM2CC^=DET13E    00491000
*                   THEN            /* (E)STA EXITS ALLOWED TO GET 0163 00492000
*                                      CONTROL AT THIS TIME          */ 00493000
         L     @12,TCBSTABB-1(,R4)                                 0163 00494000
         LA    @12,0(,@12)                                         0163 00495000
         LTR   @12,@12                                             0163 00496000
         BZ    @RF00163                                            0163 00497000
