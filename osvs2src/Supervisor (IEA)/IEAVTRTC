*                   IF RTM2FLX=NONE THEN/* EXIT WAS NOT REQUESTED    */ 00808000
         CLC   RTM2FLX(2,R8),@CB00069                              0248 00809000
         BNE   @RF00248                                            0248 00810000
*                     DO;                                          0249 00811000
*                       IF RTM2RECR='1'B&RTM2SCTR(RTM2ASYS)='1'B THEN   00812000
