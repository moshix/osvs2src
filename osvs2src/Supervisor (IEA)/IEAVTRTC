         TM    RTM2SCTR+1(@12),B'00001000'                         0557 02057000
         BNZ   @RF00557                                            0557 02058000
         TM    RTM2SCTC+1(@12),B'00001000'                         0557 02059000
         BNZ   @RF00557                                            0557 02060000
@RT00557 DS    0H                                                  0558 02061000
*                   DO;                                            0558 02062000
*                     RTM2RCRX=ON;  /* SET EXIT FLAG                 */ 02063000
         OI    RTM2RCRX(R8),B'10000000'                            0559 02064000
*                     RTM2SCTX(RTM2DPS)=ON;/* SET SECTION FLAG FOR 0560 02065000
*                                      EXIT                          */ 02066000
         OI    RTM2SCTX+1(R8),B'00001000'                          0560 02067000
*                   END;                                           0561 02068000
*                 ELSE                                             0562 02069000
*                   ;               /* RETRY WITHIN DUMP PROCESS     */ 02070000
@RF00557 DS    0H                                                  0563 02071000
*                 RESPECIFY                                        0563 02072000
*                   R11 UNRESTRICTED;/* RELEASE RBREG                */ 02073000
*               END;                                               0564 02074000
*         END;                                                     0565 02075000
*     END;                          /* END SEGMENT RTCADINT          */ 02076000
*   ELSE                                                           0567 02077000
*     ;                             /* BYPASS ALL PROCESSING IF EXIT    02078000
*                                      REQUESTED                     */ 02079000
@RF00481 DS    0H                                                  0568 02080000
*   IF RTM2FLX=NONE THEN            /* CONTINUE PROCESSING UNLESS  0568 02081000
*                                      EXIT REQUESTED                */ 02082000
*                                                                  0568 02083000
@RC00481 CLC   RTM2FLX(2,R8),@CB00069                              0568 02084000
         BNE   @RF00568                                            0568 02085000
*     /***************************************************************/ 02086000
*     /*                                                             */ 02087000
*     /* SEGMENT RTCAOR DETERMINES WHETHER RETRY WAS REQUESTED AND IS*/ 02088000
*     /* PERMITTED. IF THE TASK IS ALREADY IN ABEND (TCBFA IS ON)    */ 02089000
*     /* THEN RETRY IS PERMITTED AS ONLY THOSE EXITS ESTABLISHED     */ 02090000
*     /* SINCE ABEND PROCESSING WAS INITIATED ARE ENTERED. OTHERWISE,*/ 02091000
*     /* IF A MOTHER TASK IS IN ABEND (TCBFMW IS ON) RETRY IS NOT    */ 02092000
*     /* PERMITTED. THIS TASK MUST ENTER ABEND PROCESSING. IF A      */ 02093000
*     /* DAUGHTER TASK IS WAITING TO CONVERT ITS ABEND TO STEP       */ 02094000
*     /* (TCBFDW IS ON) THEN RETRY IS BYPASSED AND THE DAUGHTER      */ 02095000
*     /* WAITING EXIT FLAG IS SET. IF NONE OF THESE CONDITIONS EXIST */ 02096000
*     /* AND RETRY IS REQUESTED THE RETRY EXIT FLAG IS SET. IF NO    */ 02097000
*     /* EXITS FLAGS ARE ON THEN SEGMENT RTCABCTL IS ENTERED TO      */ 02098000
*     /* PERFORM ABEND PROCESSING.                                   */ 02099000
*     /*                                                             */ 02100000
*     /***************************************************************/ 02101000
*                                                                  0569 02102000
*     DO;                           /* BEGIN SEGMENT RTCAOR          */ 02103000
*       IF RTM2RETR=ON THEN         /* RETRY REQUESTED       @G33SPHW*/ 02104000
         TM    RTM2RETR(R8),B'01000000'                            0570 02105000
         BNO   @RF00570                                            0570 02106000
*         DO;                       /* MUST SETUP RBS        @G33SPHW*/ 02107000
*           IF RTM2RECR=ON&RTM2SCTR(RTM2SRBS)=ON THEN/* RECURSION IN    02108000
*                                      THIS SECTION          @G33SPHW*/ 02109000
