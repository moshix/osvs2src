*                   RTM2SCTC(RTM2IQES)='1'B;/* SET CURRENT SECTION 0326 01133000
*                                      FLAG                          */ 01134000
         OI    RTM2SCTC(R8),B'00000100'                            0326 01135000
*                   GEN REFS(R8) SETS(RTM2SFSA)                    0327 01136000
*                       (STM R14,R12,RTM2SFSA+12(R8));/* SAVE      0327 01137000
*                                      REGISTERS                     */ 01138000
         STM R14,R12,RTM2SFSA+12(R8)                                    01139000
*                   RTM2SKRA=ADDR(RCR0015);/* SET UP RECURSION     0328 01140000
*                                      ADDRESS SET UP RECURSION    0328 01141000
*                                      CONTROL                       */ 01142000
*                                                                  0328 01143000
         LA    @12,RCR0015                                         0328 01144000
         ST    @12,RTM2SKRA(,R8)                                   0328 01145000
*                   /*************************************************/ 01146000
*                   /*                                               */ 01147000
*                   /* SEGMENT RTCFIQE FLAGS ALL IQES RELATED TO THE */ 01148000
*                   /* CURRENT TASK TO PREVENT THEIR BEING SCHEDULED.*/ 01149000
*                   /* THE CHAIN OF IQES FROM THE ASXB IS SCANNED,   */ 01150000
*                   /* AND EACH IQE'S TCB POINTER IS COMPARED TO THE */ 01151000
*                   /* ADDRESS OF THE CURRENT TCB. IF IT MATCHES, THE*/ 01152000
*                   /* IQE IS FLAGGED. THE LOCAL LOCK MUST BE HELD TO*/ 01153000
*                   /* PREVENT IQES FROM BEING ADDED TO THE QUEUE    */ 01154000
*                   /*                                               */ 01155000
*                   /*************************************************/ 01156000
*                                                                  0329 01157000
*                   DO;             /* BEGIN SEGMENT RTCFIQE         */ 01158000
*                     RESPECIFY                                    0330 01159000
*                       R10 RESTRICTED;/* RESTRICT IQE REGISTER      */ 01160000
*                     R10=ASXBFIQE; /* SET REG TO ADDR OF FIRST IQE  */ 01161000
         L     @12,ASCBPTR(,R8)                                    0331 01162000
         L     @12,ASXBPTR(,@12)                                   0331 01163000
         L     R10,ASXBFIQE(,@12)                                  0331 01164000
*                     IF R10^=0 THEN/* AN IQE EXISTS                 */ 01165000
         LTR   R10,R10                                             0332 01166000
         BZ    @RF00332                                            0332 01167000
*                       DO;                                        0333 01168000
*                         DO WHILE(R10^=ASXBLIQE);/* LOOP UNTIL LAST    01169000
*                                      IQE FOUND                     */ 01170000
         B     @DE00334                                            0334 01171000
@DL00334 DS    0H                                                  0335 01172000
*                           IF IQETCBA=R4 THEN/* IQE RELATED TO    0335 01173000
*                                      CURRENT TCB                   */ 01174000
