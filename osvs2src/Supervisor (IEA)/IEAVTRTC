         CR    R4,@12                                              0277 00934000
         BNE   @RF00277                                            0277 00935000
*                                   IQEPURGE=ON;/* SET PURGE FLAG TO    00936000
*                                      PREVENT SCHEDULING            */ 00937000
         OI    IQEPURGE(R10),B'10000000'                           0278 00938000
*                                 ELSE                             0279 00939000
*                                   ;/* IQE NOT FOR CURRENT TCB      */ 00940000
@RF00277 DS    0H                                                  0280 00941000
*                               END;                               0280 00942000
*                             ELSE                                 0281 00943000
*                               ;   /* NO IQE ON CHAIN               */ 00944000
@RF00269 DS    0H                                                  0282 00945000
*                             RESPECIFY                            0282 00946000
*                               R10 UNRESTRICTED;                  0282 00947000
*                           END;    /* END SEGMENT RTCFIQE           */ 00948000
*                                                                  0283 00949000
*                           /*****************************************/ 00950000
*                           /*                                       */ 00951000
*                           /* RELEASE LOCAL LOCK                    */ 00952000
*                           /*                                       */ 00953000
*                           /*****************************************/ 00954000
*                                                                  0284 00955000
*                           ESAGLREC=ON;                           0284 00956000
