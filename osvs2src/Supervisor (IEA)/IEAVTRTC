*                                      END-OF-TASK FROM VALIDITY   0765 02957000
*                                      CHECKING ECB                  */ 02958000
         OI    TCBFMW(R7),B'01000100'                              0765 02959000
*                                   END;                           0766 02960000
*                                 ELSE                             0767 02961000
*                                   ;/* TASK NOT ELIGIBLE FOR      0767 02962000
*                                      STACKING                      */ 02963000
@RF00746 DS    0H                                                  0768 02964000
*                               END;/* END OF RTCPTCB                */ 02965000
*                             ELSE                                 0769 02966000
*                               ;   /* NO MORE SUBTASKS              */ 02967000
@RF00738 DS    0H                                                  0770 02968000
*                           END;    /* END OF TREE SCAN              */ 02969000
*                                                                  0770 02970000
@DE00713 C     R9,@CF00102                                         0770 02971000
