         ST    @12,RTM2SKRA(,R8)                                   0361 01267000
*/* SET UP RECURSION CONTROL                                         */ 01268000
*                                                                  0362 01269000
*                         /*******************************************/ 01270000
*                         /*                                         */ 01271000
*                         /* SEGMENT RTCCSUB STOPS ALL SUBTASKS AND  */ 01272000
*                         /* PERFORMS THE INITIAL PURGES FOR EACH OF */ 01273000
*                         /* THEM (I/O, PARTIALLY LOADED PROGRAMS,   */ 01274000
*                         /* AND PAGING I/O.) IEATRSCN IS USED TO    */ 01275000
*                         /* LOCATE EACH TASK IN TREE FOR INITIAL    */ 01276000
*                         /* PURGES                                  */ 01277000
*                         /*                                         */ 01278000
*                         /*******************************************/ 01279000
*                                                                  0362 01280000
*                         DO;       /* START OF RTCCSUB              */ 01281000
*                                                                  0362 01282000
*                           /*****************************************/ 01283000
*                           /*                                       */ 01284000
*                           /* STOP ALL SUBTASKS                     */ 01285000
*                           /*                                       */ 01286000
*                           /*****************************************/ 01287000
*                                                                  0363 01288000
*                           RESPECIFY                              0363 01289000
*                            (R0,                                  0363 01290000
*                             R1) RESTRICTED;/* RESTRICT STATUS    0363 01291000
*                                      INTERFACE REGS                */ 01292000
*                           R0=STOPTREE;/* MASK AND ENTRY CODE FOR 0364 01293000
*                                      STATUS                        */ 01294000
         L     R0,@CF00085                                         0364 01295000
*                           R1=R4;  /* TOP TCB OF TREE               */ 01296000
         LR    R1,R4                                               0365 01297000
*                           R13=0;  /* INDICATE CURRENT MEMORY       */ 01298000
         SLR   R13,R13                                             0366 01299000
*                           CALL STATUS;/* STOP SUBTASK TREE         */ 01300000
         L     @12,CVTPTR                                          0367 01301000
         L     @12,SCVTSPTR(,@12)                                  0367 01302000
         L     @15,SCVTSTAT(,@12)                                  0367 01303000
         BALR  @14,@15                                             0367 01304000
*                           RESPECIFY                              0368 01305000
*                            (R0,                                  0368 01306000
*                             R1) UNRESTRICTED;                    0368 01307000
*                                                                  0368 01308000
*                           /*****************************************/ 01309000
*                           /*                                       */ 01310000
*                           /* SET ABEND FLAGS IN ALL SUBTASKS       */ 01311000
*                           /*                                       */ 01312000
*                           /*****************************************/ 01313000
*                                                                  0369 01314000
*                           RESPECIFY                              0369 01315000
*                            (R10,                                 0369 01316000
*                             R8,                                  0369 01317000
*                             R11) RESTRICTED;/* INTERFACE REGS FOR     01318000
*                                      TREE SCAN                     */ 01319000
*                           RESPECIFY                              0370 01320000
*                            (R7,                                  0370 01321000
*                             R9) RESTRICTED;/* RESTRICT REGISTERS 0370 01322000
*                                      USED AS BASE AND WORKREG BY 0370 01323000
