         TITLE 'IEAVTRTC RTM2 CONTROLLER                               *00001000
                        '                                               00002000
IEAVTRTC CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVTRTC  77.180'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @06,0                                               0001 00011000
@PSTART  LA    @03,4095(,@06)                                      0001 00012000
         USING @PSTART,@06                                         0001 00013000
         USING @PSTART+4095,@03                                    0001 00014000
RBPRFXLN EQU 64                                                         00015000
RBPRFLNA EQU 32                                                         00016000
PRBLEN   EQU 136                                                        00017000
SIRBLEN  EQU 200                                                        00018000
TIRBLEN  EQU 136                                                        00019000
IRBLEN   EQU 128                                                        00020000
SVRBLEN  EQU 224                                                        00021000
*                                                                  0054 00022000
*   /*****************************************************************/ 00023000
*   /*                                                               */ 00024000
*   /* SEGMENT IEAVTRTC CONTROLS THE MAIN PATH PROCESSING IN RTM2. IT*/ 00025000
*   /* INCLUDES SEGMENT RTCMTINT TO SET UP THE INTERFACE TO MEMORY   */ 00026000
*   /* TERMINATION PURGES AND RTCTTINT TO SET UP THE INTERFACE FOR   */ 00027000
*   /* NORMAL EOT PURGES. IF ENTRY WAS NOT FOR THESE, SEGMENT        */ 00028000
*   /* RTCTLRCR DETERMINES IF THE ENTRY IS RECURSIVE AND IF THE      */ 00029000
*   /* RECURSION EXIT SHOULD BE TAKEN IMMEDIATELY. IF NOT, SEGMENT   */ 00030000
*   /* RTCRVRB DOES PROCESSING IF THE ENTRY IS POTENTIALLY           */ 00031000
*   /* RECOVERABLE OR SEGMENT RTCCNCL DOES PROCESSING IF IT IS NOT.  */ 00032000
*   /* IF RECOVERY EXITS WERE ENTERED BUT DID NOT REQUEST RETRY OR IF*/ 00033000
*   /* THE ERROR WAS NOT RECOVERABLE, SEGMENT RT2NORTY IS ENTERED TO */ 00034000
*   /* PERFORM INITIAL PURGES. IN EITHER CASE, IF A DUMP WAS         */ 00035000
*   /* REQUESTED AND IS PERMITTED SEGMENT RTCADINT IS INCLUDED TO DO */ 00036000
*   /* DUMP PROCESSING. FINALLY SEGMENT RTCAOR IS INCLUDED TO        */ 00037000
*   /* DETERMINE IF RETRY IS PERMITTED AND TO CONTROL THE ABEND PATH.*/ 00038000
*   /* ON ENTRY REG 5 LOCATES THE SVRB, REG 4 LOCATES THE TCB AND    */ 00039000
*   /* REGISTER 8 LOCATES THE RTM2WA. SEGMENT RTCTLRCR DETERMINES    */ 00040000
*   /* WHETHER THIS ENTRY IS A RECURSION IN CONNECTING CODE OF RTM2. */ 00041000
*   /* IF IT IS, THE RECURSION EXIT IS TAKEN IMMEDIATELY TO PREVENT  */ 00042000
*   /* ATTEMPTING TO EXECUTE THE CODE AGAIN. OTHERWISE, A RECURSION  */ 00043000
*   /* LOOP COULD OCCUR.                                             */ 00044000
*   /*                                                               */ 00045000
*   /*****************************************************************/ 00046000
*                                                                  0054 00047000
*   DO;                             /* START OF RTCTLRCR             */ 00048000
*     IF ESARTCR=ON THEN                                           0055 00049000
         TM    ESARTCR+96(R5),B'00000100'                          0055 00050000
         BNO   @RF00055                                            0055 00051000
*       RTM2RCRX=ON;                                               0056 00052000
         OI    RTM2RCRX(R8),B'10000000'                            0056 00053000
*     ELSE                                                         0057 00054000
*       DO;                                                        0057 00055000
         B     @RC00055                                            0057 00056000
@RF00055 DS    0H                                                  0058 00057000
*         ESARTCR=ON;                                              0058 00058000
         OI    ESARTCR+96(R5),B'00000100'                          0058 00059000
*         RESPECIFY                                                0059 00060000
*           R9 RESTRICTED;                                         0059 00061000
*         R9=RTM2CODE&CNCLMASK;     /* ISOLATE CANCEL CODE           */ 00062000
         L     R9,RTM2CODE(,R8)                                    0060 00063000
         N     R9,@CF00052                                         0060 00064000
*         IF RTM2CC=CNVRCODE        /* ENTRY BECAUSE OF ERROR      0061 00065000
*                                      CONVERSION TO STEP            */ 00066000
*             |(R9=CNCLCODE&R4=TCBJSTCA&RTM2ABTM=ON) THEN          0061 00067000
*                                                                  0061 00068000
         CLC   RTM2CC(3,R8),@CB00057                               0061 00069000
         BE    @RT00061                                            0061 00070000
         C     R9,@CF00050                                         0061 00071000
         BNE   @RF00061                                            0061 00072000
         L     @12,TCBJSTCA-1(,R4)                                 0061 00073000
         LA    @12,0(,@12)                                         0061 00074000
         CR    R4,@12                                              0061 00075000
         BNE   @RF00061                                            0061 00076000
         TM    RTM2ABTM(R8),B'00001000'                            0061 00077000
         BNO   @RF00061                                            0061 00078000
@RT00061 DS    0H                                                  0062 00079000
*           /*********************************************************/ 00080000
*           /*                                                       */ 00081000
*           /* ENTRY DUE TO CANCEL                           @ZA05956*/ 00082000
*           /*                                                       */ 00083000
*           /*********************************************************/ 00084000
*                                                                  0062 00085000
*           DO;                     /*                       @YM03570*/ 00086000
*             RTM2CNCL=ON;          /* MARK WORKAREA AS DUE TO CANCEL*/ 00087000
         OI    RTM2CNCL(R8),B'01000000'                            0063 00088000
*             IF ESADREQ=OFF THEN   /* IF CANCEL WITHOUT DUMP,     0064 00089000
*                                      SUPRESS DUMP          @YM03570*/ 00090000
         TM    ESADREQ+96(R5),B'10000000'                          0064 00091000
         BNZ   @RF00064                                            0064 00092000
*               RTM2DREQ=OFF;       /*                       @YM03570*/ 00093000
         NI    RTM2DREQ(R8),B'01111111'                            0065 00094000
*           END;                    /*                       @YM03570*/ 00095000
*         ELSE                                                     0067 00096000
*           ;                       /* NOT CANCEL-TYPE ENTRY         */ 00097000
@RF00061 DS    0H                                                  0068 00098000
*         RESPECIFY                                                0068 00099000
*           R9 UNRESTRICTED;                                       0068 00100000
@RC00061 DS    0H                                                  0069 00101000
*         IF RTM2RECR=ON THEN       /* RECURSIVE ENTRY               */ 00102000
         TM    RTM2RECR(R8),B'10000000'                            0069 00103000
         BNO   @RF00069                                            0069 00104000
*           IF RTM2CNCL=ON THEN     /* CANCEL-TYPE ENTRY             */ 00105000
         TM    RTM2CNCL(R8),B'01000000'                            0070 00106000
         BNO   @RF00070                                            0070 00107000
*             IF TCBCNCB=ON&RTM2PREV->RTM2SCTC=NOSECTN THEN/* SECOND    00108000
*                                      CANCEL AND NOT IN ANY SECTION */ 00109000
         TM    TCBCNCB(R4),B'10000000'                             0071 00110000
         BNO   @RF00071                                            0071 00111000
         L     @12,RTM2PREV(,R8)                                   0071 00112000
         CLC   RTM2SCTC(4,@12),@CB00073                            0071 00113000
         BNE   @RF00071                                            0071 00114000
*               RTM2RCRX=ON;        /* TAKE RECURSION EXIT - NO    0072 00115000
*                                      RECOVERY IS PROVIDED          */ 00116000
         OI    RTM2RCRX(R8),B'10000000'                            0072 00117000
*             ELSE                                                 0073 00118000
*               ;                   /* FIRST CANCEL - NOT TREATED AS    00119000
*                                      RECURSION UNLESS FAILING    0073 00120000
*                                      SECTION IS REACHED            */ 00121000
@RF00071 DS    0H                                                  0074 00122000
*           ELSE                    /* NOT CANCEL ENTRY              */ 00123000
*             IF ESAPGNLY=OFF       /* NOT PURGE ONLY                */ 00124000
*                 &RTM2PREV->RTM2SCTC=NOSECTN THEN/* PREVIOUS FAILURE   00125000
*                                      NOT IN ANY SECTION            */ 00126000
         B     @RC00070                                            0074 00127000
@RF00070 TM    ESAPGNLY+96(R5),B'00100000'                         0074 00128000
         BNZ   @RF00074                                            0074 00129000
         L     @12,RTM2PREV(,R8)                                   0074 00130000
         CLC   RTM2SCTC(4,@12),@CB00073                            0074 00131000
         BNE   @RF00074                                            0074 00132000
*               RTM2RCRX=ON;        /* TAKE RECURSION EXIT           */ 00133000
         OI    RTM2RCRX(R8),B'10000000'                            0075 00134000
*             ELSE                                                 0076 00135000
*               ;                   /* PURGE ONLY OR FAILURE WITHIN     00136000
*                                      SECTION                       */ 00137000
@RF00074 DS    0H                                                  0077 00138000
*         ELSE                                                     0077 00139000
*           ;                       /* NOT RECURSIVE ENTRY           */ 00140000
@RF00069 DS    0H                                                  0078 00141000
*         IF RTM2CNCL=ON THEN       /* CANCEL ENTRY                  */ 00142000
@RC00069 TM    RTM2CNCL(R8),B'01000000'                            0078 00143000
         BNO   @RF00078                                            0078 00144000
*           TCBCNCB=ON;             /* MARK TCB AS CNACELLED         */ 00145000
         OI    TCBCNCB(R4),B'10000000'                             0079 00146000
*         ESARTCR=OFF;                                             0080 00147000
@RF00078 NI    ESARTCR+96(R5),B'11111011'                          0080 00148000
*       END;                                                       0081 00149000
*   END;                            /* END OF RTCTLRCR               */ 00150000
@RC00055 DS    0H                                                  0083 00151000
*   IF RTM2FLX=NONE THEN            /* IF AN EXIT NOT REQUESTED    0083 00152000
*                                      CONTINUE PROCESSING           */ 00153000
         CLC   RTM2FLX(2,R8),@CB00069                              0083 00154000
         BNE   @RF00083                                            0083 00155000
*     DO;                                                          0084 00156000
*       IF ESAPGNLY=OFF&RTM2CNCL=OFF THEN/* THIS ENTRY IS POTENTIALLY   00157000
*                                      RECOVERABLE                   */ 00158000
*                                                                  0085 00159000
         TM    ESAPGNLY+96(R5),B'00100000'                         0085 00160000
         BNZ   @RF00085                                            0085 00161000
         TM    RTM2CNCL(R8),B'01000000'                            0085 00162000
         BNZ   @RF00085                                            0085 00163000
*         /***********************************************************/ 00164000
*         /*                                                         */ 00165000
*         /* SEGMENT RTCRVRB DOES PROCESSING THAT IS PERTINENT ONLY  */ 00166000
*         /* TO ERRORS THAT ARE POTENTIALLY RECOVERABLE. IF DUMP     */ 00167000
*         /* OPTIONS WERE PASSED BY THE USER IT ENSURES THEY ARE NOT */ 00168000
*         /* IN PROTECTED STORAGE BEFORE MOVING THEM INTO THE RTM2WA.*/ 00169000
*         /* IT THEN INCLUDES SEGMENT RTCTRINT TO BEGIN THE          */ 00170000
*         /* PROCESSING NECESSARY TO GO TO RECOVERY EXITS. WHEN ALL  */ 00171000
*         /* RECOVERY ROUTINES HAVE BEEN PROCESSED, IF THE ERROR HAS */ 00172000
*         /* NOT BEEN RECOVERED,SEGMENT RTCSCOPE DETERMINES IF THE   */ 00173000
*         /* SCOPE OF THE ABEND IS THE CURRENT TASK AND ITS SUBTASKS */ 00174000
*         /* OR THE ENTIRE JOB STEP TREE. ADDITIONALLY, THE IQE CHAIN*/ 00175000
*         /* IS SEARCHED AND ANY THAT EXIST FOR THE CURRENT TASK ARE */ 00176000
*         /* FLAGGED TO PREVENT THEIR EVER BEING SCHEDULED. ON ENTRY */ 00177000
*         /* REGISTER 8 LOCATES THE RTM2WA, REGISTER 4,THE TCB,      */ 00178000
*         /* REGISTER 5, THE SVRB.                                   */ 00179000
*         /*                                                         */ 00180000
*         /***********************************************************/ 00181000
*                                                                  0086 00182000
*         DO;                       /* BEGIN SEGMENT RTCRVRB         */ 00183000
*           IF RTM2RECR='1'B&RTM2SCTR(RTM2DMPS)='1'B THEN/* RECURSION   00184000
*                                      IN THIS SECTION               */ 00185000
         TM    RTM2RECR(R8),B'10000000'                            0087 00186000
         BNO   @RF00087                                            0087 00187000
         TM    RTM2SCTR(R8),B'00100000'                            0087 00188000
         BNO   @RF00087                                            0087 00189000
*             DO;                                                  0088 00190000
*               RTM2RCRX='1'B;      /* SET RECURSION EXIT FLAG       */ 00191000
         OI    RTM2RCRX(R8),B'10000000'                            0089 00192000
*               RTM2SCTX(RTM2DMPS)='1'B;/* SET SECTION FLAG FOR EXIT */ 00193000
         OI    RTM2SCTX(R8),B'00100000'                            0090 00194000
*             END;                                                 0091 00195000
*           ELSE                    /* NO RECURSION IN THIS SECTION  */ 00196000
*             DO;                                                  0092 00197000
         B     @RC00087                                            0092 00198000
@RF00087 DS    0H                                                  0093 00199000
*               RTM2SCTC(RTM2DMPS)='1'B;/* SET CURRENT SECTION FLAG  */ 00200000
         OI    RTM2SCTC(R8),B'00100000'                            0093 00201000
*               GEN REFS(R8) SETS(RTM2SFSA)                        0094 00202000
*                   (STM R14,R12,RTM2SFSA+12(R8));/* SAVE REGISTERS  */ 00203000
         STM R14,R12,RTM2SFSA+12(R8)                                    00204000
*               RTM2SKRA=ADDR(RCR0004);/* SET UP RECURSION ADDRESS 0095 00205000
*                                      TEST FOR RECURSION ACCESSING     00206000
*                                      DUMP OPTIONS                  */ 00207000
         LA    @12,RCR0004                                         0095 00208000
         ST    @12,RTM2SKRA(,R8)                                   0095 00209000
*               IF ESAABTRM=OFF&RTM2R0DP=ON THEN/* ENTRY VIA SVC 13,    00210000
*                                      DUMP OPTIONS PASSED           */ 00211000
         TM    ESAABTRM+96(R5),B'01000000'                         0096 00212000
         BNZ   @RF00096                                            0096 00213000
         TM    RTM2R0DP(R8),B'00100000'                            0096 00214000
         BNO   @RF00096                                            0096 00215000
*                 DO;                                              0097 00216000
*                                                                  0097 00217000
*                   /*************************************************/ 00218000
*                   /*                                               */ 00219000
*                   /* VALIDITY CHECK DUMP OPTIONS                   */ 00220000
*                   /*                                               */ 00221000
*                   /*************************************************/ 00222000
*                                                                  0098 00223000
*                   RESPECIFY                                      0098 00224000
*                    (R10,                                         0098 00225000
*                     R2) RESTRICTED;/* RESTRICT REGS USED BY MODESET*/ 00226000
*                                                                  0098 00227000
*                   /*************************************************/ 00228000
*                   /*                                               */ 00229000
*                   /* ASSUME KEY AT TIME OF ERROR                   */ 00230000
*                   /*                                               */ 00231000
*                   /*************************************************/ 00232000
*                                                                  0099 00233000
*                   R2=RTM2EPSW(2);                                0099 00234000
         SLR   R2,R2                                               0099 00235000
         IC    R2,RTM2EPSW+1(,R8)                                  0099 00236000
*                   GENERATE SETS(R10,R2,RTM2PKEY);                0100 00237000
         MODESET KEYADDR=(2),SAVEKEY=RTM2PKEY(R8),WORKREG=10            00238000
*                   RESPECIFY                                      0101 00239000
*                    (R10,                                         0101 00240000
*                     R2) UNRESTRICTED;                            0101 00241000
*                   RESPECIFY                                      0102 00242000
*                     R11 RESTRICTED;                              0102 00243000
*                   R11=ESAR0->SNPPARMS(1);/* ACCESS FIRST BYTE OF 0103 00244000
*                                      DUMP PARAMETER LIST           */ 00245000
         L     @12,ESAR0+96(,R5)                                   0103 00246000
         SLR   R11,R11                                             0103 00247000
         IC    R11,SNPPARMS(,@12)                                  0103 00248000
*                   R11=ESAR0->SNPPARMS(LENGTH(SNPPARMS));/* ACCESS     00249000
*                                      LAST BYTE                     */ 00250000
         SLR   R11,R11                                             0104 00251000
         IC    R11,SNPPARMS+23(,@12)                               0104 00252000
*                   IF ESAR0->SNPSLIST=ON THEN/* STORAGE LIST      0105 00253000
*                                      SUPPLIED                      */ 00254000
         TM    SNPSLIST(@12),B'00000010'                           0105 00255000
         BNO   @RF00105                                            0105 00256000
*                     DO;                                          0106 00257000
*                       R11=ESAR0->SNPSTOR->STORFRM(1);/* TEST FIRST    00258000
*                                      BYTE                          */ 00259000
         L     @12,SNPSTOR(,@12)                                   0107 00260000
         L     R11,STORFRM(,@12)                                   0107 00261000
*                       RESPECIFY                                  0108 00262000
*                         R9 RESTRICTED;                           0108 00263000
*                       DO R9=1 TO RGENO WHILE(ESAR0->SNPSTOR->STORBTO( 00264000
*                             R9)=OFF);/*                    @YM02793*/ 00265000
         LA    R9,1                                                0109 00266000
@DL00109 LR    @12,R9                                              0109 00267000
         SLA   @12,3                                               0109 00268000
         L     @10,ESAR0+96(,R5)                                   0109 00269000
         L     @10,SNPSTOR(,@10)                                   0109 00270000
         ALR   @10,@12                                             0109 00271000
         AL    @10,@CF02720                                        0109 00272000
         TM    STORBTO-4(@10),B'10000000'                          0109 00273000
         BNZ   @DC00109                                            0109 00274000
*                       END;                                       0110 00275000
         AL    R9,@CF00071                                         0110 00276000
         C     R9,@CF00102                                         0110 00277000
         BNH   @DL00109                                            0110 00278000
@DC00109 DS    0H                                                  0111 00279000
*                       RESPECIFY                                  0111 00280000
*                         R9 UNRESTRICTED;                         0111 00281000
*                     END;                                         0112 00282000
*                   ELSE                                           0113 00283000
*                     ;             /* STORAGE LIST NOT SUPPLIED     */ 00284000
@RF00105 DS    0H                                                  0114 00285000
*                   RESPECIFY                                      0114 00286000
*                     R11 UNRESTRICTED;                            0114 00287000
*                   RESPECIFY                                      0115 00288000
*                    (R10,                                         0115 00289000
*                     R2) RESTRICTED;                              0115 00290000
*                                                                  0115 00291000
*                   /*************************************************/ 00292000
*                   /*                                               */ 00293000
*                   /* RESTORE RTM2 PROTECT KEY                      */ 00294000
*                   /*                                               */ 00295000
*                   /*************************************************/ 00296000
*                                                                  0116 00297000
*                   GENERATE REFS(RTM2PKEY) SETS(R10,R2);          0116 00298000
         MODESET KEYADDR=RTM2PKEY(R8),WORKREG=10               @YM01515 00299000
*                   RESPECIFY                                      0117 00300000
*                    (R10,                                         0117 00301000
*                     R2) UNRESTRICTED;                            0117 00302000
*                                                                  0117 00303000
*                   /*************************************************/ 00304000
*                   /*                                               */ 00305000
*                   /* PROCESS DUMP OPTIONS                          */ 00306000
*                   /*                                               */ 00307000
*                   /*************************************************/ 00308000
*                                                                  0118 00309000
*                   SNPSDATA=SNPSDATA|ESAR0->SNPSDATA;/* COPY SDATA     00310000
*                                      OPTIONS FROM INPUT LIST     0118 00311000
*                                                            @YM03570*/ 00312000
         LA    @12,256                                             0118 00313000
         ALR   @12,R8                                              0118 00314000
         L     @09,ESAR0+96(,R5)                                   0118 00315000
         OC    SNPSDATA(2,@12),SNPSDATA(@09)                       0118 00316000
*                   SNPPDATA=SNPPDATA|ESAR0->SNPPDATA;/* COPY PDATA     00317000
*                                      OPTIONS FROM INPUT LIST     0119 00318000
*                                                            @YM03570*/ 00319000
         OC    SNPPDATA(1,@12),SNPPDATA(@09)                       0119 00320000
*                   IF ESAR0->SNPSLIST=ON&ESAR0->SNPSTOR^=0 THEN/* 0120 00321000
*                                      STORAGE LIST SUPPLIED @YM03570*/ 00322000
         TM    SNPSLIST(@09),B'00000010'                           0120 00323000
         BNO   @RF00120                                            0120 00324000
         L     @09,SNPSTOR(,@09)                                   0120 00325000
         LTR   @09,@09                                             0120 00326000
         BZ    @RF00120                                            0120 00327000
*                     DO;                                          0121 00328000
*                       RFY                                        0122 00329000
*                        (R9,                                      0122 00330000
*                         R10) RSTD;/* RESTRICT LOOP COUNTERS      0122 00331000
*                                                            @YM03570*/ 00332000
*                       IF SNPSLIST=ON THEN/* SLIST ALREADY EXISTS 0123 00333000
*                                                            @YM03570*/ 00334000
         TM    SNPSLIST(@12),B'00000010'                           0123 00335000
         BNO   @RF00123                                            0123 00336000
*                         DO;       /*                       @YM03570*/ 00337000
*                           R9=1;   /* START WITH FIRST      @YM03570*/ 00338000
         LA    R9,1                                                0125 00339000
*                           DO WHILE(STORBTO(R9)=OFF);/* LOCATE LAST    00340000
*                                      RANGE IN CURRENT LIST @YM03570*/ 00341000
         B     @DE00126                                            0126 00342000
@DL00126 DS    0H                                                  0127 00343000
*                             R9=R9+1;/*                     @YM03570*/ 00344000
         AL    R9,@CF00071                                         0127 00345000
*                           END;    /*                       @YM03570*/ 00346000
@DE00126 LR    @12,R9                                              0128 00347000
         SLA   @12,3                                               0128 00348000
         ALR   @12,R8                                              0128 00349000
         TM    STORBTO+268(@12),B'10000000'                        0128 00350000
         BZ    @DL00126                                            0128 00351000
*LRTCA:                                                            0129 00352000
*                           ;       /* PREVENT USE OF COMPILER     0129 00353000
*                                      TEMPORARY             @YM03570*/ 00354000
LRTCA    DS    0H                                                  0130 00355000
*                           STORBTO(R9)=OFF;/* CLEAR LAST ENTRY FLAG    00356000
*                                      IN CURRENT LIST       @YM03570*/ 00357000
         LR    @12,R9                                              0130 00358000
         SLA   @12,3                                               0130 00359000
         ALR   @12,R8                                              0130 00360000
         NI    STORBTO+268(@12),B'01111111'                        0130 00361000
*                           R9=R9+1;/* SET COUNTER TO FIRST ENTRY TO    00362000
*                                      BE ADDED              @YM03570*/ 00363000
         AL    R9,@CF00071                                         0131 00364000
*                         END;      /* END OF CURRENT SLIST        0132 00365000
*                                      PROCESSING            @YM03570*/ 00366000
*                       ELSE        /* NO STORAGE LIST YET   @YM03570*/ 00367000
*                         R9=1;     /* FIRST ENTRY WILL BE FIRST ONE    00368000
*                                      USED                  @YM03570*/ 00369000
         B     @RC00123                                            0133 00370000
@RF00123 LA    R9,1                                                0133 00371000
*                       RTM2DEND=OFF;/* FORCE ENTRY INTO LOOP      0134 00372000
*                                                            @YM03570*/ 00373000
@RC00123 NI    RTM2DEND(R8),B'10111111'                            0134 00374000
*                       DO R10=1 TO RGENO WHILE(RTM2DEND=OFF);/* LOOP   00375000
*                                      OVER INPUT LIST       @YM03570*/ 00376000
         LA    R10,1                                               0135 00377000
@DL00135 TM    RTM2DEND(R8),B'01000000'                            0135 00378000
         BNZ   @DC00135                                            0135 00379000
*                         IF R9>RGENO THEN/* PAST END OF OUTPUT LIST    00380000
*                                      -- START AGAIN FROM TOP     0136 00381000
*                                                            @YM03570*/ 00382000
         C     R9,@CF00102                                         0136 00383000
         BNH   @RF00136                                            0136 00384000
*                           DO;     /*                       @YM03570*/ 00385000
*                             R9=1; /* START WITH FIRST ENTRY      0138 00386000
*                                                            @YM03570*/ 00387000
         LA    R9,1                                                0138 00388000
*                             RTM2WRAP=ON;/* INDICATE ENTRIES HAVE 0139 00389000
*                                      WRAPPED AROUND        @YM03570*/ 00390000
         OI    RTM2WRAP(R8),B'00010000'                            0139 00391000
*                           END;    /*                       @YM03570*/ 00392000
*                         STORAGE(R9)=ESAR0->SNPSTOR->STORAGE(R10);/*   00393000
*                                      COPY ONE STORAGE RANGE      0141 00394000
*                                                            @YM03570*/ 00395000
@RF00136 LR    @12,R9                                              0141 00396000
         SLA   @12,3                                               0141 00397000
         ALR   @12,R8                                              0141 00398000
         LR    @11,R10                                             0141 00399000
         SLA   @11,3                                               0141 00400000
         L     @07,ESAR0+96(,R5)                                   0141 00401000
         L     @07,SNPSTOR(,@07)                                   0141 00402000
         ALR   @07,@11                                             0141 00403000
         AL    @07,@CF02723                                        0141 00404000
         MVC   STORAGE+268(8,@12),STORAGE(@07)                     0141 00405000
*LRTCB:                                                            0142 00406000
*                         ;         /* PREVENT USE OF COMPILER     0142 00407000
*                                      TEMPORARY             @YM03570*/ 00408000
