*                                        CVTUSER)                  0742 02854000
*(CALLRTM TYPE=ABTERM,TCB=(R7),COMPCOD=X'10D',DUMP=NO);            0742 02855000
         CALLRTM TYPE=ABTERM,TCB=(R7),COMPCOD=X'10D',DUMP=NO            02856000
*                                    RTM2RSCN=ON;/* IGNORE TREE BELOW   02857000
