*             /*                                                     */ 01435000
*             /*******************************************************/ 01436000
*                                                                  0381 01437000
*             RESPECIFY                                            0381 01438000
*               R14 RESTRICTED;     /* RESTRICT RETURN REGISTER      */ 01439000
@RF00375 DS    0H                                                  0381 01440000
@RC00368 DS    0H                                                  0382 01441000
*             R14=ESAR14;           /* ORIGINAL RETURN ADDRESS       */ 01442000
         L     R14,ESAR14+96(,R5)                                  0382 01443000
*             GEN(BR    R14);                                      0383 01444000
         BR    R14                                                      01445000
*             RESPECIFY                                            0384 01446000
*              (R0,                                                0384 01447000
*               R1,                                                0384 01448000
*               R10,                                               0384 01449000
*               R14,                                               0384 01450000
*               R15) UNRESTRICTED;  /* RELEASE ALL REGISTERS         */ 01451000
*           END;                    /* END OF RTECMEX                */ 01452000
*         ELSE                                                     0386 01453000
*           IF RTM2PRX=ON           /* PERMANENTLY RESIDENT TASK   0386 01454000
*                                      ENDING                        */ 01455000
*               |RTM2LTX=ON THEN    /* LAST TASK IN MEMORY ENDING    */ 01456000
*                                                                  0386 01457000
         B     @RC00326                                            0386 01458000
@RF00326 TM    RTM2PRX(R8),B'00000110'                             0386 01459000
         BZ    @RF00386                                            0386 01460000
*             /*******************************************************/ 01461000
*             /*                                                     */ 01462000
*             /* SEGMENT RTELTEX DOES THE EXIT FUNCTION IF THE TASK  */ 01463000
*             /* THAT IS ENDING IS THE LAST TASK IN THE MEMORY OR IF */ 01464000
*             /* IT IS A PERMANENTLY RESIDENT TASK. IF IT IS THE LAST*/ 01465000
*             /* TASK IN THE MEMORY, THE CALLRTM MACRO IS ISSUED TO  */ 01466000
*             /* TERMINATE THE MEMORY. IF IT IS A PERMANENTLY        */ 01467000
*             /* RESIDENT TASK THE EOT TCB IS POSTED. THIS IS DONE   */ 01468000
*             /* BECAUSE EXIT IS NOT CALLED FOR PERMANENTLY RESIDENT */ 01469000
*             /* TASKS AS THE TCB AND RB CANNOT BE FREED. IN BOTH    */ 01470000
*             /* CASES STATUS IS THEN INVOKED TO SET THE TASK        */ 01471000
*             /* PERMANENTLY NON-DISPATCHABLE                        */ 01472000
*             /*                                                     */ 01473000
*             /*******************************************************/ 01474000
*                                                                  0387 01475000
*             DO;                                                  0387 01476000
*               IF RTM2LTX=ON THEN  /* LAST TASK IN MEMORY IS ENDING.   01477000
*                                      CALLRTM TO REMOVE THE MEMORY  */ 01478000
         TM    RTM2LTX(R8),B'00000010'                             0388 01479000
         BNO   @RF00388                                            0388 01480000
*                 DO;                                              0389 01481000
*                   RESPECIFY                                      0390 01482000
*                    (R9,                                          0390 01483000
*                     R3) RESTRICTED;/* RESTRICT REGISTERS USED BY 0390 01484000
*                                      RTM1                          */ 01485000
*                   R3=NORM;        /* SET COMP CODE TO ZERO         */ 01486000
         SLR   R3,R3                                               0391 01487000
*                   R9=RMPLASID;    /* SET REG 2 WITH ASID OF MEMORY    01488000
*                                      TO BE TERMINATED              */ 01489000
         LH    R9,RMPLASID+712(,R8)                                0392 01490000
         N     R9,@CF02575                                         0392 01491000
*                   R13=ADDR(RTM2RMSA);/* SET SAVE AREA ADDR IN REG     01492000
*                                      13                            */ 01493000
         LA    R13,RTM2RMSA(,R8)                                   0393 01494000
*                   GENERATE REFS(CVTBTERM,CVTMAP,R9,R3,R13,RTM2RMSA,   01495000
*                       CVTUSER);                                  0394 01496000
         CALLRTM TYPE=MEMTERM,ASID=(R9),COMPCOD=(R3)                    01497000
*                   RESPECIFY                                      0395 01498000
*                    (R9,                                          0395 01499000
*                     R3) UNRESTRICTED;                            0395 01500000
*                                                                  0395 01501000
*                   /*************************************************/ 01502000
*                   /*                                               */ 01503000
*                   /* BRANCH ENTER STATUS TO SET CURRENT TASK       */ 01504000
*                   /* PERMANENTLY NON-DISPATCHABLE. STATUS WILL     */ 01505000
*                   /* RELEASE THE LOCAL LOCK                        */ 01506000
*                   /*                                               */ 01507000
*                   /*************************************************/ 01508000
*                                                                  0396 01509000
*                   RESPECIFY                                      0396 01510000
*                    (R0,                                          0396 01511000
*                     R1) RESTRICTED;/* RESTRICT PARAMETER REGISTERS    01512000
*                                      FOR STATUS                    */ 01513000
*                   R0=SENTCD15;    /* SET STATUS ENTRY CODE IN REG 0*/ 01514000
         LA    R0,15                                               0397 01515000
*                   R1=R4;          /* SET CURRENT TCB ADDRESS IN REG   01516000
*                                      1                             */ 01517000
         LR    R1,R4                                               0398 01518000
*                   R13=DARPNDBE;   /* SET NON-DISPATCHABILITY MASK     01519000
*                                      IN R13                        */ 01520000
         L     R13,@CF00053                                        0399 01521000
*                   CALL STATUS;    /* INVOKE STATUS                 */ 01522000
         L     @12,CVTPTR+96(,R5)                                  0400 01523000
         L     @12,SCVTSPTR(,@12)                                  0400 01524000
         L     @15,SCVTSTAT(,@12)                                  0400 01525000
         BALR  @14,@15                                             0400 01526000
*                   RESPECIFY                                      0401 01527000
*                    (R0,                                          0401 01528000
*                     R1) UNRESTRICTED;                            0401 01529000
*                 END;                                             0402 01530000
*               ELSE                /* IF TASK IS PERMANENTLY      0403 01531000
*                                      RESIDENT MEMORY SHOULD NOT BE    01532000
*                                      TERMINATED                    */ 01533000
*                                                                  0403 01534000
*                 /***************************************************/ 01535000
*                 /*                                                 */ 01536000
*                 /* POST EOT ECB IF PERMANENTLY RESIDENT TASK IS    */ 01537000
*                 /* ENDING AS EXIT WILL NOT BE ENTERED TO DO THIS   */ 01538000
*                 /*                                                 */ 01539000
*                 /***************************************************/ 01540000
*                                                                  0403 01541000
*                 DO;                                              0403 01542000
         B     @RC00388                                            0403 01543000
@RF00388 DS    0H                                                  0404 01544000
*                   RESPECIFY                                      0404 01545000
*                    (R10,                                         0404 01546000
*                     R11,                                         0404 01547000
*                     R12) RESTRICTED;/* RESTRICT REGISTERS USED BY     01548000
*                                      POST                          */ 01549000
*                   R10=RTM2CC;     /* SET POST COMPLETION CODE IN 0405 01550000
*                                      REG 10                        */ 01551000
         L     R10,RTM2CC-1(,R8)                                   0405 01552000
         LA    R10,0(,R10)                                         0405 01553000
*                   R11=TCBECB;     /* SET ADDRESS OF ECB IN REG 11  */ 01554000
         L     R11,TCBECB(,R4)                                     0406 01555000
*                   IF R11^=0 THEN  /* EOT ECB EXISTS                */ 01556000
         LTR   R11,R11                                             0407 01557000
         BZ    @RF00407                                            0407 01558000
*                     DO;                                          0408 01559000
*                       CALL POST;  /* INVOKE POST                   */ 01560000
         L     @09,CVTPTR+96(,R5)                                  0409 01561000
         L     @15,CVT0PT01(,@09)                                  0409 01562000
         BALR  @14,@15                                             0409 01563000
*                     END;                                         0410 01564000
*                   ELSE                                           0411 01565000
*                     ;             /* NO ECB                        */ 01566000
@RF00407 DS    0H                                                  0412 01567000
*                   RESPECIFY                                      0412 01568000
*                    (R10,                                         0412 01569000
*                     R11,                                         0412 01570000
*                     R12) UNRESTRICTED;/* RELEASE REGISTERS         */ 01571000
*                                                                  0412 01572000
*                   /*************************************************/ 01573000
*                   /*                                               */ 01574000
*                   /* BRANCH ENTER STATUS TO SET CURRENT TASK       */ 01575000
*                   /* PERMANENTLY NON-DISPATCHABLE USING TCBFC.     */ 01576000
*                   /* STATUS WILL RELEASE THE LOCAL LOCK            */ 01577000
*                   /*                                               */ 01578000
*                   /*************************************************/ 01579000
*                                                                  0413 01580000
*                   RESPECIFY                                      0413 01581000
*                    (R0,                                          0413 01582000
*                     R1) RESTRICTED;                              0413 01583000
*                   R0=FCBE;        /* SET R0 WITH REQUEST FOR TCBFC    01584000
*                                      NON-DISPATCHABILITY           */ 01585000
         L     R0,@CF00057                                         0414 01586000
*                   R1=R4;          /* INDICATE CURRENT TCB          */ 01587000
