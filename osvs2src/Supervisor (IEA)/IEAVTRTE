*                 END;                                             0324 01200000
*         END;                      /* END OF RTERCREX               */ 01201000
*       ELSE                        /* NOT RECURSION                 */ 01202000
*         IF RTM2EOTX=ON            /* NORMAL END OF TASK            */ 01203000
*             |RTM2MTX=ON           /* MEMORY TERMINATION            */ 01204000
