*               DO;                                                0539 02099000
*                                                                  0539 02100000
*                 /***************************************************/ 02101000
*                 /*                                                 */ 02102000
*                 /* SEGMENT RTEWADQ REMOVES THE RTM2WA FROM THE TCB */ 02103000
*                 /* QUEUE AND FROM THE ASCB QUEUE IF NECESSARY.(IF  */ 02104000
*                 /* THE WORK AREA WAS OBTAINED FROM SQA IT WAS      */ 02105000
*                 /* QUEUED TO THE ASCB AS WELL AS THE TCB). THE     */ 02106000
*                 /* QUEUE IS CHAINED SUCH THAT THE ASCB OR TCB      */ 02107000
*                 /* POINTS TO THE MOST RECENT ONE QUEUED AND THE    */ 02108000
*                 /* EARLIEST ONE QUEUED CONTAINS ZERO IN ITS CHAIN  */ 02109000
*                 /* FIELD.                                          */ 02110000
*                 /*                                                 */ 02111000
*                 /***************************************************/ 02112000
*                                                                  0540 02113000
*                 DO;               /* BEGIN SEGMENT RTEWADQ         */ 02114000
*                   RESPECIFY                                      0541 02115000
*                     R2 RESTRICTED;/* RESTRICT WORK REGISTER        */ 02116000
