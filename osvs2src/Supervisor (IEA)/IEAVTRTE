*               '0'B);                                             0504 01957000
         B     @DE00504                                            0504 01958000
@DL00504 DS    0H                                                  0505 01959000
*           R9=R9->RBLINKB;         /* LOCATE NEXT RB                */ 01960000
         L     @11,RBLINKB-1(,R9)                                  0505 01961000
         LA    @11,0(,@11)                                         0505 01962000
         LR    R9,@11                                              0505 01963000
*         END;                      /* END SCAN OF RBS               */ 01964000
@DE00504 LR    @11,R9                                              0506 01965000
         SL    @11,@CF00453                                        0506 01966000
         TM    RBABEND(@11),B'00100000'                            0506 01967000
         BNZ   @DC00504                                            0506 01968000
