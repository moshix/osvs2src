         BZ    @RT00349                                            0349 01325000
         TM    ESANEOT+96(R9),B'00001010'                          0349 01326000
         BZ    @RF00349                                            0349 01327000
@RT00349 DS    0H                                                  0350 01328000
*               DO;                                                0350 01329000
*                 DO;               /* START OF RTECLRFL             */ 01330000
*                                                                  0351 01331000
*                   /*************************************************/ 01332000
*                   /*                                               */ 01333000
*                   /* ALL FLAGS IN THE TCB ARE CLEARED SO THAT ALL  */ 01334000
*                   /* TRACES OF RTM2 WILL BE GONE                   */ 01335000
*                   /*                                               */ 01336000
*                   /*************************************************/ 01337000
*                                                                  0352 01338000
*                   TCBFX=OFF;      /* PERMIT ASYNCHRONOUS EXITS     */ 01339000
*                   TCBFT=OFF;      /* TOP TCB FLAG                  */ 01340000
         NI    TCBFX(R4),B'11111010'                               0353 01341000
*                   TCBFPRAP=OFF;   /* PERMIT PERCOLATION OF       0354 01342000
*                                      ASYNCHRONOUS ERRORS           */ 01343000
         NI    TCBFPRAP(R4),B'11101111'                            0354 01344000
*                 END;                                             0355 01345000
*                 IF R10^=0 THEN    /* RETRY CASE                    */ 01346000
         LTR   R10,R10                                             0356 01347000
         BZ    @RF00356                                            0356 01348000
*                   TCBRTM2=OFF;    /* CLEAR RTM2 FLAG               */ 01349000
         NI    TCBRTM2(R4),B'11110111'                             0357 01350000
*               END;                                               0358 01351000
*             ELSE                                                 0359 01352000
*               ;                   /* RETRY WILL STILL LEAVE RTM2 RB   01353000
*                                      ON QUEUE                      */ 01354000
@RF00349 DS    0H                                                  0360 01355000
*             RESPECIFY                                            0360 01356000
*               R9 UNRESTRICTED;    /* RELEASE POINTER TO NEXT RTM2     01357000
*                                      RB                            */ 01358000
*                                                                  0360 01359000
@RC00349 DS    0H                                                  0361 01360000
*             /*******************************************************/ 01361000
*             /*                                                     */ 01362000
*             /* RELEASE LOCAL LOCK                                  */ 01363000
*             /*                                                     */ 01364000
*             /*******************************************************/ 01365000
*                                                                  0361 01366000
*             ESAGLREC=ON;                                         0361 01367000
