         CLC   RTM2SCTX(4,R8),@CB00066                             0176 00618000
         BNE   @RF00176                                            0176 00619000
*             RTM2PRWA=RTM2PREV;    /* IMMEDIATELY PREVIOUS WORK AREA   00620000
