*                 R15=RBGRS15;                                     0373 01411000
         L     R15,RBGRS15(,R5)                                    0373 01412000
*               END;                                               0374 01413000
*             ELSE                  /* NOT RETRY                     */ 01414000
*               IF ESAEOT=ON THEN   /* TASK TERMINATING              */ 01415000
         B     @RC00368                                            0375 01416000
@RF00368 LA    @14,96                                              0375 01417000
         ALR   @14,R5                                              0375 01418000
         TM    ESAEOT(@14),B'00000100'                             0375 01419000
         BNO   @RF00375                                            0375 01420000
*                 DO;                                              0376 01421000
*                   TCBEOT=ON;      /* SET FLAG SHOWING EOT DONE     */ 01422000
         OI    TCBEOT(R4),B'00000100'                              0377 01423000
*                   IF ESANEOT=OFF THEN                            0378 01424000
