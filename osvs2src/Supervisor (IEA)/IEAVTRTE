*                                      ATED(TCB,IEAVTRTE)            */ 01387000
*             ESAGLREC=OFF;                                        0367 01388000
*                                                                  0367 01389000
         NI    ESAGLREC+96(R5),B'11101111'                         0367 01390000
*             /*******************************************************/ 01391000
*             /*                                                     */ 01392000
*             /* IF RETRY, SET REGISTERS 0,1, AND 15 WITH REQUESTED  */ 01393000
*             /* VALUES                                              */ 01394000
*             /*                                                     */ 01395000
*             /*******************************************************/ 01396000
*                                                                  0368 01397000
*             IF R10^=0 THEN        /* RETRY REQUEST                 */ 01398000
         LTR   R10,R10                                             0368 01399000
         BZ    @RF00368                                            0368 01400000
*               DO;                                                0369 01401000
*                 RESPECIFY                                        0370 01402000
*                  (R0,                                            0370 01403000
*                   R1,                                            0370 01404000
*                   R15) RESTRICTED;/* RESTRICT REGS PASSED TO RETRY    01405000
*                                      ROUTINE                       */ 01406000
*                 R0=RBGRS0;                                       0371 01407000
