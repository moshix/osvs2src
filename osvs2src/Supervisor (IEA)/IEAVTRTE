*             R13=ADDR(RTM2SFSA);                                  0330 01232000
         LA    R13,RTM2SFSA(,R8)                                   0330 01233000
*             GEN FLOWS(CMEXRCR)(STM   R14,R12,12(R13));/* SAVE REGS    01234000
*                                      FOR RECUR.                    */ 01235000
         STM   R14,R12,12(R13)                                          01236000
*             RTM2SKRA=ADDR(CMEXRCR);/* SET RECURSION ADDRESS TO SKIP   01237000
*                                      FREEING WORKAREAS             */ 01238000
*                                                                  0332 01239000
         LA    @12,CMEXRCR                                         0332 01240000
         ST    @12,RTM2SKRA(,R8)                                   0332 01241000
*             /*******************************************************/ 01242000
*             /*                                                     */ 01243000
*             /* FREE RTM2 WORK AREAS                                */ 01244000
*             /*                                                     */ 01245000
*             /*******************************************************/ 01246000
*                                                                  0333 01247000
*             IF RTM2EOTX=ON|RTM2ABX=ON THEN/* TASK IS TERMINATING   */ 01248000
         TM    RTM2EOTX(R8),B'01100000'                            0333 01249000
         BZ    @RF00333                                            0333 01250000
*               ESAEOT=ON;          /* SAVE FLAG IN ESA              */ 01251000
         OI    ESAEOT+96(R5),B'00000100'                           0334 01252000
*             CALL RTEFREWA;                                       0335 01253000
@RF00333 BAL   @14,RTEFREWA                                        0335 01254000
*CMEXRCR:                           /* REACHED IF RTEFREWA FAILS     */ 01255000
*             IF ESART2WA^=0 THEN   /* CURRENT WORK AREA NOT FREED   */ 01256000
CMEXRCR  LA    @12,96                                              0336 01257000
         ALR   @12,R5                                              0336 01258000
         L     @11,ESART2WA(,@12)                                  0336 01259000
         LTR   @11,@11                                             0336 01260000
         BZ    @RF00336                                            0336 01261000
*               ESAGMREC=ON;        /* SET RECURSION FLAG            */ 01262000
*                                                                  0337 01263000
         OI    ESAGMREC(@12),B'10000000'                           0337 01264000
*             /*******************************************************/ 01265000
*             /*                                                     */ 01266000
*             /* CLEAR RTM2 FLAGS IN TCB                             */ 01267000
*             /*                                                     */ 01268000
*             /*******************************************************/ 01269000
*                                                                  0338 01270000
*             IF R10^=0 THEN        /* IF RETRY, FLAGS ARE CLEARED 0338 01271000
*                                      ONLY IF NO MORE RTM2 RBS      */ 01272000
@RF00336 LTR   R10,R10                                             0338 01273000
         BZ    @RF00338                                            0338 01274000
*               DO;                                                0339 01275000
*                 RESPECIFY                                        0340 01276000
*                   R9 RESTRICTED;                                 0340 01277000
*                 R9=R10;           /* LOCATE STARTING RB            */ 01278000
*                                                                  0341 01279000
         LR    R9,R10                                              0341 01280000
*                 /***************************************************/ 01281000
*                 /*                                                 */ 01282000
*                 /* SCAN FOR RB WITH RBABEND ON. SCAN TERMINATED    */ 01283000
*                 /* WHEN RBTCBNXT ON                                */ 01284000
*                 /*                                                 */ 01285000
*                 /***************************************************/ 01286000
*                                                                  0342 01287000
*                 DO WHILE(EVAL(R9-LENGTH(RBPRFX))->RBABEND='0'B&R9->   01288000
*                       RBTCBNXT='0'B);                            0342 01289000
         B     @DE00342                                            0342 01290000
@DL00342 DS    0H                                                  0343 01291000
*                   R9=R9->RBLINKB; /* LOCATE NEXT RB                */ 01292000
         L     @12,RBLINKB-1(,R9)                                  0343 01293000
         LA    @12,0(,@12)                                         0343 01294000
         LR    R9,@12                                              0343 01295000
*                 END;              /* END SCAN OF RBS               */ 01296000
@DE00342 LR    @12,R9                                              0344 01297000
         SL    @12,@CF00453                                        0344 01298000
         TM    RBABEND(@12),B'00100000'                            0344 01299000
         BNZ   @DC00342                                            0344 01300000
         TM    RBTCBNXT(R9),B'10000000'                            0344 01301000
         BZ    @DL00342                                            0344 01302000
@DC00342 DS    0H                                                  0345 01303000
*                 IF EVAL(R9-LENGTH(RBPRFX))->RBABEND='0'B THEN/*  0345 01304000
*                                      CHECK WHETHER ONE WAS FOUND   */ 01305000
         LR    @12,R9                                              0345 01306000
         SL    @12,@CF00453                                        0345 01307000
         TM    RBABEND(@12),B'00100000'                            0345 01308000
         BNZ   @RF00345                                            0345 01309000
*                   R9=0;           /* NO RTM2 RB FOUND              */ 01310000
         SLR   R9,R9                                               0346 01311000
*               END;                                               0347 01312000
*             ELSE                  /* NOT RETRY                     */ 01313000
*               R9=0;               /* THEREFORE NO RTM2 RBS WILL  0348 01314000
*                                      REMAIN                        */ 01315000
         B     @RC00338                                            0348 01316000
@RF00338 SLR   R9,R9                                               0348 01317000
*             IF R9=0               /* NO MORE RTM2 RBS WILL BE LEFT    01318000
*                                                            @YM02686*/ 01319000
*                 |ADDR(R9->RBEXSAVE)->ESANEOT=ON/* OR RB IS FOR   0349 01320000
*                                      NORMAL EOT            @YM02686*/ 01321000
*                 |ADDR(R9->RBEXSAVE)->ESAVEOM=ON THEN/* OR RB IS FOR   01322000
*                                      END OF MEMORY         @YM02686*/ 01323000
