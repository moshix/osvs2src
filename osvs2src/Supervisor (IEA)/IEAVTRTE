*   R12=R14;                        /* SAVE RETURN ADDRESS           */ 01922000
         LR    R12,R14                                             0495 01923000
*   RESPECIFY                                                      0496 01924000
*     R14 UNRESTRICTED;                                            0496 01925000
*   IF RTM2RETR=ON THEN             /* RETRY WAS REQUESTED           */ 01926000
         TM    RTM2RETR(R8),B'01000000'                            0497 01927000
         BNO   @RF00497                                            0497 01928000
*     DO;                                                          0498 01929000
*       RESPECIFY                                                  0499 01930000
*         R10 RESTRICTED;           /* RESTRICT REG USED TO HOLD   0499 01931000
*                                      RETRY RB ADDRESS              */ 01932000
*       R10=RTM2RYRB;               /* SET RETRY RB REGISTER EQUAL TO   01933000
