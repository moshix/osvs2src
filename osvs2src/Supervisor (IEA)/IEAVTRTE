         ST    @12,RTM2SKRA(,R8)                                   0428 01657000
*                 RTM2STRA=0;                                      0429 01658000
*                                                                  0429 01659000
         ST    @12,RTM2STRA(,R8)                                   0429 01660000
*                 /***************************************************/ 01661000
*                 /*                                                 */ 01662000
*                 /* SET UP ENVIRONMENT TO RESUME AT JOBSTEP LEVEL   */ 01663000
*                 /*                                                 */ 01664000
*                 /***************************************************/ 01665000
*                                                                  0430 01666000
*                 RESPECIFY                                        0430 01667000
*                   R9 RESTRICTED;  /* RESTRICT PTR TO JOBSTEP       */ 01668000
*                 R9=TCBJSTCA;      /* LOCATE JOBSTEP TCB            */ 01669000
         L     R9,TCBJSTCA-1(,R4)                                  0431 01670000
         LA    R9,0(,R9)                                           0431 01671000
*                 RESPECIFY                                        0432 01672000
