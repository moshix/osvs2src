*                   R13=0;          /* INDICATE CURRENT MEMORY       */ 01589000
         SLR   R13,R13                                             0416 01590000
*                   CALL STATUS;                                   0417 01591000
         L     @12,CVTPTR+96(,R5)                                  0417 01592000
         L     @12,SCVTSPTR(,@12)                                  0417 01593000
         L     @15,SCVTSTAT(,@12)                                  0417 01594000
         BALR  @14,@15                                             0417 01595000
*                   RESPECIFY                                      0418 01596000
*                    (R0,                                          0418 01597000
*                     R1) UNRESTRICTED;                            0418 01598000
*                 END;                                             0419 01599000
*             END;                  /* END SEGMENT RTELTEX           */ 01600000
*           ELSE                                                   0421 01601000
*             IF RTM2CVX=ON THEN    /* CONVERSION TO STEP            */ 01602000
*                                                                  0421 01603000
         B     @RC00386                                            0421 01604000
@RF00386 TM    RTM2CVX(R8),B'00001000'                             0421 01605000
         BNO   @RF00421                                            0421 01606000
*               /*****************************************************/ 01607000
*               /*                                                   */ 01608000
*               /* RTECNVEX FREES ALL WORKAREAS EXCEPT THE CURRENT.  */ 01609000
*               /* IT THEN SETS UP THE JOBSTEP TCB TO BE TERMINATED  */ 01610000
*               /* AND INVOKES RTM1 TO TERMINATE IT. IT THEN CLEARS  */ 01611000
*               /* ITS OWN TCB FLAGS AND RETURNS TO THE SYSTEM.      */ 01612000
*               /*                                                   */ 01613000
*               /*****************************************************/ 01614000
*                                                                  0422 01615000
*               DO;                 /* START OF RTECNVEX             */ 01616000
*                                                                  0422 01617000
*                 /***************************************************/ 01618000
*                 /*                                                 */ 01619000
*                 /* SET UP RECURSION ENVIRONMENT                    */ 01620000
*                 /*                                                 */ 01621000
*                 /***************************************************/ 01622000
*                                                                  0423 01623000
*                 RTM2SCTC(RTM2EXTS)=OFF;/* EARLIER PROCESSING SHOULD   01624000
*                                      BE SKIPPED IF THIS SECTION  0423 01625000
*                                      FAILS                         */ 01626000
         NI    RTM2SCTC+1(R8),B'10111111'                          0423 01627000
*                 R13=ADDR(RTM2SFSA);                              0424 01628000
         LA    R13,RTM2SFSA(,R8)                                   0424 01629000
*                 GEN FLOWS(CNVEXRCR)(STM   R14,R12,12(R13));/* SAVE    01630000
*                                      REGS IN CASE OF RECURSION     */ 01631000
         STM   R14,R12,12(R13)                                          01632000
*                 RTM2SKRA=ADDR(CNVEXRCR);/* SKIP FREEMAIN IF IT   0426 01633000
*                                      FAILS                         */ 01634000
*                                                                  0426 01635000
         LA    @12,CNVEXRCR                                        0426 01636000
         ST    @12,RTM2SKRA(,R8)                                   0426 01637000
*                 /***************************************************/ 01638000
*                 /*                                                 */ 01639000
*                 /* FREE ALL PREVIOUS WORK AREAS                    */ 01640000
*                 /*                                                 */ 01641000
*                 /***************************************************/ 01642000
*                                                                  0427 01643000
*                 CALL RTEFREWA;                                   0427 01644000
         BAL   @14,RTEFREWA                                        0427 01645000
*CNVEXRCR:                          /* REACHED ON RECURSION          */ 01646000
*                                                                  0428 01647000
*                 /***************************************************/ 01648000
*                 /*                                                 */ 01649000
*                 /* CLEAR RECURSION ENVIRONMENT                     */ 01650000
*                 /*                                                 */ 01651000
*                 /***************************************************/ 01652000
*                                                                  0428 01653000
*                 RTM2SKRA=0;       /* MEMORY MUST BE TERMINATED IF     01654000
*                                      JOBSTEP CANNOT BE ABTERMED    */ 01655000
