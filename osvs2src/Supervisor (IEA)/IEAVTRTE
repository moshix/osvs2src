*         R11=0;                    /* NO RTM2 RB FOUND              */ 02049000
         SLR   R11,R11                                             0522 02050000
*     END;                                                         0523 02051000
@RF00521 DS    0H                                                  0524 02052000
*     IF(R9=R11)&RTM2CVX=OFF THEN   /* SCAN COMPLETE AND CONVERSION     02053000
*                                      EXIT NOT REQUESTED            */ 02054000
         CR    R9,R11                                              0524 02055000
         BNE   @RF00524                                            0524 02056000
         TM    RTM2CVX(R8),B'00001000'                             0524 02057000
         BNZ   @RF00524                                            0524 02058000
*       DO;                         /* PROCESS CURRENT SVRB          */ 02059000
*         R11=R5;                   /* SET REG EQUAL TO CURRENT SVRB */ 02060000
         LR    R11,R5                                              0526 02061000
