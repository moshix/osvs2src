         TITLE 'IEAVTRTE - RTM2 EXIT HANDLER                           *00001000
                        '                                               00002000
IEAVTRTE CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVTRTE  77.280'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @06,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@06                                         0001 00013000
RBPRFXLN EQU 64                                                         00014000
RBPRFLNA EQU 32                                                         00015000
PRBLEN   EQU 136                                                        00016000
SIRBLEN  EQU 200                                                        00017000
TIRBLEN  EQU 136                                                        00018000
IRBLEN   EQU 128                                                        00019000
SVRBLEN  EQU 224                                                        00020000
*                                                                  0047 00021000
*   /*****************************************************************/ 00022000
*   /*                                                               */ 00023000
*   /* IEAVTRTE DETERMINES WHETHER TASK OR MEMORY PURGES ARE REQUIRED*/ 00024000
*   /* AND ENTERS SEGMENT RTEMTINT OR RTETTINT ACCORDINGLY. THEN, IF */ 00025000
*   /* CONVERSION TO A JOBSTEP ABEND WAS REQUESTED IT ENTERS SEGMENT */ 00026000
*   /* RTECONV. IT THEN DOES EXIT PROCESSING.                        */ 00027000
*   /*                                                               */ 00028000
*   /*****************************************************************/ 00029000
*                                                                  0047 00030000
*   IF RMPLTERM=ON THEN             /* MEMORY TERMINATION            */ 00031000
*                                                                  0047 00032000
         TM    RMPLTERM+712(R8),B'01000000'                        0047 00033000
         BNO   @RF00047                                            0047 00034000
*     /***************************************************************/ 00035000
*     /*                                                             */ 00036000
*     /* RTEMTINT ESTABLISHES AN ESTAE EXIT SO THAT A FAILURE CAN BE */ 00037000
*     /* RECOVERED AND INVOKES THE MEMORY TERMINATION PURGE ROUTINE. */ 00038000
*     /* WHEN CONTROL IS RETURNED, IT SETS THE EXIT FLAG TO INDICATE */ 00039000
*     /* THE TYPE OF EXIT DESIRED AND CANCELS THE ESTAE EXIT         */ 00040000
*     /*                                                             */ 00041000
*     /***************************************************************/ 00042000
*                                                                  0048 00043000
*     DO;                           /* START OF SEGMENT RTEMTINT     */ 00044000
*       RESPECIFY                                                  0049 00045000
*        (R1) RESTRICTED;           /* RESTRICT INTERFACE REGISTERS  */ 00046000
*       R1=ADDR(RTM2WA);            /* PARAMETER REGISTER            */ 00047000
         LR    R1,R8                                               0050 00048000
*       R13=ADDR(RTM2SFSA);         /* SAVE AREA PTR                 */ 00049000
         LA    R13,RTM2SFSA(,R8)                                   0051 00050000
*       CALL IEAVTMMT;              /* INVOKE MEMORY TERMINATION   0052 00051000
*                                      PURGES                        */ 00052000
         L     @15,@CV02504                                        0052 00053000
         BALR  @14,@15                                             0052 00054000
*       RESPECIFY                                                  0053 00055000
*        (R1) UNRESTRICTED;         /* RELEASE INTERFACE REGISTERS   */ 00056000
*       RTM2MTX=ON;                 /* INDICATE EXIT DESIRED         */ 00057000
         OI    RTM2MTX(R8),B'10000000'                             0054 00058000
*     END;                          /* END OF RTEMTINT               */ 00059000
*   ELSE                            /* TASK TERMINATION              */ 00060000
*     IF RTM2FLX=NONE THEN          /* NO EXIT REQUESTED             */ 00061000
         B     @RC00047                                            0056 00062000
@RF00047 CLC   RTM2FLX(2,R8),@CB00060                              0056 00063000
         BNE   @RF00056                                            0056 00064000
*       DO;                                                        0057 00065000
*         RTM2ISPC=OFF;             /* INDICATE INITIAL SUBTASK    0058 00066000
*                                      PURGES SHOULD BE DONE AGAIN IF   00067000
*                                      ANOTHER FAILURE OCCURS        */ 00068000
         NI    RTM2ISPC(R8),B'11101111'                            0058 00069000
*         RTM2SCTR(RTM2EPS)=OFF;    /* CLEAR RECURSION FLAGS SO THAT    00070000
*                                      ANOTHER RECURSION WILL START     00071000
*                                      SUBTASK PURGES FROM BEGINNING */ 00072000
*         RTM2SCTR(RTM2ISPS)=OFF;                                  0060 00073000
*                                                                  0060 00074000
         NI    RTM2SCTR(R8),B'11111100'                            0060 00075000
*         /***********************************************************/ 00076000
*         /*                                                         */ 00077000
*         /* SEGMENT RTETTINT INVOKES TASK TERMINATION PURGES. PRIOR */ 00078000
*         /* TO DOING THIS, IF ENTRY IS FOR ABNORMAL TASK            */ 00079000
*         /* TERMINATION,IT ESTABLISHES RECURSION CONTROL. ON RETURN */ 00080000
*         /* FROM TASK PURGES IT DETERMINES THE TYPE OF EXIT         */ 00081000
*         /* NECESSARY AND CLEARS THE RECURSION FLAGS. ON ENTRY      */ 00082000
*         /* REGISTER 4 LOCATES THE TCB, REGISTER 5 LOCATES THE SVRB */ 00083000
*         /* AND REGISTER 8 LOCATES THE RTM2WA                       */ 00084000
*         /*                                                         */ 00085000
*         /***********************************************************/ 00086000
*                                                                  0061 00087000
*         DO;                       /* BEGIN SEGMENT RTETTINT        */ 00088000
*                                                                  0061 00089000
*           /*********************************************************/ 00090000
*           /*                                                       */ 00091000
*           /* DO RECURSION CONTROL                                  */ 00092000
*           /*                                                       */ 00093000
*           /*********************************************************/ 00094000
*                                                                  0062 00095000
*           IF RMPLTYPE=ON THEN     /* ENTRY IS FOR ABNORMAL TASK  0062 00096000
*                                      TERMINATION                   */ 00097000
         TM    RMPLTYPE+712(R8),B'10000000'                        0062 00098000
         BNO   @RF00062                                            0062 00099000
*             IF RTM2RECR=ON&RTM2SCTR(RTM2TSTS)=ON THEN/* A RECURSION   00100000
*                                      HAS OCCURRED IN THIS PHASE    */ 00101000
         TM    RTM2RECR(R8),B'10000000'                            0063 00102000
         BNO   @RF00063                                            0063 00103000
         TM    RTM2SCTR+1(R8),B'00000100'                          0063 00104000
         BNO   @RF00063                                            0063 00105000
*               DO;                                                0064 00106000
*                 RTM2RCRX=ON;      /* INDICATE RECURSIVE ENTRY      */ 00107000
         OI    RTM2RCRX(R8),B'10000000'                            0065 00108000
*                 RTM2SCTX(RTM2TSTS)=ON;/* INDICATE SECTION          */ 00109000
         OI    RTM2SCTX+1(R8),B'00000100'                          0066 00110000
*               END;                                               0067 00111000
*             ELSE                                                 0068 00112000
*               RTM2SCTC(RTM2TSTS)=ON;/* SET SECTION FLAG FOR THIS 0068 00113000
*                                      PHASE. NO SKIP ADDR IS SET  0068 00114000
*                                      BECAUSE IF TASK PURGES DOES 0068 00115000
*                                      NOT HAVE A VALID RECURSION  0068 00116000
*                                      ADDR THE STEP CONVERSION OR 0068 00117000
*                                      CRITICAL RECURSION RTN SHOULD    00118000
*                                      BE ENTERED                    */ 00119000
         B     @RC00063                                            0068 00120000
@RF00063 OI    RTM2SCTC+1(R8),B'00000100'                          0068 00121000
*           ELSE                                                   0069 00122000
*             ;                     /* IF NORMAL EOT,CANNOT BE     0069 00123000
*                                      RECURSION                     */ 00124000
@RF00062 DS    0H                                                  0070 00125000
*           IF RTM2FLX=NONE THEN    /* NO EXIT HAS BEEN REQUESTED    */ 00126000
@RC00062 CLC   RTM2FLX(2,R8),@CB00060                              0070 00127000
         BNE   @RF00070                                            0070 00128000
*             DO;                                                  0071 00129000
*                                                                  0071 00130000
*               /*****************************************************/ 00131000
*               /*                                                   */ 00132000
*               /* INVOKE TASK TERMINATION PURGES                    */ 00133000
*               /*                                                   */ 00134000
*               /*****************************************************/ 00135000
*                                                                  0072 00136000
*               RESPECIFY                                          0072 00137000
*                (R1) RESTRICTED;   /* RESTRICT PARM REG FOR TASK  0072 00138000
*                                      PURGE                         */ 00139000
*               RMPLTCBA=R4;        /* PUT CURRENT TCB ADDRESS IN  0073 00140000
*                                      RMPL                          */ 00141000
         ST    R4,RMPLTCBA+712(,R8)                                0073 00142000
*               R1=ADDR(RTM2WA);    /* PUT ADDR OF RTM2WA IN R1      */ 00143000
         LR    R1,R8                                               0074 00144000
*               R13=ADDR(RTM2SFSA); /* SAVE AREA ADDRESS IN R13      */ 00145000
         LA    R13,RTM2SFSA(,R8)                                   0075 00146000
*               RESPECIFY                                          0076 00147000
*                 R15 RESTRICTED;                                  0076 00148000
*               CALL IEAVTSKT;      /* REQUEST TASK PURGES           */ 00149000
         L     @15,@CV02505                                        0077 00150000
         BALR  @14,@15                                             0077 00151000
*               RESPECIFY                                          0078 00152000
*                 R1 UNRESTRICTED;                                 0078 00153000
*                                                                  0078 00154000
*               /*****************************************************/ 00155000
*               /*                                                   */ 00156000
*               /* DETERMINE TYPE OF EXIT REQUESTED                  */ 00157000
*               /*                                                   */ 00158000
*               /*****************************************************/ 00159000
*                                                                  0079 00160000
*               IF R15^=0 THEN      /* ERROR DETECTED                */ 00161000
         LTR   R15,R15                                             0079 00162000
         BZ    @RF00079                                            0079 00163000
*                 RTM2CERX=ON;      /* INDICATE ERROR IN EXIT FLAGS  */ 00164000
         OI    RTM2CERX(R8),B'01000000'                            0080 00165000
*               ELSE                /* TASK PURGES SUCCESSFUL        */ 00166000
*                 IF TCBRSTSK=ON THEN/* TASK PURGE HAS PURGED A    0081 00167000
*                                      PERMANENTLY RESIDENT TASK     */ 00168000
         B     @RC00079                                            0081 00169000
@RF00079 TM    TCBRSTSK(R4),B'00000100'                            0081 00170000
         BNO   @RF00081                                            0081 00171000
*                   RTM2PRX=ON;     /* INDICATE EXIT PROCESSING FOR     00172000
*                                      PERMANENTLY RESIDENT TASK     */ 00173000
         OI    RTM2PRX(R8),B'00000100'                             0082 00174000
*                 ELSE                                             0083 00175000
*                   IF RTM2TMEM=ON THEN/* TASK PURGE HAS PURGED THE     00176000
*                                      LAST TASK IN THE MEMORY       */ 00177000
         B     @RC00081                                            0083 00178000
@RF00081 TM    RTM2TMEM(R8),B'00100000'                            0083 00179000
         BNO   @RF00083                                            0083 00180000
*                     RTM2LTX=ON;   /* INDICATE LAST TASK EXIT       */ 00181000
         OI    RTM2LTX(R8),B'00000010'                             0084 00182000
*                   ELSE                                           0085 00183000
*                     IF RMPLTYPE=ON THEN/* TASK PURGES WERE FOR   0085 00184000
*                                      ABNORMAL TERMINATION          */ 00185000
         B     @RC00083                                            0085 00186000
@RF00083 TM    RMPLTYPE+712(R8),B'10000000'                        0085 00187000
         BNO   @RF00085                                            0085 00188000
*                       RTM2ABX=ON; /* INDICATE ABNORMAL TERMINATION    00189000
*                                      EXIT                          */ 00190000
         OI    RTM2ABX(R8),B'00100000'                             0086 00191000
*                     ELSE                                         0087 00192000
*                       RTM2EOTX=ON;/* INDICATE NORMAL EOT EXIT      */ 00193000
*                                                                  0087 00194000
         B     @RC00085                                            0087 00195000
@RF00085 OI    RTM2EOTX(R8),B'01000000'                            0087 00196000
*               /*****************************************************/ 00197000
*               /*                                                   */ 00198000
*               /* PUT ABEND CODE INTO TCB - IT MAY HAVE BEEN CHANGED*/ 00199000
*               /* BY AN ERROR WHICH HAS BEEN RECOVERED      @YM05449*/ 00200000
*               /*                                                   */ 00201000
*               /*****************************************************/ 00202000
*                                                                  0088 00203000
*               TCBCMPC=RTM2CC;     /*                       @YM05449*/ 00204000
*                                                                  0088 00205000
@RC00085 DS    0H                                                  0088 00206000
@RC00083 DS    0H                                                  0088 00207000
@RC00081 DS    0H                                                  0088 00208000
@RC00079 MVC   TCBCMPC(3,R4),RTM2CC(R8)                            0088 00209000
*               /*****************************************************/ 00210000
*               /*                                                   */ 00211000
*               /* CLEAR RECURSION CONTROL                           */ 00212000
*               /*                                                   */ 00213000
*               /*****************************************************/ 00214000
*                                                                  0089 00215000
*               RTM2SCTC(RTM2TSTS)=OFF;/* CLEAR RECURSION FLAG       */ 00216000
         NI    RTM2SCTC+1(R8),B'11111011'                          0089 00217000
*             END;                                                 0090 00218000
*           ELSE                                                   0091 00219000
*             ;                     /* IF EXIT REQUESTED BYPASS ALL     00220000
*                                      PROCESSING                    */ 00221000
@RF00070 DS    0H                                                  0092 00222000
*           RESPECIFY                                              0092 00223000
*             R15 UNRESTRICTED;                                    0092 00224000
*         END;                      /* END SEGMENT RTETTINT          */ 00225000
*         IF RTM2RCRX=OFF           /* NO RECURSION DETECTED         */ 00226000
*             &RTM2CERX=OFF         /* NO CRITICAL ERROR DETECTED    */ 00227000
*             &RTM2STPT=ON THEN     /* STEP ABEND REQUESTED          */ 00228000
*                                                                  0094 00229000
         TM    RTM2RCRX(R8),B'11000000'                            0094 00230000
         BNZ   @RF00094                                            0094 00231000
         TM    RTM2STPT(R8),B'10000000'                            0094 00232000
         BNO   @RF00094                                            0094 00233000
*           /*********************************************************/ 00234000
*           /*                                                       */ 00235000
*           /* SEGMENT RTECONV DETERMINES WHETHER CONVERSION TO STEP */ 00236000
*           /* IS POSSIBLE AND NOTIFIES THE JOBSTEP TASK IF IT IS.   */ 00237000
*           /* THE JOBSTEP TASK IS FIRST LOCATED AND SET             */ 00238000
*           /* NONDISPATCHABLE. IF THE JOBSTEP TASK IS ALREADY       */ 00239000
*           /* ABENDING OR BEING FORCED TO ABEND, IT IS NOT ELIGIBLE */ 00240000
*           /* FOR CONVERSION. IT IS RESET DISPATCHABLE, AND THE     */ 00241000
*           /* CURRENT TASK GOES TO THE EXIT HANDLER TO FINISH ITS   */ 00242000
*           /* OWN TERMINATION WITH WHATEVER EXIT FLAG WAS SET BY    */ 00243000
*           /* TASK TERMINATION. IF THE JOBSTEP TASK IS ELIGIBLE FOR */ 00244000
*           /* CONVERSION, IT IS CHECKED TO SEE WHETHER IT IS IN     */ 00245000
*           /* RTM2. IF IT IS IN RTM2, IT MUST BE PERMITTED TO       */ 00246000
*           /* COMPLETE ITS CURRENT PROCESSING. AN ECB IS SET UP, A  */ 00247000
*           /* FLAG IS SET IN THE JOBSTEP TCB, IT IS RESET           */ 00248000
*           /* DISPATCHABLE TO COMPLETE ITS CURRENT PROCESSING. WHEN */ 00249000
*           /* IT POSTS THE ECB, THE CURRENT TASK SETS AN EXIT FLAG  */ 00250000
*           /* TO FORCE THE ABTERMING OF THE JOBSTEP TASK.           */ 00251000
*           /*                                                       */ 00252000
*           /*********************************************************/ 00253000
*                                                                  0095 00254000
*           DO;                     /* START OF RTECONV              */ 00255000
*                                                                  0095 00256000
*             /*******************************************************/ 00257000
*             /*                                                     */ 00258000
*             /* OBTAIN LOCAL LOCK                                   */ 00259000
*             /*                                                     */ 00260000
*             /*******************************************************/ 00261000
*                                                                  0096 00262000
*             ESAGLREC=ON;                                         0096 00263000
         OI    ESAGLREC+96(R5),B'00010000'                         0096 00264000
*             DO;                   /* SETLOCK OBTAIN TYPE(LOCAL)MODE   00265000
*                                      (UNCOND)RELATED(TCB,IEAVTRTE(R   00266000
*                                      TECONV))                      */ 00267000
*               RESPECIFY                                          0098 00268000
*                (GPR11P,                                          0098 00269000
*                 GPR12P,                                          0098 00270000
*                 GPR13P,                                          0098 00271000
*                 GPR14P) RESTRICTED;/* RESTRICT REGS USED BY SETLOCK*/ 00272000
*               GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P,GPR13P,   00273000
*                   GPR14P);                                       0099 00274000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        C00275000
               RELATED=(TCB,IEAVTRTE(RTECONV))                          00276000
*               RESPECIFY                                          0100 00277000
*                (GPR11P,                                          0100 00278000
*                 GPR12P,                                          0100 00279000
*                 GPR13P,                                          0100 00280000
*                 GPR14P) UNRESTRICTED;/* RELEASE REGS USED BY     0100 00281000
*                                      SETLOCK                       */ 00282000
*             END;                  /* SETLOCK OBTAIN TYPE(LOCAL)MODE   00283000
*                                      (UNCOND)RELATED(TCB,IEAVTRTE(R   00284000
*                                      TECONV))                      */ 00285000
*             ESAGLREC=OFF;                                        0102 00286000
*                                                                  0102 00287000
         LA    @12,96                                              0102 00288000
         ALR   @12,R5                                              0102 00289000
         NI    ESAGLREC(@12),B'11101111'                           0102 00290000
*             /*******************************************************/ 00291000
*             /*                                                     */ 00292000
*             /* LOCATE JOBSTEP TASK                                 */ 00293000
*             /*                                                     */ 00294000
*             /*******************************************************/ 00295000
*                                                                  0103 00296000
*             RESPECIFY                                            0103 00297000
*               R9 RESTRICTED;      /* RESTRICT PTR TO J/S TCB       */ 00298000
*             R9=TCBJSTCA;                                         0104 00299000
         L     R9,TCBJSTCA-1(,R4)                                  0104 00300000
         LA    R9,0(,R9)                                           0104 00301000
*             RESPECIFY                                            0105 00302000
*               TCB BASED(R9);                                     0105 00303000
*                                                                  0105 00304000
*             /*******************************************************/ 00305000
*             /*                                                     */ 00306000
*             /* SET JOB STEP TASK NONDISPATCHABLE                   */ 00307000
*             /*                                                     */ 00308000
*             /*******************************************************/ 00309000
*                                                                  0106 00310000
*             RESPECIFY                                            0106 00311000
*              (R0,                                                0106 00312000
*               R1) RESTRICTED;     /* RESTRICT STATUS INTERFACE REGS*/ 00313000
*             R0=STOPTASK;          /* CODE TO STOP ONE TASK WITH  0107 00314000
*                                      ABWF                          */ 00315000
         L     R0,@CF00076                                         0107 00316000
*             R1=R9;                /* TASK TO BE STOPPED            */ 00317000
         LR    R1,R9                                               0108 00318000
*             R13=0;                /* INDICATE CURRENT MEMORY       */ 00319000
         SLR   R13,R13                                             0109 00320000
*             CALL STATUS;                                         0110 00321000
         L     @12,CVTPTR(,@12)                                    0110 00322000
         L     @12,SCVTSPTR(,@12)                                  0110 00323000
         L     @15,SCVTSTAT(,@12)                                  0110 00324000
         BALR  @14,@15                                             0110 00325000
*             RESPECIFY                                            0111 00326000
*              (R0,                                                0111 00327000
*               R1) UNRESTRICTED;   /* RELEASE STATUS INTERFACE REGS */ 00328000
*                                                                  0111 00329000
*             /*******************************************************/ 00330000
*             /*                                                     */ 00331000
*             /* IF CONVERSION IS NOT POSSIBLE, RELEASE LOCAL LOCK   */ 00332000
*             /* AND RESET JOBSTEP TASK DISPATCHABLE                 */ 00333000
*             /*                                                     */ 00334000
*             /*******************************************************/ 00335000
*                                                                  0112 00336000
*             IF TCBFA=ON           /* TASK ALREADY ABENDING         */ 00337000
*                 |TCBFMW=ON        /* ANCESTOR FORCING TASK TO ABEND*/ 00338000
*                 |TCBFDW=ON THEN   /* ANOTHER SUBTASK FORCING TASK     00339000
*                                      TO ABEND                      */ 00340000
         TM    TCBFA(R9),B'10000000'                               0112 00341000
         BO    @RT00112                                            0112 00342000
         TM    TCBFMW(R9),B'01100000'                              0112 00343000
         BZ    @RF00112                                            0112 00344000
@RT00112 DS    0H                                                  0113 00345000
*               DO;                                                0113 00346000
*                 ESAGLREC=ON;      /* INDICATE IN SETLOCK PROCESSING*/ 00347000
         OI    ESAGLREC+96(R5),B'00010000'                         0114 00348000
*                 DO;               /* SETLOCK RELEASE TYPE(LOCAL)REL   00349000
*                                      ATED(TCB,IEAVTRTE(RTECONV))   */ 00350000
*                   RESPECIFY                                      0116 00351000
*                    (GPR11P,                                      0116 00352000
*                     GPR12P,                                      0116 00353000
*                     GPR13P,                                      0116 00354000
*                     GPR14P) RESTRICTED;/* RESTRICT REGS USED BY  0116 00355000
*                                      SETLOCK                       */ 00356000
*                   GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P, 0117 00357000
*                       GPR13P,GPR14P);                            0117 00358000
         SETLOCK RELEASE,TYPE=LOCAL,                                   C00359000
               RELATED=(TCB,IEAVTRTE(RTECONV))                          00360000
*                   RESPECIFY                                      0118 00361000
*                    (GPR11P,                                      0118 00362000
*                     GPR12P,                                      0118 00363000
*                     GPR13P,                                      0118 00364000
*                     GPR14P) UNRESTRICTED;/* RELEASE REGS USED BY 0118 00365000
*                                      SETLOCK                       */ 00366000
*                 END;              /* SETLOCK RELEASE TYPE(LOCAL)REL   00367000
*                                      ATED(TCB,IEAVTRTE(RTECONV))   */ 00368000
*                 ESAGLREC=OFF;     /* SETLOCK COMPLETED SUCCESSFULLY*/ 00369000
         NI    ESAGLREC+96(R5),B'11101111'                         0120 00370000
*                 GEN(STATUS RESET,ND,(R9),(10),E);                0121 00371000
         STATUS RESET,ND,(R9),(10),E                                    00372000
*               END;                                               0122 00373000
*             ELSE                                                 0123 00374000
*                                                                  0123 00375000
*               /*****************************************************/ 00376000
*               /*                                                   */ 00377000
*               /* CONVERSION POSSIBLE                               */ 00378000
*               /*                                                   */ 00379000
*               /*****************************************************/ 00380000
*                                                                  0123 00381000
*               DO;                                                0123 00382000
         B     @RC00112                                            0123 00383000
@RF00112 DS    0H                                                  0124 00384000
*                 TCBFDW=ON;        /* SIGNAL JOBSTEP THAT DAUGHTER     00385000
*                                      IS WAITING                    */ 00386000
*                                                                  0124 00387000
         OI    TCBFDW(R9),B'00100000'                              0124 00388000
*                 /***************************************************/ 00389000
*                 /*                                                 */ 00390000
*                 /* IF NECESSARY, WAIT FOR JOBSTEP TO FINISH CERTAIN*/ 00391000
*                 /* RTM2 PROCESSING                                 */ 00392000
*                 /*                                                 */ 00393000
*                 /***************************************************/ 00394000
*                                                                  0125 00395000
*                 IF TCBRTM2=ON THEN/* JOBSTEP TASK MAY BE IN      0125 00396000
*                                      PROCESSING WHICH SHOULD NOT BE   00397000
*                                      INTERRUPTED                   */ 00398000
         TM    TCBRTM2(R9),B'00001000'                             0125 00399000
         BNO   @RF00125                                            0125 00400000
*                   DO;                                            0126 00401000
*                     RTM2ECBS(1)=0;/* CLEAR ECB TO BE USED          */ 00402000
         SLR   @12,@12                                             0127 00403000
         ST    @12,RTM2ECBS(,R8)                                   0127 00404000
*                     TCBAECB=ADDR(RTM2ECBS);/* PROVIDE ADDRESS OF 0128 00405000
*                                      ECB TO JOBSTEP TASK           */ 00406000
         LA    @12,RTM2ECBS(,R8)                                   0128 00407000
         ST    @12,TCBAECB(,R9)                                    0128 00408000
*                     ESAGLREC=ON;  /* INDICATE SETLOCK IN CONTROL   */ 00409000
         OI    ESAGLREC+96(R5),B'00010000'                         0129 00410000
*                     DO;           /* SETLOCK RELEASE TYPE(LOCAL)REL   00411000
*                                      ATED(TCB,IEAVTRTE(RTECONV))   */ 00412000
*                       RESPECIFY                                  0131 00413000
*                        (GPR11P,                                  0131 00414000
*                         GPR12P,                                  0131 00415000
*                         GPR13P,                                  0131 00416000
*                         GPR14P) RESTRICTED;/* RESTRICT REGS USED BY   00417000
*                                                             SETLOCK*/ 00418000
*                       GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P,  00419000
*                           GPR13P,GPR14P);                        0132 00420000
         SETLOCK RELEASE,TYPE=LOCAL,                                   C00421000
               RELATED=(TCB,IEAVTRTE(RTECONV))                          00422000
*                       RESPECIFY                                  0133 00423000
*                        (GPR11P,                                  0133 00424000
*                         GPR12P,                                  0133 00425000
*                         GPR13P,                                  0133 00426000
