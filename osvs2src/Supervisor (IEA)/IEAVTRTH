         STH   @11,RBLOCKNO(,R10)                                  0163 00346000
*         IF LRBMVFA='1'B THEN      /* DETERMINE STORAGE BOUNDS ONLY    00347000
*                                      IF THE FAILING STORAGE ADDRESS   00348000
*                                      IS VALID                      */ 00349000
         TM    LRBMVFA(R6),B'10000000'                             0164 00350000
         BNO   @RF00164                                            0164 00351000
*           DO;                                                    0165 00352000
*             IF R5>EEDNULL THEN    /* IF REPAIR STATUS EED EXISTS   */ 00353000
         C     R5,@CF00076                                         0166 00354000
         BNH   @RF00166                                            0166 00355000
*               DO;                                                0167 00356000
*                 RFY                                              0168 00357000
*                   EED BASED(R3);  /* USE REPAIR STATUS EED FOR   0168 00358000
*                                      MAPPING MACRO BASE            */ 00359000
*                 EEDHRFSA=LRBMFSA; /* SAVE THE REAL FAILING STORAGE    00360000
*                                      ADDRESS IN THE EED            */ 00361000
         ST    @12,EEDHRFSA(,R3)                                   0169 00362000
*                 RFY                                              0170 00363000
*                  (R1,                                            0170 00364000
*                   R15) RSTD;      /* GET REGS FOR CONVERSION     0170 00365000
*                                      ROUTINE PARMS                 */ 00366000
*                 R1=LRBMFSA;       /* SET REAL ADDR FOR WHICH A   0171 00367000
*                                      VIRTUAL ADDR IS DESIRED       */ 00368000
