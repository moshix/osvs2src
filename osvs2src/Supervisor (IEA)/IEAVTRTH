         BNO   @RF00153                                            0153 00282000
@RT00153 DS    0H                                                  0154 00283000
*     /***************************************************************/ 00284000
*     /*                                                             */ 00285000
*     /* THE PURPOSE OF THIS SEGMENT IS TO SET UP THE INTERFACE AND  */ 00286000
*     /* CALL THE REAL STORAGE RECONFIGURATION ROUTINE FOR STORAGE   */ 00287000
*     /* ERRORS. THIS SEGMENT ALSO SAVES THE REPAIR STATUS AFTER THE */ 00288000
*     /* CALL TO RSR IN THE LOGREC BUFFER AND ALSO IN THE REPAIR     */ 00289000
*     /* STATUS EED, IF ONE EXISTS. INPUT TO THE SEGMENT INCLUDES THE*/ 00290000
*     /* ERROR INFORMATION IN THE LRB. OUTPUT INCLUDES AN UPDATED LRB*/ 00291000
*     /* AND ALSO AN UPDATED REPAIR STATUS EED, IF ONE EXISTS.       */ 00292000
*     /*                                                             */ 00293000
*     /***************************************************************/ 00294000
*                                                                  0154 00295000
*     DO;                           /* ATTEMPT STORAGE REPAIR        */ 00296000
*       RTHTRTRY='0'B;              /* RESET RETRY BIT FOR NEXT PHASE*/ 00297000
         NI    RTHTRTRY(R4),B'11101111'                            0155 00298000
*PARMSRP:                           /* THIS LABEL IS A RETURN POINT     00299000
*                                      FROM RTH'S RECOVERY ROUTINE 0156 00300000
*                                      FOR RETRY OF BUILDING PARM  0156 00301000
*                                      LIST                          */ 00302000
*       RTHTLPN=RTHPARMS;           /* INDICATE PHASE FOR          0156 00303000
*                                      CONSTRUCTION OF STORAGE REPAIR   00304000
*                                      PARM LIST                     */ 00305000
PARMSRP  MVI   RTHTLPN(R4),X'23'                                   0156 00306000
*       IF LRBMHSPF='1'B THEN       /* IF STORAGE KEY ERROR          */ 00307000
         TM    LRBMHSPF(R6),B'00000010'                            0157 00308000
         BNO   @RF00157                                            0157 00309000
*         RTMKSKYF='1'B;            /* THEN SET STORAGE KEY ERROR  0158 00310000
*                                      FLAG IN STATUS BYTE           */ 00311000
         OI    RTMKSKYF(R10),B'10000000'                           0158 00312000
*       IF LRBMHSTO='1'B THEN       /* IF STORAGE DATA ERROR         */ 00313000
@RF00157 TM    LRBMHSTO(R6),B'00000100'                            0159 00314000
         BNO   @RF00159                                            0159 00315000
*         RTMKSCK='1'B;             /* THEN SET STORAGE DATA ERROR 0160 00316000
*                                      FLAG IN STATUS BYTE           */ 00317000
*                                                                  0160 00318000
         OI    RTMKSCK(R10),B'00010000'                            0160 00319000
*       /*************************************************************/ 00320000
*       /*                                                           */ 00321000
*       /* THE PURPOSE OF THIS SEGMENT IS TO CONSTRUCT THE PARAMETER */ 00322000
*       /* LIST FOR THE CALL TO THE REAL STORAGE RECONFIGURATION     */ 00323000
*       /* ROUTINE. DURING ITS PROCESSING, IT WILL ALSO STORE        */ 00324000
*       /* ADDITIONAL INFORMATION IN THE REPAIR STATUS EED, WHEN ONE */ 00325000
*       /* IS AVAILABLE. INPUT TO THE SEGMENT INCLUDES THE ERROR     */ 00326000
*       /* INFORMATION IN THE LRB. OUTPUT INCLUDES THE PARAMETER LIST*/ 00327000
*       /* FOR THE CALL TO RSR, AND AN UPDATED REPAIR STATUS EED IF  */ 00328000
*       /* ONE EXISTS.                                               */ 00329000
*       /*                                                           */ 00330000
*       /*************************************************************/ 00331000
*                                                                  0161 00332000
*       DO;                         /* BUILD STORAGE REPAIR PARM LIST*/ 00333000
@RF00159 DS    0H                                                  0162 00334000
*         RSRPARMS=''B;             /* CLEAR AREA IN CPU RELATED SAVE   00335000
*                                      AREA USED FOR PARM LIST       */ 00336000
         XC    RSRPARMS(12,R10),RSRPARMS(R10)                      0162 00337000
*         RBLOCKNO=(LRBMFSA/FOURK)*SIXTEEN;/* CALCULATE THE REAL   0163 00338000
*                                      BLOCK NUMBER OF THE ERROR AND    00339000
*                                      SAVE IT IN THE PARM LIST    0163 00340000
*                                                            @YM01853*/ 00341000
         L     @12,LRBMFSA(,R6)                                    0163 00342000
         LR    @11,@12                                             0163 00343000
         SRL   @11,12                                              0163 00344000
