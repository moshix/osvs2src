         TITLE 'IEAVTRTH                                               *00001000
                        '                                               00002000
IEAVTRTH CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVTRTH  77.125'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @08,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@08                                         0001 00012000
*                                      IS GOTTEN FROM THE CPU WSAVT  */ 00013000
*                                                                  0091 00014000
*   /*****************************************************************/ 00015000
*   /*                                                               */ 00016000
*   /* THE PURPOSE OF THIS SEGMENT IS TO INITIALIZE THE ENVIRONMENT. */ 00017000
*   /* THIS IS DONE MAINLY BY ESTABLISHING AN FRR AND PRESERVING     */ 00018000
*   /* INFORMATION NECESSARY FOR RECOVERY. THE RECOVERY INFORMATION  */ 00019000
*   /* STORED IN THE FRR IS AS FOLLOWS: THE LOGICAL PHASE NUMBER - TO*/ 00020000
*   /* INDICATE THE RESTART POINT IF AN ERROR WERE TO OCCUR IN       */ 00021000
*   /* PROCESSING, A RETRY FLAG - TO INDICATE WHETHER THE PROCESSING */ 00022000
*   /* IS CURRENTLY IN THE RETRY PASS, THE ADDRESS OF THE FIRST EED  */ 00023000
*   /* ACQUIRED OR 0, THE ADDRESS OF THE LOGREC BUFFER, AND THE      */ 00024000
*   /* ADDRESS OF THE CPU RELATED SAVE AREA. INPUT TO THE SEGMENT    */ 00025000
*   /* INCLUDES: THE TRAKAREA REGISTER (R4) - THIS POINTS TO THE     */ 00026000
*   /* CURRENT TRACKING AREA, AND THE ADDRLRB REGISTER (R6) - THIS   */ 00027000
*   /* POINTS TO THE LOGREC BUFFER. OUTPUT INCLUDES: THE TRAKAREA    */ 00028000
*   /* REGISTER (R4) - THIS POINTS TO THE CURRENT TRACKING AREA, AND */ 00029000
*   /* THE CPURTMSA REGISTER (R10) - THIS POINTS TO THE CPU RELATED  */ 00030000
*   /* SAVE AREA.                                                    */ 00031000
*   /*                                                               */ 00032000
*   /*****************************************************************/ 00033000
*                                                                  0092 00034000
*RTHSTART:                          /* LABEL ON FIRST EXECUTABLE   0092 00035000
*                                      INSTRUCTION OF RTH TO ALLOW 0092 00036000
*                                      RTH'S FRR TO RESTORE THE VALUE   00037000
*                                      OF RTH'S BASE REG             */ 00038000
*   DO;                                                            0092 00039000
RTHSTART DS    0H                                                  0093 00040000
*     R7=R14;                       /* PRESERVE ORIGINAL RETURN POINT   00041000
*                                      VALUE                         */ 00042000
         LR    R7,R14                                              0093 00043000
*     RFY                                                          0094 00044000
*       R14 UNRSTD;                 /* RELEASE RETURN REG            */ 00045000
*     RTHTREGS(TWO)=R6;             /* PLACE ADDRESS OF LOGREC BUFFER   00046000
*                                      IN TRACKING AREA              */ 00047000
         ST    R6,RTHTREGS+4(,R4)                                  0095 00048000
*     R10=WSACRTMK;                 /* OBTAIN ADDRESS OF RTMS CPU  0096 00049000
*                                      RELATED SAVE AREA             */ 00050000
         L     @12,PSALCCAV                                        0096 00051000
         L     @12,LCCACPUS(,@12)                                  0096 00052000
         L     R10,WSACRTMK(,@12)                                  0096 00053000
*     RTMCPUSA=''B;                 /* CLEAR THIS AREA               */ 00054000
         XC    RTMCPUSA(88,R10),RTMCPUSA(R10)                      0097 00055000
*     RTHTREGS(THREE)=R10;          /* PLACE ADDR OF RTMS CPU RELATED   00056000
*                                      SAVE AREA IN TRACKING AREA    */ 00057000
*                                                                  0098 00058000
         ST    R10,RTHTREGS+8(,R4)                                 0098 00059000
*     /***************************************************************/ 00060000
*     /*                                                             */ 00061000
*     /* INITIAL RTMTRACK                                            */ 00062000
*     /*                                                             */ 00063000
*     /***************************************************************/ 00064000
*                                                                  0099 00065000
*   END;                                                           0099 00066000
*EEDRP:                             /* THIS LABEL IS A RETURN POINT     00067000
*                                      FROM RTH'S RECOVERY ROUTINE 0100 00068000
*                                      FOR RETRY OF EED PROCESSING   */ 00069000
*   RTHTLPN=RTHEEDS;                /* INDICATE EED PROCESSING PHASE */ 00070000
EEDRP    MVI   RTHTLPN(R4),X'20'                                   0100 00071000
*   RFY                                                            0101 00072000
*     R5 RSTD;                      /* GET REG FOR EED POINTER       */ 00073000
*   R5=NULL;                        /* INITIALIZE POINTER TO CHAIN OF   00074000
*                                      EEDS TO INDICATE NO EEDS ARE     00075000
*                                      PRESENT                       */ 00076000
         SLR   R5,R5                                               0102 00077000
*   IF((LRBMHSTO='1'B&LRBMVST='1'B) /* STORAGE ERROR VALID   @G554P2M*/ 00078000
*       |LRBMHSPF='1'B)             /* OR STORAGE KEY ERROR  @G554P2M*/ 00079000
*       &(LRBMCIC(1:1)=EXTDMG       /* AND EXTERNAL DAMAGE   @G554P2M*/ 00080000
*       |LRBMCIC(1:1)=SYSRECOV) THEN                               0103 00081000
         TM    LRBMHSTO(R6),B'00000100'                            0103 00082000
         BNO   @GL00002                                            0103 00083000
         TM    LRBMVST(R6),B'00000001'                             0103 00084000
         BO    @GL00001                                            0103 00085000
@GL00002 TM    LRBMHSPF(R6),B'00000010'                            0103 00086000
         BNO   @RF00103                                            0103 00087000
@GL00001 CLI   LRBMCIC(R6),X'04'                                   0103 00088000
         BE    @RT00103                                            0103 00089000
         CLI   LRBMCIC(R6),X'20'                                   0103 00090000
         BE    @RT00103                                            0103 00091000
*     ;                             /* OR SYSTEM RECOVERY    @G554P2M*/ 00092000
*                                                                  0104 00093000
*   /*****************************************************************/ 00094000
*   /*                                                               */ 00095000
*   /* TYPE MACHINE CHECK                                    @G554P2M*/ 00096000
*   /*                                                               */ 00097000
*   /*****************************************************************/ 00098000
*                                                                  0105 00099000
*   ELSE                                                           0105 00100000
*     DO;                           /*                       @G554P2M*/ 00101000
@RF00103 DS    0H                                                  0106 00102000
*       IF LRBMTERM^=NULL|          /* IF THE ERROR IS AN ACR ERROR  */ 00103000
*           LRBMHARD^=NULL THEN                                    0106 00104000
*                                                                  0106 00105000
         CLI   LRBMTERM(R6),0                                      0106 00106000
         BNE   @RT00106                                            0106 00107000
         CLI   LRBMHARD(R6),0                                      0106 00108000
         BE    @RF00106                                            0106 00109000
@RT00106 DS    0H                                                  0107 00110000
*         /***********************************************************/ 00111000
*         /*                                                         */ 00112000
*         /* THE PURPOSE OF THIS SEGMENT IS TO OBTAIN 2 CELLS FOR    */ 00113000
*         /* EEDS WHEN SOFTWARE PROCESSING WILL BE REQUIRED FOR      */ 00114000
*         /* HARDWARE ERRORS. ONE EED WILL PRESERVE THE ENVIRONMENT  */ 00115000
*         /* AT THE TIME OF THE HARDWARE FAILURE, AND ONE EED WILL BE*/ 00116000
*         /* USED TO INDICATE THE REPAIR STATUS. IF ONLY ONE EED IS  */ 00117000
*         /* OBTAINED, IT WILL BE CONSTRUCTED AS A REPAIR STATUS EED */ 00118000
*         /* AND LATER RTM PROCESSING WILL CONTINUE AS IF NO         */ 00119000
*         /* ENVIRONMENT WERE AVAILABLE. INPUT TO THIS SEGMENT       */ 00120000
*         /* INCLUDES THE EEDLIST REGISTER (R5) WHICH INITIALLY HAS A*/ 00121000
*         /* VALUE OF ZERO. OUTPUT FROM THE SEGMENT INCLUDES THE     */ 00122000
*         /* EEDLIST REGISTER (R5) WHICH POINTS TO THE 1ST EED ON THE*/ 00123000
*         /* CHAIN, IF ANY WERE ACQUIRED, OR CONTAINS THE VALUE OF   */ 00124000
*         /* EEDNULL (1) INDICATING THAT THE ATTEMPT TO GET A CELL   */ 00125000
*         /* FAILED. OUTPUT ALSO INCLUDES THE EEDRSTAT REGISTER (R3) */ 00126000
*         /* WHICH POINTS TO THE REPAIR STATUS EED, IF ONE EXISTS.   */ 00127000
*         /*                                                         */ 00128000
*         /***********************************************************/ 00129000
*                                                                  0107 00130000
*         DO;                       /* ATTEMPT TO ACQUIRE EEDS       */ 00131000
*           RFY                                                    0108 00132000
*            (R0,                                                  0108 00133000
*             R1,                                                  0108 00134000
*             R15) RSTD;            /* GET REGS FOR GETCELL          */ 00135000
*           RFY                                                    0109 00136000
*             R13 RSTD;             /* GET REG FOR SAVE AREA POINTER */ 00137000
*           R0=RT1CPOOL;            /* SET CELLPOOL ID FOR GETCELL   */ 00138000
         L     R0,RT1CPOOL                                         0110 00139000
*           R13=ADDR(RTMKSV);       /* POINT TO STANDARD SAVE AREA   */ 00140000
         LR    R13,R10                                             0111 00141000
*           GEN(GETCELL CPID=(0),BRANCH=YES);/* ATTEMPT TO GET A CELL*/ 00142000
         GETCELL CPID=(0),BRANCH=YES                                    00143000
*           IF R15=QCELLNRM THEN    /* IF ONE CELL WAS OBTAINED      */ 00144000
         LTR   R15,R15                                             0113 00145000
         BNZ   @RF00113                                            0113 00146000
*             DO;                                                  0114 00147000
*               R5=R1;              /* MAKE THIS CELL THE FIRST ONE     00148000
*                                      ON THE CHAIN                  */ 00149000
         LR    R5,R1                                               0115 00150000
*               RFY                                                0116 00151000
*                 EED BASED(R5);    /* BASE THE EED MAPPING ON THE 0116 00152000
*                                      1ST CELL OF THE CHAIN         */ 00153000
*               EED=''B;            /* CLEAR THE EED                 */ 00154000
         XC    EED(92,R5),EED(R5)                                  0117 00155000
*               EEDID=HWREPTYP;     /* INDICATE THIS EED IS A REPAIR    00156000
*                                      STATUS EED                    */ 00157000
         MVI   EEDID(R5),X'03'                                     0118 00158000
*               RFY                                                0119 00159000
*                 R3 RSTD;          /* GET REG TO POINT TO THE REPAIR   00160000
*                                      STATUS EED                    */ 00161000
*               R3=R5;              /* INIT THIS REG TO THE ADDRESS     00162000
*                                      OF THE REPAIR STATUS EED      */ 00163000
         LR    R3,R5                                               0120 00164000
*               RTHTREGS(FIVE)=R3;  /* CHECKPOINT ADDR OF REPAIR   0121 00165000
*                                      STATUS EED INTO TRACKING AREA */ 00166000
         ST    R3,RTHTREGS+16(,R4)                                 0121 00167000
*               RTHTEED1='1'B;      /* INDICATE THAT THE INITIAL EED    00168000
*                                      HAS BEEN ACQUIRED             */ 00169000
         OI    RTHTEED1(R4),B'00001000'                            0122 00170000
*               GEN(GETCELL CPID=(0),BRANCH=YES);/* ATTEMPT TO GET A    00171000
*                                      2ND QUICKCELL                 */ 00172000
         GETCELL CPID=(0),BRANCH=YES                                    00173000
*               IF R15=QCELLNRM THEN/* IF THIS ATTEMPT IS ALSO     0124 00174000
*                                      SUCCESSFUL                    */ 00175000
         LTR   R15,R15                                             0124 00176000
         BNZ   @RF00124                                            0124 00177000
*                 DO;                                              0125 00178000
*                   R5=R1;          /* MAKE THIS CELL THE 1ST ONE ON    00179000
*                                      THE CHAIN                     */ 00180000
         LR    R5,R1                                               0126 00181000
*                   RFY                                            0127 00182000
*                    (R0,                                          0127 00183000
*                     R1,                                          0127 00184000
*                     R15) UNRSTD;  /* RELEASE REGS USED IN GETCELL  */ 00185000
*                   RFY                                            0128 00186000
*                     R13 UNRSTD;   /* RELEASE REG USED AS SAVE AREA    00187000
*                                      POINTER                       */ 00188000
*                   EED=''B;        /* CLEAR THIS EED                */ 00189000
         XC    EED(92,R5),EED(R5)                                  0129 00190000
*                   EEDID=REGSPTYP; /* INDICATE THIS EED IS AN     0130 00191000
*                                      ENVIRONMENT EED               */ 00192000
         MVI   EEDID(R5),X'01'                                     0130 00193000
*                   EEDFWRDP=R3;    /* CHAIN THE EEDS TOGETHER       */ 00194000
         ST    R3,EEDFWRDP(,R5)                                    0131 00195000
*                 END;              /* END OF PROCESSING 2ND EED     */ 00196000
*               ELSE                /* IF 2ND QUICKCELL WAS NOT    0133 00197000
*                                      OBTAINED                      */ 00198000
*                 EEDFWRDP=EEDNULL; /* INDICATE EED INFORMATION WILL    00199000
*                                      BE LOST                       */ 00200000
         B     @RC00124                                            0133 00201000
@RF00124 MVC   EEDFWRDP(4,R5),@CF00076                             0133 00202000
*             END;                  /* END OF PROCESSING NEW EEDS    */ 00203000
*           ELSE                    /* IF NO QUICKCELLS WERE OBTAINED*/ 00204000
*             R5=EEDNULL;           /* INDICATE EED INFORMATION WILL    00205000
*                                      BE LOST                       */ 00206000
         B     @RC00113                                            0135 00207000
@RF00113 LA    R5,1                                                0135 00208000
*           RTHTREGS(FOUR)=R5;      /* PLACE ADDRESS OF 1ST EED IN 0136 00209000
*                                      TRACKING AREA                 */ 00210000
@RC00113 ST    R5,RTHTREGS+12(,R4)                                 0136 00211000
*           RTHTEED2='1'B;          /* INDICATE THAT THE SECOND EED     00212000
*                                      HAS BEEN ACQUIRED             */ 00213000
         OI    RTHTEED2(R4),B'00000100'                            0137 00214000
*         END;                      /* END OF EED PROCESSING         */ 00215000
*     END;                          /*                       @G554P2M*/ 00216000
@RF00106 DS    0H                                                  0140 00217000
*   RTHTRTRY='0'B;                  /* RESET RETRY BIT FOR NEXT PHASE*/ 00218000
@RT00103 NI    RTHTRTRY(R4),B'11101111'                            0140 00219000
*TIMERP:                            /* THIS LABEL IS A RETURN POINT     00220000
*                                      FROM RTH'S RECOVERY ROUTINE 0141 00221000
*                                      FOR RETRY OF CLOCK REPAIR   0141 00222000
*                                      PHASE                         */ 00223000
*   RTHTLPN=RTHTIMER;               /* INDICATE CLOCK REPAIR PHASE   */ 00224000
TIMERP   MVI   RTHTLPN(R4),X'21'                                   0141 00225000
*   IF LRBMTERM^=NULL|              /* IF THE ERROR IS AN ACR ERROR  */ 00226000
*       LRBMINTM^=NULL THEN         /* OR A TIMER ERROR              */ 00227000
*                                                                  0142 00228000
         CLI   LRBMTERM(R6),0                                      0142 00229000
         BNE   @RT00142                                            0142 00230000
         CLI   LRBMINTM(R6),0                                      0142 00231000
         BE    @RF00142                                            0142 00232000
@RT00142 DS    0H                                                  0143 00233000
*     /***************************************************************/ 00234000
*     /*                                                             */ 00235000
*     /* THE PURPASE OF THIS SEGMENT IS TO SET UP THE CORRECT        */ 00236000
*     /* INTERFACE AND CALL THE CLOCK REPAIR ROUTINE WHEN CERTAIN    */ 00237000
*     /* TYPES OF HARDWARE ERRORS HAVE OCCURRED.                     */ 00238000
*     /*                                                             */ 00239000
*     /***************************************************************/ 00240000
*                                                                  0143 00241000
*     DO;                           /* ATTEMPT CLOCK REPAIR          */ 00242000
*       RFY                                                        0144 00243000
*        (R0,                                                      0144 00244000
*         R1,                                                      0144 00245000
*         R13) RSTD;                /* GET REGS FOR CLOCK REPAIR   0144 00246000
*                                      PARAMETERS                    */ 00247000
*       R0=PSACPUPA;                /* SET PHYSICAL CPU ADDRESS FOR     00248000
*                                      CLOCK REPAIR INTERFACE        */ 00249000
         LH    R0,PSACPUPA                                         0145 00250000
*       R1=R6;                      /* SET ADDRESS OF LOGREC BUFFER  */ 00251000
         LR    R1,R6                                               0146 00252000
*       R13=ADDR(RTMKSV);           /* POINT TO STANDARD SAVE AREA   */ 00253000
         LR    R13,R10                                             0147 00254000
*       CALL IEAVRCLS;              /* CALL CLOCK REPAIR ROUTINE     */ 00255000
         L     @15,@CV00095                                        0148 00256000
         BALR  @14,@15                                             0148 00257000
*       RFY                                                        0149 00258000
*        (R0,                                                      0149 00259000
*         R1,                                                      0149 00260000
*         R13) UNRSTD;              /* RELEASE REGS USED IN CLOCK  0149 00261000
*                                      REPAIR ROUTINE                */ 00262000
*     END;                          /* END OF CLOCK REPAIR PROCESSING*/ 00263000
*   RTHTRTRY='0'B;                  /* RESET RETRY BIT FOR NEXT PHASE*/ 00264000
@RF00142 NI    RTHTRTRY(R4),B'11101111'                            0151 00265000
*STRG1RP:                           /* THIS LABEL IS A RETURN POINT     00266000
*                                      FROM RTH'S RECOVERY ROUTINE 0152 00267000
*                                      FOR RETRY OF STORAGE REPAIR 0152 00268000
*                                      PHASE                         */ 00269000
*   RTHTLPN=RTHSTRG1;               /* INDICATE STORAGE REPAIR PHASE */ 00270000
STRG1RP  MVI   RTHTLPN(R4),X'22'                                   0152 00271000
*   IF(LRBMHSTO='1'B&LRBMCIC(1:1)^=SYSRECOV)/* IF ERROR      @G554P2M   00272000
*                                      IS STORAGE ERROR NOT  @G554P2M   00273000
*                                      RECOVERED BY HARD-    @G554P2M*/ 00274000
*       |LRBMHSPF='1'B THEN         /* WARE OR A STORAGE KEY ERROR   */ 00275000
