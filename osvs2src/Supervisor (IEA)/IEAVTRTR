         TITLE 'IEAVTRTR,RTM RECOVERY ROUTINES                         *00001000
                        '                                               00002000
IEAVTRTR CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'IEAVTRTR  76.159'                                 0001 00009000
RCOVGETM DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00099-RCOVGETM)                                   00013000
         ENTRY RCOVGETM                                                 00014000
RCOVRCRD DS    0H                                                  0001 00015000
         ENTRY RCOVRCRD                                                 00016000
RCOVRGTF DS    0H                                                  0001 00017000
         USING *,@15                                               0001 00018000
         B     @PROLOG                                             0001 00019000
         DC    AL2(@EP00178-RCOVRGTF)                                   00020000
         ENTRY RCOVRGTF                                                 00021000
FREEDCEL DS    0H                                                  0001 00022000
         USING *,@15                                               0001 00023000
         B     @PROLOG                                             0001 00024000
         DC    AL2(@EP00465-FREEDCEL)                                   00025000
         ENTRY FREEDCEL                                                 00026000
RTMRSFRR DS    0H                                                  0001 00027000
         USING *,@15                                               0001 00028000
         B     @PROLOG                                             0001 00029000
         DC    AL2(@EP00491-RTMRSFRR)                                   00030000
         ENTRY RTMRSFRR                                                 00031000
RECVRRTM DS    0H                                                  0001 00032000
         USING *,@15                                               0001 00033000
         B     @PROLOG                                             0001 00034000
         DC    AL2(@EP00595-RECVRRTM)                                   00035000
         ENTRY RECVRRTM                                                 00036000
RTMSMFRR DS    0H                                                  0001 00037000
         USING *,@15                                               0001 00038000
         B     @PROLOG                                             0001 00039000
         DC    AL2(@EP00755-RTMSMFRR)                                   00040000
         ENTRY RTMSMFRR                                                 00041000
RTHFRR   DS    0H                                                  0001 00042000
         USING *,@15                                               0001 00043000
         B     @PROLOG                                             0001 00044000
         DC    AL2(@EP00769-RTHFRR)                                     00045000
         ENTRY RTHFRR                                                   00046000
         DROP  @15                                                      00047000
@PROLOG  BALR  @09,0                                               0001 00048000
@PSTART  LA    @10,4095(,@09)                                      0001 00049000
         USING @PSTART,@09                                         0001 00050000
         USING @PSTART+4095,@10                                    0001 00051000
         AH    @15,4(,@15)                                         0001 00052000
         BR    @15                                                 0001 00053000
@EP00001 DS    0H                                                  0002 00054000
*/* ******************************************************************/ 00055000
*/* THIS SEGMENT IS A FRR WHICH IS DESIGNED TO RECOVER IEAVTRTS FROM */ 00056000
*/* A POSSIBLE GETMAIN FAILURE WHEN ATTEMPTING TO ACQUIRE A SDWA FOR */ 00057000
*/* AN ERROR IN UNLOCKED SRB MODE.  THIS FRR PERFORMS THE FOLLOWING: */ 00058000
*/* 1.REQUESTS RETRY AT THE POINT IN IEAVTRTS WHERE A LOCAL SDWA IS  */ 00059000
*/*   USED FOR THE UNLOCKED SRB ERROR PROVIDED THAT THE ERROR TYPE   */ 00060000
*/*   AS INDICATED IN THE SDWA IS NOT RESTART OR DATERR.  TWO RETRY  */ 00061000
*/*   CONDITIONS ARE POSSIBLE:                                       */ 00062000
*/*   A.IF THE ERROR TYPE IS THE SAME AS THE ERROR TYPE OF THE       */ 00063000
*/*     UNLOCKED SRB AND IEAVTRTS HAS AS YET NOT INITIALIZED AN SDWA */ 00064000
*/*     (STILL IN SEGMENT GOTOSRB) THEN THE ORIGINAL ERROR DATA HAS  */ 00065000
*/*     BEEN OVERLAYED BY THE GETMAIN ERROR.  FOR THIS REASON RETRY  */ 00066000
*/*     IS SETUP SO THAT THE AQLRTCA SEGMENT OF IEAVTRTS WILL RECEIVE*/ 00067000
*/*     CONTROL WHERE THE LOCAL SDWA IS UNCONDITIONALLY OBTAINED AND */ 00068000
*/*     THE SDWARPIV BIT IN THE SDWA WILL BE INITIALIZED INDICATING  */ 00069000
*/*     THE REGS AND PSW ARE INVALID FOR THE ORIGINAL ERROR          */ 00070000
*/*     FOR THIS RETRY PATH THIS FRR FREES THE SALLOC LOCK.          */ 00071000
*/*   B.IF THE ERROR TYPE IS NOT THE SAME AS THE ERROR FOR THE       */ 00072000
*/*     UNLOCKED SRB THEN RETRY IS SETUP TO IEAVTRTS WHERE THE LOCAL */ 00073000
*/*     SDWA IS CONDITIONALLY OBTAINED-I.E. THE ERROR DATA FOR THE   */ 00074000
*/*     UNLOCKED SRB IS STILL VALID.  RETRY IS IN SEGMENT AQLRTCA OF */ 00075000
*/*     IEAVTRTS WHERE THE SALLOC LOCK IS FREED.                     */ 00076000
*/* 2.REQUESTS PERCOLATION IF THE ERROR TYPE IS DATERR OR RESTART    */ 00077000
*/* NOTES-----                                                       */ 00078000
*/*  - REGS(R0,R1,R14) ARE RESTRICTED THROUGHOUT THE FRR             */ 00079000
*/*  - THIS FRR SHOULD RECEIVE CONTROL VIA PERCOLATION FROM          */ 00080000
*/*    GETMAINS FRR                                                  */ 00081000
*/*  - THE RSVT IN THE PSA IS RESET TO THE ORIGINAL CONDITIONS AT    */ 00082000
*/*    THE TIME OF THE ORIGINAL ENTRY TO IEAVTRTS FOR THE UNLOCKED   */ 00083000
*/*    SRB MODE ERROR INDEPENDENT OF RETRYING OR PERCOLATING         */ 00084000
*/*  - RECORDING IS ALWAYS REQUESTED                                 */ 00085000
*/*  - IF AN ERROR SHOULD OCCUR IN THIS FRR--RTM ABORT PROCESSING    */ 00086000
*/*    WILL GET CONTROL                                              */ 00087000
*/* ******************************************************************/ 00088000
*                                                                  0099 00089000
*RCOVGETM:                                                         0099 00090000
*   ENTRY;                                                         0099 00091000
@EP00099 DS    0H                                                  0100 00092000
*   RFY                                                            0100 00093000
*    (R14,                          /* REGISTERS THAT MUST BE        */ 00094000
*     R1,                           /* PRESERVED THROUGHOUT THIS     */ 00095000
*     R0) RSTD;                     /* FRR                           */ 00096000
*   RFY                                                            0101 00097000
*     SDWA BASED(R1);               /* ADDRESSABILITY TO THE SDWA    */ 00098000
*   RFY                                                            0102 00099000
*     FRRPARMS BASED(R7);           /* ADDRESSABILITY TO FRR PARM  0102 00100000
*                                      AREA                  @YM07602*/ 00101000
*   RFY                                                            0103 00102000
*     R7 RSTD;                      /* GET CONTROL OF REG    @YM07602*/ 00103000
*   R7=SDWAPARM;                    /* LET REGISTER CONTAIN ADDRESS     00104000
*                                      OF FRR PARM AREA      @YM07602*/ 00105000
         L     R7,SDWAPARM(,R1)                                    0104 00106000
*   RFY                                                            0105 00107000
*     FRRWA BASED(R0);              /* ADDRESSABILITY TO FRR WA      */ 00108000
*   FRRSRMRT=R14;                   /* PRESERVE RETURN ADDRESS TO RTS*/ 00109000
         LR    @15,R0                                              0106 00110000
         ST    R14,FRRSRMRT(,@15)                                  0106 00111000
*   RFY                                                            0107 00112000
*     FRRS BASED(PSACSTK);                                         0107 00113000
*   RT1TLPN=RT1SRMHI;               /* SET UP FOR RTM ABORT          */ 00114000
         L     @15,PSACSTK                                         0108 00115000
         LA    @11,16                                              0108 00116000
         ALR   @11,@15                                             0108 00117000
         MVI   RT1TLPN(@11),X'59'                                  0108 00118000
*   RT1TLPID=NONE;                  /* PROCESSING IF THIS FRR FAILS  */ 00119000
         MVI   RT1TLPID(@11),X'00'                                 0109 00120000
*   IF PSACSTK=PSAPSTK|PSACSTK=PSARSTK THEN/* IF THE CURRENT STACK IS   00121000
*                                      THE PCFLIH STACK OR RESTART 0110 00122000
*                                      STACK RESET THE RSVT TO ITS 0110 00123000
*                                      ORIGINAL SETTING PRIOR TO THE    00124000
*                                      GETMAIN FAILURE               */ 00125000
         C     @15,PSAPSTK                                         0110 00126000
         BE    @RT00110                                            0110 00127000
         C     @15,PSARSTK                                         0110 00128000
         BNE   @RF00110                                            0110 00129000
@RT00110 DS    0H                                                  0111 00130000
*     DO;                                                          0111 00131000
*       RFY                                                        0112 00132000
*         R5 RSTD;                                                 0112 00133000
*       R5=FRRPISTK;                /* SETUP INPUT TO RESET RSVT     */ 00134000
         L     R5,FRRPISTK(,R7)                                    0113 00135000
*       CALL STORESTK;              /* GO AND RESET THE RSVT         */ 00136000
         BAL   @14,STORESTK                                        0114 00137000
*       RFY                                                        0115 00138000
*         R5 UNRSTD;                                               0115 00139000
*     END;                                                         0116 00140000
*   ELSE                                                           0117 00141000
*     ;                                                            0117 00142000
@RF00110 DS    0H                                                  0118 00143000
*   IF SDWATEXC=ON                  /* IF THE ERROR AFTER SRM        */ 00144000
*       |SDWARKEY=ON THEN           /* INVOKED GETMAIN IS DATERR OR     00145000
*                                      RESTART ERROR THEN THIS FRR 0118 00146000
*                                      WILL PERCOLATE                */ 00147000
         TM    SDWATEXC(R1),B'00100010'                            0118 00148000
         BZ    @RF00118                                            0118 00149000
*     DO;                           /* SETUP TO PERCOLATE            */ 00150000
*       SDWAMODN=RTRMODN;           /* SET FRR MODULE NAME           */ 00151000
         MVC   SDWAMODN(8,R1),RTRMODN                              0120 00152000
*       SDWACSCT=RTRCSCT;           /* SET FRR CSECT NAME            */ 00153000
         MVC   SDWACSCT(8,R1),RTRCSCT                              0121 00154000
*       SDWAREXN=GETMREXN;          /* SET FRR NAME-ID               */ 00155000
         MVC   SDWAREXN(8,R1),GETMREXN                             0122 00156000
*       SDWARCRD=ON;                /* INDICATE TO RECORD THE SDWA   */ 00157000
         OI    SDWARCRD(R1),B'10000000'                            0123 00158000
*       SDWASALL=ON;                /* INDICATE TO FREE THE SALLOC 0124 00159000
*                                      LOCK                          */ 00160000
         OI    SDWASALL(R1),B'00000100'                            0124 00161000
*       SDWAVRA=GETMPMSG;           /* PUT FRR MSG IN SDWA           */ 00162000
         MVI   SDWAVRA+99(R1),C' '                                 0125 00163000
         MVC   SDWAVRA+100(8,R1),SDWAVRA+99(R1)                    0125 00164000
         MVC   SDWAVRA(99,R1),GETMPMSG                             0125 00165000
*       SDWAURAL=LENGTH(GETMPMSG);                                 0126 00166000
         MVI   SDWAURAL(R1),X'63'                                  0126 00167000
*       SDWAEBC=ON;                 /* DUMP THE VARIABLE RECORDING IN   00168000
*                                      EBCDIC                        */ 00169000
         OI    SDWAEBC(R1),B'01000000'                             0127 00170000
*       SDWARCDE=SDWACWT;           /* INDICATE TO PERCOLATE         */ 00171000
         MVI   SDWARCDE(R1),X'00'                                  0128 00172000
*       RFY                                                        0129 00173000
*         FRRS BASED(FRRPISTK);                                    0129 00174000
*       RT1TLPN=NONE;               /* INSURE THAT THE SUPER FRR     */ 00175000
         L     @15,FRRPISTK(,R7)                                   0130 00176000
         MVI   RT1TLPN+16(@15),X'00'                               0130 00177000
*       FRRSCURR=FRRSEMP;           /* WILL PROPOGATE THE ERROR TO 0131 00178000
*                                      THE PROPER STACK              */ 00179000
         L     @11,FRRSEMP(,@15)                                   0131 00180000
         ST    @11,FRRSCURR(,@15)                                  0131 00181000
*       RFY                                                        0132 00182000
*         FRRS BASED(PSACSTK);                                     0132 00183000
*     END;                                                         0133 00184000
*   ELSE                            /* IF THE ERROR IS NOT A DATERR     00185000
*                                      OR RESTART ERROR THEN A RETRY    00186000
*                                      BACK TO IEAVTRTS MAINLINE WILL   00187000
*                                      BE SETUP                      */ 00188000
*     DO;                           /* SETUP SDWA FOR RETRY          */ 00189000
         B     @RC00118                                            0134 00190000
@RF00118 DS    0H                                                  0135 00191000
*       SDWARCDE=SDWARETY;          /* INDICATE RETRY IN THE SDWA    */ 00192000
         MVI   SDWARCDE(R1),X'04'                                  0135 00193000
*       SDWARTYA=FRRPRTYA;          /* ESTABLISH THE RETRY ADDRESS   */ 00194000
         L     @15,FRRPRTYA(,R7)                                   0136 00195000
         ST    @15,SDWARTYA(,R1)                                   0136 00196000
*       SDWASR08=FRRPISTK;          /* ESTABLISH THE INTERRUPTED   0137 00197000
*                                      STACK SRM WAS PROCESSING PRIOR   00198000
*                                      TO THE ERROR                  */ 00199000
         L     @15,FRRPISTK(,R7)                                   0137 00200000
         ST    @15,SDWASR08(,R1)                                   0137 00201000
*       SDWASR02=PSACSTK;           /* REESTABLISH SRM RECOVERY STK  */ 00202000
         L     @15,PSACSTK                                         0138 00203000
         ST    @15,SDWASR02(,R1)                                   0138 00204000
*       SDWASR10=FRRPREG2;          /* REESTABLISH SRM BASE REG      */ 00205000
         L     @15,FRRPREG2(,R7)                                   0139 00206000
         ST    @15,SDWASR10(,R1)                                   0139 00207000
*       SDWASR09=SDWASR10+FOURKAY;  /* AND 2ND BASE REG              */ 00208000
         AL    @15,@CF02834                                        0140 00209000
         ST    @15,SDWASR09(,R1)                                   0140 00210000
*       SDWASR11=SDWAPARM;          /* SETUP REG11 FOR RETRY         */ 00211000
         L     @15,SDWAPARM(,R1)                                   0141 00212000
         ST    @15,SDWASR11(,R1)                                   0141 00213000
*       SDWASR15=TWO;               /* INSURE THAT REG15, THE RETURN    00214000
*                                      CODE FROM GETMAIN, INDICATES     00215000
*                                      THAT GETMAIN FOR SQA FAILED   */ 00216000
         MVC   SDWASR15(4,R1),@CF00110                             0142 00217000
*       IF FRRPRCUR=ON THEN         /* IF RTS WAS IN RECURSION MODE     00218000
*                                      WHEN THE ERROR OCCURRED THEN     00219000
*                                      REESTABLISH THE RECURSION   0143 00220000
*                                      STATUS                        */ 00221000
         TM    FRRPRCUR(R7),B'00100000'                            0143 00222000
         BNO   @RF00143                                            0143 00223000
*         SDWASR07=ACTIVE;          /* RTS WAS IN RECURSION MODE     */ 00224000
         MVC   SDWASR07(4,R1),@CF00079                             0144 00225000
*       ELSE                        /* IF RTS WAS NOT IN RECURSION 0145 00226000
*                                      MODE WHEN THE ERROR OCCURRED     00227000
*                                      THEN NO RECURSION IS        0145 00228000
*                                      ESTABLISHED                   */ 00229000
*         SDWASR07=NULL;            /* NO RECURSION                  */ 00230000
         B     @RC00143                                            0145 00231000
@RF00143 SLR   @15,@15                                             0145 00232000
         ST    @15,SDWASR07(,R1)                                   0145 00233000
*       IF FRRPFLPN<SRMRCRD THEN    /* IF RTS INVOKED GETMAIN ON   0146 00234000
*                                      INITIAL ENTRY FOR A SRB MODE     00235000
*                                      FAILURE THEN REG5 AND REG6  0146 00236000
*                                      MUST BE REESTABLISHED TO    0146 00237000
*                                      CONTAIN ORIGINAL ERROR DATA   */ 00238000
@RC00143 CLI   FRRPFLPN(R7),71                                     0146 00239000
         BNL   @RF00146                                            0146 00240000
*         DO;                                                      0147 00241000
*           SDWASR05=FRRPREG3;      /* REG5 TO CONTAIN DUMPOPTS OR 0148 00242000
*                                      MACH CHK DATA                 */ 00243000
         L     @15,FRRPREG3(,R7)                                   0148 00244000
         ST    @15,SDWASR05(,R1)                                   0148 00245000
*           RFY                                                    0149 00246000
*             FRRS BASED(FRRPISTK);                                0149 00247000
*           SDWASR06=RT1WEED;       /* REG6 TO CONTAIN EEDCHAIN    0150 00248000
*                                      POINTER                       */ 00249000
         L     @15,FRRPISTK(,R7)                                   0150 00250000
         L     @11,RT1WEED(,@15)                                   0150 00251000
         ST    @11,SDWASR06(,R1)                                   0150 00252000
*           RFY                                                    0151 00253000
*             FRRS BASED(PSACSTK);                                 0151 00254000
*           RFY                                                    0152 00255000
*             R4 RSTD;                                             0152 00256000
*           RFY                                                    0153 00257000
*             FRRS BASED(PSACSTK);                                 0153 00258000
*           R4=RT1TENPT;            /* PICK CURRENT ERROR TYPE       */ 00259000
         LA    @11,16                                              0154 00260000
         L     @08,PSACSTK                                         0154 00261000
         ALR   @08,@11                                             0154 00262000
         SLR   R4,R4                                               0154 00263000
         IC    R4,RT1TENPT(,@08)                                   0154 00264000
*           RFY                                                    0155 00265000
*             FRRS BASED(FRRPISTK);                                0155 00266000
*           IF R4=RT1TENPT          /* IF CURRENT ERROR TYPE AND     */ 00267000
*             THEN                  /* ORIGINAL ERROR TYPE ARE THE 0156 00268000
*                                      SAME THEN REGS TO BE INIT IN     00269000
*                                      DEFAULT LOCAL SDWA WILL     0156 00270000
*                                      REFLECT THE GETMAIN FAILURE 0156 00271000
*                                      AND NOT THE FRR FAILURE       */ 00272000
         ALR   @15,@11                                             0156 00273000
         SLR   @11,@11                                             0156 00274000
         IC    @11,RT1TENPT(,@15)                                  0156 00275000
         CR    R4,@11                                              0156 00276000
         BNE   @RF00156                                            0156 00277000
*             DO;                                                  0157 00278000
*               SDWASR04=SDWAPARM;                                 0158 00279000
         L     @15,SDWAPARM(,R1)                                   0158 00280000
         ST    @15,SDWASR04(,R1)                                   0158 00281000
*               GENERATE CODE;                                     0159 00282000
*                               /*RETRY PT IS WITH SALLOC LK NOT HELD*/ 00283000
         SETLOCK RELEASE,TYPE=SALLOC,DISABLED,                         C00284000
               RELATED=('RELEASE SALLOC BEFORE RETRY')                  00285000
         L     R3,RUNCLLK GET ADDR OF RETRY RTN TO UNCOND GET LCL SDWA  00286000
         ST    R3,SDWARTYA(,R1)  STORE RETRY ADDRESS                    00287000
*             END;                                                 0160 00288000
*           RFY                                                    0161 00289000
*             FRRS BASED(PSACSTK);                                 0161 00290000
@RF00156 DS    0H                                                  0162 00291000
*         END;                                                     0162 00292000
*       ELSE                        /* IF SRM WAS IN THE PROCESS OF     00293000
*                                      CHANGING MODES PRIOR TO     0163 00294000
*                                      PERCOLATION WHEN GETMAIN WAS     00295000
*                                      INVOKED THEN REG5 AND REG6 ARE   00296000
*                                      REESTABLISHED WITH THE SDWA 0163 00297000
*                                      ADDR BEING PROCESSED AND THE     00298000
*                                      FRR AVAILABILITY RESPECTIVELY */ 00299000
*         DO;                                                      0163 00300000
         B     @RC00146                                            0163 00301000
@RF00146 DS    0H                                                  0164 00302000
*           RFY                                                    0164 00303000
*             FRRS BASED(FRRPISTK);                                0164 00304000
*           SDWASR05=RT1WRTCA;      /* RESTORE THE SDWA ADDRESS      */ 00305000
         L     @15,FRRPISTK(,R7)                                   0165 00306000
         L     @15,RT1WRTCA(,@15)                                  0165 00307000
         ST    @15,SDWASR05(,R1)                                   0165 00308000
*           RFY                                                    0166 00309000
*             FRRS BASED(PSACSTK);                                 0166 00310000
*           SDWASR06=FRRPREG3;      /* RESTORE FRR AVAILABILITY      */ 00311000
         L     @15,FRRPREG3(,R7)                                   0167 00312000
         ST    @15,SDWASR06(,R1)                                   0167 00313000
*         END;                                                     0168 00314000
*       SDWAUPRG=ON;                /* INDICATE UPDATED REGS FOR   0169 00315000
*                                      RETRY IN THE SDWA             */ 00316000
@RC00146 OI    SDWAUPRG(R1),B'00001000'                            0169 00317000
*       SDWAVRA=GETMRMSG;           /* SET RETRY MESSAGE IN VAR    0170 00318000
*                                      RECORDING AREA OF SDWA        */ 00319000
         MVI   SDWAVRA+94(R1),C' '                                 0170 00320000
         MVC   SDWAVRA+95(13,R1),SDWAVRA+94(R1)                    0170 00321000
         MVC   SDWAVRA(94,R1),GETMRMSG                             0170 00322000
*       SDWAURAL=LENGTH(GETMRMSG);  /* SET LENGTH OF RETRY MSG       */ 00323000
         MVI   SDWAURAL(R1),X'5E'                                  0171 00324000
*       SDWAEBC=ON;                 /* DUMP VARIABLE RECORDING AREA     00325000
*                                      IN EBCDIC                     */ 00326000
         OI    SDWAEBC(R1),B'01000000'                             0172 00327000
*       SDWARCRD=ON;                /* INDICATE RECORDING            */ 00328000
         OI    SDWARCRD(R1),B'10000000'                            0173 00329000
*     END;                                                         0174 00330000
*   R14=FRRSRMRT;                   /* RESTORE RETURN ADDR TO RTS    */ 00331000
@RC00118 LR    @11,R0                                              0175 00332000
         L     R14,FRRSRMRT(,@11)                                  0175 00333000
*   RFY                                                            0176 00334000
*     R7 UNRSTD;                    /* RELEASE REGISTER      @YM07602*/ 00335000
*   RETURN;                         /* RETURN TO IEAVTRTS            */ 00336000
@EL00001 DS    0H                                                  0177 00337000
@EF00001 DS    0H                                                  0177 00338000
@ER00001 BR    @14                                                 0177 00339000
*/* ******************************************************************/ 00340000
*/* THIS SEGMENT IS AN FRR WHICH RECOVERS IEAVTRTS FROM POSSIBLE     */ 00341000
*/* ERRORS IN THE SOFTWARE RECORDING FACILITY(IEAVTRER) AND GTF      */ 00342000
*/* WHEN IEAVTRTS ATTEMPTS TO TRACE FRRS AND RECORD SDWAS,RESP.      */ 00343000
*/*  - RECOVERY FROM GTF FAILURE INVOLVES REQUESTING RETRY AT THE    */ 00344000
*/*    POINT IN IEAVTRTS AFTER THE BALR TO GTF IF THE ERROR TYPE     */ 00345000
*/*    IS NOT DATERR OR RESTART.  IF THE ERROR IS DATERR OR RESTART  */ 00346000
*/*    PERCOLATION IS REQUESTED. RECORDING IS ALWAYS REQUESTED       */ 00347000
*/*  - RECOVERY FROM RECORDING FAILURE INVOLVES REQUESTING RETRY AT  */ 00348000
*/*    THE POINT IN IEAVTRTS AFTER THE BALR TO THE SOFTWARE RECORDING*/ 00349000
*/*    FACILITY(IEAVTRER) PROVIDED THE ERROR TYPE IN THE SDWA DOES   */ 00350000
*/*    NOT INDICATE DATERR OR RESTART.  IF THE ERROR TYPE IS DATERR  */ 00351000
*/*    OR RESTART, PERCOLATION IS REQUESTED.  RECORDING OF THE SDWA  */ 00352000
*/*    IS NOT REQUESTED.                                             */ 00353000
*/* NOTES                                                            */ 00354000
*/*  - REGS(R0,R1,R14,R2) ARE RESTRICTED THROUGHOUT THIS FRR         */ 00355000
*/*  - THE RSVT IN THE PSA IS RESET TO THE ORIGINAL CONDITIONS AT THE*/ 00356000
*/*    TIME OF THE ORIGINAL ENTRY TO IEAVTRTS FOR THE ERROR          */ 00357000
*/*    INDEPENDENT OF WHETHER THIS FRR REQUESTS PERCOLATION OR RETRY */ 00358000
*/*  - RECORDING IS REQUESTED IF GTF FAILED                          */ 00359000
*/*  - RECORDING IS NOT REQUESTED IF RECORDING FAILED                */ 00360000
*/*  - IF PERCOLATION IS REQUESTED AND IF A SRB SDWA WAS GETMAINED   */ 00361000
*/*    THEN THIS SDWA IS FREED AS A PART OF THE CLEANUP PRIOR TO     */ 00362000
*/*    THE PERCOLATION                                               */ 00363000
*/*  - IF AN ERROR SHOULD OCCUR IN THIS FRR---RTM ABORT PROCESSING   */ 00364000
*/*    WILL GET CONTROL                                              */ 00365000
*/*  - THIS FRR WILL ABORT RECOVERY IN THE EVENT THAT THE            */ 00366000
*/*    ORIGINAL ADDRESS OF THE INTERRUPTED FRR STACK IEAVTRTS WAS    */ 00367000
*/*    PROCESSING PRIOR TO THE RECORDING OR GTF FAILURE HAS BEEN     */ 00368000
*/*    LOST BY A SUCCESSION OF RECURSIVE ERRORS.                     */ 00369000
*/*    RETRY BACK TO RTS FOR PROCESSING THE ORIGINAL ERROR PRIOR     */ 00370000
*/*    TO THE RECORDING OR GTF FAILURE IS IMPOSSIBLE.                */ 00371000
*/*  - THIS FRR SHOULD BE PERCOLATED TO BY EITHER RECORDINGS OR GTFS */ 00372000
*/*    FRR                                                           */ 00373000
*/* ******************************************************************/ 00374000
*                                                                  0178 00375000
*RCOVRGTF:                                                         0178 00376000
*RCOVRCRD:                                                         0178 00377000
*   ENTRY;                                                         0178 00378000
@EP00178 DS    0H                                                  0179 00379000
*   RFY                                                            0179 00380000
*    (R14,                          /* REGISTERS THAT MUST BE        */ 00381000
*     R1,                           /* RESTRICTED THROUGHOUT THIS    */ 00382000
*     R2,                           /* FRRS PROCESSING               */ 00383000
*     R0) RSTD;                                                    0179 00384000
*   RFY                                                            0180 00385000
*     FRRS BASED(PSACSTK);          /* ADDRESSABILITY TO CURRENT   0180 00386000
*                                      STACK                         */ 00387000
*   RT1TLPN=RT1SRMHI;               /* SET UP FOR ABORT RTM        0181 00388000
*                                      PROCESSING                    */ 00389000
         L     @15,PSACSTK                                         0181 00390000
         LA    @11,16                                              0181 00391000
         ALR   @11,@15                                             0181 00392000
         MVI   RT1TLPN(@11),X'59'                                  0181 00393000
*   RT1TLPID=NONE;                  /* IF THIS FRR SHOULD FAIL       */ 00394000
         MVI   RT1TLPID(@11),X'00'                                 0182 00395000
*   RFY                                                            0183 00396000
*     SDWA BASED(R1);               /* INRTCA REG POINTS TO THE SDWA    00397000
*                                      PASSED ON INPUT TO FRR        */ 00398000
*   RFY                                                            0184 00399000
*     FRRPARMS BASED(R7);           /* ADDRESSABILITY TO THE FRR   0184 00400000
*                                      PARAMETER AREA        @YM07602*/ 00401000
*   RFY                                                            0185 00402000
*     R7 RSTD;                      /* GET CONTROL OF REG    @YM07602*/ 00403000
*   R7=SDWAPARM;                    /* LET REGISTER CONTAIN ADDRESS     00404000
*                                      OF FRR PARM AREA      @YM07602*/ 00405000
         L     R7,SDWAPARM(,R1)                                    0186 00406000
*   RFY                                                            0187 00407000
*     FRRS BASED(FRRPISTK);         /* ADDRESSABILITY TO THE       0187 00408000
*                                      INTERRUPTED STACK SRM WAS   0187 00409000
*                                      PROCESSING PRIOR TO THE     0187 00410000
*                                      FAILURE                       */ 00411000
*   RFY                                                            0188 00412000
*     FRRWA BASED(R0);              /* ADDRESSABILITY TO THE FRR WORK   00413000
*                                      AREA PASSED IN REG0           */ 00414000
*   FRRSRMRT=R14;                   /* CHKPT RETURN ADDRESS TO     0189 00415000
*                                      IEAVTRTS                      */ 00416000
         LR    @11,R0                                              0189 00417000
         ST    R14,FRRSRMRT(,@11)                                  0189 00418000
*   IF PSACSTK=R2 THEN              /* IF THE INTERRUPTED STACK IS 0190 00419000
*                                      THE SAME AS THE STACK USED BY    00420000
*                                      RTS FOR ITS RECOVERY AND IS 0190 00421000
*                                      ALSO THE SAME STACK BEING   0190 00422000
*                                      RECOVERED THEN ABORT RECOVERY    00423000
*                                      SINCE RESTORATION OF THE    0190 00424000
*                                      ORIGINAL STACK IS IMPOSSIBLE  */ 00425000
         CR    @15,R2                                              0190 00426000
         BNE   @RF00190                                            0190 00427000
*     IF PSACSTK=FRRPISTK THEN                                     0191 00428000
         C     @15,FRRPISTK(,R7)                                   0191 00429000
         BNE   @RF00191                                            0191 00430000
*       GEN(SVC   13);              /* ABORT RECOVERY                */ 00431000
         SVC   13                                                       00432000
*     ELSE                          /* IF THE STACKS CAN BE UNWOUND     00433000
*                                      THEN IF THE CURRENT STACK IS     00434000
*                                      EITHER THE PCFLIH STACK OR  0193 00435000
*                                      RESTART STACK THE RSVT MUST BE   00436000
*                                      RESTORED TO ITS STATE PRIOR TO   00437000
*                                      THE IEAVTRTS ERROR            */ 00438000
*       IF PSACSTK=PSAPSTK|PSACSTK=PSARSTK THEN                    0193 00439000
         B     @RC00191                                            0193 00440000
@RF00191 L     @15,PSACSTK                                         0193 00441000
         C     @15,PSAPSTK                                         0193 00442000
         BE    @RT00193                                            0193 00443000
         C     @15,PSARSTK                                         0193 00444000
         BNE   @RF00193                                            0193 00445000
@RT00193 DS    0H                                                  0194 00446000
*         DO;                                                      0194 00447000
*           RFY                                                    0195 00448000
*             R5 RSTD;                                             0195 00449000
*           R5=FRRPISTK;                                           0196 00450000
         L     R5,FRRPISTK(,R7)                                    0196 00451000
*           CALL STORESTK;          /* RESET THE RSVT TO ITS PROPER     00452000
*                                      STATE                         */ 00453000
         BAL   @14,STORESTK                                        0197 00454000
*         END;                                                     0198 00455000
*   IF SDWATEXC=ON                  /* IF THE ERROR IS A DATERR      */ 00456000
*       |SDWARKEY=ON THEN           /* OR A RESTART ERROR THEN THIS     00457000
*                                      FRR WILL SETUP TO PERCOLATE   */ 00458000
@RF00193 DS    0H                                                  0199 00459000
@RC00191 DS    0H                                                  0199 00460000
@RF00190 TM    SDWATEXC(R1),B'00100010'                            0199 00461000
         BZ    @RF00199                                            0199 00462000
*     DO;                           /* PERCOLATE THE ERROR ON A    0200 00463000
*                                      RESTART OR DATERR             */ 00464000
*       IF FRRPHOOK=ON THEN         /* IF SRM FAILED AFTER INVOKING     00465000
*                                      GTF THEN THE PERCOLATION SETUP   00466000
*                                      IS MADE ASSUMING THAT THE   0201 00467000
*                                      PCFLIH STACK WAS CURRENT AT 0201 00468000
*                                      THE TIME OF THE FAILURE       */ 00469000
         TM    FRRPHOOK(R7),B'01000000'                            0201 00470000
         BNO   @RF00201                                            0201 00471000
*         DO;                                                      0202 00472000
*           SDWAMODN=RTRMODN;       /* SET MODULE NAME OF FRR IN THE    00473000
*                                      SDWA                          */ 00474000
         MVC   SDWAMODN(8,R1),RTRMODN                              0203 00475000
*           SDWACSCT=RTRCSCT;       /* SET CSECT NAME OF FRR IN THE     00476000
*                                      SDWA                          */ 00477000
         MVC   SDWACSCT(8,R1),RTRCSCT                              0204 00478000
*           SDWAREXN=GTFREXN;       /* SET RECOVERY ROUTINE NAME FOR    00479000
*                                      THIS FRR WHICH IS RECOVERING     00480000
*                                      GTF IN THE SDWA               */ 00481000
         MVC   SDWAREXN(8,R1),GTFREXN                              0205 00482000
*           SDWARCRD=ON;            /* INDICATE THAT RECORDING IS TO    00483000
*                                      TAKE PLACE                    */ 00484000
         OI    SDWARCRD(R1),B'10000000'                            0206 00485000
*           SDWAVRA=GTFPMSG;        /* PLACE PERCOLATE MESSAGE IN THE   00486000
*                                      VARIABLE RECORDING AREA OF THE   00487000
*                                      SDWA                          */ 00488000
         MVI   SDWAVRA+89(R1),C' '                                 0207 00489000
         MVC   SDWAVRA+90(18,R1),SDWAVRA+89(R1)                    0207 00490000
         MVC   SDWAVRA(89,R1),GTFPMSG                              0207 00491000
*           SDWAURAL=LENGTH(GTFPMSG);                              0208 00492000
         MVI   SDWAURAL(R1),X'59'                                  0208 00493000
*           SDWAEBC=ON;             /* VARIABLE RECORDING AREA TO BE    00494000
*                                      DUMPED IN EBCDIC              */ 00495000
         OI    SDWAEBC(R1),B'01000000'                             0209 00496000
*         END;                                                     0210 00497000
*       ELSE                        /* IF RTS DID NOT FAIL AFTER   0211 00498000
*                                      INVOKING GTF THEN SRM MUST  0211 00499000
*                                      HAVE FAILED AFTER INVOKING  0211 00500000
*                                      RECORDING                     */ 00501000
*         SDWARCRD=OFF;             /* SINCE RTS FAILED AFTER      0211 00502000
*                                      INVOKING RECORDING THE OF THIS   00503000
*                                      SDWA IS NOT REQUESTED         */ 00504000
         B     @RC00201                                            0211 00505000
@RF00201 NI    SDWARCRD(R1),B'01111111'                            0211 00506000
*       SDWARCDE=SDWACWT;           /* INDICATE PERCOLATION IN THE 0212 00507000
*                                      SDWA                          */ 00508000
@RC00201 MVI   SDWARCDE(R1),X'00'                                  0212 00509000
*       IF FRRPISTK^=PSANSTK THEN   /* IF THE INTERRUPTED STACK PRIOR   00510000
*                                      TO THE RTS ERROR IS A SUPER 0213 00511000
*                                      STACK THEN PERCOLATE TO THE 0213 00512000
*                                      SUPER FRR WITH THIS STACK   0213 00513000
*                                      EMPTY AND READY TO ACCEPT A 0213 00514000
*                                      NEW ERROR                     */ 00515000
         L     @15,FRRPISTK(,R7)                                   0213 00516000
         C     @15,PSANSTK                                         0213 00517000
         BE    @RF00213                                            0213 00518000
*         DO;                                                      0214 00519000
*           FRRSRCUR=OFF;                                          0215 00520000
         L     @11,FRRSCURR(,@15)                                  0215 00521000
         NI    FRRSRCUR(@11),B'01111111'                           0215 00522000
*           FRRSCURR=FRRSEMP;       /* MAKE THE STACK EMPTY          */ 00523000
         L     @11,FRRSEMP(,@15)                                   0216 00524000
         ST    @11,FRRSCURR(,@15)                                  0216 00525000
*           RT1TLPN=NONE;           /* READY FOR THE NEW ERROR       */ 00526000
         MVI   RT1TLPN+16(@15),X'00'                               0217 00527000
*         END;                                                     0218 00528000
*       ELSE                        /* OTHERWISE PERC TO THE SUPER 0219 00529000
*                                      FRR WITH THIS STACK SET TO  0219 00530000
*                                      INDICATE A RECURSIVE FRR    0219 00531000
*                                      FAILURE                       */ 00532000
*         RT1TLPN=VALIDREC;         /* INDICATE VALID RECURSION INTO    00533000
*                                      IEAVTRTS                      */ 00534000
         B     @RC00213                                            0219 00535000
@RF00213 L     @15,FRRPISTK(,R7)                                   0219 00536000
         MVI   RT1TLPN+16(@15),X'FF'                               0219 00537000
*     END;                                                         0220 00538000
*                                                                  0220 00539000
*/*   SET UP RETRY OF THE ERROR FOR IEAVTRTS                         */ 00540000
*                                                                  0221 00541000
*   ELSE                            /* IF THE ERROR IS NEITHER A   0221 00542000
*                                      DATERR OR RESTART ERROR A   0221 00543000
*                                      RETRY BACK TO IEAVTRTS WILL BE   00544000
*                                      SETUP                         */ 00545000
*     IF FRRPHOOK=ON THEN           /* IF RTS WAS TRACEING A FRR WHEN   00546000
*                                      THE ERROR OCCURRED THEN THE 0221 00547000
*                                      RETRY WILL BE SETUP TO RTS  0221 00548000
*                                      AFTER THE CALL TO GTF         */ 00549000
         B     @RC00199                                            0221 00550000
@RF00199 TM    FRRPHOOK(R7),B'01000000'                            0221 00551000
         BNO   @RF00221                                            0221 00552000
*       DO;                                                        0222 00553000
*         RFY                                                      0223 00554000
*           FRRS BASED(FRRPISTK);                                  0223 00555000
*         SDWARCRD=ON;              /* INDICATE RECORDING            */ 00556000
         OI    SDWARCRD(R1),B'10000000'                            0224 00557000
*         SDWAMODN=RTRMODN;         /* SET MODULE NAME OF IEAVTRTS 0225 00558000
*                                      FRR                           */ 00559000
         MVC   SDWAMODN(8,R1),RTRMODN                              0225 00560000
*         SDWACSCT=RTRCSCT;         /* SET CSECT NAME OF IEAVTRTS    */ 00561000
         MVC   SDWACSCT(8,R1),RTRCSCT                              0226 00562000
*         SDWAREXN=GTFREXN;         /* SET RECOVERY ROUTINE NAME     */ 00563000
         MVC   SDWAREXN(8,R1),GTFREXN                              0227 00564000
*         SDWAVRA=GTFRMSG;          /* PUT RETRY MESSAGE IN VAR    0228 00565000
*                                      RECORDING AREA OF SDWA        */ 00566000
         MVI   SDWAVRA+74(R1),C' '                                 0228 00567000
         MVC   SDWAVRA+75(33,R1),SDWAVRA+74(R1)                    0228 00568000
         MVC   SDWAVRA(74,R1),GTFRMSG                              0228 00569000
*         SDWAURAL=LENGTH(GTFRMSG); /* SET LENGTH OF MSG             */ 00570000
         MVI   SDWAURAL(R1),X'4A'                                  0229 00571000
*         SDWAEBC=ON;               /* DUMP VARIABLE RECORDING AREA     00572000
*                                      IN EBCDIC                     */ 00573000
         OI    SDWAEBC(R1),B'01000000'                             0230 00574000
*         SDWARCDE=SDWARETY;        /* INDICATE RETRY IN SDWA        */ 00575000
         MVI   SDWARCDE(R1),X'04'                                  0231 00576000
*         SDWAUPRG=ON;              /* REGS WILL BE UPDATED FOR RETRY*/ 00577000
         OI    SDWAUPRG(R1),B'00001000'                            0232 00578000
*         SDWARTYA=FRRPRTYA;        /* INITIALIZE RETRY ADDRESS      */ 00579000
         L     @15,FRRPRTYA(,R7)                                   0233 00580000
         ST    @15,SDWARTYA(,R1)                                   0233 00581000
*         SDWASR00=FRRPREG2;        /* REPRIME REG 0                 */ 00582000
         L     @15,FRRPREG2(,R7)                                   0234 00583000
         ST    @15,SDWASR00(,R1)                                   0234 00584000
*         SDWASR01=RT1WRTCA;        /* REPRIME REG1 PTR TO SDWA      */ 00585000
         L     @15,FRRPISTK(,R7)                                   0235 00586000
         L     @11,RT1WRTCA(,@15)                                  0235 00587000
         ST    @11,SDWASR01(,R1)                                   0235 00588000
*         SDWASR02=FRRPREG1;        /* REPRIME SRMS RECOVERY STACK 0236 00589000
*                                      POINTER                       */ 00590000
         L     @08,FRRPREG1(,R7)                                   0236 00591000
         ST    @08,SDWASR02(,R1)                                   0236 00592000
*         SDWASR05=RT1WRTCA;        /* REPRIME TARGRTCA REG          */ 00593000
         ST    @11,SDWASR05(,R1)                                   0237 00594000
*         SDWASR06=FRRPREG3;        /* REPRIME FRR AVAILABILITY      */ 00595000
         L     @11,FRRPREG3(,R7)                                   0238 00596000
         ST    @11,SDWASR06(,R1)                                   0238 00597000
*         SDWASR07=NULL;            /* ON GTF FAILURE SRM IS NOT IN     00598000
*                                      RECURSION                     */ 00599000
         SLR   @11,@11                                             0239 00600000
         ST    @11,SDWASR07(,R1)                                   0239 00601000
*         SDWASR08=FRRPISTK;        /* REPRIME INTERRUPTED STACK   0240 00602000
*                                      POINTER                       */ 00603000
         ST    @15,SDWASR08(,R1)                                   0240 00604000
*         RFY                                                      0241 00605000
*           SRMRGSV BASED(R1+LENGTH(SDWA));                        0241 00606000
*         SDWASR10=SRMREG10;        /* REESTABLISH SRM BASE REGS     */ 00607000
         L     @15,SRMREG10+512(,R1)                               0242 00608000
         ST    @15,SDWASR10(,R1)                                   0242 00609000
*         SDWASR09=SDWASR10+FOURKAY;/* REGISTERS 9 AND 10            */ 00610000
         AL    @15,@CF02834                                        0243 00611000
         ST    @15,SDWASR09(,R1)                                   0243 00612000
*       END;                                                       0244 00613000
*     ELSE                          /* IF RTS FAILED AFTER CALLING 0245 00614000
*                                      RECORDING OF THE SDWA THEN  0245 00615000
*                                      SETUP RETRY BACK TO RTS AFTER    00616000
*                                      THE CALL TO RECORDING         */ 00617000
*       DO;                                                        0245 00618000
         B     @RC00221                                            0245 00619000
@RF00221 DS    0H                                                  0246 00620000
*         SDWARCRD=OFF;             /* DONT RECORD THIS SDWA         */ 00621000
         NI    SDWARCRD(R1),B'01111111'                            0246 00622000
*         SDWARCDE=SDWARETY;        /* INDICATE RETRY                */ 00623000
         MVI   SDWARCDE(R1),X'04'                                  0247 00624000
*         SDWAUPRG=ON;              /* UPDATED REGS USED FOR RETRY   */ 00625000
         OI    SDWAUPRG(R1),B'00001000'                            0248 00626000
*         SDWARTYA=FRRPRTYA;        /* SET THE RETRY ADDRESS         */ 00627000
         L     @15,FRRPRTYA(,R7)                                   0249 00628000
         ST    @15,SDWARTYA(,R1)                                   0249 00629000
*         RFY                                                      0250 00630000
*           R4 RSTD;                                               0250 00631000
*         R4=SRMWSAV2;                                             0251 00632000
         L     R4,SRMWSAV2+512(,R1)                                0251 00633000
*         SDWASR00=R4;              /* RESTORE REG0                  */ 00634000
         ST    R4,SDWASR00(,R1)                                    0252 00635000
*         RFY                                                      0253 00636000
*           R4 UNRSTD;                                             0253 00637000
*         SDWASR01=RT1WRTCA;        /* REPRIME SDWA POINTER          */ 00638000
         L     @15,FRRPISTK(,R7)                                   0254 00639000
         L     @11,RT1WRTCA(,@15)                                  0254 00640000
         ST    @11,SDWASR01(,R1)                                   0254 00641000
*         SDWASR02=PSACSTK;         /* RESTORE RTS RECOVERY STK      */ 00642000
         L     @08,PSACSTK                                         0255 00643000
         ST    @08,SDWASR02(,R1)                                   0255 00644000
*         SDWASR05=RT1WRTCA;        /* RESTORE TARGRTCA REG          */ 00645000
         ST    @11,SDWASR05(,R1)                                   0256 00646000
*         SDWASR06=FRRPREG2;        /* RESTORE FRR AVAILABILITY      */ 00647000
         L     @04,FRRPREG2(,R7)                                   0257 00648000
         ST    @04,SDWASR06(,R1)                                   0257 00649000
*         SDWASR07=FRRPREG3;        /* RESTORE RECURSION STATUS      */ 00650000
         L     @04,FRRPREG3(,R7)                                   0258 00651000
         ST    @04,SDWASR07(,R1)                                   0258 00652000
*         SDWASR08=FRRPISTK;        /* RESTORE INTERRUPTED STK ADDR  */ 00653000
         ST    @15,SDWASR08(,R1)                                   0259 00654000
*         SDWASR10=FRRPREG1;        /* RESTORE SRMS BASE REG         */ 00655000
         L     @15,FRRPREG1(,R7)                                   0260 00656000
         ST    @15,SDWASR10(,R1)                                   0260 00657000
*         SDWASR09=SDWASR10+FOURKAY;/* RESTORE 2ND BASE REG          */ 00658000
         AL    @15,@CF02834                                        0261 00659000
         ST    @15,SDWASR09(,R1)                                   0261 00660000
*         RFY                                                      0262 00661000
*           R4 RSTD;                                               0262 00662000
*         RFY                                                      0263 00663000
*           FRRS BASED(PSACSTK);                                   0263 00664000
*         R4=RT1TENPT;              /* PICK UP ERROR TYPE            */ 00665000
         LA    @15,16                                              0264 00666000
         ALR   @08,@15                                             0264 00667000
         SLR   R4,R4                                               0264 00668000
         IC    R4,RT1TENPT(,@08)                                   0264 00669000
*         RFY                                                      0265 00670000
*           FRRS BASED(FRRPISTK);                                  0265 00671000
*         IF R4=RT1TENPT            /* IF THE ERROR TYPE FOR THIS    */ 00672000
*             &FRRPREG3=ACTIVE THEN /* FRR IS THE SAME AS THE ERROR     00673000
*                                      ORIGINALLY BEING HANDLED AND     00674000
*                                      RECORDING WAS INVOKED BECAUSE    00675000
*                                      THE FRR FAILED THEN THE REGS     00676000
*                                      AND PSW AT THE TIME OF THE  0266 00677000
*                                      WILL BE OVERLAYED WITH THE  0266 00678000
*                                      ERREGS AT THE TIME OF THE   0266 00679000
*                                      RECORDING FAILURE             */ 00680000
         L     @08,FRRPISTK(,R7)                                   0266 00681000
         ALR   @08,@15                                             0266 00682000
         SLR   @15,@15                                             0266 00683000
         IC    @15,RT1TENPT(,@08)                                  0266 00684000
         CR    R4,@15                                              0266 00685000
         BNE   @RF00266                                            0266 00686000
         L     @15,FRRPREG3(,R7)                                   0266 00687000
         C     @15,@CF00079                                        0266 00688000
         BNE   @RF00266                                            0266 00689000
*           DO;                                                    0267 00690000
*             R4=RT1WRTCA;                                         0268 00691000
         LR    R4,@11                                              0268 00692000
*             RFY                                                  0269 00693000
*               SDWA BASED(R4);                                    0269 00694000
*             SDWARPIV=ON;          /* INDICATES REGS-PSW INVALID IN    00695000
*                                      THE SDWA ASSOCIATED WITH THE     00696000
*                                      INTERRUPTED STACK BEING     0270 00697000
*                                      PROCESSED PRIOR TO THE      0270 00698000
*                                      RECORDING FAILURE             */ 00699000
         OI    SDWARPIV(R4),B'00000100'                            0270 00700000
*             RFY                                                  0271 00701000
*               SDWA BASED(R1);                                    0271 00702000
*           END;                                                   0272 00703000
*         ELSE                                                     0273 00704000
*           ;                       /* OTHERWISE THE OLD SDWA IS OK  */ 00705000
@RF00266 DS    0H                                                  0274 00706000
*       END;                                                       0274 00707000
*   R14=FRRSRMRT;                   /* RESTORE RETURN ADDR TO RTS    */ 00708000
@RC00221 DS    0H                                                  0275 00709000
@RC00199 LR    @11,R0                                              0275 00710000
         L     R14,FRRSRMRT(,@11)                                  0275 00711000
*   RFY                                                            0276 00712000
*     R7 UNRSTD;                    /* RELEASE REGISTER      @YM07602*/ 00713000
*   RETURN;                         /* RETURN TO IEAVTRTS            */ 00714000
         B     @EL00001                                            0277 00715000
*/* ******************************************************************/ 00716000
*/* THIS SEGMENT IS AN INTERNAL PROCEDURE CALLED BY SEGMENT SRMDRCOV */ 00717000
*/* AS PART OF THE LPRR DESIGNED TO RECOVER IEAVTRTS FROM A FAILURE  */ 00718000
*/* FOR WHICH FRR RECOVERY IS NOT PRESENT.  THIS SEGMENT INITIALIZES */ 00719000
*/* THE SDWA TO DESCRIBE THE IEAVTRTS FAILURE.                       */ 00720000
*/* INPUTS-----                                                      */ 00721000
*/*  -RT1TENPT FIELD OF THE RTM1 WORK AREA PORTION OF THE INTERRUPTED*/ 00722000
*/*    FRR STACK CONTAINS THE ERROR TYPE OF THE FAILURE IN IEAVTRTS  */ 00723000
*/*  - FLAGSCC REGISTER(R1) CONTAINS THE COMPLETION CODE             */ 00724000
*/*  - PSWPTR1 REGISTER(R2) POINTS TO THE 1ST HALF OF THE ERROR PSW  */ 00725000
*/*  - PSWPTR2 REGISTER(R3) POINTS TO THE 2ND HALF OF THE ERROR PSW  */ 00726000
*/*  - DUMPOPTS REGISTER(R5) CONTAINS THE ADDRESS OF DUMPOPTS        */ 00727000
*/*    ASSOCIATED WITH THE IEAVTRTS FAILURE IF THE ERROR TYPE IS     */ 00728000
*/*    NOT A MACHINE CHECK                                           */ 00729000
*/*  - EEDCHAIN REGISTER(R6) POINTS TO A COMPLETE ERROR DESCRIPTION  */ 00730000
*/*    FOR A MACHINE CHECK IN IEAVTRTS                               */ 00731000
*/*  - REPRDATA REGISTER(R5) CONTAINS ABBREVIATED MACHINE CHECK      */ 00732000
*/*    ERROR DATA FOR THE FAILURE IN IEAVTRTS IF A COMPLETE          */ 00733000
*/*    DESCRIPTION IS UNAVAILABLE                                    */ 00734000
*/*  - STAKPTR REGISTER(R7) POINTS TO THE FRR STACK THAT WAS CURRENT */ 00735000
*/*    AT THE TIME OF THE IEAVTRTS FAILURE                           */ 00736000
*/*  - SAVAREAP REGISTER(R13) POINTS THE REGISTERS AT THE TIME OF    */ 00737000
*/*    THE IEAVTRTS FAILURE                                          */ 00738000
*/* OUTPUTS-----                                                     */ 00739000
*/*  - RT1WRTCA FIELD IN THE RTM1 WORK AREA PORTION OF THE           */ 00740000
*/*    INTERRUPTED FRR STACK POINTS TO THE INITIALIZED SDWA          */ 00741000
*/* NOTES-----                                                       */ 00742000
*/*  - REGS(R0,R1,R2,R3,R4,R5,R6,R7,R9,R10,R13,R14) ARE RESTRICTED   */ 00743000
*/*  - ALL OTHER REGS ARE VOLATILE                                   */ 00744000
*/* ******************************************************************/ 00745000
*                                                                  0278 00746000
*SETRTCA:                                                          0278 00747000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                               0278 00748000
SETRTCA  DS    0H                                                  0279 00749000
*   RFY                                                            0279 00750000
*    (R1,                           /* PRESERVE THESE REGISTERS      */ 00751000
*     R0,                                                          0279 00752000
*     R5,                                                          0279 00753000
*     R6,                                                          0279 00754000
*     R7,                                                          0279 00755000
*     R9,                                                          0279 00756000
*     R12,                                                         0279 00757000
*     R13,                                                         0279 00758000
*     R2,                                                          0279 00759000
*     R3,                                                          0279 00760000
*     R14,                                                         0279 00761000
*     R4) RSTD;                                                    0279 00762000
*   RFY                                                            0280 00763000
*     FRRS BASED(R7);               /* ADDRESSABILITY TO FRR STACK   */ 00764000
*   RFY                                                            0281 00765000
*     SDWA BASED(RT1WRTCA);         /* ADDRESSABILITY TO SDWA        */ 00766000
*   RFY                                                            0282 00767000
*     PSW BASED(R2);                /* POINTER TO 1ST HALF OF PSW    */ 00768000
*   RFY                                                            0283 00769000
*     PSWEXT1 BASED(R3);            /* POINTER TO 2ND HALF OF PSW  0283 00770000
*                                                            @YM04995*/ 00771000
*/* ******************************************************************/ 00772000
*/* THIS SEGMENT BEGINS THE INITIALIZATION OF THE SDWA THAT WILL     */ 00773000
*/* DESCRIBE THE IEAVTRTS FAILURE                                    */ 00774000
*/* IT IS PART OF THE SRMDRCOV DEFAULT RECOVERY LPRR FOR IEAVTRTS    */ 00775000
*/* INPUTS---                                                        */ 00776000
*/*  -RT1WRTCA FIELD OF THE RTM1 WORK AREA PORTION OF THE INTERRUPTED*/ 00777000
*/*  - STAKPTR REGISTER(R7) POINTS TO THE INTERRUPTED FRR STACK      */ 00778000
*/*  - FLAGSCC REGISTER(R1) CONTAINS THE COMPLETION CODE OF THE ERROR*/ 00779000
*/*  - PSWPTR1 REGISTER(R2) POINTS TO THE 1ST HALF OF THE ERROR PSW  */ 00780000
*/*  - PSWPTR2 REGISTER(R3) POINTS TO THE 2ND HALF OF THE ERROR PSW  */ 00781000
*/*  - DUMPOPTS REGISTER(R5) POINTS TO THE SNAP PARM LIST DESCRIBING */ 00782000
*/*    THE DUMPOPTIONS IF ANY FOR THE IEAVTRTS ERROR PROVIDED THAT   */ 00783000
*/*    THE ERROR IS NOT A MACHINE CHECK                              */ 00784000
*/*  - EEDCHAIN REGISTER(R6) POINTS TO A COMPLETE DESCRIPTION OF     */ 00785000
*/*    THE ERROR IF IT IS A MACHINE CHECK                            */ 00786000
*/*  - REPRDATA REGISTER(R5) CONTAINS ABBREVIATED MACHINE CHECK      */ 00787000
*/*    INFORMATION FOR THE ERROR IN IEAVTRTS IF A COMPLETE           */ 00788000
*/*    DESCRIPTION OF THE IEAVTRTS ERROR COULD NOT BE OBTAINED       */ 00789000
*/*  - SAVAREAP REGISTER(R13) POINTS TO THE REGISTERS AT THE TIME    */ 00790000
*/*    OF THE IEAVTRTS FAILURE                                       */ 00791000
*/* OUTPUT---                                                        */ 00792000
*/*  - A COMPLETED SDWA DESCRIBING THE ERROR IN IEAVTRTS             */ 00793000
*/* ******************************************************************/ 00794000
*                                                                  0284 00795000
*   /*****************************************************************/ 00796000
*   /*                                                               */ 00797000
*   /* ****ZERO THE RTCA*****                                        */ 00798000
*   /*                                                               */ 00799000
*   /*****************************************************************/ 00800000
*                                                                  0284 00801000
*   DO;                                                            0284 00802000
*     SDWA(1:256)=''B;              /* ZERO 1ST 256 BYTES            */ 00803000
         L     @15,RT1WRTCA(,R7)                                   0285 00804000
         XC    SDWA(256,@15),SDWA(@15)                             0285 00805000
*     SDWA(257:LENGTH(SDWA))=''B;   /* ZERO REMAINING BYTES          */ 00806000
         XC    SDWA+256(256,@15),SDWA+256(@15)                     0286 00807000
*     IF RT1WSRBR^=NULL             /* IF A SRB SDWA WAS             */ 00808000
*         &RT1WRTCA=RT1WSRBR THEN   /* SUCCESSFULLY GETMAINED TO   0287 00809000
*                                      PROCESS THE SRB ERROR THEN  0287 00810000
*                                      FINISH THE INITIALIZATION OF     00811000
*                                      OF THE SRB SDWA IF IT IS THE     00812000
*                                      CURRENTLY ACTIVE SDWA AT THE     00813000
*                                      TIME OF THE ERROR             */ 00814000
         L     @11,RT1WSRBR(,R7)                                   0287 00815000
         LTR   @11,@11                                             0287 00816000
         BZ    @RF00287                                            0287 00817000
         CR    @15,@11                                             0287 00818000
         BNE   @RF00287                                            0287 00819000
*       SDWAIDNT=SQARTCA;           /* INITIALIZE THE LENGTH AND   0288 00820000
*                                      SUBPOOL ID OF THE GETMAINED 0288 00821000
*                                      SRB SDWA                      */ 00822000
         L     @11,SQARTCA                                         0288 00823000
         ST    @11,SDWAIDNT(,@15)                                  0288 00824000
*     ELSE                                                         0289 00825000
*       ;                           /* IF THE ALLOCATED SDWA IS IS 0289 00826000
*                                      NOT A GETMAINED SRB SDWA THEN    00827000
*                                      NO SUBPOOL ID AND LENGTH NEED    00828000
*                                      BE INITIALIZED                */ 00829000
@RF00287 DS    0H                                                  0290 00830000
*   END;                                                           0290 00831000
*   SDWAABCC=R1;                    /* SET THE COMPLETION CODE       */ 00832000
         L     @15,RT1WRTCA(,R7)                                   0291 00833000
         ST    R1,SDWAABCC(,@15)                                   0291 00834000
*   SDWADPLA=ADDR(SDWASNPA);        /* SET SNAP PARM LIST ADDRESS    */ 00835000
         LA    @11,SDWASNPA(,@15)                                  0292 00836000
         ST    @11,SDWADPLA(,@15)                                  0292 00837000
*   SDWARECA=ADDR(SDWARA);          /* SET ADDRESS TO VARIABLE     0293 00838000
*                                      RECORDING AREA                */ 00839000
         LA    @11,SDWARA(,@15)                                    0293 00840000
         ST    @11,SDWARECA(,@15)                                  0293 00841000
*   SDWARCRD=ON;                    /* INDICATE RECORDING OF THE SDWA*/ 00842000
         OI    SDWARCRD(@15),B'10000000'                           0294 00843000
*   SDWAVRAL=LENGTH(SDWAVRA);       /* INITIALIZE LENGTH OF VARIABLE    00844000
*                                      RECORDING AREA IN SDWA        */ 00845000
         MVC   SDWAVRAL(2,@15),@CH01866                            0295 00846000
*/* ******************************************************************/ 00847000
*/* THIS SEGMENT(PART OF SRMDRCOV-IEAVTRTS DEFAULT RECOVERY LPRR)    */ 00848000
*/* INITIALIZES THE SDWA WITH THE ERROR TYPE,REGS AND PSW, AND       */ 00849000
*/* ERRORID AT THE TIME OF THE IEAVTRTS FAILURE.             @G17EP5W*/ 00850000
*/* INPUTS---                                                        */ 00851000
*/*  - STAKPTR REGISTER(R7) POINTS TO THE INTERRUPTED FRR STACK THAT */ 00852000
*/*    WAS CURRENT AT THE TIME OF THE IEAVTRTS FAILURE               */ 00853000
*/*  - THE RTM1 WORK AREA PORTION OF THE INTERRUPTED FRR STACK       */ 00854000
*/*  - RT1TENPT FILED OF THE RTM1 WORK AREA CONTAINS THE ERROR TYPE  */ 00855000
*/*  - PSWPTR1 REGISTER(R2) POINTS TO THE 1ST HALF OF THE ERROR PSW  */ 00856000
*/*  - PSWPTR2 REGISTER(R3) POINTS TO THE 2ND HALF OF THE ERROR PSW  */ 00857000
*/*  - SAVAREAP REGISTER(R13) POINTS TO THE REGS AT TIME OF ERROR    */ 00858000
*/*  - RT1WRTCA FIELD IN THE RTM1 WORK AREA POINTS TO THE SDWA       */ 00859000
*/* OUTPUT---                                                        */ 00860000
*/*  - THE SDWA IS INITIALIZED WITH THE ERROR TYPE,ERROR PSW, AND    */ 00861000
*/*    ERROR REGS AT THE TIME OF THE IEAVTRTS FAILURE                */ 00862000
*/* NOTES---                                                         */ 00863000
*/*  - REGS(R1,R2,R3,R5,R7,R9,R10,R13) RESTRICTED                    */ 00864000
*/* ******************************************************************/ 00865000
*                                                                  0296 00866000
*   IF RT1WPCFL^=RT1TENPT THEN      /* IF ERROR IS NOT PROGCK        */ 00867000
         LA    @11,16                                              0296 00868000
         ALR   @11,R7                                              0296 00869000
         CLI   RT1TENPT(@11),1                                     0296 00870000
         BE    @RF00296                                            0296 00871000
*     IF RT1WREST^=RT1TENPT THEN    /* AND NOT RESTART AND NOT       */ 00872000
         CLI   RT1TENPT(@11),2                                     0297 00873000
         BE    @RF00297                                            0297 00874000
*       IF RT1WSVC^=RT1TENPT THEN   /* AN SVC ISSUED BY NON-TASK     */ 00875000
         CLI   RT1TENPT(@11),3                                     0298 00876000
         BE    @RF00298                                            0298 00877000
*         IF RT1WDAT^=RT1TENPT THEN /* MODE RTN AND NOT DAT          */ 00878000
         CLI   RT1TENPT(@11),4                                     0299 00879000
         BE    @RF00299                                            0299 00880000
*           IF RT1WMCHR=RT1TENPT THEN/* ERROR BUT IS MACHINE         */ 00881000
         CLI   RT1TENPT(@11),5                                     0300 00882000
         BNE   @RF00300                                            0300 00883000
*             DO;                   /* CHECK THEN DO FOLLOWING       */ 00884000
*               SDWAMCHK=ON;        /* INDICATE MACHK IN SDWA        */ 00885000
         OI    SDWAMCHK(@15),B'10000000'                           0302 00886000
*               IF R6>EEDNULL THEN  /* IF HARWARE DATA IS            */ 00887000
         C     R6,@CF00079                                         0303 00888000
         BNH   @RF00303                                            0303 00889000
*                 DO;               /* AVAILABE DO FOLLOWING         */ 00890000
*                   SDWAMCH=HRDWDATA;/* SET MCH HARDWARE DATA IN SDWA*/ 00891000
         MVC   SDWAMCH(28,@15),HRDWDATA(R5)                        0305 00892000
*                 END;                                             0306 00893000
*               ELSE                /* OTHERWISE ABBREVIATED MCH     */ 00894000
*                 DO;                                              0307 00895000
         B     @RC00303                                            0307 00896000
@RF00303 DS    0H                                                  0308 00897000
*                   ABBMACH=R5;     /* DATA IS IN REPRDATA REG AND IS   00898000
*                                      PLACED IN SDWA                */ 00899000
         L     @15,RT1WRTCA(,R7)                                   0308 00900000
         ST    R5,ABBMACH(,@15)                                    0308 00901000
*                   SDWAMCIV=ON;    /* INDICATE IN SDWA THAT MACH  0309 00902000
*                                      CHECK DATA IN UNAVAILABLE     */ 00903000
         OI    SDWAMCIV(@15),B'00000010'                           0309 00904000
*                 END;                                             0310 00905000
*               R5=NULL;            /* ZERO REPRDATA                 */ 00906000
@RC00303 SLR   R5,R5                                               0311 00907000
*             END;                                                 0312 00908000
*           ELSE                                                   0313 00909000
*             ;                     /* OTHERWISE THE SDWAERRA FIELD     00910000
*                                      OF THE SDWA IS NOT SET A    0313 00911000
*                                      CANNOT OCURR SITUATION        */ 00912000
@RF00300 DS    0H                                                  0314 00913000
*         ELSE                      /* OTHERWISE A DAT ERROR IS      */ 00914000
*           DO;                                                    0314 00915000
         B     @RC00299                                            0314 00916000
@RF00299 DS    0H                                                  0315 00917000
*             SDWATEXC=ON;          /* INDICATED IN THE SDWA         */ 00918000
         L     @15,RT1WRTCA(,R7)                                   0315 00919000
         OI    SDWATEXC(@15),B'00000010'                           0315 00920000
*             SDWACLUP=ON;          /* INDICATE THAT CLEANUP AND   0316 00921000
*                                      PERCOLATION IS ALL THAT IS  0316 00922000
*                                      ALLOWED BY FRRS FOR THIS ERROR   00923000
*                                      NOT RETRY                     */ 00924000
         OI    SDWACLUP(@15),B'10000000'                           0316 00925000
*             RT1WCLUP=ON;          /* PRESERVED THE CLEANUP AND   0317 00926000
*                                      PERCOLATE INDICATION IN THE 0317 00927000
*                                      RTM1 WORK AREA                */ 00928000
         OI    RT1WCLUP(R7),B'10000000'                            0317 00929000
*             IF ASCBASID=NULL THEN /* IF THE CURRENT ASID IS ZERO 0318 00930000
*                                      (I.E.CURRENT ASCB WAS       0318 00931000
*                                      OVERLAID) INSURE THAT FRRS DO    00932000
*                                      NOT CONSIDER THAT THE CURRENT    00933000
*                                      MEMORY IS VALID AND HAS NOT 0318 00934000
*                                      FAILED                        */ 00935000
         L     @11,PSAAOLD                                         0318 00936000
         LH    @11,ASCBASID(,@11)                                  0318 00937000
         N     @11,@CF03408                                        0318 00938000
         LTR   @11,@11                                             0318 00939000
         BNZ   @RF00318                                            0318 00940000
*               SDWAFMID=INVLASID;  /* INDICATE A FAILING INVALID  0319 00941000
*                                      MEMORY IN THE SDWA            */ 00942000
         MVC   SDWAFMID(2,@15),@CB02840                            0319 00943000
*             ELSE                  /* OTHERWISE ASSUME THAT THE   0320 00944000
*                                      CURRENT ASCB CONTAINS A VALID    00945000
*                                      ASID                          */ 00946000
*               SDWAFMID=ASCBASID;  /* USE THE CURRENT ASID          */ 00947000
         B     @RC00318                                            0320 00948000
@RF00318 L     @15,RT1WRTCA(,R7)                                   0320 00949000
         L     @11,PSAAOLD                                         0320 00950000
         MVC   SDWAFMID(2,@15),ASCBASID(@11)                       0320 00951000
*           END;                                                   0321 00952000
*       ELSE                        /* OTHERWISE A NON TASK MODE RTN */ 00953000
*         SDWASVCE=ON;              /* ISSUED A SVC ERROR INDICATION    00954000
*                                      IS MADE IN THE SDWA           */ 00955000
         B     @RC00298                                            0322 00956000
@RF00298 L     @15,RT1WRTCA(,R7)                                   0322 00957000
         OI    SDWASVCE(@15),B'00000100'                           0322 00958000
*     ELSE                          /* OTHERWISE A RESTART ERROR     */ 00959000
*       DO;                                                        0323 00960000
         B     @RC00297                                            0323 00961000
@RF00297 DS    0H                                                  0324 00962000
*         SDWARKEY=ON;              /* INDICATION IS MADE IN THE SDWA*/ 00963000
         L     @15,RT1WRTCA(,R7)                                   0324 00964000
         OI    SDWARKEY(@15),B'00100000'                           0324 00965000
*         SDWACLUP=ON;              /* INDICATE THAT ONLY CLEANUP AND   00966000
*                                      PERCOLATION IS ALLOWED BY FRRS   00967000
*                                      FOR THIS ERROR- NOT RETRY     */ 00968000
         OI    SDWACLUP(@15),B'10000000'                           0325 00969000
*         RT1WCLUP=ON;              /* PRESERVED THE CLEANUP AND   0326 00970000
*                                      PERCOLATE INDICATION IN THE 0326 00971000
*                                      RTM1 WORK AREA FOR THIS ERROR */ 00972000
         OI    RT1WCLUP(R7),B'10000000'                            0326 00973000
*       END;                                                       0327 00974000
*   ELSE                            /* OTHERWISE A PROGRAM CHK ERROR */ 00975000
*     SDWAPCHK=ON;                  /* INDICATION IS MADE IN SDWA    */ 00976000
*                                                                  0328 00977000
         B     @RC00296                                            0328 00978000
@RF00296 L     @15,RT1WRTCA(,R7)                                   0328 00979000
         OI    SDWAPCHK(@15),B'01000000'                           0328 00980000
*   /*****************************************************************/ 00981000
*   /*                                                               */ 00982000
*   /* THIS SEGMENT INITIALIZES THE REGISTERS AND PSA AT THE TIME OF */ 00983000
*   /* THE ERROR IN THE ALLOCATED SDWA INPUT - STAKPTR REGISTER(R7)  */ 00984000
*   /* POINTS TO THE INTERRUPTED FRR STACK - RT1WRTCA FIELD OF THE   */ 00985000
*   /* RTM1WORK AREA PORTION OF THE INTERRUPTED STACK POINTS TO THE  */ 00986000
*   /* ALLOCATED SDWA THAT IS TO BE INITIALIZED - PSWPTR1,PSWPTR2    */ 00987000
*   /* REGISTERS(R2,R3,RESP) POINT TO RESPECTIVE HALFS(8 BYTES) OF   */ 00988000
*   /* THE PSW AT TIME OF ERROR - SAVAREAP REGISTER(R13) POINTS TO   */ 00989000
*   /* THE REGISTERS SAVED AT THE TIME OF THE ERROR OUTPUT -         */ 00990000
*   /* SDWAGRSV,SDWASRSV FIELDS IN THE ALLOCATED SDWA CONTAIN THE    */ 00991000
*   /* REGISTER CONTENTS AT TIME OF ERROR -                          */ 00992000
*   /* SDWAEC1,SDWAAEC1,SDWAEC2,SDWAAEC2 FIELDS IN THE SDWA CONTAINS */ 00993000
*   /* THE PSW AT TIME OF THE ERROR NOTES - REGS(R0,R2,R5,R7 )MUST   */ 00994000
*   /* REMAIN UNCHANGED THROUGHOUT THIS SEGMENT - REGISTER R13 STILL */ 00995000
*   /* POINTS TO THE REGS AT TIME OF ERROR                           */ 00996000
*   /*                                                               */ 00997000
*   /*****************************************************************/ 00998000
*                                                                  0329 00999000
*   DO;                                                            0329 01000000
@RC00296 DS    0H                                                  0330 01001000
*     SDWAGRSV=ERREGS;              /* INITIALIZE THE ERROR REGS IN     01002000
*                                      THE SDWA                      */ 01003000
         L     @15,RT1WRTCA(,R7)                                   0330 01004000
         MVC   SDWAGRSV(64,@15),ERREGS(R13)                        0330 01005000
*     SDWASRSV=ERREGS;              /* INITIALIZE THE ERROR REGS IN     01006000
*                                      THE SDWA                      */ 01007000
         MVC   SDWASRSV(64,@15),ERREGS(R13)                        0331 01008000
*     SDWAEC1=PSW;                  /* INITIALIZE 1ST HALF OF ERROR     01009000
*                                      PSW IN SDWA                   */ 01010000
         MVC   SDWAEC1(8,@15),PSW(R2)                              0332 01011000
*     SDWAEC2=PSW;                  /* INITIALIZE 1ST HALF OF ERROR     01012000
*                                      PSW IN SDWA                   */ 01013000
         MVC   SDWAEC2(8,@15),PSW(R2)                              0333 01014000
*     IF SDWAPCHK^=ON&SDWATEXC^=ON THEN/* IF THE ERROR IS NOT A    0334 01015000
*                                      PROGRAM INTERRUPTION THEN   0334 01016000
*                                      INITIALIZE THE PSW AS FOLLOWS    01017000
*                                                            @YM06390*/ 01018000
         TM    SDWAPCHK(@15),B'01000010'                           0334 01019000
         BNZ   @RF00334                                            0334 01020000
*       DO;                         /*                       @YM06390*/ 01021000
*         IF R3^=NULL THEN          /* IF THE ILC AND IC OF THE ERROR   01022000
*                                      PSW IS AVAILABLE THEN       0336 01023000
*                                      INITIALIZE IT         @YM06390*/ 01024000
         LTR   R3,R3                                               0336 01025000
         BZ    @RF00336                                            0336 01026000
*           DO;                     /*                       @YM06390*/ 01027000
*             ECPEXT1=PSWILCIC;     /* SET ILC AND IC        @YM06390*/ 01028000
         MVC   ECPEXT1(4,@15),PSWILCIC(R3)                         0338 01029000
*             ECPEXT2=PSWILCIC;     /* SET ILC AND IC        @YM06390*/ 01030000
         MVC   ECPEXT2(4,@15),PSWILCIC(R3)                         0339 01031000
*           END;                    /*                       @YM06390*/ 01032000
*         ELSE                                                     0341 01033000
*           ;                       /* IF THE ILC AND IC ARE NOT   0341 01034000
*                                      AVAILABLE THEN JUST INITIALIZE   01035000
*                                      THE TRANS ADDR        @YM06390*/ 01036000
@RF00336 DS    0H                                                  0342 01037000
*         DO;                       /*                       @YM06390*/ 01038000
*           SDWATRAN=LCCAPVAD;      /* SET TRANS ADDR        @YM06390*/ 01039000
         L     @15,RT1WRTCA(,R7)                                   0343 01040000
         L     @11,PSALCCAV                                        0343 01041000
         L     @11,LCCAPVAD(,@11)                                  0343 01042000
         ST    @11,SDWATRAN(,@15)                                  0343 01043000
*           SDWATRN2=LCCAPVAD;      /* SET TRANS ADDR        @YM06390*/ 01044000
         ST    @11,SDWATRN2(,@15)                                  0344 01045000
*         END;                      /*                       @YM06390*/ 01046000
*       END;                        /*                       @YM06390*/ 01047000
*     ELSE                          /* IF THE ERROR IS A PROGRAM   0347 01048000
*                                      INTERRUPTION THE ALL PSW DATA    01049000
*                                      IS AVAILABLE          @YM06390*/ 01050000
*       DO;                         /*                       @YM06390*/ 01051000
         B     @RC00334                                            0347 01052000
@RF00334 DS    0H                                                  0348 01053000
*         SDWAAEC2=PSWEXT1;         /* INITIALIZE 2ND HALF OF ERROR     01054000
*                                      PSW IN SDWA                   */ 01055000
         L     @15,RT1WRTCA(,R7)                                   0348 01056000
         MVC   SDWAAEC2(8,@15),PSWEXT1(R3)                         0348 01057000
*         SDWAAEC1=PSWEXT1;         /* INITIALIZE 2ND HALF OF ERROR     01058000
*                                      PSW IN SDWA                   */ 01059000
         MVC   SDWAAEC1(8,@15),PSWEXT1(R3)                         0349 01060000
*       END;                        /*                       @YM06390*/ 01061000
*   END;                                                           0351 01062000
*                                                                  0351 01063000
@RC00334 DS    0H                                                  0352 01064000
*/********************************************************************/ 01065000
*/*                                                                  */ 01066000
*/* THIS SEGMENT PUTS THE ERRORID INTO THE SDWA. IF SDWAMCH=HRDWDATA */ 01067000
*/* THEN HARDWARE DATA IS AVAILABLE IN THE EED'S.  THE EEDCHAIN IS   */ 01068000
*/* SEARCHED AND IF AN ERRORID IS FOUND, IT IS MOVED INTO THE SDWA.  */ 01069000
*/* OTHERWISE, A NEW ERRORID IS GENERATED AND SDWACPUI, SDWAERTM, AND*/ 01070000
*/* SDWASEQ# ARE SET.                                                */ 01071000
*/* INPUTS--                                                         */ 01072000
*/*   - RTM1 WORKAREA PORTION OF THE INTERRUPTED STACK.              */ 01073000
*/*   - RT1WRTCA FIELD POINTS TO THE SDWA.                           */ 01074000
*/*   - RTCTSEQ# IN THE RTCA CONTAINS THE CURRENT SEQUENCE NUMBER.   */ 01075000
*/* OUTPUTS--                                                        */ 01076000
*/*   - THE SDWA IS INITIALIZED WITH ERRORID INFORMATION.            */ 01077000
*/*                                                                  */ 01078000
*/********************************************************************/ 01079000
*                                                                  0352 01080000
*   DO;                             /* PUT ERRORID IN SDWA   @G17EP5W*/ 01081000
*     STM(R0,R2,SDWARCPL);          /* SAVE REGS TO BE USED FOR    0353 01082000
*                                      ERRORID PROCESSING    @G17EP5W*/ 01083000
         L     @15,RT1WRTCA(,R7)                                   0353 01084000
         STM   R0,R2,SDWARCPL(@15)                                 0353 01085000
*     RFY                                                          0354 01086000
*      (R0,                                                        0354 01087000
*       R1,                                                        0354 01088000
*       R2) RSTD;                   /* REGS USED BY MACROS   @G17EP5W*/ 01089000
*     IF SDWAMCHK=ON THEN           /* IF THIS IS A HARD ERROR     0355 01090000
*                                                            @G17EP5W*/ 01091000
         TM    SDWAMCHK(@15),B'10000000'                           0355 01092000
         BNO   @RF00355                                            0355 01093000
*       DO;                         /* LOOK FOR ERRORID IN EED     0356 01094000
*                                                            @G17EP5W*/ 01095000
*         R2=R6;                    /* MOVE 1ST EED PTR TO REG     0357 01096000
*                                                            @G19EP5W*/ 01097000
         LR    R2,R6                                               0357 01098000
*         DO WHILE(R2>EEDNULL);     /* SEARCH THRU EED CHAIN @G17EP5W*/ 01099000
         B     @DE00358                                            0358 01100000
@DL00358 DS    0H                                                  0359 01101000
*           IF R2->EEDERFL=ON THEN  /* IF ERRORID IN EED     @G17EP5W*/ 01102000
         TM    EEDERFL(R2),B'10000000'                             0359 01103000
         BNO   @RF00359                                            0359 01104000
*             DO;                   /* MOVE IT TO SDWA       @G17EP5W*/ 01105000
*               SDWAERTM=R2->EEDETIME;/* MOVE IN TIME STAMP  @G17EP5W*/ 01106000
         L     @15,RT1WRTCA(,R7)                                   0361 01107000
         MVC   SDWAERTM(4,@15),EEDETIME(R2)                        0361 01108000
*               SDWASEQ#=R2->EEDESEQ#;/* MOVE IN SEQUENCE #  @G17EP5W*/ 01109000
         MVC   SDWASEQ#(2,@15),EEDESEQ#(R2)                        0362 01110000
*               SDWACPUI=R2->EEDECPUI;/* MOVE IN CPU ID      @G17EP5W*/ 01111000
         LH    @11,EEDECPUI(,R2)                                   0363 01112000
         STC   @11,SDWACPUI(,@15)                                  0363 01113000
*               SDWAERFL=ON;        /* ERRORID IN SDWA       @G17EP5W*/ 01114000
         OI    SDWAERFL(@15),B'00000001'                           0364 01115000
*             END;                  /* END MOVE ERRORID INFO @G17EP5W*/ 01116000
*           R2=R2->EEDFWRDP;        /* GET NEXT POINTER      @G17EP5W*/ 01117000
@RF00359 L     R2,EEDFWRDP(,R2)                                    0366 01118000
*         END;                      /* END ERRORID SEARCH    @G17EP5W*/ 01119000
@DE00358 C     R2,@CF00079                                         0367 01120000
         BH    @DL00358                                            0367 01121000
*       END;                        /* END HARD ERROR ERRORID      0368 01122000
*                                                            @G17EP5W*/ 01123000
*     IF SDWAERFL=OFF THEN          /* IF NO ERRORID,GENERATE      0369 01124000
*                                                            @G17EP5W*/ 01125000
@RF00355 L     @15,RT1WRTCA(,R7)                                   0369 01126000
         TM    SDWAERFL(@15),B'00000001'                           0369 01127000
         BNZ   @RF00369                                            0369 01128000
*       DO;                         /* A NEW ERRORID         @G17EP5W*/ 01129000
*         R2=ADDR(SDWARCPL)+LENGTH(REGSTOR);/* R2 POINTS TO AN 8 BYTE   01130000
*                                      AREA TO BE USED TO STORE THE     01131000
*                                      TOD CLOCK VALUE       @G17EP5W*/ 01132000
         LA    R2,SDWARCPL(,@15)                                   0371 01133000
         AL    R2,@CF00191                                         0371 01134000
*         GEN;                                                     0372 01135000
*                                  /*INVOKE CONVERT TIME MAC @G17EP5W*/ 01136000
       CONTIME TIMER=(2)           /*SPECIFY ADDR FOR TOD    @G17EP5W*/ 01137000
*         SDWAERTM=R1;              /* PUT TIMESTAMP IN SDWA @G17EP5W*/ 01138000
         L     @15,RT1WRTCA(,R7)                                   0373 01139000
         ST    R1,SDWAERTM(,@15)                                   0373 01140000
*         RFY                                                      0374 01141000
*          (R8) RSTD;               /* REG FOR SEQ ADDR      @G17EP5W*/ 01142000
*         R8=ADDR(SDWASEQ#);        /* SEQ # ADDR            @G17EP5W*/ 01143000
         LA    R8,SDWASEQ#(,@15)                                   0375 01144000
*         GEN;                                                     0376 01145000
*                                  /* GET SEQ# FOR ERRORID   @G17EP5W*/ 01146000
        GETSEQ (8)                 /* SAVE VALUE IN SDWA     @G17EP5W*/ 01147000
*         SDWACPUI=PSACPULA;        /* LOGICAL CPU ID IN SDWA      0377 01148000
*                                                            @G17EP5W*/ 01149000
