*           R1=R12;                 /* RESTORE R1 - RTCA ADDRESS     */ 02003000
         LR    R1,R12                                              0578 02004000
*           RFY                                                    0579 02005000
*            (R12,                                                 0579 02006000
*             R6) UNRSTD;           /* RELEASE REGS                  */ 02007000
*         END;                      /* END OF FREECELL PROCESSING    */ 02008000
*       DO;                         /* SETRP RECORD(YES)RECPARM(RESCH   02009000
*                                      ID)RC(0)                      */ 02010000
@RF00569 DS    0H                                                  0582 02011000
*         RESPECIFY                                                0582 02012000
*          (GPR00P,                                                0582 02013000
*           GPR01P,                                                0582 02014000
*           GPR14P,                                                0582 02015000
*           GPR15P) RSTD;                                          0582 02016000
*         GPR01P->SDWARCDE=0;       /* SAVE RC VALUE                 */ 02017000
         MVI   SDWARCDE(GPR01P),X'00'                              0583 02018000
*         GPR01P->SDWARCRD='1'B;    /* TURN ON RECORD INDICATOR      */ 02019000
         OI    SDWARCRD(GPR01P),B'10000000'                        0584 02020000
*         GPR15P=ADDR(RESCHID);     /* ACCESS RECORD PARAMETER LIST  */ 02021000
         LA    GPR15P,RESCHID                                      0585 02022000
*         GPR01P->SDWARECP=GPR15P->I256C(1:24);/* COPY RECORD      0586 02023000
*                                      PARAMETERS                    */ 02024000
         MVC   SDWARECP(24,GPR01P),I256C(GPR15P)                   0586 02025000
*         RESPECIFY                                                0587 02026000
*          (GPR00P,                                                0587 02027000
*           GPR01P,                                                0587 02028000
*           GPR14P,                                                0587 02029000
*           GPR15P) UNRSTD;                                        0587 02030000
*       END;                                                       0588 02031000
*     END;                          /* END OF PERCOLATION PROCESSNG  */ 02032000
*   RFY                                                            0590 02033000
*     R14 RSTD;                     /* GET REG TO RETURN TO RTS      */ 02034000
@RC00502 DS    0H                                                  0591 02035000
*   R14=R5;                         /* RESTORE RETURN ADDRESS        */ 02036000
         LR    R14,R5                                              0591 02037000
*   RFY                                                            0592 02038000
*    (R1,                                                          0592 02039000
*     R5,                                                          0592 02040000
*     R7,                                                          0592 02041000
*     R13,                                                         0592 02042000
*     R8) UNRSTD;                   /* FREE UP REGS USED BY FRR      */ 02043000
*   RETURN;                         /* RETURN TO RTS                 */ 02044000
         B     @EL00001                                            0593 02045000
*   RFY                                                            0594 02046000
*     R14 UNRSTD;                   /* RELEASE REG FOR RETURN ADDR -    02047000
*                                      END OF RTMRSFRR SEGMENT       */ 02048000
*RECVRRTM:                                                         0595 02049000
*   ENTRY;                                                         0595 02050000
*                                                                  0595 02051000
@EP00595 DS    0H                                                  0596 02052000
*   /*****************************************************************/ 02053000
*   /*                                                               */ 02054000
*   /* THE PURPOSE OF THE RECVRRTM SEGMENT IS TO PROCESS AN          */ 02055000
*   /* UNANTICIPATED RECURSION INTO RTM1. THIS TAKES PLACE WHEN AN   */ 02056000
*   /* ERROR OCCURS IN A SECTION OF RTM1 NOT PROTECTED BY AN FRR.    */ 02057000
*   /* THIS SEGMENT WILL ROUTE CONTROL TO A LOGICAL PHASE RECOVERY   */ 02058000
*   /* ROUTINE IF ONE IS APPLICABLE TO THE PHASE IN RTM1 AT WHICH THE*/ 02059000
*   /* RECURSIVE ERROR OCCURRED. IF NO LOGICAL PHASE RECOVERY ROUTINE*/ 02060000
*   /* HAS BEEN SPECIFIED, THEN ABORT PROCESSING IS PERFORMED. INPUT */ 02061000
*   /* TO THIS SEGMENT IS AS FOLLOWS: FUNCTION REGISTER (R0) -       */ 02062000
*   /* INDICATES TYPE OF ENTRY INTO RTM1 FOR THE NEW ERROR. STAKPTR  */ 02063000
*   /* REGISTER (R7) - IN SLIH MODE, CONTAINS A PTR TO THE CURRENT   */ 02064000
*   /* STACK. TRACKPTR REGISTER (R12) - IN SLIH MODE, CONTAINS A PTR */ 02065000
*   /* TO THE TRACKING AREA IN THE RTM1WA (WHICH STILL CONTAINS      */ 02066000
*   /* INFORMATION ABOUT THE ORIGINAL ERROR). THE FOLLOWING REGISTERS*/ 02067000
*   /* MAY BE DEFINED - DEPENDING ON THE TYPE OF ENTRY TO RTM1 - AND */ 02068000
*   /* SHOULD BE PRESERVED IF CONTROL IS TO BE RETURNED TO RTM1 FOR  */ 02069000
*   /* CONTINUED PROCESSING OF THE NEW ERROR: R1-R6,R13. RTM1'S BASE */ 02070000
*   /* REGISTER IS R9 AND WOULD ALSO HAVE TO BE RESTORED.            */ 02071000
*   /*                                                               */ 02072000
*   /*****************************************************************/ 02073000
*                                                                  0596 02074000
*   RFY                                                            0596 02075000
*    (R0,                           /* TYPE OF ENTRY INTO RTM1       */ 02076000
*     R1,                           /* PRESERVED FOR CALLER          */ 02077000
*     R2,                           /* PRESERVED FOR CALLER          */ 02078000
*     R3,                           /* PRESERVED FOR CALLER          */ 02079000
*     R4,                           /* PRESERVED FOR CALLER          */ 02080000
*     R5,                           /* PRESERVED FOR CALLER          */ 02081000
*     R6,                           /* PRESERVED FOR CALLER          */ 02082000
*     R7,                           /* PTR TO CURRENT STACK          */ 02083000
*     R12,                          /* PTR TO TRACKING AREA IN WA    */ 02084000
*     R13) RSTD;                    /* PRESERVED FOR CALLER          */ 02085000
*   RFY                                                            0597 02086000
*     FRRS BASED(R7);               /* USE REGISTER BASE FOR CURRENT    02087000
*                                      STACK                         */ 02088000
*   RFY                                                            0598 02089000
*     RT1TRACK BASED(R12);          /* USE REGISTER BASE FOR TRACK 0598 02090000
*                                      AREA IN RTM1WA                */ 02091000
*   IF RT1TLPID^=NULL THEN          /* IF THE LPID IS NOT NULL (I.E.    02092000
*                                      AN LPRR SHOULD BE DEFINED FOR    02093000
*                                      THE LPID)                     */ 02094000
         CLI   RT1TLPID(R12),0                                     0599 02095000
         BE    @RF00599                                            0599 02096000
*     DO;                           /* THEN ROUTE TO THIS LPRR       */ 02097000
*       RFY                                                        0601 02098000
*         R8 RSTD;                  /* GET CONTROL OF REG TO POINT TO   02099000
*                                      THE LPRR                      */ 02100000
*       R8=RT1TLPID;                /* SAVE LPID VALUE IN REG        */ 02101000
         SLR   R8,R8                                               0602 02102000
         IC    R8,RT1TLPID(,R12)                                   0602 02103000
*       RT1TLPID=NULL;              /* SET THE LPID TO INDICATE ABORT   02104000
*                                      PROCESSING                    */ 02105000
         MVI   RT1TLPID(R12),X'00'                                 0603 02106000
*       GO TO LPRRADDR(R8);         /* GO TO THE SPECIFIED LPRR TO 0604 02107000
*                                      COMPLETE RECOVERY PROCESSING     02108000
*                                      AND RETURN TO RTM1            */ 02109000
         LR    @11,R8                                              0604 02110000
         SLA   @11,2                                               0604 02111000
         L     @11,LPRRADDR-4(@11)                                 0604 02112000
         BR    @11                                                 0604 02113000
*       RFY                                                        0605 02114000
*         R8 UNRSTD;                /* RELEASE REG USED TO POINT TO     02115000
*                                      THE LPRR                      */ 02116000
*     END;                          /* END OF LPRR ROUTING           */ 02117000
*   ELSE                            /* IF THE LPID IS NULL           */ 02118000
*                                                                  0607 02119000
*     /***************************************************************/ 02120000
*     /*                                                             */ 02121000
*     /* THE PURPOSE OF THE ABORT SEGMENT IS TO TERMINATE RTM1'S     */ 02122000
*     /* PROCESSING OF AN ERROR IF, DURING SOME VITAL STAGE OF THIS  */ 02123000
*     /* PROCESSING, RTM1 ITSELF SUFFERS AN ERROR. THE TECHNIQUE IS  */ 02124000
*     /* TO ATTEMPT TO REMOVE ALL TRACES OF THE ORIGINAL ERROR       */ 02125000
*     /* SITUATION FROM THE CPU AND TO ENTER THE DISPATCHER. IT      */ 02126000
*     /* SHOULD BE NOTED THAT IF A FURTHER ERROR OCCURS DURING ABORT */ 02127000
*     /* PROCESSING, THE POTENTIAL FOR A CLOSED, DISABLED,           */ 02128000
*     /* UNRECOVERABLE LOOP EXISTS. THIS SEGMENT RECEIVES NO INPUT   */ 02129000
*     /* FROM AND PROVIDES NO OUTPUT TO SUCCEEDING SEGMENTS OF RTM1. */ 02130000
*     /*                                                             */ 02131000
*     /***************************************************************/ 02132000
*                                                                  0607 02133000
*     DO;                           /* START OF ABORT SEGMENT        */ 02134000
@RF00599 DS    0H                                                  0608 02135000
*       RFY                                                        0608 02136000
*         FRRS BASED(PSACSTK);      /* BASE CURRENT STACK ON VALUE OF   02137000
*                                      PSACSTK                       */ 02138000
*       RFY                                                        0609 02139000
*         RT1TRACK BASED(ADDR(RT1WLPTA));/* USE TRACKING AREA IN   0609 02140000
*                                      RTM1WA OF CURRENT STACK       */ 02141000
*       RFY                                                        0610 02142000
*        (R8,                                                      0610 02143000
*         R14,                                                     0610 02144000
*         R15) RSTD;                /* GET REGS FOR USE IN PURGING 0610 02145000
*                                      RECOVERY STACKS               */ 02146000
*       DO R8=SSTAK1 TO DIM(PSARSVT) BY SSTAKINC;/* SET UP LOOP TO 0611 02147000
*                                      PURGE ALL SUPER STACKS        */ 02148000
         LA    R8,3                                                0611 02149000
@DL00611 DS    0H                                                  0612 02150000
*         PSACSTK=PSARSVT(R8);      /* MAKE EACH STACK TO BE PURGED     02151000
*                                      THE CURRENT STACK             */ 02152000
         LR    @11,R8                                              0612 02153000
         SLA   @11,2                                               0612 02154000
         L     @11,PSARSVT-4(@11)                                  0612 02155000
         ST    @11,PSACSTK                                         0612 02156000
*         GEN(SETFRR P,WRKREGS=(R14,R15));/* PURGE THE STAK          */ 02157000
         SETFRR P,WRKREGS=(R14,R15)                                     02158000
*         RT1TLPN=NULL;             /* CLEAR RTM1'S RECURSION      0614 02159000
*                                      INDICATOR IN EACH STACK       */ 02160000
         L     @11,PSACSTK                                         0614 02161000
         MVI   RT1TLPN+16(@11),X'00'                               0614 02162000
*       END;                        /* END OF LOOP TO CLEAN UP SUPER    02163000
*                                      STACKS                        */ 02164000
         AL    R8,@CF00110                                         0615 02165000
         C     R8,@CF00200                                         0615 02166000
         BNH   @DL00611                                            0615 02167000
*       PSACSTK=PSANSTK;            /* MAKE THE NORMAL STAK CURRENT  */ 02168000
         L     @11,PSANSTK                                         0616 02169000
         ST    @11,PSACSTK                                         0616 02170000
*       GEN(SETFRR P,WRKREGS=(R14,R15));/* PURGE THE STACK           */ 02171000
         SETFRR P,WRKREGS=(R14,R15)                                     02172000
*       RT1TLPN=NULL;               /* ZERO RTM1'S RECURSION INDCTR  */ 02173000
         L     @11,PSACSTK                                         0618 02174000
         MVI   RT1TLPN+16(@11),X'00'                               0618 02175000
*       RFY                                                        0619 02176000
*        (R8,                                                      0619 02177000
*         R14,                                                     0619 02178000
*         R15) UNRSTD;              /* RELEASE REGS USED TO PURGE  0619 02179000
*                                      STACKS                        */ 02180000
*       RFY                                                        0620 02181000
*         R15 RSTD;                 /* GET REG FOR SETLOCK TEST RC   */ 02182000
*       GEN(SETLOCK TEST,TYPE=LOCAL);/* TEST IF LOCAL LOCK IS HELD   */ 02183000
         SETLOCK TEST,TYPE=LOCAL                                        02184000
*       IF R15=STLOKNRM THEN        /* IF THE LOCAL LOCK IS HELD     */ 02185000
         LTR   R15,R15                                             0622 02186000
         BNZ   @RF00622                                            0622 02187000
*         ASCBTYP1='0'B;            /* ZERO THE TYPE 1 SVC SWITCH IN    02188000
*                                      THE ASCB                      */ 02189000
         L     @14,PSAAOLD                                         0623 02190000
         NI    ASCBTYP1(@14),B'11111101'                           0623 02191000
*       RFY                                                        0624 02192000
*         R15 UNRSTD;               /* RELEASE REG FOR SETLOCK TEST  */ 02193000
@RF00622 DS    0H                                                  0625 02194000
*       RFY                                                        0625 02195000
*        (R11,                                                     0625 02196000
*         R12,                                                     0625 02197000
*         R13,                                                     0625 02198000
*         R14) RSTD;                /* GET REGS DESTROYED BY   SETLCK*/ 02199000
*       GEN;                                                       0626 02200000
*                                      /*FREE ALL LOCKS ON THIS CPU  */ 02201000
         SETLOCK RELEASE,TYPE=ALL,DISABLED,RELATED=('RTM PROCESSING',  X02202000
               ANYMODUL(ANYLABEL))                                      02203000
*       RFY                                                        0627 02204000
*        (R11,                                                     0627 02205000
*         R12,                                                     0627 02206000
*         R13,                                                     0627 02207000
*         R14) UNRSTD;              /* RELEASE REGS USED BY   SETLOCK*/ 02208000
*       PSAANEW=NULL;               /* ZERO PSAANEW TO FORCE THE   0628 02209000
*                                      DISPATCHER TO RESEARCH HIS  0628 02210000
*                                      QUEUES                        */ 02211000
         SLR   @12,@12                                             0628 02212000
         ST    @12,PSAANEW                                         0628 02213000
*       PSASUPER=NULL;              /* CLEAR ALL SUPER FLAGS         */ 02214000
         ST    @12,PSASUPER                                        0629 02215000
*       RFY                                                        0630 02216000
*         R14 RSTD;                 /* GET REG FOR RETURN ADDRESS    */ 02217000
*       IF LCCASRBM='1'B THEN       /* IF CPU IS IN SRB MODE         */ 02218000
         L     @15,PSALCCAV                                        0631 02219000
         TM    LCCASRBM(@15),B'10000000'                           0631 02220000
         BNO   @RF00631                                            0631 02221000
*         R14=CVTSRBRT;             /* THEN EXIT TO THE SRB DISP     */ 02222000
         L     @12,CVTPTR                                          0632 02223000
         L     R14,CVTSRBRT(,@12)                                  0632 02224000
*       ELSE                        /* OTHERWISE                     */ 02225000
*         R14=CVT0DS;               /* TAKE THE NORMAL DISPATCHER  0633 02226000
*                                      EXIT                          */ 02227000
         B     @RC00631                                            0633 02228000
@RF00631 L     @12,CVTPTR                                          0633 02229000
         L     R14,CVT0DS(,@12)                                    0633 02230000
*       RETURN;                     /* GO TO THE DISPATCHER          */ 02231000
         B     @EL00001                                            0634 02232000
*       RFY                                                        0635 02233000
*         R14 UNRSTD;               /* RELEASE REGISTER              */ 02234000
*       RFY                                                        0636 02235000
*         FRRS BASED(R7);           /* USE REGISTER BASE FOR CURRENT    02236000
*                                      STACK                         */ 02237000
*       RFY                                                        0637 02238000
*         RT1TRACK BASED(R12);      /* USE REGISTER BASE FOR TRACK 0637 02239000
*                                      AREA IN RTM1WA                */ 02240000
*     END;                          /* END OF ABORT SEGMENT          */ 02241000
*                                                                  0638 02242000
*   /*****************************************************************/ 02243000
*   /*                                                               */ 02244000
*   /* THE PURPOSE OF THE LPRECOV1 SEGMENT IS TO PROVIDE LOW LEVEL   */ 02245000
*   /* RECOVERY IF A RECURSIVE ERROR OCCURS WHEN RTM1 HAD BEEN       */ 02246000
*   /* ORIGINALLY ENTERED IN SLIH MODE. RTM1'S PROCESSING, AS COVERED*/ 02247000
*   /* BY THE LOGICAL PHASE RECOVERY ROUTINES, CAN BE DIVIDED INTO   */ 02248000
*   /* SEVERAL SECTIONS: ITS INITIAL PROCESSING OF THE ERROR, SYSTEM */ 02249000
*   /* RECOVERY (PERFORMED BY IEAVTRTS), AND RTM1'S EXIT PROCESSING. */ 02250000
*   /* DIFFERENT RECOVERY SCHEMES WILL BE ATTEMPTED DEPENDING ON THE */ 02251000
*   /* LOGICAL PHASE RTM1 WAS IN WHEN THE SECOND ERROR OCCURRED.     */ 02252000
*   /* INPUT TO THIS SEGMENT IS AS FOLLOWS: FUNCTION REGISTER (R0) - */ 02253000
*   /* INDICATES TYPE OF ENTRY INTO RTM1 FOR THE NEW ERROR. STAKPTR  */ 02254000
*   /* REGISTER (R7) - IN SLIH MODE, CONTAINS A PTR TO THE CURRENT   */ 02255000
*   /* STACK. TRACKPTR REGISTER (R12) - IN SLIH MODE, CONTAINS A PTR */ 02256000
*   /* TO THE TRACKING AREA IN THE RTM1WA (WHICH STILL CONTAINS      */ 02257000
*   /* INFORMATION ABOUT THE ORIGINAL ERROR). THE FOLLOWING REGISTERS*/ 02258000
*   /* MAY BE DEFINED - DEPENDING ON THE TYPE OF ENTRY TO RTM1 - AND */ 02259000
*   /* SHOULD BE PRESERVED IF CONTROL IS TO BE RETURNED TO RTM1 FOR  */ 02260000
*   /* CONTINUED PROCESSING OF THE NEW ERROR: R1-R6,R13. RTM1'S BASE */ 02261000
*   /* REGISTER IS R9 AND WOULD ALSO HAVE TO BE RESTORED.            */ 02262000
*   /*                                                               */ 02263000
*   /*****************************************************************/ 02264000
*                                                                  0639 02265000
*RT1LPRR1:                          /* LOGICAL PHASE RECOVERY ROUTINE   02266000
*                                      1 - REPRESENTS FIRST LEVEL OF    02267000
*                                      RECOVERY BEYOND ABORT       0639 02268000
*                                      PROCESSING                    */ 02269000
*   RFY                                                            0639 02270000
*    (R0,                           /* TYPE OF ENTRY INTO RTM1       */ 02271000
*     R1,                           /* PRESERVED FOR CALLER          */ 02272000
*     R2,                           /* PRESERVED FOR CALLER          */ 02273000
*     R3,                           /* PRESERVED FOR CALLER          */ 02274000
*     R4,                           /* PRESERVED FOR CALLER          */ 02275000
*     R5,                           /* PRESERVED FOR CALLER          */ 02276000
*     R6,                           /* PRESERVED FOR CALLER          */ 02277000
*     R7,                           /* PTR TO CURRENT STACK          */ 02278000
*     R12,                          /* PTR TO TRACKING AREA IN WA    */ 02279000
*     R13) RSTD;                    /* PRESERVED FOR CALLER          */ 02280000
RT1LPRR1 DS    0H                                                  0640 02281000
*   IF RT1TLPN<=RT1SRMHI&           /* IF LPN INDICATES ERROR WAS    */ 02282000
*       RT1TLPN>=RT1SRMLO THEN      /* IN IEAVTRTS                   */ 02283000
         CLI   RT1TLPN(R12),89                                     0640 02284000
         BH    @RF00640                                            0640 02285000
         CLI   RT1TLPN(R12),60                                     0640 02286000
         BL    @RF00640                                            0640 02287000
*/* ******************************************************************/ 02288000
*/* THIS SEGMENT IS A LOGICAL PHASE RECOVERY ROUTINE(LPRR) FOR RTM   */ 02289000
*/* WHICH RECOVERS IEAVTRTS FROM A POSSIBLE FAILURE WHERE NO FRR     */ 02290000
*/* PROTECTION IS PRESENT.  THIS LPRR PERFORMS THE FOLLOWING         */ 02291000
*/* FUNCTIONS.                                                       */ 02292000
*/*  1. ACQUIRES,INITIALIZES AND RECORDS A SDWA THAT DESCRIBES THE   */ 02293000
*/*     IEAVTRTS FAILURE.                                            */ 02294000
*/*  2. RETURNS TO IEAVTRTM AS IEAVTRTS WOULD HAVE RETURNED TO       */ 02295000
*/*     IEAVTRTM(IN ITS EXIT SEGMENT) IF THE CURRENT STACK AT THE    */ 02296000
*/*     TIME OF THE IEAVTRTS FAILURE IS THE NORMAL FRR STACK.        */ 02297000
*/*  3. ABORTS THE LPRR RECOVERY IF THE CURRENT FRR STACK IS ONE OF  */ 02298000
*/*     THE SUPER FRR STACKS WHEN IEAVTRTS FAILED.                   */ 02299000
*/* INPUTS--------                                                   */ 02300000
*/*  - FUNCTION REGISTER(R0) CONTAINS THE ERROR TYPE OF THE IEAVTRTS */ 02301000
*/*    FAILURE                                                       */ 02302000
*/*  - RT1TLPN FIELD OF THE RTM1 WORK AREA PORTION OF THE CURRENT    */ 02303000
*/*    FRR STACK CONTAINS THE LOGICAL PHASE IN IEAVTRTS WHERE THE    */ 02304000
*/*    FAILURE OCCURRED                                              */ 02305000
*/*  - FLAGSCC REGISTER(R1) CONTAINS THE COMPLETION CODE OF THE ERROR*/ 02306000
*/*  - DUMPOPTS REGISTER(R5) CONTAINS THE DUMPOPTS ASSOCIATED WITH   */ 02307000
*/*    THE IEAVTRTS ERROR IF THE ERROR IS NOT A MACHINE CHECK        */ 02308000
*/*  - EEDCHAIN REGISTER(R6) CONTAINS THE POINTER TO COMPLETE        */ 02309000
*/*    MACHINE CHECK INFO IF THE ERROR IS A MACHINE CHECK            */ 02310000
*/*  - REPRDATA REGISTER(R5) CONTAINS ABBREVIATED MACHINE CHECK      */ 02311000
*/*    INFORMATION IF THE ERROR IS A MACHINE CHECK AND COMPLETE DATA */ 02312000
*/*    IS UNAVAILABLE                                                */ 02313000
*/*  - PSWPTR1 REGISTER(R2) POINTS TO THE 1ST HALF OF THE ERROR PSW  */ 02314000
*/*  - PSWPTR2 REGISTER(R3) POINTS TO THE 2ND HALF OF THE ERROR PSW  */ 02315000
*/*  - SAVAREAP REGISTER(R13) POINTS TO THE REGISTERS AT THE TIME    */ 02316000
*/*    OF THE IEAVTRTS FAILURE                                       */ 02317000
*/* OUTPUTS-------                                                   */ 02318000
*/*  - A SDWA DESCRIBING THE IEAVTRTS FAILURE IS RECORDED            */ 02319000
*/* NOTES-----                                                       */ 02320000
*/*  - REGS(R0,R1,R2,R3,R5,R6,R7,R9,10,R13,R4) ARE RESTRICTED        */ 02321000
*/*    THROUGHOUT THIS SEGMENT                                       */ 02322000
*/*  - ALL OTHER REGS ARE VOLATILE                                   */ 02323000
*/* ******************************************************************/ 02324000
*                                                                  0641 02325000
*     DO;                                                          0641 02326000
*       RFY                                                        0642 02327000
*        (R0,                                                      0642 02328000
*         R1,                                                      0642 02329000
*         R5,                       /* REGS THAT MUST BE PRESERVED   */ 02330000
*         R6,                       /* DURING DEFAULT RECOVERY OF    */ 02331000
*         R2,                       /* IEAVTRTS                      */ 02332000
*         R3,                                                      0642 02333000
*         R13,                                                     0642 02334000
*         R4) RSTD;                                                0642 02335000
*       RFY                                                        0643 02336000
*         SDWA BASED(RT1WRTCA);                                    0643 02337000
*       RFY                                                        0644 02338000
*         FRRS BASED(R7);                                          0644 02339000
*       RT1TLPID=NONE;              /* SET UP FOR RTM ABORT        0645 02340000
*                                      PROCESSING IF DEFAULT RECOVERY   02341000
*                                      FOR IEAVTRTS SHOULD FAIL      */ 02342000
         MVI   RT1TLPID(R12),X'00'                                 0645 02343000
*       RT1TENPT=R0;                /* SET THE ERROR TYPE INTO THE 0646 02344000
*                                      RTM1 WORK AREA FOR FUTURE   0646 02345000
*                                      REFERENCE BY LOWER LEVEL    0646 02346000
*                                      SEGMENTS                      */ 02347000
         STC   R0,RT1TENPT(,R12)                                   0646 02348000
*/* ******************************************************************/ 02349000
*/* THIS SEGMENT IS PART OF IEAVTRTS DEFAULT RECOVERY LPRR AND       */ 02350000
*/* ACQUIRES A GLOBAL SDWA ASSOCIATED WITH THE CURRENT FRR STACK     */ 02351000
*/* IEAVTRTS WAS PROCESSING WHEN IT FAILED.  THIS SEGMENT IS USED    */ 02352000
*/* WHEN IEAVTRTS HAS FAILED PRIOR TO ACQUIRING A SDWA AND WHEN NO   */ 02353000
*/* FRR RECOVERY IS PRESENT.  IN THIS CASE, SINCE IEAVTRTS RUNS      */ 02354000
*/* DISABLED A GLOBAL SDWA IS ACQUIRED SO THAT SERIALIZATION IS      */ 02355000
*/* MAINTAINED FOR AN EVENTUAL RECORDING OF THE IEAVTRTS FAILURE.    */ 02356000
*/* INPUT---                                                         */ 02357000
*/*  - PSACSTK-POINTS TO THE INTERRUPTED STACK IEAVTRTS WAS          */ 02358000
*/*    PROCESSING WHEN IT FAILED.                                    */ 02359000
*/*  - PSARSVT---THE ARRAY IN THE PSA CONTAINING ADDRESSES OF ALL    */ 02360000
*/*    FRR STACKS(A GLOBAL SDWA IS ASSOCIATED WITH EACH ONE OF       */ 02361000
*/*    THESE STACKS                                                  */ 02362000
*/* OUTPUT---                                                        */ 02363000
*/*  - RT1WRTCA FIELD OF THE RTM1 WORK AREA PORTION OF THE           */ 02364000
*/*    INTERRUPTED FRR STACK WILL POINT TO THE ACQUIRED GLOBAL       */ 02365000
*/*    SDWA                                                          */ 02366000
*/* NOTES-----                                                       */ 02367000
*/*  - REGS(R11,R4,R0,R1,R2,R3,R5,R6,R7,R9,R10,R13) RESTRICTED       */ 02368000
*/* ******************************************************************/ 02369000
*                                                                  0647 02370000
*       DO;                                                        0647 02371000
*         RFY                                                      0648 02372000
*          (R4,                                                    0648 02373000
*           R11) RSTD;                                             0648 02374000
*         IF PSACSTK=PSANSTK THEN   /* IF THE INTERRUPTED STACK IS 0649 02375000
*                                      THE NORMAL STACK      @YM07602*/ 02376000
         CLC   PSACSTK(4),PSANSTK                                  0649 02377000
         BNE   @RF00649                                            0649 02378000
*           DO;                     /* SEE IF RTM1WA CONTAINS CHECK-    02379000
*                                      POINTED INFORMATION   @YM07602*/ 02380000
*             IF RT1WRTCA=NULL THEN /* AND AN SDWA HAS NOT YET BEEN     02381000
*                                      OBTAINED              @YM07602*/ 02382000
         SLR   @08,@08                                             0651 02383000
         C     @08,RT1WRTCA(,R7)                                   0651 02384000
         BNE   @RF00651                                            0651 02385000
*               DO;                 /* USE THE NORMAL GLOBAL SDWA  0652 02386000
*                                                            @YM07602*/ 02387000
*                 RT1WRTCA=ADDR(NGLBRTCA);/* LOCATE THE GLOBAL SDWA     02388000
*                                      ASSOCIATED WITH THE NORMAL  0653 02389000
*                                      STACK                 @YM07602*/ 02390000
         L     @15,YSTAK+24                                        0653 02391000
         SLA   @15,5                                               0653 02392000
         AL    @15,@CF03393                                        0653 02393000
         AL    @15,PSARSTK                                         0653 02394000
         ST    @15,RT1WRTCA(,R7)                                   0653 02395000
*                 RT1WSRBR=NULL;    /* INSURE SUBSEQUENT PROCESSING     02396000
*                                      REALIZES THE SDWA IS GLOBAL 0654 02397000
*                                                            @YM07602*/ 02398000
         ST    @08,RT1WSRBR(,R7)                                   0654 02399000
*               END;                /* NORMAL GLOBAL SDWA OBTAINED 0655 02400000
*                                                            @YM07602*/ 02401000
*             ELSE                                                 0656 02402000
*               ;                   /* OTHERWISE THE SDWA ALREADY  0656 02403000
*                                      OBTAINED IS USED      @YM07602*/ 02404000
@RF00651 DS    0H                                                  0657 02405000
*             IF RT1WEED>EEDNULL THEN/* IF EEDS WERE ACQUIRED FOR THE   02406000
*                                      ERROR BEING HANDLED BY      0657 02407000
*                                      IEAVTRTS WHEN IT FAILED THEN     02408000
*                                      THEY MUST FIRST BE FREED PRIOR   02409000
*                                      TO RECOVERING THE RTS FAILURE    02410000
*                                                            @YM04995*/ 02411000
         CLC   RT1WEED(4,R7),@CF00079                              0657 02412000
         BNH   @RF00657                                            0657 02413000
*               DO;                 /* FREE THE EEDS         @YM04995*/ 02414000
*                 GENERATE;                                        0659 02415000
*                                      /*SAVE REGS           @YM04995*/ 02416000
         L     R11,RT1WRTCA(0,R7)                         @YM04995      02417000
         STM   R0,R15,SRMREGS+512(R11)                        @YM04995  02418000
*                 R6=RT1WEED;       /* INITIALIZE TO FREE THE OLD  0660 02419000
*                                      EEDS                  @YM04995*/ 02420000
         L     R6,RT1WEED(,R7)                                     0660 02421000
*                 CALL FREEDCEL;    /* FREE THE EEDS         @YM04995*/ 02422000
         L     @15,@CA03293                                        0661 02423000
         BALR  @14,@15                                             0661 02424000
*                 GENERATE;                                        0662 02425000
*                                      /*RESTORE REGS        @YM04995*/ 02426000
         L     R11,RT1WRTCA(0,R7)  FIND THE SDWA          @YM04995      02427000
         LM    R0,R15,SRMREGS+512(R11)  RELOAD THE REGS       @YM04995  02428000
*                 RT1WEED=R6;       /* INSURE THAT THE RTM1WA      0663 02429000
*                                      REFLECTS THE EXISTENCE OF EEDS   02430000
*                                      (IF ANY) FOR THE IEAVTRTS   0663 02431000
*                                      FAILURE               @YM04995*/ 02432000
         ST    R6,RT1WEED(,R7)                                     0663 02433000
*               END;                /* FINISH FREEING EEDS   @YM04995*/ 02434000
*             ELSE                                                 0665 02435000
*               ;                   /* IF THERE ARE NO EEDS EXITING     02436000
*                                      PRIOR TO THE IEAVTRTS FAILURE    02437000
*                                      THEN NO EEDS NEED BE FREED  0665 02438000
*                                                            @YM04995*/ 02439000
@RF00657 DS    0H                                                  0666 02440000
*           END;                    /* END OF NORMAL STACK PROCESSING   02441000
*                                                            @YM07602*/ 02442000
*         ELSE                      /* OTHERWISE A SUPER STACKS SDWA    02443000
*                                      IS TO BE LOCATED              */ 02444000
*           DO;                                                    0667 02445000
         B     @RC00649                                            0667 02446000
@RF00649 DS    0H                                                  0668 02447000
*             R4=THREE;             /* INITIALIZE RSVTIDX FOR      0668 02448000
*                                      FOLLOWING LOOP THRU THE SUPER    02449000
*                                      STACKS DEFINED ON THE RSVT    */ 02450000
         LA    R4,3                                                0668 02451000
*             R11=TWO;              /* INIT STKIDX FOR LOOP ALSO     */ 02452000
         LA    R11,2                                               0669 02453000
*             DO WHILE PSARSVT(R4)^=PSACSTK;/* LOOP THRU THE RSVT TO    02454000
*                                      LOCATE THE CURRENT SUPER STACK*/ 02455000
         B     @DE00670                                            0670 02456000
@DL00670 DS    0H                                                  0671 02457000
*               R11=R11+NRMSTK;     /* UPDATE THE STK INDEX          */ 02458000
         AL    R11,@CF00079                                        0671 02459000
*               R4=TWO*R11-NRMSTK;  /* AND THE RSVT INDEX            */ 02460000
         LR    R4,R11                                              0672 02461000
         ALR   R4,R4                                               0672 02462000
         BCTR  R4,0                                                0672 02463000
*             END;                  /* CONTINUE UNTIL THE CURRENT  0673 02464000
*                                      SUPER STACK IS FOUND          */ 02465000
@DE00670 L     @08,PSACSTK                                         0673 02466000
         LR    @15,R4                                              0673 02467000
         SLA   @15,2                                               0673 02468000
         C     @08,PSARSVT-4(@15)                                  0673 02469000
         BNE   @DL00670                                            0673 02470000
*             RT1WRTCA=ADDR(GLOBRTCA);/* THE GLOBAL SDWA IS ACTIVE   */ 02471000
         LR    @15,R11                                             0674 02472000
         SLA   @15,2                                               0674 02473000
         L     @15,YSTAK-8(@15)                                    0674 02474000
         SLA   @15,5                                               0674 02475000
         AL    @15,@CF01515                                        0674 02476000
         ALR   @08,@15                                             0674 02477000
         ST    @08,RT1WRTCA(,R7)                                   0674 02478000
*             RT1WSRBR=NULL;        /* INSURE THAT SUBSEQUENT      0675 02479000
*                                      PROCESSING REALIZES THE SDWA     02480000
*                                      IS GLOBAL             @YM07602*/ 02481000
         SLR   @08,@08                                             0675 02482000
         ST    @08,RT1WSRBR(,R7)                                   0675 02483000
*           END;                                                   0676 02484000
*         RFY                                                      0677 02485000
*          (R4,                                                    0677 02486000
*           R11) UNRSTD;                                           0677 02487000
@RC00649 DS    0H                                                  0678 02488000
*       END;                                                       0678 02489000
*       CALL SETRTCA;               /* INITIALIZE THE SDWA WITH ERROR   02490000
*                                      INFORMATION ABOUT IEAVTRTS    */ 02491000
         BAL   @14,SETRTCA                                         0679 02492000
*       RFY                                                        0680 02493000
*         R5 RSTD;                                                 0680 02494000
*       R5=RT1WRTCA;                                               0681 02495000
         L     R5,RT1WRTCA(,R7)                                    0681 02496000
*       CALL RCRDRTCA;              /* RECORD THE INITIALIZED SDWA   */ 02497000
         BAL   @14,RCRDRTCA                                        0682 02498000
*       IF PSACSTK=PSANSTK THEN     /* IF RTS FAILED WHILE THE NORMAL   02499000
*                                      STACK WAS CURRENT THEN        */ 02500000
         CLC   PSACSTK(4),PSANSTK                                  0683 02501000
         BNE   @RF00683                                            0683 02502000
*         IF RT1WSRBR^=NULL THEN    /* IF THE SDWA BEING USED WAS  0684 02503000
*                                      GETMAINED THEN EXIT TO RTM  0684 02504000
*                                      WITH THIS SDWA MUST BE WITH NO   02505000
*                                      LOCKS HELD                    */ 02506000
         L     @11,RT1WSRBR(,R7)                                   0684 02507000
         LTR   @11,@11                                             0684 02508000
         BZ    @RF00684                                            0684 02509000
*           DO;                     /* EXIT WITH SRB SDWA AND ALL  0685 02510000
*                                      LOCKS FREED                   */ 02511000
*             RFY                                                  0686 02512000
*               NEWRTCA BASED(RT1WSRBR);                           0686 02513000
*             NEWRTCA(1:256)=SDWA(1:256);                          0687 02514000
         L     @08,RT1WRTCA(,R7)                                   0687 02515000
         MVC   NEWRTCA(256,@11),SDWA(@08)                          0687 02516000
*             NEWRTCA(257:512)=SDWA(257:512);                      0688 02517000
         MVC   NEWRTCA+256(256,@11),SDWA+256(@08)                  0688 02518000
*             RT1WRTCA=RT1WSRBR;                                   0689 02519000
         ST    @11,RT1WRTCA(,R7)                                   0689 02520000
*             GENERATE CODE;                                       0690 02521000
*                                /*RELEASE ALL LOCKS-STAY DISABLED   */ 02522000
         SETLOCK RELEASE,TYPE=ALL,DISABLED,                            C02523000
               RELATED=('EXIT TO RTM IN SRB MODE')                      02524000
*           END;                                                   0691 02525000
*         ELSE                                                     0692 02526000
*           ;                       /* IF THE SDWA BEING USED WAS  0692 02527000
*                                      GETMAINED THEN THE LOCKS    0692 02528000
*                                      CURRENTLY HELD WILL BE      0692 02529000
*                                      RETAINED                      */ 02530000
@RF00684 DS    0H                                                  0693 02531000
*       ELSE                                                       0693 02532000
*         GEN(SVC   13);            /* IF A SUPER STACK WAS CURRENT     02533000
*                                      WHEN RTS FAILED THEN ABORT THE   02534000
*                                      ERROR                         */ 02535000
         B     @RC00683                                            0693 02536000
@RF00683 DS    0H                                                  0693 02537000
         SVC   13                                                       02538000
*       RFY                                                        0694 02539000
*        (R14,                                                     0694 02540000
*         R9) RSTD;                                                0694 02541000
@RC00683 DS    0H                                                  0695 02542000
*       R14=RTMRETAD;               /* SETUP FOR RETURN TO RTM       */ 02543000
         LA    @11,48                                              0695 02544000
         ALR   @11,R7                                              0695 02545000
         L     R14,RTMRETAD(,@11)                                  0695 02546000
*       R9=RTMBASE;                 /* REPRIME RTMS BASE REG         */ 02547000
         L     R9,RTMBASE(,@11)                                    0696 02548000
*       GO TO R14;                  /* RETURN TO RTM WITH SDWA       */ 02549000
         BR    R14                                                 0697 02550000
*     END;                                                         0698 02551000
*   RFY                                                            0699 02552000
*     R4 RSTD;                                                     0699 02553000
@RF00640 DS    0H                                                  0700 02554000
*   IF RT1TLPN^=CPURSTRT|           /* IF LPN INDICATES ORIGINAL     */ 02555000
*       RT1TLPN^=NORSTRT THEN       /* ERROR WAS NOT A RESTART ERROR    02556000
*                                      (IF NOT, RTM1 WILL ATTEMPT TO    02557000
*                                      CONTINUE PROCESSING OF THE  0700 02558000
*                                      ORIGINAL ERROR, BUT IF IT IS,    02559000
*                                      RTM1 WILL PROCESS THE SECOND     02560000
*                                      ERROR)                        */ 02561000
         CLI   RT1TLPN(R12),4                                      0700 02562000
         BNE   @RT00700                                            0700 02563000
         CLI   RT1TLPN(R12),5                                      0700 02564000
         BE    @RF00700                                            0700 02565000
@RT00700 DS    0H                                                  0701 02566000
*     DO;                           /* SEE IF EEDS NEED TO BE FREED     02567000
*                                      BEFORE CONTINUING TO PROCESS     02568000
*                                      THE ORIGINAL ERROR - EEDS   0701 02569000
*                                      MIGHT HAVE BEEN ACQUIRED IF 0701 02570000
*                                      NEW ERROR WAS A MACHCK, FOR 0701 02571000
*                                      EXAMPLE                       */ 02572000
*       RT1WVARI(ONE)=R1;           /* SAVE COMPLETION CODE REG -  0702 02573000
*                                      DESTROYED BY FREEDCEL ROUTN   */ 02574000
         ST    R1,RT1WVARI(,R7)                                    0702 02575000
*       IF R6>EEDNULL THEN          /* IF EEDS NEED TO BE FREED      */ 02576000
         C     R6,@CF00079                                         0703 02577000
         BNH   @RF00703                                            0703 02578000
*         DO;                       /* GO TO FREE THE EEDS           */ 02579000
*           RFY                                                    0705 02580000
*            (R9,                                                  0705 02581000
*             R11) RSTD;                                           0705 02582000
*           R11=R9;                 /* SAVE THE VALUE OF R9 SINCE IT    02583000
*                                      IS DESTROYED BY FREEDCEL      */ 02584000
         LR    R11,R9                                              0706 02585000
*           CALL FREEDCEL;          /* CALL THE FREECELL ROUTINE     */ 02586000
         L     @15,@CA03293                                        0707 02587000
         BALR  @14,@15                                             0707 02588000
*           R9=R11;                 /* RESTORE THE VALUE OF R9       */ 02589000
         LR    R9,R11                                              0708 02590000
*           RFY                                                    0709 02591000
*            (R11,                                                 0709 02592000
*             R9) UNRSTD;                                          0709 02593000
*         END;                                                     0710 02594000
*     END;                          /* END OF FREECELL PROCESSING    */ 02595000
@RF00703 DS    0H                                                  0712 02596000
*   RFY                                                            0712 02597000
*    (R11,                                                         0712 02598000
*     R14) RSTD;                    /* GET REGS USED TO RETURN TO RTM*/ 02599000
@RF00700 DS    0H                                                  0713 02600000
