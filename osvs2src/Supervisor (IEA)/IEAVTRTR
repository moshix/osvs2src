*FREEDCEL:                                                         0465 01604000
*   ENTRY;                                                         0465 01605000
*                                                                  0465 01606000
@EP00465 DS    0H                                                  0466 01607000
*   /*****************************************************************/ 01608000
*   /*                                                               */ 01609000
*   /* THE PURPOSE OF THE FREEDCEL SEGMENT IS TO FREECELL ANY EEDS   */ 01610000
*   /* ACQUIRED DURING RTM1'S PROCESSING THAT ARE NO LONGER NEEDED.  */ 01611000
*   /* THIS ROUTINE IS CALLED FROM SEVERAL PLACES IN RTM1'S          */ 01612000
*   /* PROCESSING. THE VALUES IN REGISTERS R0-R4 ARE DESTROYED BY THE*/ 01613000
*   /* PROCESSING IN THIS ROUTINE. THE VALUES IN REGISTERS R5, R7,   */ 01614000
*   /* R12, AND R13 ARE PRESERVED FOR THE CALLER. REGISTER R8 IS     */ 01615000
*   /* ASSUMED AVAILABLE FOR USE BY THIS ROUTINE. INPUT TO THIS      */ 01616000
*   /* SEGMENT IS THE EEDCHAIN REGISTER (R6). OUTPUT IS EEDS RETURNED*/ 01617000
*   /* TO THEIR QUICK CELL POOL.                                     */ 01618000
*   /*                                                               */ 01619000
*   /*****************************************************************/ 01620000
*                                                                  0466 01621000
*   RFY                                                            0466 01622000
*    (R5,                                                          0466 01623000
*     R7,                                                          0466 01624000
*     R9,                                                          0466 01625000
*     R12,                                                         0466 01626000
*     R13) RSTD;                    /* PRESERVED FOR CALLER          */ 01627000
*   RFY                                                            0467 01628000
*    (R8,                                                          0467 01629000
*     R14) RSTD;                    /* GET REG FOR SAVING RETN ADDR  */ 01630000
*   R8=R14;                         /* SAVE THE RETURN ADDRESS       */ 01631000
         LR    R8,R14                                              0468 01632000
*   RFY                                                            0469 01633000
*     R14 UNRSTD;                   /* RELEASE REGISTER R14          */ 01634000
*   RFY                                                            0470 01635000
*     R1 RSTD;                      /* GET CONTROL OF QUICKCELL PARM    01636000
*                                      REG - FOR CELL ADDRESS        */ 01637000
*   RFY                                                            0471 01638000
*     R0 RSTD;                      /* GET CONTROL OF QUICKCELL PARM    01639000
*                                      REG - FOR CELLPOOL ID         */ 01640000
*   RFY                                                            0472 01641000
*     R6 RSTD;                      /* PTR TO EEDS TO BE FREED       */ 01642000
*   RFY                                                            0473 01643000
*    (R2,                                                          0473 01644000
*     R3,                                                          0473 01645000
*     R4) RSTD;                     /* REGS CLOBBERED BY QUICKCELL   */ 01646000
*   R0=RT1CPOOL;                    /* PUT POOLID IN PARM REGISTER   */ 01647000
*                                                                  0474 01648000
         L     R0,RT1CPOOL                                         0474 01649000
*   /*****************************************************************/ 01650000
*   /*                                                               */ 01651000
*   /* ESTABLISH LOOP TO FREE CHAIN OF EEDS.                         */ 01652000
*   /*                                                               */ 01653000
*   /*****************************************************************/ 01654000
