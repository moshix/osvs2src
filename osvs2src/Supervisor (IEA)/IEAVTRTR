*RTMRSFRR:                                                         0491 01705000
*   ENTRY;                                                         0491 01706000
*                                                                  0491 01707000
@EP00491 DS    0H                                                  0492 01708000
*   /*****************************************************************/ 01709000
*   /*                                                               */ 01710000
*   /* THE PURPOSE OF THE RTMRSFRR SEGMENT IS TO PROVIDE RECOVERY FOR*/ 01711000
*   /* RTM1'S PROCESSING IF A RECURSIVE ERROR HAD OCCURRED DURING AN */ 01712000
*   /* ATTEMPT BY RTM1 TO PERFORM ITS RESCHEDULE FUNCTION. THE       */ 01713000
*   /* RECOVERY SCHEME IS AS FOLLOWS: IN A FEW INSTANCES OF          */ 01714000
*   /* NONCRITICAL RTM1 PROCESSING, NAMELY EED PROCESSING, RETRY WILL*/ 01715000
*   /* BE ATTEMPTED IF A RECURSIVE ERROR OCCURS. OTHERWISE, RTMRSFRR */ 01716000
*   /* WILL SIMPLY ATTEMPT TO CLEAN UP RESOURCES ACQUIRED BY RTM1,   */ 01717000
*   /* SUCH AS EEDS, AND ALSO TO CLEAN UP TRACES OF UNFINISHED RTM1  */ 01718000
*   /* PROCESSING, SUCH AS INCOMPLETE TASK TERMINATION. INPUT TO THIS*/ 01719000
*   /* SEGMENT IS AS FOLLOWS: FRRWAREG REGISTER (R0) - POINTS TO A   */ 01720000
*   /* 200 BYTE WORK AREA AVAILABLE FOR THE FRR'S USE. RTCAPTR       */ 01721000
*   /* REGISTER (R1) - POINTS TO THE SDWA(RTCA) WHICH CONTAINS       */ 01722000
*   /* INFORMATION ABOUT THE ERROR. RETNREG REGISTER (R14) - CONTAINS*/ 01723000
*   /* THE RETURN ADDRESS TO IEAVTRTS, THE SYSTEM RECOVERY MANAGER.  */ 01724000
*   /*                                                               */ 01725000
*   /*****************************************************************/ 01726000
*                                                                  0492 01727000
*   RFY                                                            0492 01728000
*    (R0,                           /* ON ENTRY, PTR TO 200 BYTE WA  */ 01729000
*     R1,                           /* ON ENTRY, PTR TO SDWA (RTCA)  */ 01730000
*     R5,                           /* RETURN ADDR TO RTS - SAVED ON    01731000
*                                      ENTRY                         */ 01732000
*     R7,                           /* PTR TO THIS FRR'S PARM AREA   */ 01733000
*     R13,                          /* PTR TO PREVIOUS FRR'S PARM  0492 01734000
*                                      AREA - PREVIOUS FRR IS THE  0492 01735000
*                                      SLIHFRR IF ORIGINAL ENTRY TO     01736000
*                                      RTM1 HAD BEEN IN SLIH MODE, IT   01737000
*                                      IS RT1'S FRR IF ORIGINAL ENTRY   01738000
*                                      HAD BEEN IN SERVICE ROUTINE 0492 01739000
*                                      MODE.                         */ 01740000
*     R8,                           /* PTR TO 200 BYTE WA -HERE SINCE   01741000
*                                      THE VALUE IN R0 IS DESTROYED  */ 01742000
*     R14) RSTD;                    /* RETURN ADDR TO RTS ON ENTRY   */ 01743000
*   RFY                                                            0493 01744000
*     FRRS BASED(PSACSTK);          /* BASE FRRS ON CURRENT STACK    */ 01745000
