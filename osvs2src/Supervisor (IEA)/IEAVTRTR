*         GPR00P=GPR15P->I015F;     /* ACCESS REGISTER BIT PATTERN   */ 01842000
         LH    GPR00P,I015F(,GPR15P)                               0525 01843000
*         GPR15P=GPR15P+2;          /* CREATE PTR TO USER REG VALUES */ 01844000
         AL    GPR15P,@CF00110                                     0526 01845000
*         GPR14P=ADDR(GPR01P->SDWASRSV);/* CREATE PTR TO SDWA SAVE 0527 01846000
*                                      AREA                          */ 01847000
         LA    GPR14P,SDWASRSV(,GPR01P)                            0527 01848000
*         GEN(SLL   0,16);          /* SHIFT BIT PATTERN TO HIGH   0528 01849000
*                                      ORDER                         */ 01850000
         SLL   0,16                                                     01851000
*R0002:                                                            0529 01852000
*         GEN FLOWS(I0002);                                        0529 01853000
R0002    DS    0H                                                  0529 01854000
         ALR   0,0                    SHIFT BIT TO SIGN POSITION        01855000
         BC    12,I0002               BRANCH IF BIT NOT ON              01856000
         MVC   0(4,14),0(15)          MODIFY REGISTER IN SDWA           01857000
         LA    15,4(15)               UPDATE POINTER                    01858000
*I0002:                                                            0530 01859000
*         GENERATE FLOWS(R0002);                                   0530 01860000
I0002    DS    0H                                                  0530 01861000
         LA    14,4(14)               UPDATE PTR IN SDWA                01862000
         BC    5,R0002                TEST ALR CONDITION CODE TO TEST   01863000
*                                     IF ALL BITS EXHAUSTED             01864000
*         RESPECIFY                                                0531 01865000
*          (GPR00P,                                                0531 01866000
*           GPR01P,                                                0531 01867000
*           GPR14P,                                                0531 01868000
*           GPR15P) UNRSTD;                                        0531 01869000
*       END;                                                       0532 01870000
*       RFY                                                        0533 01871000
*         R2 UNRSTD;                /* RELEASE REG USED FOR INDEX    */ 01872000
*     END;                          /* END OF RETRY INTERFACE        */ 01873000
*   ELSE                            /* IS ERROR IS NOT RETRYABLE, SET   01874000
*                                      UP TO PERCOLATE               */ 01875000
*     DO;                           /* PERCOLATION PATH              */ 01876000
         B     @RC00502                                            0535 01877000
@RF00502 DS    0H                                                  0536 01878000
*       RFY                                                        0536 01879000
*         R6 RSTD;                  /* GET REG FOR EEDS TO BE FREED  */ 01880000
*       R6=R13->RT1TREGS(THREE);    /* LOCATE PTR TO EEDS ACQUIRED 0537 01881000
*                                      FOR ORIGINAL ERROR            */ 01882000
         L     R6,RT1TREGS+8(,R13)                                 0537 01883000
*       IF SDWATEXC^='1'B THEN      /* IF SECOND ERROR WAS NOT A   0538 01884000
