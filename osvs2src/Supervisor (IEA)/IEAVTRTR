*         RCRDNAME=ASCBJBNI->JOBNAME;/* JOBNAME FOR RECORD   @G17EP5W*/ 01443000
         L     @11,RT1WRTCA(,R7)                                   0432 01444000
         MVC   RCRDNAME(8,@11),JOBNAME(@15)                        0432 01445000
*       ELSE                        /* OTHERWISE, CHECK      @G17EP5W*/ 01446000
*         IF ASCBJBNS^=NULL THEN    /* LOGON JOBNAME PTR     @G17EP5W*/ 01447000
         B     @RC00431                                            0433 01448000
@RF00431 L     @15,PSAAOLD                                         0433 01449000
         L     @15,ASCBJBNS(,@15)                                  0433 01450000
         LTR   @15,@15                                             0433 01451000
         BZ    @RF00433                                            0433 01452000
*           RCRDNAME=ASCBJBNS->JOBNAME;/* USE THIS JOBNAME   @G17EP5W*/ 01453000
         L     @11,RT1WRTCA(,R7)                                   0434 01454000
         MVC   RCRDNAME(8,@11),JOBNAME(@15)                        0434 01455000
*         ELSE                      /* OTHERWISE SPECIFY     @G17EP5W*/ 01456000
*           RCRDNAME=FRRJNAME;      /* DEFAULT CONSTANT      @YM05011*/ 01457000
         B     @RC00433                                            0435 01458000
@RF00433 L     @15,RT1WRTCA(,R7)                                   0435 01459000
         MVC   RCRDNAME(8,@15),FRRJNAME                            0435 01460000
*     RCRDSEQ#=SDWASEQ#;            /* ERRORID SEQ NBR       @G17EP5W*/ 01461000
