         BO    @RT00429                                            0429 01427000
         L     @15,PSAAOLD                                         0429 01428000
         CLC   ASCBASCB(4,@15),@CC02807                            0429 01429000
         BE    @RF00429                                            0429 01430000
@RT00429 DS    0H                                                  0430 01431000
*       RCRDNAME=FRRJNAME;          /* NO VALID JOB NAME     @G17EP5W*/ 01432000
         L     @15,RT1WRTCA(,R7)                                   0430 01433000
         MVC   RCRDNAME(8,@15),FRRJNAME                            0430 01434000
*     ELSE                          /* ELSE ATTEMPT JOBNAME@G17EP5W  */ 01435000
*       IF ASCBJBNI^=NULL THEN      /* IF INITIATED JOBNAME POINTER     01436000
*                                      NOT NULL THEN PUT     @G17EP5W*/ 01437000
