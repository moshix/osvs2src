*     RCRDERTM=SDWAERTM;            /* ERRORID TIME STAMP    @G17EP5W*/ 01470000
         MVC   RCRDERTM(4,@15),SDWAERTM(@15)                       0438 01471000
*     IF(SDWAFMID^=0)&              /* IF ERROR ASID^ZERO    @G17EP5W*/ 01472000
*         (SDWAFMID^=INVLASID) THEN /* AND NOT INVALID       @G17EP5W*/ 01473000
