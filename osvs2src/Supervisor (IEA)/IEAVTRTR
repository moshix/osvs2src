*     R13) UNRSTD;                  /* RELEASE REGS - END OF RECVRRTM   02741000
*                                      SEGMENT                       */ 02742000
*                                                                  0754 02743000
*   /*****************************************************************/ 02744000
*   /*                                                               */ 02745000
*   /* THE PURPOSE OF THE RTMSMFRR SEGMENT IS TO PASS ON THE CONTINUE*/ 02746000
*   /* WITH TERMINATION ACTION INITIATED BY RTMRSFRR. THE FRR        */ 02747000
*   /* RTMSMFRR IS PLACED ON THE ERROR STACK ONLY IF RTM1 HAD BEEN   */ 02748000
*   /* ENTERED INITIALLY IN SLIH MODE AND THEN PROCEEDED TO PERFORM  */ 02749000
*   /* SOME RESCHEDULE FUNCTION. IT IS PLACED ON THE ERROR STACK SO  */ 02750000
*   /* THAT ITS SIX WORD PARAMETER AREA CAN BE USED TO STORE         */ 02751000
*   /* ADDITIONAL RECOVERY INFORMATION THAT RTMRSFRR MAY BE ABLE TO  */ 02752000
*   /* UTILIZE. IN ALL CASES, THIS FRR WILL INDICATE CONTINUE WITH   */ 02753000
*   /* TERMINATION. INPUT TO THIS SEGMENT IS AS FOLLOWS: RTCAPTR     */ 02754000
*   /* REGISTER (R1) - POINTS TO THE SDWA(RTCA) WHICH CONTAINS       */ 02755000
*   /* INFORMATION ABOUT THE ERROR. RETNREG REGISTER (R14) - CONTAINS*/ 02756000
*   /* THE RETURN ADDRESS TO IEAVTRTS, THE SYSTEM RECOVERY MANAGER.  */ 02757000
*   /*                                                               */ 02758000
*   /*****************************************************************/ 02759000
*                                                                  0755 02760000
*RTMSMFRR:                                                         0755 02761000
*   ENTRY;                                                         0755 02762000
@EP00755 DS    0H                                                  0755 02763000
@EC00755 DS    0H                                                  0756 02764000
*   RFY                                                            0756 02765000
*    (R1,                           /* ON ENTRY, PTR TO SDWA(RTCA)   */ 02766000
*     R5,                           /* RETURN ADDR TO RTS - SAVED ON    02767000
*                                      ENTRY                         */ 02768000
*     R14) RSTD;                    /* RETURN ADDR TO RTS ON ENTRY   */ 02769000
*   R5=R14;                         /* SAVE RETURN ADDRESS TO RTS    */ 02770000
         LR    R5,R14                                              0757 02771000
*   DO;                             /* SETRP RECORD(YES)RECPARM(SLIHM   02772000
*                                      ID)RC(0)                      */ 02773000
*     RESPECIFY                                                    0759 02774000
*      (GPR00P,                                                    0759 02775000
*       GPR01P,                                                    0759 02776000
*       GPR14P,                                                    0759 02777000
*       GPR15P) RSTD;                                              0759 02778000
*     GPR01P->SDWARCDE=0;           /* SAVE RC VALUE                 */ 02779000
         MVI   SDWARCDE(GPR01P),X'00'                              0760 02780000
*     GPR01P->SDWARCRD='1'B;        /* TURN ON RECORD INDICATOR      */ 02781000
         OI    SDWARCRD(GPR01P),B'10000000'                        0761 02782000
*     GPR15P=ADDR(SLIHMID);         /* ACCESS RECORD PARAMETER LIST  */ 02783000
         LA    GPR15P,SLIHMID                                      0762 02784000
*     GPR01P->SDWARECP=GPR15P->I256C(1:24);/* COPY RECORD PARAMETERS */ 02785000
         MVC   SDWARECP(24,GPR01P),I256C(GPR15P)                   0763 02786000
*     RESPECIFY                                                    0764 02787000
*      (GPR00P,                                                    0764 02788000
*       GPR01P,                                                    0764 02789000
*       GPR14P,                                                    0764 02790000
*       GPR15P) UNRSTD;                                            0764 02791000
*   END;                                                           0765 02792000
*   R14=R5;                         /* RESTORE RETURN ADDRESS        */ 02793000
         LR    R14,R5                                              0766 02794000
*   RETURN;                         /* RETURN TO RTS                 */ 02795000
         B     @EL00001                                            0767 02796000
*   RFY                                                            0768 02797000
*    (R1,                                                          0768 02798000
*     R5,                                                          0768 02799000
*     R14) UNRSTD;                  /* FREE REGS USED BY FRR         */ 02800000
*RTHFRR:                                                           0769 02801000
*   ENTRY;                                                         0769 02802000
*                                                                  0769 02803000
@EP00769 DS    0H                                                  0770 02804000
*   /*****************************************************************/ 02805000
*   /*                                                               */ 02806000
*   /* THE PURPOSE OF THE RTHFRR SEGMENT IS TO PROVIDE RECOVERY FOR  */ 02807000
*   /* RTH'S PROCESSING. THE RECOVERY SCHEME IS AS FOLLOWS: IF THE   */ 02808000
*   /* ERROR WAS THE FIRST ERROR IN A PARTICULAR SEGMENT OF RTH, THEN*/ 02809000
*   /* THE FRR WILL ATTEMPT TO RETRY THAT SEGMENT. IF, HOWEVER, THE  */ 02810000
*   /* ERROR IS THE SECOND ERROR OCCURRING IN A SEGMENT, THEN THE FRR*/ 02811000
*   /* WILL ATTEMPT TO RETRY TO THE NEXT LOGICAL SEGMENT IN RTH, THUS*/ 02812000
*   /* SKIPPING THE SEGMENT IN WHICH THE ERROR WAS OCCURRING. THE    */ 02813000
*   /* RECOVERY DESCRIBED IS ATTEMPTED PROVIDED THAT THE ERROR IS NOT*/ 02814000
*   /* AN UNRECOVERABLE TRANSLATION EXCEPTION OR A RESTART INTERRUPT.*/ 02815000
*   /* IN THESE CASES, RETRY IS NOT PERMITTED AND SO THE ONLY THING  */ 02816000
*   /* THE FRR IS ABLE TO DO IS CLEAN UP ANY RESOURCES ACQUIRED BY   */ 02817000
*   /* RTH, NAMELY EEDS, AND CONTINUE WITH TERMINATION. INPUT TO THIS*/ 02818000
*   /* SEGMENT IS AS FOLLOWS: FRRWAREG REGISTER (R0) - POINTS TO A   */ 02819000
*   /* 200 BYTE WORK AREA AVAILABLE FOR THE FRR'S USE. RTCAPTR       */ 02820000
*   /* REGISTER (R1) - POINTS TO THE SDWA(RTCA) WHICH CONTAINS       */ 02821000
*   /* INFORMATION ABOUT THE ERROR. RETNREG REGISTER (R14) - CONTAINS*/ 02822000
*   /* THE RETURN ADDRESS TO IEAVTRTS, THE SYSTEM RECOVERY MANAGER.  */ 02823000
*   /*                                                               */ 02824000
*   /*****************************************************************/ 02825000
*                                                                  0770 02826000
*   RFY                                                            0770 02827000
*    (R0,                           /* ON ENTRY, PTR TO 200 BYTE WA  */ 02828000
*     R1,                           /* ON ENTRY, PTR TO SDWA (RTCA)  */ 02829000
*     R5,                           /* RETURN ADDR TO RTS - SAVED ON    02830000
*                                      ENTRY                         */ 02831000
*     R7,                           /* PTR TO THIS FRR'S PARM AREA   */ 02832000
*     R8,                           /* PTR TO 200 BYTE WA - HERE   0770 02833000
*                                      SINCE THE VALUE IN R0 IS    0770 02834000
*                                      DESTROYED                     */ 02835000
*     R14) RSTD;                    /* RETURN ADDR TO RTS ON ENTRY   */ 02836000
*   R5=R14;                         /* SAVE RETURN ADDRESS TO RTS    */ 02837000
         LR    R5,R14                                              0771 02838000
*   R8=R0;                          /* SAVE PTR TO 200 BYTE WA       */ 02839000
         LR    R8,R0                                               0772 02840000
*   RFY                                                            0773 02841000
*    (R0,                                                          0773 02842000
*     R14) UNRSTD;                  /* FREE UP SAVED REGISTERS       */ 02843000
*   RFY                                                            0774 02844000
*     FRRWORKA BASED(R8);                                          0774 02845000
*   RFY                                                            0775 02846000
*     SDWA BASED(R1);               /* USE SDWA PASSED TO FRR        */ 02847000
*   R7=SDWAPARM;                    /* LOCATE FRR'S PARM AREA        */ 02848000
         L     R7,SDWAPARM(,R1)                                    0776 02849000
*   RFY                                                            0777 02850000
*     RTHTRACK BASED(R7);                                          0777 02851000
*   IF SDWATEXC='0'B&               /* IF ERROR IS NOT A DATERR      */ 02852000
*       SDWARKEY='0'B THEN          /* AND IS NOT A RESTART ERROR    */ 02853000
         TM    SDWATEXC(R1),B'00100010'                            0778 02854000
         BNZ   @RF00778                                            0778 02855000
*     DO;                           /* SET UP FOR RETRY              */ 02856000
*       RFY                                                        0780 02857000
*        (R2,                       /* INDEX TO RECOVERY ACTION      */ 02858000
*         R4,                       /* REG CONTAINING RETRY ADDR     */ 02859000
*         R6,                       /* PTR TO CPU RELATED SAV AREA   */ 02860000
*         R13) RSTD;                /* PTR TO CALLER'S REGS AT TIME     02861000
*                                      THE CALLRTM WAS ISSUED        */ 02862000
*       R13=RTHTREGS(ONE);          /* RESTORE PTR TO CALLER'S REGS  */ 02863000
         L     R13,RTHTREGS(,R7)                                   0781 02864000
*       RFY                                                        0782 02865000
*         ERREGS BASED(R13);                                       0782 02866000
*       R6=RTHTREGS(THREE);         /* RESTORE PTR TO CPU RELATED RTM   02867000
*                                      SAVE AREA                     */ 02868000
         L     R6,RTHTREGS+8(,R7)                                  0783 02869000
*       RFY                                                        0784 02870000
*         RTMCPUSA BASED(R6);                                      0784 02871000
*       IF RTHTLPN^=NULL THEN       /* IF RTH'S PROCESSING HAD GOTTEN   02872000
*                                      PAST INITIALIZATION           */ 02873000
         CLI   RTHTLPN(R7),0                                       0785 02874000
         BE    @RF00785                                            0785 02875000
*         R2=RTHTLPN-RT1MCHLO;      /* CALC RECOVERY INDEX FROM LPN  */ 02876000
         SLR   R2,R2                                               0786 02877000
         IC    R2,RTHTLPN(,R7)                                     0786 02878000
         SL    R2,@CF00118                                         0786 02879000
*       ELSE                        /* OTHERWISE RTH WAS STILL     0787 02880000
*                                      INITIALIZING                  */ 02881000
*         R2=ONE;                   /* SO INITIALIZE INDEX TO 1      */ 02882000
         B     @RC00785                                            0787 02883000
@RF00785 LA    R2,1                                                0787 02884000
*       REGID=ALLREGS;              /* INDICATE ALL REGS ARE TO BE 0788 02885000
*                                      PRIMED FOR RETRY              */ 02886000
@RC00785 MVC   REGID(2,R8),@CB02840                                0788 02887000
*       RTYREGS=''B;                /* INITIALIZE RETRY REGS TO 0    */ 02888000
         XC    RTYREGS(64,R8),RTYREGS(R8)                          0789 02889000
*       GO TO RTHRECOV(R2);         /* GO TAKE PROPER RECOVERY ACTION*/ 02890000
         LR    @12,R2                                              0790 02891000
         SLA   @12,2                                               0790 02892000
         L     @12,RTHRECOV-4(@12)                                 0790 02893000
         BR    @12                                                 0790 02894000
*RRTHSTRT:                          /* RECOVERY ACTION FOR RTH'S   0791 02895000
*                                      INITIALIZATION SEGMENT        */ 02896000
*       RTHTREGS(TWO)=ERREG1;       /* INITIALIZE SLOT IN TRACKING 0791 02897000
*                                      AREA WITH LRB ADDRESS (REG1 0791 02898000
*                                      VALUE OF CALLER'S REGS)       */ 02899000
RRTHSTRT L     @12,ERREG1(,R13)                                    0791 02900000
         ST    @12,RTHTREGS+4(,R7)                                 0791 02901000
*       R6=WSACRTMK;                /* OBTAIN ADDRESS OF RTM'S CPU 0792 02902000
*                                      RELATED SAVE AREA             */ 02903000
         L     @12,PSALCCAV                                        0792 02904000
         L     @12,LCCACPUS(,@12)                                  0792 02905000
         L     R6,WSACRTMK(,@12)                                   0792 02906000
*       RTHTREGS(THREE)=R6;         /* INITIALIZE SLOT IN TRACKING 0793 02907000
*                                      AREA WITH ADDR OF SAVE AREA   */ 02908000
         ST    R6,RTHTREGS+8(,R7)                                  0793 02909000
*       R4=RTHRLBL1(R2);            /* SET UP RETRY ADDRESS          */ 02910000
         LR    @12,R2                                              0794 02911000
         SLA   @12,2                                               0794 02912000
         L     R4,RTHRLBL1-4(@12)                                  0794 02913000
*       GO TO RESTREGS;             /* GO SET UP RETRY REGS          */ 02914000
         B     RESTREGS                                            0795 02915000
*RRTHEEDS:                          /* RECOVERY ACTION FOR EED     0796 02916000
*                                      PROCESSING IN RTH             */ 02917000
*       IF RTHTEED1='1'B|           /* IF INITIAL EED WAS ACQUIRED   */ 02918000
*           RTHTRTRY='1'B THEN      /* OR THIS IS THE 2ND ERROR      */ 02919000
RRTHEEDS TM    RTHTEED1(R7),B'00011000'                            0796 02920000
         BZ    @RF00796                                            0796 02921000
*         DO;                       /* DO EXTRA EED PROCESSING       */ 02922000
*           RTHTRTRY='1'B;          /* INSURE THAT THIS APPEARS TO BE   02923000
*                                      A SECOND ERROR                */ 02924000
         OI    RTHTRTRY(R7),B'00010000'                            0798 02925000
*           IF RTHTEED1='1'B THEN   /* IF INITIAL EED WAS ACQUIRED 0799 02926000
*                                      AND CHECKPOINTED              */ 02927000
         TM    RTHTEED1(R7),B'00001000'                            0799 02928000
         BNO   @RF00799                                            0799 02929000
*             IF RTHTEED2='0'B THEN /* BUT SECOND EED WAS NOT        */ 02930000
         TM    RTHTEED2(R7),B'00000100'                            0800 02931000
         BNZ   @RF00800                                            0800 02932000
*               DO;                                                0801 02933000
*                 RTHTREGS(FOUR)=RTHTREGS(FIVE);/* INITIALIZE SLOT IN   02934000
*                                      TRACKING AREA WITH ADDR OF 1ST   02935000
*                                      EED - NOW SAME AS ADDR OF   0802 02936000
*                                      REPAIR STATUS EED             */ 02937000
         L     @12,RTHTREGS+16(,R7)                                0802 02938000
         ST    @12,RTHTREGS+12(,R7)                                0802 02939000
*                 RFY                                              0803 02940000
*                   EED BASED(RTHTREGS(FIVE));                     0803 02941000
*                 EEDFWRDP=EEDNULL; /* INDICATE ONLY 1 EED WAS     0804 02942000
*                                      ACQUIRED                      */ 02943000
         L     @12,RTHTREGS+16(,R7)                                0804 02944000
         MVC   EEDFWRDP(4,@12),@CF00079                            0804 02945000
*               END;                                               0805 02946000
*             ELSE                                                 0806 02947000
*               ;                   /* IF SECOND EED WAS ACQUIRED AND   02948000
*                                      CHECKPOINTED, NO FURTHER    0806 02949000
*                                      ACTION IS NEEDED              */ 02950000
@RF00800 DS    0H                                                  0807 02951000
*           ELSE                    /* IF INIT EED WAS NOT ACQUIRED  */ 02952000
*             RTHTREGS(FOUR)=EEDNULL;/* INITIALIZE EED PTR IN TRACK     02953000
*                                      AREA TO INDICATE INFORMATION     02954000
*                                      IS LOST                       */ 02955000
         B     @RC00799                                            0807 02956000
@RF00799 MVC   RTHTREGS+12(4,R7),@CF00079                          0807 02957000
*           RTHTEED2='1'B;          /* SET BIT INDICATING EED PTR IS    02958000
