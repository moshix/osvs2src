*                                      PARAMETERS                    */ 03194000
         MVC   SDWARECP(24,GPR01P),I256C(GPR15P)                   0887 03195000
*         GPR01P->SDWAUPRG='1'B;    /* TURN ON RETREGS INDICATOR     */ 03196000
         OI    SDWAUPRG(GPR01P),B'00001000'                        0888 03197000
*         GPR15P=ADDR(REGID);       /* GET PTR TO REG UPDATE BLOCK   */ 03198000
         LA    GPR15P,REGID(,R8)                                   0889 03199000
*         GPR00P=GPR15P->I015F;     /* ACCESS REGISTER BIT PATTERN   */ 03200000
         LH    GPR00P,I015F(,GPR15P)                               0890 03201000
*         GPR15P=GPR15P+2;          /* CREATE PTR TO USER REG VALUES */ 03202000
         AL    GPR15P,@CF00110                                     0891 03203000
*         GPR14P=ADDR(GPR01P->SDWASRSV);/* CREATE PTR TO SDWA SAVE 0892 03204000
*                                      AREA                          */ 03205000
         LA    GPR14P,SDWASRSV(,GPR01P)                            0892 03206000
*         GEN(SLL   0,16);          /* SHIFT BIT PATTERN TO HIGH   0893 03207000
*                                      ORDER                         */ 03208000
         SLL   0,16                                                     03209000
*R0008:                                                            0894 03210000
*         GEN FLOWS(I0008);                                        0894 03211000
R0008    DS    0H                                                  0894 03212000
         ALR   0,0                    SHIFT BIT TO SIGN POSITION        03213000
         BC    12,I0008               BRANCH IF BIT NOT ON              03214000
         MVC   0(4,14),0(15)          MODIFY REGISTER IN SDWA           03215000
         LA    15,4(15)               UPDATE POINTER                    03216000
*I0008:                                                            0895 03217000
*         GENERATE FLOWS(R0008);                                   0895 03218000
I0008    DS    0H                                                  0895 03219000
         LA    14,4(14)               UPDATE PTR IN SDWA                03220000
         BC    5,R0008                TEST ALR CONDITION CODE TO TEST   03221000
*                                     IF ALL BITS EXHAUSTED             03222000
*         RESPECIFY                                                0896 03223000
*          (GPR00P,                                                0896 03224000
*           GPR01P,                                                0896 03225000
*           GPR14P,                                                0896 03226000
*           GPR15P) UNRSTD;                                        0896 03227000
*       END;                                                       0897 03228000
*       RFY                                                        0898 03229000
*        (R2,                                                      0898 03230000
*         R4,                                                      0898 03231000
*         R6,                                                      0898 03232000
*         R13) UNRSTD;              /* FREE REGS USED FOR RETRY      */ 03233000
*     END;                          /* END OF SET UP FOR RETRY       */ 03234000
*   ELSE                            /* IF ERROR IS A DATERR OR A   0900 03235000
*                                      RESTART ERROR                 */ 03236000
*     DO;                           /* SET UP FOR PERCOLATION        */ 03237000
         B     @RC00778                                            0900 03238000
@RF00778 DS    0H                                                  0901 03239000
*       IF RTHTREGS(FOUR)>EEDNULL THEN/* IF EEDS NEED TO BE FREED    */ 03240000
         L     @12,RTHTREGS+12(,R7)                                0901 03241000
         C     @12,@CF00079                                        0901 03242000
         BNH   @RF00901                                            0901 03243000
*         DO;                                                      0902 03244000
*           RFY                                                    0903 03245000
*            (R6,                                                  0903 03246000
*             R12) RSTD;            /* GET REGS FOR FREECELL         */ 03247000
*           R6=RTHTREGS(FOUR);      /* PLACE EED PTR IN REGISTER     */ 03248000
         L     @11,RTHTREGS+12(,R7)                                0904 03249000
         LR    R6,@11                                              0904 03250000
*           R12=R1;                 /* SAVE REG1 VALUE - DESTROYED BY   03251000
*                                      FREECELL ROUTINE              */ 03252000
         LR    R12,R1                                              0905 03253000
*           RFY                                                    0906 03254000
*            (R9,                                                  0906 03255000
*             R11) RSTD;                                           0906 03256000
*           R11=R9;                 /* SAVE THE VALUE OF R9 SINCE IT    03257000
*                                      IS DESTROYED BY FREEDCEL      */ 03258000
         LR    R11,R9                                              0907 03259000
*           CALL FREEDCEL;          /* CALL FREECELL ROUTINE         */ 03260000
         L     @15,@CA03293                                        0908 03261000
         BALR  @14,@15                                             0908 03262000
*           R9=R11;                 /* RESTORE THE VALUE OF R9       */ 03263000
         LR    R9,R11                                              0909 03264000
*           RFY                                                    0910 03265000
*            (R11,                                                 0910 03266000
*             R9) UNRSTD;                                          0910 03267000
*           R1=R12;                 /* RESTORE REG1 - RTCA ADDRESS   */ 03268000
         LR    R1,R12                                              0911 03269000
*           RFY                                                    0912 03270000
*            (R6,                                                  0912 03271000
*             R12) UNRSTD;          /* RELEASE REGS                  */ 03272000
*         END;                      /* END OF FREECELL PROCESSING    */ 03273000
*       DO;                         /* SETRP                       0914 03274000
*                                      RECORD(YES)RECPARM(RTHID)RC(0)*/ 03275000
@RF00901 DS    0H                                                  0915 03276000
*         RESPECIFY                                                0915 03277000
*          (GPR00P,                                                0915 03278000
*           GPR01P,                                                0915 03279000
*           GPR14P,                                                0915 03280000
*           GPR15P) RSTD;                                          0915 03281000
*         GPR01P->SDWARCDE=0;       /* SAVE RC VALUE                 */ 03282000
         MVI   SDWARCDE(GPR01P),X'00'                              0916 03283000
*         GPR01P->SDWARCRD='1'B;    /* TURN ON RECORD INDICATOR      */ 03284000
         OI    SDWARCRD(GPR01P),B'10000000'                        0917 03285000
*         GPR15P=ADDR(RTHID);       /* ACCESS RECORD PARAMETER LIST  */ 03286000
         LA    GPR15P,RTHID                                        0918 03287000
*         GPR01P->SDWARECP=GPR15P->I256C(1:24);/* COPY RECORD      0919 03288000
*                                      PARAMETERS                    */ 03289000
         MVC   SDWARECP(24,GPR01P),I256C(GPR15P)                   0919 03290000
*         RESPECIFY                                                0920 03291000
*          (GPR00P,                                                0920 03292000
*           GPR01P,                                                0920 03293000
*           GPR14P,                                                0920 03294000
*           GPR15P) UNRSTD;                                        0920 03295000
*       END;                                                       0921 03296000
*     END;                          /* END OF PERCOLATION ACTION     */ 03297000
*   RFY                                                            0923 03298000
*     R14 RSTD;                     /* GET REG TO RETURN TO RTS      */ 03299000
@RC00778 DS    0H                                                  0924 03300000
*   R14=R5;                         /* RESTORE RETURN ADDRESS        */ 03301000
         LR    R14,R5                                              0924 03302000
*   RFY                                                            0925 03303000
*    (R1,                                                          0925 03304000
*     R5,                                                          0925 03305000
*     R7,                                                          0925 03306000
*     R8) UNRSTD;                   /* FREE REGS USED BY FRR         */ 03307000
*   RETURN;                         /* RETURN TO RTS                 */ 03308000
         B     @EL00001                                            0926 03309000
*   RFY                                                            0927 03310000
*     R14 UNRSTD;                   /* RELEASE REG FOR RETURN ADDR -    03311000
*                                      END OF RTHFRR SEGMENT         */ 03312000
*/* ******************************************************************/ 03313000
*/* THIS SEGMENT IS AN INTERNAL PROCEDURE WHICH RESETS THE RECOVERY  */ 03314000
*/* STACK VECTOR TABLE FO THE PSA(PSARSVT) TO THE STATE THAT EXISTED */ 03315000
*/* PRIOR TO A IEAVTRTS FAILURE.  THIS FUNCTION IS PERFORMED BY      */ 03316000
*/* FINDING THE RSVT ENTRY ASSOCIATED WITH THE FRR STACK THAT WAS    */ 03317000
*/* CURRENT AT THE TIME OF THE IEAVTRTS FAILURE AND STORING INTO     */ 03318000
*/* THE FRR STACK SAVE AREA PORTION OF THIS ENTRY THE ADDRESS OF     */ 03319000
*/* THIS INTERRUPTED FRR STACK WHICH WAS CURRENT WHEN IEAVTRTS       */ 03320000
*/* FAILED                                                           */ 03321000
*/* THIS SEGMENT IS ONLY CALLED BY FRR WHICH PROTECT IEAVTRTS SO     */ 03322000
*/* THAT IS BOTH RETRY AND PERCOLATION SITUATIONS THE PROPER STATUS  */ 03323000
*/* OF THE RSVT CAN BE MAINTAINED THROUGHOUT THE RECOVERY PROCESS    */ 03324000
*/* INPUT---                                                         */ 03325000
*/*  - INTSTACK REGISTER(R5) CONTAINS THE ADDRESS OF THE FRR STACK   */ 03326000
*/*    TO BE STORED IN THE RSVT                                      */ 03327000
*/*  - PSACSTK FIELD OF THE PSA POINTS TO THE INTERRUPTED STACK      */ 03328000
*/*    WHOSE ENTRY MUST BE FOUND IN THE RSVT                         */ 03329000
*/*  - RSVTIDX REGISTER(R4),STKIDX REGISTER(R11) ARE USED AS WORK    */ 03330000
*/*    TO INDEX INTO THE RSVT                                        */ 03331000
*/* OUTPUT---                                                        */ 03332000
*/*  - THE FRR STACK ADDRESS IN INTSTACK REGISTER(R5) IS STORED IN   */ 03333000
*/*    THE FRR STACK ADDRESS SAVE AREA OF THE RSVT ENTRY ASSOCIATED  */ 03334000
*/*    WITH THE FRR STACK POINTED TO BY PSACSTK                      */ 03335000
*/* NOTES---                                                         */ 03336000
*/*  - REGS(R1,R0,R4,R11,R14) ARE RESTRICTED THROUGHOUT THIS INTERNAL*/ 03337000
*/*    PROCEDURE                                                     */ 03338000
*/* ******************************************************************/ 03339000
*                                                                  0928 03340000
*STORESTK:                                                         0928 03341000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                               0928 03342000
