         LH    @11,PSACPULA                                        0377 01151000
         STC   @11,SDWACPUI(,@15)                                  0377 01152000
*         SDWAERFL=ON;              /* MARK ERRORID IN SDWA  @G17EP5W*/ 01153000
         OI    SDWAERFL(@15),B'00000001'                           0378 01154000
*         RFY                                                      0379 01155000
*          (R0,                                                    0379 01156000
*           R1,                                                    0379 01157000
*           R2,                                                    0379 01158000
*           R8) UNRSTD;             /* FREE UP REGS          @G17EP5W*/ 01159000
*       END;                        /* END GENERATE ERRORID  @G17EP5W*/ 01160000
*     LM(R0,R2,SDWARCPL);           /* RESTORE REG VALUES    @G17EP5W*/ 01161000
@RF00369 L     @15,RT1WRTCA(,R7)                                   0381 01162000
         LM    R0,R2,SDWARCPL(@15)                                 0381 01163000
*     RFY                                                          0382 01164000
*      (R0,                                                        0382 01165000
*       R1,                                                        0382 01166000
*       R2) RSTD;                   /* RERESTRICT REGS THAT WERE   0382 01167000
*                                      PREVIOUSLY RESTRICTED @G17EP5W*/ 01168000
*     SDWARCPL=SDWARCPL&&SDWARCPL;  /* CLEAR OUT SDWA AREA USED FOR     01169000
*                                      TERMPORARY REG. STORAGE     0383 01170000
*                                                            @G17EP5W*/ 01171000
         L     @15,RT1WRTCA(,R7)                                   0383 01172000
         XC    SDWARCPL(28,@15),SDWARCPL(@15)                      0383 01173000
*   END;                            /* END ERRORID SEGMENT   @G17EP5W*/ 01174000
*                                                                  0384 01175000
*   /*****************************************************************/ 01176000
*   /*                                                               */ 01177000
*   /* THIS SEGMENT INITIALIZES THE SDWA WITH ERROR INFORMATION      */ 01178000
*   /* CONCERNING THE MODE OF THE SYSTEM AT THE TIME OF THE ERROR    */ 01179000
*   /* INPUT - RT1WRTCA FIELD OF THE RTM1 WORK AREA PORTION OF THE   */ 01180000
*   /* INTERRUPTED STACK POINTS TO THE RTCA TO BE INITIALIZED - THE  */ 01181000
*   /* MODE BYTE IN THE RTM1 WORK AREA PORTION OF THE INTERRUPTED    */ 01182000
*   /* STACK(RT1WMODE) CONTAINS THE MODE OF THE SYSTEM AT THE TIME OF*/ 01183000
*   /* THE ERROR - STAKPTR REGISTER(R7) POINTS TO THE INTERRUPTED    */ 01184000
*   /* STACK OUTPUT - THE SDWATYP1 BIT IS SET ON IN THE RTCA IF A    */ 01185000
*   /* TYPE 1 SVC WAS IN CONTROL AT THE TIME OF THE ERROR - THE      */ 01186000
*   /* SDWALDIS BIT IS SET ON IN THE RTCA IF A GLOBAL OR LOCAL LOCK  */ 01187000
*   /* WAS HELD AT THE TIME OF THE ERROR - THE SDWASRBM BIT IS SET ON*/ 01188000
*   /* IN THE RTCA IF THE MODEBYTE IN THE RTM1 WORK AREA INDICATES   */ 01189000
*   /* THAT THE SYSTEM WAS IN SRB MODE AT THE TIME OF THE ERROR NOTES*/ 01190000
*   /* - ALL RETRICTED GENERAL PURPOSE REGISTER CONTENTS ON ENTRY ARE*/ 01191000
*   /* UNCHANGED ON EXIT FROM THIS SEGMENT - THIS SEGMENT IS PART OF */ 01192000
*   /* THE SRMDRCOV LPRR FOR IEAVTRTS                                */ 01193000
*   /*                                                               */ 01194000
*   /*****************************************************************/ 01195000
*                                                                  0385 01196000
*   IF MODETYP1=ON THEN             /* IF TYPE1 SVC IN CONTROL AT    */ 01197000
         TM    MODETYP1+81(R7),B'00000100'                         0385 01198000
         BNO   @RF00385                                            0385 01199000
*     SDWATYP1=ON;                  /* TIME OF ERROR MAKE INDICATION    01200000
*                                      IN SDWA                       */ 01201000
         OI    SDWATYP1(@15),B'00001000'                           0386 01202000
*   ELSE                                                           0387 01203000
*     ;                             /* IF TYPE1 SVC NOT IN CONTROL AT   01204000
*                                      TIME OF ERROR NOT INDICATION     01205000
*                                      IS MADE                       */ 01206000
@RF00385 DS    0H                                                  0388 01207000
*   MODEDIS=ON;                     /* SRM IS ALWAYS DISABLED        */ 01208000
         LA    @15,81                                              0388 01209000
         ALR   @15,R7                                              0388 01210000
         OI    MODEDIS(@15),B'01000000'                            0388 01211000
*   IF MODEBYTE>=LOCLMODE THEN      /* IF GLOBAL OR LOCAL MODE THEN  */ 01212000
         CLI   MODEBYTE(@15),8                                     0389 01213000
         BL    @RF00389                                            0389 01214000
*     SDWALDIS=ON;                  /* INDICATE LOGICAL OR PHYSICAL     01215000
*                                      DISABLEMENT AT TIME OF ERROR  */ 01216000
         L     @15,RT1WRTCA(,R7)                                   0390 01217000
         OI    SDWALDIS(@15),B'00000010'                           0390 01218000
*   ELSE                                                           0391 01219000
*     ;                             /* IF THE MODE IS NEITHER GLOBAL    01220000
*                                      NOR LOCAL THEN THERE IS NO  0391 01221000
*                                      LOGICAL OR PHYSICAL         0391 01222000
*                                      DISABLEMENT                   */ 01223000
@RF00389 DS    0H                                                  0392 01224000
*   IF MODESRB=ON THEN              /* IF SRB MODE WAS ACTVE AT TIME    01225000
*                                      OF ERROR                      */ 01226000
         TM    MODESRB+81(R7),B'00000010'                          0392 01227000
         BNO   @RF00392                                            0392 01228000
*     SDWASRBM=ON;                  /* THEN INDICATE SRB MODE IN THE    01229000
*                                      SDWA                          */ 01230000
         L     @15,RT1WRTCA(,R7)                                   0393 01231000
         OI    SDWASRBM(@15),B'00000001'                           0393 01232000
*   ELSE                                                           0394 01233000
*     ;                                                            0394 01234000
@RF00392 DS    0H                                                  0395 01235000
*   IF R5^=NULL THEN                /* IF DUMP OPTIONS WERE SUPPLIED    01236000
*                                      PERFORM DUMPOPTS PROCESSING   */ 01237000
         LTR   R5,R5                                               0395 01238000
         BZ    @RF00395                                            0395 01239000
*/* ******************************************************************/ 01240000
*/* THIS SEGMENT IS PART OF THE IEAVTRTS DEFAULT RECOVERY LPRR WHICH */ 01241000
*/* INITIALIZES THE SDWA WITH ANY DUMP OPTIONS ASSOCIATED WITH THE   */ 01242000
*/* IEAVTRTS FAILURE.  THE SNAP PARAMETER LIST AND A MAX OF 4 STORAGE*/ 01243000
*/* ARE MOVED INTO THE SDWA                                          */ 01244000
*/* INPUT----                                                        */ 01245000
*/*  - DUMPOPTS REGISTER(R5) POINTS TO THE SNAP PARM LIST            */ 01246000
*/*  - THE SNAP PARAMETER LIST INDICATES IF STORAGE RANGES ARE       */ 01247000
*/*    SUPPLIED                                                      */ 01248000
*/* OUTPUT---                                                        */ 01249000
*/*  - THE DUMP OPTIONS ASSOCIATED WITH THE IEAVTRTS FAILURE ARE     */ 01250000
*/*    INITIALIZED IN THE SDWA                                       */ 01251000
*/* ******************************************************************/ 01252000
*                                                                  0396 01253000
*     DO;                                                          0396 01254000
*       RFY                                                        0397 01255000
*        (R3,                                                      0397 01256000
*         R4) RSTD;                                                0397 01257000
*       SDWASNPA=SNPCDMP;           /* MOVE INTO THE SDWA THE COMMON    01258000
*                                      SECTION OF SNAP PARMS         */ 01259000
         L     @15,RT1WRTCA(,R7)                                   0398 01260000
         MVC   SDWASNPA(8,@15),SNPCDMP(R5)                         0398 01261000
*       IF SNPSLIST=ON THEN         /* IF STORAGE RANGES ARE PROVIDED   01262000
*                                      IN THE SNAP PARMS PROCESS THE    01263000
*                                      STORAGE RANGES                */ 01264000
         TM    SNPSLIST(R5),B'00000010'                            0399 01265000
         BNO   @RF00399                                            0399 01266000
*         DO;                       /* PROCESS TO MOVE STORAGE RANGE    01267000
*                                      ADDRESSES INTO SDWA           */ 01268000
*           IF STOREND(ONE)=ON THEN /* IF ONLY ONE SET OF STORAGE  0401 01269000
*                                      RANGES IS PROVIDED THEN       */ 01270000
         L     @15,SNPSTOR(,R5)                                    0401 01271000
         TM    STOREND(@15),B'10000000'                            0401 01272000
         BNO   @RF00401                                            0401 01273000
*             R3=LENGTH(LISTPAIR);  /* SET THE NUMBER OF BYTES TO  0402 01274000
*                                      MOVE IN ONE PAIR              */ 01275000
         LA    R3,8                                                0402 01276000
*           ELSE                    /* OTHERWISE MORE THAN ONE SET OF   01277000
*                                      STORAGE RANGES IS SUPPLIED    */ 01278000
*             DO;                   /* DETERMINE THE NUMBER OF     0403 01279000
*                                      STORAGE RANGES SUPPLIED       */ 01280000
         B     @RC00401                                            0403 01281000
@RF00401 DS    0H                                                  0404 01282000
*               R4=NULL;            /* INITIALIZE SWITCH FOR LOOP  0404 01283000
*                                      EXIT                          */ 01284000
         SLR   R4,R4                                               0404 01285000
*               DO R3=1 TO DIM(LISTPAIR)/* CALCULATE AMOUNT          */ 01286000
*                     WHILE(R4=NULL);/* TO MOVE BY INITIALIZING    0405 01287000
*                                      LENGTH TO THE FIRST PAIR AND     01288000
*                                      CONTINUING TO SEARCH UNTIL THE   01289000
*                                      MAX NUMBER OF RANGES ALLOWED     01290000
*                                      IN THE SDWA IS REACHED OR   0405 01291000
*                                      UNTIL AN END OF THE LIST IS 0405 01292000
*                                      DETECTED                      */ 01293000
         LA    R3,1                                                0405 01294000
@DL00405 LTR   R4,R4                                               0405 01295000
         BNZ   @DC00405                                            0405 01296000
*                 IF STOREND(R3)=ON THEN/* IF THE END OF THE LIST HAS   01297000
*                                      BEEN REACHED                  */ 01298000
         LR    @15,R3                                              0406 01299000
         SLA   @15,3                                               0406 01300000
         L     @11,SNPSTOR(,R5)                                    0406 01301000
         ALR   @11,@15                                             0406 01302000
         AL    @11,@CF03411                                        0406 01303000
         TM    STOREND-4(@11),B'10000000'                          0406 01304000
         BNO   @RF00406                                            0406 01305000
*                   R4=HEX7FOXS;    /* CAUSE LOOP EXIT               */ 01306000
         L     R4,@CF02842                                         0407 01307000
*                 ELSE                                             0408 01308000
*                   ;                                              0408 01309000
@RF00406 DS    0H                                                  0409 01310000
*               END;                                               0409 01311000
         AL    R3,@CF00079                                         0409 01312000
         C     R3,@CF00113                                         0409 01313000
         BNH   @DL00405                                            0409 01314000
@DC00405 DS    0H                                                  0410 01315000
*               R3=R3*LENGTH(LISTPAIR);/* CONVERT THE NUMBER OF PAIRS   01316000
*                                      TO BYTES                      */ 01317000
         SLL   R3,3                                                0410 01318000
*             END;                  /* END OF LENGTH CALCULATION     */ 01319000
*           SDWADPSL(1:R3)=STORLIST(1:R3);/* MOVE THE STORAGE RANGE     01320000
*                                      LIST INTO THE SDWA            */ 01321000
@RC00401 L     @15,RT1WRTCA(,R7)                                   0412 01322000
         LR    @11,R3                                              0412 01323000
         BCTR  @11,0                                               0412 01324000
         L     @08,SNPSTOR(,R5)                                    0412 01325000
         EX    @11,@SM03412                                        0412 01326000
*         END;                      /* END STORAGE RANGE PROCESSING  */ 01327000
*       ELSE                                                       0414 01328000
*         ;                         /* IF NO STORAGE RANGES SUPPLIED    01329000
*                                      THEN NO STORAGE RANGES ARE  0414 01330000
*                                      INITIALIZED IN THE SDWA       */ 01331000
@RF00399 DS    0H                                                  0415 01332000
*     END;                          /* END DUMPOPTS PROCESSING       */ 01333000
*   ELSE                                                           0416 01334000
*     ;                             /* IF DUMP OPTIONS ARE NOT     0416 01335000
*                                      SPECIFIED THEN NO           0416 01336000
*                                      INITIALIZATION OF THE SDWA  0416 01337000
*                                      WITH DUMPOPTS TAKES PLACE     */ 01338000
@RF00395 DS    0H                                                  0417 01339000
*   END SETRTCA;                                                   0417 01340000
*                                                                  0417 01341000
@EL00002 DS    0H                                                  0417 01342000
@EF00002 DS    0H                                                  0417 01343000
@ER00002 BR    @14                                                 0417 01344000
*/* ******************************************************************/ 01345000
*/* THIS SEGMENT IS AN INTERNAL PROCEDURE CALLED BY THE LPRR SEGMENT */ 01346000
*/* ---SRMDRCOV--- AND PERFORMS THE FUNCTION OF RECORDING THE SDWA   */ 01347000
*/* INITIALIZED TO THE DESCRIBE THE FAILURE IN IEAVTRTS              */ 01348000
*/* INPUTS---                                                        */ 01349000
*/*  - TARGRTCA REGISTER(R5) POINTS TO THE SDWA TO BE RECORDED       */ 01350000
*/*  - R14 REGISTER CONTAINING RETURN ADDRESS OF CALLER              */ 01351000
*/*  - STAKPTR REGISTER(R7) POINTS TO INTERRUPTED FRR STACK AT THE   */ 01352000
*/*    IEAVTRTS FAILURE                                              */ 01353000
*/*  - WORKREG REGISTER(R4) USE AS A WORK REGISTER                   */ 01354000
*/* OUTPUTS---                                                       */ 01355000
*/*  - THE SDWA DESCRIBING THE IEAVTRTS ERROR IS RECORDED            */ 01356000
*/* NOTES-----                                                       */ 01357000
*/*  - REGS(R5,R7,R9,R10) RESTRICTED                                 */ 01358000
*/* ******************************************************************/ 01359000
*                                                                  0418 01360000
*RCRDRTCA:                                                         0418 01361000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                               0418 01362000
RCRDRTCA DS    0H                                                  0419 01363000
*   RFY                                                            0419 01364000
*     SDWA BASED(RT1WRTCA);         /* SDWA POINTER                  */ 01365000
*   RFY                                                            0420 01366000
*     FRRS BASED(R7);               /* INTERRUPTED FRR STACK PTR     */ 01367000
*   RFY                                                            0421 01368000
*    (R7,                                                          0421 01369000
*     R5,                                                          0421 01370000
*     R14,                                                         0421 01371000
*     R4) RSTD;                     /* PRESERVE THESE REGS           */ 01372000
*/* ******************************************************************/ 01373000
*/*     THIS SEGMENT INITIALIZES THE INPUT INTERFACE TO THE SOFTWARE */ 01374000
*/*     RECORDING FACILITY IN PREPARATION FOR RECORDING THE SDWA     */ 01375000
*/*     INPUT                                                        */ 01376000
*/*      - TARGRTCA REGISTER(R5) POINTS TO THE SDWA TO BE RECORDED   */ 01377000
*/*     OUTPUT                                                       */ 01378000
*/*      - INPARM REGISTER(R1)POINTS TO THE RECORDING PARAMETER LIST */ 01379000
*/*        PASSED TO THE SOFTWARE RECORDING FACILITY                 */ 01380000
*/*      - THE RECORDING PARAMETER LIST CONTAINS                     */ 01381000
*/*        .A RECORD TYPE INDICATION(PROG CHEK,MACH CHEK, OR RESTART)*/ 01382000
*/*        .NAME OF THE ASSOCIATED JOB                       @G17EP5W*/ 01383000
*/*        .ERRORID INFORMATION                              @G17EP5W*/ 01384000
*/*        .THE ADDRESS OF THE SDWA TO BE RECORDED                   */ 01385000
*/*      - RECLEN REGISTER(R3) CONTAINS THE LENGTH OF THE DATA TO BE */ 01386000
*/*        RECORDED(I.E.THE FIXED PORTION OF THE SDWA AND THE        */ 01387000
*/*        VARIABLE PORTION OF THE SDWA SUPPLIED BY THE FRR          */ 01388000
*/*      - SAVAREAP REGISTER(R13)POINTS TO THE REGISTER SAVE AREA TO */ 01389000
*/*        BE USED BY THE SOFTWARE RECORDING FACILITY TO SAVE SRMS   */ 01390000
*/*       REGISTERS                                                  */ 01391000
*/*     NOTES                                                        */ 01392000
*/*      - THE RECORDING PARAMETER LIST IS PART OF THE SDWA AND      */ 01393000
*/*        CONTAINS THE RECORD TYPE AND THE ADDRESS OF THE SDWA      */ 01394000
*/*      - REGS(R1,R13,R3) ARE THE INPUT REGS TO THE SOFTWARE        */ 01395000
*/*        RECORDING FACILITY                                        */ 01396000
*/*      - REGS(R14,R15) ARE LINKAGE REGISTERS TO THE SOFTWARE       */ 01397000
*/*       RECORDING FACILITY                                         */ 01398000
*/*  - THIS SEGMENT IS PART OF THE SRMDRCOV LPRR                     */ 01399000
*/* ******************************************************************/ 01400000
*                                                                  0422 01401000
*   DO;                                                            0422 01402000
*     SDWAVRA=DRCRDMSG;             /* PUT IEAVTRTS FAILURE MSG IN 0423 01403000
*                                      SDWA                          */ 01404000
         L     @15,RT1WRTCA(,R7)                                   0423 01405000
         MVI   SDWAVRA+32(@15),C' '                                0423 01406000
         MVC   SDWAVRA+33(75,@15),SDWAVRA+32(@15)                  0423 01407000
         MVC   SDWAVRA(32,@15),DRCRDMSG                            0423 01408000
*     SDWAURAL=LENGTH(DRCRDMSG);    /* SET IN SDWA -LENGTH OF THIS 0424 01409000
*                                      MSG                           */ 01410000
         MVI   SDWAURAL(@15),X'20'                                 0424 01411000
*     SDWAEBC=ON;                   /* DUMP VARIABLE RECORDING AREA     01412000
*                                      IN EBCDIC             @YM08142*/ 01413000
         OI    SDWAEBC(@15),B'01000000'                            0425 01414000
*     RCRDINFO=R5;                  /* INITIALIZE THE RECORDING    0426 01415000
*                                      PARAMETER LIST WITH THE ADDR     01416000
*                                      OF THE SDWA TO BE RECORDED    */ 01417000
         ST    R5,RCRDINFO(,@15)                                   0426 01418000
*     RFY                                                          0427 01419000
*       ASCB BASED(PSAAOLD);        /* ASCB ADDRESSABILTY    @G17EP5W*/ 01420000
*     RFY                                                          0428 01421000
*       MODEBYTE BASED(ADDR(RT1WMODE));/* MODE INFORMATION@G17EP5W   */ 01422000
*     IF((MODESRB=ON)|              /* IF SRB MODE OR        @G17EP5W*/ 01423000
*         (PSAAOLD->ASCBASCB^=NASCB)) THEN/* INVALID PSAAOLD       0429 01424000
*                                      PTR@G17EP5W                   */ 01425000
