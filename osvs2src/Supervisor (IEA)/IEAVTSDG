         TITLE 'IEAVTSDG-SVC DUMP GLOBAL STORAGE PROCESSING            *00001000
                        '                                               00002000
IEAVTSDG CSECT ,                                                   0001 00003000
@MAINENT BALR  @15,0                                               0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVTSDG  78.188'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @09,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@09                                         0001 00013000
*   ERRBASE1=R9;                    /* SAVE BASE REGISTER FOR      0087 00014000
*                                      RECOVERY                      */ 00015000
         ST    R9,ERRBASE1(,R3)                                    0087 00016000
*   SVCDMPNM=CSECTNAM;              /* SAVE MODULE NAME FOR RECOVERY    00017000
*                                      ROUTINE                       */ 00018000
         MVC   SVCDMPNM(8,R7),@CC03120                             0088 00019000
*   RFY                                                            0089 00020000
*     SDUMP BASED(SDWSDPL);         /* BASE MAPPINGS ON PARMLST    0089 00021000
*                                      ADDRESS IN SDWORK     @G33VPHD*/ 00022000
*   RFY                                                            0090 00023000
*     STORANGE BASED(RANGEPTR);     /* MAP ADDRESS RANGE TABLE       */ 00024000
*   PRDASID=COMNASID;               /* PUT COMMON AREA ASID IN HEADER   00025000
*                                      RECORD FOR PRDMP      @G33VPHD*/ 00026000
         L     @12,PRDINPTR(,R7)                                   0091 00027000
         MVC   PRDASID(2,@12),@CB03091                             0091 00028000
*   IF ERRENDMP='0'B THEN           /* IF NO ERRORS FOUND SO FAR     */ 00029000
         TM    ERRENDMP(R3),B'01000000'                            0092 00030000
         BNZ   @RF00092                                            0092 00031000
*     DO;                                                          0093 00032000
*                                                                  0093 00033000
*/********************************************************************/ 00034000
*/*   THIS SEGMENT BUILDS THE ADDRESS RANGE TABLE IN THE 4K SQA      */ 00035000
*/*   BUFFER.  THE ADDRESS RANGE TABLE IS DIVIDED INTO THREE PARTS   */ 00036000
*/*   THE FIRST PART IS COMPRISED OF ADDRESS RANGES FOR GLOBAL,      */ 00037000
*/*   VOLATILE STORAGES AREAS (SQA,CSA,PSA) AND AMDPRDMP REQUIRED    */ 00038000
*/*   CONTROL BLOCKS (CVT,ASVT,ASCB).  THE SECOND PART OF THE        */ 00039000
*/*   ADDRESS RANGE TABLE IS COMPRISED OF ADDRESS RANGES FOR LOCAL   */ 00040000
*/*   AREAS WITHIN THE PRIVATE AREA OF THE ADDRESS SPACE             */ 00041000
*/*   (RGN,LSQA,SWA) AND MODULES, SVCS IN THE LINK PACK AREA (LPA)   */ 00042000
*/*   THE THIRD PART OF THE ADDRESS RANGE TABLE CONTAINS ADDRESS     */ 00043000
*/*   RANGES FOR THE NUCLEUS.                                        */ 00044000
*/*                                                                  */ 00045000
*/*   INPUT: SDUMP PARM LIST, 4K SQA BUFFER, SYSTEM CONTROL BLOCKS   */ 00046000
*/*                                                                  */ 00047000
*/*   OUTPUT: ADDRESS RANGE TABLE AND RETURN CODE, NULL - ADDRESS    */ 00048000
*/*   RANGE TABLE COMPLETE, PARTDUMP - ADDRESS RANGE TABLE FILLED,   */ 00049000
*/*   PARTIAL DUMP WILL BE TAKEN.                                    */ 00050000
*/*   INCLUDED BY: TOP SEGMENT                                       */ 00051000
*/*                                                                  */ 00052000
*/********************************************************************/ 00053000
*                                                                  0094 00054000
*       FLGRTFUL='0'B;              /* IND ADDR RANGE TABLE NOT FUL  */ 00055000
         NI    FLGRTFUL(R7),B'01111111'                            0094 00056000
*       ERRRETRY=ADDR(RTYADR10);    /* NEXT RETRY LOCATION ADDRESS   */ 00057000
         LA    @12,RTYADR10                                        0095 00058000
         ST    @12,ERRRETRY(,R3)                                   0095 00059000
*       IF SVCDSQA='1'B|SVCDCSA='1'B THEN/* IF SQA OR CSA TO BE    0096 00060000
*                                      DUMPED                        */ 00061000
         TM    SVCDSQA(R7),B'00010000'                             0096 00062000
         BO    @RT00096                                            0096 00063000
         TM    SVCDCSA(R7),B'10000000'                             0096 00064000
         BNO   @RF00096                                            0096 00065000
@RT00096 DS    0H                                                  0097 00066000
*         DO;                                                      0097 00067000
*                                                                  0098 00068000
*/********************************************************************/ 00069000
*/*   THIS SEGMENT WILL FIX THE SVC DUMP MODULE, IEAVAD00, AND       */ 00070000
*/*   THE SVC DUMP WORK AREA IN REAL STORAGE.  THE SALLOC LOCK IS    */ 00071000
*/*   OBTAINED WHEN SQA OR CSA IS TO BE INCLUDED IN THE DUMP.        */ 00072000
*/*   REFERENCED PAGES ARE FIXED TO PREVENT A PAGE FAULT.            */ 00073000
*/*                                                                  */ 00074000
*/*   INPUT: NO LOCKS HELD                                           */ 00075000
*/*                                                                  */ 00076000
*/*   OUTPUT: FIXED PAGES, INDICATOR IN ERROR WORK AREA              */ 00077000
*/*                                                                  */ 00078000
*/*   INCLUDED BY: SDADDR                                            */ 00079000
*/********************************************************************/ 00080000
*                                                                  0098 00081000
*           RFY                                                    0098 00082000
*            (R0,                                                  0098 00083000
*             R1,                                                  0098 00084000
*             R2,                                                  0098 00085000
*             R1) RSTD;                                            0098 00086000
*           R0=ADDR(SDECB);         /* ADDR OF ECB, POSTED BY PGFIX  */ 00087000
         LR    R0,R7                                               0099 00088000
*           SDECB=NULL;             /* CLEAR ECB FOR WAIT            */ 00089000
         SLR   @12,@12                                             0100 00090000
         ST    @12,SDECB(,R7)                                      0100 00091000
*           R1=ADDR(IEAVTSDG);      /* START ADDRESS TO FIX          */ 00092000
         L     R1,@CA00028                                         0101 00093000
*           R2=NULL;                /* NO END ADDRESS TO FIX         */ 00094000
         SLR   R2,R2                                               0102 00095000
*           GEN;                                                   0103 00096000
         PGFIX R,LONG=Y,RELEASE=N,                                     X00097000
               A=(R1),ECB=(R0)                                          00098000
*           R1=ADDR(SDECB);         /* ADDRESS OF ECB TO WAIT ON     */ 00099000
         LR    R1,R7                                               0104 00100000
*           GEN(WAIT  1,ECB=(R1));  /* WAIT FOR PAGES TO BE FIXED    */ 00101000
         WAIT  1,ECB=(R1)                                               00102000
*           ERRSDFIX='1'B;          /* INDICATE MODULE PAGES FIXED   */ 00103000
         OI    ERRSDFIX(R3),B'00100000'                            0106 00104000
*           IF SDUABEND='0'B THEN   /* IF THIS NOT A SYSMDUMP REQUEST*/ 00105000
         L     @12,SDWSDPL(,R7)                                    0107 00106000
         TM    SDUABEND(@12),B'01000000'                           0107 00107000
         BNZ   @RF00107                                            0107 00108000
*             DO;                   /* PGFIX WORK AREA IN ONE PIECE     00109000
*                                      OF STORAGE                    */ 00110000
*               SDECB=NULL;         /* CLEAR ECB AFTER IT IS POSTED  */ 00111000
         SLR   @12,@12                                             0109 00112000
         ST    @12,SDECB(,R7)                                      0109 00113000
*               R0=ADDR(SDECB);     /* ADDR OF ECB, POSTED BY PGFIX  */ 00114000
         LR    R0,R7                                               0110 00115000
*               R1=R7;              /* START ADDRESS TO FIX  @G33SPHD*/ 00116000
         LR    R1,R7                                               0111 00117000
*               R2=R7+SDWORKLN;     /* END ADDRESS TO FIX    @G33SPHD*/ 00118000
         LR    R2,R7                                               0112 00119000
         AL    R2,SDWORKLN                                         0112 00120000
*               ERRWKFIX='1'B;      /* INDICATE WORKAREA FIXED       */ 00121000
         OI    ERRWKFIX(R3),B'00010000'                            0113 00122000
*               GEN;                                               0114 00123000
         PGFIX R,LONG=Y,RELEASE=N,                                     X00124000
               A=(R1),EA=(R2),ECB=(R0)                                  00125000
*               R1=ADDR(SDECB);     /* ADDRESS OF ECB TO WAIT ON     */ 00126000
         LR    R1,R7                                               0115 00127000
*               GEN(WAIT  1,ECB=(R1));/* WAIT FOR PAGES TO BE FIXED  */ 00128000
         WAIT  1,ECB=(R1)                                               00129000
*               SDECB=NULL;         /* CLEAR ECB AFTER IT IS POSTED  */ 00130000
         SLR   @12,@12                                             0117 00131000
         ST    @12,SDECB(,R7)                                      0117 00132000
*             END;                  /* END OF THIS IS NOT A SYSMDUMP    00133000
*                                      REQUEST                       */ 00134000
*           ELSE                                                   0119 00135000
*             DO;                   /* THIS IS A SYSMDUMP REQUEST SO    00136000
*                                      PGFIX BOTH AREAS              */ 00137000
         B     @RC00107                                            0119 00138000
@RF00107 DS    0H                                                  0120 00139000
*               SDECB=NULL;         /* CLEAR ECB AFTER IT IS POSTED  */ 00140000
         SLR   @12,@12                                             0120 00141000
         ST    @12,SDECB(,R7)                                      0120 00142000
*               R0=ADDR(SDECB);     /* ADDR OF ECB, POSTED BY PGFIX  */ 00143000
         LR    R0,R7                                               0121 00144000
*               R1=R7;              /* START ADDRESS TO FIX  @G33SPHD*/ 00145000
         LR    R1,R7                                               0122 00146000
*               R2=R7+LENGTH(SDCNTRL);/* END ADDRESS TO FIX          */ 00147000
         LA    R2,432                                              0123 00148000
         ALR   R2,R7                                               0123 00149000
*               ERRWKFIX='1'B;      /* INDICATE WORKAREA FIXED       */ 00150000
         OI    ERRWKFIX(R3),B'00010000'                            0124 00151000
*               GEN;                                               0125 00152000
         PGFIX R,LONG=Y,RELEASE=N,                                     X00153000
               A=(R1),EA=(R2),ECB=(R0)                                  00154000
*               R1=ADDR(SDECB);     /* ADDRESS OF ECB TO WAIT ON     */ 00155000
         LR    R1,R7                                               0126 00156000
*               GEN(WAIT  1,ECB=(R1));/* WAIT FOR PAGES TO BE FIXED  */ 00157000
         WAIT  1,ECB=(R1)                                               00158000
*               SDECB=NULL;         /* CLEAR ECB AFTER IT IS POSTED  */ 00159000
         SLR   @12,@12                                             0128 00160000
         ST    @12,SDECB(,R7)                                      0128 00161000
*               R0=ADDR(SDECB);     /* ADDR OF ECB, POSTED BY PGFIX  */ 00162000
         LR    R0,R7                                               0129 00163000
*               R1=SDSMBUFF;        /* START ADDRESS TO FIX  @G33SPHD*/ 00164000
         L     @12,SDSMBUFF(,R7)                                   0130 00165000
         LR    R1,@12                                              0130 00166000
*               R2=SDSMBUFF+LENGTH(SDBUFFER);/* END ADDRESS TO FIX   */ 00167000
         AL    @12,@CF03032                                        0131 00168000
         LR    R2,@12                                              0131 00169000
*               GEN;                                               0132 00170000
         PGFIX R,LONG=Y,RELEASE=N,                                     X00171000
               A=(R1),EA=(R2),ECB=(R0)                                  00172000
*               R1=ADDR(SDECB);     /* ADDRESS OF ECB TO WAIT ON     */ 00173000
         LR    R1,R7                                               0133 00174000
*               GEN(WAIT  1,ECB=(R1));/* WAIT FOR PAGES TO BE FIXED  */ 00175000
         WAIT  1,ECB=(R1)                                               00176000
*               SDECB=NULL;         /* CLEAR ECB AFTER IT IS POSTED  */ 00177000
         SLR   @12,@12                                             0135 00178000
         ST    @12,SDECB(,R7)                                      0135 00179000
*             END;                  /* END OF THIS IS A SYSMDUMP   0136 00180000
*                                      REQUEST                       */ 00181000
*           RFY                                                    0137 00182000
*            (R0,                                                  0137 00183000
*             R1,                                                  0137 00184000
*             R2,                                                  0137 00185000
*             R1) UNRSTD;                                          0137 00186000
@RC00107 DS    0H                                                  0138 00187000
*         END;                                                     0138 00188000
*       ELSE                                                       0139 00189000
*         ;                                                        0139 00190000
@RF00096 DS    0H                                                  0140 00191000
*RTYADR10:                                                         0140 00192000
*       ERRRETRY=ADDR(RTYENDMP);    /* NEXT RETRY LOCATION ADDRESS   */ 00193000
RTYADR10 LA    @12,RTYENDMP                                        0140 00194000
         ST    @12,ERRRETRY(,R3)                                   0140 00195000
*       ERRLLOCK='1'B;              /* INDICATE LOCAL LOCK HELD      */ 00196000
         OI    ERRLLOCK(R3),B'00000010'                            0141 00197000
*       GEN REFS(PSALITA);                                         0142 00198000
GETLOCK2 SETLOCK   OBTAIN,TYPE=LOCAL,MODE=UNCOND,                      X00199000
               RELATED=(SVCDUMP,IEAVTSDG(FRELOCK2))                     00200000
*       RFY                                                        0143 00201000
*        (R10,                                                     0143 00202000
*         R11) RSTD;                                               0143 00203000
*       ERRFRR='1'B;                /* INDICATE FRR PROTECT ACTIVE   */ 00204000
         OI    ERRFRR(R3),B'10000000'                              0144 00205000
*       GEN(SETFRR A,FRRAD=ADDRFRR,WRKREGS=(R10,R11),PARMAD=(R3))  0145 00206000
*       REFS(ADDRFRR,PSACSTK,FRRSCURR,FRRSLAST,PSALSFCC,FRRS,FRRSPARM,  00207000
*           FRRSFRRA,FRRSENTR,PSA);                                0145 00208000
         SETFRR A,FRRAD=ADDRFRR,WRKREGS=(R10,R11),PARMAD=(R3)           00209000
*       RFY                                                        0146 00210000
*        (R10,                                                     0146 00211000
*         R11) UNRSTD;                                             0146 00212000
*       ERRWORK=ERRORWK;            /* STATUS FOR FRR ERROR EXIT     */ 00213000
         MVC   ERRWORK(24,R3),ERRORWK(R7)                          0147 00214000
*                                                                  0148 00215000
*/********************************************************************/ 00216000
*/*   THIS SEGMENT BUILDS THE GLOBAL PART OF THE ADDRESS RANGE       */ 00217000
*/*   TABLE.  THE GLOBAL PART IS COMPRISED OF ADDRESS RANGES FOR     */ 00218000
*/*   SQA, CSA, PSA, CVT, ASCB, ASVT AND SUPERVISOR TRACE TABLE.     */ 00219000
*/*                                                                  */ 00220000
*/*   INPUT: SDUMP PARM LIST, RANGEPTR-CONTAINING ADDRESS OF NEXT    */ 00221000
*/*          AVAILABLE SLOT IN ADDRESS RANGE TABLE,                  */ 00222000
*/*          GLOBADDR CONTAINS ADDRESS BEGINNING OF GLOBAL PART OF   */ 00223000
*/*          ADDRESS RANGE TABLE.                                    */ 00224000
*/*                                                                  */ 00225000
*/*   OUTPUT: GLOBAL PART OF ADDRESS RANGE TABLE CONTAINS ADDRESS    */ 00226000
*/*           RANGES FOR REQUESTED GLOBAL, VOLATILE AREAS            */ 00227000
*/*           OF THE SYSTEM                                          */ 00228000
*/********************************************************************/ 00229000
*                                                                  0148 00230000
*       ERRRETRY=ADDR(RTYGLO10);    /* NEXT RETRY LOCATION ADDRESS   */ 00231000
         LA    @12,RTYGLO10                                        0148 00232000
         ST    @12,ERRRETRY(,R3)                                   0148 00233000
*       IF ERRSDFIX='1'B&           /* IF THIS MODULE AND THE SVC  0149 00234000
*                                      DUMP                          */ 00235000
*           ERRWKFIX='1'B THEN      /* WORK AREAS ARE FIXED IN REAL     00236000
*                                      STORAGE. IT IS SAFE TO OBTAIN    00237000
*                                      THE SALLOC LOCK               */ 00238000
         TM    ERRSDFIX(R3),B'00110000'                            0149 00239000
         BNO   @RF00149                                            0149 00240000
*         DO;                       /* OBTAIN THE SALLOC LOCK        */ 00241000
*           GEN REFS(PSALITA);                                     0151 00242000
GETLOCKD SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                       X00243000
               RELATED=(SVCDUMP,IEAVAD00(FRELOCKD,FRELOCKE))            00244000
*           ERRSLOCK='1'B;          /* INDICATE SALLOC LOCK HELD     */ 00245000
         OI    ERRSLOCK(R3),B'00000001'                            0152 00246000
*         END;                                                     0153 00247000
*                                                                  0153 00248000
*       /*************************************************************/ 00249000
*       /*                                                           */ 00250000
*       /* PUT SQA ADDRESS RANGES IN THE ADDRESS RANGE TABLE         */ 00251000
*       /*                                                           */ 00252000
*       /*************************************************************/ 00253000
*                                                                  0154 00254000
*RTYGLO10:                                                         0154 00255000
*       ERRRETRY=ADDR(RTYGLO30);    /* NEXT RETRY LOCATION ADDRESS   */ 00256000
@RF00149 DS    0H                                                  0154 00257000
RTYGLO10 LA    @12,RTYGLO30                                        0154 00258000
         ST    @12,ERRRETRY(,R3)                                   0154 00259000
*       IF FLGRTFUL='0'B&           /* IF RANGE TABLE NOT FULL AND   */ 00260000
*           SVCDNSQA='0'B THEN      /* SQA IS TO BE DUMPED           */ 00261000
         TM    FLGRTFUL(R7),B'10000000'                            0155 00262000
         BNZ   @RF00155                                            0155 00263000
         TM    SVCDNSQA(R7),B'00000100'                            0155 00264000
         BNZ   @RF00155                                            0155 00265000
*         DO;                                                      0156 00266000
*           SPQEADDR=SQASPQEP;      /* ADDR OF FIRST SQA SPQE        */ 00267000
         L     @12,CVTPTR                                          0157 00268000
         L     @12,GDAPTR(,@12)                                    0157 00269000
         L     @12,SQASPQEP(,@12)                                  0157 00270000
         ST    @12,SPQEADDR(,R7)                                   0157 00271000
*           CALL SDSPQE;            /* ADD SQA RANGES TO RANGE TABLE */ 00272000
         BAL   @14,SDSPQE                                          0158 00273000
*         END;                                                     0159 00274000
*                                                                  0159 00275000
*       /*************************************************************/ 00276000
*       /*                                                           */ 00277000
*       /* PUT CSA STORAGE ADDRESSES IN THE ADDRESS RANGE TABLE      */ 00278000
*       /*                                                           */ 00279000
*       /*************************************************************/ 00280000
*                                                                  0160 00281000
*RTYGLO30:                                                         0160 00282000
*       ERRRETRY=ADDR(RTYGLO40);    /* NEXT RETRY LOCATION ADDRESS   */ 00283000
@RF00155 DS    0H                                                  0160 00284000
RTYGLO30 LA    @12,RTYGLO40                                        0160 00285000
         ST    @12,ERRRETRY(,R3)                                   0160 00286000
*       IF FLGRTFUL='0'B&           /* IF RANGE TABLE NOT FULL AND   */ 00287000
*           SVCDCSA='1'B THEN       /* CSA IS TO BE DUMPED           */ 00288000
         TM    FLGRTFUL(R7),B'10000000'                            0161 00289000
         BNZ   @RF00161                                            0161 00290000
         TM    SVCDCSA(R7),B'10000000'                             0161 00291000
         BNO   @RF00161                                            0161 00292000
*         DO;                                                      0162 00293000
*           SPQEADDR=CSASPQEP;      /* ADDR OF FIRST CSA SPQE        */ 00294000
         L     @12,CVTPTR                                          0163 00295000
         L     @12,GDAPTR(,@12)                                    0163 00296000
         L     @12,CSASPQEP(,@12)                                  0163 00297000
         ST    @12,SPQEADDR(,R7)                                   0163 00298000
*           CALL SDSPQE;            /* ADD CSA RANGES TO RANGE TABLE */ 00299000
         BAL   @14,SDSPQE                                          0164 00300000
*         END;                                                     0165 00301000
*                                                                  0165 00302000
*       /*************************************************************/ 00303000
*       /*                                                           */ 00304000
*       /* PUT SUPERVISOR TRACE TABLE ADDRESSES IN ADDRESS RANGE     */ 00305000
*       /* TABLE IF TRACE IS ACTIVE                                  */ 00306000
*       /*                                                           */ 00307000
*       /*************************************************************/ 00308000
*                                                                  0166 00309000
*RTYGLO40:                                                         0166 00310000
*       ERRRETRY=ADDR(RTYGLO50);    /* NEXT RETRY LOCATION ADDRESS   */ 00311000
@RF00161 DS    0H                                                  0166 00312000
RTYGLO40 LA    @12,RTYGLO50                                        0166 00313000
         ST    @12,ERRRETRY(,R3)                                   0166 00314000
*       IF FLGRTFUL='0'B&           /* IF RANGE TABLE NOT FULL AND   */ 00315000
*           SVCDTRT='1'B&           /* TRACE DATA TO BE DUMPED AND   */ 00316000
*           CVTSDTRC='1'B THEN      /* SVC DUMP SET SUPERVISOR TRACE    00317000
*                                      INACTIVE EARLIER      @ZA19976*/ 00318000
         TM    FLGRTFUL(R7),B'10000000'                            0167 00319000
         BNZ   @RF00167                                            0167 00320000
         TM    SVCDTRT(R7),B'00000001'                             0167 00321000
         BNO   @RF00167                                            0167 00322000
         L     @12,CVTPTR                                          0167 00323000
         TM    CVTSDTRC(@12),B'00000001'                           0167 00324000
         BNO   @RF00167                                            0167 00325000
*         DO;                                                      0168 00326000
*           STARTADR=FLCTRACE;      /* START ADDRESS OF TRACE TABLE  */ 00327000
         L     @12,FLCTRACE                                        0169 00328000
