@DL00469 DS    0H                                                  0470 01497000
*           R10=R10+LEN1ADRN;       /* ADDR OF SLOT TO FILL          */ 01498000
         AL    R10,@CF00041                                        0470 01499000
*           DO WHILE(R11->ENDADDR=NULL);/* FIND NEXT ^ ZERO SLOT     */ 01500000
         B     @DE00471                                            0471 01501000
@DL00471 DS    0H                                                  0472 01502000
*             R11=R11+LEN1ADRN;                                    0472 01503000
         AL    R11,@CF00041                                        0472 01504000
*           END;                                                   0473 01505000
@DE00471 L     @12,ENDADDR(,R11)                                   0473 01506000
         LTR   @12,@12                                             0473 01507000
         BZ    @DL00471                                            0473 01508000
*           R10->STORANGE=R11->STORANGE;/* MOVE GOOD RANGE UP        */ 01509000
