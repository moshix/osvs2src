         BZ    @DL00469                                            0476 01515000
*       END;                                                       0477 01516000
*   END;                                                           0478 01517000
@RC00464 DS    0H                                                  0478 01518000
@DE00463 TM    LASTRNGE(R10),B'10000000'                           0478 01519000
         BZ    @DL00463                                            0478 01520000
*   RANGEPTR=R10;                   /* ADDR OF LAST RANGE IN PART    */ 01521000
         ST    R10,RANGEPTR(,R7)                                   0479 01522000
*   IF RANGEPTR<(ENDADRNT-LEN1ADRN) THEN/* IF ADDR RANGE TAB NOT FULL*/ 01523000
         L     @12,RANGEPTR(,R7)                                   0480 01524000
