*   END;                            /* END SEARCH ALL USER STORAGE 0351 01035000
*                                      RANGES                        */ 01036000
@RC00348 DS    0H                                                  0351 01037000
@DE00331 LTR   R10,R10                                             0351 01038000
         BNZ   @DL00331                                            0351 01039000
*   R14=USERRETA;                   /* RESTORE RETURN ADDRESS        */ 01040000
         L     R14,USERRETA(,R7)                                   0352 01041000
*   END USERNGES;                                                  0353 01042000
@EL00003 DS    0H                                                  0353 01043000
@EF00003 DS    0H                                                  0353 01044000
@ER00003 BR    @14                                                 0353 01045000
*                                                                  0354 01046000
*/********************************************************************/ 01047000
*/*   THIS SEGMENT WILL SET THE SYSTEM DISPATCHABLE BY BRANCH        */ 01048000
*/*   ENTRY TO STATUS AND INFORM THE SYS RES MGR THAT THE ADDRESS    */ 01049000
*/*   SPACE CAN BE SWAPPED.                                          */ 01050000
*/*                                                                  */ 01051000
*/*   INPUT: LOCAL LOCK IS HELD                                      */ 01052000
*/*                                                                  */ 01053000
*/*   OUTPUT: SYSTEM IS SET DISPATCHABLE, ADDRESS SPACE CAN BE       */ 01054000
*/*           SWAPPED                                                */ 01055000
*/*                                                                  */ 01056000
*/*   CALLED BY: SDUMP                                               */ 01057000
*/********************************************************************/ 01058000
*                                                                  0354 01059000
*SETSDISP:                                                         0354 01060000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0354 01061000
SETSDISP DS    0H                                                  0355 01062000
*   DISPRETA=R14;                   /* SAVE RETURN ADDRESS           */ 01063000
         ST    R14,DISPRETA(,R7)                                   0355 01064000
*   RFY                                                            0356 01065000
*    (R0,                                                          0356 01066000
*     R1) RSTD;                                                    0356 01067000
*   R0=SYSDISP;                     /* SYSTEM DISPATCHABILITY CODE   */ 01068000
         LA    R0,9                                                0357 01069000
*   R1=NULL|HIGHBIT1;               /* NO TCB ADDR, RESET FUNCTION   */ 01070000
         L     R1,@CF03228                                         0358 01071000
*   R13=SDISPFLG;                   /* SVC DUMP DISPATCHABILITY FLG  */ 01072000
         L     R13,@CF03106                                        0359 01073000
*   CALL STATUS;                    /* SET SYSTEM DISPATCHABLE       */ 01074000
         L     @12,CVTPTR                                          0360 01075000
         L     @12,SCVTSPTR(,@12)                                  0360 01076000
         L     @15,SCVTSTAT(,@12)                                  0360 01077000
         BALR  @14,@15                                             0360 01078000
*   RTCTSDND='0'B;                  /* INDICATE SYSTEM IS          0361 01079000
*                                      DISPATCHABLE          @ZA04148*/ 01080000
         L     @15,CVTPTR                                          0361 01081000
         L     @15,CVTRTMCT(,@15)                                  0361 01082000
         NI    RTCTSDND(@15),B'10111111'                           0361 01083000
*   IF RTCTSDFX='1'B THEN           /* ALLOW FOR ASYNCRONOUS @ZA27434*/ 01084000
         TM    RTCTSDFX(@15),B'00000001'                           0362 01085000
         BNO   @RF00362                                            0362 01086000
*     DO;                           /* INTERRUPTS.           @ZA27434*/ 01087000
*       PSATOLD->TCBFX='0'B;        /*                       @ZA27434*/ 01088000
         L     @12,PSATOLD                                         0364 01089000
         NI    TCBFX(@12),B'11111110'                              0364 01090000
*       RTCTSDFX='0'B;              /*                       @ZA27434*/ 01091000
         NI    RTCTSDFX(@15),B'11111110'                           0365 01092000
*     END;                          /*                       @ZA27434*/ 01093000
*   ELSE                                                           0367 01094000
*     ;                             /*                       @ZA27434*/ 01095000
@RF00362 DS    0H                                                  0368 01096000
*   RFY                                                            0368 01097000
*    (R0,                                                          0368 01098000
*     R1) UNRSTD;                                                  0368 01099000
*   R13=ADDR(SDWRGSV2);             /* SAVE AREA FOR SYS RES MGR     */ 01100000
         LA    R13,SDWRGSV2(,R7)                                   0369 01101000
*   GEN REFS(CVT,CVTOPTE)(SYSEVENT OKSWAP,ENTRY=BRANCH);/* TELL SYS     01102000
*                                      RESOURCE MNGR OK TO SWAP A. S.*/ 01103000
         SYSEVENT OKSWAP,ENTRY=BRANCH                                   01104000
*   R14=DISPRETA;                   /* RESTORE RETURN ADDRESS        */ 01105000
         L     R14,DISPRETA(,R7)                                   0371 01106000
*   END SETSDISP;                                                  0372 01107000
@EL00004 DS    0H                                                  0372 01108000
@EF00004 DS    0H                                                  0372 01109000
@ER00004 BR    @14                                                 0372 01110000
*                                                                  0373 01111000
*/********************************************************************/ 01112000
*/*   THIS SEGMENT WILL CONVERT SUBPOOLS TO ADDRESS RANGES AND       */ 01113000
*/*   CALL SDADDADR TO INSERT EACH ADDRESS RANGE INTO THE ADDRESS    */ 01114000
*/*   RANGE TABLE.  SUBPOOLS ARE DEFINED BY VSM CONTROL BLOCKS       */ 01115000
*/*   SPQE AND DQE.                                                  */ 01116000
*/*                                                                  */ 01117000
*/*   INPUT: SPQEADDR CONTAINS ADDRESS OF FIRST SPQE ON SPQE CHAIN   */ 01118000
*/*   TO SEARCH                                                      */ 01119000
*/*                                                                  */ 01120000
*/*   OUTPUT: ADDRESS RANGES ADDED TO ADDRESS RANGE TABLE            */ 01121000
*/*                                                                  */ 01122000
*/*   CALLED BY: SDGLOBAL,SDLOCAL,SDTCB                              */ 01123000
*/********************************************************************/ 01124000
*                                                                  0373 01125000
*SDSPQE:                                                           0373 01126000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0373 01127000
SDSPQE   DS    0H                                                  0374 01128000
*   SPQERETA=R14;                   /* SAVE RETURN ADDRESS           */ 01129000
         ST    R14,SPQERETA(,R7)                                   0374 01130000
*   RFY                                                            0375 01131000
*    (R10,                                                         0375 01132000
*     R11,                                                         0375 01133000
*     R4,                                                          0375 01134000
*     R5) RSTD;                                                    0375 01135000
*   RFY                                                            0376 01136000
*     SPQESECT BASED(R4);                                          0376 01137000
*   RFY                                                            0377 01138000
*     DQESECT BASED(R5);                                           0377 01139000
*   R4=SPQEADDR;                    /* ADDR OF FIRST SPQE ON CHAIN   */ 01140000
*                                                                  0378 01141000
         L     R4,SPQEADDR(,R7)                                    0378 01142000
*   /*****************************************************************/ 01143000
*   /*                                                               */ 01144000
*   /* SEARCH ALL SPQE'S FOR THIS SUBPOOL FINDING STORAGE TO DUMP    */ 01145000
*   /*                                                               */ 01146000
*   /*****************************************************************/ 01147000
*                                                                  0379 01148000
*   DO WHILE R4^=NULL;              /* SEARCH ALL SPQE ON CHAIN      */ 01149000
         B     @DE00379                                            0379 01150000
@DL00379 DS    0H                                                  0380 01151000
*     R5=SPDQEAD;                   /* ADDRESS OF FIRST DQE OFF SPQE */ 01152000
*                                                                  0380 01153000
         L     R5,SPDQEAD(,R4)                                     0380 01154000
*     /***************************************************************/ 01155000
*     /*                                                             */ 01156000
*     /* SEARCH EACH DQE AND PUT ADDRESSES IN THE ADDRESS RANGE TABLE*/ 01157000
*     /*                                                             */ 01158000
*     /***************************************************************/ 01159000
*                                                                  0381 01160000
*     DO WHILE R5^=NULL;            /* SEARCH ALL DQES HUNG FROM SPQE*/ 01161000
         B     @DE00381                                            0381 01162000
@DL00381 DS    0H                                                  0382 01163000
*       STARTADR=DQEBLKAD;          /* START ADDRESS FOR RANGE       */ 01164000
         L     @15,RANGEPTR(,R7)                                   0382 01165000
         L     @12,DQEBLKAD(,R5)                                   0382 01166000
         ST    @12,STARTADR(,@15)                                  0382 01167000
*       ENDADDR=STARTADR+DQELNTH;   /* END ADDRESS FOR RANGE         */ 01168000
         AL    @12,DQELNTH(,R5)                                    0383 01169000
         ST    @12,ENDADDR(,@15)                                   0383 01170000
*       CALL SDADDADR;              /* ADD RANGE TO ADDR RANGE TABLE */ 01171000
         BAL   @14,SDADDADR                                        0384 01172000
*       IF FLGRTFUL='0'B THEN       /* IF RANGE TABLE NOT FULL,      */ 01173000
         TM    FLGRTFUL(R7),B'10000000'                            0385 01174000
         BNZ   @RF00385                                            0385 01175000
*         R5=DQEPTR;                /* GET ADDRESS OF NEXT DQE       */ 01176000
         L     R5,DQEPTR(,R5)                                      0386 01177000
*       ELSE                        /* RANGE TABLE FULL, END LOOP    */ 01178000
*         R5=NULL;                  /* END DQE CHAIN SEARCH          */ 01179000
         B     @RC00385                                            0387 01180000
@RF00385 SLR   R5,R5                                               0387 01181000
*     END;                          /* END SEARCH ALL DQES ON CHAIN  */ 01182000
@RC00385 DS    0H                                                  0388 01183000
@DE00381 LTR   R5,R5                                               0388 01184000
         BNZ   @DL00381                                            0388 01185000
*     IF FLGRTFUL='0'B THEN         /* IF ADDR RANGE TABLE NOT FULL, */ 01186000
         TM    FLGRTFUL(R7),B'10000000'                            0389 01187000
         BNZ   @RF00389                                            0389 01188000
*       R4=SPQEPTR;                 /* GET ADDRESS OF NEXT SPQE      */ 01189000
         L     R4,SPQEPTR(,R4)                                     0390 01190000
*     ELSE                          /* RANGE TABLE FULL, END LOOP    */ 01191000
*       R4=NULL;                    /* END SPQE CHAIN SEARCH         */ 01192000
         B     @RC00389                                            0391 01193000
@RF00389 SLR   R4,R4                                               0391 01194000
*   END;                            /* END SEARCH ALL SPQES IN SUB 0392 01195000
*                                      POOL                          */ 01196000
@RC00389 DS    0H                                                  0392 01197000
@DE00379 LTR   R4,R4                                               0392 01198000
         BNZ   @DL00379                                            0392 01199000
*   RFY                                                            0393 01200000
*    (SPQESECT,                                                    0393 01201000
*     DQESECT) BASED;                                              0393 01202000
*   RFY                                                            0394 01203000
*    (R10,                                                         0394 01204000
*     R11,                                                         0394 01205000
*     R4,                                                          0394 01206000
*     R5) UNRSTD;                                                  0394 01207000
*   R14=SPQERETA;                   /* RESTORE RETURN ADDRESS        */ 01208000
         L     R14,SPQERETA(,R7)                                   0395 01209000
*   END SDSPQE;                                                    0396 01210000
@EL00005 DS    0H                                                  0396 01211000
@EF00005 DS    0H                                                  0396 01212000
@ER00005 BR    @14                                                 0396 01213000
*                                                                  0397 01214000
*/********************************************************************/ 01215000
*/*   THIS SEGMENT WILL ADD AN ADDRESS RANGE TO THE ADDRESS RANGE    */ 01216000
*/*   TABLE.  THE ADDRESSES TO BE ADDED ARE IN STARTADR AND          */ 01217000
*/*   ENDADDR (IN THE ADDRESS RANGE TABLE).  IF THEIR IS ROOM IN     */ 01218000
*/*   THE ADDRESS RANGE TABLE, THE START ADDRESS WILL BE ROUNDED     */ 01219000
*/*   DOWN TO A 4K BOUNDARY AND THE END ADDRESS WILL BE ROUNDED UP   */ 01220000
*/*   TO A 4K BOUNDARY.  IF THEIR IS NO MORE ROOM IN THE ADDRESS     */ 01221000
*/*   RANGE TABLE, A RETURN CODE OF PARTDUMP WILL BE STORED IN       */ 01222000
*/*   SDRETCOD.  IF THE START ADDRESS IS GREATER THAN THE END        */ 01223000
*/*   ADDRESS A C0D ABEND WILL BE ISSUED.                            */ 01224000
*/*                                                                  */ 01225000
*/*   INPUT: RANGEPTR CONTAINS ADDRESS OF NEXT AVAILABLE SLOT IN     */ 01226000
*/*   ADDRESS RANGE TABLE, ENDADRNT CONTAINS ADDRESS OF LAST SLOT    */ 01227000
*/*   IN ADDRESS RANGE TABLE, STARTADR CONTAINS BEG ADDRESS OF       */ 01228000
*/*   RANGE, ENDADDR CONTAINS END ADDRESS OF RANGE                   */ 01229000
*/*                                                                  */ 01230000
*/*   OUTPUT: RANGE ADDED TO ADDRESS RANGE TABLE, RANGEPTR           */ 01231000
*/*   CONTAINS ADDRESS OF NEXT AVAILABLE SLOT OR RETURN CODE OF      */ 01232000
*/*   PARTDUMP OR C0D ABEND                                          */ 01233000
*/*                                                                  */ 01234000
*/*   CALLED BY: SDALLPSA,USERNGES,SDSPQE,SDGLOBAL,SDNUC,            */ 01235000
*/*                                                                  */ 01236000
*/********************************************************************/ 01237000
*                                                                  0397 01238000
*SDADDADR:                                                         0397 01239000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0397 01240000
SDADDADR DS    0H                                                  0398 01241000
*   RFY                                                            0398 01242000
*    (R10,                                                         0398 01243000
*     R11,                                                         0398 01244000
*     R4,                                                          0398 01245000
*     R5,                                                          0398 01246000
*     R14) RSTD;                                                   0398 01247000
*   ADDADRWA=R10;                   /* SAVE WORK REGISTER            */ 01248000
         ST    R10,ADDADRWA(,R7)                                   0399 01249000
*   ADDADRWB=R11;                   /* SAVE WORK REGISTER            */ 01250000
         ST    R11,ADDADRWB(,R7)                                   0400 01251000
*   ADDADRWC=R4;                    /* SAVE WORK REGISTER            */ 01252000
         ST    R4,ADDADRWC(,R7)                                    0401 01253000
*   ADDADRWD=R5;                    /* SAVE WORK REGISTER            */ 01254000
         ST    R5,ADDADRWD(,R7)                                    0402 01255000
*   RFY                                                            0403 01256000
*    (R10,                                                         0403 01257000
*     R11,                                                         0403 01258000
*     R4,                                                          0403 01259000
*     R5) UNRSTD;                                                  0403 01260000
*   IF RANGEPTR<(ENDADRNT-LEN1ADRN) THEN/* IF RANGE TABLE NOT FULL   */ 01261000
         L     @15,RANGEPTR(,R7)                                   0404 01262000
         L     @12,ENDADRNT(,R7)                                   0404 01263000
         SL    @12,@CF00041                                        0404 01264000
         CR    @15,@12                                             0404 01265000
         BNL   @RF00404                                            0404 01266000
*     DO;                                                          0405 01267000
*       STARTADR=STARTADR&FOURKBDY; /* ROUND START ADDR DOWN-4K BDY  */ 01268000
         L     @12,@CF03112                                        0406 01269000
         L     @08,STARTADR(,@15)                                  0406 01270000
         NR    @08,@12                                             0406 01271000
         ST    @08,STARTADR(,@15)                                  0406 01272000
*       ENDADDR=ENDADDR&HIGHBIT0;   /* END ADDRESS IS AN ADDRESS     */ 01273000
         L     @08,ENDADDR(,@15)                                   0407 01274000
         N     @08,@CF03099                                        0407 01275000
         ST    @08,ENDADDR(,@15)                                   0407 01276000
*       IF(ENDADDR//FOURK)^=NULL THEN/* IF END ADDR NOT ON 4K BDY,   */ 01277000
         LR    @00,@08                                             0408 01278000
         SRDA  @00,32                                              0408 01279000
         D     @00,@CF00078                                        0408 01280000
         LTR   @00,@00                                             0408 01281000
         BZ    @RF00408                                            0408 01282000
*         ENDADDR=(ENDADDR+FOURK)&FOURKBDY;/* ROUND UP TO 4K BDY     */ 01283000
         AL    @08,@CF00078                                        0409 01284000
         NR    @08,@12                                             0409 01285000
         ST    @08,ENDADDR(,@15)                                   0409 01286000
*       ELSE                                                       0410 01287000
*         ;                                                        0410 01288000
@RF00408 DS    0H                                                  0411 01289000
*       IF STARTADR>ENDADDR THEN    /* ERROR IF START > END          */ 01290000
         L     @15,RANGEPTR(,R7)                                   0411 01291000
         CLC   STARTADR(4,@15),ENDADDR(@15)                        0411 01292000
         BNH   @RF00411                                            0411 01293000
*         GEN(ABEND X'C0D',DUMP,,SYSTEM);                          0412 01294000
         ABEND X'C0D',DUMP,,SYSTEM                                      01295000
*       ELSE                                                       0413 01296000
*         RANGEPTR=RANGEPTR+LEN1ADRN;/* ADDR OF NEXT SLOT IN TAB     */ 01297000
         B     @RC00411                                            0413 01298000
@RF00411 LA    @15,8                                               0413 01299000
         AL    @15,RANGEPTR(,R7)                                   0413 01300000
         ST    @15,RANGEPTR(,R7)                                   0413 01301000
*     END;                                                         0414 01302000
*   ELSE                            /* IF ADDRESS RANGE TABLE FULL,  */ 01303000
*     DO;                                                          0415 01304000
         B     @RC00404                                            0415 01305000
@RF00404 DS    0H                                                  0416 01306000
*       FLGRTFUL='1'B;              /* IND ADDRESS RANGE TABLE FULL  */ 01307000
         OI    FLGRTFUL(R7),B'10000000'                            0416 01308000
*       SDRETCOD=PARTDUMP;          /* GIVE PERMANENT PARTIAL DUMP 0417 01309000
*                                                            @G33VPHD*/ 01310000
         MVC   SDRETCOD(4,R7),@CF00054                             0417 01311000
*     END;                                                         0418 01312000
*   RFY                                                            0419 01313000
*    (R10,                                                         0419 01314000
*     R11,                                                         0419 01315000
*     R4,                                                          0419 01316000
*     R5) RSTD;                                                    0419 01317000
@RC00404 DS    0H                                                  0420 01318000
*   R10=ADDADRWA;                   /* RESTORE WORK REGISTER         */ 01319000
         L     R10,ADDADRWA(,R7)                                   0420 01320000
*   R11=ADDADRWB;                   /* RESTORE WORK REGISTER         */ 01321000
         L     R11,ADDADRWB(,R7)                                   0421 01322000
*   R4=ADDADRWC;                    /* RESTORE WORK REGISTER         */ 01323000
         L     R4,ADDADRWC(,R7)                                    0422 01324000
*   R5=ADDADRWD;                    /* RESTORE WORK REGISTER         */ 01325000
         L     R5,ADDADRWD(,R7)                                    0423 01326000
*   RFY                                                            0424 01327000
*    (R10,                                                         0424 01328000
*     R11,                                                         0424 01329000
*     R4,                                                          0424 01330000
*     R5,                                                          0424 01331000
*     R14) UNRSTD;                                                 0424 01332000
*   END SDADDADR;                                                  0425 01333000
@EL00006 DS    0H                                                  0425 01334000
@EF00006 DS    0H                                                  0425 01335000
@ER00006 BR    @14                                                 0425 01336000
*                                                                  0426 01337000
*/********************************************************************/ 01338000
*/*   THIS SEGMENT WILL ARRANGE A PART OF THE ADDRESS RANGE TABLE.   */ 01339000
*/*   THE ADDRESS RANGES WILL BE PLACED IN ASCENDING ORDER.          */ 01340000
*/*   DUPLICATE RANGES WILL BE DELETED.  OVERLAPPING RANGES WILL     */ 01341000
*/*   BE COMBINED INTO ONE RANGE.                                    */ 01342000
*/*                                                                  */ 01343000
*/*   INPUT: RANGEPTR CONTAINS ADDRESS OF FIRST ADDRESS RANGE IN     */ 01344000
*/*   THE PART OF TABLE TO BE ARRANGED, LAST RANGE IN THE PART OF    */ 01345000
*/*   THE TABLE IS INDICATED BY HAVING A STARTADR OF ENDPART         */ 01346000
*/*   AND AN ENDADDR OF NULL WITH LASTRNGE FLAG ON.                  */ 01347000
*/*                                                                  */ 01348000
*/*   OUTPUT: COMPRESSED, ASCENDING ORDERED PART OF ADDRESS RANGE    */ 01349000
*/*   TABLE WITH RANGEPTR CONTAINING ADDRESS OF LAST RANGE IN THIS   */ 01350000
*/*   PART OF THE ADDRESS RANGE TABLE(ENDPART ADDRESS RANGE).        */ 01351000
*/*   FLGRTFUL='1'B IF TABLE FULL ELSE FLGRTFUL='0'B.                */ 01352000
*/*                                                                  */ 01353000
*/*   CALLED BY: SDGLOBAL,SDNUC                                      */ 01354000
*/********************************************************************/ 01355000
*                                                                  0426 01356000
*ARRANGE:                                                          0426 01357000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0426 01358000
ARRANGE  DS    0H                                                  0427 01359000
*   RFY                                                            0427 01360000
*    (R10,                                                         0427 01361000
*     R11,                                                         0427 01362000
*     R4) RSTD;                                                    0427 01363000
*                                                                  0427 01364000
*   /*****************************************************************/ 01365000
*   /*                                                               */ 01366000
*   /* PUT RANGES IN ASCENDING ORDER BY STARTADR WORKA - ADDRESS OF  */ 01367000
*   /* CURRENT RANGE WORKB - ADDRESS OF NEXT RANGE WORKC - INDICATE A*/ 01368000
*   /* SWITCH IN ORDER OF RANGES MADE                                */ 01369000
*   /*                                                               */ 01370000
*   /*****************************************************************/ 01371000
*                                                                  0428 01372000
*   R4=ONE;                         /* INDICATE SWITCH-REG NON-ZERO  */ 01373000
         LA    R4,1                                                0428 01374000
*   DO WHILE R4^=NULL;              /* PUT RANGES IN ASCEND ORDER    */ 01375000
         B     @DE00429                                            0429 01376000
@DL00429 DS    0H                                                  0430 01377000
*     R10=RANGEPTR;                 /* ADDR OF FIRST CURRENT RANGE   */ 01378000
         L     R10,RANGEPTR(,R7)                                   0430 01379000
*     R11=R10+LEN1ADRN;             /* ADDRESS OF NEXT RANGE         */ 01380000
         LA    R11,8                                               0431 01381000
         ALR   R11,R10                                             0431 01382000
*     R4=NULL;                      /* NO SWITCH MADE                */ 01383000
         SLR   R4,R4                                               0432 01384000
*     DO WHILE R10->LASTRNGE='0'B;  /* CHECK ALL RANGES IN THIS PART */ 01385000
         B     @DE00433                                            0433 01386000
@DL00433 DS    0H                                                  0434 01387000
*       IF R11->STARTADR<           /* IF NEXT START ADDR LESS THAN  */ 01388000
*           R10->STARTADR THEN      /* CURRENT START ADDRESS,        */ 01389000
         CLC   STARTADR(4,R11),STARTADR(R10)                       0434 01390000
         BNL   @RF00434                                            0434 01391000
*         DO;                       /* SWITCH RANGES                 */ 01392000
*           R10->STORANGE=R10->STORANGE&&R11->STORANGE;            0436 01393000
         XC    STORANGE(8,R10),STORANGE(R11)                       0436 01394000
*           R11->STORANGE=R11->STORANGE&&R10->STORANGE;            0437 01395000
         XC    STORANGE(8,R11),STORANGE(R10)                       0437 01396000
*           R10->STORANGE=R10->STORANGE&&R11->STORANGE;            0438 01397000
         XC    STORANGE(8,R10),STORANGE(R11)                       0438 01398000
*           R4=ONE;                 /* INDICATE SWITCH MADE          */ 01399000
         LA    R4,1                                                0439 01400000
*         END;                                                     0440 01401000
*       ELSE                                                       0441 01402000
*         ;                                                        0441 01403000
@RF00434 DS    0H                                                  0442 01404000
*       R10=R11;                    /* UPDATE CURRENT RANGE POINTER  */ 01405000
         LR    R10,R11                                             0442 01406000
*       R11=R11+LEN1ADRN;           /* UPDATE NEXT RANGE POINTER     */ 01407000
         AL    R11,@CF00041                                        0443 01408000
*     END;                                                         0444 01409000
@DE00433 TM    LASTRNGE(R10),B'10000000'                           0444 01410000
         BZ    @DL00433                                            0444 01411000
*   END;                                                           0445 01412000
*                                                                  0445 01413000
@DE00429 LTR   R4,R4                                               0445 01414000
         BNZ   @DL00429                                            0445 01415000
*   /*****************************************************************/ 01416000
*   /*                                                               */ 01417000
*   /* REMOVE DUPLICATE RANGES, COMBINE OVERLAPPING RANGES. DELETED  */ 01418000
*   /* RANGES WILL HAVE A ZEROED ENTRY IN THE TABLE. WORKA - ADDRESS */ 01419000
*   /* OF CURRENT RANGE WORKB - ADDRESS OF NEXT RANGE                */ 01420000
*   /*                                                               */ 01421000
*   /*****************************************************************/ 01422000
*                                                                  0446 01423000
*   R10=RANGEPTR;                   /* ADDR OF FIRST CURRENT RANGE   */ 01424000
         L     R10,RANGEPTR(,R7)                                   0446 01425000
*   R11=R10+LEN1ADRN;               /* ADDRESS OF NEXT RANGE         */ 01426000
         LA    R11,8                                               0447 01427000
         ALR   R11,R10                                             0447 01428000
*   DO WHILE R10->LASTRNGE='0'B;    /* CHECK ALL RNGES IN THIS PART  */ 01429000
         B     @DE00448                                            0448 01430000
@DL00448 DS    0H                                                  0449 01431000
*     IF R11->STARTADR<=            /* IF NEXT RANGE OVERLAPS CURR   */ 01432000
*         R10->ENDADDR THEN                                        0449 01433000
         L     @12,ENDADDR(,R10)                                   0449 01434000
         C     @12,STARTADR(,R11)                                  0449 01435000
         BL    @RF00449                                            0449 01436000
*       DO;                         /* COMBINE RANGES                */ 01437000
*         IF R11->ENDADDR>          /* IF NEXT RANGE ENDS AFTER      */ 01438000
*             R10->ENDADDR THEN     /* CURRENT RANGE                 */ 01439000
