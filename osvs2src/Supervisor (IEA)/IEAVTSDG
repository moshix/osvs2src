         L     @12,NUCADDR(,R7)                                    0289 00777000
         ST    @12,RANGEPTR(,R7)                                   0289 00778000
*       R13=ADDR(SDWRGSV2);         /* GET SAVE AREA ADDRESS @G33VPHD*/ 00779000
         LA    R13,SDWRGSV2(,R7)                                   0290 00780000
*       CALL IEAVTSDO;              /* PUT OUT NUCLEUS STORAGE AREA  */ 00781000
         L     @15,@CV03144                                        0291 00782000
         BALR  @14,@15                                             0291 00783000
*     END;                                                         0292 00784000
*RTYENDMP:                                                         0293 00785000
*   R13=ADDR(SDWRGSV1);             /* REUSE FIRST SAVE AREA @G33VPHD*/ 00786000
@RF00287 DS    0H                                                  0293 00787000
RTYENDMP LA    R13,SDWRGSV1(,R7)                                   0293 00788000
*   RETURN;                                                        0294 00789000
@EL00001 DS    0H                                                  0294 00790000
@EF00001 DS    0H                                                  0294 00791000
@ER00001 LM    @14,@12,12(@13)                                     0294 00792000
         BR    @14                                                 0294 00793000
*   RFY                                                            0295 00794000
*     R14 RSTD;                     /* RESTRICT RETURN ADDRESS REG   */ 00795000
*                                                                  0296 00796000
*   /*****************************************************************/ 00797000
*   /*                                                               */ 00798000
*   /* THIS SEGMENT WILL FREE PAGES FIXED WHILE THE SALLOC LOCK WAS  */ 00799000
*   /* HELD. INPUT: LOCAL LOCK HELD, ANY OTHER LOCK (SALLOC OR       */ 00800000
*   /* HIGHER) CANNOT BE HELD OUTPUT: PAGES FREED CALLED BY: SDGLOBAL*/ 00801000
*   /*                                                               */ 00802000
*   /*****************************************************************/ 00803000
*                                                                  0296 00804000
*SDPGFREE:                                                         0296 00805000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0296 00806000
SDPGFREE DS    0H                                                  0297 00807000
*   PGFRRETA=R14;                   /* SAVE RETURN ADDRESS           */ 00808000
         ST    R14,PGFRRETA(,R7)                                   0297 00809000
*   ERRSDFIX='0'B;                  /* INDICATE SVCDUMP PAGES FREED  */ 00810000
         NI    ERRSDFIX(R3),B'11011111'                            0298 00811000
*   RFY                                                            0299 00812000
*    (R0,                                                          0299 00813000
*     R1,                                                          0299 00814000
*     R2,                                                          0299 00815000
*     R4) RSTD;                                                    0299 00816000
*   R0=NULL;                        /* NO ECB ADDRESS                */ 00817000
         SLR   R0,R0                                               0300 00818000
*   R1=ADDR(IEAVTSDG)|PGFREESW;     /* BEGIN ADDRESS TO FREE         */ 00819000
         L     R1,@CA00028                                         0301 00820000
         O     R1,@CF03116                                         0301 00821000
*   R2=NULL;                        /* NO ENDING ADDRESS NEEDED      */ 00822000
         SLR   R2,R2                                               0302 00823000
*   R4=PSATOLD;                     /* CURRENT TCB ADDRESS           */ 00824000
         L     R4,PSATOLD                                          0303 00825000
*   CALL PAGEFREE;                  /* FREE SVC DUMP PAGES           */ 00826000
         L     @12,CVTPTR                                          0304 00827000
         L     @12,PVTPTR(,@12)                                    0304 00828000
         L     @15,PVTPSIB(,@12)                                   0304 00829000
         BALR  @14,@15                                             0304 00830000
*   IF ERRWKFIX='1'B THEN           /* IF WORKAREA PAGES FIXED       */ 00831000
         TM    ERRWKFIX(R3),B'00010000'                            0305 00832000
         BNO   @RF00305                                            0305 00833000
*     DO;                                                          0306 00834000
*       ERRWKFIX='0'B;              /* INDICATE WKAREA PAGES FREED   */ 00835000
         NI    ERRWKFIX(R3),B'11101111'                            0307 00836000
*       IF SDUABEND='0'B THEN       /* IF THIS IS NOT A SYSMDUMP   0308 00837000
*                                      REQUEST                       */ 00838000
         L     @15,SDWSDPL(,R7)                                    0308 00839000
         TM    SDUABEND(@15),B'01000000'                           0308 00840000
         BNZ   @RF00308                                            0308 00841000
*         DO;                       /* FREE WORK AREA ASSUMING ONE 0309 00842000
*                                      PIECE                         */ 00843000
*           R1=R7|PGFREESW;         /* START ADDR TO FREE            */ 00844000
         LR    R1,R7                                               0310 00845000
         O     R1,@CF03116                                         0310 00846000
*           R2=(R7+SDWORKLN)|PGLASTSW;/* END ADDR TO FREE            */ 00847000
         LR    R2,R7                                               0311 00848000
         AL    R2,SDWORKLN                                         0311 00849000
         O     R2,@CF03101                                         0311 00850000
*           CALL PAGEFREE;          /* FREE WORKAREA PAGES           */ 00851000
         L     @12,CVTPTR                                          0312 00852000
         L     @12,PVTPTR(,@12)                                    0312 00853000
         L     @15,PVTPSIB(,@12)                                   0312 00854000
         BALR  @14,@15                                             0312 00855000
*         END;                      /* END OF FREE WORK AREA IN ONE     00856000
*                                      PIECE                         */ 00857000
*       ELSE                                                       0314 00858000
*         DO;                       /* IS A SYSMDUMP REQUEST SO    0314 00859000
*                                      PAGEFREE IN 2 PIECES          */ 00860000
         B     @RC00308                                            0314 00861000
@RF00308 DS    0H                                                  0315 00862000
*           R1=R7|PGFREESW;         /* GET START ADDRESS TO FREE     */ 00863000
         LR    R1,R7                                               0315 00864000
         O     R1,@CF03116                                         0315 00865000
*           R2=(R7+LENGTH(SDCNTRL))|PGLASTSW;/* END ADDRESS TO FREE  */ 00866000
         LA    R2,432                                              0316 00867000
         ALR   R2,R7                                               0316 00868000
