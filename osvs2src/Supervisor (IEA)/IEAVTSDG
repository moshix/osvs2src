*           ENDADDR=TRTEND;         /* END ADDRESS OF TRACE TABLE    */ 00331000
         L     @12,TRTEND(,@12)                                    0170 00332000
         ST    @12,ENDADDR(,@08)                                   0170 00333000
*           CALL SDADDADR;          /* ADD SUPERVISOR TRACE TABLE  0171 00334000
*                                      RANGE TO TABLE                */ 00335000
         BAL   @14,SDADDADR                                        0171 00336000
*         END;                                                     0172 00337000
*RTYGLO50:                                                         0173 00338000
*       ERRRETRY=ADDR(RTYGLO60);    /* NEXT RETRY LOCATION ADDRESS   */ 00339000
@RF00167 DS    0H                                                  0173 00340000
RTYGLO50 LA    @12,RTYGLO60                                        0173 00341000
         ST    @12,ERRRETRY(,R3)                                   0173 00342000
*       IF FLGRTFUL='0'B&           /* IF RANGE TABLE NOT FULL AND   */ 00343000
*           SVCDNAPA='0'B THEN      /* ALL PSAS TO BE DUMPED         */ 00344000
         TM    FLGRTFUL(R7),B'10000000'                            0174 00345000
         BNZ   @RF00174                                            0174 00346000
         TM    SVCDNAPA(R7),B'00001000'                            0174 00347000
         BNZ   @RF00174                                            0174 00348000
*         DO;                                                      0175 00349000
*                                                                  0175 00350000
*/********************************************************************/ 00351000
*/*   THIS SEGMENT WILL CONVERT THE 'ALLPSA' SDATA OPTION ON THE     */ 00352000
*/*   SDUMP MACRO TO ADDRESS RANGES.  AN ADDRESS RANGE FOR EACH      */ 00353000
*/*   PSA, FOR EACH ALIVE CPU, WILL BE ADDED TO THE ADDRESS RANGE    */ 00354000
*/*   TABLE.                                                         */ 00355000
*/*                                                                  */ 00356000
*/*   INPUT: CSDCPUAL CONTAINS A BIT MASK REFLECTING ALIVE CPUS.     */ 00357000
*/*          PCCAT00P CONTAINS ADDRESSES OF ALL PCCAS.  EACH PCCA,   */ 00358000
*/*          PCCAPSAV FIELD, CONTAINS ADDRESS OF CORRESPONDING PSA.  */ 00359000
*/*                                                                  */ 00360000
*/*   OUTPUT: STORAGE RANGE FOR EACH PSA ADDED TO ADDRESS RANGE      */ 00361000
*/*           TABLE                                                  */ 00362000
*/*                                                                  */ 00363000
*/*   INCLUDED BY: SDGLOBAL                                          */ 00364000
*/********************************************************************/ 00365000
*                                                                  0176 00366000
*           RFY                                                    0176 00367000
*            (R10,                                                 0176 00368000
*             R11,                                                 0176 00369000
*             R4) RSTD;                                            0176 00370000
*           R10=NULL;               /* CLEAR REGISTER        @YM04725*/ 00371000
         SLR   R10,R10                                             0177 00372000
*           R11=CSDCPUAL;           /* MASK IN ODD REG       @YM04725*/ 00373000
         L     @12,CVTPTR                                          0178 00374000
         L     @12,CVTCSD(,@12)                                    0178 00375000
         LH    R11,CSDCPUAL(,@12)                                  0178 00376000
         N     R11,@CF03236                                        0178 00377000
*           GEN(SLL   R11,16);      /* MASK IN HIGH END      @YM04725*/ 00378000
         SLL   R11,16                                                   00379000
*           RFY                                                    0180 00380000
*             PCCA BASED(PCCAT00P(R4));                            0180 00381000
*                                                                  0180 00382000
*           /*********************************************************/ 00383000
*           /*                                                       */ 00384000
*           /* PROCESS ALL PSA AREAS FOR ALL CPUS. IF THE CPU IS     */ 00385000
*           /* ALIVE ADD ITS PSA ADDRESS TO THE RANGE TABLE          */ 00386000
*           /*                                                       */ 00387000
*           /*********************************************************/ 00388000
*                                                                  0181 00389000
*           DO R4=1 TO MAXCPUS;     /* CHECK ALL ALLOWABLE CPUS      */ 00390000
         LA    R4,1                                                0181 00391000
@DL00181 DS    0H                                                  0182 00392000
