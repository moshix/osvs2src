*         END;                      /* END OF FREE WORK AREA IN TWO     00889000
*                                      PIECES                        */ 00890000
*     END;                                                         0322 00891000
*   ELSE                                                           0323 00892000
*     ;                                                            0323 00893000
@RF00305 DS    0H                                                  0324 00894000
*   RFY                                                            0324 00895000
*    (R0,                                                          0324 00896000
*     R1,                                                          0324 00897000
*     R2,                                                          0324 00898000
*     R4) UNRSTD;                                                  0324 00899000
@RC00305 DS    0H                                                  0325 00900000
*   R14=PGFRRETA;                   /* RESTORE RETURN ADDRESS        */ 00901000
         L     R14,PGFRRETA(,R7)                                   0325 00902000
*   END SDPGFREE;                                                  0326 00903000
@EL00002 DS    0H                                                  0326 00904000
@EF00002 DS    0H                                                  0326 00905000
@ER00002 BR    @14                                                 0326 00906000
*                                                                  0327 00907000
*/********************************************************************/ 00908000
*/*   THIS SEGMENT WILL ADD CALLER SUPPLIED ADDRESS RANGES TO THE    */ 00909000
*/*   ADDRESS RANGE TABLE.  USER RANGES WILL BE ADDED TO THE PART    */ 00910000
*/*   OF THE ADDRESS RANGE TABLE WHERE THEY BEST BELONG.  ADDRESS    */ 00911000
*/*   RANGES WHICH HAVE A START ADDRESS GREATER THAN THE ADDRESS     */ 00912000
*/*   OF THE END OF THE PRIVATE AREA WILL BE ADDED TO THE GLOBAL     */ 00913000
*/*   PART OF THE ADDRESS RANGE TABLE.  ADDRESS RANGES WHICH HAVE    */ 00914000
*/*   A START ADDRESS WHICH LIES IN THE PRIVATE AREA WILL BE ADDED   */ 00915000
*/*   TO THE LOCAL PART OF THE ADDRESS RANGE TABLE.  ADDRESS         */ 00916000
*/*   RANGES WHICH HAVE A START ADDRESS WHICH IS LESS THAN THE       */ 00917000
*/*   ADDRESS OF THE BEGINNING OF THE PRIVATE AREA WILL BE ADDED     */ 00918000
*/*   TO THE NUCLEUS PART OF THE ADDRESS RANGE TABLE.  THIS          */ 00919000
*/*   ROUTINE WILL BE CALLED THREE TIMES, ONCE FOR EACH PART OF      */ 00920000
*/*   THE ADDRESS RANGE TABLE, IF THE CALLER SUPPLIED STORAGE        */ 00921000
*/*   ADDRESSES.                                                     */ 00922000
*/*                                                                  */ 00923000
*/*   INPUT: USER STORAGE RANGES, USESTART CONTAINS LOW ADDRESS      */ 00924000
*/*         TO COMPARE AGAINST USER STORAGE RANGE START ADDRESS FOR  */ 00925000
*/*         ADDING THE USER STORAGE RANGE TO THE ADDRESS RANGE TABLE,*/ 00926000
*/*         USEEND CONTAINS HIGH ADDRESS TO COMPARE AGAINST USER     */ 00927000
*/*         STORAGE START ADDRESS                                    */ 00928000
*/*                                                                  */ 00929000
*/*   OUTPUT: USER STORAGE RANGES ADDED TO ADDRESS RANGE TABLE       */ 00930000
*/*                                                                  */ 00931000
*/*   CALLED BY: SDGLOBAL,SDNUC                                      */ 00932000
*/********************************************************************/ 00933000
*                                                                  0327 00934000
*USERNGES:                                                         0327 00935000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0327 00936000
USERNGES DS    0H                                                  0328 00937000
*   USERRETA=R14;                   /* SAVE RETURN ADDRESS           */ 00938000
         ST    R14,USERRETA(,R7)                                   0328 00939000
*   RFY                                                            0329 00940000
*     R10 RSTD;                                                    0329 00941000
*   R10=SDUSTORA;                   /* ADDR OF USER STOR ADDRESSES   */ 00942000
         L     @15,SDWSDPL(,R7)                                    0330 00943000
         L     R10,SDUSTORA(,@15)                                  0330 00944000
*   DO WHILE R10^=NULL;             /* SEARCH USER STORAGE ADDRESSES    00945000
*                                      FOR INSERTION IN CURRENT PART    00946000
*                                      OF ADDRESS RANGE TABLE        */ 00947000
         B     @DE00331                                            0331 00948000
@DL00331 DS    0H                                                  0332 00949000
*     IF R10->STARTADR>=USESTART&   /* IF USER START ADDR LIES IN    */ 00950000
*         R10->STARTADR<USEEND THEN /* CURRENT STORAGE AREA          */ 00951000
         L     @15,STARTADR(,R10)                                  0332 00952000
         C     @15,USESTART(,R7)                                   0332 00953000
         BL    @RF00332                                            0332 00954000
         L     @12,USEEND(,R7)                                     0332 00955000
         CR    @15,@12                                             0332 00956000
         BNL   @RF00332                                            0332 00957000
*       DO;                                                        0333 00958000
*         STARTADR=R10->STARTADR;   /* START ADDR, ADD TO TABLE      */ 00959000
         L     @11,RANGEPTR(,R7)                                   0334 00960000
         ST    @15,STARTADR(,@11)                                  0334 00961000
*         IF(R10->ENDADDR&ENDPART)  /* IF ENDING ADDRESS     @ZA29803*/ 00962000
*             <=USEEND THEN         /* ALSO IN THIS STORAGE AREA   0335 00963000
*                                                            @ZA29803*/ 00964000
         L     @15,ENDADDR(,R10)                                   0335 00965000
         N     @15,@CF03099                                        0335 00966000
         CR    @15,@12                                             0335 00967000
         BH    @RF00335                                            0335 00968000
*           ENDADDR=(R10->ENDADDR&  /* USE CALLER SUPPLIED   @ZA29803*/ 00969000
*               ENDPART);           /* END ADDRESS           @ZA29803*/ 00970000
         ST    @15,ENDADDR(,@11)                                   0336 00971000
*         ELSE                                                     0337 00972000
*           ENDADDR=USEEND;         /* ENDING ADDRESS NOT IN RANGE SO   00973000
*                                      USE STORAGE AREA END ADDRESS     00974000
*                                      FOR NOW                       */ 00975000
         B     @RC00335                                            0337 00976000
@RF00335 L     @15,RANGEPTR(,R7)                                   0337 00977000
         L     @12,USEEND(,R7)                                     0337 00978000
         ST    @12,ENDADDR(,@15)                                   0337 00979000
*         CALL SDADDADR;            /* ADD THIS RANGE TO ADDRESS   0338 00980000
*                                      RANGE TABLE                   */ 00981000
@RC00335 BAL   @14,SDADDADR                                        0338 00982000
*       END;                                                       0339 00983000
*     ELSE                          /* START ADDRESS IS NOT IN THIS     00984000
*                                      STORAGE AREA                  */ 00985000
*       IF R10->STARTADR<USESTART&  /* IF PART OF RANGE IS IN        */ 00986000
*           (R10->ENDADDR&ENDPART)>=USESTART THEN/*          @ZA29803   00987000
*                                      THIS STORAGE AREA     @ZA29803*/ 00988000
         B     @RC00332                                            0340 00989000
@RF00332 L     @15,USESTART(,R7)                                   0340 00990000
         C     @15,STARTADR(,R10)                                  0340 00991000
         BNH   @RF00340                                            0340 00992000
         L     @12,ENDADDR(,R10)                                   0340 00993000
         N     @12,@CF03099                                        0340 00994000
         CR    @12,@15                                             0340 00995000
         BL    @RF00340                                            0340 00996000
*         DO;                       /* PUT THIS PART OF CALLER RANGE    00997000
*                                      THE ADDRESS RANGE TABLE       */ 00998000
*           STARTADR=USESTART;      /* START ADDRESS IS STORAGE ARE     00999000
*                                      START ADDRESS                 */ 01000000
         L     @11,RANGEPTR(,R7)                                   0342 01001000
         ST    @15,STARTADR(,@11)                                  0342 01002000
*           IF(R10->ENDADDR&ENDPART)/* IF END ADDR IN        @ZA29803*/ 01003000
*               <=USEEND THEN       /* IN RANGE              @ZA29803*/ 01004000
