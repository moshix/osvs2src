         L     @15,PVTPSIB(,@12)                                   0317 00873000
         BALR  @14,@15                                             0317 00874000
*           R1=SDSMBUFF|PGFREESW;   /* GET START ADDRESS TO FREE     */ 00875000
         L     @15,SDSMBUFF(,R7)                                   0318 00876000
         LR    R1,@15                                              0318 00877000
         O     R1,@CF03116                                         0318 00878000
*           R2=(SDSMBUFF+LENGTH(SDBUFFER))|PGLASTSW;/* END ADDRESS TO   00879000
*                                      FREE                          */ 00880000
         AL    @15,@CF03032                                        0319 00881000
