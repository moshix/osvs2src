         NI    FLGRTFUL(R7),B'01111111'                            0481 01530000
*   RFY                                                            0482 01531000
*    (R10,                                                         0482 01532000
*     R11,                                                         0482 01533000
*     R4) UNRSTD;                                                  0482 01534000
*   END ARRANGE;                                                   0483 01535000
@EL00007 DS    0H                                                  0483 01536000
@EF00007 DS    0H                                                  0483 01537000
@ER00007 BR    @14                                                 0483 01538000
*   DCL                                                            0484 01539000
*     PATCHLOC FIXED(31) STATIC;                                   0484 01540000
*   GEN DATA DEFS(PATCHLOC);                                       0485 01541000
*   DECLARE                         /* COMMON VARIABLES              */ 01542000
*     I256C CHAR(256) BASED,                                       0486 01543000
*     I031F FIXED(31) BASED,                                       0486 01544000
*     I031P PTR(31) BASED,                                         0486 01545000
*     I015F FIXED(15) BASED,                                       0486 01546000
