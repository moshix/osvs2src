         BH    @RF00343                                            0343 01006000
*             ENDADDR=(R10->ENDADDR /* USE CALLERS END       @ZA29803*/ 01007000
*                 &ENDPART);        /* ADDRESS               @ZA29803*/ 01008000
         ST    @12,ENDADDR(,@11)                                   0344 01009000
*           ELSE                                                   0345 01010000
*             ENDADDR=USEEND;       /* END ADDR NOT IN RANGE USE   0345 01011000
*                                      RANGE END ADDRESS             */ 01012000
         B     @RC00343                                            0345 01013000
@RF00343 L     @15,RANGEPTR(,R7)                                   0345 01014000
         L     @12,USEEND(,R7)                                     0345 01015000
         ST    @12,ENDADDR(,@15)                                   0345 01016000
*           CALL SDADDADR;          /* ADD THIS RANGE TO RANGE TABLE */ 01017000
@RC00343 BAL   @14,SDADDADR                                        0346 01018000
*         END;                      /* END ADD PART OF CALLERS RANGE    01019000
*                                      TO TABLE                      */ 01020000
*     IF FLGRTFUL='1'B|             /* IF ADDR RANGE TABLE FULL OR   */ 01021000
*         R10->LASTRNGE='1'B THEN   /* END OF USER STOR ADDRESSES    */ 01022000
@RF00340 DS    0H                                                  0348 01023000
@RC00332 TM    FLGRTFUL(R7),B'10000000'                            0348 01024000
         BO    @RT00348                                            0348 01025000
         TM    LASTRNGE(R10),B'10000000'                           0348 01026000
         BNO   @RF00348                                            0348 01027000
@RT00348 DS    0H                                                  0349 01028000
*       R10=NULL;                   /* END SEARCH OF USER STOR ADDR  */ 01029000
         SLR   R10,R10                                             0349 01030000
*     ELSE                                                         0350 01031000
*       R10=R10+LEN1ADRN;           /* ADDR OF NEXT USER STOR RANGE  */ 01032000
         B     @RC00348                                            0350 01033000
