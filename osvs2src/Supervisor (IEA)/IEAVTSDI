         OI    RTCTSDNO(@15),B'10000000'                           0427 00719000
*   /*****************************************************************/ 00720000
*   /*                                                               */ 00721000
*   /* THIS ROUTINE ROUTES CONTROL TO THE SDIVALID ROUTINE TO SCAN   */ 00722000
*   /* THE DUMP DEVICES SPECIFIED BY THE OPERATOR AT IPL TIME IF NO  */ 00723000
*   /* DEVICES HAVE BEEN SPECIFIED THE DEFAULT IS DASD IF OPTIONS ARE*/ 00724000
*   /* DETERMINED TO BE INVALID THE OPERATOR IS PROMPTED TO RESPECIFY*/ 00725000
*   /* THE DEVICES                                                   */ 00726000
*   /*                                                               */ 00727000
*   /*****************************************************************/ 00728000
*                                                                  0428 00729000
*   DACTION=0;                      /* ZERO REQUEST FLAGS AND      0428 00730000
*                                      INTERNAL SWITCHES             */ 00731000
         SLR   @15,@15                                             0428 00732000
         STH   @15,DACTION(,R5)                                    0428 00733000
*   IF NIPPTLST='1'B                /* WAS LIST SPECIFIED            */ 00734000
*     THEN                          /* YES                           */ 00735000
