*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0996 02774000
*                                                                  0996 02775000
DASDINIT DS    0H                                                  0997 02776000
*   /*****************************************************************/ 02777000
*   /*                                                               */ 02778000
*   /* THIS ROUTINE IS ENTERED TO PLACE THE DASD DEVICES ON A DEVICE */ 02779000
*   /* ENTRY QUEUE-THE Q IS BUILT TO MAINTAIN THE DEVICES SPECIFIED  */ 02780000
*   /* IN THE SEQUENCE SPECIFIED BY THE OPERATOR.THIS ROUTINE IS     */ 02781000
*   /* ENTERED ONLY ONCE. BEFORE THIS ROUTINE IS CALLED A CHECK MUST */ 02782000
*   /* BE MADE-IF ENTDASD = 1 B THEN BYPASS THE CALL OTHERWISE TURN  */ 02783000
*   /* THE BIT ON AND ISSUE THE CALL                                 */ 02784000
*   /*                                                               */ 02785000
*   /*****************************************************************/ 02786000
*                                                                  0997 02787000
*   RFY                                                            0997 02788000
*     R4 RSTD;                                                     0997 02789000
*   R4=RQUEADD;                     /* THE Q IS BUILT IN THE 4K SQA     02790000
*                                      BUFFER AND RQUEADD POINTS TO     02791000
*                                      THE FIRST ELEMENT ON THE Q OR    02792000
*                                      IS 0                          */ 02793000
         L     R4,RQUEADD(,R5)                                     0998 02794000
*   IF R4=0                         /* ANYTHING ON Q                 */ 02795000
*     THEN                          /* NO                            */ 02796000
         LTR   R4,R4                                               0999 02797000
         BNZ   @RF00999                                            0999 02798000
*     DO;                                                          1000 02799000
*       R4=ADDR(QBEGIN);            /* START AT Q BEGIN              */ 02800000
         LA    R4,QBEGIN(,R5)                                      1001 02801000
*       RQUEADD=R4;                 /* POINT TO FIRST ELEMENT        */ 02802000
         ST    R4,RQUEADD(,R5)                                     1002 02803000
*     END;                                                         1003 02804000
*   ELSE                                                           1004 02805000
*     DO;                                                          1004 02806000
         B     @RC00999                                            1004 02807000
@RF00999 DS    0H                                                  1005 02808000
*       DO WHILE(NXTENTAD^=0);      /* FIND THE END OF Q             */ 02809000
         B     @DE01005                                            1005 02810000
@DL01005 DS    0H                                                  1006 02811000
*         R4=NXTENTAD;              /* POINT TO NEXT ELEMENT         */ 02812000
         L     R4,NXTENTAD(,R4)                                    1006 02813000
*       END;                        /* END OF DO WHILE               */ 02814000
@DE01005 L     @15,NXTENTAD(,R4)                                   1007 02815000
         LTR   @15,@15                                             1007 02816000
         BNZ   @DL01005                                            1007 02817000
*       NXTENTAD=R4+LENGTH(DVICERQT);/* Q IS BUILT SO THAT EACH    1008 02818000
*                                      ELEMENT IMMEDIATELY FOLLOWS IN   02819000
*                                      THE NEXT CORE LOCATION        */ 02820000
         LA    @15,8                                               1008 02821000
         ALR   @15,R4                                              1008 02822000
         ST    @15,NXTENTAD(,R4)                                   1008 02823000
*       R4=NXTENTAD;                /* SET ADDRESS FOR NEXT ELEMENT  */ 02824000
         LR    R4,@15                                              1009 02825000
*     END;                                                         1010 02826000
*   CNT1=0;                         /* START COUNT AT 0              */ 02827000
@RC00999 SLR   CNT1,CNT1                                           1011 02828000
*   DO WHILE(DUMPF='0'B);           /* DO LOOP UNTIL 10 ENTRIES    1012 02829000
*                                      PLACED ON THE Q               */ 02830000
         B     @DE01012                                            1012 02831000
@DL01012 DS    0H                                                  1013 02832000
*     DVICERQT=DVICERQT&&DVICERQT;  /* CLEAR ENTRY TO 0              */ 02833000
         XC    DVICERQT(8,R4),DVICERQT(R4)                         1013 02834000
*     FLAG1='80'X;                  /* INDICATE DASD                 */ 02835000
         MVI   FLAG1(R4),X'80'                                     1014 02836000
*     DVNAME='00 ';                 /* INITIALIZE NAME TO 00 BLANK   */ 02837000
         MVC   DVNAME(3,R4),@CC03818                               1015 02838000
*     DCHAR2=DCHAR2|CNT1;           /* THE SECOND CHAR IN THE NAME 1016 02839000
*                                      FIELD IS CHANGED TO THE     1016 02840000
*                                      SPECIFIC SYS1.DUMP DATA SET 1016 02841000
*                                      IE- FOR DEVICE SYS1.DUMP 08 1016 02842000
*                                      THE NAME FIELD IS SET TO    1016 02843000
*                                      00(BLANK) THE AN 8 IS ORED TO    02844000
*                                      THE SECOND CHARACTER TO MAKE     02845000
*                                      IT A 08(BLANK)                */ 02846000
         SLR   @15,@15                                             1016 02847000
         IC    @15,DCHAR2(,R4)                                     1016 02848000
         OR    @15,CNT1                                            1016 02849000
         STC   @15,DCHAR2(,R4)                                     1016 02850000
*     IF CNT1='09'X                 /* ALL TEN DEVICES DONE ?        */ 02851000
*       THEN                        /* YES                           */ 02852000
         C     CNT1,@CF03792                                       1017 02853000
         BNE   @RF01017                                            1017 02854000
*       DUMPF='1'B;                 /* INDICATE FINISHED             */ 02855000
         OI    DUMPF(R5),B'00000001'                               1018 02856000
*     ELSE                                                         1019 02857000
*       DO;                                                        1019 02858000
         B     @RC01017                                            1019 02859000
@RF01017 DS    0H                                                  1020 02860000
*         NXTENTAD=R4+LENGTH(DVICERQT);/* POINT TO NEXT ENTRY        */ 02861000
         LA    @15,8                                               1020 02862000
         ALR   @15,R4                                              1020 02863000
         ST    @15,NXTENTAD(,R4)                                   1020 02864000
*         R4=NXTENTAD;                                             1021 02865000
         LR    R4,@15                                              1021 02866000
*         CNT1=CNT1+1;              /* UP COUNT BY 1                 */ 02867000
         LA    CNT1,1(,CNT1)                                       1022 02868000
*       END;                                                       1023 02869000
*   END;                                                           1024 02870000
@RC01017 DS    0H                                                  1024 02871000
@DE01012 TM    DUMPF(R5),B'00000001'                               1024 02872000
         BZ    @DL01012                                            1024 02873000
*   DUMPF='0'B;                     /* RESET FINISHED SWITCH         */ 02874000
         NI    DUMPF(R5),B'11111110'                               1025 02875000
*   RETURN;                         /* RETURN TO INVOKER             */ 02876000
@EL00003 DS    0H                                                  1026 02877000
@EF00003 DS    0H                                                  1026 02878000
@ER00003 BR    @14                                                 1026 02879000
*   RFY                                                            1027 02880000
*     R4 UNRSTD;                                                   1027 02881000
*   END DASDINIT;                                                  1028 02882000
*TAPEINIT:                                                         1029 02883000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1029 02884000
*                                                                  1029 02885000
TAPEINIT DS    0H                                                  1030 02886000
*   /*****************************************************************/ 02887000
*   /*                                                               */ 02888000
*   /* THIS PROCEDURE IS ENTERED TO PLACE A TAPE DEVICE ADDRESS ON   */ 02889000
*   /* THE DEVICE Q- WHENEVER A TA, HAS BEEN FOUND BY THE SCAN       */ 02890000
*   /* ROUTINES THIS ROUTINE IS INVOKED WITH THE POINTER CALLED      */ 02891000
*   /* SCANBEG POINTING TO THE XXX OF TA,XXX WHERE XXX IS THE DEVICE */ 02892000
*   /* ADDRESS THIS ROUTINE INSURES THAT THE DEVICE HAS NOT BEEN     */ 02893000
*   /* SPECIFIED TWICE IF IT HAS THE REPROSS BIT IS INTERROGATED IT  */ 02894000
*   /* MEANS THAT AN ERROR WAS ENCOUNTERED ON THIS DEVICE PREVIOUSLY */ 02895000
*   /* AND THE OPERATOR WISHES TO RETRY IF THE REPROSS BIT IS OFF THE*/ 02896000
*   /* VALIDSW IS TURNED OFF AND THE INVOKER IS RETURNED TO          */ 02897000
*   /*                                                               */ 02898000
*   /*****************************************************************/ 02899000
*                                                                  1030 02900000
*   RFY                                                            1030 02901000
*     R4 RSTD;                                                     1030 02902000
*   R4=RQUEADD;                     /* GET BEGINNING ADDRESS OF Q    */ 02903000
         L     R4,RQUEADD(,R5)                                     1031 02904000
*   IF R4=0                         /* ANY Q                         */ 02905000
*     THEN                          /* NO                            */ 02906000
         LTR   R4,R4                                               1032 02907000
         BNZ   @RF01032                                            1032 02908000
*     DO;                                                          1033 02909000
*       R4=ADDR(QBEGIN);                                           1034 02910000
         LA    R4,QBEGIN(,R5)                                      1034 02911000
*       RQUEADD=R4;                                                1035 02912000
         ST    R4,RQUEADD(,R5)                                     1035 02913000
*       DVICERQT=DVICERQT&&DVICERQT;                               1036 02914000
         XC    DVICERQT(8,R4),DVICERQT(R4)                         1036 02915000
*       DVNAME=FIRST3;                                             1037 02916000
         L     @15,SCANBEG(,R5)                                    1037 02917000
         MVC   DVNAME(3,R4),FIRST3(@15)                            1037 02918000
*     END;                                                         1038 02919000
*   ELSE                            /* THERE IS A Q                  */ 02920000
*     DO;                                                          1039 02921000
         B     @RC01032                                            1039 02922000
@RF01032 DS    0H                                                  1040 02923000
*       DO WHILE(R4^=0);            /* SCAN Q FOR THIS DEVICE        */ 02924000
         B     @DE01040                                            1040 02925000
@DL01040 DS    0H                                                  1041 02926000
*         QSAVE=R4;                 /* SAVE ADDRESS                  */ 02927000
         ST    R4,QSAVE(,R5)                                       1041 02928000
*         IF DVNAME=FIRST3&REPROCSS='0'B/* VALID RESPECIFY?          */ 02929000
*           THEN                    /* NO CAN NOT BE ALLOWED TWICE   */ 02930000
         L     @15,SCANBEG(,R5)                                    1042 02931000
         CLC   DVNAME(3,R4),FIRST3(@15)                            1042 02932000
         BNE   @RF01042                                            1042 02933000
         TM    REPROCSS(R4),B'00100000'                            1042 02934000
         BNZ   @RF01042                                            1042 02935000
*           DO;                                                    1043 02936000
*             VALIDSW='0'B;                                        1044 02937000
         NI    VALIDSW(R5),B'11111101'                             1044 02938000
*             R4=0;                                                1045 02939000
         SLR   R4,R4                                               1045 02940000
*           END;                                                   1046 02941000
*         ELSE                                                     1047 02942000
*           R4=NXTENTAD;            /* GET NEXT ADDRESS              */ 02943000
         B     @RC01042                                            1047 02944000
@RF01042 L     R4,NXTENTAD(,R4)                                    1047 02945000
*       END;                                                       1048 02946000
@RC01042 DS    0H                                                  1048 02947000
@DE01040 LTR   R4,R4                                               1048 02948000
         BNZ   @DL01040                                            1048 02949000
*       IF VALIDSW='1'B             /* WAS DEVICE ALREADY ON Q       */ 02950000
*         THEN                      /* NO                            */ 02951000
         TM    VALIDSW(R5),B'00000010'                             1049 02952000
         BNO   @RF01049                                            1049 02953000
*         DO;                                                      1050 02954000
*           R4=QSAVE;                                              1051 02955000
         L     R4,QSAVE(,R5)                                       1051 02956000
*           NXTENTAD=R4+LENGTH(DVICERQT);                          1052 02957000
         LA    @15,8                                               1052 02958000
         ALR   @15,R4                                              1052 02959000
         ST    @15,NXTENTAD(,R4)                                   1052 02960000
*           R4=NXTENTAD;                                           1053 02961000
         LR    R4,@15                                              1053 02962000
*           DVICERQT=DVICERQT&&DVICERQT;                           1054 02963000
         XC    DVICERQT(8,R4),DVICERQT(R4)                         1054 02964000
*           DVNAME=FIRST3;                                         1055 02965000
         L     @15,SCANBEG(,R5)                                    1055 02966000
         MVC   DVNAME(3,R4),FIRST3(@15)                            1055 02967000
*         END;                                                     1056 02968000
*     END;                                                         1057 02969000
*   RETURN;                         /* RETURN TO INVOKER             */ 02970000
@EL00004 DS    0H                                                  1058 02971000
@EF00004 DS    0H                                                  1058 02972000
@ER00004 BR    @14                                                 1058 02973000
*   RFY                                                            1059 02974000
*     R4 UNRSTD;                                                   1059 02975000
*   END TAPEINIT;                                                  1060 02976000
         B     @EL00004                                            1060 02977000
*CHECKTBL:                                                         1061 02978000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1061 02979000
*                                                                  1061 02980000
CHECKTBL DS    0H                                                  1062 02981000
*   /*****************************************************************/ 02982000
*   /*                                                               */ 02983000
*   /* THIS PROCEDURE IS ENTERED TO DETERMINE IF THERE ARE ANY       */ 02984000
*   /* ELEMENTS ON THE DEVICE QUEUE THAT HAVE NOT BEEN PROCESSED. IF */ 02985000
*   /* THIS IS TRUE THEN THE DATA SET TABLE HAS TEN ENTRIES,AND MORE */ 02986000
*   /* HAVE BEEN SPECIFIED THAN CAN BE PROCESSED---THEN ISSUE MESSAGE*/ 02987000
*   /* IEA881I. IF DASD HAS BEEN SPECIFIED AND NO DASD DEVICES HAVE  */ 02988000
*   /* BEEN ACCEPTED THEN ISSUE MESSAGE IEA878I.                     */ 02989000
*   /*                                                               */ 02990000
*   /*****************************************************************/ 02991000
*                                                                  1062 02992000
*   RFY                                                            1062 02993000
*    (R4,                                                          1062 02994000
*     R6) RSTD;                                                    1062 02995000
*   QSAVE=ADDR(RQUEADD);            /* INIT QSAVE PTR TO TRUNCATE Q  */ 02996000
         LA    @15,RQUEADD(,R5)                                    1063 02997000
         ST    @15,QSAVE(,R5)                                      1063 02998000
*   R4=RQUEADD;                     /* GET BEGINNING OF Q            */ 02999000
         L     R4,RQUEADD(,R5)                                     1064 03000000
*   R6=R14;                         /* SAVE RETURN ADDRESS           */ 03001000
         LR    R6,R14                                              1065 03002000
*   DO WHILE(R4^=0);                                               1066 03003000
         B     @DE01066                                            1066 03004000
@DL01066 DS    0H                                                  1067 03005000
*     IF DPROCSD='0'B               /* HAS THIS BEEN PROCESSED ?     */ 03006000
*       THEN                        /* NO                            */ 03007000
         TM    DPROCSD(R4),B'01000000'                             1067 03008000
         BNZ   @RF01067                                            1067 03009000
*       DO;                         /* ISSUE TOO MANY DEVICES MSG    */ 03010000
*         GEN REFS(MSG03,RPARM,R14,R15,RNVT)                       1069 03011000
*             ( IEAPMNIP TYPE=WTO,PARAM=MSG03);                    1069 03012000
          IEAPMNIP TYPE=WTO,PARAM=MSG03                                 03013000
*         R4=0;                     /* SET POINTER TO 0              */ 03014000
         SLR   R4,R4                                               1070 03015000
*       END;                                                       1071 03016000
*     ELSE                          /* IT HAS BEEN PROCESSED         */ 03017000
*       DO;                                                        1072 03018000
         B     @RC01067                                            1072 03019000
@RF01067 DS    0H                                                  1073 03020000
*         QSAVE=R4;                 /* SAVE QPTR                     */ 03021000
         ST    R4,QSAVE(,R5)                                       1073 03022000
*         R4=NXTENTAD;              /* GET NEXT BLOCK TO CHECK       */ 03023000
         L     R4,NXTENTAD(,R4)                                    1074 03024000
*       END;                                                       1075 03025000
*   END;                                                           1076 03026000
@RC01067 DS    0H                                                  1076 03027000
@DE01066 SLR   @15,@15                                             1076 03028000
         CR    R4,@15                                              1076 03029000
         BNE   @DL01066                                            1076 03030000
*   QSAVE->NXTENTAD=0;              /* IGNORE DEVICES NOT PROCESSED  */ 03031000
         L     @11,QSAVE(,R5)                                      1077 03032000
         ST    @15,NXTENTAD(,@11)                                  1077 03033000
*   IF DUMPD='1'B                   /* WAS DASD SPECIFIED ?          */ 03034000
*     THEN                          /* YES                           */ 03035000
         TM    DUMPD(R5),B'00100000'                               1078 03036000
         BNO   @RF01078                                            1078 03037000
*     DO;                           /* DETERMINE IF ANY DASD HAVE  1079 03038000
*                                      BEEN PLACED IN THE DEVICE   1079 03039000
*                                      TABLE                         */ 03040000
*       CNT3=1;                     /* SET INDEX TO FIRST ENTRY OF 1080 03041000
*                                      TABLE                         */ 03042000
         MVI   CNT3,X'01'                                          1080 03043000
*       DO WHILE(CNT3^=CNT1);       /* DO UNTIL THE LAST ENTRY HAS 1081 03044000
*                                      BEEN INTERROGATED             */ 03045000
         B     @DE01081                                            1081 03046000
@DL01081 DS    0H                                                  1082 03047000
*         IF RTCTDETP(CNT3)='1'B    /* IS IT A DASD DEVICE           */ 03048000
*           THEN                    /* YES                           */ 03049000
         SLR   @15,@15                                             1082 03050000
         IC    @15,CNT3                                            1082 03051000
         MH    @15,@CH01192                                        1082 03052000
         L     @11,CVTRTMCT(,RCVT)                                 1082 03053000
         ALR   @11,@15                                             1082 03054000
         TM    RTCTDETP-12(@11),B'00100000'                        1082 03055000
         BNO   @RF01082                                            1082 03056000
*           DO;                                                    1083 03057000
*             DUMPD='0'B;           /* SHOW DASD FOUND IN RTCT       */ 03058000
         NI    DUMPD(R5),B'11011111'                               1084 03059000
*             CNT3=CNT1;            /* FORCE LOOP EXIT               */ 03060000
         STC   CNT1,CNT3                                           1085 03061000
*           END;                                                   1086 03062000
*         ELSE                      /* NOT A DASD DEVICE             */ 03063000
*           DO;                                                    1087 03064000
         B     @RC01082                                            1087 03065000
@RF01082 DS    0H                                                  1088 03066000
*             CNT3=CNT3+1;          /* GET NEXT ENTRY                */ 03067000
         IC    @15,CNT3                                            1088 03068000
         LA    @15,1(,@15)                                         1088 03069000
         STC   @15,CNT3                                            1088 03070000
*           END;                                                   1089 03071000
*       END;                        /* END OF DO WHILE               */ 03072000
@RC01082 DS    0H                                                  1090 03073000
@DE01081 SLR   @15,@15                                             1090 03074000
         IC    @15,CNT3                                            1090 03075000
         CR    CNT1,@15                                            1090 03076000
         BNE   @DL01081                                            1090 03077000
*       IF DUMPD='1'B               /* DASD SPEC BUT NONE ARE      1091 03078000
*                                      AVAILABLE                     */ 03079000
*         THEN                      /* YES                           */ 03080000
         TM    DUMPD(R5),B'00100000'                               1091 03081000
         BNO   @RF01091                                            1091 03082000
*         DO;                       /* WRITE NO DASD MSG             */ 03083000
*           GENERATE REFS(MSG07) SETS(RPARM,R14,R15);              1093 03084000
      IEAPMNIP TYPE=WTO,PARAM=MSG07                                     03085000
*         END;                                                     1094 03086000
*     END;                                                         1095 03087000
@RF01091 DS    0H                                                  1096 03088000
*   R14=R6;                         /* RESTORE RETURN ADDRESS        */ 03089000
@RF01078 LR    R14,R6                                              1096 03090000
*   RETURN;                                                        1097 03091000
@EL00005 DS    0H                                                  1097 03092000
@EF00005 DS    0H                                                  1097 03093000
@ER00005 BR    @14                                                 1097 03094000
*   RFY                                                            1098 03095000
*    (R4,                                                          1098 03096000
*     R6) UNRSTD;                                                  1098 03097000
*   END CHECKTBL;                                                  1099 03098000
*SDIEXCP:                                                          1100 03099000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1100 03100000
*                                                                  1100 03101000
SDIEXCP  DS    0H                                                  1101 03102000
*   /*****************************************************************/ 03103000
*   /*                                                               */ 03104000
*   /* THIS ROUTINE IS ENTERED TO BUILD AN IOB AND ISSUE EXCP REG 1  */ 03105000
*   /* POINTS TO A CCW CHAIN TO BE EXECUTED UPON ENTRY THE INVOKER OF*/ 03106000
*   /* THIS ROUTINE IS RESPONSIBLE FOR CHECKING FOR PROPER COMPLETION*/ 03107000
*   /* OF THE IO OPERATION                                           */ 03108000
*   /*                                                               */ 03109000
*   /*****************************************************************/ 03110000
*                                                                  1101 03111000
*   INTRETAD=R14;                   /* SAVE RETURN ADDRESS           */ 03112000
         ST    R14,INTRETAD(,R5)                                   1101 03113000
*   SDIIOB=''B;                     /* CLEAR IOB AREA                */ 03114000
         XC    SDIIOB(40),SDIIOB                                   1102 03115000
*   IOBFLAG1='42'X;                 /* CMD CHAIN AND UNRELATED       */ 03116000
         L     @15,IOBSTPTR                                        1103 03117000
         MVI   IOBFLAG1(@15),X'42'                                 1103 03118000
*   IOBECBPT=ADDR(EXCPECB);         /* SET ECB ADDRESS               */ 03119000
         LA    @11,EXCPECB                                         1104 03120000
         ST    @11,IOBECBPT(,@15)                                  1104 03121000
*   EXCPECB=EXCPECB&&EXCPECB;       /* 0 ECB                         */ 03122000
         XC    EXCPECB(4),EXCPECB                                  1105 03123000
*   IOBSTART=RPARM;                 /* SET ADDRESS OF CCW STRING     */ 03124000
         ST    RPARM,IOBSTART(,@15)                                1106 03125000
*   IOBDCBPT=ADDR(RTCTDCB(CNT1))-44;/* SET DUMMY DCB ADDRESS TO DEB     03126000
*                                      PTR MINUS THE OFFSET OF THE 1107 03127000
*                                      DEB FIELD OF THE DCB          */ 03128000
         LR    @11,CNT1                                            1107 03129000
         MH    @11,@CH01192                                        1107 03130000
         L     @04,CVTRTMCT(,RCVT)                                 1107 03131000
         LA    @09,RTCTDCB-12(@11,@04)                             1107 03132000
         SL    @09,@CF00288                                        1107 03133000
         ST    @09,IOBDCBPT(,@15)                                  1107 03134000
*   DEBPTR=RTCTDCB(CNT1);           /* GET DEB ADDRESS               */ 03135000
         L     @11,RTCTDCB-12(@11,@04)                             1108 03136000
         ST    @11,DEBPTR                                          1108 03137000
*   DEBDAPTR=ADDR(DEBBASND);        /* ADDR OF DEB EXTENSION         */ 03138000
         LA    DEBDAPTR,DEBBASND(,@11)                             1109 03139000
*   SRCCHH(1:2)=DEBSTRCC;           /* SEARCH -CC ADDRESS            */ 03140000
         MVC   SRCCHH(2),DEBSTRCC(DEBDAPTR)                        1110 03141000
*   SRCCHH(3:4)=DEBSTRHH;           /* SEARCH -HH ADDRESS            */ 03142000
         MVC   SRCCHH+2(2),DEBSTRHH(DEBDAPTR)                      1111 03143000
*   IOBCC=DEBSTRCC;                 /* SEEK ADDRESS -CC              */ 03144000
         MVC   IOBCC(2,@15),DEBSTRCC(DEBDAPTR)                     1112 03145000
*   IOBHH=DEBSTRHH;                 /* SEEK ADDRESS -HH              */ 03146000
         MVC   IOBHH(2,@15),DEBSTRHH(DEBDAPTR)                     1113 03147000
*   RPARM=ADDR(SDIIOB);             /* REG1 POINTS TO IOB FOR EXCP   */ 03148000
         LA    RPARM,SDIIOB                                        1114 03149000
*   GENERATE REFS(RPARM,SDIIOB,EXCPECB) SETS(R0,RPARM,SDIIOB,SDIIOB);   03150000
      EXCP  (RPARM)                                                     03151000
      WAIT  1,ECB=EXCPECB                                               03152000
*   R14=INTRETAD;                   /* RESTORE RETURN ADDRESS        */ 03153000
         L     R14,INTRETAD(,R5)                                   1116 03154000
*   RETURN;                                                        1117 03155000
@EL00006 DS    0H                                                  1117 03156000
@EF00006 DS    0H                                                  1117 03157000
@ER00006 BR    @14                                                 1117 03158000
*   END SDIEXCP;                                                   1118 03159000
*SDITAPE:                                                          1119 03160000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1119 03161000
*                                                                  1119 03162000
SDITAPE  DS    0H                                                  1120 03163000
*   /*****************************************************************/ 03164000
*   /*                                                               */ 03165000
*   /* IF ANY TAPE DEVICES HAVE BEEN PLACED ON THE DEVICE Q THIS     */ 03166000
*   /* ROUTINE IS ENTERED.THIS ROUTINE WILL DETERMINE IF THE NEXT    */ 03167000
*   /* DEVICE TO BE PROCESSED IS TAPE.IF IT IS TAPE IT WILL VALIDATE */ 03168000
*   /* THE DEVICE TYPE AND STATUS.                                   */ 03169000
*   /*                                                               */ 03170000
*   /*****************************************************************/ 03171000
*                                                                  1120 03172000
*   RFY                                                            1120 03173000
*     R4 RSTD;                                                     1120 03174000
*   INTRETAD=R14;                                                  1121 03175000
         ST    R14,INTRETAD(,R5)                                   1121 03176000
*   SKIP='0'B;                      /* TURN OFF THE SKIP FLAG        */ 03177000
         NI    SKIP(R5),B'01111111'                                1122 03178000
*   IF DUMPF='1'B                   /* HAVE 10 DEVICES BEEN ACCEPTED    03179000
*                                      OR HAVE ALL THE REQUESTED   1123 03180000
*                                      DEVICES BEEN PROCESSED        */ 03181000
*     THEN                          /* YES                           */ 03182000
         TM    DUMPF(R5),B'00000001'                               1123 03183000
         BNO   @RF01123                                            1123 03184000
*     DO;                                                          1124 03185000
*       SKIP='1'B;                                                 1125 03186000
         OI    SKIP(R5),B'10000000'                                1125 03187000
*     END;                                                         1126 03188000
*   ELSE                            /* NOT FINISHED YET              */ 03189000
*     DO;                                                          1127 03190000
*                                                                  1127 03191000
         B     @RC01123                                            1127 03192000
@RF01123 DS    0H                                                  1128 03193000
*       /*************************************************************/ 03194000
*       /*                                                           */ 03195000
*       /* THIS SECTION NOW SCANS THE DEVICE QUEUE TO FIND THE NEXT  */ 03196000
*       /* ENTRY TO BE PROCESSED.IF THE QUEUE IS EMPTY OR IF THE     */ 03197000
*       /* ENTRY IS A DASD TYPE DEVICE THE TAPE PROCESSING IS        */ 03198000
*       /* BYPASSED                                                  */ 03199000
*       /*                                                           */ 03200000
*       /*************************************************************/ 03201000
*                                                                  1128 03202000
*       R4=RQUEADD;                 /* ADDRESS OF FIRST ELEMENT ON 1128 03203000
*                                      THE DEVICE Q                  */ 03204000
         L     R4,RQUEADD(,R5)                                     1128 03205000
*       DO WHILE(R4^=0&DPROCSD='1'B);/* FIND FIRST ENTRY THAT HAS NOT   03206000
*                                      BEEN PROCESSED                */ 03207000
         B     @DE01129                                            1129 03208000
@DL01129 DS    0H                                                  1130 03209000
*         R4=NXTENTAD;              /* NEXT ENTRY ADDRESS            */ 03210000
         L     R4,NXTENTAD(,R4)                                    1130 03211000
*       END;                                                       1131 03212000
@DE01129 LTR   R4,R4                                               1131 03213000
         BZ    @DC01129                                            1131 03214000
         TM    DPROCSD(R4),B'01000000'                             1131 03215000
         BO    @DL01129                                            1131 03216000
@DC01129 DS    0H                                                  1132 03217000
*       IF R4=0                     /* ANY LEFT TO PROCESS           */ 03218000
*         THEN                                                     1132 03219000
         LTR   R4,R4                                               1132 03220000
         BNZ   @RF01132                                            1132 03221000
*         DO;                       /* NO                            */ 03222000
*           DUMPF='1'B;                                            1134 03223000
         OI    DUMPF(R5),B'00000001'                               1134 03224000
*           SKIP='1'B;                                             1135 03225000
         OI    SKIP(R5),B'10000000'                                1135 03226000
*         END;                                                     1136 03227000
*       ELSE                                                       1137 03228000
*         DO;                                                      1137 03229000
         B     @RC01132                                            1137 03230000
@RF01132 DS    0H                                                  1138 03231000
*           IF DASDDV='1'B          /* IS IT A DASD DEVICE           */ 03232000
*             THEN                  /* YES                           */ 03233000
         TM    DASDDV(R4),B'10000000'                              1138 03234000
         BNO   @RF01138                                            1138 03235000
*             SKIP='1'B;                                           1139 03236000
         OI    SKIP(R5),B'10000000'                                1139 03237000
*           ELSE                                                   1140 03238000
*             DPROCSD='1'B;         /* INDICATE THIS DEV IS PROCESSED*/ 03239000
         B     @RC01138                                            1140 03240000
@RF01138 OI    DPROCSD(R4),B'01000000'                             1140 03241000
*         END;                                                     1141 03242000
@RC01138 DS    0H                                                  1142 03243000
*     END;                                                         1142 03244000
@RC01132 DS    0H                                                  1143 03245000
*   IF SKIP='0'B                    /* IS A TAPE TO BE PROCESSED     */ 03246000
*     THEN                          /* YES                           */ 03247000
@RC01123 TM    SKIP(R5),B'10000000'                                1143 03248000
         BNZ   @RF01143                                            1143 03249000
*     DO;                                                          1144 03250000
*                                                                  1144 03251000
*       /*************************************************************/ 03252000
*       /*                                                           */ 03253000
*       /* THE TAPE DEVICE SPECIFIED BY THE OPERATOR IS NOW CHECKED  */ 03254000
*       /* TO INSURE THAT IT DOES EXIST AND IT IS MOUNTED AND THAT IT*/ 03255000
*       /* IS A SUPPORTED DEVICE TYPE - IF THE DEVICE IS VALID IT IS */ 03256000
*       /* OPENED AND THE DEVICE IS PLACED IN THE DUMP DATA SET TABLE*/ 03257000
*       /* REG4 UPON ENTRY POINTS TO THE DEVICE ENTRY ON THE DEVICE  */ 03258000
*       /* QUEUE                                                     */ 03259000
*       /*                                                           */ 03260000
*       /*************************************************************/ 03261000
*                                                                  1145 03262000
*       TNAME1=DVNAME;              /* PLACE NAME IN FIELD TO LOAD IN   03263000
*                                      REGISTER EBCDIC NAME IN HI 3     03264000
*                                      BYTES                         */ 03265000
         MVI   TNAME1+3(R5),C' '                                   1145 03266000
         MVC   TNAME1(3,R5),DVNAME(R4)                             1145 03267000
*       RPARM=TNAME1;                                              1146 03268000
         L     RPARM,TNAME1(,R5)                                   1146 03269000
*       GEN REFS(RPARM) SETS(RPARM,R14,R15)                        1147 03270000
*           ( IEAPMNIP TYPE=UCBFN,PARAM=RPARM);                    1147 03271000
          IEAPMNIP TYPE=UCBFN,PARAM=RPARM                               03272000
*       UCBPTR=RPARM;               /* UCB ADDRESS RETURNED IN RPARM */ 03273000
         ST    RPARM,UCBPTR(,R5)                                   1148 03274000
*       IF UCBPTR=0|                /* IS THERE A UCB AND            */ 03275000
*           UCB3TAPE^='1'B|         /* IS IT TAPE                    */ 03276000
*           (UCBTBYT4^='03'X&       /* 3400 TYPE?                    */ 03277000
*           UCBTBYT4^='01'X)        /* OR 2400 TYPE ?                */ 03278000
*         THEN                      /* NO                            */ 03279000
         L     @15,UCBPTR(,R5)                                     1149 03280000
         LTR   @15,@15                                             1149 03281000
         BZ    @RT01149                                            1149 03282000
         TM    UCB3TAPE(@15),B'10000000'                           1149 03283000
         BNO   @RT01149                                            1149 03284000
         CLI   UCBTBYT4(@15),X'03'                                 1149 03285000
         BE    @RF01149                                            1149 03286000
         CLI   UCBTBYT4(@15),X'01'                                 1149 03287000
         BE    @RF01149                                            1149 03288000
@RT01149 DS    0H                                                  1150 03289000
*         SKIP='1'B;                                               1150 03290000
         OI    SKIP(R5),B'10000000'                                1150 03291000
*       ELSE                                                       1151 03292000
*         DO;                                                      1151 03293000
         B     @RC01149                                            1151 03294000
@RF01149 DS    0H                                                  1152 03295000
*           RFY                                                    1152 03296000
*             R6 RSTD;                                             1152 03297000
*           R6=ADDR(MOUNTT);        /* GET ADDRESS OF TAPE MOUNT   1153 03298000
*                                      PARMLIST                      */ 03299000
         LA    R6,MOUNTT                                           1153 03300000
*           R6->NMNTUCB=UCBPTR;     /* PUT UCB ADDRESS IN MOUNT PARM    03301000
*                                      AREA                          */ 03302000
         L     @15,UCBPTR(,R5)                                     1154 03303000
         STCM  @15,15,NMNTUCB(R6)                                  1154 03304000
*           R6->MNTDEC1=UCBTBYT3;   /* SET DEVICE CODE FOR MOUNT     */ 03305000
         MVC   MNTDEC1(1,R6),UCBTBYT3(@15)                         1155 03306000
*           R6->MNTDEC2=UCBTBYT4;                                  1156 03307000
         MVC   MNTDEC2(1,R6),UCBTBYT4(@15)                         1156 03308000
*           GEN REFS(RPARM,MOUNTT) SETS(RPARM,R14,R15);            1157 03309000
      IEAPMNIP TYPE=MOUNT,PARAM=MOUNTT,OPT=(LB,LC)                      03310000
*           IF RPARM=0              /* MOUNT FAIL                    */ 03311000
*             THEN                  /* YES                           */ 03312000
         LTR   RPARM,RPARM                                         1158 03313000
         BNZ   @RF01158                                            1158 03314000
*             SKIP='1'B;                                           1159 03315000
         OI    SKIP(R5),B'10000000'                                1159 03316000
*           ELSE                    /* OKAY MOUNT PROCESS            */ 03317000
*             DO;                   /* NOW ISSUE NIP OPEN            */ 03318000
         B     @RC01158                                            1160 03319000
@RF01158 DS    0H                                                  1161 03320000
*               DSNAME=SYS1DUMP;    /* PLACE SYS1.DUMP IN NAME       */ 03321000
         MVI   DSNAME+9,C' '                                       1161 03322000
         MVC   DSNAME+10(34),DSNAME+9                              1161 03323000
         MVC   DSNAME(9),@CC00283                                  1161 03324000
*               R6=ADDR(DMPOPN);                                   1162 03325000
         LA    R6,DMPOPN                                           1162 03326000
*               R6->NOPNDCB=ADDR(RTCTTDCB(9))-44;/* SET DUMMY DCB  1163 03327000
*                                      ADDRESS IN OPEN PARMS         */ 03328000
         L     @15,CVTRTMCT(,RCVT)                                 1163 03329000
         LA    @15,RTCTTDCB+32(,@15)                               1163 03330000
         SL    @15,@CF00288                                        1163 03331000
         ST    @15,NOPNDCB(,R6)                                    1163 03332000
*               R6->NOPNUCB=UCBPTR; /* PLACE UCB ADDRESS IN OPEN PARM   03333000
*                                      LIST                          */ 03334000
         L     @15,UCBPTR(,R5)                                     1164 03335000
         ST    @15,NOPNUCB(,R6)                                    1164 03336000
*               GEN REFS(DMPOPN) SETS(RPARM,R14,R15);              1165 03337000
      IEAPMNIP TYPE=OPEN,PARAM=DMPOPN,OPT=(NB,LB,LC,SX,,LI,,RS)         03338000
*               IF RPARM<0          /* OPEN FAIL IS INDICATED BY   1166 03339000
*                                      RPARM BEING NEGATIVE          */ 03340000
*                 THEN                                             1166 03341000
         LTR   RPARM,RPARM                                         1166 03342000
         BNM   @RF01166                                            1166 03343000
*                 SKIP='1'B;                                       1167 03344000
         OI    SKIP(R5),B'10000000'                                1167 03345000
*             END;                                                 1168 03346000
@RF01166 DS    0H                                                  1169 03347000
*         END;                                                     1169 03348000
@RC01158 DS    0H                                                  1170 03349000
*       IF SKIP='0'B                /* CAN THE DEVICE BE PUT IN THE     03350000
*                                      DUMP DATA SET TABLE ?         */ 03351000
*         THEN                                                     1170 03352000
@RC01149 TM    SKIP(R5),B'10000000'                                1170 03353000
         BNZ   @RF01170                                            1170 03354000
*         DO;                       /* YES                           */ 03355000
*           RTCTDSNM(CNT1)=DVNAME;  /* PUT NAME IN TABLE             */ 03356000
         LR    @15,CNT1                                            1172 03357000
         MH    @15,@CH01192                                        1172 03358000
         L     @11,CVTRTMCT(,RCVT)                                 1172 03359000
         ST    @15,@TF00001                                        1172 03360000
         ALR   @15,@11                                             1172 03361000
         MVC   RTCTDSNM-12(3,@15),DVNAME(R4)                       1172 03362000
*           RTCTFLG(CNT1)='00'X;    /* INDICATE EMPTY TAPE ENTRY     */ 03363000
         LR    @15,@11                                             1173 03364000
         AL    @15,@TF00001                                        1173 03365000
         MVI   RTCTFLG-12(@15),X'00'                               1173 03366000
*           RTCTDCB(CNT1)=RTCTTDCB(9);/* PUT DEB ADDRESS IN TABLE    */ 03367000
         L     @15,RTCTTDCB+32(,@11)                               1174 03368000
         L     @09,@TF00001                                        1174 03369000
         ST    @15,RTCTDCB-12(@09,@11)                             1174 03370000
*           RTCTDEV(CNT1)=UCBTYP;   /* PLACE DEVICE CODE IN TABLE    */ 03371000
         LR    @15,@11                                             1175 03372000
         AL    @15,@TF00001                                        1175 03373000
         L     @09,UCBPTR(,R5)                                     1175 03374000
         MVC   RTCTDEV-12(4,@15),UCBTYP(@09)                       1175 03375000
*           CNT1=CNT1+1;            /* UP ARRAY INDEX                */ 03376000
         LA    CNT1,1(,CNT1)                                       1176 03377000
*           RTCTSDNO='0'B;          /* INDICATE DEVICE IS IN TABLE   */ 03378000
*                                                                  1177 03379000
         NI    RTCTSDNO(@11),B'01111111'                           1177 03380000
*           /*********************************************************/ 03381000
*           /*                                                       */ 03382000
*           /* NOW THAT A TAPE HAS BEEN PUT INTO THE DUMP DATA SET   */ 03383000
*           /* TABLE IT MUST BE REWOUND TO LOAD POINT SO THAT THE    */ 03384000
*           /* FIRST RECORD ON TAPE WILL BE A DUMP RECORD AND NOT A  */ 03385000
*           /* TAPE MARK                                     @YM04249*/ 03386000
*           /*                                                       */ 03387000
*           /*********************************************************/ 03388000
*                                                                  1178 03389000
*           SDIIOB=''B;             /* CLEAR IOB             @YM04249*/ 03390000
         XC    SDIIOB(40),SDIIOB                                   1178 03391000
*           IOBUNREL='1'B;          /* UNRELATED             @YM04249*/ 03392000
         L     @15,IOBSTPTR                                        1179 03393000
         OI    IOBUNREL(@15),B'00000010'                           1179 03394000
*           EXCPECB=EXCPECB&&EXCPECB;/* CLEAR ECB            @YM04249*/ 03395000
         XC    EXCPECB(4),EXCPECB                                  1180 03396000
*           IOBECBPT=ADDR(EXCPECB); /* ECB IN IOB            @YM04249*/ 03397000
         LA    @09,EXCPECB                                         1181 03398000
         ST    @09,IOBECBPT(,@15)                                  1181 03399000
*           IOBSTART=ADDR(REWCCW);  /* REW CCW               @YM04249*/ 03400000
         LA    @09,REWCCW                                          1182 03401000
         ST    @09,IOBSTART(,@15)                                  1182 03402000
*           IOBDCBPT=ADDR(RTCTTDCB(9))-44;/* USE THE DUMMY DCB IN  1183 03403000
*                                      RTCT                  @YM04249*/ 03404000
         LA    @09,RTCTTDCB+32(,@11)                               1183 03405000
         SL    @09,@CF00288                                        1183 03406000
         ST    @09,IOBDCBPT(,@15)                                  1183 03407000
*           IOBINCAM='0001'X;       /*                       @YM04249*/ 03408000
         MVC   IOBINCAM(2,@15),@CH03833                            1184 03409000
*           RTCTTDCB(1)=0;          /* CLEAR BLOCK COUNT IN DCB    1185 03410000
*                                                            @YM04249*/ 03411000
         SLR   @15,@15                                             1185 03412000
         ST    @15,RTCTTDCB(,@11)                                  1185 03413000
*           RPARM=ADDR(SDIIOB);     /* SET R1 FOR EXCP TO BE ISSUED     03414000
*                                                            @YM04249*/ 03415000
         LA    RPARM,SDIIOB                                        1186 03416000
*           GEN REFS(RPARM,SDIIOB,EXCPECB) SETS(RPARM,SDIIOB);     1187 03417000
*                                                  /*       @YM04249*/  03418000
         EXCP (RPARM)                                                   03419000
         WAIT 1,ECB=EXCPECB                                             03420000
*           IF CNT1>10              /* ALL 10 ENTRIES PROCESSED ?    */ 03421000
*             THEN                  /* YES                           */ 03422000
         C     CNT1,@CF01260                                       1188 03423000
         BNH   @RF01188                                            1188 03424000
*             DO;                                                  1189 03425000
*               DUMPF='1'B;         /* INDICATE FINISHED             */ 03426000
         OI    DUMPF(R5),B'00000001'                               1190 03427000
*             END;                                                 1191 03428000
*         END;                                                     1192 03429000
@RF01188 DS    0H                                                  1193 03430000
*       RFY                                                        1193 03431000
*         R6 UNRSTD;                                               1193 03432000
@RF01170 DS    0H                                                  1194 03433000
*       IF SKIP='1'B                /* IS THE DEVICE VALID           */ 03434000
*         THEN                      /* NO                            */ 03435000
         TM    SKIP(R5),B'10000000'                                1194 03436000
         BNO   @RF01194                                            1194 03437000
*         DO;                                                      1195 03438000
*           CALL INVALTA;           /* PROCESS INVALID TAPE REQUEST  */ 03439000
         BAL   @14,INVALTA                                         1196 03440000
*         END;                                                     1197 03441000
*     END;                                                         1198 03442000
@RF01194 DS    0H                                                  1199 03443000
*   SKIP='0'B;                      /* RESET SKIP FLAG               */ 03444000
@RF01143 NI    SKIP(R5),B'01111111'                                1199 03445000
*   RFY                                                            1200 03446000
*     R4 UNRSTD;                                                   1200 03447000
*   R14=INTRETAD;                                                  1201 03448000
         L     R14,INTRETAD(,R5)                                   1201 03449000
*   RETURN;                                                        1202 03450000
@EL00007 DS    0H                                                  1202 03451000
@EF00007 DS    0H                                                  1202 03452000
@ER00007 BR    @14                                                 1202 03453000
*   END SDITAPE;                                                   1203 03454000
*INVALTA:                                                          1204 03455000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1204 03456000
INVALTA  DS    0H                                                  1205 03457000
*   RFY                                                            1205 03458000
*    (R4,                                                          1205 03459000
*     R6) RSTD;                                                    1205 03460000
*                                                                  1205 03461000
*   /*****************************************************************/ 03462000
*   /*                                                               */ 03463000
*   /* THIS ROUTINE IS ENTERED BECAUSE AN ERROR OCCURRED WHILE       */ 03464000
*   /* PROCESSING A TAPE DEVICE.UPON ENTRY REG 4 POINTS TO THE ENTRY */ 03465000
*   /* IN THE DEVICE Q BEING PROCESSED THE DEVICE ADDRESS IS PLACED  */ 03466000
*   /* IN MESSAGE IEA879A AND THE OPERATOR CAN ENTER AN EOB TO BYPASS*/ 03467000
*   /* THE DEVICE OR RESPECIFY THE SAME DEVICE.IF A DEVICE IS        */ 03468000
*   /* SPECIFIED THAT HAS BEEN PREVIOUSLY SPECIFIED IT IS AN ERROR   */ 03469000
*   /*                                                               */ 03470000
*   /*****************************************************************/ 03471000
*                                                                  1206 03472000
*   R6=R14;                                                        1206 03473000
         LR    R6,R14                                              1206 03474000
*   INVTA=DVNAME;                   /* PLACE DEVICE ADDRESS IN     1207 03475000
*                                      MESSAGE                       */ 03476000
         MVC   INVTA(3),DVNAME(R4)                                 1207 03477000
*   REPROCSS='1'B;                  /* INDICATE THAT THIS DEVICE CAN    03478000
*                                      BE RESPECIFIED                */ 03479000
         OI    REPROCSS(R4),B'00100000'                            1208 03480000
*   ENTSAVE=R4;                     /* SAVE THE ADDRESS FOR THIS   1209 03481000
*                                      ELEMENT                       */ 03482000
         ST    R4,ENTSAVE(,R5)                                     1209 03483000
*   VALIDSW='0'B;                   /* SET FOR LOOP CONTROL UNTIL  1210 03484000
*                                      OPERATOR RESPONDS CORRECTLY TO   03485000
*                                      MSG IEA879A                   */ 03486000
         NI    VALIDSW(R5),B'11111101'                             1210 03487000
*   DO WHILE(VALIDSW='0'B);                                        1211 03488000
         B     @DE01211                                            1211 03489000
@DL01211 DS    0H                                                  1212 03490000
*     GENERATE REFS(MSG04,MSG05,RNVT) SETS(RPARM,R14,R15);         1212 03491000
     IEAPMNIP TYPE=WTOR,PARAM=MSG05   WRITE MESSAGE IEA879A             03492000
*     CURRENT=RPARM;                /* REPLY IS POINTED TO BY RPARM  */ 03493000
         ST    RPARM,CURRENT(,R5)                                  1213 03494000
*     BEGIN=RPARM;                                                 1214 03495000
         ST    RPARM,BEGIN(,R5)                                    1214 03496000
*     ENDCOL=RPARM;                                                1215 03497000
         ST    RPARM,ENDCOL(,R5)                                   1215 03498000
*     IF CURCHAR=' '&NXTCHAR=''''   /* EOB IS INDICATED BY A BLANK 1216 03499000
*                                      QUOTE                         */ 03500000
*       THEN                        /* YES IT IS AN EOB              */ 03501000
         L     @15,CURRENT(,R5)                                    1216 03502000
         CLI   CURCHAR(@15),C' '                                   1216 03503000
         BNE   @RF01216                                            1216 03504000
         CLI   NXTCHAR(@15),C''''                                  1216 03505000
         BNE   @RF01216                                            1216 03506000
*       DO;                                                        1217 03507000
*         VALIDSW='1'B;             /* SKIP THE RETRY PROCESS        */ 03508000
         OI    VALIDSW(R5),B'00000010'                             1218 03509000
*       END;                                                       1219 03510000
*     ELSE                          /* NOT AN EOB RESPONSE           */ 03511000
*       DO;                         /* RESPONSE MUST BE IN THE FORMAT   03512000
*                                      (TA,DDD)                      */ 03513000
         B     @RC01216                                            1220 03514000
@RF01216 DS    0H                                                  1221 03515000
*         DO WHILE(CURCHAR^='''');  /* FIND ENDING QUOTE             */ 03516000
         B     @DE01221                                            1221 03517000
@DL01221 DS    0H                                                  1222 03518000
*           CURRENT=CURRENT+1;      /* GET NEXT BYTE                 */ 03519000
         LA    @15,1                                               1222 03520000
         AL    @15,CURRENT(,R5)                                    1222 03521000
         ST    @15,CURRENT(,R5)                                    1222 03522000
*         END;                                                     1223 03523000
@DE01221 L     @15,CURRENT(,R5)                                    1223 03524000
         CLI   CURCHAR(@15),C''''                                  1223 03525000
         BNE   @DL01221                                            1223 03526000
*         ENDCOL=CURRENT;           /* SAVE ENDING ADDRESS           */ 03527000
         ST    @15,ENDCOL(,R5)                                     1224 03528000
*         CURRENT=BEGIN+8;          /* RESPONSE MUST BE 8 CHAR       */ 03529000
         LA    @11,8                                               1225 03530000
         AL    @11,BEGIN(,R5)                                      1225 03531000
         ST    @11,CURRENT(,R5)                                    1225 03532000
*         IF CURRENT^=ENDCOL        /* IS IT 8 BYTES LONG            */ 03533000
*           THEN                    /* NO                            */ 03534000
         CR    @11,@15                                             1226 03535000
         BE    @RF01226                                            1226 03536000
*           CURRENT=0;                                             1227 03537000
         SLR   @15,@15                                             1227 03538000
         ST    @15,CURRENT(,R5)                                    1227 03539000
*         ELSE                      /* IT IS 8 BYTES                 */ 03540000
*           DO;                                                    1228 03541000
         B     @RC01226                                            1228 03542000
@RF01226 DS    0H                                                  1229 03543000
*             CURRENT=BEGIN;                                       1229 03544000
         L     @15,BEGIN(,R5)                                      1229 03545000
         ST    @15,CURRENT(,R5)                                    1229 03546000
*             SCANBEG=CURRENT+1;                                   1230 03547000
*                                                                  1230 03548000
         LA    @11,1                                               1230 03549000
         ALR   @11,@15                                             1230 03550000
         ST    @11,SCANBEG(,R5)                                    1230 03551000
*             /*******************************************************/ 03552000
*             /*                                                     */ 03553000
*             /* NOW CHECK FOR (TA,XXX)                              */ 03554000
*             /*                                                     */ 03555000
*             /*******************************************************/ 03556000
*                                                                  1231 03557000
*             IF CURCHAR^='('|FIRST3^='TA,' THEN/* SYNTAX ERROR      */ 03558000
