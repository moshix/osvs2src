*                 CNT1=CNT1+1;      /* UP COUNTER TO ADDRESS NEXT  0677 01613000
*                                      ELEMENT IN DATA SET TABLE     */ 01614000
         LA    CNT1,1(,CNT1)                                       0677 01615000
*               END;                                               0678 01616000
*             ELSE                  /* NOT A GOOD READ               */ 01617000
*               DO;                                                0679 01618000
         B     @RC00674                                            0679 01619000
@RF00674 DS    0H                                                  0680 01620000
*                 IF UNITCHK='1'B|  /* UNIT CHECK OR                 */ 01621000
*                     PGMCHK='1'B|  /* PROGRAM CHECK OR              */ 01622000
*                     PROTECT='1'B| /* PROTECT CHECK OR              */ 01623000
*                     DATACHK='1'B| /* CHANNEL DATA CHECK OR         */ 01624000
*                     CONTROL='1'B| /* CHAN CONTROL CHECK OR         */ 01625000
*                     INTFCHK='1'B| /* INTERFACE CNTRL CHECK OR      */ 01626000
*                     CHAINCK='1'B| /* CHAIN CHECK OR                */ 01627000
*                     UNITEXC='0'B  /* UNIT EXCEPTION IS NOT PRESENT */ 01628000
*                   THEN            /* CANNOT READ THE FIRST RECORD     01629000
*                                      SO NOW TRY TO EMPTY IT BY   0680 01630000
*                                      WRITING AN EOF MARKER ON IT 0680 01631000
*                                                            @YM05431*/ 01632000
         L     @15,IOBSTPTR                                        0680 01633000
