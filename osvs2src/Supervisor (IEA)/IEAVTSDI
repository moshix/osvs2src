         TITLE 'IEAVTSDI-SDUMP INITIALIZATION RIM                      *00001000
                        '                                               00002000
IEAVTSDI CSECT ,                                                   0001 00003000
@MAINENT BALR  @15,0                                               0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVTSDI  77.341'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @07,0                                               0001 00011000
@PSTART  LA    @10,4095(,@07)                                      0001 00012000
         USING @PSTART,@07                                         0001 00013000
         USING @PSTART+4095,@10                                    0001 00014000
*                                                                       00015000
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF           00016000
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES       00017000
* WILL ASSEMBLE PROPERLY.                                               00018000
*                                                                  0228 00019000
*   /*****************************************************************/ 00020000
*   /*                                                               */ 00021000
*   /* THIS SECTION OF CODE OBTAINS 4K OF STORAGE FROM SUBPOOL 245   */ 00022000
*   /* SQA TO BE USED AS THE SDUMP 4K BUFFER AND STORE THE BUFFER    */ 00023000
*   /* ADDRESS IN THE CVT. A GETMAIN IS ISSUED FOR SUBPOOL 231 CSA.  */ 00024000
*   /* THE AMOUNT OF STORAGE OBTAINED IS DETERMINED BY A CONSTANT    */ 00025000
*   /* DECLARED IN IEAVTSDI SDWLEN. THE SPACE BEING OBTAINED WILL BE */ 00026000
*   /* USED AS THE SVC DUMP WORK AREA. THE ADDRESS OF THE WORK AREA  */ 00027000
*   /* IS PLACED INTO THE RTCT. STORAGE IS OBTAINED FROM SUBPOOL 245 */ 00028000
*   /* SQA FOR THE SUMMARY DUMP WORK AREA. THE ADDRESS OF THIS AREA  */ 00029000
*   /* IS STORED IN THE RTCTSDSW FIELD OF THE RTCT.          @G33VPHD*/ 00030000
*   /*                                                               */ 00031000
*   /*****************************************************************/ 00032000
*                                                                  0228 00033000
*   CVTSDBF=0;                      /* CLEAR BUFFER ADDRESS          */ 00034000
*                                                                  0228 00035000
         SLR   @15,@15                                             0228 00036000
         ST    @15,CVTSDBF(,RCVT)                                  0228 00037000
*   /*****************************************************************/ 00038000
*   /*                                                               */ 00039000
*   /* GETMAIN FOR SDUMP 4K BUFFER FROM SQA                          */ 00040000
*   /*                                                               */ 00041000
*   /*****************************************************************/ 00042000
*                                                                  0229 00043000
*   DO;                             /* GETMAIN (EU) BNDRY LV(4096) 0229 00044000
*                                      A(CVTSDBF) SP(245)            */ 00045000
*     RESPECIFY                                                    0230 00046000
*      (GPR01P) RESTRICTED;                                        0230 00047000
*     I004=I004&&I004;              /* INIT LIST                     */ 00048000
         XC    I004(10),I004                                       0231 00049000
*     I00404='10'X;                 /* TYPE                          */ 00050000
         MVI   I00404,X'10'                                        0232 00051000
*     I00401=4096;                  /* LENGTH                        */ 00052000
         MVC   I00401(4),@CF03750                                  0233 00053000
*     I00403=ADDR(CVTSDBF);         /* ADDR OF ADDR LIST             */ 00054000
         LA    @15,CVTSDBF(,RCVT)                                  0234 00055000
         STCM  @15,7,I00403                                        0234 00056000
*     I00405=245;                   /* SUBPOOL VALUE                 */ 00057000
         MVI   I00405,X'F5'                                        0235 00058000
*     GPR01P=ADDR(I004);            /* REG1 POINTS TO LIST           */ 00059000
         LA    GPR01P,I004                                         0236 00060000
*     SVC(4);                       /* STANDARD-FORM OF GETMAIN      */ 00061000
         SVC   4                                                   0237 00062000
*     RESPECIFY                                                    0238 00063000
*      (GPR01P) UNRESTRICTED;                                      0238 00064000
*   END;                            /* GETMAIN (EU) BNDRY LV(4096) 0239 00065000
*                                      A(CVTSDBF) SP(245)            */ 00066000
*   CVTSDBF=CVTSDBF|'80000000'X;    /* TURN ON HI-ORDER BIT BECAUSE     00067000
*                                      THIS ROUTINE WILL USE THE   0240 00068000
*                                      BUFFER BIT INDICATES THAT THE    00069000
*                                      BUFFER IS IN USE              */ 00070000
         L     @15,CVTSDBF(,RCVT)                                  0240 00071000
         O     @15,@CF03755                                        0240 00072000
         ST    @15,CVTSDBF(,RCVT)                                  0240 00073000
*   RTCTSDSW=0;                     /* CLEAR SUMDUMP WORK AREA     0241 00074000
*                                      ADDRESS               @G33VPHD*/ 00075000
*                                                                  0241 00076000
         L     @15,CVTRTMCT(,RCVT)                                 0241 00077000
         SLR   @12,@12                                             0241 00078000
         ST    @12,RTCTSDSW(,@15)                                  0241 00079000
*   /*****************************************************************/ 00080000
*   /*                                                               */ 00081000
*   /* GETMAIN SQA WORK SPACE FOR SUMDUMP WORK AREA          @G33VPHD*/ 00082000
*   /*                                                               */ 00083000
*   /*****************************************************************/ 00084000
*                                                                  0242 00085000
*   DO;                             /* GETMAIN (EU) LV(SMWKLEN)    0242 00086000
*                                      A(RTCTSDSW) SP(245)           */ 00087000
*     RESPECIFY                                                    0243 00088000
*      (GPR01P) RESTRICTED;                                        0243 00089000
*     I004=I004&&I004;              /* INIT LIST                     */ 00090000
         XC    I004(10),I004                                       0244 00091000
*     I00404='00'X;                 /* TYPE                          */ 00092000
         MVI   I00404,X'00'                                        0245 00093000
*     I00401=SMWKLEN;               /* LENGTH                        */ 00094000
         L     @12,SMWKLEN                                         0246 00095000
         ST    @12,I00401                                          0246 00096000
*     I00403=ADDR(RTCTSDSW);        /* ADDR OF ADDR LIST             */ 00097000
         LA    @15,RTCTSDSW(,@15)                                  0247 00098000
         STCM  @15,7,I00403                                        0247 00099000
*     I00405=245;                   /* SUBPOOL VALUE                 */ 00100000
         MVI   I00405,X'F5'                                        0248 00101000
*     GPR01P=ADDR(I004);            /* REG1 POINTS TO LIST           */ 00102000
         LA    GPR01P,I004                                         0249 00103000
*     SVC(4);                       /* STANDARD-FORM OF GETMAIN      */ 00104000
         SVC   4                                                   0250 00105000
*     RESPECIFY                                                    0251 00106000
*      (GPR01P) UNRESTRICTED;                                      0251 00107000
*   END;                            /* GETMAIN (EU) LV(SMWKLEN)    0252 00108000
*                                      A(RTCTSDSW) SP(245) OBTAIN  0252 00109000
*                                      SUMMARY DUMP WORK AREA SQA  0252 00110000
*                                      SPACE                 @G33VPHD*/ 00111000
*   SMWK(1:256)=''B;                /* INITIALIZE            @ZA28457*/ 00112000
         L     @15,CVTRTMCT(,RCVT)                                 0253 00113000
         L     @12,RTCTSDSW(,@15)                                  0253 00114000
         XC    SMWK(256,@12),SMWK(@12)                             0253 00115000
*   SMWK(257:512)=''B;              /* THE                   @ZA28457*/ 00116000
         XC    SMWK+256(256,@12),SMWK+256(@12)                     0254 00117000
*   SMWK(513:768)=''B;              /* OLD                   @ZA28457*/ 00118000
         XC    SMWK+512(256,@12),SMWK+512(@12)                     0255 00119000
*   SMWK(769:LENGTH(SMWK))=''B;     /* SMWK                  @ZA28457*/ 00120000
         XC    SMWK+768(212,@12),SMWK+768(@12)                     0256 00121000
*   SMWKNAME='SMWK';                /* PUT SMWK NAME IN SUMDUMP WORK    00122000
*                                      AREA                  @G33VPHD*/ 00123000
*                                                                  0257 00124000
         MVC   SMWKNAME(4,@12),@CC03761                            0257 00125000
*   /*****************************************************************/ 00126000
*   /*                                                               */ 00127000
*   /* GETMAIN CSA SPACE FOR SDUMP WORK AREA                 @G33VPHD*/ 00128000
*   /*                                                               */ 00129000
*   /*****************************************************************/ 00130000
*                                                                  0258 00131000
*   DO;                             /* GETMAIN (EU) LV(SDWLEN)     0258 00132000
*                                      A(RTCTSDWK) SP(231)           */ 00133000
*     RESPECIFY                                                    0259 00134000
*      (GPR01P) RESTRICTED;                                        0259 00135000
*     I004=I004&&I004;              /* INIT LIST                     */ 00136000
         XC    I004(10),I004                                       0260 00137000
*     I00404='00'X;                 /* TYPE                          */ 00138000
         MVI   I00404,X'00'                                        0261 00139000
*     I00401=SDWLEN;                /* LENGTH                        */ 00140000
         LH    @12,SDWLEN                                          0262 00141000
         ST    @12,I00401                                          0262 00142000
*     I00403=ADDR(RTCTSDWK);        /* ADDR OF ADDR LIST             */ 00143000
         LA    @15,RTCTSDWK(,@15)                                  0263 00144000
         STCM  @15,7,I00403                                        0263 00145000
*     I00405=231;                   /* SUBPOOL VALUE                 */ 00146000
         MVI   I00405,X'E7'                                        0264 00147000
*     GPR01P=ADDR(I004);            /* REG1 POINTS TO LIST           */ 00148000
         LA    GPR01P,I004                                         0265 00149000
*     SVC(4);                       /* STANDARD-FORM OF GETMAIN      */ 00150000
         SVC   4                                                   0266 00151000
*     RESPECIFY                                                    0267 00152000
*      (GPR01P) UNRESTRICTED;                                      0267 00153000
*   END;                            /* GETMAIN (EU) LV(SDWLEN)     0268 00154000
*                                      A(RTCTSDWK) SP(231)   @G33VPHD*/ 00155000
*   R5=CVTSDBF;                     /* SET ADDRESSABILITY FOR SQA  0269 00156000
*                                      SPACE FOR USE AS WORK SPACE BY   00157000
*                                      THIS MODULE                   */ 00158000
         L     R5,CVTSDBF(,RCVT)                                   0269 00159000
*   WORKAREA=WORKAREA&&WORKAREA;    /* CLEAR WORK AREA               */ 00160000
         XC    WORKAREA(152,R5),WORKAREA(R5)                       0270 00161000
*   SDIECB=0;                       /* INITIALIZE ECB        @G33VPHD*/ 00162000
         SLR   @15,@15                                             0271 00163000
         ST    @15,SDIECB                                          0271 00164000
*   RFY                                                            0272 00165000
*     SDWORK BASED(RTCTSDWK);       /* ADDRESS SDUMP WORK AREA     0272 00166000
*                                                            @ZM44953*/ 00167000
*   SDCNTRL(1:256)=''B;             /* INITIALIZE SDUMP WORK AREA  0273 00168000
*                                                            @ZM44953*/ 00169000
         L     @15,CVTRTMCT(,RCVT)                                 0273 00170000
         L     @15,RTCTSDWK(,@15)                                  0273 00171000
         XC    SDCNTRL(256,@15),SDCNTRL(@15)                       0273 00172000
*   SDCNTRL(257:LENGTH(SDCNTRL))=''B;/* INITIALIZE SDUMP WORK AREA 0274 00173000
*                                                            @ZM44953*/ 00174000
         XC    SDCNTRL+256(176,@15),SDCNTRL+256(@15)               0274 00175000
*   SAVNVTAD=RNVT;                  /* SAVE NVT ADDRESS IN WORK AREA    00176000
*                                                            @G33VPHD*/ 00177000
         ST    RNVT,SAVNVTAD(,R5)                                  0275 00178000
*   SAVCVTAD=RCVT;                  /* SAVE CVT ADDRESS IN WORK AREA    00179000
*                                                            @G33VPHD*/ 00180000
         ST    RCVT,SAVCVTAD(,R5)                                  0276 00181000
*   SVER13=R13;                     /* SAVE SAVE AREA ADDRESS      0277 00182000
*                                                            @G33VPHD*/ 00183000
         ST    R13,SVER13(,R5)                                     0277 00184000
*   SDWORKEN=RTCTSDWK+LENGTH(SDWORK);/* GET SDWORK END ADDR  @G33VPHD*/ 00185000
         AL    @15,@CF03855                                        0278 00186000
         ST    @15,SDWORKEN(,R5)                                   0278 00187000
*   GEN REFS(PSALITA);                                             0279 00188000
*                                                                  0279 00189000
*                      /*GET LOCAL LOCK FOR PGFIX INTERFACE @G33VPHD*/  00190000
GETLOCKA SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                       XX00191000
               RELATED=(SDWORK,IEAVTSDI(FRELOCKA))                      00192000
*   /*****************************************************************/ 00193000
*   /*                                                               */ 00194000
*   /* USE PGFIX BRANCH ENTRY TO PAGE FIX SDUMP WORK AREA    @G33VPHD*/ 00195000
*   /*                                                               */ 00196000
*   /*****************************************************************/ 00197000
*                                                                  0280 00198000
*   RFY                                                            0280 00199000
*    (R0,                                                          0280 00200000
*     R1,                                                          0280 00201000
*     R4) RSTD;                     /* R2 IS ALREADY RSTD    @G33VPHD*/ 00202000
*   R0=ADDR(SDIECB);                /* GET ECB ADDRESS FOR PGFIX   0281 00203000
*                                                            @G33VPHD*/ 00204000
         LA    R0,SDIECB                                           0281 00205000
*   R1=RTCTSDWK;                    /* GET PGFIX BEGINNING ADDR    0282 00206000
*                                                            @G33VPHD*/ 00207000
         L     @15,CVTRTMCT(,RCVT)                                 0282 00208000
         L     R1,RTCTSDWK(,@15)                                   0282 00209000
*   R1=R1|PGFIXFLG;                 /* OR ON BITS IN R1 FOR PGFIX  0283 00210000
*                                                            @G33VPHD*/ 00211000
         O     R1,@CF00148                                         0283 00212000
*   R2=SDWORKEN;                    /* GET PGFIX ENDING ADDR @G33VPHD*/ 00213000
         L     R2,SDWORKEN(,R5)                                    0284 00214000
*   R4=NULL;                        /* ZERO TCB ADDR FOR NO TCB    0285 00215000
*                                      AFFINITY              @G33VPHD*/ 00216000
         SLR   R4,R4                                               0285 00217000
*   CALL IEAVPSIB;                  /* BRANCH ENTER PGFIX AND PAGE 0286 00218000
*                                      FIX SVC DUMP WORK AREA      0286 00219000
*                                                            @G33VPHD*/ 00220000
         L     @12,PVTPTR(,RCVT)                                   0286 00221000
         L     @15,PVTPSIB(,@12)                                   0286 00222000
         BALR  @14,@15                                             0286 00223000
*   GEN REFS(PSALITA);                                             0287 00224000
*                       /*FREE LOCAL AFTER PGFIX RETURNS    @G33VPHD*/  00225000
FRELOCKA SETLOCK RELEASE,TYPE=LOCAL,                                  XX00226000
               RELATED=(SDWORK,IEAVTSDI(GETLOCKA))                      00227000
*   R1=ADDR(SDIECB);                /* FET ECB ADDRESS FOR WAIT    0288 00228000
*                                                            @G33VPHD*/ 00229000
         LA    R1,SDIECB                                           0288 00230000
*   GEN(WAIT  1,ECB=(R1));          /* WAIT FOR PGFIX TO COMPLETE  0289 00231000
*                                                            @G33VPHD*/ 00232000
         WAIT  1,ECB=(R1)                                               00233000
*   SDWORKEN=0;                     /* ZERO WORK AREA FIELD USED   0290 00234000
*                                                            @G33VPHD*/ 00235000
         SLR   @15,@15                                             0290 00236000
         ST    @15,SDWORKEN(,R5)                                   0290 00237000
*   RNVT=SAVNVTAD;                  /* RESTORE NVT ADDRESS   @G33VPHD*/ 00238000
         L     RNVT,SAVNVTAD(,R5)                                  0291 00239000
*                                                                  0292 00240000
*/********************************************************************/ 00241000
*/*   THIS SEGMENT FINDS THE ADDRESSES OF LPALIB SVC DUMP MODULES    */ 00242000
*/*   AND STORES THEIR ADDRESSES IN CONTROL BLOCKS AND WORK AREAS    */ 00243000
*/*   FOR LATER USE. LOAD IS USED TO FIND THE ADDRESS AND LENGTH     */ 00244000
*/*   OF THE MODULES. DELETE IS USED TO DELETE THE EXTRA CDE         */ 00245000
*/*   ENTRIES CREATED BY LOADING THESE LPA MODULES.  MODULE          */ 00246000
*/*   IEAVTSDR IS LOCATED AND ITS ADDRESS IS STORED IN THE CVT. IF   */ 00247000
*/*   MODULE IEAVTSDR COULD NOT BE FOUND, MESSAGE IEA883I IS ISSUED. */ 00248000
*/*   THE ADDRESS OF MODULE IEAVTSSD IS FOUND AND SAVED IN THE SMWK. */ 00249000
*/*   IF IEAVTSSD COULD NOT BE FOUND, MESSAGE IEA883I IS ISSUED.     */ 00250000
*/*   THE ENDING ADDRESS OF IEAVTSSD IS CALCULATED AND STORED IN THE */ 00251000
*/*   SMWK. THE BEGINNING AND ENDING ADDRESSES OF THE SECTION OF THE */ 00252000
*/*   LOAD MODULE TO BE PAGE FIXED (CSECTS: IEAVTSDT, IEAVTSDH,      */ 00253000
*/*   IEAVTSDW, IEAVTSDO) ARE CALCULATED AND STORED IN THE SMWK.     */ 00254000
*/*   IEAVTSSD AND THIS SECTION OF LOAD MODULE IGC0005A ARE PAGE     */ 00255000
*/*   FIXED IN REAL STORAGE. IF MODULES IGC0005A OR IEAVTSDT ARE NOT */ 00256000
*/*   FOUND MESSAGE IEA883I IS ISSUED.                               */ 00257000
*/*                                                                  */ 00258000
*/*   **NOTE**  IEAVTSDO, IEAVTSDT, IEAVTSDH, IEAVTSDW MUST BE THE   */ 00259000
*/*             LAST 4 CSECTS OF LOAD MODULE IGC0005A. IEAVTSDT      */ 00260000
*/*             MUST BE ON A PAGE BOUNDARY                           */ 00261000
*/*                                                                  */ 00262000
*/*   INCLUDE BY: TOP SEGMENT                                        */ 00263000
*/*                                                                  */ 00264000
*/********************************************************************/ 00265000
*                                                                  0292 00266000
*   CVTSDRM=NULL;                   /* INITIALIZE SDR POINTER      0292 00267000
*                                                            @G33VPHD*/ 00268000
         ST    @15,CVTSDRM(,RCVT)                                  0292 00269000
*   SMWKENTY=NULL;                  /* INITIALIZE SSD ADDRESS      0293 00270000
*                                                            @G33VPHD*/ 00271000
         L     @12,CVTRTMCT(,RCVT)                                 0293 00272000
         L     @12,RTCTSDSW(,@12)                                  0293 00273000
         ST    @15,SMWKENTY(,@12)                                  0293 00274000
*   SMWKSSDE=NULL;                  /* INIT SSD END ADDRESS  @G33VPHD*/ 00275000
         ST    @15,SMWKSSDE(,@12)                                  0294 00276000
*   SMWKPFXB=NULL;                  /* INIT IGC0005A FIXED AREA    0295 00277000
*                                                            @G33VPHD*/ 00278000
         ST    @15,SMWKPFXB(,@12)                                  0295 00279000
*   SMWKPFXE=NULL;                  /* INIT END OF IGC0005A ADDR   0296 00280000
*                                                            @G33VPHD*/ 00281000
         ST    @15,SMWKPFXE(,@12)                                  0296 00282000
*   SDRNAME='IEAVTSDR';             /* SET NAME TO SEARCH FOR SDUMP     00283000
*                                      RESOURCE MANAGER              */ 00284000
*                                                                  0297 00285000
         MVC   SDRNAME(8,R5),@CC03764                              0297 00286000
*   /*****************************************************************/ 00287000
*   /*                                                               */ 00288000
*   /* LOAD MODULE IEAVTSDR                                  @G33VPHD*/ 00289000
*   /*                                                               */ 00290000
*   /*****************************************************************/ 00291000
*                                                                  0298 00292000
*   DO;                             /* LOAD EP(SDRNAME)            0298 00293000
*                                      ERRET(NSDRLOAD)               */ 00294000
*     RESPECIFY                                                    0299 00295000
*      (GPR00P,                                                    0299 00296000
*       GPR01P,                                                    0299 00297000
*       GPR15F) RESTRICTED;                                        0299 00298000
*     GPR00P=ADDR(SDRNAME);         /* LOAD ENTRY POINT NAME ADDRESS */ 00299000
         LA    GPR00P,SDRNAME(,R5)                                 0300 00300000
*     GPR01P=0;                     /* JOBLIB OR LINKLIB DCB         */ 00301000
         SLR   GPR01P,GPR01P                                       0301 00302000
*     GPR01P=GPR01P|'80000000'X;    /* ERROR RETURN LABEL SPECIFIED  */ 00303000
         O     GPR01P,@CF03755                                     0302 00304000
*     SVC(8);                       /* ISSUE LOAD SVC                */ 00305000
         SVC   8                                                   0303 00306000
*     IF GPR15F^=0 THEN             /* LOAD WAS UNSUCCESSFUL         */ 00307000
         SLR   @12,@12                                             0304 00308000
         CR    GPR15F,@12                                          0304 00309000
         BNE   @RT00304                                            0304 00310000
*       GOTO NSDRLOAD;              /* BRANCH TO ERROR LOCATION      */ 00311000
*     RESPECIFY                                                    0306 00312000
*      (GPR00P,                                                    0306 00313000
*       GPR01P,                                                    0306 00314000
*       GPR15F) UNRESTRICTED;                                      0306 00315000
*   END;                            /* LOAD EP(SDRNAME)            0307 00316000
*                                      ERRET(NSDRLOAD) OBTAIN ADDRESS   00317000
*                                      OF MODULE IEAVTSDR    @G33VPHD*/ 00318000
*   CVTSDRM=R0+20;                  /* PUT RESOURCE MANAGER ENTRY  0308 00319000
*                                      ADDRESS IN CVT        @ZM45193*/ 00320000
         LA    @15,20                                              0308 00321000
         ALR   @15,R0                                              0308 00322000
         ST    @15,CVTSDRM(,RCVT)                                  0308 00323000
*   IF CVTSDRM=NULL THEN            /* IF MODULE IEAVTSDR NOT FOUND     00324000
*                                                            @G33VPHD*/ 00325000
         CR    @15,@12                                             0309 00326000
         BNE   @RF00309                                            0309 00327000
*     DO;                           /* ISSUE OPERATOR MESSAGE      0310 00328000
*                                                            @G33VPHD*/ 00329000
*NSDRLOAD:                          /* LABEL BRANCHED TO IF NON-ZERO    00330000
*                                      RETURN CODE OBTAINED FROM LOAD   00331000
*                                                            @G33VPHD*/ 00332000
*       RNVT=SAVNVTAD;              /* RESTORE NVT ADDRESS   @G33VPHD*/ 00333000
NSDRLOAD L     RNVT,SAVNVTAD(,R5)                                  0311 00334000
*       GEN(IEAPMNIP TYPE=WTO,PARAM=MSG01);/* ISSUE MESSAGE  @G33VPHD*/ 00335000
         IEAPMNIP TYPE=WTO,PARAM=MSG01                                  00336000
*     END;                          /* END OF SDR NOT FOUND  @G33VPHD*/ 00337000
*   ELSE                                                           0314 00338000
*     DO;                           /* IF SDR FOUND DELETE IT      0314 00339000
*                                                            @G33VPHD*/ 00340000
         B     @RC00309                                            0314 00341000
@RF00309 DS    0H                                                  0315 00342000
*       DO;                         /* DELETE EP(SDRNAME)            */ 00343000
*         RESPECIFY                                                0316 00344000
*          (GPR00P) RESTRICTED;                                    0316 00345000
*         GPR00P=ADDR(SDRNAME);     /* ->ENTRY NAME                  */ 00346000
         LA    GPR00P,SDRNAME(,R5)                                 0317 00347000
*         SVC(9);                   /* DELETE SVC                    */ 00348000
         SVC   9                                                   0318 00349000
*         RESPECIFY                                                0319 00350000
*          (GPR00P) UNRESTRICTED;                                  0319 00351000
*       END;                        /* DELETE EP(SDRNAME) DELETE   0320 00352000
*                                      EXTRA CDE FOR SDR     @G33VPHD*/ 00353000
*     END;                          /* END DELETE EXTRA CDE  @G33VPHD*/ 00354000
*                                                                  0322 00355000
*/*  LOAD, PGFIX AND DELETE EXTRA CDE FOR MODULE IEAVTSSD    @G33VPHD*/ 00356000
*                                                                  0322 00357000
*   DO;                             /* LOAD EP(SSDNAME)            0322 00358000
*                                      ERRET(NSSDLOAD)               */ 00359000
@RC00309 DS    0H                                                  0323 00360000
*     RESPECIFY                                                    0323 00361000
*      (GPR00P,                                                    0323 00362000
*       GPR01P,                                                    0323 00363000
*       GPR15F) RESTRICTED;                                        0323 00364000
*     GPR00P=ADDR(SSDNAME);         /* LOAD ENTRY POINT NAME ADDRESS */ 00365000
         LA    GPR00P,@CC00134                                     0324 00366000
*     GPR01P=0;                     /* JOBLIB OR LINKLIB DCB         */ 00367000
         SLR   GPR01P,GPR01P                                       0325 00368000
*     GPR01P=GPR01P|'80000000'X;    /* ERROR RETURN LABEL SPECIFIED  */ 00369000
         O     GPR01P,@CF03755                                     0326 00370000
*     SVC(8);                       /* ISSUE LOAD SVC                */ 00371000
         SVC   8                                                   0327 00372000
*     IF GPR15F^=0 THEN             /* LOAD WAS UNSUCCESSFUL         */ 00373000
         SLR   @12,@12                                             0328 00374000
         CR    GPR15F,@12                                          0328 00375000
         BNE   @RT00328                                            0328 00376000
*       GOTO NSSDLOAD;              /* BRANCH TO ERROR LOCATION      */ 00377000
*     RESPECIFY                                                    0330 00378000
*      (GPR00P,                                                    0330 00379000
