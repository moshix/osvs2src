*             CURRENT=ENDCOL;       /* NO SCAN REQUIRED              */ 01948000
         L     @15,ENDCOL(,R5)                                     0770 01949000
         ST    @15,CURRENT(,R5)                                    0770 01950000
*             VALIDSW='1'B;         /* AND AN EOB IS VALID           */ 01951000
         OI    VALIDSW(R5),B'00000010'                             0771 01952000
*           END;                                                   0772 01953000
*                                                                  0772 01954000
*         /***********************************************************/ 01955000
*         /*                                                         */ 01956000
*         /* IF PARENS SYNTAX IS INVALID OR RESPONSE IS AN EOB THEN  */ 01957000
*         /* CURRENT IS EQUAL TO ENDCOL TO BYPASS THE SCAN           */ 01958000
*         /*                                                         */ 01959000
*         /***********************************************************/ 01960000
*                                                                  0773 01961000
*         DO WHILE(CURRENT^=ENDCOL);                               0773 01962000
*                                                                  0773 01963000
@RF00768 B     @DE00773                                            0773 01964000
@DL00773 DS    0H                                                  0774 01965000
*           /*********************************************************/ 01966000
*           /*                                                       */ 01967000
*           /* THE OPERATOR REPLY TO MESSAGE IEA877A OR IEA882A IS   */ 01968000
*           /* SCANNED TO INSURE THAT THE SYNTAX IS CORRECT AND THAT */ 01969000
*           /* THE DEVICES SPECIFIED ARE VALID IE- DA DEVICES MUST BE*/ 01970000
*           /* INDICATED AS BEING FULL IN THE DUMP DATA SET TABLE AND*/ 01971000
*           /* THE TAPE DEVICES HAVE NOT ALREADY BEEN MADE AVAILABLE */ 01972000
*           /* TO SVC DUMP                                           */ 01973000
*           /*                                                       */ 01974000
*           /*********************************************************/ 01975000
*                                                                  0774 01976000
*           VALIDSW='0'B;           /* INDICATE THE REQUEST IS     0774 01977000
*                                      INVALID.IF THE SWITCH IS OFF     01978000
*                                      AT THE END OF THE LOOP THE  0774 01979000
*                                      REQUEST IS INVALID            */ 01980000
         NI    VALIDSW(R5),B'11111101'                             0774 01981000
*           IF CURCHAR='('          /* LEFT PAREN ?                  */ 01982000
*             THEN                  /* YES                           */ 01983000
         L     @15,CURRENT(,R5)                                    0775 01984000
         CLI   CURCHAR(@15),C'('                                   0775 01985000
         BNE   @RF00775                                            0775 01986000
*             DO;                                                  0776 01987000
*                                                                  0776 01988000
*               /*****************************************************/ 01989000
*               /*                                                   */ 01990000
*               /* THE ONLY CHARACTERS TO FOLLOW A LEFT PAREN ARE-   */ 01991000
*               /* D,T, OR ( ANY OTHER CHARACTERS ARE INVALID        */ 01992000
*               /*                                                   */ 01993000
*               /*****************************************************/ 01994000
*                                                                  0777 01995000
*               IF NXTCHAR='('      /* IS THE NEXT CHARACTER ANOTHER    01996000
*                                      PAREN                         */ 01997000
*                 THEN              /* YES -                         */ 01998000
         CLI   NXTCHAR(@15),C'('                                   0777 01999000
         BNE   @RF00777                                            0777 02000000
*                 CURRENT=CURRENT+1;/* ONLY TWO LEFT PARENS ARE    0778 02001000
*                                      ALLOWED BACK TO BACK AFTER THE   02002000
*                                      SECOND PAREN THERE MUST BE A     02003000
*                                      -D,T OR G                     */ 02004000
         AL    @15,@CF00055                                        0778 02005000
         ST    @15,CURRENT(,R5)                                    0778 02006000
*               IF NXTCHAR='T'      /* IS IT A-T-                    */ 02007000
*                 THEN              /* YES                           */ 02008000
@RF00777 L     @15,CURRENT(,R5)                                    0779 02009000
         CLI   NXTCHAR(@15),C'T'                                   0779 02010000
         BNE   @RF00779                                            0779 02011000
*                 DO;                                              0780 02012000
*                   CURRENT=CURRENT+1;                             0781 02013000
         AL    @15,@CF00055                                        0781 02014000
         ST    @15,CURRENT(,R5)                                    0781 02015000
*                   CALL TACHECK;   /* VALIDATE TA SYNTAX AND PLACE     02016000
*                                      DEVICES ON DEVICE Q           */ 02017000
         BAL   @14,TACHECK                                         0782 02018000
*                 END;                                             0783 02019000
*               ELSE                                               0784 02020000
*                 DO;                                              0784 02021000
         B     @RC00779                                            0784 02022000
@RF00779 DS    0H                                                  0785 02023000
*                   IF NXTCHAR='D'  /* A D IS OKAY                   */ 02024000
*                     THEN                                         0785 02025000
         L     @15,CURRENT(,R5)                                    0785 02026000
         CLI   NXTCHAR(@15),C'D'                                   0785 02027000
         BNE   @RF00785                                            0785 02028000
*                     CURRENT=CURRENT+1;                           0786 02029000
         AL    @15,@CF00055                                        0786 02030000
         ST    @15,CURRENT(,R5)                                    0786 02031000
*                   ELSE                                           0787 02032000
*                     DO;                                          0787 02033000
         B     @RC00785                                            0787 02034000
@RF00785 DS    0H                                                  0788 02035000
*                       CURRENT=ENDCOL;/* ANY OTHER CHARACTER IS   0788 02036000
*                                      INVALID                       */ 02037000
         L     @15,ENDCOL(,R5)                                     0788 02038000
         ST    @15,CURRENT(,R5)                                    0788 02039000
*                     END;                                         0789 02040000
*                 END;                                             0790 02041000
@RC00785 DS    0H                                                  0791 02042000
*             END;                                                 0791 02043000
@RC00779 DS    0H                                                  0792 02044000
*           IF CURCHAR='G'          /* POSSIBLE GO REPLY ?           */ 02045000
*             THEN                  /* YES                           */ 02046000
@RF00775 L     @15,CURRENT(,R5)                                    0792 02047000
         CLI   CURCHAR(@15),C'G'                                   0792 02048000
         BNE   @RF00792                                            0792 02049000
*             DO;                                                  0793 02050000
*                                                                  0793 02051000
*               /*****************************************************/ 02052000
*               /*                                                   */ 02053000
*               /* IF GO IS SPECIFIED IT CAN BE THE ONLY PARAMETER   */ 02054000
*               /* SPECIFIED                                         */ 02055000
*               /*                                                   */ 02056000
*               /*****************************************************/ 02057000
*                                                                  0794 02058000
*               IF NXTCHAR^='O'|    /* SPELLED CORRECTLY AND         */ 02059000
*                   CURRENT^=BEGIN  /* BY ITSELF                     */ 02060000
*                 THEN                                             0794 02061000
         CLI   NXTCHAR(@15),C'O'                                   0794 02062000
         BNE   @RT00794                                            0794 02063000
         C     @15,BEGIN(,R5)                                      0794 02064000
         BE    @RF00794                                            0794 02065000
@RT00794 DS    0H                                                  0795 02066000
*                 CURRENT=ENDCOL;   /* GET OUT WITH VALIDSW OFF      */ 02067000
         L     @15,ENDCOL(,R5)                                     0795 02068000
         ST    @15,CURRENT(,R5)                                    0795 02069000
*               ELSE                                               0796 02070000
*                 DO;                                              0796 02071000
         B     @RC00794                                            0796 02072000
@RF00794 DS    0H                                                  0797 02073000
*                   CURRENT=CURRENT+2;/* POINT CURRENT TO AFTER GO   */ 02074000
         LA    @15,2                                               0797 02075000
         AL    @15,CURRENT(,R5)                                    0797 02076000
         ST    @15,CURRENT(,R5)                                    0797 02077000
*                   IF CURRENT=ENDCOL/* WAS GO BY ITSELF             */ 02078000
*                     THEN          /* YES                           */ 02079000
         C     @15,ENDCOL(,R5)                                     0798 02080000
         BNE   @RF00798                                            0798 02081000
*                     VALIDSW='1'B;                                0799 02082000
         OI    VALIDSW(R5),B'00000010'                             0799 02083000
*                   ELSE            /* NOT BY ITSELF IS INVALID      */ 02084000
*                     CURRENT=ENDCOL;/* GET OUT WITH VALID OFF       */ 02085000
         B     @RC00798                                            0800 02086000
@RF00798 L     @15,ENDCOL(,R5)                                     0800 02087000
         ST    @15,CURRENT(,R5)                                    0800 02088000
*                 END;                                             0801 02089000
@RC00798 DS    0H                                                  0802 02090000
*               CURCHAR='''';       /* IF GO IS VALID OR INVALID NO     02091000
*                                      OTHER CHECKS SHOULD BE MADE SO   02092000
*                                      SET CURCHAR TO A QUOTE        */ 02093000
@RC00794 L     @15,CURRENT(,R5)                                    0802 02094000
         MVI   CURCHAR(@15),C''''                                  0802 02095000
*             END;                                                 0803 02096000
*           IF CURCHAR='D'          /* POSSIBLE DA TO EMPTY ?        */ 02097000
*             THEN                  /* YES                           */ 02098000
@RF00792 L     @15,CURRENT(,R5)                                    0804 02099000
         CLI   CURCHAR(@15),C'D'                                   0804 02100000
         BNE   @RF00804                                            0804 02101000
*             DO;                                                  0805 02102000
*                                                                  0805 02103000
*               /*****************************************************/ 02104000
*               /*                                                   */ 02105000
*               /* ONCE A -D- HAS BEEN FOUND IT MUST BE FOLLOWED BY  */ 02106000
*               /* A=( . AFTER THE SYNTAX HAS BEEN CHECKED THE RIGHT */ 02107000
*               /* PAREN IS FOUND AND ALL THE DA DEVICES SPECIFIED   */ 02108000
*               /* ARE CHECKED TO INSURE THAT THEY EXIST IN THE DUMP */ 02109000
*               /* DATA SET TABLE AND THAT THEY ARE MARKED AS BEING  */ 02110000
*               /* FULL.AFTER THE DEVICE IS VERIFIED IT IS PLACED ON */ 02111000
*               /* THE DEVICE Q TO BE PROCESSED LATER.IF THE SAME    */ 02112000
*               /* DEVICE IS SPECIFIED TWICE IT IS INVALID           */ 02113000
*               /*                                                   */ 02114000
*               /*****************************************************/ 02115000
*                                                                  0806 02116000
*               SCANBEG=CURRENT;    /* SAVE THE ADDRESS OF D         */ 02117000
         ST    @15,SCANBEG(,R5)                                    0806 02118000
*               CURRENT=CURRENT+3;  /* POINT TO WHERE THE PAREN    0807 02119000
*                                      SHOULD BE                     */ 02120000
         LA    @11,3                                               0807 02121000
         ALR   @11,@15                                             0807 02122000
         ST    @11,CURRENT(,R5)                                    0807 02123000
*               IF CURRENT>ENDCOL|  /* ENOUGH SPACE AVAILABLE        */ 02124000
*                   FIRST4^='DA=('  /* AND PROPER SYNTAX             */ 02125000
*                 THEN              /* NO                            */ 02126000
         C     @11,ENDCOL(,R5)                                     0808 02127000
         BH    @RT00808                                            0808 02128000
         CLC   FIRST4(4,@15),@CC03806                              0808 02129000
         BE    @RF00808                                            0808 02130000
@RT00808 DS    0H                                                  0809 02131000
*                 CURRENT=ENDCOL;   /* INVALID                       */ 02132000
         L     @15,ENDCOL(,R5)                                     0809 02133000
         ST    @15,CURRENT(,R5)                                    0809 02134000
*               DO WHILE(CURRENT^=ENDCOL&CURCHAR^=')');/* FIND RT  0810 02135000
*                                      PAREN IF SYNTAX OK            */ 02136000
@RF00808 B     @DE00810                                            0810 02137000
@DL00810 DS    0H                                                  0811 02138000
*                 CURRENT=CURRENT+1;/* GET NEXT CHARACTER            */ 02139000
         LA    @15,1                                               0811 02140000
         AL    @15,CURRENT(,R5)                                    0811 02141000
         ST    @15,CURRENT(,R5)                                    0811 02142000
*               END;                                               0812 02143000
@DE00810 L     @15,CURRENT(,R5)                                    0812 02144000
         C     @15,ENDCOL(,R5)                                     0812 02145000
         BE    @DC00810                                            0812 02146000
         CLI   CURCHAR(@15),C')'                                   0812 02147000
         BNE   @DL00810                                            0812 02148000
@DC00810 DS    0H                                                  0813 02149000
*               IF CURCHAR=')'      /* IF CLOSING PAREN FOUND        */ 02150000
*                 THEN              /* CHECK NEXT CHARACTER          */ 02151000
         L     @15,CURRENT(,R5)                                    0813 02152000
         CLI   CURCHAR(@15),C')'                                   0813 02153000
         BNE   @RF00813                                            0813 02154000
*                 DO;                                              0814 02155000
*                   CURRENT=CURRENT+1;                             0815 02156000
         AL    @15,@CF00055                                        0815 02157000
         ST    @15,CURRENT(,R5)                                    0815 02158000
*                   IF CURCHAR^=')'&/* IT MUST BE A -)-              */ 02159000
*                       CURCHAR^=','&/* OR A-,-                      */ 02160000
*                       CURRENT^=ENDCOL/* OR END OF DATA             */ 02161000
*                     THEN          /* INVALID                       */ 02162000
         CLI   CURCHAR(@15),C')'                                   0816 02163000
         BE    @RF00816                                            0816 02164000
         CLI   CURCHAR(@15),C','                                   0816 02165000
         BE    @RF00816                                            0816 02166000
         L     @11,ENDCOL(,R5)                                     0816 02167000
         CR    @15,@11                                             0816 02168000
         BE    @RF00816                                            0816 02169000
*                     DO;                                          0817 02170000
*                       CURRENT=ENDCOL;/* OUT WITH VALIDSW OFF       */ 02171000
         ST    @11,CURRENT(,R5)                                    0818 02172000
*                     END;                                         0819 02173000
*                   ELSE                                           0820 02174000
*                     DO;                                          0820 02175000
         B     @RC00816                                            0820 02176000
@RF00816 DS    0H                                                  0821 02177000
*                       VALIDSW='1'B;/* VALID SO FAR                 */ 02178000
         OI    VALIDSW(R5),B'00000010'                             0821 02179000
*                       SCANBEG=SCANBEG+4;/* POINT TO FIRST DA ENTRY */ 02180000
         LA    @15,4                                               0822 02181000
         AL    @15,SCANBEG(,R5)                                    0822 02182000
         ST    @15,SCANBEG(,R5)                                    0822 02183000
*                     END;                                         0823 02184000
*                 END;                                             0824 02185000
@RC00816 DS    0H                                                  0825 02186000
*               IF VALIDSW='1'B     /* IS THE SYNTAX OK              */ 02187000
*                 THEN                                             0825 02188000
@RF00813 TM    VALIDSW(R5),B'00000010'                             0825 02189000
         BNO   @RF00825                                            0825 02190000
*                 DO;               /* YES                           */ 02191000
*                   CALL DALIST;    /* PUT DEVICES ON Q TO BE      0827 02192000
*                                      PROCESSED                     */ 02193000
         BAL   @14,DALIST                                          0827 02194000
*                 END;                                             0828 02195000
*             END;                                                 0829 02196000
@RF00825 DS    0H                                                  0830 02197000
*           IF CURCHAR=','          /* COMMA ?                       */ 02198000
*             THEN                  /* YES                           */ 02199000
@RF00804 L     @15,CURRENT(,R5)                                    0830 02200000
         CLI   CURCHAR(@15),C','                                   0830 02201000
         BNE   @RF00830                                            0830 02202000
*             DO;                                                  0831 02203000
*               IF NXTCHAR='('|     /* IS IT FOLLOWED BY A ( OR      */ 02204000
*                   NXTCHAR='D'     /* A D                           */ 02205000
*                 THEN              /* IT IS OKAY                    */ 02206000
         CLI   NXTCHAR(@15),C'('                                   0832 02207000
         BE    @RT00832                                            0832 02208000
         CLI   NXTCHAR(@15),C'D'                                   0832 02209000
         BNE   @RF00832                                            0832 02210000
@RT00832 DS    0H                                                  0833 02211000
*                 DO;                                              0833 02212000
*                   VALIDSW='1'B;                                  0834 02213000
         OI    VALIDSW(R5),B'00000010'                             0834 02214000
*                   CURRENT=CURRENT+1;/* POINT TO NEXT CHAR          */ 02215000
         LA    @15,1                                               0835 02216000
         AL    @15,CURRENT(,R5)                                    0835 02217000
         ST    @15,CURRENT(,R5)                                    0835 02218000
*                 END;                                             0836 02219000
*               ELSE                /* IT IS AN INVALID SEQUENCE     */ 02220000
*                 DO;                                              0837 02221000
         B     @RC00832                                            0837 02222000
@RF00832 DS    0H                                                  0838 02223000
*                   VALIDSW='0'B;                                  0838 02224000
         NI    VALIDSW(R5),B'11111101'                             0838 02225000
*                   CURRENT=ENDCOL; /* GET OUT OF LOOP               */ 02226000
         L     @15,ENDCOL(,R5)                                     0839 02227000
         ST    @15,CURRENT(,R5)                                    0839 02228000
*                 END;                                             0840 02229000
*             END;                                                 0841 02230000
@RC00832 DS    0H                                                  0842 02231000
*           IF CURCHAR=')'          /* IS IT A RIGHT PAREN ?         */ 02232000
*             THEN                  /* YES                           */ 02233000
@RF00830 L     @15,CURRENT(,R5)                                    0842 02234000
         CLI   CURCHAR(@15),C')'                                   0842 02235000
         BNE   @RF00842                                            0842 02236000
*             DO;                                                  0843 02237000
*               IF NXTCHAR=''''     /* IS IT THE LAST CHARACTER TO BE   02238000
*                                      SCANNED.THE END OF DATA IS  0844 02239000
*                                      INDICATED BY A SINGLE QUOTE   */ 02240000
*                 THEN              /* IT IS A VALID SEQUENCE        */ 02241000
         CLI   NXTCHAR(@15),C''''                                  0844 02242000
         BNE   @RF00844                                            0844 02243000
*                 DO;                                              0845 02244000
*                   VALIDSW='1'B;                                  0846 02245000
         OI    VALIDSW(R5),B'00000010'                             0846 02246000
*                   CURRENT=CURRENT+1;                             0847 02247000
         AL    @15,@CF00055                                        0847 02248000
         ST    @15,CURRENT(,R5)                                    0847 02249000
*                 END;                                             0848 02250000
*               ELSE                /* IT IS INVALID                 */ 02251000
*                 DO;                                              0849 02252000
         B     @RC00844                                            0849 02253000
@RF00844 DS    0H                                                  0850 02254000
*                   VALIDSW='0'B;                                  0850 02255000
         NI    VALIDSW(R5),B'11111101'                             0850 02256000
*                   CURRENT=ENDCOL;                                0851 02257000
         L     @15,ENDCOL(,R5)                                     0851 02258000
         ST    @15,CURRENT(,R5)                                    0851 02259000
*                 END;                                             0852 02260000
*             END;                                                 0853 02261000
@RC00844 DS    0H                                                  0854 02262000
*           IF VALIDSW='0'B         /* IS THE REQUEST STILL INVALID ?*/ 02263000
*             THEN                  /* YES                           */ 02264000
@RF00842 TM    VALIDSW(R5),B'00000010'                             0854 02265000
         BNZ   @RF00854                                            0854 02266000
*             CURRENT=ENDCOL;       /* GET OUT OF LOOP               */ 02267000
         L     @15,ENDCOL(,R5)                                     0855 02268000
         ST    @15,CURRENT(,R5)                                    0855 02269000
*         END;                                                     0856 02270000
@RF00854 DS    0H                                                  0856 02271000
@DE00773 CLC   CURRENT(4,R5),ENDCOL(R5)                            0856 02272000
         BNE   @DL00773                                            0856 02273000
*         RPARM=BEGIN;              /* START OF AREA TO FREE         */ 02274000
         L     RPARM,BEGIN(,R5)                                    0857 02275000
*         GEN SETS(RPARM,R14,R15);                                 0858 02276000
     IEAPMNIP TYPE=WTOR2,PARAM=RPARM,BUFREL=YES  FREE REPLY AREA        02277000
*         IF VALIDSW='0'B           /* WAS REPLY VALID ?             */ 02278000
*           THEN                    /* NO- ISSUE MESSAGE IEA882A     */ 02279000
         TM    VALIDSW(R5),B'00000010'                             0859 02280000
         BNZ   @RF00859                                            0859 02281000
*           DO;                                                    0860 02282000
*             RFY                                                  0861 02283000
*               R4 RSTD;                                           0861 02284000
*             GEN REFS(MSG10,MSG11) SETS(RPARM,R14,R15);           0862 02285000
     IEAPMNIP TYPE=WTOR,PARAM=MSG11   PROMPT OPERATOR                   02286000
*             DACTION=0;            /* RESET ANY FLAGS THAT MAY HAVE    02287000
*                                      BEEN SET                      */ 02288000
         SLR   @15,@15                                             0863 02289000
         STH   @15,DACTION(,R5)                                    0863 02290000
*             QSAVE=ADDR(RQUEADD);                                 0864 02291000
         LA    @15,RQUEADD(,R5)                                    0864 02292000
         ST    @15,QSAVE(,R5)                                      0864 02293000
*             R4=RQUEADD;                                          0865 02294000
*                                                                  0865 02295000
         L     R4,RQUEADD(,R5)                                     0865 02296000
*             /*******************************************************/ 02297000
*             /*                                                     */ 02298000
*             /* THE DEVICE QUEUE IS NOW SCANNED TO DETERMINE IF ANY */ 02299000
*             /* DEVICES HAVE BEEN PLACED ON THE Q WHILE SCANNING THE*/ 02300000
*             /* REQUEST. BECAUSE THIS REQUEST IS INVALID ALL THOSE  */ 02301000
*             /* ENTRIES ADDED ARE LOGICALLY REMOVED                 */ 02302000
*             /*                                                     */ 02303000
*             /*******************************************************/ 02304000
*                                                                  0866 02305000
*             DO WHILE(R4^=0&DPROCSD='1'B);                        0866 02306000
         B     @DE00866                                            0866 02307000
@DL00866 DS    0H                                                  0867 02308000
*               QSAVE=R4;           /* THIS IS NOT A NEW ENTRY SO  0867 02309000
*                                      SAVE ITS ADDRESS              */ 02310000
         ST    R4,QSAVE(,R5)                                       0867 02311000
*               R4=NXTENTAD;        /* ADDRESS OF NEXT BLOCK         */ 02312000
         L     R4,NXTENTAD(,R4)                                    0868 02313000
*             END;                                                 0869 02314000
@DE00866 LTR   R4,R4                                               0869 02315000
         BZ    @DC00866                                            0869 02316000
         TM    DPROCSD(R4),B'01000000'                             0869 02317000
         BO    @DL00866                                            0869 02318000
@DC00866 DS    0H                                                  0870 02319000
*             IF R4^=0              /* WERE ANY DEVICES PLACED ON THE   02320000
*                                      Q                             */ 02321000
*               THEN                /* YES                           */ 02322000
         SLR   @15,@15                                             0870 02323000
         CR    R4,@15                                              0870 02324000
         BE    @RF00870                                            0870 02325000
*               DO;                                                0871 02326000
*                 QSAVE->NXTENTAD=0;/* TRUNCATE AT PREVIOUS ENTRY    */ 02327000
         L     @11,QSAVE(,R5)                                      0872 02328000
         ST    @15,NXTENTAD(,@11)                                  0872 02329000
*               END;                                               0873 02330000
*             RFY                                                  0874 02331000
*               R4 UNRSTD;                                         0874 02332000
@RF00870 DS    0H                                                  0875 02333000
*           END;                                                   0875 02334000
*       END;                        /* OF DO WHILE VALID IS ZERO     */ 02335000
@RF00859 DS    0H                                                  0876 02336000
@DE00743 TM    VALIDSW(R5),B'00000010'                             0876 02337000
         BZ    @DL00743                                            0876 02338000
*       IF DUMPD='1'B               /* ANY DA TO EMPTY               */ 02339000
*         THEN                      /* YES                           */ 02340000
         TM    DUMPD(R5),B'00100000'                               0877 02341000
         BNO   @RF00877                                            0877 02342000
*         DO;                                                      0878 02343000
*           RFY                                                    0879 02344000
*             R4 RSTD;                                             0879 02345000
*                                                                  0879 02346000
*           /*********************************************************/ 02347000
*           /*                                                       */ 02348000
*           /* THIS SECTION OF CODE WRITES AN EOF ON ALL THE DA      */ 02349000
*           /* DEVICES THAT ARE FULL AND THE OPERATOR HAS REQUESTED  */ 02350000
*           /* TO BE MARKED AS EMPTY VIA THE LIST REQUEST            */ 02351000
*           /*                                                       */ 02352000
*           /*********************************************************/ 02353000
*                                                                  0880 02354000
*           DUMPF='0'B;                                            0880 02355000
         NI    DUMPF(R5),B'11111110'                               0880 02356000
*           DO WHILE(DUMPF='0'B);   /* PROCESS ALL THE DA DEVICES  0881 02357000
*                                      REQUESTED                     */ 02358000
         B     @DE00881                                            0881 02359000
@DL00881 DS    0H                                                  0882 02360000
*             R4=RQUEADD;           /* GET DVICE Q ADDRESS           */ 02361000
         L     R4,RQUEADD(,R5)                                     0882 02362000
*             DO WHILE(R4^=0&(DPROCSD='1'B|DASDDV='0'B));/* FIND THE    02363000
*                                      FIRST ELIGIBLE DA DEVICE      */ 02364000
         B     @DE00883                                            0883 02365000
@DL00883 DS    0H                                                  0884 02366000
*               R4=NXTENTAD;                                       0884 02367000
         L     R4,NXTENTAD(,R4)                                    0884 02368000
*             END;                                                 0885 02369000
@DE00883 LTR   R4,R4                                               0885 02370000
         BZ    @DC00883                                            0885 02371000
         TM    DPROCSD(R4),B'01000000'                             0885 02372000
         BNZ   @DL00883                                            0885 02373000
