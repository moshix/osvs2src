*   CURRENT=BEGIN;                  /* SET POINTER TO FIRST ENTRY    */ 00746000
         ST    @15,CURRENT(,R5)                                    0432 00747000
*   IF BEGIN=0                      /* ARE THERE ANY OPTIONS         */ 00748000
*     THEN                          /* NO-USE DASD AND DO NOT SCAN   */ 00749000
         LTR   @15,@15                                             0433 00750000
         BNZ   @RF00433                                            0433 00751000
*     DO;                                                          0434 00752000
*       ENDCOL=BEGIN;               /* SET ENDCOL TO BEGINNING OF THE   00753000
*                                      FIELD                         */ 00754000
         ST    @15,ENDCOL(,R5)                                     0435 00755000
*       VALIDSW='1'B;               /* DEFAULT IS VALID              */ 00756000
*       DUMPD='1'B;                 /* INDICATE DASD DEVICE ON Q     */ 00757000
         OI    VALIDSW(R5),B'00100010'                             0437 00758000
*       CALL DASDINIT;              /* PUT DASD DEVICES ON THE QUEUE    00759000
*                                      TO BE VERIFIED                */ 00760000
         BAL   @14,DASDINIT                                        0438 00761000
*     END;                                                         0439 00762000
*   ELSE                                                           0440 00763000
*     DO;                           /* OPTIONS HAVE BEEN SPECIFIED 0440 00764000
*                                      SET ENDCOL TO END OF DATA     */ 00765000
         B     @RC00433                                            0440 00766000
@RF00433 DS    0H                                                  0441 00767000
*       DO WHILE(CURCHAR^='''');    /* END OF DATA IS INDICATED BY A    00768000
*                                      SINGLE QUOTE                  */ 00769000
         B     @DE00441                                            0441 00770000
@DL00441 DS    0H                                                  0442 00771000
*         CURRENT=CURRENT+1;        /* UPDATE CURRENT BY ONE         */ 00772000
         LA    @15,1                                               0442 00773000
         AL    @15,CURRENT(,R5)                                    0442 00774000
         ST    @15,CURRENT(,R5)                                    0442 00775000
*       END;                        /* END OF DO WHILE               */ 00776000
@DE00441 L     @15,CURRENT(,R5)                                    0443 00777000
         CLI   CURCHAR(@15),C''''                                  0443 00778000
         BNE   @DL00441                                            0443 00779000
*       ENDCOL=CURRENT;             /* SET ENDING ADDRESS            */ 00780000
         ST    @15,ENDCOL(,R5)                                     0444 00781000
*       CURRENT=BEGIN;              /* RESET CURRENT TO BEGINNING    */ 00782000
         L     @15,BEGIN(,R5)                                      0445 00783000
         ST    @15,CURRENT(,R5)                                    0445 00784000
*     END;                                                         0446 00785000
*   DO WHILE(VALIDSW='0'B);         /* DO UNTIL THE OPTIONS ARE VALID*/ 00786000
*                                                                  0447 00787000
@RC00433 B     @DE00447                                            0447 00788000
@DL00447 DS    0H                                                  0448 00789000
*     /***************************************************************/ 00790000
*     /*                                                             */ 00791000
*     /* THIS SECTION OF CODE VALIDITY SCANS THE DUMP PARAMETERS THAT*/ 00792000
*     /* HAVE BEEN TYPED BY THE OPERATOR                             */ 00793000
*     /*                                                             */ 00794000
*     /***************************************************************/ 00795000
*                                                                  0448 00796000
*     DO WHILE(CURRENT^=ENDCOL);                                   0448 00797000
         B     @DE00448                                            0448 00798000
@DL00448 DS    0H                                                  0449 00799000
*       VALIDSW='0'B;               /* TURN OFF VALID SWITCH         */ 00800000
         NI    VALIDSW(R5),B'11111101'                             0449 00801000
*       IF CURCHAR='(' THEN         /* IS THE CURRENT CHARACTER A  0450 00802000
*                                      LEFT PAREN ?                  */ 00803000
         L     @15,CURRENT(,R5)                                    0450 00804000
         CLI   CURCHAR(@15),C'('                                   0450 00805000
         BNE   @RF00450                                            0450 00806000
*         DO;                       /* YES                           */ 00807000
*                                                                  0451 00808000
*           /*********************************************************/ 00809000
*           /*                                                       */ 00810000
*           /* THIS SECTION OF CODE FINDS THE NEXT CHARACTER AFTER   */ 00811000
*           /* THE PAREN IF THERE ARE BACK TO BACK PARENS THEY ARE   */ 00812000
*           /* SCANNED OVER TO FIND THE NEXT CHAR THAT IS NOT A      */ 00813000
*           /* PAREN. IF THE CHARACTER AFTER THE PAREN IS A T A CALL */ 00814000
*           /* IS ISSUED TO TACHECK TO PLACE THE TA DEVICES ON THE   */ 00815000
*           /* DEVICE QUEUE. ANY OTHER CHARACTERS THAT FOLLOW THE    */ 00816000
*           /* PAREN MUST BE A-D-L-,-) AND THE RULES THAT APPLY TO   */ 00817000
*           /* THOSE CHARACTERS ARE APPLICABLE. AFTER THE END OF THIS*/ 00818000
*           /* SECTION CURRENT POINTS TO THE NEXT CHARACTER TO BE    */ 00819000
*           /* SCANNED AND THE VALIDSW REMAINS OFF IN THE EVENT THE  */ 00820000
*           /* NEXT CHARACTER IS NOT SCANNABLE.                      */ 00821000
*           /*                                                       */ 00822000
*           /*********************************************************/ 00823000
*                                                                  0452 00824000
*           DO WHILE(CURCHAR='(');  /* FIND FIRST CHAR THAT IS NOT A    00825000
*                                      (                             */ 00826000
         B     @DE00452                                            0452 00827000
@DL00452 DS    0H                                                  0453 00828000
*             CURRENT=CURRENT+1;                                   0453 00829000
         LA    @15,1                                               0453 00830000
         AL    @15,CURRENT(,R5)                                    0453 00831000
         ST    @15,CURRENT(,R5)                                    0453 00832000
*           END;                                                   0454 00833000
@DE00452 L     @15,CURRENT(,R5)                                    0454 00834000
         CLI   CURCHAR(@15),C'('                                   0454 00835000
         BE    @DL00452                                            0454 00836000
*           IF CURCHAR=')' THEN     /* RIGHT PAREN NEXT ?            */ 00837000
         CLI   CURCHAR(@15),C')'                                   0455 00838000
         BNE   @RF00455                                            0455 00839000
*             DO;                   /* YES                           */ 00840000
*               IF DUMPL='0'B THEN  /* THIS SITUATION IS ONLY VALID     00841000
*                                      IF LIST HAS BEEN REQUESTED-IF    00842000
*                                      THE OPERATOR TYPED (,L) THE ,L   00843000
*                                      HAS BEEN REMOVED BY NIP AND 0457 00844000
*                                      INDICATED SUCH WITH THE     0457 00845000
*                                      NIPPTLST BIT BEING ON FOR THE    00846000
*                                      DUMP PARAMETERS               */ 00847000
         TM    DUMPL(R5),B'01000000'                               0457 00848000
         BNZ   @RF00457                                            0457 00849000
*                 CURRENT=ENDCOL;   /* L HAS NOT BEEN SPECIFIED SO 0458 00850000
*                                      THIS IS AN ERROR              */ 00851000
         L     @15,ENDCOL(,R5)                                     0458 00852000
         ST    @15,CURRENT(,R5)                                    0458 00853000
*             END;                                                 0459 00854000
*           ELSE                                                   0460 00855000
*             DO;                                                  0460 00856000
         B     @RC00455                                            0460 00857000
@RF00455 DS    0H                                                  0461 00858000
*               IF CURCHAR='T'      /* POSSIBLE TA REQUEST ?         */ 00859000
*                 THEN              /* YES                           */ 00860000
         L     @15,CURRENT(,R5)                                    0461 00861000
         CLI   CURCHAR(@15),C'T'                                   0461 00862000
         BNE   @RF00461                                            0461 00863000
*                 CALL TACHECK;     /* CALL TACHECK WITH CURRENT   0462 00864000
*                                      POINTING TO THE T AND VALIDSW    00865000
*                                      OFF                           */ 00866000
         BAL   @14,TACHECK                                         0462 00867000
*             END;                                                 0463 00868000
@RF00461 DS    0H                                                  0464 00869000
*         END;                                                     0464 00870000
@RC00455 DS    0H                                                  0465 00871000
*       IF CURCHAR=')' THEN         /* IS THIS A RIGHT PAREN ?       */ 00872000
@RF00450 L     @15,CURRENT(,R5)                                    0465 00873000
         CLI   CURCHAR(@15),C')'                                   0465 00874000
         BNE   @RF00465                                            0465 00875000
*         DO;                       /* YES                           */ 00876000
*           VALIDSW='0'B;           /* PRESUPPOSE ERROR              */ 00877000
*                                                                  0467 00878000
         NI    VALIDSW(R5),B'11111101'                             0467 00879000
*           /*********************************************************/ 00880000
*           /*                                                       */ 00881000
*           /* A RIGHT PAREN CAN ONLY BE FOLLOWED BY ANOTHER RIGHT   */ 00882000
*           /* PAREN OR BE THE LAST CHARACTER TYPED FOR DUMP = IF    */ 00883000
*           /* TAPE HAS BEEN SPECIFIED IE: (TA,XXX) THE RIGHT PAREN  */ 00884000
*           /* IS CHECKED THEN AND BYPASSED SO THIS ROUTINE DOES NOT */ 00885000
*           /* CHECK FOR THE ENSUING COMMA                           */ 00886000
*           /*                                                       */ 00887000
*           /*********************************************************/ 00888000
*                                                                  0468 00889000
*           CURRENT=CURRENT+1;      /* UPDATE TO NEXT CHARACTER      */ 00890000
         AL    @15,@CF00055                                        0468 00891000
         ST    @15,CURRENT(,R5)                                    0468 00892000
*           IF CURRENT^=ENDCOL&CURCHAR^=')'/* IS THERE ANOTHER     0469 00893000
*                                      CHARACTER AND IS IT A RIGHT 0469 00894000
*                                      PAREN ?                       */ 00895000
*             THEN                  /* NO ITS INVALID                */ 00896000
         L     @12,ENDCOL(,R5)                                     0469 00897000
         CR    @15,@12                                             0469 00898000
         BE    @RF00469                                            0469 00899000
         CLI   CURCHAR(@15),C')'                                   0469 00900000
         BE    @RF00469                                            0469 00901000
*             CURRENT=ENDCOL;       /* OUT OF DO WHILE WITH VALID  0470 00902000
*                                      SWITCH OFF                    */ 00903000
         ST    @12,CURRENT(,R5)                                    0470 00904000
*           ELSE                                                   0471 00905000
*             VALIDSW='1'B;         /* OTHERWISE INDICATE IT IS VALID*/ 00906000
         B     @RC00469                                            0471 00907000
@RF00469 OI    VALIDSW(R5),B'00000010'                             0471 00908000
*         END;                                                     0472 00909000
@RC00469 DS    0H                                                  0473 00910000
*       IF CURCHAR=',' THEN         /* COMMA ?                       */ 00911000
@RF00465 L     @15,CURRENT(,R5)                                    0473 00912000
         CLI   CURCHAR(@15),C','                                   0473 00913000
         BNE   @RF00473                                            0473 00914000
*         DO;                       /* YES                           */ 00915000
*           VALIDSW='0'B;           /* PRESUPPOSE ERROR              */ 00916000
         NI    VALIDSW(R5),B'11111101'                             0475 00917000
*           IF NXTCHAR^='D'&        /* NEXT CHAR CAN BE A-D-         */ 00918000
*               NXTCHAR^='('        /* OR A -(-                      */ 00919000
*             THEN                                                 0476 00920000
         CLI   NXTCHAR(@15),C'D'                                   0476 00921000
         BE    @RF00476                                            0476 00922000
         CLI   NXTCHAR(@15),C'('                                   0476 00923000
         BE    @RF00476                                            0476 00924000
*             DO;                   /* ANY OTHER CHAR IS INVALID     */ 00925000
*               CURRENT=ENDCOL;                                    0478 00926000
         L     @15,ENDCOL(,R5)                                     0478 00927000
         ST    @15,CURRENT(,R5)                                    0478 00928000
*             END;                                                 0479 00929000
*           ELSE                    /* COMMA IS FOLLOWED BY A VALID     00930000
*                                      CHARACTER                     */ 00931000
*             DO;                                                  0480 00932000
         B     @RC00476                                            0480 00933000
@RF00476 DS    0H                                                  0481 00934000
*               VALIDSW='1'B;       /* VALID SEQUENCE                */ 00935000
         OI    VALIDSW(R5),B'00000010'                             0481 00936000
*               CURRENT=CURRENT+1;  /* UPDATE POINTER                */ 00937000
         LA    @15,1                                               0482 00938000
         AL    @15,CURRENT(,R5)                                    0482 00939000
         ST    @15,CURRENT(,R5)                                    0482 00940000
*             END;                                                 0483 00941000
*         END;                                                     0484 00942000
@RC00476 DS    0H                                                  0485 00943000
*       IF CURCHAR='D' THEN         /* POSSIBLE DASD ?               */ 00944000
@RF00473 L     @15,CURRENT(,R5)                                    0485 00945000
         CLI   CURCHAR(@15),C'D'                                   0485 00946000
         BNE   @RF00485                                            0485 00947000
*         DO;                       /* YES                           */ 00948000
*           VALIDSW='0'B;           /* PRESUPPOSE ERROR              */ 00949000
*                                                                  0487 00950000
         NI    VALIDSW(R5),B'11111101'                             0487 00951000
*           /*********************************************************/ 00952000
*           /*                                                       */ 00953000
*           /* THE CURRENT CHARACTER IS A D IN ORDER TO BE VALID IT  */ 00954000
*           /* MUST BE FOLLOWED BY -ASD- TO INDICATE DASD. DASD MUST */ 00955000
*           /* BE FOLLOWED BY A COMMA OR A RIGHT PAREN OR BE THE LAST*/ 00956000
*           /* ENTRY IN THE TABLE IF ANY OF THESE RULES ARE VIOLATED */ 00957000
*           /* OR IF DASD HAS BEEN SPECIFIED TWICE THE CURRENT       */ 00958000
*           /* POINTER IS SET TO ENDCOL TO FORCE AN EXIT FROM THE DO */ 00959000
*           /* WHILE WITH THE VALIDSW OFF TO PROMPT THE OPERATOR     */ 00960000
*           /*                                                       */ 00961000
*           /*********************************************************/ 00962000
*                                                                  0488 00963000
*           SCANBEG=CURRENT;        /* ADDRESS OF -D-                */ 00964000
         ST    @15,SCANBEG(,R5)                                    0488 00965000
*           CURRENT=CURRENT+4;      /* ADDRESS OF NEXT ENTRY         */ 00966000
         LA    @12,4                                               0489 00967000
         ALR   @12,@15                                             0489 00968000
         ST    @12,CURRENT(,R5)                                    0489 00969000
*           IF CURRENT>ENDCOL|      /* SHORT SPELLING ?              */ 00970000
*               FIRST4^='DASD'      /* OR MISPELLED                  */ 00971000
*             THEN                  /* YES- INVALID                  */ 00972000
         C     @12,ENDCOL(,R5)                                     0490 00973000
         BH    @RT00490                                            0490 00974000
         CLC   FIRST4(4,@15),@CC03786                              0490 00975000
         BE    @RF00490                                            0490 00976000
@RT00490 DS    0H                                                  0491 00977000
*             CURRENT=ENDCOL;                                      0491 00978000
         L     @15,ENDCOL(,R5)                                     0491 00979000
         ST    @15,CURRENT(,R5)                                    0491 00980000
*           ELSE                                                   0492 00981000
*             DO;                                                  0492 00982000
         B     @RC00490                                            0492 00983000
@RF00490 DS    0H                                                  0493 00984000
*               IF DASDFND='1'B|    /* SECOND DASD ENTRY OR          */ 00985000
*                   (CURRENT^=ENDCOL&/* THIS IS NOT THE LAST CHAR    */ 00986000
*                   CURCHAR^=','&   /* AND IT IS NOT FOLLOWED BY A 0493 00987000
*                                      COMMA OR                      */ 00988000
*                   CURCHAR^=')')   /* A RIGHT PAREN                 */ 00989000
*                 THEN              /* IT IS INVALID                 */ 00990000
         TM    DASDFND(R5),B'00000100'                             0493 00991000
         BO    @RT00493                                            0493 00992000
         L     @15,CURRENT(,R5)                                    0493 00993000
         C     @15,ENDCOL(,R5)                                     0493 00994000
         BE    @RF00493                                            0493 00995000
         CLI   CURCHAR(@15),C','                                   0493 00996000
         BE    @RF00493                                            0493 00997000
         CLI   CURCHAR(@15),C')'                                   0493 00998000
         BE    @RF00493                                            0493 00999000
@RT00493 DS    0H                                                  0494 01000000
*                 CURRENT=ENDCOL;                                  0494 01001000
         L     @15,ENDCOL(,R5)                                     0494 01002000
         ST    @15,CURRENT(,R5)                                    0494 01003000
*               ELSE                                               0495 01004000
*                 DO;                                              0495 01005000
         B     @RC00493                                            0495 01006000
@RF00493 DS    0H                                                  0496 01007000
*                   DASDFND='1'B;   /* INDICATE DASD FOUND           */ 01008000
*                   DUMPD='1'B;                                    0497 01009000
*                   VALIDSW='1'B;   /* INDICATE VALID                */ 01010000
         OI    DASDFND(R5),B'00100110'                             0498 01011000
*                   IF ENTDASD='0'B /* WAS DASD INIT ENTERED       0499 01012000
*                                      PREVIOUSLY BECAUSE AN L WAS 0499 01013000
*                                      FOUND                         */ 01014000
*                     THEN          /* NO                            */ 01015000
         TM    ENTDASD(R5),B'00001000'                             0499 01016000
         BNZ   @RF00499                                            0499 01017000
*                     DO;                                          0500 01018000
*                       ENTDASD='1'B;/* DASD INIT BEING ENTERED ONLY    01019000
*                                      ONE TIME                      */ 01020000
         OI    ENTDASD(R5),B'00001000'                             0501 01021000
*                       CALL DASDINIT;                             0502 01022000
         BAL   @14,DASDINIT                                        0502 01023000
*                     END;                                         0503 01024000
*                 END;                                             0504 01025000
@RF00499 DS    0H                                                  0505 01026000
*             END;                                                 0505 01027000
@RC00493 DS    0H                                                  0506 01028000
*         END;                                                     0506 01029000
@RC00490 DS    0H                                                  0507 01030000
*       IF CURCHAR='N' THEN         /* POSSIBLE NO                   */ 01031000
@RF00485 L     @15,CURRENT(,R5)                                    0507 01032000
         CLI   CURCHAR(@15),C'N'                                   0507 01033000
         BNE   @RF00507                                            0507 01034000
*         DO;                       /* YES                           */ 01035000
*           VALIDSW='0'B;           /* PRESUPPOSE ERROR              */ 01036000
*                                                                  0509 01037000
         NI    VALIDSW(R5),B'11111101'                             0509 01038000
*           /*********************************************************/ 01039000
*           /*                                                       */ 01040000
*           /* ONCE AN-N- HAS BEEN ENCOUNTERED THE ONLY VALID CHAR TO*/ 01041000
*           /* FOLLOW IS AN O IF NO IS SPECIFIED NO OTHER PARAMETERS */ 01042000
*           /* ARE ALLOWED                                           */ 01043000
*           /*                                                       */ 01044000
*           /*********************************************************/ 01045000
*                                                                  0510 01046000
*           SCANBEG=CURRENT;                                       0510 01047000
         ST    @15,SCANBEG(,R5)                                    0510 01048000
*           CURRENT=CURRENT+2;                                     0511 01049000
         LA    @12,2                                               0511 01050000
         ALR   @12,@15                                             0511 01051000
         ST    @12,CURRENT(,R5)                                    0511 01052000
*           IF(SCANBEG=BEGIN&       /* NO HAS TO BE FIRST            */ 01053000
*               CURRENT=ENDCOL&     /* AND ONLY                      */ 01054000
*               FIRST2='NO')        /* AND CORRECT           @ZA04824*/ 01055000
*             THEN                  /* OKAY                          */ 01056000
         C     @15,BEGIN(,R5)                                      0512 01057000
         BNE   @RF00512                                            0512 01058000
         C     @12,ENDCOL(,R5)                                     0512 01059000
         BNE   @RF00512                                            0512 01060000
         CLC   FIRST2(2,@15),@CC03788                              0512 01061000
         BNE   @RF00512                                            0512 01062000
*             DO;                                                  0513 01063000
*               DUMPN='1'B;         /* INDICATE DUMP-NO-             */ 01064000
*               DUMPL='0'B;         /* LIST INCOMPATIBLE WITH DUMP=NO   01065000
*                                                            @ZA04824*/ 01066000
*               VALIDSW='1'B;       /* AND VALID                     */ 01067000
         OI    DUMPN(R5),B'10000010'                               0516 01068000
         NI    DUMPL(R5),B'10111111'                               0516 01069000
*             END;                                                 0517 01070000
*           ELSE                    /* INVALID                       */ 01071000
*             CURRENT=ENDCOL;                                      0518 01072000
         B     @RC00512                                            0518 01073000
@RF00512 L     @15,ENDCOL(,R5)                                     0518 01074000
         ST    @15,CURRENT(,R5)                                    0518 01075000
*         END;                                                     0519 01076000
@RC00512 DS    0H                                                  0520 01077000
*       IF CURCHAR='L'              /* LIST SPECIFIED                */ 01078000
*         THEN                                                     0520 01079000
@RF00507 L     @15,CURRENT(,R5)                                    0520 01080000
         CLI   CURCHAR(@15),C'L'                                   0520 01081000
         BNE   @RF00520                                            0520 01082000
*         DO;                       /* YES                           */ 01083000
*           VALIDSW='0'B;           /* PRESUPPOSE ERROR              */ 01084000
         NI    VALIDSW(R5),B'11111101'                             0522 01085000
*           IF DUMPL='1'B           /* SECOND TIME?                  */ 01086000
*             THEN                  /* YES                           */ 01087000
         TM    DUMPL(R5),B'01000000'                               0523 01088000
         BNO   @RF00523                                            0523 01089000
*             CURRENT=ENDCOL;                                      0524 01090000
         L     @15,ENDCOL(,R5)                                     0524 01091000
         ST    @15,CURRENT(,R5)                                    0524 01092000
*           ELSE                                                   0525 01093000
*             DO;                                                  0525 01094000
         B     @RC00523                                            0525 01095000
@RF00523 DS    0H                                                  0526 01096000
*               CURRENT=CURRENT+1;  /* UPDATE POINTER                */ 01097000
         LA    @15,1                                               0526 01098000
         AL    @15,CURRENT(,R5)                                    0526 01099000
         ST    @15,CURRENT(,R5)                                    0526 01100000
*               IF CURCHAR=')'|     /* PAREN AFTER L OR              */ 01101000
*                   CURCHAR=','|    /* A COMMA OR                    */ 01102000
*                   CURCHAR=''''    /* IS IT THE END                 */ 01103000
*                 THEN              /* CHARACTER AFTER L IS OKAY     */ 01104000
         CLI   CURCHAR(@15),C')'                                   0527 01105000
         BE    @RT00527                                            0527 01106000
         CLI   CURCHAR(@15),C','                                   0527 01107000
         BE    @RT00527                                            0527 01108000
         CLI   CURCHAR(@15),C''''                                  0527 01109000
         BNE   @RF00527                                            0527 01110000
@RT00527 DS    0H                                                  0528 01111000
*                 DO;               /* CONTINUE WITH L PROCESSING    */ 01112000
*                   VALIDSW='1'B;   /* VALID SEQUENCE                */ 01113000
*                   DUMPL='1'B;                                    0530 01114000
*                   DUMPD='1'B;     /* INDICATE DASD                 */ 01115000
         OI    VALIDSW(R5),B'01100010'                             0531 01116000
*                   IF ENTDASD='0'B /* IF DASD INIT HAS ALREADY BEEN    01117000
*                                      ENTERED BYPASS SECOND ENTRANCE*/ 01118000
*                     THEN                                         0532 01119000
         TM    ENTDASD(R5),B'00001000'                             0532 01120000
