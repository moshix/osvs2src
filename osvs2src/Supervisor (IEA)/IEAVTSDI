*             UCBPTR=RPARM;         /* UCB ADDRESS RETURNED IN RPARM */ 01481000
         ST    RPARM,UCBPTR(,R5)                                   0644 01482000
*             IF RPARM=0            /* UCB SUPPLIED                  */ 01483000
*               THEN                /* NO                            */ 01484000
         LTR   RPARM,RPARM                                         0645 01485000
         BNZ   @RF00645                                            0645 01486000
*               SKIP='1'B;          /* SKIP PROCESSING               */ 01487000
         OI    SKIP(R5),B'10000000'                                0646 01488000
*             RFY                                                  0647 01489000
*               R6 UNRSTD;                                         0647 01490000
@RF00645 DS    0H                                                  0648 01491000
*           END;                                                   0648 01492000
*         IF SKIP='0'B              /* STILL OKAY TO CONTINUE ?      */ 01493000
*           THEN                    /* YES                           */ 01494000
@RF00633 TM    SKIP(R5),B'10000000'                                0649 01495000
         BNZ   @RF00649                                            0649 01496000
*           DO;                                                    0650 01497000
*                                                                  0650 01498000
*             /*******************************************************/ 01499000
*             /*                                                     */ 01500000
*             /* SYS1.DUMP DASD IS NOW OPENED VIA THE NIP OPEN       */ 01501000
*             /* SERVICE ROUTINE AND THE UCB IS MARKED PERMENTLY     */ 01502000
*             /* RESIDENT                                            */ 01503000
*             /*                                                     */ 01504000
*             /*******************************************************/ 01505000
*                                                                  0651 01506000
*             RFY                                                  0651 01507000
*               R6 RSTD;                                           0651 01508000
*             RTCTSDDS(CNT1)=''B;   /* CLEAR CURRENT ENTRY           */ 01509000
         LR    @15,CNT1                                            0652 01510000
         MH    @15,@CH01192                                        0652 01511000
         L     @11,CVTRTMCT(,RCVT)                                 0652 01512000
         ST    @15,@TF00001                                        0652 01513000
         ALR   @15,@11                                             0652 01514000
         XC    RTCTSDDS-12(12,@15),RTCTSDDS-12(@15)                0652 01515000
*             R6=ADDR(DMPOPN);      /* ADDRESS OF OPEN PARMS         */ 01516000
         LA    R6,DMPOPN                                           0653 01517000
*             R6->NOPNUCB=UCBPTR;   /* UCB ADDRESSIN OPEN PARMS      */ 01518000
*                                                                  0654 01519000
         L     @15,UCBPTR(,R5)                                     0654 01520000
         ST    @15,NOPNUCB(,R6)                                    0654 01521000
*             /*******************************************************/ 01522000
*             /*                                                     */ 01523000
*             /* THE DCB ADDRESS IS COMPUTED BY SUBTRACTING THE      */ 01524000
*             /* OFFSET OF DCBDEBAD FROM THE ADDRESS OF THE          */ 01525000
*             /* RTCTDCB(CNT1) WHERE CNT1 = TO THE ARRAY ENTRY NUMBER*/ 01526000
*             /* BEING FILLED IN.THIS RESULTS IN NIPOPEN PLACING THE */ 01527000
*             /* DEB ADDRESS IN THE SPECIFIC RTCTDCB FIELD OF THE    */ 01528000
*             /* DUMP DATASET TABLE ---- NOTE THIS IS A DUMMY DCB    */ 01529000
*             /* BEING OPENED.                                       */ 01530000
*             /*                                                     */ 01531000
*             /*******************************************************/ 01532000
*                                                                  0655 01533000
*             R6->NOPNDCB=ADDR(RTCTDCB(CNT1))-44;/* PLACE DCB ADDRESS   01534000
*                                      IN OPEN PARMS                 */ 01535000
         L     @15,@TF00001                                        0655 01536000
         LA    @15,RTCTDCB-12(@15,@11)                             0655 01537000
         SL    @15,@CF00288                                        0655 01538000
         ST    @15,NOPNDCB(,R6)                                    0655 01539000
*             DSNAME(1:LENGTH(DSNAME))=' ';/* CLEAR NAME FIELD       */ 01540000
         MVI   DSNAME+1,C' '                                       0656 01541000
         MVC   DSNAME+2(42),DSNAME+1                               0656 01542000
         MVI   DSNAME,C' '                                         0656 01543000
*             DUMPNAME=SYS1DUMP;    /* SET FIRST 9 BYTES TO SYS1.DUMP*/ 01544000
         MVC   DUMPNAME(9),@CC00283                                0657 01545000
*             DEVNUM=DVNAME;        /* SET NEXT 3 BYTES TO-XX - WHERE   01546000
*                                      XX= 00 -09                    */ 01547000
         MVC   DEVNUM(3),DVNAME(R4)                                0658 01548000
*             GEN REFS(DSNAME,DMPOPN) SETS(RPARM);                 0659 01549000
      IEAPMNIP TYPE=OPEN,PARAM=DMPOPN,OPT=(NB,LB,LC,SX,,LI,,RS)         01550000
*             IF RPARM<0            /* IF OPEN FAILED RPARM IS     0660 01551000
*                                      NEGATIVE                      */ 01552000
*               THEN                                               0660 01553000
         LTR   RPARM,RPARM                                         0660 01554000
         BNM   @RF00660                                            0660 01555000
*               SKIP='1'B;          /* SKIP FURTHER PROCESSING       */ 01556000
         OI    SKIP(R5),B'10000000'                                0661 01557000
*             ELSE                                                 0662 01558000
*               DO;                                                0662 01559000
         B     @RC00660                                            0662 01560000
@RF00660 DS    0H                                                  0663 01561000
*                 RTCTDSNM(CNT1)=DVNAME;/* SET NAME IN DATA SET TBL  */ 01562000
