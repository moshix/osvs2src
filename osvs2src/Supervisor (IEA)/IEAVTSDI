*             IF ECBCC='7F'X        /* SUCCESSFUL READ ?             */ 01600000
*               THEN                /* YES                           */ 01601000
         L     @15,ECBPTR                                          0674 01602000
         CLI   ECBCC(@15),X'7F'                                    0674 01603000
         BNE   @RF00674                                            0674 01604000
*               DO;                                                0675 01605000
*                 RTCTDSUS(CNT1)='1'B;/* INDICATE DATA SET IS FULL 0676 01606000
*                                      AND USED              @ZM44949*/ 01607000
         LR    @15,CNT1                                            0676 01608000
