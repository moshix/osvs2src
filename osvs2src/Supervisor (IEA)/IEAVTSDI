*               END;                                               0666 01576000
*             RFY                                                  0667 01577000
*               R6 UNRSTD;                                         0667 01578000
@RC00660 DS    0H                                                  0668 01579000
*           END;                                                   0668 01580000
*         IF SKIP='0'B THEN                                        0669 01581000
@RF00649 TM    SKIP(R5),B'10000000'                                0669 01582000
         BNZ   @RF00669                                            0669 01583000
*           DO;                                                    0670 01584000
*                                                                  0670 01585000
*             /*******************************************************/ 01586000
*             /*                                                     */ 01587000
*             /* NOW THE FIRST RECORD OF THE DASD SYS1.DUMP DATA SET */ 01588000
*             /* IS READ TO SEE IF THE DATA SET IS FULL OR EMPTY     */ 01589000
*             /*                                                     */ 01590000
*             /*******************************************************/ 01591000
*                                                                  0671 01592000
*             RPARM=ADDR(RCCWCHAN); /* SET REG 1 TO ADDRESS OF READ     01593000
*                                      CCW CHAIN                     */ 01594000
         LA    RPARM,RCCWCHAN                                      0671 01595000
*             SRECNU='01'X;         /* SEARCH FOR RECORD 1           */ 01596000
         MVI   SRECNU,X'01'                                        0672 01597000
*             CALL SDIEXCP;         /* ISSUE EXCP                    */ 01598000
