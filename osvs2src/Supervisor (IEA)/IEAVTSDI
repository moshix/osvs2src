*                     BUFCR='01'X;  /* RECORD 1              @YM05431*/ 01658000
         MVI   BUFCR,X'01'                                         0687 01659000
*                     SRECNU='00'X; /* SEARCH REC 0          @YM05431*/ 01660000
         MVI   SRECNU,X'00'                                        0688 01661000
*                     R1=ADDR(WRITECCW);/* POINT TO WRITE    @YM05431*/ 01662000
         LA    R1,WRITECCW                                         0689 01663000
*                     CALL SDIEXCP; /* WRITE EOF             @YM05431*/ 01664000
         BAL   @14,SDIEXCP                                         0690 01665000
*                     IF ECBCC^='7F'X/* ERROR ON WRITE?      @YM05431*/ 01666000
*                       THEN        /* THIS ERROR CANNOT BE PROCESSED   01667000
*                                      BY SDUMP                      */ 01668000
         L     @15,ECBPTR                                          0691 01669000
