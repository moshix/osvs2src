         MVI   DSNAME+1,C' '                                       0637 01464000
         MVC   DSNAME+2(42),DSNAME+1                               0637 01465000
         MVI   DSNAME,C' '                                         0637 01466000
*             DUMPNAME=SYS1DUMP;    /* SET NAME TO SYS1.DUMP         */ 01467000
         MVC   DUMPNAME(9),@CC00283                                0638 01468000
*             DEVNUM=DVNAME;        /* SET DUMP NUMBER IE-SYS1.DUMP06   01469000
*                                      -                             */ 01470000
         MVC   DEVNUM(3),DVNAME(R4)                                0639 01471000
*             R6->NMNTVS=VOLSERNU;  /* SET VOLUME SERIAL NUMBER      */ 01472000
         MVC   NMNTVS(6,R6),VOLSERNU                               0640 01473000
*             R6->NMNTDT=DVCOD;     /* SET TWO BYTE DEVICE CODE      */ 01474000
         MVC   NMNTDT(2,R6),DVCOD                                  0641 01475000
*             R13=ADDR(REGSAVE);                                   0642 01476000
         LA    R13,REGSAVE(,R5)                                    0642 01477000
*             GEN REFS(DSNAME,MOUNTD) SETS(RPARM)                  0643 01478000
*                 (IEAPMNIP TYPE=MOUNT,PARAM=MOUNTD,OPT=(,LC,LI)); 0643 01479000
