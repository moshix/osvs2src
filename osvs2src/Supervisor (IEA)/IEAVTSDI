*                     DO;                                          0533 01122000
*                       ENTDASD='1'B;                              0534 01123000
         OI    ENTDASD(R5),B'00001000'                             0534 01124000
*                       CALL DASDINIT;                             0535 01125000
         BAL   @14,DASDINIT                                        0535 01126000
*                     END;                                         0536 01127000
*                 END;                                             0537 01128000
*               ELSE                /* CHARACTER AFTER L IS INVALID  */ 01129000
*                 CURRENT=ENDCOL;   /* STOP SCANNING                 */ 01130000
         B     @RC00527                                            0538 01131000
@RF00527 L     @15,ENDCOL(,R5)                                     0538 01132000
         ST    @15,CURRENT(,R5)                                    0538 01133000
*             END;                                                 0539 01134000
@RC00527 DS    0H                                                  0540 01135000
*         END;                                                     0540 01136000
@RC00523 DS    0H                                                  0541 01137000
*       IF VALIDSW='0'B             /* STILL INVALID?                */ 01138000
*         THEN                      /* YES MAY BE AN UNSCANABLE    0541 01139000
*                                      CHARACTER                     */ 01140000
@RF00520 TM    VALIDSW(R5),B'00000010'                             0541 01141000
         BNZ   @RF00541                                            0541 01142000
*         CURRENT=ENDCOL;           /* SO GET OUT OF LOOP            */ 01143000
         L     @15,ENDCOL(,R5)                                     0542 01144000
         ST    @15,CURRENT(,R5)                                    0542 01145000
*     END;                          /* END OF DO WHILE               */ 01146000
@RF00541 DS    0H                                                  0543 01147000
@DE00448 CLC   CURRENT(4,R5),ENDCOL(R5)                            0543 01148000
         BNE   @DL00448                                            0543 01149000
*     IF VALIDSW='0'B               /* ARE THE OPTIONS VALID ?       */ 01150000
*       THEN                        /* NO PROMPT THE OPERATOR        */ 01151000
         TM    VALIDSW(R5),B'00000010'                             0544 01152000
         BNZ   @RF00544                                            0544 01153000
*       DO;                                                        0545 01154000
*                                                                  0545 01155000
*         /***********************************************************/ 01156000
*         /*                                                         */ 01157000
*         /* IT HAS BEEN DETERMINED THAT THE OPTIONS SPECIFIED BY THE*/ 01158000
*         /* OPERATOR ARE INCORRECT. IN ORDER TO PROCEED THE OPERATOR*/ 01159000
*         /* IS PROMPTED TO RESPECIFY THE DUMP= PARAMETERS. THIS IS  */ 01160000
*         /* DONE BY USING THE NIP PROMPT ROUTINE.THIS RESULTS IN A  */ 01161000
*         /* COMPLETE RESCAN OF THE OPTIONS                          */ 01162000
*         /*                                                         */ 01163000
*         /***********************************************************/ 01164000
*                                                                  0546 01165000
*         DACTION=0;                /* INITIALIZE FLAGS              */ 01166000
         SLR   @15,@15                                             0546 01167000
         STH   @15,DACTION(,R5)                                    0546 01168000
*         BEGIN=0;                  /* SET BEGIN ADDRESS TO 0        */ 01169000
         ST    @15,BEGIN(,R5)                                      0547 01170000
*         ENDCOL=0;                                                0548 01171000
         ST    @15,ENDCOL(,R5)                                     0548 01172000
*         RQUEADD=0;                                               0549 01173000
         ST    @15,RQUEADD(,R5)                                    0549 01174000
*         CURSVE=0;                                                0550 01175000
         ST    @15,CURSVE(,R5)                                     0550 01176000
*         CURRENT=0;                                               0551 01177000
         ST    @15,CURRENT(,R5)                                    0551 01178000
*         SDRNAME='DUMP    ';       /* SET NAME TO DUMP SO THAT    0552 01179000
*                                      PROMPT KNOWS WHERE TO ENTER 0552 01180000
*                                      THE OPTIONS                   */ 01181000
         MVC   SDRNAME(8,R5),@CC03790                              0552 01182000
*         R13=ADDR(REGSAVE);        /* SET SAVE AREA ADDRESS         */ 01183000
         LA    R13,REGSAVE(,R5)                                    0553 01184000
*         NIPPTLST='0'B;            /* TURN OFF LIST IN PARM TAB   0554 01185000
*                                      BECAUSE PROMT DOES NOT        */ 01186000
         L     @15,NVTPTAB(,RNVT)                                  0554 01187000
         NI    NIPPTLST+88(@15),B'10111111'                        0554 01188000
*         GEN(IEAPMNIP TYPE=PRMPT,PARAM=SDRNAME(R5),OPT=ERROR);    0555 01189000
         IEAPMNIP TYPE=PRMPT,PARAM=SDRNAME(R5),OPT=ERROR                01190000
*         IF RPARM<0|NIPPTCAN='1'B  /* DID PROMPT FIND DUMP IN PARM     01191000
*                                      TAB OR DID OPERATOR SAY AN EOB   01192000
*                                      ?                             */ 01193000
*           THEN                                                   0556 01194000
         LTR   RPARM,RPARM                                         0556 01195000
         BM    @RT00556                                            0556 01196000
         L     @15,NVTPTAB(,RNVT)                                  0556 01197000
         TM    NIPPTCAN+88(@15),B'00100000'                        0556 01198000
         BO    @RT00556                                            0556 01199000
*           ;                       /* YES - USE DEFAULT             */ 01200000
*         ELSE                      /* OPTIONS WERE RESPECIFIED      */ 01201000
*           DO;                                                    0558 01202000
*             IF NIPPTLST='1'B      /* WAS LIST SPECIFIED ?          */ 01203000
*               THEN                /* YES                           */ 01204000
         L     @15,NVTPTAB(,RNVT)                                  0559 01205000
         TM    NIPPTLST+88(@15),B'01000000'                        0559 01206000
         BNO   @RF00559                                            0559 01207000
*               DO;                                                0560 01208000
*                 DUMPL='1'B;                                      0561 01209000
*                 DUMPD='1'B;       /* DASD IS IMPLIED               */ 01210000
         OI    DUMPL(R5),B'01100000'                               0562 01211000
*               END;                                               0563 01212000
*             BEGIN=NIPPTADR;       /* SET TO BEGINNING OF DATA      */ 01213000
@RF00559 L     @15,NVTPTAB(,RNVT)                                  0564 01214000
         L     @15,NIPPTADR+88(,@15)                               0564 01215000
         ST    @15,BEGIN(,R5)                                      0564 01216000
*             CURRENT=BEGIN;                                       0565 01217000
         ST    @15,CURRENT(,R5)                                    0565 01218000
*           END;                                                   0566 01219000
*         IF BEGIN=0                /* ANY DATA ?                    */ 01220000
*           THEN                    /* NO                            */ 01221000
@RT00556 L     @15,BEGIN(,R5)                                      0567 01222000
         LTR   @15,@15                                             0567 01223000
         BNZ   @RF00567                                            0567 01224000
*           DO;                                                    0568 01225000
*             VALIDSW='1'B;         /* USE DEFAULT                   */ 01226000
*             DUMPD='1'B;                                          0570 01227000
*             ENTDASD='1'B;         /* INDICATE DASD INIT BEING    0571 01228000
*                                      ENTERED                       */ 01229000
         OI    VALIDSW(R5),B'00101010'                             0571 01230000
*             CALL DASDINIT;                                       0572 01231000
         BAL   @14,DASDINIT                                        0572 01232000
*           END;                                                   0573 01233000
*         ELSE                                                     0574 01234000
*           DO;                     /* FIND LENGTH OF DATA           */ 01235000
         B     @RC00567                                            0574 01236000
@RF00567 DS    0H                                                  0575 01237000
*             DO WHILE(CURCHAR^='''');/* FIND TRAILING QUOTE         */ 01238000
         B     @DE00575                                            0575 01239000
@DL00575 DS    0H                                                  0576 01240000
*               CURRENT=CURRENT+1;  /* UPDATE TO NEXT ENTRY          */ 01241000
         LA    @15,1                                               0576 01242000
         AL    @15,CURRENT(,R5)                                    0576 01243000
         ST    @15,CURRENT(,R5)                                    0576 01244000
*             END;                  /* END OF DO WHILE               */ 01245000
@DE00575 L     @15,CURRENT(,R5)                                    0577 01246000
         CLI   CURCHAR(@15),C''''                                  0577 01247000
         BNE   @DL00575                                            0577 01248000
*             ENDCOL=CURRENT;       /* SAVE ADDRESS OF END OF DATA   */ 01249000
         ST    @15,ENDCOL(,R5)                                     0578 01250000
*             CURRENT=BEGIN;                                       0579 01251000
         L     @15,BEGIN(,R5)                                      0579 01252000
         ST    @15,CURRENT(,R5)                                    0579 01253000
*           END;                                                   0580 01254000
*       END;                        /* END OF PROMT                  */ 01255000
*     ELSE                                                         0582 01256000
*       DO;                                                        0582 01257000
         B     @RC00544                                            0582 01258000
@RF00544 DS    0H                                                  0583 01259000
*         IF DUMPL='1'B&ENTDASD='0'B/* WAS LIST SPECIFIED AND      0583 01260000
*                                      DASDINIT WAS NOT ENTERED    0583 01261000
*                                      BECAUSE DASD WAS NOT FOUND    */ 01262000
*           THEN                                                   0583 01263000
         TM    DUMPL(R5),B'01000000'                               0583 01264000
         BNO   @RF00583                                            0583 01265000
         TM    ENTDASD(R5),B'00001000'                             0583 01266000
         BNZ   @RF00583                                            0583 01267000
*           DO;                     /* ENTER DASD INIT               */ 01268000
*             DUMPD='1'B;                                          0585 01269000
*             ENTDASD='1'B;                                        0586 01270000
         OI    DUMPD(R5),B'00101000'                               0586 01271000
*             CALL DASDINIT;                                       0587 01272000
         BAL   @14,DASDINIT                                        0587 01273000
*           END;                                                   0588 01274000
*       END;                                                       0589 01275000
@RF00583 DS    0H                                                  0590 01276000
*   END;                            /* END OF DO WHILE INVALID       */ 01277000
@RC00544 DS    0H                                                  0590 01278000
@DE00447 TM    VALIDSW(R5),B'00000010'                             0590 01279000
         BZ    @DL00447                                            0590 01280000
*   CNT1=1;                         /* SET INDEX FOR DATA SET TABLE     01281000
*                                      ARRAY                         */ 01282000
         LA    CNT1,1                                              0591 01283000
*   DO WHILE(DUMPN='0'B&DUMPF='0'B);/* DO IF DUMP NO IS NOT SPECIFIED   01284000
*                                      OR WHILE DATA SETS ARE STILL     01285000
*                                      TO BE PROCESSED               */ 01286000
         B     @DE00592                                            0592 01287000
@DL00592 DS    0H                                                  0593 01288000
*     IF DUMPD='1'B                 /* WAS DUMP DASD OR L OR ZERO  0593 01289000
*                                      SPECIFIED                     */ 01290000
*       THEN                        /* YES DASD PROCESS TO OCCUR     */ 01291000
         TM    DUMPD(R5),B'00100000'                               0593 01292000
         BNO   @RF00593                                            0593 01293000
*       DO;                                                        0594 01294000
*         RFY                                                      0595 01295000
*           R4 RSTD;                                               0595 01296000
*                                                                  0595 01297000
*         /***********************************************************/ 01298000
*         /*                                                         */ 01299000
*         /* ONCE A DASD DEVICE HAS BEEN PLACED ON THE DEVICE QUEUE  */ 01300000
*         /* IT MUST BE PROCESSED BY THIS ROUTINE TO INSURE THE      */ 01301000
*         /* STATUS OF THE DEVICE.CHECKS ARE MADE TO INSURE THAT THE */ 01302000
*         /* DASD DEVICES ARE CATALOGED AND THAT THE DEVICE TYPE IS A*/ 01303000
*         /* SUPPORTED DEVICE FOR SDUMP PROCESSING IF THE DEVICE IS  */ 01304000
*         /* SUPPORTED AND MOUNTED AND THE DATA SET IS OPENED THEN   */ 01305000
*         /* THE DEVICE IS CHECKED TO SEE IF THE DATASET IS EMPTY OR */ 01306000
*         /* FULL                                                    */ 01307000
*         /*                                                         */ 01308000
*         /***********************************************************/ 01309000
*                                                                  0596 01310000
*         SKIP='0'B;                /* TURN OFF THE SKIP SWICH       */ 01311000
*                                                                  0596 01312000
         NI    SKIP(R5),B'01111111'                                0596 01313000
*         /***********************************************************/ 01314000
*         /*                                                         */ 01315000
*         /* THIS ROUTINE NOW CHECKS THE DASD DEVICES TO INSURE THAT */ 01316000
*         /* THEY ARE CATALOGED.THE DEVICE Q IS SEARCHED TO FIND THE */ 01317000
*         /* NEXT DEVICE TO BE PROCESSED IF THERE ARE NO DEVICES THE */ 01318000
*         /* DUMPF FLAG IS TURNED ON IF THE DEVICE IS NOT DASD THE   */ 01319000
*         /* SKIP FLAG IS TURNED ON IF IT IS DASD A LOCATE MACRO IS  */ 01320000
*         /* ISSUED TO INSURE THE DEVICE IS CATALOGED                */ 01321000
*         /*                                                         */ 01322000
*         /***********************************************************/ 01323000
*                                                                  0597 01324000
*         R4=RQUEADD;               /* GET ADDRESS OF QUEUE          */ 01325000
         L     R4,RQUEADD(,R5)                                     0597 01326000
*         DO WHILE(R4^=0&DPROCSD='1'B);/* FIND NEXT DEVICE TO PROCESS*/ 01327000
         B     @DE00598                                            0598 01328000
@DL00598 DS    0H                                                  0599 01329000
*           R4=NXTENTAD;            /* ADDRESS OF NEXT BLOCK         */ 01330000
         L     R4,NXTENTAD(,R4)                                    0599 01331000
*         END;                                                     0600 01332000
@DE00598 LTR   R4,R4                                               0600 01333000
         BZ    @DC00598                                            0600 01334000
         TM    DPROCSD(R4),B'01000000'                             0600 01335000
         BO    @DL00598                                            0600 01336000
@DC00598 DS    0H                                                  0601 01337000
*         IF R4=0                   /* ANY DEVICES LEFT ON Q         */ 01338000
*           THEN                                                   0601 01339000
         LTR   R4,R4                                               0601 01340000
         BNZ   @RF00601                                            0601 01341000
*           DO;                     /* NO                            */ 01342000
*             DUMPF='1'B;           /* INDICATE FINISHED             */ 01343000
         OI    DUMPF(R5),B'00000001'                               0603 01344000
*             SKIP='1'B;                                           0604 01345000
         OI    SKIP(R5),B'10000000'                                0604 01346000
*           END;                                                   0605 01347000
*         ELSE                      /* MORE TO PROCESS               */ 01348000
*           DO;                                                    0606 01349000
         B     @RC00601                                            0606 01350000
@RF00601 DS    0H                                                  0607 01351000
*             IF DASDDV='0'B        /* IS IT DASD                    */ 01352000
*               THEN                                               0607 01353000
         TM    DASDDV(R4),B'10000000'                              0607 01354000
         BNZ   @RF00607                                            0607 01355000
*               DO;                 /* NO                            */ 01356000
*                 SKIP='1'B;                                       0609 01357000
         OI    SKIP(R5),B'10000000'                                0609 01358000
*               END;                                               0610 01359000
*             ELSE                  /* IT IS A DASD DEVICE           */ 01360000
*               DO;                                                0611 01361000
         B     @RC00607                                            0611 01362000
@RF00607 DS    0H                                                  0612 01363000
*                 DPROCSD='1'B;     /* INDICATE THIS DEVICE AS BEING    01364000
*                                      PROCESSED                     */ 01365000
         OI    DPROCSD(R4),B'01000000'                             0612 01366000
*                 LOCATBL(1:200)=''B;/* CLEAR LOCATE WORK AREA       */ 01367000
         XC    LOCATBL(200),LOCATBL                                0613 01368000
*                 LOCATBL(201:LENGTH(LOCATBL))=''B;                0614 01369000
         XC    LOCATBL+200(65),LOCATBL+200                         0614 01370000
*                 DSNAME=SYS1DUMP;  /* SET NAME TO SYS1.DUMPXX FOR 0615 01371000
*                                      LOCATE                        */ 01372000
         MVI   DSNAME+9,C' '                                       0615 01373000
         MVC   DSNAME+10(34),DSNAME+9                              0615 01374000
         MVC   DSNAME(9),@CC00283                                  0615 01375000
*                 DEVNUM=DVNAME;    /* UPDATE XX TO NUMBER INDICATED    01376000
*                                      BY DEVICE TABLE               */ 01377000
         MVC   DEVNUM(3),DVNAME(R4)                                0616 01378000
*                 RFY                                              0617 01379000
*                   R15 RSTD;                                      0617 01380000
*                 GEN REFS(LIST) SETS(RPARM,R15,LOCATBL)(LOCATE LIST);  01381000
         LOCATE LIST                                                    01382000
*                 IF R15^=0         /* DATA SET FOUND ?              */ 01383000
*                   THEN            /* NO                            */ 01384000
         LTR   R15,R15                                             0619 01385000
         BZ    @RF00619                                            0619 01386000
*                   SKIP='1'B;      /* SKIP THIS DEVICE              */ 01387000
         OI    SKIP(R5),B'10000000'                                0620 01388000
*                 ELSE                                             0621 01389000
*                   DO;                                            0621 01390000
         B     @RC00619                                            0621 01391000
@RF00619 DS    0H                                                  0622 01392000
*                     IF NUMENTRY^=1|/* ONLY 1 ENTRY ALLOWED         */ 01393000
*                         DEVCLAS^='20'X|/* MUST BE DASD             */ 01394000
*                         (DEVCOD^='06'X&/* DEVICE A 2305 MOD 1      */ 01395000
*                         DEVCOD^='07'X&/* OR A 2305 MOD 2           */ 01396000
*                         DEVCOD^='08'X&/* OR 2319 - 2314            */ 01397000
*                         DEVCOD^='09'X&/* OR 3330 -A                */ 01398000
*                         DEVCOD^='0A'X&/* OR 3340                   */ 01399000
*                         DEVCOD^='0B'X&/* OR 3350           @Z30RSTA*/ 01400000
*                         DEVCOD^='0D'X)/* OR 3330 -C                */ 01401000
*                       THEN        /* DEVICE IS NOT CORRECT FOR   0622 01402000
*                                      SDUMP                         */ 01403000
         CLC   NUMENTRY(2),@CH00055                                0622 01404000
         BNE   @RT00622                                            0622 01405000
         CLI   DEVCLAS,32                                          0622 01406000
         BNE   @RT00622                                            0622 01407000
         CLI   DEVCOD,6                                            0622 01408000
         BE    @RF00622                                            0622 01409000
         CLI   DEVCOD,7                                            0622 01410000
         BE    @RF00622                                            0622 01411000
         CLI   DEVCOD,8                                            0622 01412000
         BE    @RF00622                                            0622 01413000
         CLI   DEVCOD,9                                            0622 01414000
         BE    @RF00622                                            0622 01415000
         CLI   DEVCOD,10                                           0622 01416000
         BE    @RF00622                                            0622 01417000
         CLI   DEVCOD,11                                           0622 01418000
         BE    @RF00622                                            0622 01419000
         CLI   DEVCOD,13                                           0622 01420000
         BE    @RF00622                                            0622 01421000
@RT00622 DS    0H                                                  0623 01422000
*                       DO;                                        0623 01423000
*                         SKIP='1'B;/* SKIP FURTHER PROCESS FOR THIS    01424000
*                                      DEVICE                        */ 01425000
         OI    SKIP(R5),B'10000000'                                0624 01426000
*                         NSDEV=DVNAME;/* PUT DEVICE NUMBER IN     0625 01427000
*                                      MESSAGE 2                     */ 01428000
         MVC   NSDEV(3),DVNAME(R4)                                 0625 01429000
*                         R13=ADDR(REGSAVE);                       0626 01430000
         LA    R13,REGSAVE(,R5)                                    0626 01431000
*                         GEN REFS(MSG02) SETS(RPARM,R14,R15)      0627 01432000
*                             (IEAPMNIP TYPE=WTO,PARAM=MSG02);     0627 01433000
         IEAPMNIP TYPE=WTO,PARAM=MSG02                                  01434000
*                       END;                                       0628 01435000
*                   END;                                           0629 01436000
@RF00622 DS    0H                                                  0630 01437000
*                 RFY                                              0630 01438000
*                   R15 UNRSTD;                                    0630 01439000
@RC00619 DS    0H                                                  0631 01440000
*               END;                                               0631 01441000
*           END;                                                   0632 01442000
@RC00607 DS    0H                                                  0633 01443000
*         IF SKIP='0'B              /* WAS DEVICE LOCATED PROPERLY   */ 01444000
*           THEN                                                   0633 01445000
@RC00601 TM    SKIP(R5),B'10000000'                                0633 01446000
         BNZ   @RF00633                                            0633 01447000
*           DO;                     /* YES                           */ 01448000
*                                                                  0634 01449000
*             /*******************************************************/ 01450000
*             /*                                                     */ 01451000
*             /* IT HAS BEEN DETERMINED THAT THIS DEVICE IS CATALOGED*/ 01452000
*             /* AND SUPPORTED.NOW WE MUST INSURE THAT IT IS MOUNTED */ 01453000
*             /*                                                     */ 01454000
*             /*******************************************************/ 01455000
*                                                                  0635 01456000
*             RFY                                                  0635 01457000
*               R6 RSTD;                                           0635 01458000
