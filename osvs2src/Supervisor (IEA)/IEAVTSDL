*   END SDADDADR;                                                  0385 01202000
@EL00004 DS    0H                                                  0385 01203000
@EF00004 DS    0H                                                  0385 01204000
@ER00004 BR    @14                                                 0385 01205000
*                                                                  0386 01206000
*/********************************************************************/ 01207000
*/*   THIS SEGMENT WILL ARRANGE A PART OF THE ADDRESS RANGE TABLE.   */ 01208000
*/*   THE ADDRESS RANGES WILL BE PLACED IN ASCENDING ORDER.          */ 01209000
*/*   DUPLICATE RANGES WILL BE DELETED.  OVERLAPPING RANGES WILL     */ 01210000
*/*   BE COMBINED INTO ONE RANGE.                                    */ 01211000
*/*                                                                  */ 01212000
*/*   INPUT: RANGEPTR CONTAINS ADDRESS OF FIRST ADDRESS RANGE IN     */ 01213000
*/*      THE PART OF TABLE TO BE ARRANGED, LAST RANGE IN THE PART OF */ 01214000
*/*        OF THE TABLE IS INDICATED BY HAVING A STARTADR OF ENDPART */ 01215000
*/*         AND AN ENDADDR OF NULL WITH LASTRNGE FLAG ON.            */ 01216000
*/*                                                                  */ 01217000
*/*   OUTPUT: COMPRESSED, ASCENDING ORDERED PART OF ADDRESS RANGE    */ 01218000
*/*      TABLE WITH RANGEPTR CONTAINING ADDRESS OF LAST RANGE IN THIS*/ 01219000
*/*      PART OF THE ADDRESS RANGE TABLE(ENDPART ADDRESS RANGE).     */ 01220000
*/*      FLGRTFUL='1'B IF TABLE FULL ELSE FLGRTFUL='0'B.             */ 01221000
*/*                                                                  */ 01222000
*/*      CALLED BY: SDLOCAL                                          */ 01223000
*/********************************************************************/ 01224000
*                                                                  0386 01225000
*ARRANGE:                                                          0386 01226000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0386 01227000
ARRANGE  DS    0H                                                  0387 01228000
*   RFY                                                            0387 01229000
*    (R10,                                                         0387 01230000
*     R11,                                                         0387 01231000
*     R4) RSTD;                                                    0387 01232000
*                                                                  0387 01233000
*   /*****************************************************************/ 01234000
*   /*                                                               */ 01235000
*   /* PUT RANGES IN ASCENDING ORDER BY STARTADR R10 - ADDRESS OF    */ 01236000
*   /* CURRENT RANGE R11 - ADDRESS OF NEXT RANGE R12 - INDICATE A    */ 01237000
*   /* SWITCH IN ORDER OF RANGES MADE                                */ 01238000
*   /*                                                               */ 01239000
*   /*****************************************************************/ 01240000
*                                                                  0388 01241000
*   R4=ONE;                         /* INDICATE SWITCH-REG NON-ZERO  */ 01242000
         LA    R4,1                                                0388 01243000
*   DO WHILE R4^=NULL;              /* PUT RANGES IN ASCEND ORDER    */ 01244000
         B     @DE00389                                            0389 01245000
@DL00389 DS    0H                                                  0390 01246000
*     R10=RANGEPTR;                 /* ADDR OF FIRST CURRENT RANGE   */ 01247000
         L     R10,RANGEPTR(,R7)                                   0390 01248000
*     R11=R10+LEN1ADRN;             /* ADDRESS OF NEXT RANGE         */ 01249000
         LA    R11,8                                               0391 01250000
         ALR   R11,R10                                             0391 01251000
*     R4=NULL;                      /* NO SWITCH MADE                */ 01252000
         SLR   R4,R4                                               0392 01253000
*     DO WHILE R10->LASTRNGE='0'B;  /* CHECK ALL RNGES IN THIS PART  */ 01254000
         B     @DE00393                                            0393 01255000
@DL00393 DS    0H                                                  0394 01256000
*       IF R11->STARTADR<           /* IF NEXT START ADDR LESS THAN  */ 01257000
*           R10->STARTADR THEN      /* CURRENT START ADDRESS,        */ 01258000
         CLC   STARTADR(4,R11),STARTADR(R10)                       0394 01259000
         BNL   @RF00394                                            0394 01260000
*         DO;                       /* SWITCH RANGES                 */ 01261000
*           R10->STORANGE=R10->STORANGE&&R11->STORANGE;            0396 01262000
         XC    STORANGE(8,R10),STORANGE(R11)                       0396 01263000
*           R11->STORANGE=R11->STORANGE&&R10->STORANGE;            0397 01264000
         XC    STORANGE(8,R11),STORANGE(R10)                       0397 01265000
*           R10->STORANGE=R10->STORANGE&&R11->STORANGE;            0398 01266000
         XC    STORANGE(8,R10),STORANGE(R11)                       0398 01267000
*           R4=ONE;                 /* INDICATE SWITCH MADE          */ 01268000
         LA    R4,1                                                0399 01269000
*         END;                                                     0400 01270000
*       ELSE                                                       0401 01271000
*         ;                                                        0401 01272000
@RF00394 DS    0H                                                  0402 01273000
*       R10=R11;                    /* UPDATE CURRENT RANGE POINTER  */ 01274000
         LR    R10,R11                                             0402 01275000
*       R11=R11+LEN1ADRN;           /* UPDATE NEXT RANGE POINTER     */ 01276000
         AL    R11,@CF00059                                        0403 01277000
*     END;                                                         0404 01278000
@DE00393 TM    LASTRNGE(R10),B'10000000'                           0404 01279000
         BZ    @DL00393                                            0404 01280000
*   END;                                                           0405 01281000
*                                                                  0405 01282000
@DE00389 LTR   R4,R4                                               0405 01283000
         BNZ   @DL00389                                            0405 01284000
*   /*****************************************************************/ 01285000
*   /*                                                               */ 01286000
*   /* REMOVE DUPLICATE RANGES, COMBINE OVERLAPPING RANGES. DELETED  */ 01287000
*   /* RANGES WILL HAVE A ZEROED ENTRY IN THE TABLE. R10 - ADDRESS OF*/ 01288000
*   /* CURRENT RANGE R11 - ADDRESS OF NEXT RANGE                     */ 01289000
*   /*                                                               */ 01290000
*   /*****************************************************************/ 01291000
*                                                                  0406 01292000
*   R10=RANGEPTR;                   /* ADDR OF FIRST CURRENT RANGE   */ 01293000
         L     R10,RANGEPTR(,R7)                                   0406 01294000
*   R11=R10+LEN1ADRN;               /* ADDRESS OF NEXT RANGE         */ 01295000
         LA    R11,8                                               0407 01296000
         ALR   R11,R10                                             0407 01297000
*   DO WHILE R10->LASTRNGE='0'B;    /* CHECK ALL RNGES IN THIS PART  */ 01298000
         B     @DE00408                                            0408 01299000
@DL00408 DS    0H                                                  0409 01300000
*     IF R11->STARTADR<=            /* IF NEXT RANGE OVERLAPS CURR   */ 01301000
*         R10->ENDADDR THEN                                        0409 01302000
