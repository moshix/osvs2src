         CR    @15,@12                                             0307 00897000
         BNL   @RF00307                                            0307 00898000
*       DO;                         /* SET UP ADDRESS RANGE TABLE  0308 00899000
*                                      RANGE                         */ 00900000
*         STARTADR=R10->STARTADR;   /* START ADDR, ADD TO TABLE      */ 00901000
         L     @08,RANGEPTR(,R7)                                   0309 00902000
         ST    @15,STARTADR(,@08)                                  0309 00903000
*         IF(R10->ENDADDR&ENDPART)<=/* IF ENDING ADDRESS     @ZA29803*/ 00904000
*             USEEND THEN           /* ALSO IN THIS STORAGE AREA   0310 00905000
*                                                            @ZA29803*/ 00906000
