         BNE   @DL00197                                            0240 00619000
*                 RFY                                              0241 00620000
*                  (R4,                                            0241 00621000
*                   R5) UNRSTD;                                    0241 00622000
*                 GEN REFS(PSALITA);                               0242 00623000
FRELOCKL SETLOCK   RELEASE,TYPE=CMS,                                   ,00624000
               RELATED=(LPDE/CDE,IEAVAD00(GETLOCKL))                    00625000
*                 ERRCMSLK='0'B;    /* INDICATE CMS LOCK FREED       */ 00626000
         NI    ERRCMSLK(R3),B'11110111'                            0243 00627000
*               END;                                               0244 00628000
*             ELSE                                                 0245 00629000
*               ;                                                  0245 00630000
@RF00175 DS    0H                                                  0246 00631000
*             IF FLGRTFUL='0'B THEN /* IF RANGE TABLE NOT FULL,      */ 00632000
