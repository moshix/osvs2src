         BNZ   @RF00246                                            0246 00634000
*               DO;                 /* OBTAIN NEXT TCB ADDRESS       */ 00635000
*                 R13=ADDR(SDWRGSV2);/* ADDR OF SVC DUMP SAVE AREA   */ 00636000
*                                                                  0248 00637000
         LA    R13,SDWRGSV2(,R7)                                   0248 00638000
*                 /***************************************************/ 00639000
*                 /*                                                 */ 00640000
*                 /* INTERFACE TO IEATRSCN R7 - WORKADDR - DESTROYED */ 00641000
*                 /* R8 - BASEREG3 - ADDR OF TOP TCB IN FAMILY R9 -  */ 00642000
*                 /* BASEREG2 - DESTROYED R10 - TRSCNTCB - IN, START */ 00643000
*                 /* TCB(PRE-SET)/OUT, NEXT TCB R11 - TRSCERET - RET */ 00644000
*                 /* ADDR, NO MORE TCBS R14 - TRSCNRET - RET ADDR,   */ 00645000
*                 /* TCB FOUND R15 - TRSCENPT - IEATRSCN ENTRY POINT */ 00646000
*                 /* ADDR, BRANCH REG                                */ 00647000
*                 /*                                                 */ 00648000
*                 /***************************************************/ 00649000
*                                                                  0249 00650000
*                 GEN REFS(CVTRSCN);                               0249 00651000
         STM   R11,R9,0(R13)      SAVE ALL REGS EXCEPT R10              00652000
         LA    R11,ENDTRSCN       RETURN ADDRESS, NO MORE TCBS          00653000
         LA    R14,TCBFOUND       RETURN ADDRESS, TCB FOUND             00654000
         L     R8,PSAAOLD        ADDRESS OF CURRENT ASCB                00655000
         L     R8,ASCBASXB(R8)  ADDR OF ASXB                            00656000
         L     R8,ASXBFTCB(R8)  ADDR OF TOP TCB IN FAMILY               00657000
         L     R15,FLCCVT              ADDRESS OF CVT                   00658000
         L     R15,CVTRSCN(R15)  TREE SCAN ENTRY POINT ADDR             00659000
         BR    R15                GET ADDR NEXT TCB IN FAMILY           00660000
ENDTRSCN SR    R10,R10       NO MORE TCBS, END LOOP                     00661000
TCBFOUND LM    R11,R9,0(R13)      RESTORE SVC DUMP REGISTERS            00662000
*               END;                                               0250 00663000
*             ELSE                                                 0251 00664000
*               R10=NULL;           /* RANGE TABLE FULL-END LOOP     */ 00665000
         B     @RC00246                                            0251 00666000
@RF00246 SLR   R10,R10                                             0251 00667000
*           END;                                                   0252 00668000
@RC00246 DS    0H                                                  0252 00669000
@DE00163 LTR   R10,R10                                             0252 00670000
         BNZ   @DL00163                                            0252 00671000
*           RFY                                                    0253 00672000
*            (TCB,                                                 0253 00673000
*             TCBFIX) BASED;                                       0253 00674000
*           RFY                                                    0254 00675000
*             R10 UNRSTD;                                          0254 00676000
*         END;                                                     0255 00677000
*RTYLOC30:                                                         0256 00678000
