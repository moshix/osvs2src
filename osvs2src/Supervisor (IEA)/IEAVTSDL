*                                    CALL LPALPDE;/* BUILD ADDR RANGE   00584000
*                                      FOR SVC                       */ 00585000
         BAL   @14,LPALPDE                                         0229 00586000
*                                   ELSE                           0230 00587000
*                                    ;                             0230 00588000
@RF00228 DS    0H                                                  0231 00589000
*                                 END;                             0231 00590000
*                               ELSE/* IF A CDE WAS FOUND            */ 00591000
*                                 CALL LPACDE;/* BUILD ADDR RNAGE  0232 00592000
*                                      FROM CDE                      */ 00593000
         B     @RC00222                                            0232 00594000
@RF00222 BAL   @14,LPACDE                                          0232 00595000
*                             END;                                 0233 00596000
*                           ELSE                                   0234 00597000
*                             ;                                    0234 00598000
@RF00210 DS    0H                                                  0235 00599000
*                         END;                                     0235 00600000
@RC00210 DS    0H                                                  0236 00601000
*                     END;                                         0236 00602000
@RC00204 DS    0H                                                  0237 00603000
*                   IF FLGRTFUL='0'B THEN/* IF RANGE TABLE NOT FULL,    00604000
*                                      GET                           */ 00605000
@RC00198 TM    FLGRTFUL(R7),B'10000000'                            0237 00606000
         BNZ   @RF00237                                            0237 00607000
*                     R4=RBLINKB;   /* ADDRESS OF NEXT RB ON CHAIN   */ 00608000
