         N     @15,@CF03456                                        0310 00908000
         CR    @15,@12                                             0310 00909000
         BH    @RF00310                                            0310 00910000
*           ENDADDR=(R10->ENDADDR&  /* USE CALLER            @ZA29803*/ 00911000
*               ENDPART);           /* SPECIFIED END ADDRESS @ZA29803*/ 00912000
         ST    @15,ENDADDR(,@08)                                   0311 00913000
*         ELSE                                                     0312 00914000
*           ENDADDR=USEEND;         /* ENDING ADDRESS NOT IN STORAGE    00915000
*                                      AREA SO USE ENDING STORAGE  0312 00916000
*                                      AREA ADDRESS                  */ 00917000
         B     @RC00310                                            0312 00918000
@RF00310 L     @15,RANGEPTR(,R7)                                   0312 00919000
         L     @12,USEEND(,R7)                                     0312 00920000
         ST    @12,ENDADDR(,@15)                                   0312 00921000
*         CALL SDADDADR;            /* ADD RANGE TO ADDR RANGE TABLE */ 00922000
@RC00310 BAL   @14,SDADDADR                                        0313 00923000
*       END;                        /* END OF START ADDRESS IN     0314 00924000
*                                      STORAGE AREA                  */ 00925000
*     ELSE                          /* USER START ADDRESS IS NOT IN     00926000
*                                      STORAGE AREA                  */ 00927000
*       IF R10->STARTADR<USESTART&  /* IF PART OF RANGE IS IN        */ 00928000
*           (R10->ENDADDR&ENDPART)  /* THIS STORAGE AREA     @ZA29803*/ 00929000
*           >=USESTART THEN         /*                       @ZA29803*/ 00930000
