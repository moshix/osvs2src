         BNO   @RF00210                                            0210 00484000
*                             DO;                                  0211 00485000
*                                                                  0211 00486000
*/********************************************************************/ 00487000
*/*   THIS SEGMENT WILL BUILD AN ADDRESS RANGE FOR AN SVC WHOSE      */ 00488000
*/*   RBCDE1 FIELD IS ZERO.  PROGRAM MANAGER SUBROUTINES,            */ 00489000
*/*   IEAVVMSR AND IEAQCDSR, WILL BE USED TO LOCATE AN LPDE OR       */ 00490000
*/*   CDE FOR THE SVC WHOSE NAME IS CREATED FROM THE INTERRUPT       */ 00491000
*/*   CODE IN THE PREVIOUS RB.                                       */ 00492000
*/*                                                                  */ 00493000
*/*   INPUT: TRSCNTCB CONTAINS ADDRESS OF INPUT TCB, TCBCB           */ 00494000
*/*      CONTAINS ADDRESS OF INPUT RB, LPACB IS A WORK REGISTER USED */ 00495000
*/*      TO CONTAIN ADDRESS OF CDE OR LPDE, OR, IF NOT FOUND, ZERO.  */ 00496000
*/*                                                                  */ 00497000
*/*   OUTPUT: ADDRESS RANGE FOR SVC                                  */ 00498000
*/*                                                                  */ 00499000
*/*   INCLUDED BY: SDLPA                                             */ 00500000
*/********************************************************************/ 00501000
*                                                                  0212 00502000
*                               RFY                                0212 00503000
*                                 R11 RSTD;                        0212 00504000
*                               R11=RBLINKB->RBINTCOD;/* INTERRUPT 0213 00505000
*                                      CODE FROM PREVIOUS RB IS    0213 00506000
*                                      CURRENT RB SVC NUMBER         */ 00507000
         L     @12,RBLINKB-1(,R4)                                  0213 00508000
         LH    R11,RBINTCOD(,@12)                                  0213 00509000
         N     R11,@CF03610                                        0213 00510000
*                               GEN(CVD   R11,SVCNAME(R7));/* CONVERT   00511000
*                                      SVC NUMBER TO DEC             */ 00512000
         CVD   R11,SVCNAME(R7)                                          00513000
*                               UNPK(SVCNM6T8,SVCNM7T8);/* MAKE SVC     00514000
*                                      NUMBER PRINTABLE              */ 00515000
         UNPK  SVCNM6T8(3,R7),SVCNM7T8(2,R7)                       0215 00516000
*                               SVCNM8=SVCNM8|'F0'X;/* FIX LAST BYTE    00517000
*                                      OF NAME -                     */ 00518000
         OI    SVCNM8(R7),X'F0'                                    0216 00519000
*                               SVCNM8=SVCNM8&'CF'X;/* MAKE IT EBCDIC*/ 00520000
         NI    SVCNM8(R7),X'CF'                                    0217 00521000
*                               SVCNM1T5=STDSVCNM;/* ADD STANDARD  0218 00522000
*                                      PART OF NAME                  */ 00523000
         MVC   SVCNM1T5(5,R7),@CC03460                             0218 00524000
*                               RFY                                0219 00525000
*                                 R11 UNRSTD;                      0219 00526000
*                               R13=ADDR(SDWRGSV2);/* ADDR OF SVC  0220 00527000
*                                      DUMP SAVE AREA        @G33VPHD*/ 00528000
         LA    R13,SDWRGSV2(,R7)                                   0220 00529000
*                               GEN REFS(CVTQCDSR,CVTQLPAQ);       0221 00530000
         STM   R12,R10,0(R13)     SAVE ALL REG EXCEPT R11               00531000
*                                                                       00532000
*        INTERFACE TO IEAQCDSR - CMS LOCK HELD                          00533000
*        R0  - R0 -                    - FIRST 4 CHARS NAME             00534000
*        R1  - R1 -                    - LAST 4 CHARS NAME              00535000
*        R8  - R8 - ADDR OF QUEUE TO SEARCH                             00536000
*        R9  - R9  - ADDR OF ENTRY POINT NAME (SEARCH ARG)              00537000
*        R11 - R11 -                   - FOUND CDE ADDR OR ZERO         00538000
*        R14 - R14  - RET ADDR, +0 FOUND CDE, +4 NOT FOUND              00539000
*        R15 - R15 - ENTRY POINT ADDRESS OF IEAQCDSR RTN                00540000
*                                                                       00541000
         L     R3,FLCCVT          ADDRESS OF CVT                        00542000
         L     R8,CVTQLPAQ(R3) ADDR OF SYSTEM LPA CDE QUEUE             00543000
         LA    R9,SVCNAME(R7)  ADDRESS OF SVC NAME                      00544000
         L     R15,CVTQCDSR(R3) ADDR OF IEAQCDSR SEARCH RTN             00545000
         BALR  R14,R15        FIND ADDRESS OF CDE FOR SVC               00546000
         NOP   0                       RET POINT, CDE FOUND             00547000
         LM    R12,R10,0(R13)     RESTORE ALL REG EXCEPT R11            00548000
         LR    R5,R11          CDE ADDR OR 0 IN PROPER REG              00549000
*                               IF R5=NULL THEN/* IF A CDE WAS NOT 0222 00550000
*                                      FOUND,                        */ 00551000
         LTR   R5,R5                                               0222 00552000
         BNZ   @RF00222                                            0222 00553000
*                                 DO;/* LOOK FOR A LPDE              */ 00554000
*                                   RFY                            0224 00555000
*                                     R0 RSTD;                     0224 00556000
*                                   GEN REFS(CVTLPDSR);            0225 00557000
         STM   R1,R15,0(R13)      SAVE ALL REGS EXECT R0                00558000
*        INTERFACE TO IEAVVMSR                                          00559000
*        R0  - R0 - IN, FIRST 4 CHAR NAME - OUT, ADDR OF LPDE           00560000
*        R1  - R1 - IN, LAST 4 CHAR NAME                                00561000
*        R3  - R3  - IN, ADDRESS OF CVT                                 00562000
*        R6  - BASEREG1 - DESTROYED                                     00563000
*        R7  - R7 - DESTROYED                                           00564000
*        R8  - BASEREG3 - DESTROYED                                     00565000
*        R9  - BASEREG2 - DESTROYED                                     00566000
*        R14 - R14  - RET+0, LPDE FOUND     - RET+4, NOT FOUND          00567000
*        R15 - R15 - ENTRY POINT ADDRESS                                00568000
         LM    R0,R1,SVCNAME(R7)  NAME OF LPA SVC                       00569000
         L     R3,FLCCVT             ADDRESS OF CVT                     00570000
         L     R15,CVTLPDSR(R3) IEAVVMSR ENTRY POINT ADDRESS            00571000
         BALR  R14,R15           OBTAIN ADDRESS OF LPDE                 00572000
         NOP   0                          RET POINT, LPDE NOT FOUND     00573000
         LM    R1,R15,0(R13)         RET POINT, LPDE FOUND              00574000
*                                   R5=R0;/* ADDR OF LPDE IN PROPER     00575000
*                                      REG                           */ 00576000
         LR    R5,R0                                               0226 00577000
*                                   RFY                            0227 00578000
*                                     R0 UNRSTD;                   0227 00579000
