*   END USERNGES;                                                  0328 00985000
@EL00002 DS    0H                                                  0328 00986000
@EF00002 DS    0H                                                  0328 00987000
@ER00002 BR    @14                                                 0328 00988000
*                                                                  0329 00989000
*/********************************************************************/ 00990000
*/*   THIS SEGMENT WILL CONVERT SUBPOOLS TO ADDRESS RANGES AND       */ 00991000
*/*   CALL SDADDADR TO INSERT EACH ADDRESS RANGE INTO THE ADDRESS    */ 00992000
*/*   RANGE TABLE.  SUBPOOLS ARE DEFINED BY VSM CONTROL BLOCKS       */ 00993000
*/*   SPQE AND DQE.                                                  */ 00994000
*/*                                                                  */ 00995000
*/*   INPUT: SPQEADDR CONTAINS ADDRESS OF FIRST SPQE ON SPQE CHAIN   */ 00996000
*/*          TO SEARCH                                               */ 00997000
*/*                                                                  */ 00998000
*/*   OUTPUT: ADDRESS RANGES ADDED TO ADDRESS RANGE TABLE            */ 00999000
*/*                                                                  */ 01000000
*/*   CALLED BY: SDGLOBAL,SDLOCAL,SDTCB                              */ 01001000
*/********************************************************************/ 01002000
*                                                                  0329 01003000
*SDSPQE:                                                           0329 01004000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0329 01005000
SDSPQE   DS    0H                                                  0330 01006000
*   SPQERETA=R14;                   /* SAVE RETURN ADDRESS           */ 01007000
         ST    R14,SPQERETA(,R7)                                   0330 01008000
*   RFY                                                            0331 01009000
*    (R10,                                                         0331 01010000
*     R11,                                                         0331 01011000
*     R4,                                                          0331 01012000
*     R5) RSTD;                                                    0331 01013000
*   RFY                                                            0332 01014000
*     SPQESECT BASED(R4);                                          0332 01015000
*   RFY                                                            0333 01016000
*     DQESECT BASED(R5);                                           0333 01017000
*   R4=SPQEADDR;                    /* ADDR OF FIRST SPQE ON CHAIN   */ 01018000
         L     R4,SPQEADDR(,R7)                                    0334 01019000
*   DO WHILE R4^=NULL;              /* SEARCH ALL SPQE ON CHAIN      */ 01020000
         B     @DE00335                                            0335 01021000
@DL00335 DS    0H                                                  0336 01022000
*     IF SPSHARE='0'B THEN          /* IF SUBPOOL OWNED              */ 01023000
         TM    SPSHARE(R4),B'10000000'                             0336 01024000
         BNZ   @RF00336                                            0336 01025000
*       DO;                                                        0337 01026000
*         R5=SPDQEAD;               /* ADDR OF FIRST DQE OFF SPQE    */ 01027000
         L     R5,SPDQEAD(,R4)                                     0338 01028000
*         DO WHILE R5^=NULL;        /* SEARCH ALL DQE HUNG OFF SPQE  */ 01029000
         B     @DE00339                                            0339 01030000
@DL00339 DS    0H                                                  0340 01031000
*           STARTADR=DQEBLKAD;      /* START ADDRESS FOR RANGE       */ 01032000
         L     @15,RANGEPTR(,R7)                                   0340 01033000
         L     @12,DQEBLKAD(,R5)                                   0340 01034000
         ST    @12,STARTADR(,@15)                                  0340 01035000
*           ENDADDR=STARTADR+DQELNTH;/* END ADDRESS FOR RANGE        */ 01036000
         AL    @12,DQELNTH(,R5)                                    0341 01037000
         ST    @12,ENDADDR(,@15)                                   0341 01038000
*           CALL SDADDADR;          /* ADD RANGE TO ADDR RNGE TABLE  */ 01039000
         BAL   @14,SDADDADR                                        0342 01040000
*           IF FLGRTFUL='0'B THEN   /* IF RNGE TABLE NOT FULL,       */ 01041000
         TM    FLGRTFUL(R7),B'10000000'                            0343 01042000
         BNZ   @RF00343                                            0343 01043000
*             R5=DQEPTR;            /* ADDRESS OF NEXT DQE           */ 01044000
         L     R5,DQEPTR(,R5)                                      0344 01045000
*           ELSE                    /* RNGE TABLE FULL, END LOOP     */ 01046000
*             R5=NULL;              /* END DQE CHAIN SEARCH          */ 01047000
         B     @RC00343                                            0345 01048000
@RF00343 SLR   R5,R5                                               0345 01049000
*         END;                                                     0346 01050000
@RC00343 DS    0H                                                  0346 01051000
@DE00339 LTR   R5,R5                                               0346 01052000
         BNZ   @DL00339                                            0346 01053000
*         IF FLGRTFUL='0'B THEN     /* IF ADDR RNGE TABLE NOT FULL,  */ 01054000
         TM    FLGRTFUL(R7),B'10000000'                            0347 01055000
         BNZ   @RF00347                                            0347 01056000
*           R4=SPQEPTR;             /* ADDRESS OF NEXT SPQE          */ 01057000
         L     R4,SPQEPTR(,R4)                                     0348 01058000
*         ELSE                      /* RANGE TABLE FULL, END LOOP    */ 01059000
*           R4=NULL;                /* END SPQE CHAIN SEARCH         */ 01060000
         B     @RC00347                                            0349 01061000
@RF00347 SLR   R4,R4                                               0349 01062000
*       END;                                                       0350 01063000
*     ELSE                                                         0351 01064000
*       R4=SPQEPTR;                 /* SP NOT OWNED, GET NEXT SPQE   */ 01065000
         B     @RC00336                                            0351 01066000
@RF00336 L     R4,SPQEPTR(,R4)                                     0351 01067000
*   END;                                                           0352 01068000
@RC00336 DS    0H                                                  0352 01069000
@DE00335 LTR   R4,R4                                               0352 01070000
         BNZ   @DL00335                                            0352 01071000
*   RFY                                                            0353 01072000
*    (SPQESECT,                                                    0353 01073000
*     DQESECT) BASED;                                              0353 01074000
*   RFY                                                            0354 01075000
*    (R10,                                                         0354 01076000
*     R11,                                                         0354 01077000
*     R4,                                                          0354 01078000
*     R5) UNRSTD;                                                  0354 01079000
*   R14=SPQERETA;                   /* RESTORE RETURN ADDRESS        */ 01080000
         L     R14,SPQERETA(,R7)                                   0355 01081000
*   END SDSPQE;                                                    0356 01082000
@EL00003 DS    0H                                                  0356 01083000
@EF00003 DS    0H                                                  0356 01084000
@ER00003 BR    @14                                                 0356 01085000
*                                                                  0357 01086000
*/********************************************************************/ 01087000
*/*   THIS SEGMENT WILL ADD AN ADDRESS RANGE TO THE ADDRESS RANGE    */ 01088000
*/*   TABLE.  THE ADDRESSES TO BE ADDED ARE IN STARTADR AND          */ 01089000
*/*   ENDADDR (IN THE ADDRESS RANGE TABLE).  IF THEIR IS ROOM IN     */ 01090000
*/*   THE ADDRESS RANGE TABLE, THE START ADDRESS WILL BE ROUNDED     */ 01091000
*/*   DOWN TO A 4K BOUNDARY AND THE END ADDRESS WILL BE ROUNDED UP   */ 01092000
*/*   TO A 4K BOUNDARY.  IF THEIR IS NO MORE ROOM IN THE ADDRESS     */ 01093000
*/*   RANGE TABLE, A RETURN CODE OF PARTDUMP WILL BE STORED IN       */ 01094000
*/*   SDRETCOD.  IF THE START ADDRESS IS GREATER THAN THE END        */ 01095000
*/*   ADDRESS A C0D ABEND WILL BE ISSUED.                            */ 01096000
*/*                                                                  */ 01097000
*/*   INPUT: RANGEPTR CONTAINS ADDRESS OF NEXT AVAILABLE SLOT IN     */ 01098000
*/*      ADDRESS RANGE TABLE, ENDADRNT CONTAINS ADDRESS OF LAST SLOT */ 01099000
*/*      IN ADDRESS RANGE TABLE, STARTADR CONTAINS BEG ADDRESS OF    */ 01100000
*/*      RANGE, ENDADDR CONTAINS END ADDRESS OF RANGE                */ 01101000
*/*                                                                  */ 01102000
*/*   OUTPUT: RANGE ADDED TO ADDRESS RANGE TABLE, RANGEPTR           */ 01103000
*/*      CONTAINS ADDRESS OF NEXT AVAILABLE SLOT OR RETURN CODE OF   */ 01104000
*/*      PARTDUMP OR C0D ABEND                                       */ 01105000
*/*                                                                  */ 01106000
*/*   CALLED BY: USERNGES,SDSPQE,LPACDE,LPALPDE,SDLOCAL              */ 01107000
*/********************************************************************/ 01108000
*                                                                  0357 01109000
*SDADDADR:                                                         0357 01110000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0357 01111000
SDADDADR DS    0H                                                  0358 01112000
*   RFY                                                            0358 01113000
*    (R10,                                                         0358 01114000
*     R11,                                                         0358 01115000
*     R4,                                                          0358 01116000
*     R5) RSTD;                                                    0358 01117000
*   ADDADRWA=R10;                   /* SAVE WORK REGISTER            */ 01118000
         ST    R10,ADDADRWA(,R7)                                   0359 01119000
*   ADDADRWB=R11;                   /* SAVE WORK REGISTER            */ 01120000
         ST    R11,ADDADRWB(,R7)                                   0360 01121000
*   ADDADRWC=R4;                    /* SAVE WORK REGISTER            */ 01122000
         ST    R4,ADDADRWC(,R7)                                    0361 01123000
*   ADDADRWD=R5;                    /* SAVE WORK REGISTER            */ 01124000
         ST    R5,ADDADRWD(,R7)                                    0362 01125000
*   RFY                                                            0363 01126000
*    (R10,                                                         0363 01127000
*     R11,                                                         0363 01128000
*     R4,                                                          0363 01129000
*     R5) UNRSTD;                                                  0363 01130000
*   IF RANGEPTR<(ENDADRNT-LEN1ADRN) THEN/* IF RNGE TABLE NOT FULL    */ 01131000
         L     @15,RANGEPTR(,R7)                                   0364 01132000
         L     @12,ENDADRNT(,R7)                                   0364 01133000
         SL    @12,@CF00059                                        0364 01134000
         CR    @15,@12                                             0364 01135000
         BNL   @RF00364                                            0364 01136000
*     DO;                                                          0365 01137000
*       STARTADR=STARTADR&FOURKBDY; /* ROUND START ADDR DOWN-4K BDY  */ 01138000
         L     @12,@CF03465                                        0366 01139000
         L     @08,STARTADR(,@15)                                  0366 01140000
         NR    @08,@12                                             0366 01141000
         ST    @08,STARTADR(,@15)                                  0366 01142000
*       ENDADDR=ENDADDR&HIGHBIT0;   /* END ADDRESS IS AN ADDRESS     */ 01143000
         L     @08,ENDADDR(,@15)                                   0367 01144000
         N     @08,@CF03456                                        0367 01145000
         ST    @08,ENDADDR(,@15)                                   0367 01146000
*       IF(ENDADDR//FOURK)^=NULL THEN/* IF END ADDR NOT ON 4K BDY,   */ 01147000
         LR    @04,@08                                             0368 01148000
         SRDA  @04,32                                              0368 01149000
         D     @04,@CF03418                                        0368 01150000
         LTR   @04,@04                                             0368 01151000
         BZ    @RF00368                                            0368 01152000
*         ENDADDR=(ENDADDR+FOURK)&FOURKBDY;/* ROUND UP TO 4K BDY     */ 01153000
         AL    @08,@CF03418                                        0369 01154000
         NR    @08,@12                                             0369 01155000
         ST    @08,ENDADDR(,@15)                                   0369 01156000
*       ELSE                                                       0370 01157000
*         ;                                                        0370 01158000
@RF00368 DS    0H                                                  0371 01159000
*       IF STARTADR>ENDADDR THEN    /* ERROR IF START > END          */ 01160000
         L     @15,RANGEPTR(,R7)                                   0371 01161000
         CLC   STARTADR(4,@15),ENDADDR(@15)                        0371 01162000
         BNH   @RF00371                                            0371 01163000
*         GEN(ABEND X'C0D',DUMP,,SYSTEM);                          0372 01164000
         ABEND X'C0D',DUMP,,SYSTEM                                      01165000
*       ELSE                                                       0373 01166000
*         RANGEPTR=RANGEPTR+LEN1ADRN;/* ADDR OF NEXT SLOT IN TAB     */ 01167000
         B     @RC00371                                            0373 01168000
@RF00371 LA    @15,8                                               0373 01169000
         AL    @15,RANGEPTR(,R7)                                   0373 01170000
         ST    @15,RANGEPTR(,R7)                                   0373 01171000
*     END;                                                         0374 01172000
*   ELSE                            /* IF ADDRESS RANGE TABLE FULL,  */ 01173000
*     DO;                                                          0375 01174000
         B     @RC00364                                            0375 01175000
@RF00364 DS    0H                                                  0376 01176000
*       FLGRTFUL='1'B;              /* IND ADDRESS RANGE TABLE FULL  */ 01177000
         OI    FLGRTFUL(R7),B'10000000'                            0376 01178000
*       SDRETCOD=PARTDUMP;          /* INDICATE PERMANENT PARTIAL  0377 01179000
*                                      DUMP                          */ 01180000
         MVC   SDRETCOD(4,R7),@CF00053                             0377 01181000
*     END;                                                         0378 01182000
*   RFY                                                            0379 01183000
*    (R10,                                                         0379 01184000
*     R11,                                                         0379 01185000
*     R4,                                                          0379 01186000
*     R5) RSTD;                                                    0379 01187000
@RC00364 DS    0H                                                  0380 01188000
*   R10=ADDADRWA;                   /* RESTORE WORK REGISTER         */ 01189000
