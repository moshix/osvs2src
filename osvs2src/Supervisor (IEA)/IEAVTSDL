         TITLE 'IEAVTSDL - SVC DUMP LOCAL STORAGE PROCESSOR            *00001000
                        '                                               00002000
IEAVTSDL CSECT ,                                                   0001 00003000
@MAINENT BALR  @15,0                                               0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVTSDL  78.066'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @09,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@09                                         0001 00013000
RBPRFXLN EQU 64                                                         00014000
RBPRFLNA EQU 32                                                         00015000
PRBLEN   EQU 136                                                        00016000
SIRBLEN  EQU 200                                                        00017000
TIRBLEN  EQU 136                                                        00018000
IRBLEN   EQU 128                                                        00019000
SVRBLEN  EQU 224                                                        00020000
*                                                            @G33VPHD*/ 00021000
*   ERRRETRY=ADDR(RTYENDL);         /* NEXT RETRY LOCATION ADDRESS   */ 00022000
         LA    @12,RTYENDL                                         0099 00023000
         ST    @12,ERRRETRY(,R3)                                   0099 00024000
*   ERRBASE1=R9;                    /* SAVE BASE REGISTER FOR      0100 00025000
*                                      RECOVERY                      */ 00026000
         ST    R9,ERRBASE1(,R3)                                    0100 00027000
*   SVCDMPNM=CSECTNAM;              /* SAVE CSECT NAME FOR RECOVERY  */ 00028000
         MVC   SVCDMPNM(8,R7),CSECTNAM                             0101 00029000
*   IF ERRENDMP='0'B THEN           /* IF NO ERRORS SO FAR           */ 00030000
         TM    ERRENDMP(R3),B'01000000'                            0102 00031000
         BNZ   @RF00102                                            0102 00032000
*     DO;                                                          0103 00033000
*       GEN REFS(PSALITA);                                         0104 00034000
*                            /*OBTAIN LOCK LOCK AND SET UP FOR EXIT  */ 00035000
GETLOCKH SETLOCK   OBTAIN,TYPE=LOCAL,MODE=UNCOND,                      X00036000
               RELATED=(SVCDUMP,IEAVTSDL(FRELOCKH))                     00037000
*       ERRLLOCK='1'B;              /* INDICATE LOCAL LOCK IS HELD   */ 00038000
*       RFY                                                        0106 00039000
*        (R10,                                                     0106 00040000
*         R11) RSTD;                                               0106 00041000
*       ERRFRR='1'B;                /* INDICATE FRR PROTECTION ACTIVE*/ 00042000
         OI    ERRLLOCK(R3),B'10000010'                            0107 00043000
*       GEN(SETFRR A,FRRAD=ADDRFRR,WRKREGS=(R10,R11),PARMAD=(R3))  0108 00044000
*       REFS(ADDRFRR,PSACSTK,FRRSCURR,FRRSLAST,FRRS,FRRSPARM,PSALSFCC,  00045000
*           PSA,FRRSFRRA,FRRSENTR);                                0108 00046000
         SETFRR A,FRRAD=ADDRFRR,WRKREGS=(R10,R11),PARMAD=(R3)           00047000
*       RFY                                                        0109 00048000
*        (R10,                                                     0109 00049000
*         R11) RSTD;                                               0109 00050000
*       ERRWORK=ERRORWK;            /* SAVE STATUS FOR FRR           */ 00051000
         MVC   ERRWORK(24,R3),ERRORWK(R7)                          0110 00052000
*/* SET UP AND REUSE THE ADDRESS RANGE TABLE                         */ 00053000
*                                                                  0111 00054000
*       IF SDUABEND='1'B THEN       /* IF THIS IS A SYSMDUMP REQUEST */ 00055000
         L     @12,SDWSDPL(,R7)                                    0111 00056000
         TM    SDUABEND(@12),B'01000000'                           0111 00057000
         BNO   @RF00111                                            0111 00058000
*         RANGEPTR=SDUSYSMS;        /* USE ADDRESS OF ADDRESS RANGE     00059000
*                                      TABLE SUPPLIED IN PARAMETER 0112 00060000
*                                      LIST BY SYSMDUMP              */ 00061000
         L     @12,SDUSYSMS(,@12)                                  0112 00062000
         ST    @12,RANGEPTR(,R7)                                   0112 00063000
*       ELSE                                                       0113 00064000
*         RANGEPTR=CVTSDBF&HIGHBIT0;/* IF THIS IS NOT A SYSMDUMP   0113 00065000
*                                      REQUEST RANGE TABLE IS BUILT     00066000
*                                      IN THE 4K SQA BUFFER          */ 00067000
         B     @RC00111                                            0113 00068000
@RF00111 L     @12,CVTPTR                                          0113 00069000
         L     @12,CVTSDBF(,@12)                                   0113 00070000
         N     @12,@CF03456                                        0113 00071000
         ST    @12,RANGEPTR(,R7)                                   0113 00072000
*       RFY                                                        0114 00073000
*         STORANGE BASED(RANGEPTR); /* MAP ADDRESS RANGE TABLE       */ 00074000
@RC00111 DS    0H                                                  0115 00075000
*       STARTADR=NULL;              /* FIRST ADDRESS RANGE IS ZEROS  */ 00076000
         SLR   @12,@12                                             0115 00077000
         L     @08,RANGEPTR(,R7)                                   0115 00078000
         ST    @12,STARTADR(,@08)                                  0115 00079000
*       ENDADDR=NULL;               /* FIRST ADDRESS RANGE IS ZEROS  */ 00080000
         ST    @12,ENDADDR(,@08)                                   0116 00081000
*       LASTRNGE='1'B;              /* FIRST RANGE IS A DUMMY        */ 00082000
         OI    LASTRNGE(@08),B'10000000'                           0117 00083000
*       ENDADRNT=RANGEPTR+FOURK-LEN1ADRN;/* GET END ADDRESS OF THE 0118 00084000
*                                      ADDRESS RANGE TABLE           */ 00085000
         LR    @12,@08                                             0118 00086000
         SL    @12,@CF03603                                        0118 00087000
         ST    @12,ENDADRNT(,R7)                                   0118 00088000
*       RANGEPTR=RANGEPTR+LEN1ADRN; /* GET ADDRESS OF 1ST USABLE   0119 00089000
*                                      RANGE                         */ 00090000
         AL    @08,@CF00059                                        0119 00091000
         ST    @08,RANGEPTR(,R7)                                   0119 00092000
*       LOCALADR=RANGEPTR;          /* BEGINNING OF LOCAL PART OF THE   00093000
*                                      ADDRESS RANGE TABLE           */ 00094000
         ST    @08,LOCALADR(,R7)                                   0120 00095000
*/********************************************************************/ 00096000
*/*   THIS SEGMENT WILL BUILD THE LOCAL PART OF THE ADDRESS RANGE    */ 00097000
*/*   TABLE.  THE LOCAL PART OF THE ADDRESS RANGE TABLE IS BUILT     */ 00098000
*/*   FROM USER SUPPLIED STORAGE RANGES WHICH LIE IN THE PRIVATE     */ 00099000
*/*   AREA OF THE ADDRESS SPACE AND THE FOLLOWING SDATA OPTIONS:     */ 00100000
*/*   LSQA, RGN, SWA, AND LPA FOR THE REQUESTED ADDRESS SPACE.       */ 00101000
*/*                                                                  */ 00102000
*/*   INPUT: SDUMP PARM LIST, SDATA OPTIONS, LOCALADR CONTAINS       */ 00103000
*/*         ADDRESS OF BEGINNING OF LOCAL PART OF ADDRESS RANGE TABLE*/ 00104000
*/*                                                                  */ 00105000
*/*   OUTPUT: LOCAL PART OF ADDRESS RANGE TABLE BUILT AND RETURN     */ 00106000
*/*           CODE, PARTDUMP (ADDRESS RANGE TABLE FULL) OR NULL      */ 00107000
*/********************************************************************/ 00108000
*                                                                  0121 00109000
*       ERRRETRY=ADDR(RTYLOC10);    /* NEXT RETRY LOCATION ADDRESS   */ 00110000
*                                                                  0121 00111000
         LA    @12,RTYLOC10                                        0121 00112000
         ST    @12,ERRRETRY(,R3)                                   0121 00113000
*       /*************************************************************/ 00114000
*       /*                                                           */ 00115000
*       /* PUT CALLER - SUPPLIED STORAGE RANGES WHICH BEGIN IN THE   */ 00116000
*       /* LOCAL STORAGE AREA IN THE ADDRESS RANGE TABLE             */ 00117000
*       /*                                                           */ 00118000
*       /*************************************************************/ 00119000
*                                                                  0122 00120000
*       IF SDUSTOR='1'B THEN        /* IF USER SUPPLIED STOR RNGES   */ 00121000
         L     @12,SDWSDPL(,R7)                                    0122 00122000
         TM    SDUSTOR(@12),B'00100000'                            0122 00123000
         BNO   @RF00122                                            0122 00124000
*         DO;                                                      0123 00125000
*           USESTART=PASTRT;        /* LOW LIMIT FOR USER RANGES     */ 00126000
         L     @12,CVTPTR                                          0124 00127000
         L     @12,GDAPTR(,@12)                                    0124 00128000
         L     @08,PASTRT(,@12)                                    0124 00129000
         ST    @08,USESTART(,R7)                                   0124 00130000
*           USEEND=USESTART+PASIZE; /* END LIMIT FOR USER RANGES     */ 00131000
         AL    @08,PASIZE(,@12)                                    0125 00132000
         ST    @08,USEEND(,R7)                                     0125 00133000
*           CALL USERNGES;          /* ADD USER RANGES WHICH START IN   00134000
*                                      THE PRIVATE AREA OF THE     0126 00135000
*                                      ADDRESS SPACE                 */ 00136000
         BAL   @14,USERNGES                                        0126 00137000
*         END;                                                     0127 00138000
*RTYLOC10:                                                         0128 00139000
*       ERRRETRY=ADDR(RTYLOC20);    /* NEXT RETRY LOCATION ADDRESS   */ 00140000
*                                                                  0128 00141000
@RF00122 DS    0H                                                  0128 00142000
RTYLOC10 LA    @12,RTYLOC20                                        0128 00143000
         ST    @12,ERRRETRY(,R3)                                   0128 00144000
*       /*************************************************************/ 00145000
*       /*                                                           */ 00146000
*       /* IF REQUESTED PUT STORAGE RANGES FOR THE PRIVATE AREA IN   */ 00147000
*       /* THE TABLE                                                 */ 00148000
*       /*                                                           */ 00149000
*       /*************************************************************/ 00150000
*                                                                  0129 00151000
*       IF FLGRTFUL='0'B&           /* IF RANGE TABLE NOT FULL AND   */ 00152000
*           SVCDRGN='1'B THEN       /* RGN REQUESTED                 */ 00153000
         TM    FLGRTFUL(R7),B'10000000'                            0129 00154000
         BNZ   @RF00129                                            0129 00155000
         TM    SVCDRGN(R7),B'00000100'                             0129 00156000
         BNO   @RF00129                                            0129 00157000
*         DO;                                                      0130 00158000
*           STARTADR=PASTRT;        /* START ADDR OF PRIVATE AREA    */ 00159000
         L     @12,RANGEPTR(,R7)                                   0131 00160000
         L     @08,CVTPTR                                          0131 00161000
         L     @08,GDAPTR(,@08)                                    0131 00162000
         L     @06,PASTRT(,@08)                                    0131 00163000
         ST    @06,STARTADR(,@12)                                  0131 00164000
*           ENDADDR=STARTADR+PASIZE;/* END ADDR OF PRIVATE AREA      */ 00165000
         AL    @06,PASIZE(,@08)                                    0132 00166000
         ST    @06,ENDADDR(,@12)                                   0132 00167000
*           CALL SDADDADR;          /* ADD RGN RANGE TO ADDR TABLE   */ 00168000
         BAL   @14,SDADDADR                                        0133 00169000
*           RFY                                                    0134 00170000
*             TCB BASED(ASXBFTCB);  /* ADDRESS OF TOP TCB            */ 00171000
*           RFY                                                    0135 00172000
*             TCBFIX BASED(ASXBFTCB-32);/* ADDRESS OF TCB PREFIX     */ 00173000
*           RFY                                                    0136 00174000
*             PQESECT BASED(TCBPQE);/* ADDRESS OF V=R DUMMY PQE-8    */ 00175000
*           IF FLGRTFUL='0'B&       /* IF RNGE TABLE NOT FULL AND    */ 00176000
*               TCBRV='1'B THEN     /* RGN IS VIRTUAL EQUAL REAL     */ 00177000
         TM    FLGRTFUL(R7),B'10000000'                            0137 00178000
         BNZ   @RF00137                                            0137 00179000
         L     @12,ASCBPTR                                         0137 00180000
         L     @12,ASXBPTR(,@12)                                   0137 00181000
         L     @12,ASXBFTCB(,@12)                                  0137 00182000
         TM    TCBRV(@12),B'10000000'                              0137 00183000
         BNO   @RF00137                                            0137 00184000
*             DO;                                                  0138 00185000
*               STARTADR=PQEREGN;   /* BEG ADDR OF V=R REGION        */ 00186000
         L     @08,RANGEPTR(,R7)                                   0139 00187000
         L     @12,TCBPQE(,@12)                                    0139 00188000
         L     @06,PQEREGN(,@12)                                   0139 00189000
         ST    @06,STARTADR(,@08)                                  0139 00190000
*               ENDADDR=PQEREGN+PQESIZE;/* END ADDR OF V=R REGION    */ 00191000
         AL    @06,PQESIZE(,@12)                                   0140 00192000
         ST    @06,ENDADDR(,@08)                                   0140 00193000
*               CALL SDADDADR;      /* ADD V=R RGN EXTENTS           */ 00194000
         BAL   @14,SDADDADR                                        0141 00195000
*             END;                                                 0142 00196000
*           ELSE                                                   0143 00197000
*             ;                                                    0143 00198000
@RF00137 DS    0H                                                  0144 00199000
*           RFY                                                    0144 00200000
*             TCB BASED;                                           0144 00201000
*           RFY                                                    0145 00202000
*             TCBFIX BASED;                                        0145 00203000
*           RFY                                                    0146 00204000
*             PQESECT BASED;                                       0146 00205000
*         END;                                                     0147 00206000
*       ELSE                                                       0148 00207000
*         DO;                                                      0148 00208000
*                                                                  0148 00209000
         B     @RC00129                                            0148 00210000
@RF00129 DS    0H                                                  0149 00211000
*           /*********************************************************/ 00212000
*           /*                                                       */ 00213000
*           /* IF REGION NOT REQUESTED AND LSQA IS, ADD LSQA RANGES  */ 00214000
*           /* TO THE ADDRESS RANGE TABLE                            */ 00215000
*           /*                                                       */ 00216000
*           /*********************************************************/ 00217000
*                                                                  0149 00218000
*           IF FLGRTFUL='0'B&       /* IF RANGE TABLE NOT FULL AND   */ 00219000
*               SVCDLSQA='1'B THEN  /* LSQA IS REQUESTED             */ 00220000
         TM    FLGRTFUL(R7),B'10000000'                            0149 00221000
         BNZ   @RF00149                                            0149 00222000
         TM    SVCDLSQA(R7),B'00001000'                            0149 00223000
         BNO   @RF00149                                            0149 00224000
*             DO;                                                  0150 00225000
*               SPQEADDR=LSQAPTR;   /* ADDR OF FIRST LSQA SPQE       */ 00226000
         L     @12,ASCBPTR                                         0151 00227000
         L     @12,LDAPTR(,@12)                                    0151 00228000
         L     @12,LSQAPTR(,@12)                                   0151 00229000
         ST    @12,SPQEADDR(,R7)                                   0151 00230000
*               CALL SDSPQE;        /* ADD LSQA RANGES TO TABLE      */ 00231000
         BAL   @14,SDSPQE                                          0152 00232000
*             END;                                                 0153 00233000
*           ELSE                                                   0154 00234000
*             ;                                                    0154 00235000
@RF00149 DS    0H                                                  0155 00236000
*         END;                                                     0155 00237000
*RTYLOC20:                                                         0156 00238000
*       ERRRETRY=ADDR(RTYLOC30);    /* NEXT RETRY LOCATION ADDRESS   */ 00239000
@RC00129 DS    0H                                                  0156 00240000
RTYLOC20 LA    @12,RTYLOC30                                        0156 00241000
         ST    @12,ERRRETRY(,R3)                                   0156 00242000
*       IF FLGRTFUL='0'B&           /* IF RNGE TABLE NOT FULL AND    */ 00243000
*           (SVCDLPA='1'B|          /* LPA WAS REQUESTED OR          */ 00244000
*           (SVCDRGN='0'B&          /* RGN WAS NOT REQUESTED BUT     */ 00245000
*           (SVCDLSQA='1'B|         /* LSQA OR                       */ 00246000
*           SVCDSWA='1'B))) THEN    /* SWA WAS,                      */ 00247000
         TM    FLGRTFUL(R7),B'10000000'                            0157 00248000
         BNZ   @RF00157                                            0157 00249000
         TM    SVCDLPA(R7),B'00000010'                             0157 00250000
         BO    @RT00157                                            0157 00251000
         TM    SVCDRGN(R7),B'00000100'                             0157 00252000
         BNZ   @RF00157                                            0157 00253000
         TM    SVCDLSQA(R7),B'00001000'                            0157 00254000
         BO    @RT00157                                            0157 00255000
         TM    SVCDSWA(R7),B'01000000'                             0157 00256000
         BNO   @RF00157                                            0157 00257000
@RT00157 DS    0H                                                  0158 00258000
*         DO;                                                      0158 00259000
*                                                                  0158 00260000
*/********************************************************************/ 00261000
*/*   THIS SEGMENT WILL ADD ADDRESS RANGES TO THE ADDRESS RANGE      */ 00262000
*/*   TABLE FOR REQUESTED SDATA OPTIONS THAT ARE TCB RELATED -       */ 00263000
*/*   LPA, SWA AND LSQA SUBPOOLS 229,230.  THE ADDRESS OF THE TOP    */ 00264000
*/*   TCB IN THE ADDRESS SPACE IS OBTAINED FROM THE ASXB(ASXBFTCB)   */ 00265000
*/*   EACH TCB IN THE FAMILY IS OBTAINED BY USE OF THE TREE SCAN     */ 00266000
*/*   ROUTINE OF STATUS(IEATRSCN).                                   */ 00267000
*/*                                                                  */ 00268000
*/*   INPUT: SDATA DUMP OPTIONS                                      */ 00269000
*/*                                                                  */ 00270000
*/*   OUTPUT: ADDRESS RANGES FOR TCB RELATED SDATA OPTIONS AND       */ 00271000
*/*           RETCODE EITHER NULL OR PARTDUMP IN SDRETCOD            */ 00272000
*/********************************************************************/ 00273000
*                                                                  0159 00274000
*           RFY                                                    0159 00275000
*             R10 RSTD;                                            0159 00276000
*           R10=ASXBFTCB;           /* TOP TCB IN ADDR SPACE ADDR    */ 00277000
         L     @12,ASCBPTR                                         0160 00278000
         L     @12,ASXBPTR(,@12)                                   0160 00279000
         L     R10,ASXBFTCB(,@12)                                  0160 00280000
*           RFY                                                    0161 00281000
*             TCB BASED(R10);                                      0161 00282000
*           RFY                                                    0162 00283000
*             TCBFIX BASED(R10-32);                                0162 00284000
*                                                                  0162 00285000
*           /*********************************************************/ 00286000
*           /*                                                       */ 00287000
*           /* SEARCH ALL TCB'S IN THE ADDRESS SPACE. ADD RANGES TO  */ 00288000
*           /* THE TO THE ADDRESS RANGE TABLE DESCRIBING STORAGE     */ 00289000
*           /* ASSOCIATED WITH THE TCBS                              */ 00290000
*           /*                                                       */ 00291000
*           /*********************************************************/ 00292000
*                                                                  0163 00293000
*           DO WHILE(R10^=NULL);    /* SCAN ALL TCBS IN ADDR SPACE   */ 00294000
         B     @DE00163                                            0163 00295000
@DL00163 DS    0H                                                  0164 00296000
*             IF FLGRTFUL='0'B&     /* IF RANGE TABLE NOT FULL AND   */ 00297000
*                 SVCDRGN='0'B&     /* RGN WAS NOT SPECIFIED BUT     */ 00298000
*                 SVCDLSQA='1'B THEN/* LSQA WAS,                     */ 00299000
         TM    FLGRTFUL(R7),B'10000000'                            0164 00300000
         BNZ   @RF00164                                            0164 00301000
         TM    SVCDLSQA(R7),B'00001000'                            0164 00302000
         BNO   @RF00164                                            0164 00303000
         TM    SVCDRGN(R7),B'00000100'                             0164 00304000
         BNZ   @RF00164                                            0164 00305000
*               DO;                                                0165 00306000
*                 SPQEADDR=TCBUKYSP;/* FIRST SPQE ADDR-S.P. 229,230  */ 00307000
         L     @12,TCBUKYSP(,R10)                                  0166 00308000
         ST    @12,SPQEADDR(,R7)                                   0166 00309000
*                 CALL SDSPQE;      /* ADD RANGES TO ADDR RNGE TAB   */ 00310000
         BAL   @14,SDSPQE                                          0167 00311000
*               END;                                               0168 00312000
*             IF FLGRTFUL='0'B&     /* IF RANGE TABLE NOT FULL AND   */ 00313000
*                 SVCDRGN='0'B&     /* RGN WAS NOT SPECIFIED BUT     */ 00314000
*                 SVCDSWA='1'B THEN /* SWA WAS,                      */ 00315000
@RF00164 TM    FLGRTFUL(R7),B'10000000'                            0169 00316000
         BNZ   @RF00169                                            0169 00317000
         TM    SVCDRGN(R7),B'00000100'                             0169 00318000
         BNZ   @RF00169                                            0169 00319000
         TM    SVCDSWA(R7),B'01000000'                             0169 00320000
         BNO   @RF00169                                            0169 00321000
*               DO;                                                0170 00322000
*                 SPQEADDR=TCBSWA;  /* ADDR FIRST SPQE FOR SWA       */ 00323000
         L     @12,TCBSWA(,R10)                                    0171 00324000
         ST    @12,SPQEADDR(,R7)                                   0171 00325000
*                 CALL SDSPQE;      /* ADD SWA RANGES TO TABLE       */ 00326000
         BAL   @14,SDSPQE                                          0172 00327000
*               END;                                               0173 00328000
*             ELSE                                                 0174 00329000
*               ;                                                  0174 00330000
@RF00169 DS    0H                                                  0175 00331000
*             IF FLGRTFUL='0'B&     /* IF RANGE TABLE NOT FULL AND   */ 00332000
*                 SVCDLPA='1'B THEN /* LPA WAS SPECIFIED,            */ 00333000
         TM    FLGRTFUL(R7),B'10000000'                            0175 00334000
         BNZ   @RF00175                                            0175 00335000
         TM    SVCDLPA(R7),B'00000010'                             0175 00336000
         BNO   @RF00175                                            0175 00337000
*               DO;                                                0176 00338000
*                 GEN REFS(PSALITA);                               0177 00339000
*                                      /*SERIALIZE GLOBAL LPDE, CDE  */ 00340000
GETLOCKL SETLOCK   OBTAIN,TYPE=CMS,MODE=UNCOND,                        ,00341000
               RELATED=(LPDE/CDE,IEAVAD00(FRELOCKL,FRELOCKP,FRELOCKQ))  00342000
*                 ERRCMSLK='1'B;    /* INDICATE CMS LOCK HELD        */ 00343000
         OI    ERRCMSLK(R3),B'00001000'                            0178 00344000
*/********************************************************************/ 00345000
*/*   THIS SEGMENT PROCESSES THE LPA SDATA OPTION.  AN ADDRESS       */ 00346000
*/*   RANGE WILL BE CREATED FOR EACH MODULE OR SVC THAT RESIDES      */ 00347000
*/*   IN THE LPA AND IS ASSOCIATED WITH THE INPUT TCB BY A LLE,      */ 00348000
*/*   PRB OR SVRB.                                                   */ 00349000
*/*   A MODULE OR SVC, HAVING A CDE, IS IN THE LPA IF -              */ 00350000
*/*   (CDNIP=1 | (CDNIC=0 & CDJPA=0 & CDSPZ=0)).                     */ 00351000
*/*   A AN SVC HAVE AN LPDE IS IN THE LPA. WHAT A SVC HAS,           */ 00352000
*/*   CDE/LPDE, IS DETERMINED BY THE RBTRSVRB FLAG AND RBCDE1        */ 00353000
*/*   FIELD -                                                        */ 00354000
*/*   (RBTRSVRB=0 & RBCDE1=0)  - SVC NOT IN LPA                      */ 00355000
*/*   (RBTRSVRB=0 & RBCDE1^=0) - IN LPA, RBCDE1 CONTAINS CDE ADDR    */ 00356000
*/*   (RBTRSVRB=1 & RBCDE1=0)  - INDETERMINE, USE PGM MGR SUBRTNS    */ 00357000
*/*   (RBTRSVRB=1 & RBCDE1^=0) - IN LPA, RBCDE1 CONTAINS LPDE ADDR   */ 00358000
*/*   SEE COMMENTS ON RBTRSVRB FLAG IN RB MAPPING MACRO.             */ 00359000
*/*                                                                  */ 00360000
*/*   INPUT: TRSCNTCB CONTAINS ADDRESS OF INPUT TCB                  */ 00361000
*/*                                                                  */ 00362000
*/*   OUTPUT: ADDRESS RANGES FOR LPA MODULE AND SVCS                 */ 00363000
*/*                                                                  */ 00364000
*/*   INCLUDED BY: SDTCB                                             */ 00365000
*/********************************************************************/ 00366000
*                                                                  0179 00367000
*                 RFY                                              0179 00368000
*                  (R4,                                            0179 00369000
*                   R5) RSTD;                                      0179 00370000
*                 R4=TCBLLS;        /* ADDRESS OF FIRST LLE OFF TCB  */ 00371000
         L     R4,TCBLLS(,R10)                                     0180 00372000
*                 RFY                                              0181 00373000
*                   LLE BASED(R4);                                 0181 00374000
*                 DO WHILE(R4^=NULL);/* SCAN ALL LOADED MODULES      */ 00375000
         B     @DE00182                                            0182 00376000
@DL00182 DS    0H                                                  0183 00377000
*                   R5=LLECDPT;     /* ADDRESS OF CDE FOR MODULE     */ 00378000
         L     R5,LLECDPT(,R4)                                     0183 00379000
*                   CALL LPACDE;    /* IF LPA MOD, BUILD ADDR RANGE  */ 00380000
         BAL   @14,LPACDE                                          0184 00381000
*                   IF FLGRTFUL='0'B THEN/* IF RANGE TABLE ^FULL,  0185 00382000
*                                      OBTAIN                        */ 00383000
         TM    FLGRTFUL(R7),B'10000000'                            0185 00384000
         BNZ   @RF00185                                            0185 00385000
*                     R4=LLECHN;    /* ADDRESS OF NEXT LLE           */ 00386000
         L     R4,LLECHN(,R4)                                      0186 00387000
*                   ELSE                                           0187 00388000
*                     R4=NULL;      /* IF RANGE TABLE FULL-END LOOP  */ 00389000
         B     @RC00185                                            0187 00390000
@RF00185 SLR   R4,R4                                               0187 00391000
*                 END;                                             0188 00392000
@RC00185 DS    0H                                                  0188 00393000
@DE00182 LTR   R4,R4                                               0188 00394000
         BNZ   @DL00182                                            0188 00395000
*                 RFY                                              0189 00396000
*                   LLE BASED;                                     0189 00397000
*                 IF TCBFC='0'B THEN/* IF TCB ACTIVE (NOT COMPLETE)  */ 00398000
         TM    TCBFC(R10),B'10000000'                              0190 00399000
