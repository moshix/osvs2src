         BNZ   @RF00198                                            0198 00432000
         TM    RBTRSVRB(R4),B'00010000'                            0198 00433000
         BNZ   @RF00198                                            0198 00434000
@RT00198 DS    0H                                                  0199 00435000
*                     DO;                                          0199 00436000
*                       R5=RBCDE1;  /* ADDRESS OF CDE                */ 00437000
         L     R5,RBCDE1-1(,R4)                                    0200 00438000
         LA    R5,0(,R5)                                           0200 00439000
*                       CALL LPACDE;/* IF IN LPA, BUILD ADDR RANGE   */ 00440000
         BAL   @14,LPACDE                                          0201 00441000
*                     END;                                         0202 00442000
*                   ELSE                                           0203 00443000
*                     DO;                                          0203 00444000
         B     @RC00198                                            0203 00445000
@RF00198 DS    0H                                                  0204 00446000
*                       IF RBCDE1^=NULL&/* IF A LPDE EXISTS AND CODE    00447000
*                                      IS                            */ 00448000
*                           (RBFTP='110'B&RBTRSVRB='1'B) THEN/* ^ 1ST   00449000
*                                      LOAD SVC                      */ 00450000
