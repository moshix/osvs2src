         BR    @14                                                 0318 01193000
SDRMERRX DS    0H                                                  0319 01194000
*   RFY                                                            0319 01195000
*     R14 RSTD;                                                    0319 01196000
*   RFY                                                            0320 01197000
*     R10 RSTD;                     /* WORKA WILL POINT TO WK AREA 0320 01198000
*                                      FOR RES MGR - RMPLRMWA        */ 01199000
*   RFY                                                            0321 01200000
*     SDRMWORK BASED(R10);          /* MAP ERROR WORK AREA   @G33VPHD*/ 01201000
*   DO;                             /* ENTESTAE (R5)ERREXIT(ERRXPERC)   01202000
*                                      PARMREG(R10)                  */ 01203000
*                                                                  0322 01204000
*     /***************************************************************/ 01205000
*     /*                                                             */ 01206000
*     /* ENTRY CODE FOR ESTAE ROUTINE. IF RTCA ALLOCATED THEN RESTORE*/ 01207000
*     /* CODE AND DATA REGS FROM START OF USERS PARM LIST, AND       */ 01208000
*     /* ESTABLISH ADDR TO ESTAE SAVE AREA IF REQUIRED. IF NO RTCA   */ 01209000
*     /* AVAILABLE THEN BRANCH TO ERROR CODE AFTER ESTABLISHING      */ 01210000
*     /* ADDRESSABILITY                                              */ 01211000
*     /*                                                             */ 01212000
*     /***************************************************************/ 01213000
*                                                                  0323 01214000
*     GEN(USING *,GPR15P);                                         0323 01215000
         USING *,GPR15P                                                 01216000
*     GEN(CH    GPR00F,*+18);       /* CHECK IF RTCA ALLOC FAILED    */ 01217000
         CH    GPR00F,*+18                                              01218000
*     GEN(BNE   *+16);              /* NO,SAVE RTM REGS              */ 01219000
         BNE   *+16                                                     01220000
*     GEN(DROP  GPR15P);            /* DROP LOCAL ADDR               */ 01221000
         DROP  GPR15P                                                   01222000
*     GEN(LR    R10,GPR02P);        /* PTR TO USER PARM LIST         */ 01223000
         LR    R10,GPR02P                                               01224000
*     GEN(L     R5,00(R10));                                       0328 01225000
         L     R5,00(R10)                                               01226000
*     GEN(B     ERRXPERC);          /* BRANCH TO ERROR EXIT          */ 01227000
         B     ERRXPERC                                                 01228000
*     GEN(DC    H'12');             /* NO RTCA AVAIL RC              */ 01229000
         DC    H'12'                                                    01230000
*     GEN(STM   GPR14P,GPR12F,12(GPR13P));/* GOOD R13, SAVE RTM'S  0331 01231000
*                                      REGS                          */ 01232000
         STM   GPR14P,GPR12F,12(GPR13P)                                 01233000
*     GEN(L     R10,0(GPR01P));     /* PTR TO USER PARM LIST         */ 01234000
         L     R10,0(GPR01P)                                            01235000
*     GEN(L     R5,00(R10));                                       0333 01236000
         L     R5,00(R10)                                               01237000
*   END;                            /* ENTESTAE (R5)ERREXIT(ERRXPERC)   01238000
*                                      PARMREG(R10)                  */ 01239000
*   RFY                                                            0335 01240000
*     SDWA BASED(R1);                                              0335 01241000
*   SDWARCRD='1'B;                  /* REQUEST RECORDING             */ 01242000
         OI    SDWARCRD(R1),B'10000000'                            0336 01243000
*   SDWACSCT=CSECTNAM;              /* CSECT NAME                    */ 01244000
         MVC   SDWACSCT(8,R1),@CC03004                             0337 01245000
*   SDWAMODN=LMODNAME;              /* LOAD MODULE NAME              */ 01246000
