         IC    @12,RTCTINDX(,@15)                                  0130 00372000
         SLA   @12,2                                               0130 00373000
         ALR   @15,@12                                             0130 00374000
         OI    RTCTSDDO-4(@15),B'00000001'                         0130 00375000
*         END;                      /* END FIND CURRENT ENTRY AND  0131 00376000
*                                      MARK IT COMPLETE      @G33VPHD*/ 00377000
*                                                                  0132 00378000
*       /*************************************************************/ 00379000
*       /*                                                           */ 00380000
*       /* THIS SECTION IS EXECUTED FOR ALL SRBS. THIS TASKS OF THE  */ 00381000
*       /* ADDRESS SPACE ARE SET DISPATCHABLE AND THE CALLERS ECB IS */ 00382000
*       /* POSTED IF ONE WAS SPECIFIED AND THIS IS THE LAST ACTIVE   */ 00383000
*       /* ADDRESS SPACE OF THIS DUMP REQUEST                @G33VPHD*/ 00384000
*       /*                                                           */ 00385000
*       /*************************************************************/ 00386000
*                                                                  0132 00387000
*       IF RTCTSDAN(RTCTINDX)='1'B THEN/* IF TASKS SET NON DISP    0132 00388000
*                                                            @G33VPHD*/ 00389000
