         BE    @DC00128                                            0128 00352000
*           END;                    /* END OF FIND CURRENT TABLE   0129 00353000
*                                      ENTRY                 @G33VPHD*/ 00354000
         L     @12,CVTPTR                                          0129 00355000
         L     @12,CVTRTMCT(,@12)                                  0129 00356000
         LA    @15,1                                               0129 00357000
         SLR   @04,@04                                             0129 00358000
         IC    @04,RTCTINDX(,@12)                                  0129 00359000
         ALR   @15,@04                                             0129 00360000
         L     @12,CVTPTR                                          0129 00361000
         L     @12,CVTRTMCT(,@12)                                  0129 00362000
         STC   @15,RTCTINDX(,@12)                                  0129 00363000
         C     @15,@CF00141                                        0129 00364000
         BNH   @DL00128                                            0129 00365000
@DC00128 DS    0H                                                  0130 00366000
*           RTCTSDDO(RTCTINDX)='1'B;/* MARK CURRENT RTCT ASID ENTRY     00367000
*                                      COMPLETE              @G33VPHD*/ 00368000
         L     @15,CVTPTR                                          0130 00369000
         L     @15,CVTRTMCT(,@15)                                  0130 00370000
