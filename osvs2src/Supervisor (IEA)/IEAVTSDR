*                                                            @G33VPHD*/ 01149000
*SDRMRTY9:                                                         0308 01150000
*               ;                   /* RETRY LOCATION                */ 01151000
@RF00272 DS    0H                                                  0308 01152000
SDRMRTY9 DS    0H                                                  0309 01153000
*             END;                  /* END.... THIS ASID INVLOVED IN    01154000
*                                      ACTIVE DUMP           @ZA27048*/ 01155000
*           R13=ERRSAVE;            /* RESTORE SAVE AREA ADDRESS   0310 01156000
*                                                            @ZA27048*/ 01157000
@RF00219 L     @15,RMPLRMWA(,R8)                                   0310 01158000
         L     R13,ERRSAVE(,@15)                                   0310 01159000
*           GEN(ESTAE 0);           /* DELETE THE ESTAE ERROR EXIT   */ 01160000
         ESTAE 0                                                        01161000
*         END;                      /* END.... SVC DUMP IN PROGRESS     01162000
*                                                            @G33VPHD*/ 01163000
*       RFY                                                        0313 01164000
*         RMPL BASED;               /* RMPL IS NOT ADDRESSABLE IF  0313 01165000
*                                      RETRY TO THIS LOCATION FROM 0313 01166000
