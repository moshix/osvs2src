*       SDWAUPRG='1'B;              /* PRIME REGS ON RETRY           */ 01259000
         OI    SDWAUPRG(R1),B'00001000'                            0344 01260000
*       SDWASR05=R5;                /* PRIME BASE REGISTER           */ 01261000
         ST    R5,SDWASR05(,R1)                                    0345 01262000
*       SDWASR13=ERRSAVE;           /* PRIME CALLERS SAVE AREA ADDR     01263000
*                                                            @G33VPHD*/ 01264000
         L     @15,ERRSAVE(,R10)                                   0346 01265000
