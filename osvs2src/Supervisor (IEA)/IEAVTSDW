         AL    @02,PSEUDOAD(,@12)                                  0162 00527000
         ST    @02,PSEUDOAD(,@12)                                  0162 00528000
*       END;                                                       0163 00529000
*   END;                            /* OF LOOPING THROUGH THE REAL 0164 00530000
