*   PRDKEY2=0;                      /* INIT DUMP RECORD HEADER       */ 00103000
         MVI   PRDKEY2(@12),X'00'                                  0088 00104000
*   PRDASID=SMDPASID;               /* USE THE SUMMARY DUMP PSEUDO 0089 00105000
*                                      ASID TO PUT THE DATA INTO THE    00106000
*                                      DUMP IN A FORMAT COMPATIBLE 0089 00107000
*                                      WITH THE REST OF THE DUMP, BUT   00108000
*                                      UNIQUELY IDENTIFIED           */ 00109000
         MVC   PRDASID(2,@12),@CB01372                             0089 00110000
*   PSEUDOAD=PASTRT;                /* BEGIN THE PSEUDO ADDRESSES FOR   00111000
*                                      THE PSEUDO ADDRESS SPACE THAT    00112000
*                                      WILL CONTAIN THE SUMMARY DUMP    00113000
*                                      DATA AT THE START OF THE    0090 00114000
*                                      PRIVATE AREA SO THAT AMDPRDMP    00115000
*                                      WILL NOT CONFUSE IT WITH    0090 00116000
*                                      COMMON STORAGE                */ 00117000
*                                                                  0090 00118000
