*       RFY                                                        0098 00171000
*        (GPR11P,                                                  0098 00172000
*         GPR12P,                                                  0098 00173000
*         GPR13P,                                                  0098 00174000
*         GPR14P) UNRSTD;                                          0098 00175000
*     END;                          /* SETLOCK (OBTAIN) MODE(UNCOND)    00176000
*                                      TYPE(SALLOC)                0099 00177000
*                                      RELATED(IEAVTSDW(FL))         */ 00178000
*     RFY                                                          0100 00179000
*       RETCODE UNRSTD;                                            0100 00180000
*     RFY                                                          0101 00181000
*      (FWR1,                                                      0101 00182000
*       FWR2) RSTD;                 /* THIS FRR WILL SUPERCEDE THE 0101 00183000
*                                      ESTAE WHICH IS THE CURRENT  0101 00184000
*                                      RECOVERY ENVIRONMENT. THE   0101 00185000
*                                      ESTAE WILL REMAIN AS COVERAGE    00186000
*                                      FOR AFTER THE FRR IS DELETED.    00187000
*                                      THE BASE FOR THE ERROR RTN  0101 00188000
*                                      PARMLIST WILL BE ALTERED TO 0101 00189000
