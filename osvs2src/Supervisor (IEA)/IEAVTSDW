         ST    @12,PSEUDOAD(,@02)                                  0133 00396000
*           END;                                                   0134 00397000
*         ELSE                      /* IF IT IS OFF                  */ 00398000
*           DO;                     /* THEN SHOW THE CURRENT FRAME 0135 00399000
*                                      RETURNED                      */ 00400000
         B     @RC00130                                            0135 00401000
@RF00130 DS    0H                                                  0136 00402000
*             SMWKFRMA(INDEX)=0;    /* CLEAR THE ENTRY IN THE TABLE  */ 00403000
         LR    @12,INDEX                                           0136 00404000
         SLA   @12,2                                               0136 00405000
         LA    @02,196                                             0136 00406000
         ALR   @02,SMWKBASE                                        0136 00407000
         SLR   @15,@15                                             0136 00408000
         ST    @15,SMWKFRMA-4(@12,@02)                             0136 00409000
*             SMWKFRHD=SMWKFRHD-1;  /* DECREMENT THE COUNT OF FRAMES    00410000
*                                      HELD                          */ 00411000
         SLR   @12,@12                                             0137 00412000
         ICM   @12,3,SMWKFRHD(@02)                                 0137 00413000
         BCTR  @12,0                                               0137 00414000
         STH   @12,SMWKFRHD(,@02)                                  0137 00415000
*             SMWKNFRI=SMWKNFRI+1;  /* INCR THE COUNT OF THE NUMBER     00416000
*                                      OF REAL STORAGE FRAMES      0138 00417000
*                                      RELEASED SINCE THE LAST TIME     00418000
*                                      THE DIE RTN IN IEAVTSDX HAS 0138 00419000
*                                      RUN. THIS WILL PREVENT IT FROM   00420000
*                                      THINKING NOTHING IS GOING ON     00421000
*                                      AND THEN FREEING FRAMES ON ITS   00422000
*                                      OWN.                          */ 00423000
         LA    @12,1                                               0138 00424000
         AH    @12,SMWKNFRI(,SMWKBASE)                             0138 00425000
         STH   @12,SMWKNFRI(,SMWKBASE)                             0138 00426000
*           END;                                                   0139 00427000
*       END;                        /* OF PROCESSING AN ENTRY IN THE    00428000
*                                      REAL STORAGE BUFFER LIST      */ 00429000
@RC00130 DS    0H                                                  0141 00430000
*     ERR6WPTR=ADDR(ERRORWK);       /* CHANGE THE ERROR RTN PARMLIST    00431000
*                                      BASE TO POINT BACK TO THE   0141 00432000
*                                      ESTAE PARMLIST. THE FRR     0141 00433000
*                                      PARMLIST IS NOT COPIED INTO IT   00434000
*                                      BECAUSE THE ONLY CHANGE MADE     00435000
*                                      WAS TO ERRSLOCK AND THAT IS 0141 00436000
*                                      ABOUT TO BE UNDONE            */ 00437000
