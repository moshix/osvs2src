*                                      PARMLIST THAT THE FRR IS BEING   00193000
*                                      USED                          */ 00194000
         OI    ERRFRR(ERR6WPTR),B'10000000'                        0102 00195000
*     GEN SETS(FWR1,FWR2,ERR6WPTR) REFS(ADDRFRR,PSACSTK,FRRSCURR,FRRS,  00196000
*         FRRSLAST,PSALSFCC,PSA,FRRSFRRA,FRRSENTR,FRRSPARM) CODE   0103 00197000
*(SETFRR A,FRRAD=ADDRFRR,WRKREGS=(FWR1,FWR2),PARMAD=(ERR6WPTR));   0103 00198000
         SETFRR A,FRRAD=ADDRFRR,WRKREGS=(FWR1,FWR2),PARMAD=(ERR6WPTR)   00199000
*     RFY                                                          0104 00200000
*      (FWR1,                                                      0104 00201000
*       FWR2) UNRSTD;                                              0104 00202000
*     ERRWORK=ERRORWK;              /* COPY THE ESTAE PARMLIST INTO     00203000
*                                      THE THE FRR PARMLIST WHICH IS    00204000
*                                      NOW SUPERCEDING IT.           */ 00205000
         MVC   ERRWORK(24,ERR6WPTR),ERRORWK(SDWORKP)               0105 00206000
*     ERRSLOCK=ON;                  /* SET THE FLAG THAT SHOWS THE 0106 00207000
*                                      SALLOC LOCK IS HELD           */ 00208000
         OI    ERRSLOCK(ERR6WPTR),B'00000001'                      0106 00209000
*                                                                  0107 00210000
*     /***************************************************************/ 00211000
*     /*                                                             */ 00212000
*     /* LOCATE THE NEXT REAL STORAGE BUFFER FRAME TO BE PROCESSED   */ 00213000
*     /* AND COPY THE CONTENTS TO THE OUTPUT BUFFER. THIS INVOLVES   */ 00214000
*     /* TURNING DAT OFF BECAUSE THE REAL STORAGE BUFFER FRAME       */ 00215000
*     /* CURRENTLY HAS NO VIRTUAL PAGE ASSIGNED TO IT.               */ 00216000
*     /*                                                             */ 00217000
*     /***************************************************************/ 00218000
*                                                                  0107 00219000
*     IF SMWKFRMA(INDEX)^=0         /* PROCESS THIS ENTRY IN THE LIST   00220000
*                                      IF IT STILL POINTS TO A FRAME */ 00221000
*         &SMWKFRHD>0               /* AND IF THE LAST REAL STORAGE     00222000
*                                      BUFFER FRAME HAS NOT BEEN   0107 00223000
*                                      PROCESSED. (IF SUM DUMP     0107 00224000
*                                      PREVIOUSLY FAILED, OR IF THE     00225000
*                                      REAL STORAGE HAS BEEN STOLEN     00226000
*                                      BACK OR THE DIE HAS RETURNED     00227000
*                                      IT, THIS COUNT WILL ALREADY BE   00228000
*                                      0)                            */ 00229000
*       THEN                        /* YES THIS FRAME CAN BE       0107 00230000
*                                      PROCESSED                     */ 00231000
         LR    @12,INDEX                                           0107 00232000
         SLA   @12,2                                               0107 00233000
         LA    @02,196                                             0107 00234000
