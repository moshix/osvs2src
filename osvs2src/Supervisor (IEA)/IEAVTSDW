*         FROMLEN=PAGESIZE;         /* SET UP LENGTH FOR MOVE        */ 00250000
         LR    FROMLEN,@02                                         0111 00251000
*         FROMADDR=SMWKFRMA(INDEX); /* COPY FROM THE REAL STORAGE  0112 00252000
*                                      FRAME POINTED TO BY THIS ENTRY*/ 00253000
