*         TOADDR=ADDR(PRDDATA);     /* INTO THE OUTPUT BUFFER        */ 00255000
         L     TOADDR,SDSMBUFF(,SDWORKP)                           0113 00256000
         LA    TOADDR,PRDDATA(,TOADDR)                             0113 00257000
*                                                                  0114 00258000
*         /***********************************************************/ 00259000
*         /*                                                         */ 00260000
*         /* COPY FROM REAL TO VIRTUAL                               */ 00261000
*         /*                                                         */ 00262000
*         /***********************************************************/ 00263000
*                                                                  0114 00264000
*         SMWKPSW=''B;              /* INIT THE PSW THAT WILL BE USED   00265000
*                                      TO SHIFT FROM DAT ON TO OFF 0114 00266000
*                                      AND BACK ON AGAIN             */ 00267000
         XC    SMWKPSW(8,SMWKBASE),SMWKPSW(SMWKBASE)               0114 00268000
*         SMWKPSWK='0C'X;           /* SET UP KEY 0, EC MODE, AND  0115 00269000
*                                      ENABLED FOR MACH CHK          */ 00270000
         MVI   SMWKPSWK(SMWKBASE),X'0C'                            0115 00271000
*         GEN SETS(SMWKPSWS,SMWKBASE,SMWKPSWA,TOADDR,TOLEN,WORKREG,     00272000
*             FROMLEN) REFS(SMWKPSW,FROMADDR) CODE;                0116 00273000
         STOSM SMWKPSWS(SMWKBASE),X'00' USE THE CURRENT SYSTEM MASK     00274000
         LRA   TOADDR,0(TOADDR)   CONVERT THE OUTPUT BUFFER ADDRESS     00275000
*                                 FROM VIRTUAL TO REAL FOR DAT OFF      00276000
         NI    SMWKPSWS(SMWKBASE),X'FB' TURN OFF DAT IN THE MODEL PSW   00277000
*                                 NOTE: DAT WILL BE ON FOR THE FRR      00278000
*                                 EVEN IF AN ERROR OCCURS HERE          00279000
         LRA   WORKREG,DATOFF     GET THE REAL ADDRESS OF THE DAT OFF   00280000
*                                 CODE BEGINNING                        00281000
         ST    WORKREG,SMWKPSWA(SMWKBASE) AND STICK IT IN THE MODEL     00282000
*                                 PSW SO THAT IT WILL GET CONTROL       00283000
*                                 WHEN DAT IS TURNED OFF                00284000
         LA    WORKREG,DATON      WHILE DAT IS STILL WORKING, SAVE      00285000
*                                 THE VIRTUAL ADDRESS OF THE DAT ON     00286000
*                                 CODE THAT IS TO FOLLOW THE DAT OFF    00287000
*                                 CODE                                  00288000
         LPSW  SMWKPSW(SMWKBASE)  TURN DAT OFF                          00289000
DATOFF   EQU   *                                                        00290000
*  THIS CODE EXECUTES WITH DAT OFF TO ALLOW ACCESS TO REAL STORAGE      00291000
*  WHICH HAS NO VIRTUAL ADDRESS ASSIGNED TO IT.                         00292000
         MVCL  TOADDR,FROMADDR    MOVE THE DATA AS PREVIOUSLY SET UP    00293000
         LRA   SMWKBASE,0(SMWKBASE) CONVERT THE SUMDUMP WORK AREA BASE  00294000
*                                 FOR USE WITH DAT OFF                  00295000
         OI    SMWKPSWS(SMWKBASE),X'04' TURN ON DAT IN THE MODEL PSW    00296000
         ST    WORKREG,SMWKPSWA(SMWKBASE) CHANGE THE MODEL PSW ADDRESS  00297000
*                                 TO THE VIRTUAL ADDRESS OF THE CODE    00298000
*                                 FOLLOWING THE DAT ON GROUP            00299000
         LPSW  SMWKPSW(SMWKBASE)  TURN DAT ON                           00300000
DATON    EQU   *                                                        00301000
*   THIS IS THE END OF THE CODE THAT EXECUTES WITH DAT OFF              00302000
*         RFY                                                      0117 00303000
*          (TOADDR,                                                0117 00304000
*           TOLEN,                                                 0117 00305000
*           FROMADDR,                                              0117 00306000
*           FROMLEN) UNRSTD;                                       0117 00307000
*         SMWKBASE=RTCTSDSW;        /* RESTORE THE SUMDUMP WORK AREA    00308000
*                                      BASE TO A VIRTUAL VERSION     */ 00309000
         L     @12,CVTPTR                                          0118 00310000
         L     @12,CVTRTMCT(,@12)                                  0118 00311000
         L     SMWKBASE,RTCTSDSW(,@12)                             0118 00312000
*                                                                  0119 00313000
*         /***********************************************************/ 00314000
*         /*                                                         */ 00315000
*         /* CHECK TO SEE IF THERE HAS BEEN AN ERROR PROCESSING THE  */ 00316000
*         /* DUMP. IF THERE HAS THEN CEASE TRYING TO WRITE DATA INTO */ 00317000
*         /* THE DUMP DATASET AND JUST FREE ALL THE REAL STORAGE     */ 00318000
*         /* BUFFER FRAMES                                           */ 00319000
*         /*                                                         */ 00320000
*         /***********************************************************/ 00321000
*                                                                  0119 00322000
*         IF ERRENDMP=ON            /* CHECK THE TERMINATE DUE TO  0119 00323000
*                                      ERROR FLAG                    */ 00324000
*             |SMWKFRHD=1           /* AND THE FRAMES HELD COUNT     */ 00325000
*           THEN                    /* IF AN ERROR HAS OCCURED OR  0119 00326000
*                                      THIS IS THE LAST TIME RSM WILL   00327000
*                                      BE CALLED TO FREE FRAMES,   0119 00328000
*                                      INDICATE THAT RSM SHOULD FREE    00329000
*                                      ALL THE FRAME IT THINKS WE  0119 00330000
*                                      HAVE                          */ 00331000
         TM    ERRENDMP(ERR6WPTR),B'01000000'                      0119 00332000
         BO    @RT00119                                            0119 00333000
         CLC   SMWKFRHD+196(2,SMWKBASE),@CH00063                   0119 00334000
         BNE   @RF00119                                            0119 00335000
@RT00119 DS    0H                                                  0120 00336000
*           SMWKEVRY=ON;            /* INDICATE TO RSM THAT ALL THE     00337000
*                                      FRAMES ARE BEING RETURNED     */ 00338000
         OI    SMWKEVRY+196(SMWKBASE),B'10000000'                  0120 00339000
*         ELSE                      /* IF NO TERMINATING ERROR       */ 00340000
*           DO;                                                    0121 00341000
         B     @RC00119                                            0121 00342000
@RF00119 DS    0H                                                  0122 00343000
*             SMWKEVRY=OFF;         /* INDICATE TO RSM NOT TO FREE 0122 00344000
*                                      THE WHOLE REAL STORAGE BUFFER,   00345000
*                                      SO ONLY ONE FRAME WILL BE   0122 00346000
*                                      FREED                         */ 00347000
         LA    @12,196                                             0122 00348000
         ALR   @12,SMWKBASE                                        0122 00349000
         NI    SMWKEVRY(@12),B'01111111'                           0122 00350000
*             SMWKFRPT=SMWKFRMA(INDEX);/* SHOW RSM WHICH FRAME IS  0123 00351000
*                                      BEING RETURNED                */ 00352000
         LR    @02,INDEX                                           0123 00353000
         SLA   @02,2                                               0123 00354000
         L     @02,SMWKFRMA-4(@02,@12)                             0123 00355000
         ST    @02,SMWKFRPT(,@12)                                  0123 00356000
*           END;                                                   0124 00357000
*                                                                  0124 00358000
*         /***********************************************************/ 00359000
*         /*                                                         */ 00360000
*         /* CALL RSM TO RETURN THE REAL STORAGE BUFFER FRAME AND    */ 00361000
*         /* THEN DROP THE LOCK                                      */ 00362000
*         /*                                                         */ 00363000
*         /***********************************************************/ 00364000
*                                                                  0125 00365000
*         RFY                                                      0125 00366000
*           R1 RSTD;                /* RSM INTERFACE                 */ 00367000
@RC00119 DS    0H                                                  0126 00368000
*         R1=ADDR(SMWKRSCB);        /* PUT THE RSM PARMLIST ADDR INTO   00369000
*                                      THE INTERFACE REG             */ 00370000
         LA    @12,196                                             0126 00371000
         ALR   @12,SMWKBASE                                        0126 00372000
         LR    R1,@12                                              0126 00373000
*         R13=ADDR(SDWRGSV2);       /* PROVIDE A SAVE AREA           */ 00374000
         LA    R13,SDWRGSV2(,SDWORKP)                              0127 00375000
*         CALL IEAVPRSR;            /* RSM FRAME RETURN RTN WHICH  0128 00376000
*                                      DOES NOT EXPECT A SAVEAREA    */ 00377000
         L     @15,SMWKPRSR(,@12)                                  0128 00378000
