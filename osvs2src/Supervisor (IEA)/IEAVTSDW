         L     SMWKBASE,RTCTSDSW(,@12)                             0063 00028000
*   RFY                                                            0064 00029000
*     SMWK BASED(SMWKBASE);                                        0064 00030000
*   SVCDMPNM='IEAVTSDW';            /* IDENTIFY THIS MODULE, TO THE     00031000
*                                      ERROR ROUNTINE, AS IN CONTROL */ 00032000
         MVC   SVCDMPNM(8,SDWORKP),@CC01758                        0065 00033000
*   ERRBASE1=BASEREG;               /* PUT THE BASE REG IN THE ERROR    00034000
*                                      RTN PARMLIST TO BE RESTORED IF   00035000
*                                      THERE IS A RETRY              */ 00036000
         ST    BASEREG,ERRBASE1(,ERR6WPTR)                         0066 00037000
*   ERRADDR=ADDR(RETRYPT);          /* AND THEN SET UP THE RETRY   0067 00038000
*                                      ADDRESS                       */ 00039000
*                                                                  0067 00040000
         LA    @12,RETRYPT                                         0067 00041000
