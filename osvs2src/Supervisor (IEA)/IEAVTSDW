*         RFY                                                      0129 00380000
*           R1 UNRSTD;                                             0129 00381000
*         IF ERRENDMP=ON            /* CHECK TO SEE IF THIS IS ERROR    00382000
*                                      PROCESSING                    */ 00383000
*           THEN                    /* IF IT IS ALL THE FRAMES WERE     00384000
*                                      RETURNED TO RSM AND NO MORE 0130 00385000
*                                      SUMDUMP OUTPUT IS DESIRED     */ 00386000
         TM    ERRENDMP(ERR6WPTR),B'01000000'                      0130 00387000
         BNO   @RF00130                                            0130 00388000
*           DO;                                                    0131 00389000
*             SMWKFRHD=0;           /* SO SHOW NO FRAMES HELD        */ 00390000
