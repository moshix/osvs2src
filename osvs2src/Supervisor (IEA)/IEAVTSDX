         B     @RC00513                                            0528 01880000
@RF00513 DS    0H                                                  0529 01881000
*       SCHFRRRT=SDWARTYA;          /* SAVE RTMS SAVED RETURN ADDRESS   01882000
*                                                            @ZM43830*/ 01883000
         L     @12,SDWARTYA(,R1)                                   0529 01884000
         ST    @12,SCHFRRRT(,R4)                                   0529 01885000
*       SDWARTYA=NULL;              /* INDICATE NO RETRY TO RTM    0530 01886000
*                                                            @G33VPHD*/ 01887000
