         LR    @05,R12                                             0568 02088000
         SLA   @05,2                                               0568 02089000
         ALR   @08,@05                                             0568 02090000
         OI    RTCTSDNC-4(@08),B'01000000'                         0568 02091000
*         END;                      /* END SET UP INDEX      @ZM44937*/ 02092000
*       ELSE                                                       0570 02093000
*         DO;                       /* NOT GLOBAL AREA SRB SO DO   0570 02094000
*                                      CURRENT ENTRY LOOP    @ZM44937*/ 02095000
         B     @RC00565                                            0570 02096000
@RF00565 DS    0H                                                  0571 02097000
*           R12=ONE;                /* SET INDEX FOR SEARCH  @G33VPHD*/ 02098000
         LA    R12,1                                               0571 02099000
*           DO WHILE(SCHFRRND=OFF&R12<=DIM(RTCTSDAS));/* SEARCH ALL     02100000
