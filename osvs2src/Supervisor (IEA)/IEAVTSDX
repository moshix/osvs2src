*               END;                /* END OF FOUND TABLE ENTRY    0577 02124000
*                                                            @G33VPHD*/ 02125000
*             ELSE                                                 0578 02126000
*               R12=R12+ONE;        /* MOVE TO NEXT ENTRY    @G33VPHD*/ 02127000
