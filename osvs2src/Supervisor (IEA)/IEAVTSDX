@RF00546 DS    0H                                                  0549 01961000
*       R14=SCHFRRRT;               /* RETORE RTMS SAVE RETURN     0549 01962000
*                                      ADDRESS               @ZM43803*/ 01963000
@RC00534 L     R14,SCHFRRRT(,R4)                                   0549 01964000
*     END;                          /* END OF CANT RETRY SO PERCOLATE   01965000
*                                                            @G33VPHD*/ 01966000
*   END SCHFRR;                                                    0551 01967000
@EL00003 DS    0H                                                  0551 01968000
@EF00003 DS    0H                                                  0551 01969000
@ER00003 BR    @14                                                 0551 01970000
*                                                                  0552 01971000
*/* ******************************************************************/ 01972000
*/*    INTERNAL PROCEDURE NAME: SCHSRB                               */ 01973000
*/*                                                                  */ 01974000
*/*    THIS SEGMENT IS THE SRB SCHEDULED BY SCHEDULE DUMP MAINLINE.  */ 01975000
*/*    IT WILL ESTABLISH A RECOVERY EXIT, AND OBTAIN THE LOCAL       */ 01976000
*/*    LOCK.  IF THIS SRB IS EXECUTING IN THE MASTER ADDRESS SPACE   */ 01977000
*/*    AND A SUMMARY DUMP IS REQUESTED OR SQA, CSA AND QUIESCE=YES   */ 01978000
*/*    ARE REQUESTED, THE SYSTEM IS SET NONDISPATCHABLE.  IN ALL     */ 01979000
*/*    CASES ALL OF THE TASKS IN THE ADDRESS SPACE EXCEPT COMM TASK  */ 01980000
*/*    RCT, AND DSS ARE SET NONDISPATCHABLE.  IF A SUMMARY DUMP IS   */ 01981000
*/*    TO BE WRITTEN A TIMER DIE EXIT INTERVAL IS ESTABLISHED USING  */ 01982000
*/*    SETDIE. FINALLY THE LOCAL LOCK IS RELEASED AND THE DUMP       */ 01983000
*/*    TASK IN THIS ADDRESS SPACE IS RESUMED.  CONTROL DOES NOT      */ 01984000
*/*    RETURN FROM RESUME.  CONTROL IS PASSED DIRECTLY TO THE DUMP   */ 01985000
*/*    TASK.                                                         */ 01986000
*/*                                                                  */ 01987000
*/*    INPUT: REG0 - ADDRESS OF SRB                                  */ 01988000
*/*           REG1 - CONTENTS OF SRBPARM FIELD(BASEREG)              */ 01989000
*/*           REG14- RETURN ADDRESS                                  */ 01990000
*/*           REG15- ENTRY POINT ADDRESS                             */ 01991000
*/*    SVC DUMP IS LOCKED, SDUMP PARM LIST IS IN SCHPARML, ADDRESS   */ 01992000
*/*    OF SDUMP PARM LIST IS IN RTCTSDPL                             */ 01993000
*/*                                                                  */ 01994000
*/*    OUTPUT: DUMP TASK IS RESUMED                                  */ 01995000
*/* ******************************************************************/ 01996000
*                                                                  0552 01997000
*SCHSRB:                                                           0552 01998000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0552 01999000
SCHSRB   DS    0H                                                  0553 02000000
*   R9=R1;                          /* RESTORE BASE REGISTER         */ 02001000
         LR    R9,R1                                               0553 02002000
*   R3=R9+4095;                     /* RESTORE SECOND BASE REGISTER     02003000
*                                                            @ZM45082*/ 02004000
         LA    R3,4095                                             0554 02005000
         ALR   R3,R9                                               0554 02006000
*   RFY                                                            0555 02007000
*    (R10,                                                         0555 02008000
*     R11) RSTD;                                                   0555 02009000
*   IF CVTSDRM^=NULL THEN           /* IEAVTSDR FOUND DURING NIP     */ 02010000
         L     @12,CVTPTR                                          0556 02011000
         L     @12,CVTSDRM(,@12)                                   0556 02012000
         LTR   @12,@12                                             0556 02013000
         BZ    @RF00556                                            0556 02014000
*     R15=CVTSDRM-LENPREFX;         /* FRR FOR SRB IS IN IEAVTSDR    */ 02015000
         SL    @12,@CF01311                                        0557 02016000
         LR    R15,@12                                             0557 02017000
*   ELSE                                                           0558 02018000
*     R15=ADDR(CVTBRET);            /* FRR IS A BR 14 INSTRUCTION    */ 02019000
         B     @RC00556                                            0558 02020000
@RF00556 L     R15,CVTPTR                                          0558 02021000
         LA    R15,CVTBRET(,R15)                                   0558 02022000
*                                                                  0559 02023000
*   /*****************************************************************/ 02024000
*   /*                                                               */ 02025000
*   /* GET THE LOCAL LOCK AND ESTABLISH A RECOVERY EXIT      @G33VPHD*/ 02026000
*   /*                                                               */ 02027000
*   /*****************************************************************/ 02028000
*                                                                  0559 02029000
*   GEN REFS(PSALITA,PSA,PSACSTK,FRRS,FRRSCURR,FRRSLAST,PSALSFCC,  0559 02030000
*       FRRSFRRA,FRRSENTR);                                        0559 02031000
@RC00556 DS    0H                                                  0559 02032000
*                                /*SET UP IEAVTSDR           @G33VPHD*/ 02033000
GETLOCK  SETLOCK   OBTAIN,TYPE=LOCAL,MODE=UNCOND,                      X02034000
               RELATED=(SCHSDUMP,IEAVTSDX(FREELOCK))                    02035000
         SETFRR    A,FRRAD=(R15),WRKREGS=(R10,R11),                    X02036000
               PARMAD=(R4)                                              02037000
*   RFY                                                            0560 02038000
*    (R10,                                                         0560 02039000
*     R11) UNRSTD;                  /*                       @G33VPHD*/ 02040000
*   SCHFRRBR=R9;                    /* SAVE BASE REGISTER FOR      0561 02041000
*                                      RECOVERY              @G33VPHD*/ 02042000
*                                                                  0561 02043000
         ST    R9,SCHFRRBR(,R4)                                    0561 02044000
*   /*****************************************************************/ 02045000
*   /*                                                               */ 02046000
*   /* CHECK ASCBDUMP FOR ZERO INDICATING THIS ADDRESS SPACE IS IN   */ 02047000
*   /* THE PROCESS OF TERMINATING. IF ASCBDUMP IS ZERO CLEAN UP AND  */ 02048000
*   /* EXIT.... DON'T DO ANY DUMP PROCESSING.                @ZA27930*/ 02049000
*   /*                                                               */ 02050000
*   /*****************************************************************/ 02051000
*                                                                  0562 02052000
*   IF ASCBDUMP^=NULL THEN          /* IF DUMP TASK STILL OKAY     0562 02053000
*                                                            @ZA27930*/ 02054000
         L     @12,PSAAOLD                                         0562 02055000
         L     @08,ASCBDUMP(,@12)                                  0562 02056000
         LTR   @08,@08                                             0562 02057000
         BZ    @RF00562                                            0562 02058000
*     DO;                           /* DO SDUMP SRB PROCESSING.    0563 02059000
*                                                            @ZA27930*/ 02060000
*                                                                  0563 02061000
*       /*************************************************************/ 02062000
*       /*                                                           */ 02063000
*       /* THIS LOOP FINDS THE CURRENT ENTRY IN THE RTCT ASID ARRAY  */ 02064000
*       /*                                                           */ 02065000
*       /*************************************************************/ 02066000
*                                                                  0564 02067000
*       RFY                                                        0564 02068000
*         R12 RSTD;                 /*                       @G33VPHD*/ 02069000
*       IF RTCTSDMA='1'B&           /* IF THIS IS THE GLOBAL AREA SRB   02070000
*                                                            @ZM44937*/ 02071000
*           ASCBASID=MASTASID THEN  /* USE TABLE ENTRY 16 ONLY     0565 02072000
*                                                            @ZM44937*/ 02073000
