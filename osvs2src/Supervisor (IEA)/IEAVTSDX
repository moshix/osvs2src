         TITLE 'IEAVTSDX - SVC DUMP BRANCH ENTRY AND SCHEDULE DUMP ROUT*00001000
               TINE     '                                               00002000
IEAVTSDX CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEAVTSDX  78.125'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @09,0                                               0001 00012000
@PSTART  LA    @03,4095(,@09)                                      0001 00013000
         USING @PSTART,@09                                         0001 00014000
         USING @PSTART+4095,@03                                    0001 00015000
         STOSM 0(R13),0                                                 00016000
*   IF PSASUPER=NULL&               /* IF CALLER IS NOT DISABLE AND  */ 00017000
*       PSATOLD^=NULL THEN          /* NOT IN SRB MODE,      @G33VPHD*/ 00018000
         SLR   @12,@12                                             0127 00019000
         CL    @12,PSASUPER                                        0127 00020000
         BNE   @RF00127                                            0127 00021000
         C     @12,PSATOLD                                         0127 00022000
         BE    @RF00127                                            0127 00023000
*     DO;                           /* SEE IF ANY LOCKS ARE HELD     */ 00024000
*       RFY                                                        0129 00025000
*         R10 RSTD;                                                0129 00026000
*       GEN(SETLOCK TEST,TYPE=ALL,REGS=(R10));/* ARE ANY LOCKS HELD  */ 00027000
         SETLOCK TEST,TYPE=ALL,REGS=(R10)                               00028000
*       IF R15^=NULL THEN           /* NO LOCKS-NOT DISABLD-NOT SRB  */ 00029000
         LTR   R15,R15                                             0131 00030000
         BZ    @RF00131                                            0131 00031000
*         DO;                       /* CALLER NOT AUTHORIZED         */ 00032000
*           R1=CC133;               /* INTERFACE ABEND 133,DUMP      */ 00033000
         L     R1,@CF05023                                         0133 00034000
*           SVC(13);                /* ABEND CALLER WITH DUMP        */ 00035000
         SVC   13                                                  0134 00036000
*         END;                                                     0135 00037000
*       ELSE                                                       0136 00038000
*         ;                                                        0136 00039000
@RF00131 DS    0H                                                  0137 00040000
*       RFY                                                        0137 00041000
*         R10 UNRSTD;                                              0137 00042000
*     END;                                                         0138 00043000
*   ELSE                                                           0139 00044000
*     ;                                                            0139 00045000
@RF00127 DS    0H                                                  0140 00046000
*   RFY                                                            0140 00047000
*    (R10,                                                         0140 00048000
*     R11,                                                         0140 00049000
*     R4) RSTD;                                                    0140 00050000
*                                                                  0140 00051000
*   /*****************************************************************/ 00052000
*   /*                                                               */ 00053000
*   /* SET UP AN FRR RECOVERY EXIT                                   */ 00054000
*   /*                                                               */ 00055000
*   /*****************************************************************/ 00056000
*                                                                  0141 00057000
*   GEN REFS(SCHFRRAD,FRRS,FRRSCURR,FRRSLAST,PSALSFCC,FRRSFRRA,FRRSENTR 00058000
*       ,FRRSPARM);                                                0141 00059000
*                                /*ESTABLISH A RECOVERY EXIT @G33VPHD*/ 00060000
         SETFRR   A,FRRAD=SCHFRRAD,                                    X00061000
               PARMAD=(R4),WRKREGS=(R10,R11)                            00062000
*   RFY                                                            0142 00063000
*    (R10,                                                         0142 00064000
*     R11) UNRSTD;                                                 0142 00065000
*   RFY                                                            0143 00066000
*     SCHFRR6W BASED(R4);           /* FRR6WPTR POINTS TO FRR 6 WDS  */ 00067000
*   SCHFRRBR=R9;                    /* SAVE BASE REGISTER            */ 00068000
         ST    R9,SCHFRRBR(,R4)                                    0144 00069000
*   SCHFRRSA=R13;                   /* SAVE ADDR OF CALLERS   SAVEARA*/ 00070000
         ST    R13,SCHFRRSA(,R4)                                   0145 00071000
*   SCHFRRRT=ADDR(RECNODMP);        /* GET NEXT RETRY ADDRESS      0146 00072000
*                                                            @G33VPHD*/ 00073000
         LA    @12,RECNODMP                                        0146 00074000
         ST    @12,SCHFRRRT(,R4)                                   0146 00075000
*                                                                  0147 00076000
*/* ******************************************************************/ 00077000
*/*    THIS SEGMENT DETERMINES WHETHER OR NOT A DUMP CAN BE TAKEN.   */ 00078000
*/*    ONLY ONE SVC DUMP CAN BE TAKEN AT A TIME.  IF ANOTHER DUMP    */ 00079000
*/*    IS IN PROGRESS, THIS REQUEST MUST BE DENIED.  IF NO           */ 00080000
*/*    SYS1.DUMP DATASETS WERE DEFINED AT IPL AND THE CALLER DID     */ 00081000
*/*    NOT SUPPLY A DCB, THE DUMP CANNOT BE TAKEN.  IF THE ADDRESS   */ 00082000
*/*    SPACE IS NOT ACTIVE OR THE DUMP TASK IS NOT ACTIVE IN THE     */ 00083000
*/*    ADDRESS SPACE THE DUMP CANNOT BE TAKEN.                       */ 00084000
*/*                                                                  */ 00085000
*/*    INPUT: SDUMP PARM LIST, RTMCT, CVTSDBF, ASVT AND ASCB FOR     */ 00086000
*/*           TARGET ADDRESS SPACE                                   */ 00087000
*/*                                                                  */ 00088000
*/*    OUTPUT: SVC DUMP LOCKED FOR THIS REQUEST OR ABEND CALLER      */ 00089000
*/*            WITH 233 COMPLETION CODE (INVALID PARMS) OR SCHFRREN  */ 00090000
*/*            SET TO INDICATE ANOTHER DUMP IS IN PROGRESS           */ 00091000
*/*                                                                  */ 00092000
*/*    INCLUDED BY: TOP SEQMENT                                      */ 00093000
*/* ******************************************************************/ 00094000
*                                                                  0147 00095000
*   RFY                                                            0147 00096000
*    (R10,                                                         0147 00097000
*     R11) RSTD;                                                   0147 00098000
*   R10=CVTSDBF&HIGHBIT0;           /* 4K SQA BUFFER LOCK WORD, LOCK    00099000
*                                      BIT INDICATES UNLOCKED STATE  */ 00100000
         L     @12,CVTPTR                                          0148 00101000
         L     @08,CVTSDBF(,@12)                                   0148 00102000
         LR    R10,@08                                             0148 00103000
         N     R10,@CF04983                                        0148 00104000
*   R11=CVTSDBF|HIGHBIT1;           /* 4K SQA BUFFER LOCK WORD, LOCK    00105000
*                                      BIT INDICATES LOCKED STATE    */ 00106000
         L     @05,@CF04985                                        0149 00107000
         OR    @08,@05                                             0149 00108000
         LR    R11,@08                                             0149 00109000
*   R15=R11;                        /* CONSTANT LOCKED VALUE OF BUF  */ 00110000
         LR    R15,R11                                             0150 00111000
*   CS(R10,R11,CVTSDBF);            /* IF UNLOCKED, LOCK 4K SQA BUF  */ 00112000
         CS    R10,@11,CVTSDBF(@12)                                0151 00113000
*   IF R15=R10 THEN                 /* IF BUFFER WAS LOCKED DID      */ 00114000
         CR    R15,R10                                             0152 00115000
         BNE   @RF00152                                            0152 00116000
*     DO;                           /* CALLER LOCK IT                */ 00117000
*       IF SDUBUF='1'B THEN         /* IF CALLER REQUESTED BUFFER    */ 00118000
         TM    SDUBUF(R1),B'01000000'                              0154 00119000
         BNO   @RF00154                                            0154 00120000
*         DO;                       /* IS A DUMP IN PROGRESS         */ 00121000
*           R10=NULL;               /* SVC DUMP UNLOCKED VALUE       */ 00122000
         SLR   R10,R10                                             0156 00123000
*           R11=ADDR(SCHPARML)|HIGHBIT1;/* SVC DUMP LOCKED VALUE     */ 00124000
         LA    R11,SCHPARML                                        0157 00125000
         OR    R11,@05                                             0157 00126000
*           CS(R10,R11,RTCTSDPL);   /* IS A DUMP IN PROGRESS         */ 00127000
         L     @12,CVTRTMCT(,@12)                                  0158 00128000
         CS    R10,@11,RTCTSDPL(@12)                               0158 00129000
*           BC(CSCC0,SWAPPED1);     /* NO-GO IND THIS CURR BUFFER=YES   00130000
*                                      BAD PARM                      */ 00131000
         BC    8,SWAPPED1                                          0159 00132000
*           R1=CC233;               /* INTERFACE ABEND 233           */ 00133000
         L     R1,@CF05025                                         0160 00134000
*           R15=RSN38;              /* PUT REASON CODE IN R15      0161 00135000
*                                                            @G33VPHD*/ 00136000
         LA    R15,56                                              0161 00137000
*           SVC(13);                /* ABEND CALLER WITH DMP         */ 00138000
         SVC   13                                                  0162 00139000
*SWAPPED1:                                                         0163 00140000
*           ;                                                      0163 00141000
SWAPPED1 DS    0H                                                  0164 00142000
*           SCHFRRSD='1'B;          /* IND THIS IS CURRENT DUMP      */ 00143000
         OI    SCHFRRSD(R4),B'10000000'                            0164 00144000
*           RTCTSDID=PSAAOLD->ASCBASID;/* A.S. ID OF ACTIVE   SVCDUMP*/ 00145000
         L     @12,CVTPTR                                          0165 00146000
         L     @12,CVTRTMCT(,@12)                                  0165 00147000
         L     @08,PSAAOLD                                         0165 00148000
         LH    @08,ASCBASID(,@08)                                  0165 00149000
         N     @08,@CF05164                                        0165 00150000
         STH   @08,RTCTSDID(,@12)                                  0165 00151000
*         END;                                                     0166 00152000
*       ELSE                                                       0167 00153000
*         SCHFRREN='1'B;            /* IF BUFFER LOCKED AND CALLER 0167 00154000
*                                      DID NOT LOCK IT, ANOTHER DUMP    00155000
*                                      IS ALREADY IN PROGRESS        */ 00156000
         B     @RC00154                                            0167 00157000
@RF00154 OI    SCHFRREN(R4),B'01000000'                            0167 00158000
*     END;                                                         0168 00159000
*   ELSE                            /* IF BUFFER WAS UNLOCKED        */ 00160000
*     DO;                           /* IS ANOTHER DUMP IN PROGRESS   */ 00161000
         B     @RC00152                                            0169 00162000
@RF00152 DS    0H                                                  0170 00163000
*       R10=NULL;                   /* SVC DUMP UNLOCKED VALUE       */ 00164000
         SLR   R10,R10                                             0170 00165000
*       R11=ADDR(SCHPARML)|HIGHBIT1;/* SVC DUMP LOCKED VALUE         */ 00166000
         LA    R11,SCHPARML                                        0171 00167000
         O     R11,@CF04985                                        0171 00168000
*       CS(R10,R11,RTCTSDPL);       /* IS A DUMP IN PROGRESS         */ 00169000
         L     @12,CVTPTR                                          0172 00170000
         L     @12,CVTRTMCT(,@12)                                  0172 00171000
         CS    R10,@11,RTCTSDPL(@12)                               0172 00172000
*       BC(CSCC0,SWAPPED2);         /* NO-GO IND THIS IS CURR DUMP 0173 00173000
*                                      YES & BUF UNLOCKED-ERROR      */ 00174000
         BC    8,SWAPPED2                                          0173 00175000
*       R1=CC233;                   /* INTERFACE-ABEND 233,DUMP      */ 00176000
         L     R1,@CF05025                                         0174 00177000
*       R15=RSN18;                  /* SAVE REASON CODE FOR ABEND  0175 00178000
*                                                            @G33VPHD*/ 00179000
         LA    R15,24                                              0175 00180000
*       SVC(13);                    /* ABEND CALLER WITH DUMP        */ 00181000
         SVC   13                                                  0176 00182000
*SWAPPED2:                                                         0177 00183000
*       ;                                                          0177 00184000
SWAPPED2 DS    0H                                                  0178 00185000
*       SCHFRRSD='1'B;              /* IND THIS IS CURRENT SVC DUMP  */ 00186000
         OI    SCHFRRSD(R4),B'10000000'                            0178 00187000
*       RTCTSDID=PSAAOLD->ASCBASID; /* A.S. ID OF ACTIVE SVC DUMP    */ 00188000
         L     @12,CVTPTR                                          0179 00189000
         L     @12,CVTRTMCT(,@12)                                  0179 00190000
         L     @08,PSAAOLD                                         0179 00191000
         LH    @08,ASCBASID(,@08)                                  0179 00192000
         N     @08,@CF05164                                        0179 00193000
         STH   @08,RTCTSDID(,@12)                                  0179 00194000
*       IF SDUBUF='1'B THEN         /* IF BUF REQ BY CALLER BUT NOT  */ 00195000
         TM    SDUBUF(R1),B'01000000'                              0180 00196000
         BNO   @RF00180                                            0180 00197000
*         DO;                       /* LOCKED BY HIM, ERROR          */ 00198000
*           R15=RSN18;              /* SAVE REASON CODE FOR ABEND  0182 00199000
*                                                            @G33VPHD*/ 00200000
         LA    R15,24                                              0182 00201000
*           R1=CC233;               /* INTERFACE ABEND 233,DUMP      */ 00202000
         L     R1,@CF05025                                         0183 00203000
*           SVC(13);                /* ABEND CALLER WITH DUMP        */ 00204000
         SVC   13                                                  0184 00205000
*         END;                                                     0185 00206000
*     END;                                                         0186 00207000
@RF00180 DS    0H                                                  0187 00208000
*   IF RTCTISVC='1'B|               /* IF CHNGDUMP=NO SVC DUMPS OR   */ 00209000
*       (SDUDCB='0'B&               /* NO CALLER SUPPLIED DATASET    */ 00210000
*       RTCTSDNO='1'B) THEN         /* AND NO SYS1.DUMP DATASETS     */ 00211000
@RC00152 L     @12,CVTPTR                                          0187 00212000
         L     @12,CVTRTMCT(,@12)                                  0187 00213000
         TM    RTCTISVC+236(@12),B'00000001'                       0187 00214000
         BO    @RT00187                                            0187 00215000
         TM    SDUDCB(R1),B'10000000'                              0187 00216000
         BNZ   @RF00187                                            0187 00217000
         TM    RTCTSDNO(@12),B'10000000'                           0187 00218000
         BNO   @RF00187                                            0187 00219000
@RT00187 DS    0H                                                  0188 00220000
*     SCHFRREN='1'B;                /* DUMP CANNOT BE TAKEN          */ 00221000
         OI    SCHFRREN(R4),B'01000000'                            0188 00222000
*   IF(SDUBRANH=ON                  /* FOR BRANCH ENTRY      @G33NPHE*/ 00223000
*       &ADDR(PSACSTK->RT1WLPTA)->RT1TLPN='FF'X/* CHECK FOR A VALID     00224000
*                                      RT1 WORK AREA         @G33NPHE*/ 00225000
*       &PSACSTK->RT1NODMP=ON)      /* AND IF THE SLIP NODUMP FLAG IS   00226000
*                                      ON                    @G33NPHE*/ 00227000
*       |                           /* OR                    @G33NPHE*/ 00228000
*       (SDUBRANH=OFF               /* FOR NONBRANCH ENTRY   @G33NPHE*/ 00229000
*       &PSATOLD^=0                 /* WITH A CURRENT TCB    @G33NPHE*/ 00230000
*       &PSATOLD->TCBRTM2=ON        /* THAT SHOWS RTM2 ENTERED     0189 00231000
*                                                            @G33NPHE*/ 00232000
*       &PSATOLD->TCBRTWA^=0        /* AND SHOWS AN RTM2 WORK AREA 0189 00233000
*                                                            @G33NPHE*/ 00234000
*       &PSATOLD->TCBRTWA->RTM2NODP=ON)/* IF THE SLIP NODUMP FLAG IS    00235000
*                                      ON                    @G33NPHE*/ 00236000
*     THEN                          /* SLIP HAS REQUESTED DUMPS BE 0189 00237000
*                                      SUPRESSED FOR THIS CALL TO RTM   00238000
*                                                            @G33NPHE*/ 00239000
@RF00187 TM    SDUBRANH(R1),B'00000001'                            0189 00240000
         BNO   @GL00004                                            0189 00241000
         L     @12,PSACSTK                                         0189 00242000
         CLI   RT1TLPN+16(@12),255                                 0189 00243000
         BNE   @GL00004                                            0189 00244000
         TM    RT1NODMP(@12),B'00000100'                           0189 00245000
         BO    @RT00189                                            0189 00246000
@GL00004 TM    SDUBRANH(R1),B'00000001'                            0189 00247000
         BNZ   @RF00189                                            0189 00248000
         L     @12,PSATOLD                                         0189 00249000
         SLR   @08,@08                                             0189 00250000
         CR    @12,@08                                             0189 00251000
         BE    @RF00189                                            0189 00252000
         TM    TCBRTM2(@12),B'00001000'                            0189 00253000
         BNO   @RF00189                                            0189 00254000
         L     @12,TCBRTWA(,@12)                                   0189 00255000
         CR    @12,@08                                             0189 00256000
         BE    @RF00189                                            0189 00257000
         TM    RTM2NODP(@12),B'00010000'                           0189 00258000
         BNO   @RF00189                                            0189 00259000
@RT00189 DS    0H                                                  0190 00260000
*     SCHFRREN='1'B;                /* SO DUMP CANNOT BE TAKEN     0190 00261000
*                                                            @G33NPHE*/ 00262000
         OI    SCHFRREN(R4),B'01000000'                            0190 00263000
*   RFY                                                            0191 00264000
*    (R10,                                                         0191 00265000
*     R11) UNRSTD;                                                 0191 00266000
@RF00189 DS    0H                                                  0192 00267000
*   IF SCHFRREN='0'B THEN           /* IF A DUMP CAN STILL BE TAKEN  */ 00268000
         TM    SCHFRREN(R4),B'01000000'                            0192 00269000
         BNZ   @RF00192                                            0192 00270000
*     DO;                           /* MOVE PARAMETER LIST           */ 00271000
*       SCHFRRRP='1'B;              /* INDICATE PARAMETER LIST     0194 00272000
*                                      REFERENCED            @G33VPHD*/ 00273000
         OI    SCHFRRRP(R4),B'00001000'                            0194 00274000
*       IF SDUNEW='0'B THEN         /* IF OLD PARAMTER LIST  @G33VPHD*/ 00275000
         TM    SDUNEW(R1),B'00100000'                              0195 00276000
         BNZ   @RF00195                                            0195 00277000
*         SCHPARML(1:24)=SDUMP(1:24);/* MOVE PARAMETER LIST USING OLD   00278000
*                                      LENGTH                @G33VPHD*/ 00279000
         MVC   SCHPARML(24),SDUMP(R1)                              0196 00280000
*       ELSE                                                       0197 00281000
*         SCHPARML=SDUMP;           /* NEW PARAMTER LIST USE FULL  0197 00282000
*                                      LENGTH                @G33VPHD*/ 00283000
         B     @RC00195                                            0197 00284000
@RF00195 MVC   SCHPARML(40),SDUMP(R1)                              0197 00285000
*       RFY                                                        0198 00286000
*         SDUMP BASED(RTCTSDPL);    /* MAP CALLER PARAMETER LIST   0198 00287000
*                                                            @G33VPHD*/ 00288000
@RC00195 DS    0H                                                  0199 00289000
*/********************************************************************/ 00290000
*/*   THIS SEGMENT VALIDATES THE INFORMATION IN THE SDUMP PARAM      */ 00291000
*/*   LIST.  THE PARAM LIST AND WHAT IT POINTS TO CANNOT BE IN       */ 00292000
*/*   THE 4K SQA BUFFER.  IF THE CALLER PASSED A DCB ADDRESS, THE    */ 00293000
*/*   DCB MUST BE OPEN AND THE DATASET MUST RESIDE ON A SUPPORTED    */ 00294000
*/*   DEVICE.                                                        */ 00295000
*/*                                                                  */ 00296000
*/*   INPUT: SVC DUMP PARAMETER LIST                                 */ 00297000
*/*                                                                  */ 00298000
*/*   OUTPUT: ABAEND CALLER WITH X'233' COMPLETION CODE IF           */ 00299000
*/*           ANY INFORMATION IN THE SDUMP PARAM LIST IS INVALID     */ 00300000
*/*                                                                  */ 00301000
*/*   INCLUDED BY: SDTOP                                     @ZM44936*/ 00302000
*/********************************************************************/ 00303000
*                                                                  0199 00304000
*       RFY                                                        0199 00305000
*        (R10,                                                     0199 00306000
*         R11) RSTD;                                               0199 00307000
*       R10=CVTSDBF&HIGHBIT0;       /* ADDRESS OF BEGINNING OF 4K SQA   00308000
*                                      BUFFER WITH HIGH ORDER BIT  0200 00309000
*                                      ZERO                          */ 00310000
         L     @12,@CF04983                                        0200 00311000
         L     @08,CVTPTR                                          0200 00312000
         L     R10,CVTSDBF(,@08)                                   0200 00313000
         NR    R10,@12                                             0200 00314000
*       R11=R10+FOURK;              /* ADDRESS OF END OF BUFFER      */ 00315000
         LR    R11,R10                                             0201 00316000
         AL    R11,@CF04971                                        0201 00317000
*       IF(R10<=(ADDR(SDUMP)&HIGHBIT0))&/* IF PARM LIST IN   @G33VPHD*/ 00318000
*           ((ADDR(SDUMP)&HIGHBIT0)<R11) THEN/* SDUMP 4K BUFFER    0202 00319000
*                                                            @G33VPHD*/ 00320000
         L     @08,CVTRTMCT(,@08)                                  0202 00321000
         N     @12,RTCTSDPL(,@08)                                  0202 00322000
         CR    R10,@12                                             0202 00323000
         BH    @RF00202                                            0202 00324000
         CR    @12,R11                                             0202 00325000
         BNL   @RF00202                                            0202 00326000
*         DO;                       /* ABEND CALLER W/233    @G33VPHD*/ 00327000
*           R15=RSN1C;              /* SET REASON CODE REG   @G33VPHD*/ 00328000
         LA    R15,28                                              0204 00329000
*           GEN(ABEND X'233',DUMP,,SYSTEM);/* ABEND CALLER           */ 00330000
         ABEND X'233',DUMP,,SYSTEM                                      00331000
*         END;                      /* END ABEND CALLER      @G33VPHD*/ 00332000
*       IF SDUDCB='1'B THEN         /* IF DCB SUPPLIED, VALIDATE     */ 00333000
@RF00202 L     @12,CVTPTR                                          0207 00334000
         L     @12,CVTRTMCT(,@12)                                  0207 00335000
         L     @12,RTCTSDPL(,@12)                                  0207 00336000
         TM    SDUDCB(@12),B'10000000'                             0207 00337000
         BNO   @RF00207                                            0207 00338000
*         DO;                                                      0208 00339000
*           RFY                                                    0209 00340000
*             IHADCB BASED(SDUDCBAD);/* ADDRESS OF DCB IN PARM LIST  */ 00341000
*           RFY                                                    0210 00342000
*             DEBBASIC BASED(DCBDEBAD);/* ADDRESS OF DEB IN DCB      */ 00343000
*           RFY                                                    0211 00344000
*             UCBOB BASED(DEBSUCBA);/* ADDRESS OF UCB IN DEB         */ 00345000
*           IF(R10<=SDUDCBAD&       /* IF DCB IN 4K SQA BUFFER OR    */ 00346000
*               SDUDCBAD<R11)|(DCBOFOPN^='1'B)|/* DCB NOT OPENED OR  */ 00347000
*               (UCB3TAPE='0'B&     /* DATASET NOT ON TAPE AND       */ 00348000
*               UCB3DACC='0'B) THEN /* DATASET NOT ON DASD THEN      */ 00349000
         L     @12,SDUDCBAD(,@12)                                  0212 00350000
         CR    R10,@12                                             0212 00351000
         BH    @GL00013                                            0212 00352000
         CR    @12,R11                                             0212 00353000
         BL    @RT00212                                            0212 00354000
@GL00013 L     @12,CVTPTR                                          0212 00355000
         L     @12,CVTRTMCT(,@12)                                  0212 00356000
         L     @12,RTCTSDPL(,@12)                                  0212 00357000
         LA    @08,40                                              0212 00358000
         AL    @08,SDUDCBAD(,@12)                                  0212 00359000
         TM    DCBOFOPN(@08),B'00010000'                           0212 00360000
         BNO   @RT00212                                            0212 00361000
         L     @12,DCBDEBAD(,@08)                                  0212 00362000
         L     @12,DEBSUCBA(,@12)                                  0212 00363000
         TM    UCB3TAPE(@12),B'10100000'                           0212 00364000
         BNZ   @RF00212                                            0212 00365000
@RT00212 DS    0H                                                  0213 00366000
*             DO;                   /* ABEND CALLER W/233    @G33VPHD*/ 00367000
*               R15=RSN20;          /* SET REASON CODE REG   @G33VPHD*/ 00368000
         LA    R15,32                                              0214 00369000
*               GEN(ABEND X'233',DUMP,,SYSTEM);/* ABEND CALLER       */ 00370000
         ABEND X'233',DUMP,,SYSTEM                                      00371000
*             END;                  /* END ABEND CALLER      @G33VPHD*/ 00372000
*           ELSE                                                   0217 00373000
*             ;                                                    0217 00374000
@RF00212 DS    0H                                                  0218 00375000
*           IF UCB3TAPE='1'B&       /* IF DATASET ON TAPE BUT TAPE   */ 00376000
*               (UCBTBYT4^='03'X&   /* IS NOT 2400 SERIES            */ 00377000
*               UCBTBYT4^='01'X) THEN/* COMPATIBLE THEN              */ 00378000
         L     @12,CVTPTR                                          0218 00379000
         L     @12,CVTRTMCT(,@12)                                  0218 00380000
         L     @12,RTCTSDPL(,@12)                                  0218 00381000
         L     @12,SDUDCBAD(,@12)                                  0218 00382000
