*           R0=SYSDISP;             /* SET SYSTEM NON-DISP CODE    0587 02209000
*                                                            @G33VPHD*/ 02210000
         LA    R0,9                                                0587 02211000
*           R1=NULL;                /* NO TCB ADDR, SET FUNCTION   0588 02212000
*                                                            @G33VPHD*/ 02213000
         SLR   R1,R1                                               0588 02214000
*           R13=SDISPFLG;           /* SVC DUMP NON-DISP FLAG      0589 02215000
*                                                            @G33VPHD*/ 02216000
         L     R13,@CF04979                                        0589 02217000
*           IF ASCBDUMP->TCBFX='0'B THEN/* DISALLOW ASYNCRONOUS    0590 02218000
*                                                            @ZA29469*/ 02219000
