         BNO   @RF00532                                            0532 01902000
         TM    SCHFRRBS(R4),B'00100000'                            0532 01903000
         BNZ   @RF00532                                            0532 01904000
         TM    SCHFRROS(R4),B'00010000'                            0532 01905000
         BNZ   @RF00532                                            0532 01906000
*         CALL SCHBDSRB;            /* TRY TO SCHEDULE THE DUMP SRBS    01907000
*                                                            @G33VPHD*/ 01908000
         BAL   @14,SCHBDSRB                                        0533 01909000
*       IF SCHFRROS=OFF THEN        /* IF NO DUMP HAS BEEN SCHEDULED    01910000
*                                                            @G33VPHD*/ 01911000
@RF00532 TM    SCHFRROS(R4),B'00010000'                            0534 01912000
         BNZ   @RF00534                                            0534 01913000
*         DO;                       /* CHECK IF SVC DUMP SHOULD BE 0535 01914000
*                                      UNLOCKED              @G33VPHD*/ 01915000
*           IF SCHFRRSD=ON THEN     /* IF SVC DUMP WAS LOCKED      0536 01916000
*                                                            @G33VPHD*/ 01917000
         TM    SCHFRRSD(R4),B'10000000'                            0536 01918000
         BNO   @RF00536                                            0536 01919000
*             DO;                   /* UNLOCK SVC DUMP LOCK WORDS  0537 01920000
*                                                            @G33VPHD*/ 01921000
*               RTCTSDPL=NULL;      /* SVC DUMP UNLOCKED     @G33VPHD*/ 01922000
         SLR   @12,@12                                             0538 01923000
         L     @08,CVTPTR                                          0538 01924000
         L     @05,CVTRTMCT(,@08)                                  0538 01925000
         ST    @12,RTCTSDPL(,@05)                                  0538 01926000
*               RTCTSDID=NULL;      /* SVC DUMP NOT ACTIVE   @G33VPHD*/ 01927000
         STH   @12,RTCTSDID(,@05)                                  0539 01928000
*               CVTSDBF=CVTSDBF&HIGHBIT0;/* 4K SQA BUFFER UNLOCKED 0540 01929000
*                                                            @G33VPHD*/ 01930000
         L     @12,CVTSDBF(,@08)                                   0540 01931000
         N     @12,@CF04983                                        0540 01932000
         ST    @12,CVTSDBF(,@08)                                   0540 01933000
*             END;                  /* END SVC DUMP WAS LOCKED     0541 01934000
*                                                            @G33VPHD*/ 01935000
*           R15=NODUMP;             /* NO DUMP SCHEDULED RETURN CODE    01936000
*                                                            @G33VPHD*/ 01937000
@RF00536 LA    R15,8                                               0542 01938000
*         END;                      /* END OF NODUMP SCHEDULED     0543 01939000
*                                                            @G33VPHD*/ 01940000
*       ELSE                                                       0544 01941000
*         DO;                       /* DUMP SCHEDULED AT LEAST 1 ADDR   01942000
*                                      SPACE                 @G33VPHD*/ 01943000
         B     @RC00534                                            0544 01944000
@RF00534 DS    0H                                                  0545 01945000
*           R15=SCHEDUMP;           /* RETURN CODE INDICATES DUMP HAS   01946000
*                                      BEEN SCHEDULED        @G33VPHD*/ 01947000
         SLR   R15,R15                                             0545 01948000
*           IF SCHFRRNS=ON THEN     /* IF AT LEAST ONE DUMP NOT    0546 01949000
*                                                            @G33VPHD*/ 01950000
         TM    SCHFRRNS(R4),B'00100000'                            0546 01951000
         BNO   @RF00546                                            0546 01952000
*             RTCTSDPR=PARTDUMP;    /* SCHEDULED SAVE PARTIAL DUMP 0547 01953000
*                                      RETURN CODE TO BE POSTED IN 0547 01954000
