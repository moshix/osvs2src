         L     @08,CVTRTMCT(,@08)                                  0581 02187000
         TM    RTCTSDPH+236(@08),B'01000000'                       0581 02188000
         BNZ   @RF00581                                            0581 02189000
@RT00581 DS    0H                                                  0582 02190000
*         DO;                       /* SET THE SYSTEM NON          0582 02191000
*                                      DISPATCHABLE          @G33VPHD*/ 02192000
*           RFY                                                    0583 02193000
*             R0 RSTD;              /*                       @ZM44936*/ 02194000
*           R13=WSALSDMP;           /* GET LOCAL SAVE AREA ADDR    0584 02195000
*                                                            @ZM44936*/ 02196000
