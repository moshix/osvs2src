         ST    @12,SDWARTYA(,R1)                                   0530 01889000
*       SDWARCDE=SDWACWT;           /* INDICATE PERCOLATE TO RTM   0531 01890000
*                                                            @G33VPHD*/ 01891000
         MVI   SDWARCDE(R1),X'00'                                  0531 01892000
*       IF RTCTSDSH=ON&             /* IF ITS OK TO SCHEDULE SRBS  0532 01893000
*                                                            @G33VPHD*/ 01894000
*           SCHFRRBS=OFF&           /* AND NOT SCHEDULING SRBS     0532 01895000
*                                                            @G33VPHD*/ 01896000
*           SCHFRROS=OFF THEN       /* AND NO SRBS HAVE BEEN       0532 01897000
*                                      SCHEDULED             @G33VPHD*/ 01898000
         L     @12,CVTPTR                                          0532 01899000
         L     @12,CVTRTMCT(,@12)                                  0532 01900000
