         L     @08,CVTRTMCT(,@08)                                  0581 02168000
         CLI   RTCTSDNA(@08),1                                     0581 02169000
         BNE   @RF00581                                            0581 02170000
@GL00053 L     @08,CVTPTR                                          0581 02171000
         L     @08,CVTRTMCT(,@08)                                  0581 02172000
         TM    RTCTSDRW(@08),B'00000010'                           0581 02173000
         BO    @RT00581                                            0581 02174000
         TM    RTCTASCS(@08),B'10000000'                           0581 02175000
         BNZ   @GL00056                                            0581 02176000
         TM    RTCTASNQ(@08),B'00000100'                           0581 02177000
         BO    @RF00581                                            0581 02178000
@GL00056 L     @08,CVTPTR                                          0581 02179000
         L     @08,CVTRTMCT(,@08)                                  0581 02180000
         TM    RTCTSDPG+236(@08),B'10000000'                       0581 02181000
         BO    @GL00057                                            0581 02182000
