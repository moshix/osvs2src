*         RTM2TRRA=0;               /* INDICATE NO RECURSION       0123 00273000
*                                      PROTECTION                    */ 00274000
         ST    @12,RTM2TRRA(,RTM2PTR)                              0123 00275000
*         RFY                                                      0124 00276000
*           TCB BASED(RMPLTCBA);                                   0124 00277000
*         RFY                                                      0125 00278000
*           TCBTEMP UNRSTD;                                        0125 00279000
*                                                                  0125 00280000
*         /***********************************************************/ 00281000
*         /*                                                         */ 00282000
*         /* RELEASE LOCAL LOCK                                      */ 00283000
*         /*                                                         */ 00284000
*         /***********************************************************/ 00285000
*                                                                  0126 00286000
*         DO;                       /* SETLOCK (RELEASE) TYPE(LOCAL)    00287000
