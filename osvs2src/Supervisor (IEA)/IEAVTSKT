@DL00107 DS    0H                                                  0108 00189000
*             TCBTEMP=TCBLTC;       /* BUMP TO NEXT DAUGHTER         */ 00190000
         L     TCBTEMP,TCBLTC(,TCBTEMP)                            0108 00191000
*           END;                    /* FALL THROUGH WHEN BOTTOM OF 0109 00192000
*                                      DAUGHTER CHAIN IS REACHED     */ 00193000
*                                                                  0109 00194000
@DE00107 SLR   @12,@12                                             0109 00195000
         C     @12,TCBLTC(,TCBTEMP)                                0109 00196000
         BNE   @DL00107                                            0109 00197000
*           /*********************************************************/ 00198000
*           /*                                                       */ 00199000
*           /* SET UP TCB FOR DETACH                                 */ 00200000
*           /*                                                       */ 00201000
*           /*********************************************************/ 00202000
*                                                                  0110 00203000
*           TCBPGNLY=ON;            /* INDICATE PURGE ONLY (IN CASE     00204000
*                                      RTM2 IS INVOKED VIA AN ABTERM    00205000
