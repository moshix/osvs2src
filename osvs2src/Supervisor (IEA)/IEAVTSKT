*   /*                                                               */ 00317000
*   /* RESET RMPL FLAGS THAT DON'T APPLY TO TASK PURGE               */ 00318000
*   /*                                                               */ 00319000
*   /*****************************************************************/ 00320000
*                                                                  0136 00321000
*   RMPLRBPP=OFF;                   /* NOT RB ONLY PURGE             */ 00322000
@RC00083 DS    0H                                                  0137 00323000
*   RMPLIPUF=OFF;                   /* DON'T INHIBIT PAGE UN-FIXES   */ 00324000
*   RMPLRCOV=OFF;                   /* CALLER IS NOT TASK RECOVERY   */ 00325000
