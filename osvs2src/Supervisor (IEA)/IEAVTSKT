*           TCBECB=0;               /* PREVENTS ECB POSTING BY DETACH*/ 00208000
*                                                                  0111 00209000
         ST    @12,TCBECB(,TCBTEMP)                                0111 00210000
*           /*********************************************************/ 00211000
*           /*                                                       */ 00212000
*           /* RESET TCBABWF NON-DISPATCHABLE FLAG. THIS WAS SET BY  */ 00213000
*           /* RTM2 AND WOULD NOT BE RESET DURING ABTERM PROCESSING, */ 00214000
*           /* THEREBY PREVENTING TASK FROM BEING DISPATCHED AFTER   */ 00215000
*           /* THE ABTERM.                                           */ 00216000
*           /*                                                       */ 00217000
*           /*********************************************************/ 00218000
*                                                                  0112 00219000
