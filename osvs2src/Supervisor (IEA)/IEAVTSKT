         L     REG0,@CF02538                                       0113 00227000
*           REG1=ADDR(TCB)|'80000000'X;/* TCB ADDRESS IN R1 HI-BIT 0114 00228000
*                                      INDICATES RESET FUNCTION      */ 00229000
         LR    REG1,TCBTEMP                                        0114 00230000
         O     REG1,@CF02539                                       0114 00231000
*           REG13=0;                /* INDICATE NOT CROSS MEMORY     */ 00232000
