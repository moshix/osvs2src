*           ABRSTWD=OFF;            /* INDICATE LOCK NOT HELD        */ 02141000
         NI    ABRSTWD(FRRPLPTR),B'11111011'                       0563 02142000
*SKIPWAIT:                          /* IF CS FAILS TO GET THE RESTART   02143000
*                                      RESOURCE IT BRANCHES HERE     */ 02144000
*           PSARTM=OFF;             /* INDICATE SUPER IN PSA         */ 02145000
SKIPWAIT NI    PSARTM,B'11111101'                                  0564 02146000
*           ABPSARTM=OFF;                                          0565 02147000
         NI    ABPSARTM(FRRPLPTR),B'11111101'                      0565 02148000
*           RFY                                                    0566 02149000
*             R1 RSTD;                                             0566 02150000
*           R1=SAVEMASK;            /* RESTORE SYSTEM MASK           */ 02151000
         SLR   R1,R1                                               0567 02152000
         IC    R1,SAVEMASK(,FRRPLPTR)                              0567 02153000
*           GEN REFS(R1) SETS(SAVEMASK) CODE(  EX R1,STOSM);       0568 02154000
