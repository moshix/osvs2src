         BNZ   @RF00212                                            0212 00493000
*       DO;                         /* THEN TEST IT FOR A MATCH      */ 00494000
*                                                                  0213 00495000
*         /***********************************************************/ 00496000
*         /*                                                         */ 00497000
*         /* CHECK THE CONDITIONS AT THE TIME OF ERROR AGAINST THE   */ 00498000
*         /* CONDITIONS OUTLINED IN THE SCE FOR THIS SLIP TRAP.      */ 00499000
*         /* TESTING FOR A MATCH IS DONE BY CALLING A MATCH RTN FOR  */ 00500000
*         /* EACH PARAMETER OF THE ERROR CONDITION DESCRIPTION (THESE*/ 00501000
*         /* CORRESPOND TO THE SLIP COMMAND KEYWORDS). THEY ARE      */ 00502000
*         /* PASSED AS VARIABLE LENGTH RECORDS IN THE SCVA, EACH WITH*/ 00503000
*         /* AN ID. IF MORE THAN ONE RECORD WITH THE SAME ID IS      */ 00504000
*         /* PRESENT THE TES WILL BE FOR EITHER TO MATCH. IT IS      */ 00505000
*         /* REQUIRED THAT RECORDS WITH THE SAME ID BE CONSECUTIVE   */ 00506000
*         /* FOR THE EITHER CONDITION; IF THEY ARE NOT THEN BOTH WILL*/ 00507000
*         /* HAVE TO MATCH                                           */ 00508000
*         /*                                                         */ 00509000
*         /***********************************************************/ 00510000
*                                                                  0214 00511000
*         SCVAPTR=ADDR(FAKEMODE);   /* BECAUSE ERRTYPE & MODE ARE IN    00512000
*                                      THE SCE INSTEAD OF THE SCVA 0214 00513000
*                                      WITH THE OTHER RECORDS THEY 0214 00514000
*                                      WILL ALWAYS EXIST AND MUST BE    00515000
*                                      SET UP TO BE CALLED BY USING     00516000
*                                      FAKE SCVA RECORDS.            */ 00517000
         LA    SCVAPTR,FAKEMODE                                    0214 00518000
*         DO WHILE(SCVAPTR^=SCVASTOP);/* EACH MATCHRTN WILL INCREMENT   00519000
*                                      SCVAPTR TO THE NEXT RECORD IF    00520000
*                                      PROCESSING IS TO CONTINUE OR     00521000
*                                      SET IT TO 0 IF IT IS TO STOP. */ 00522000
         B     @DE00215                                            0215 00523000
@DL00215 DS    0H                                                  0216 00524000
*           PREVID=RECID;           /* SAVE THE RECORD ID TO USE   0216 00525000
