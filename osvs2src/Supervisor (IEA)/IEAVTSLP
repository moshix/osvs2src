*                 ABPSARTM=ON;      /* INDICATE PSARTM SET   @ZA25757*/ 02006000
         OI    ABPSARTM(FRRPLPTR),B'00000010'                      0524 02007000
*               END;                /*                       @ZA25757*/ 02008000
*             ELSE                  /* FOR THE RT2 ENVIRONMENT       */ 02009000
*               IF RTM2FMID^=0      /* CHECK FOR THE FAILING MEMORY     02010000
*                                      ID                            */ 02011000
*                 THEN                                             0526 02012000
         B     @RC00518                                            0526 02013000
@RF00518 LH    @08,RTM2FMID(,RTM2PTR)                              0526 02014000
         N     @08,@CF04005                                        0526 02015000
