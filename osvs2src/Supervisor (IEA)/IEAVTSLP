*           END;                    /*                       @ZA16855*/ 00344000
*       END;                        /*                       @ZA16855*/ 00345000
*                                                                  0182 00346000
*   /*****************************************************************/ 00347000
*   /*                                                               */ 00348000
*   /* TO PREVENT ALTERATION OF THE CHAIN OF SCE INCREMENT THE USE   */ 00349000
*   /* COUNTS FOR THE FORWARD POINTERS TO EACH STARTING AT THE SHDR. */ 00350000
*   /* THIS MEANS THE USE COUNT IN THE SHDR AND ALL OF THE SCE EXCEPT*/ 00351000
*   /* THE LAST ONE WILL BE INCREMENTED. IF THE SCE CHAIN IS IN USE  */ 00352000
*   /* BY THIS MODULE IN ANOTHER ADDRESS SPACE THERE WILL BE NO      */ 00353000
*   /* CONFLICT. IF IT IS IN USE BY IEECB905 (ADD OR DELETE AN SCE)  */ 00354000
*   /* INCREMENTING THE USE COUNT WILL CAUSE IEECB905 TO ABORT ITS   */ 00355000
*   /* PROCESSING                                                    */ 00356000
*   /*                                                               */ 00357000
*   /*****************************************************************/ 00358000
*                                                                  0182 00359000
*   SCEPTR=ADDR(SHDR);              /* THE HEADER IS KNOWN TO EXIST     00360000
*                                      BECAUSE THAT IS A CONDITION 0182 00361000
*                                      FOR CALLING THIS MODULE. SHDR    00362000
*                                      ALSO SERVES AS A DUMMY SCE TO    00363000
*                                      BEGIN THE CHAIN               */ 00364000
@RC00136 DS    0H                                                  0182 00365000
@RC00110 L     @01,CVTPTR                                          0182 00366000
         L     SCEPTR,CVTRTMS(,@01)                                0182 00367000
*   DO WHILE(SCEFWD^=0);            /* LOOP FORWARD THROUGH THE CHAIN*/ 00368000
         B     @DE00183                                            0183 00369000
@DL00183 DS    0H                                                  0184 00370000
*     IF SCEFWD->SCECBID^=IDSCE|    /* VALIDATE NEXT SCE             */ 00371000
*         SCEFWD->SCEBKWD^=SCEPTR THEN                             0184 00372000
         L     @15,SCEFWD(,SCEPTR)                                 0184 00373000
         CLC   SCECBID(4,@15),@CC03846                             0184 00374000
         BNE   @RT00184                                            0184 00375000
         C     SCEPTR,SCEBKWD(,@15)                                0184 00376000
         BE    @RF00184                                            0184 00377000
@RT00184 DS    0H                                                  0185 00378000
*       DO;                         /* IF SCE INVALID, REPLACE PTR TO   00379000
*                                      IT WITH PTR TO VALID SCE BY 0185 00380000
*                                      STARTING WITH THE SCE POINTED    00381000
*                                      TO VIA THE SHDRBKWD AND     0185 00382000
*                                      BACKING UP THE CHAIN UNTIL AN    00383000
*                                      INVALID SCE IS FOUND OR THE 0185 00384000
*                                      SCE SCEPTR IS FOUND           */ 00385000
*         RFY                                                      0186 00386000
*          (BCKSCE,                                                0186 00387000
*           NXTBKSCE) RSTD;                                        0186 00388000
*         NXTBKSCE=SHDRBKWD;        /* START AT LAST SCE             */ 00389000
         L     @07,CVTPTR                                          0187 00390000
         L     @07,CVTRTMS(,@07)                                   0187 00391000
         L     NXTBKSCE,SHDRBKWD(,@07)                             0187 00392000
*         BCKSCE=0;                                                0188 00393000
         SLR   BCKSCE,BCKSCE                                       0188 00394000
*         DO WHILE NXTBKSCE->SCECBID=IDSCE&/* FOLLOW BACKWORD CHAIN     00395000
*                                      UNTIL                         */ 00396000
*               NXTBKSCE->SCEFWD=BCKSCE&/* INVALID SCE IS FOUND OR 0189 00397000
*                                      THE                           */ 00398000
*               NXTBKSCE^=SCEPTR;   /* SCEPTR SCE IS FOUND           */ 00399000
         B     @DE00189                                            0189 00400000
@DL00189 DS    0H                                                  0190 00401000
*           BCKSCE=NXTBKSCE;        /* UPDATE PTRS TO NEXT SCE       */ 00402000
         LR    BCKSCE,NXTBKSCE                                     0190 00403000
*           NXTBKSCE=NXTBKSCE->SCEBKWD;                            0191 00404000
         L     NXTBKSCE,SCEBKWD(,NXTBKSCE)                         0191 00405000
*         END;                                                     0192 00406000
