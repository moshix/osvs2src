*           ABSYSMSK=ON;            /* INDICATE SYS MASK CHANGED     */ 02088000
         OI    ABSYSMSK(FRRPLPTR),B'00001000'                      0545 02089000
*           PSARTM=ON;              /* INDICATE SUPER IN PSA         */ 02090000
         OI    PSARTM,B'00000010'                                  0546 02091000
*           ABPSARTM=ON;            /* INDICATE PSARTM TURNED ON     */ 02092000
         OI    ABPSARTM(FRRPLPTR),B'00000010'                      0547 02093000
*           RFY                                                    0548 02094000
*            (COMPREG,                                             0548 02095000
*             SWAPREG) RSTD;                                       0548 02096000
*           COMPREG=0;              /* PREPARE FOR CS TO OBTAIN THE     02097000
*                                      RESTART LOC, WHICH WILL BE 0     02098000
*                                      IF AVAILABLE                  */ 02099000
         SLR   COMPREG,COMPREG                                     0549 02100000
*           LWCPUID=PSACPULA;       /* BUILD A LOCKWORD TO REPLACE 0550 02101000
*                                      THE 0                         */ 02102000
         LH    @08,PSACPULA                                        0550 02103000
         STH   @08,LWCPUID(,PARMPTR)                               0550 02104000
*           LWLOCKID=RTMLOKID;      /* ID US                         */ 02105000
         MVC   LWLOCKID(2,PARMPTR),@CC03859                        0551 02106000
*           SWAPREG=LOCKWORD;       /* PUT LOCKWORD INTO REG FOR CS  */ 02107000
         L     SWAPREG,LOCKWORD(,PARMPTR)                          0552 02108000
*           CS(COMPREG,SWAPREG,CVTRSTWD);/* TRY FOR IT               */ 02109000
         L     @08,CVTPTR                                          0553 02110000
         CS    COMPREG,@09,CVTRSTWD(@08)                           0553 02111000
*           BC(NE,SKIPWAIT);        /* IF THE LOCK WAS NOT AVAILABLE    02112000
*                                      FORGET IT                     */ 02113000
         BC    4,SKIPWAIT                                          0554 02114000
*           ABRSTWD=ON;             /* INDICATE THE LOCK IS HELD     */ 02115000
         OI    ABRSTWD(FRRPLPTR),B'00000100'                       0555 02116000
*           RFY                                                    0556 02117000
*            (COMPREG,                                             0556 02118000
*             SWAPREG) UNRSTD;                                     0556 02119000
*           RFY                                                    0557 02120000
*            (R0,                                                  0557 02121000
*             R1) RSTD;                                            0557 02122000
*           R0=SLIPWAIT;            /* SET UP SLIP WAIT STATE CODE   */ 02123000
         LA    R0,27                                               0558 02124000
*           R1=0;                   /* NO STATUS SAVEAREA PTR,     0559 02125000
*                                      THEREFORE IEESTPRS WILL NOT 0559 02126000
*                                      QUIESCE BEFORE STOPPING       */ 02127000
         SLR   R1,R1                                               0559 02128000
*           CALL IEESTPRS;          /* CALL THE SYSTEM STOP-RESTART     02129000
*                                      ROUTINE                       */ 02130000
         L     @08,CVTPTR                                          0560 02131000
         L     @15,CVTSTPRS(,@08)                                  0560 02132000
         BALR  @14,@15                                             0560 02133000
*           RFY                                                    0561 02134000
*            (R0,                                                  0561 02135000
*             R1) UNRSTD;                                          0561 02136000
*           CVTRSTWD=0;             /* FREE THE RESTART LOCK         */ 02137000
         L     @08,CVTPTR                                          0562 02138000
