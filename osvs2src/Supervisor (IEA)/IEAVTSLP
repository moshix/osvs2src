         L     @08,CVTPTR                                          0635 02376000
         L     SCANPTR,CVTRTMS(,@08)                               0635 02377000
*         DO WHILE(SCANPTR->SCEFWD^=ADDR(SCE));                    0636 02378000
         B     @DE00636                                            0636 02379000
@DL00636 DS    0H                                                  0637 02380000
*           SCANPTR=SCANPTR->SCEFWD;                               0637 02381000
         L     SCANPTR,SCEFWD(,SCANPTR)                            0637 02382000
*         END;                                                     0638 02383000
@DE00636 L     @08,SDWAPARM(,R1)                                   0638 02384000
         CLC   SCEFWD(4,SCANPTR),FRRSCEPT(@08)                     0638 02385000
         BNE   @DL00636                                            0638 02386000
*         FRRSCEPT=SCANPTR;         /* THIS IS THE PREVIOUS SCE      */ 02387000
         ST    SCANPTR,FRRSCEPT(,@08)                              0639 02388000
*         RFY                                                      0640 02389000
*           SCANPTR UNRSTD;                                        0640 02390000
*       END;                                                       0641 02391000
*     RFY                                                          0642 02392000
*      (COMPREG,                                                   0642 02393000
*       SWAPREG) RSTD;                                             0642 02394000
@RC00631 DS    0H                                                  0643 02395000
*     COMPREG=SCECTR;               /* INIT COMPARE VALUE FOR CS     */ 02396000
         L     @08,SDWAPARM(,R1)                                   0643 02397000
         L     @08,FRRSCEPT(,@08)                                  0643 02398000
         L     COMPREG,SCECTR(,@08)                                0643 02399000
*DECRLOOP:                          /* CS LOOP TO DECREMENT THE SCE     02400000
*                                      USE COUNT                     */ 02401000
*     SWAPREG=COMPREG-1;                                           0644 02402000
DECRLOOP LR    SWAPREG,COMPREG                                     0644 02403000
         BCTR  SWAPREG,0                                           0644 02404000
*     CS(COMPREG,SWAPREG,SCECTR);                                  0645 02405000
         L     @08,SDWAPARM(,R1)                                   0645 02406000
         L     @08,FRRSCEPT(,@08)                                  0645 02407000
         CS    COMPREG,@09,SCECTR(@08)                             0645 02408000
*     BC(NE,DECRLOOP);              /* END OF CS LOOP                */ 02409000
         BC    4,DECRLOOP                                          0646 02410000
*     RFY                                                          0647 02411000
*      (COMPREG,                                                   0647 02412000
*       SWAPREG) UNRSTD;                                           0647 02413000
*   END;                                                           0648 02414000
@DE00630 L     @08,SDWAPARM(,R1)                                   0648 02415000
         L     @08,FRRSCEPT(,@08)                                  0648 02416000
         LTR   @08,@08                                             0648 02417000
         BZ    @DC00630                                            0648 02418000
         L     @07,CVTPTR                                          0648 02419000
         C     @08,CVTRTMS(,@07)                                   0648 02420000
         BNE   @DL00630                                            0648 02421000
@DC00630 DS    0H                                                  0649 02422000
*   IF ABRTSENV=ON                  /* IF THIS IS THE RTS ENVIRONMENT   02423000
*                                      THE FRR STACKS HAVE TO BE PUT    02424000
*                                      BACK IN ORDER                 */ 02425000
*     THEN                                                         0649 02426000
         L     @08,SDWAPARM(,R1)                                   0649 02427000
         TM    ABRTSENV(@08),B'10000000'                           0649 02428000
         BNO   @RF00649                                            0649 02429000
*     DO;                           /* FIRST RESET THE RTS ERROR   0650 02430000
*                                      RECOVERY                      */ 02431000
*/* ******************************************************************/ 02432000
*/* THIS SEGMENT IS DESIGNED TO RECOVER FROM                         */ 02433000
*/* A POSSIBLE ERROR OCCURRING WHEN SLIP IS INVOKED FROM             */ 02434000
*/*  IEAVTRTS.  IT PERFORMS THE FOLLOWING:                           */ 02435000
*/* 1.REQUESTS RETRY IF THE ERROR TYPE IS NOT RESTART OR DATERR AND  */ 02436000
*/*   THE CURRENT FRR STACK IS NOT EQUAL TO THE RECOVERY STACK USED  */ 02437000
*/*   BY IEAVTRTS OR IS NOT EQUAL TO THE INTERRUPTED STACK IEAVTRTS  */ 02438000
*/*  WAS PROCESSING PRIOR TO INVOKING SLIP                           */ 02439000
*/* 2.REQUESTS PERCOLATION IF THE ERROR TYPE IS DATERR OR RESTART AND*/ 02440000
*/*   THE STACK CONDITION DESCRIBED IN 1. EXISTS.                    */ 02441000
*/* 3.ABORTS RECOVERY BY ACTIVATING(VIA SVC 13) RTM ABORT PROCESSING */ 02442000
*/*   WHEN THE STACK CONDITION DESCRIBED IN 1. DOES NOT EXIST        */ 02443000
*/* NOTES------                                                      */ 02444000
*/*  - REGS(R0,R1,R14,R2) MUST BE RESTRICTED THROUGHOUT THIS FRR     */ 02445000
*/*  - THE RSVT IN THE PSA IS RESET TO THE ORIGINAL CONDITIONS AT THE*/ 02446000
*/*    TIME OF THE ORIGINAL ENTRY TO IEAVTRTS FOR THE ERROR          */ 02447000
*/*    INDEPENDENT OF WHETHER THIS FRR REQUESTS PERCOLATION OR RETRY */ 02448000
*/*  - RECORDING IS REQUESTED                                        */ 02449000
*/*  - IF AN ERROR SHOULD OCCUR IN THIS FRR---RTM ABORT PROCESSING   */ 02450000
*/*    WILL GET CONTROL                                              */ 02451000
*/* ******************************************************************/ 02452000
*                                                                  0651 02453000
*       RFY                                                        0651 02454000
*         FRRS BASED(FRRISTK);      /* ADDRESSABILITY TO STACK RTS 0651 02455000
*                                      WAS PROCESSING PRIOR TO SLIP     02456000
*                                      CHANGING THEM                 */ 02457000
*                                                                  0651 02458000
*       /*************************************************************/ 02459000
*       /*                                                           */ 02460000
*       /* ABORT RECOVERY IF THE RTS RECOVERY STACK AND THE          */ 02461000
*       /* INTERRUPTED STACK PRIOR TO THE SLIP FAILURE ARE EQUAL TO  */ 02462000
*       /* THE CURRENT STACK--UNWINDING STACKS SO THAT RETRY OR      */ 02463000
*       /* PERCOLATE CAN BE ACCOMPLISHED IS IMPOSSIBLE. RESTORATION  */ 02464000
*       /* OF RTS PROCESSING FOR THE ORIGINAL ERROR CANNOT BE        */ 02465000
*       /* ACCOMPLISHED                                              */ 02466000
*       /*                                                           */ 02467000
*       /*************************************************************/ 02468000
*                                                                  0652 02469000
*       IF PSACSTK=R2               /* IF THE RTS RECOVERY FRRS IS 0652 02470000
*                                      CURRENTLY THE SAME AS THE   0652 02471000
*                                      CURRENT FRRS                  */ 02472000
*         THEN                      /* THEN THE FRR STACKS ARE     0652 02473000
*                                      CONFUSED                      */ 02474000
         L     @07,PSACSTK                                         0652 02475000
         CR    @07,R2                                              0652 02476000
         BNE   @RF00652                                            0652 02477000
*         IF PSACSTK=FRRISTK        /* IF THE CURRENT FRRS IS THE  0653 02478000
*                                      SAME AS THE ONE INTERRUPTED BY   02479000
*                                      SLIP                          */ 02480000
*           THEN                    /* THEN RECOVERY IS IMPOSSIBLE   */ 02481000
         C     @07,FRRISTK(,@08)                                   0653 02482000
         BNE   @RF00653                                            0653 02483000
*           SVC(13);                /* ACTIVATE RTM ABORT PROCESSING */ 02484000
         SVC   13                                                  0654 02485000
*         ELSE                      /* THE FRR STACKS CAN BE UNWOUND.   02486000
*                                      FIRST RESET THE RSVT TO THE 0655 02487000
*                                      STATE THAT EXISTED PRIOR TO 0655 02488000
*                                      THE SLIP FAILURE              */ 02489000
*           IF PSACSTK=PSAPSTK      /* IF THE CURRENT FRRS IS FOR THE   02490000
*                                      PCFLIH                        */ 02491000
*             THEN                                                 0655 02492000
         B     @RC00653                                            0655 02493000
@RF00653 CLC   PSACSTK(4),PSAPSTK                                  0655 02494000
         BNE   @RF00655                                            0655 02495000
*             PSAPSAV=FRRISTK;                                     0656 02496000
         L     @08,SDWAPARM(,R1)                                   0656 02497000
         L     @08,FRRISTK(,@08)                                   0656 02498000
         ST    @08,PSAPSAV                                         0656 02499000
*           ELSE                                                   0657 02500000
*             IF PSACSTK=PSARSTK    /* IF THE CURRENT FRRS IS FOR  0657 02501000
*                                      RESTART                       */ 02502000
*               THEN                                               0657 02503000
         B     @RC00655                                            0657 02504000
@RF00655 CLC   PSACSTK(4),PSARSTK                                  0657 02505000
         BNE   @RF00657                                            0657 02506000
*               PSARSAV=FRRISTK;                                   0658 02507000
*                                                                  0658 02508000
         L     @08,SDWAPARM(,R1)                                   0658 02509000
         L     @08,FRRISTK(,@08)                                   0658 02510000
         ST    @08,PSARSAV                                         0658 02511000
*       /*************************************************************/ 02512000
*       /*                                                           */ 02513000
*       /* FOR CONDITIONS WHERE RECOVERY IS POSSIBLE CHECK TO SEE IF */ 02514000
*       /* IT WAS A RESTART OR DAT ERROR                             */ 02515000
*       /*                                                           */ 02516000
*       /*************************************************************/ 02517000
*                                                                  0659 02518000
*       IF SDWATEXC=ON              /* IF THE ERROR IS A DATERR      */ 02519000
*           |SDWARKEY=ON THEN       /* OR A RESTART ERROR THE THIS 0659 02520000
*                                      FRR WILL SETUP TO PERCOLATE   */ 02521000
@RF00657 DS    0H                                                  0659 02522000
@RC00655 DS    0H                                                  0659 02523000
@RC00653 DS    0H                                                  0659 02524000
@RF00652 TM    SDWATEXC(R1),B'00100010'                            0659 02525000
         BZ    @RF00659                                            0659 02526000
*         DO;                       /* SETUP TO PERCOLATE            */ 02527000
*           DO;                     /* SETRP RC(0)                   */ 02528000
*             RESPECIFY                                            0662 02529000
*              (GPR00P,                                            0662 02530000
*               GPR01P,                                            0662 02531000
*               GPR14P,                                            0662 02532000
*               GPR15P) RSTD;                                      0662 02533000
*             GPR01P->SDWARCDE=0;   /* SAVE RC VALUE                 */ 02534000
         MVI   SDWARCDE(GPR01P),X'00'                              0663 02535000
*             RESPECIFY                                            0664 02536000
*              (GPR00P,                                            0664 02537000
*               GPR01P,                                            0664 02538000
*               GPR14P,                                            0664 02539000
*               GPR15P) UNRSTD;                                    0664 02540000
*           END;                    /* INDICATE PERCOLATION IN THE 0665 02541000
*                                      SDWA                  @ZA17761*/ 02542000
*           IF FRRISTK^=PSANSTK THEN/* IF THE STACK BEING PROCESSED     02543000
*                                      PRIOR TO THE SLIP FAILURE IS A   02544000
*                                      SUPER STACK SET UP THIS STACK    02545000
*                                      SO PERCOLATION TO THE SUPER 0666 02546000
*                                      FRR WILL RESULT IN THE CURRENT   02547000
*                                      ERROR BEING GENERATED TO THE     02548000
*                                      LAST INTERRUPTED STK          */ 02549000
         L     @08,SDWAPARM(,R1)                                   0666 02550000
         L     @08,FRRISTK(,@08)                                   0666 02551000
         C     @08,PSANSTK                                         0666 02552000
         BE    @RF00666                                            0666 02553000
*             DO;                                                  0667 02554000
*               FRRSRCUR=OFF;                                      0668 02555000
         L     @07,FRREPTR(,@08)                                   0668 02556000
         NI    FRRSRCUR(@07),B'01111111'                           0668 02557000
*               FRRSCURR=FRRSEMP;   /* INSURE THAT THE SUPER FRR IS     02558000
*                                      CURRENT                       */ 02559000
         L     @07,FRRSEMP(,@08)                                   0669 02560000
         ST    @07,FRRSCURR(,@08)                                  0669 02561000
*               RT1TLPN=NONE;       /* PROPOGATE THE ERROR AS NEW    */ 02562000
         MVI   RT1TLPN+16(@08),X'00'                               0670 02563000
*             END;                                                 0671 02564000
*           ELSE                    /* IF THE INTERRUPTED STACK PRIOR   02565000
*                                      TO THE SLIP FAILURE WAS THE 0672 02566000
*                                      NORMAL STACK THEN PROPOGATE 0672 02567000
*                                      THE ERROR AS A FRR FAILURE    */ 02568000
*             RT1TLPN=VALIDREC;     /* INDICATE VALID RECURSION INTO    02569000
*                                      RTS                           */ 02570000
         B     @RC00666                                            0672 02571000
@RF00666 L     @08,SDWAPARM(,R1)                                   0672 02572000
         L     @08,FRRISTK(,@08)                                   0672 02573000
         MVI   RT1TLPN+16(@08),X'FF'                               0672 02574000
*           ABFRR=OFF;                                             0673 02575000
@RC00666 L     @08,SDWAPARM(,R1)                                   0673 02576000
         NI    ABFRR(@08),B'11111110'                              0673 02577000
*           SHDRFRR=OFF;            /* IF THE FRR HAS SUCCESSFULLY 0674 02578000
*                                      DONE ITS WORK THE TURN OFF  0674 02579000
*                                      THIS FLAG TO ALLOW SLIP TO RUN   02580000
*                                      AGAIN                         */ 02581000
         L     @08,CVTPTR                                          0674 02582000
         L     @08,CVTRTMS(,@08)                                   0674 02583000
         NI    SHDRFRR(@08),B'01111111'                            0674 02584000
*           RETURN CODE(0);         /* EXIT WITH PERCOLATION CODE FOR   02585000
*                                      DAT OR RESTART ERRORS         */ 02586000
         SLR   @15,@15                                             0675 02587000
@EL00012 DS    0H                                                  0675 02588000
@EF00012 DS    0H                                                  0675 02589000
@ER00012 BR    @14                                                 0675 02590000
*         END;                                                     0676 02591000
*       ELSE                                                       0677 02592000
*         ;                         /* IF THE ERROR IS NEITHER DATERR   02593000
*                                      OR RESTART THEN A RETRY BACK     02594000
*                                      TO SLIP WILL BE SETUP         */ 02595000
@RF00659 DS    0H                                                  0678 02596000
*     END;                                                         0678 02597000
*                                                                  0678 02598000
*   /*****************************************************************/ 02599000
*   /*                                                               */ 02600000
*   /* RETRY IN IEAVTSLP AT THE POINT WHERE THE ERROR RECOVERY IS    */ 02601000
*   /* DELETED. REGISTERS 2,3,12,&13 ARE RESTORED FROM THE FRR       */ 02602000
*   /* PARMLIST TO BE SURE THAT THE IEAVTSLP EXIT CODE WILL FUNCTION.*/ 02603000
*   /* IT ASSUMES THE CONTENTS OF ONLY THESE 4 REGS. THE OTHER REGS  */ 02604000
*   /* FROM THE SDWA MIGHT BE THOSE OF SDUMP, RECORD, OR IEESTPRS.   */ 02605000
*   /*                                                               */ 02606000
*   /*****************************************************************/ 02607000
*                                                                  0679 02608000
*   RUBBITS=RUBMASK;                /* REG UPDATE MASK               */ 02609000
@RF00649 LR    @08,R0                                              0679 02610000
         MVC   RUBBITS(2,@08),RUBMASK                              0679 02611000
*   RUB1=FRRISTK;                   /* PTR TO THE FRRS INTERRUPTED BY   02612000
*                                      SLIP                          */ 02613000
         L     @08,SDWAPARM(,R1)                                   0680 02614000
         LR    @07,R0                                              0680 02615000
         MVC   RUB1(4,@07),FRRISTK(@08)                            0680 02616000
*   RUB2=ADDR(FRRPARM);             /* FRR PARM LIST BASE            */ 02617000
         LR    @07,R0                                              0681 02618000
         STCM  @08,15,RUB2(@07)                                    0681 02619000
*   RUB3=FRRBASE;                   /* IEAVTSLP BASE                 */ 02620000
         LR    @07,R0                                              0682 02621000
         MVC   RUB3(4,@07),FRRBASE(@08)                            0682 02622000
*   RUB4=FRRSAPTR;                  /* SAVEAREA PTR                  */ 02623000
         LR    @07,R0                                              0683 02624000
         MVC   RUB4(4,@07),FRRSAPTR(@08)                           0683 02625000
*   DO;                             /* SETRP RC(4)RETADDR(RETRYPT)RET   02626000
*                                      REGS(YES)RUB(RUB)             */ 02627000
*     RESPECIFY                                                    0685 02628000
*      (GPR00P,                                                    0685 02629000
*       GPR01P,                                                    0685 02630000
*       GPR14P,                                                    0685 02631000
*       GPR15P) RSTD;                                              0685 02632000
*     GPR01P->SDWARCDE=4;           /* STORE RC INTO SDWA            */ 02633000
         MVI   SDWARCDE(GPR01P),X'04'                              0686 02634000
*     GPR01P->SDWARTYA=ADDR(RETRYPT);/* SAVE RETRY ADDRESS           */ 02635000
         LA    @08,RETRYPT                                         0687 02636000
         ST    @08,SDWARTYA(,GPR01P)                               0687 02637000
*     GPR01P->SDWAUPRG='1'B;        /* TURN ON RETREGS INDICATOR     */ 02638000
         OI    SDWAUPRG(GPR01P),B'00001000'                        0688 02639000
*     GPR15P=ADDR(RUB);             /* GET PTR TO REG UPDATE BLOCK   */ 02640000
         LR    GPR15P,R0                                           0689 02641000
*     GPR00P=GPR15P->I015F;         /* ACCESS REGISTER BIT PATTERN   */ 02642000
         LH    GPR00P,I015F(,GPR15P)                               0690 02643000
*     GPR15P=GPR15P+2;              /* CREATE PTR TO USER REG VALUES */ 02644000
         AL    GPR15P,@CF00076                                     0691 02645000
*     GPR14P=ADDR(GPR01P->SDWASRSV);/* CREATE PTR TO SDWA SAVE AREA  */ 02646000
         LA    GPR14P,SDWASRSV(,GPR01P)                            0692 02647000
*     GEN(SLL   0,16);              /* SHIFT BIT PATTERN TO HIGH   0693 02648000
*                                      ORDER                         */ 02649000
         SLL   0,16                                                     02650000
*R0007:                                                            0694 02651000
*     GEN FLOWS(I0007);                                            0694 02652000
R0007    DS    0H                                                  0694 02653000
         ALR   0,0                    SHIFT BIT TO SIGN POSITION        02654000
         BC    12,I0007               BRANCH IF BIT NOT ON              02655000
         MVC   0(4,14),0(15)          MODIFY REGISTER IN SDWA           02656000
         LA    15,4(15)               UPDATE POINTER                    02657000
*I0007:                                                            0695 02658000
*     GENERATE FLOWS(R0007);                                       0695 02659000
I0007    DS    0H                                                  0695 02660000
         LA    14,4(14)               UPDATE PTR IN SDWA                02661000
         BC    5,R0007                TEST ALR CONDITION CODE TO TEST   02662000
*                                     IF ALL BITS EXHAUSTED             02663000
*     RESPECIFY                                                    0696 02664000
*      (GPR00P,                                                    0696 02665000
*       GPR01P,                                                    0696 02666000
*       GPR14P,                                                    0696 02667000
*       GPR15P) UNRSTD;                                            0696 02668000
*   END;                            /*                       @ZA17761*/ 02669000
*   ABFRR=OFF;                                                     0698 02670000
         L     @08,SDWAPARM(,R1)                                   0698 02671000
         NI    ABFRR(@08),B'11111110'                              0698 02672000
*   IF ABRTSENV=ON THEN                                            0699 02673000
         TM    ABRTSENV(@08),B'10000000'                           0699 02674000
         BNO   @RF00699                                            0699 02675000
*     SHDRFRR=OFF;                  /* IF THE FRR HAS SUCCESSFULLY 0700 02676000
*                                      DONE ITS WORK THE TURN OFF  0700 02677000
*                                      THIS FLAG TO ALLOW RTS SLIP 0700 02678000
*                                      ENTRY AGAIN                   */ 02679000
         L     @08,CVTPTR                                          0700 02680000
         L     @08,CVTRTMS(,@08)                                   0700 02681000
         NI    SHDRFRR(@08),B'01111111'                            0700 02682000
*   R14=SAVRET;                     /* RESTORE RETURN ADDRESS      0701 02683000
*                                                            @ZA17761*/ 02684000
@RF00699 LR    R14,SAVRET                                          0701 02685000
*   RFY                                                            0702 02686000
*     SAVRET UNRSTD;                /*                       @ZA17761*/ 02687000
*   RETURN CODE(4);                 /* RETURN RETRY CODE             */ 02688000
         LA    @15,4                                               0703 02689000
         BR    @14                                                 0703 02690000
*   END SLIPFRR;                                                   0704 02691000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 02692000
*     GPR00P PTR(31) REG(0),                                       0705 02693000
*     GPR01P PTR(31) REG(1),                                       0705 02694000
*     GPR14P PTR(31) REG(14),                                      0705 02695000
*     GPR15P PTR(31) REG(15);                                      0705 02696000
*   DECLARE                         /* COMMON VARIABLES              */ 02697000
*     I256C CHAR(256) BASED,                                       0706 02698000
*     I031F FIXED(31) BASED,                                       0706 02699000
*     I031P PTR(31) BASED,                                         0706 02700000
*     I015F FIXED(15) BASED,                                       0706 02701000
*     I015P PTR(15) BASED,                                         0706 02702000
*     I008P PTR(8) BASED,                                          0706 02703000
*     I001C CHAR(1) BASED;                                         0706 02704000
*   END IEAVTSLP                                                   0707 02705000
*                                                                  0707 02706000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 02707000
