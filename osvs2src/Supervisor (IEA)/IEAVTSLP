         OI    ABFRR(@08),B'00000001'                              0586 02210000
*   SAVRET=R14;                     /* SAVE RETURN ADDRESS   @ZA17761*/ 02211000
         LR    SAVRET,R14                                          0587 02212000
*   IF ABRTSENV=ON                  /* IF THIS IS THE RTS ENVIRONMENT*/ 02213000
