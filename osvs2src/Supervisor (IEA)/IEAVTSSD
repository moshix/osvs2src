         STH   @02,SMWKNFUS(,R9)                                   0229 00384000
*   SMWKNTDI=ZERO;                  /* ZERO NUMBER TIMES DIE ENTERED    00385000
*                                                            @ZM45073*/ 00386000
         STH   @02,SMWKNTDI(,R9)                                   0230 00387000
*   SMWKNTDQ=ZERO;                  /* ZERO NUMBER TIMES DIE REQUED     00388000
*                                                            @ZM45073*/ 00389000
         STH   @02,SMWKNTDQ(,R9)                                   0231 00390000
*   SMWKNFFD=ZERO;                  /* ZERO NUMBER FRAMES FREED    0232 00391000
*                                                            @ZM45073*/ 00392000
         STH   @02,SMWKNFFD(,R9)                                   0232 00393000
*   SMWKNSTB=ZERO;                  /* ZERO NUMBER OF STEAL BACKS  0233 00394000
*                                                            @ZM45073*/ 00395000
         STH   @02,SMWKNSTB(,R9)                                   0233 00396000
*   SMWKHEAD=SMWKHEAD&&SMWKHEAD;    /* INITIALIZE RECORD HEADER AREA */ 00397000
         XC    SMWKHEAD(20,R9),SMWKHEAD(R9)                        0234 00398000
*   SMWKSCBF=ZERO;                  /* INITIALIZE CURRENT BUFFER   0235 00399000
*                                      SPACE                         */ 00400000
         STH   @02,SMWKSCBF(,R9)                                   0235 00401000
*   SMWKTQEA=ADDR(SMWKTQE);         /* PUT ADDR TQE IN WORK AREA FOR    00402000
*                                      SRB                           */ 00403000
         LA    @02,SMWKTQE(,R9)                                    0236 00404000
         ST    @02,SMWKTQEA(,R9)                                   0236 00405000
*   SMWKTQE=SMWKTQE&&SMWKTQE;       /* ZERO TQE FOR USE BY DIE       */ 00406000
         XC    SMWKTQE(128,R9),SMWKTQE(R9)                         0237 00407000
*   SMWKTSDS=ADDR(IEAVTSDS);        /* GET STEAL BACK ADDRESS FOR RSM*/ 00408000
         L     @02,@CA03297                                        0238 00409000
         ST    @02,SMWKTSDS+196(,R9)                               0238 00410000
*   END SUMINIT;                                                   0239 00411000
@EL00002 DS    0H                                                  0239 00412000
@EF00002 DS    0H                                                  0239 00413000
@ER00002 BR    @14                                                 0239 00414000
*                                                                  0240 00415000
*/********************************************************************/ 00416000
*/*   THIS SEGMENT ATTEMPTS TO OBTAIN THE SALLOC LOCK. IF THE LOCK   */ 00417000
*/*   IS ALREADY OWNED ON THIS CPU, BIT SCHFRRSL IS SET TO ZERO      */ 00418000
*/*   A RETURN CODE OF ZERO IS PASSED TO THE CALLER. IF THE LOCK IS  */ 00419000
*/*   NOT ALREADY OWNED A TEST IS MADE TO SEE IF ANY HIGHER LEVEL    */ 00420000
*/*   LOCKS ARE HELD. IF THERE ARE (IE DISPATCHER OR ASM HELD),      */ 00421000
*/*   THE SALLOC LOCK IS OBTAINED CONDITIONALLY. IF NO HIGHER LEVEL  */ 00422000
*/*   LOCKS ARE HELD, THE SALLOC IS OBTAINED UNCONDITIONALLY. IF THE */ 00423000
*/*   SALLOC LOCK IS OBTAINED BIT SCHFRRSL IS SET TO 1 AND A RETURN  */ 00424000
*/*   CODE OF ZERO IS PASSED TO THE CALLER. IF THE LOCK CANNOT BE    */ 00425000
*/*   OBTAINED RETURN CODE 8 IS RETURNED TO THE CALLER.              */ 00426000
*/*                                                                  */ 00427000
*/*   MACROS USED=SETLOCK                                            */ 00428000
*/*                                                                  */ 00429000
*/*   CALLED BY: TOP SEGMENT                                         */ 00430000
*/*                                                                  */ 00431000
*/********************************************************************/ 00432000
*                                                                  0240 00433000
*SUMLOCK:                                                          0240 00434000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0240 00435000
SUMLOCK  DS    0H                                                  0241 00436000
*   SMWKSMLK=R14;                   /* SAVE CALLERS RETURN ADDRESS   */ 00437000
         ST    R14,SMWKSMLK(,R9)                                   0241 00438000
*   R15=NULL;                       /* INITIALIZE RETURN CODE TO ZERO   00439000
*                                                            @ZM45054*/ 00440000
         SLR   R15,R15                                             0242 00441000
*   GEN REFS(PSALITA);                                             0243 00442000
*                          /*TEST TO SEE WHAT LOCKS ARE HELD @ZM43803*/ 00443000
         SETLOCK   TEST,TYPE=SALLOC,BRANCH=(HELD,SSDSLHLD)             X00444000
               RELATED=(SVCDUMP,IEAVTSSD(SUMLOCK))                      00445000
         SETLOCK   TEST,TYPE=DISP,BRANCH=(HELD,SSDCOND)                X00446000
               RELATED=(SVCDUMP,IEAVTSSD(SUMLOCK))                      00447000
         SETLOCK   TEST,TYPE=ASM,BRANCH=(HELD,SSDCOND)                 X00448000
               RELATED=(SVCDUMP,IEAVTSSD(SUMLOCK))                      00449000
GETLOCKU SETLOCK   OBTAIN,TYPE=SALLOC,MODE=UNCOND,REGS=USE,            X00450000
               RELATED=(SVCDUMP,IEAVTSSD(FRELOCKU))                     00451000
*   IF R15=NULL THEN                /* IF SALLOC LOCK WAS OBTAINED   */ 00452000
         LTR   R15,R15                                             0244 00453000
         BNZ   @RF00244                                            0244 00454000
*     SUMFRRSL='1'B;                /* TELL FRR THAT SALLOC WAS    0245 00455000
*                                      OBTAINED                      */ 00456000
         OI    SUMFRRSL(R8),B'00100000'                            0245 00457000
*   IF R15^=NOLOCK THEN             /* IF SALLOC WAS ALREADY HELD    */ 00458000
@RF00244 C     R15,@CF00036                                        0246 00459000
         BE    @RF00246                                            0246 00460000
*     R15=NULL;                     /* SET A ZERO RETURN CODE TO   0247 00461000
*                                      CALLER                        */ 00462000
         SLR   R15,R15                                             0247 00463000
*   GO TO SSDLKEND;                 /* BRANCH AROUND CONDITIONAL   0248 00464000
*                                      SETLOCK               @ZM43803*/ 00465000
*                                                                  0248 00466000
         B     SSDLKEND                                            0248 00467000
*   /*****************************************************************/ 00468000
*   /*                                                               */ 00469000
*   /* THIS SECTION OF CODE ATTEMPTS TO OBTAIN THE SALLOC LOCK       */ 00470000
*   /* CONDITIONALLY 1024 TIMES. A HIGHER LEVEL LOCK AS BEEN FOUND TO*/ 00471000
*   /* BE HELD BY THE TESTS ABOVE                            @ZM43803*/ 00472000
*   /*                                                               */ 00473000
*   /*****************************************************************/ 00474000
*                                                                  0249 00475000
*SSDCOND:                           /* LABEL BRANCHED TO IF HIGHER 0249 00476000
*                                      LEVEL LOCK IS HELD    @ZM43803*/ 00477000
*   R15=NOLOCK;                     /* INITIALIZE BAD RETURN CODE FOR   00478000
*                                      LOOP                  @ZM43803*/ 00479000
SSDCOND  LA    R15,8                                               0249 00480000
*   DO SMWKIDX1=ONE TO MAXLOOP WHILE(R15=NOLOCK);                  0250 00481000
         LA    @02,1                                               0250 00482000
         STH   @02,SMWKIDX1(,R9)                                   0250 00483000
@DL00250 C     R15,@CF00036                                        0250 00484000
         BNE   @DC00250                                            0250 00485000
*     GEN REFS(PSALITA);                                           0251 00486000
*                          /*OBTAIN SALLOC LOCK CONDITIONALLY @ZM43803* 00487000
GETLOCKC SETLOCK   OBTAIN,TYPE=SALLOC,MODE=COND,REGS=USE,              X00488000
               RELATED=(SVCDUMP,IEAVTSSD(FRELOCKU))                     00489000
*     IF R15=NULL THEN              /* IF SALLOC LOCK WAS OBTAINED   */ 00490000
         LTR   R15,R15                                             0252 00491000
         BNZ   @RF00252                                            0252 00492000
*       SUMFRRSL='1'B;              /* TELL FRR THAT SALLOC WAS    0253 00493000
*                                      OBTAINED                      */ 00494000
         OI    SUMFRRSL(R8),B'00100000'                            0253 00495000
*     IF R15^=NOLOCK THEN           /* IF SALLOC LOCK WAS ALREADY  0254 00496000
*                                      HELD                          */ 00497000
@RF00252 C     R15,@CF00036                                        0254 00498000
         BE    @RF00254                                            0254 00499000
*       R15=NULL;                   /* GIVE CALLER A ZERO RETURN CODE*/ 00500000
         SLR   R15,R15                                             0255 00501000
*   END;                            /* END LOOP TO OBTAIN SALLOC LOCK*/ 00502000
@RF00254 LA    @02,1                                               0256 00503000
         AH    @02,SMWKIDX1(,R9)                                   0256 00504000
         STH   @02,SMWKIDX1(,R9)                                   0256 00505000
         C     @02,@CF03180                                        0256 00506000
         BNH   @DL00250                                            0256 00507000
@DC00250 DS    0H                                                  0257 00508000
*SSDSLHLD:                          /* LABEL BRANCHED TO IF SALLOC 0257 00509000
*                                      ALREADY HELD          @ZM43803*/ 00510000
*   SMWKSLCL=ON;                    /* INDICATE CALLER HELD SALLOC 0257 00511000
*                                      LOCK ON ENTRY         @ZM45073*/ 00512000
SSDSLHLD OI    SMWKSLCL(R9),B'00001000'                            0257 00513000
*SSDLKEND:                          /* LABEL BRANCHED TO AFTER SALLOC   00514000
*                                      UNCOND                @ZM45073*/ 00515000
*   R14=SMWKSMLK;                   /* RESTORE CALLERS RETURN ADDRESS*/ 00516000
SSDLKEND L     R14,SMWKSMLK(,R9)                                   0258 00517000
*   END SUMLOCK;                                                   0259 00518000
@EL00003 DS    0H                                                  0259 00519000
@EF00003 DS    0H                                                  0259 00520000
@ER00003 BR    @14                                                 0259 00521000
*                                                                  0260 00522000
*/********************************************************************/ 00523000
*/*  THIS SEGMENT IS A SUBROUTINE USED BY THE SUMMARY DUMP TOP       */ 00524000
*/*  SEGMENT TO DECIDE WHAT STORAGE AREAS ARE TO BE MOVED TO THE     */ 00525000
*/*  REAL STORAGE BUFFER AND TO INVOKE OTHER SUMMARY DUMP SUBROUTINES*/ 00526000
*/*  TO HAVE THE DATA MOVED TO THE REAL STORAGE BUFFER. THE FOLLOWING*/ 00527000
*/*  AREAS ARE MOVED TO THE REAL STORAGE BUFFER, SPACE PERMITTING:   */ 00528000
*/*           - SUMMARY STORAGE LIST (SPECIFIED BY CALLER).          */ 00529000
*/*           - PCCA, LCCA AND PSA FOR ALL CPUS.                     */ 00530000
*/*           - SUPERVISOR TRACE TABLE (IF ACTIVE).                  */ 00531000
*/*           - THE RTM1WA , IHSA, AND SDWA IF ONE EXISTS.           */ 00532000
*/*           - THE SUPER FRR STACKS (THE NORMAL FRR STACK IS        */ 00533000
*/*             INCLUDED IN THE PSA DUMP)                            */ 00534000
*/*           - 2K OF STORAGE + AND - REGISTERS AT TIME OF ERROR     */ 00535000
*/*           - THE GLOBAL AND CPU WORK SAVE AREA VECTOR TABLES AND  */ 00536000
*/*             WORK SAVE AREAS                                      */ 00537000
*/*           - THE LOCAL WORK SAVE AREA VECTOR TABLE AND LOCAL WORK */ 00538000
*/*             SAVE AREA FOR THE CURRENT ADDRESS SPACE              */ 00539000
*/*           - 2K OF STORAGE + AND - THE INSTRUCTION COUNTER        */ 00540000
*/*             VALUES FOR THE EXTERNAL, PROGRAM CHECK, RESTART AND  */ 00541000
*/*             I/O OLD PSWS SAVED IN THE PCCAS FOR ALL CPUS         */ 00542000
*/*                                                                  */ 00543000
*/*  INPUT - SUMDUMP STORAGE LIST, SMWK, AREAS TO BE MOVED.          */ 00544000
*/*                                                                  */ 00545000
*/*  OUTPUT - REAL STORAGE BUFFER CONTAINS DUMPED DATA.              */ 00546000
*/*                                                                  */ 00547000
*/*  CALLED BY - TOP SEGMENT                                         */ 00548000
*/*                                                                  */ 00549000
*/*  ROUTINES CALLED - SUMLIST, SUMREGS, SUMDATA                     */ 00550000
*/*                                                                  */ 00551000
*/*  SEGMENTS INCLUDED: SUMCPU,SUMTRT,SUMRTM,SUMGWSA,SUMLWSA,SUMCWSA */ 00552000
*/*                     SUMPSWS                                      */ 00553000
*/********************************************************************/ 00554000
*                                                                  0260 00555000
*SUMDUMP:                                                          0260 00556000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0260 00557000
SUMDUMP  DS    0H                                                  0261 00558000
*   SMWKSDMP=R14;                   /* SAVE CALLERS RETURN ADDRESS   */ 00559000
         ST    R14,SMWKSDMP(,R9)                                   0261 00560000
*   SUMFRRRT=ADDR(SUMRTY05);        /* GET NEXT RETRY LOC ADDRESS  0262 00561000
*                                                            @ZA19201*/ 00562000
         LA    @02,SUMRTY05                                        0262 00563000
         ST    @02,SUMFRRRT(,R8)                                   0262 00564000
*   RFY                                                            0263 00565000
*    (R10,                                                         0263 00566000
*     R11) RSTD;                    /* SAVE NEEDED REGISTERS @ZA19201*/ 00567000
*   SMDLRID=SMDASIDR;               /* PUT ASID RECORD ID IN HEADER     00568000
*                                                            @ZA19201*/ 00569000
         LA    @02,656                                             0264 00570000
         ALR   @02,R9                                              0264 00571000
         MVC   SMDLRID(2,@02),@CH01370                             0264 00572000
*   SMDLRLEN=TWO;                   /* PUT ASID LENGTH IN HEADER   0265 00573000
*                                                            @ZA19201*/ 00574000
         MVC   SMDLRLEN(4,@02),@CF00107                            0265 00575000
*   SMDLRADR=ADDR(ASCBASID);        /* PUT ASID ADDRESS IN HEADER  0266 00576000
*                                                            @ZA19201*/ 00577000
         L     @07,PSAPTR                                          0266 00578000
         L     @07,ASCBPTR(,@07)                                   0266 00579000
         LA    @00,ASCBASID(,@07)                                  0266 00580000
         ST    @00,SMDLRADR(,@02)                                  0266 00581000
*   CALL SUMDATA;                   /* MOVE THIS ASID TO BUFFER    0267 00582000
*                                                            @ZA19201*/ 00583000
         BAL   @14,SUMDATA                                         0267 00584000
*SUMRTY05:                                                         0268 00585000
*   SUMFRRRT=ADDR(SUMRTY10);        /* GET NEXT RETRY LOC ADDRESS  0268 00586000
*                                                            @ZA19201*/ 00587000
SUMRTY05 LA    @02,SUMRTY10                                        0268 00588000
         ST    @02,SUMFRRRT(,R8)                                   0268 00589000
*   RFY                                                            0269 00590000
*     SDUMP BASED(RTCTSDPL);        /* MAP CALLER PARAMETER LIST     */ 00591000
*   IF SDUSULST=ON THEN             /* IF SUMLIST KEYWORD WAS      0270 00592000
*                                      SPECIFIED                     */ 00593000
         L     @02,PSAPTR                                          0270 00594000
         L     @02,CVTPTR(,@02)                                    0270 00595000
         L     @02,CVTRTMCT(,@02)                                  0270 00596000
         L     @02,RTCTSDPL(,@02)                                  0270 00597000
         TM    SDUSULST(@02),B'00001000'                           0270 00598000
         BNO   @RF00270                                            0270 00599000
*     CALL SUMLIST;                 /* MOVE SUMLIST STORAGE TO BUFFER*/ 00600000
         BAL   @14,SUMLIST                                         0271 00601000
*SUMRTY10:                                                         0272 00602000
*   SUMFRRRT=ADDR(SUMRTY20);        /* GET NEXT RETRY LOCATION     0272 00603000
*                                      ADDRESS                       */ 00604000
@RF00270 DS    0H                                                  0272 00605000
SUMRTY10 LA    @02,SUMRTY20                                        0272 00606000
         ST    @02,SUMFRRRT(,R8)                                   0272 00607000
*                                                                  0273 00608000
*/********************************************************************/ 00609000
*/*   THIS SEGMENT MOVES THE PCCA, LCCA, AND PSA FOR ALL ALIVE CPUS  */ 00610000
*/*   TO THE REAL STORAGE BUFFER                                     */ 00611000
*/*                                                                  */ 00612000
*/*   INCLUDED BY: SUMDUMP                                           */ 00613000
*/*                                                                  */ 00614000
*/********************************************************************/ 00615000
*                                                                  0273 00616000
*   /*****************************************************************/ 00617000
*   /*                                                               */ 00618000
*   /* THIS SECTION MOVES THE CURRENT PSA TO THE REAL BUFFER         */ 00619000
*   /*                                                               */ 00620000
*   /*****************************************************************/ 00621000
*                                                                  0273 00622000
*   SMDLRID=SMDPSA;                 /* PUT PSA ID IN HEADER          */ 00623000
         LA    @02,656                                             0273 00624000
         ALR   @02,R9                                              0273 00625000
         MVC   SMDLRID(2,@02),@CH00033                             0273 00626000
*   SMDLRLEN=LENGTH(PSA);           /* PUT LENGTH OF PSA IN HEADER   */ 00627000
         MVC   SMDLRLEN(4,@02),@CF03187                            0274 00628000
*   SMDLRADR=ZERO;                  /* PUT CURRENT PSA ADDRESS IN  0275 00629000
*                                      HEADER                        */ 00630000
         SLR   @00,@00                                             0275 00631000
         ST    @00,SMDLRADR(,@02)                                  0275 00632000
*   CALL SUMDATA;                   /* MOVE CURRENT PSA TO THE BUFFER*/ 00633000
*                                                                  0276 00634000
         BAL   @14,SUMDATA                                         0276 00635000
*   /*****************************************************************/ 00636000
*   /*                                                               */ 00637000
*   /* THIS SECTION CHECKS EACH CPU TO SEE IF IT IS ALIVE. IF THE CPU*/ 00638000
*   /* IS ALIVE, ITS PCCA LCCA AND PSA(IF NOT CURRENT) ARE MOVED TO  */ 00639000
*   /* THE REAL STORAGE BUFFER.                              @ZM44941*/ 00640000
*   /*                                                               */ 00641000
*   /*****************************************************************/ 00642000
*                                                                  0277 00643000
*   DO SMWKIDX1=1 TO CPUMAX WHILE(SMWKRBFL=OFF);/* PROCESS ALL CPUS  */ 00644000
         LA    @02,1                                               0277 00645000
         STH   @02,SMWKIDX1(,R9)                                   0277 00646000
@DL00277 TM    SMWKRBFL(R9),B'00100000'                            0277 00647000
         BNZ   @DC00277                                            0277 00648000
*     IF PCCAT00P(SMWKIDX1)^=NULL THEN/* PCCA EXISTS FOR THIS CPU    */ 00649000
         LH    @02,SMWKIDX1(,R9)                                   0278 00650000
         SLA   @02,2                                               0278 00651000
         L     @07,PSAPTR                                          0278 00652000
         L     @07,CVTPTR(,@07)                                    0278 00653000
         L     @07,CVTPCCAT(,@07)                                  0278 00654000
         AL    @07,@CF03427                                        0278 00655000
         L     @02,PCCAT00P(@02,@07)                               0278 00656000
         LTR   @02,@02                                             0278 00657000
         BZ    @RF00278                                            0278 00658000
*       DO;                         /* DUMP PCCA FOR THIS CPU        */ 00659000
*         SMDLRID=SMDPCCA;          /* PUT PCCA ID IN HEADER         */ 00660000
         LA    @07,656                                             0280 00661000
         ALR   @07,R9                                              0280 00662000
         MVC   SMDLRID(2,@07),@CH00067                             0280 00663000
*         SMDLRLEN=LENGTH(PCCA);    /* PUT LENGTH OF PCCA IN HEADER  */ 00664000
         MVC   SMDLRLEN(4,@07),@CF03414                            0281 00665000
*         SMDLRADR=PCCAT00P(SMWKIDX1);/* ADDR OF PCCA IN HEADER      */ 00666000
         ST    @02,SMDLRADR(,@07)                                  0282 00667000
*         CALL SUMDATA;             /* MOVE THIS PCCA TO THE BUFFER  */ 00668000
         BAL   @14,SUMDATA                                         0283 00669000
*       END;                        /* END OF DUMP PCCA FOR THIS CPU */ 00670000
*     IF LCCAT00P(SMWKIDX1)^=NULL&  /* IF LCCA EXISTS FOR THIS CPU   */ 00671000
*         SMWKRBFL=OFF THEN         /* AND REAL BUFFER IS STILL NOT     00672000
*                                      FULL                          */ 00673000
