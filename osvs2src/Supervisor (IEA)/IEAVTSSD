*                                                                  0197 00266000
@RC00192 DS    0H                                                  0198 00267000
*       /*************************************************************/ 00268000
*       /*                                                           */ 00269000
*       /* RELEASE SALLOC LOCK IF OBTAINED                           */ 00270000
*       /*                                                           */ 00271000
*       /*************************************************************/ 00272000
*                                                                  0198 00273000
*SUMUNLCK:                                                         0198 00274000
*       SUMFRRRT=NULL;              /* NO RETRY LOCATION ADDRESS     */ 00275000
@RF00164 DS    0H                                                  0198 00276000
SUMUNLCK SLR   @02,@02                                             0198 00277000
         ST    @02,SUMFRRRT(,R8)                                   0198 00278000
*       IF SUMFRRSL=ON THEN         /* IF SDUMP OBTAINED SALLOC LOCK */ 00279000
         TM    SUMFRRSL(R8),B'00100000'                            0199 00280000
         BNO   @RF00199                                            0199 00281000
*         DO;                       /* FREE SALLOC LOCK              */ 00282000
*           GEN REFS(PSALITA);                                     0201 00283000
*                                     /*RELEASE SALLOC LOCK          */ 00284000
FREELOCK SETLOCK   RELEASE,TYPE=SALLOC,REGS=USE,                       X00285000
               RELATED=(SUMDUMP,IEAVTSSD(GETLOCK))                      00286000
*           SUMFRRSL=OFF;           /* INDICATE SUM DUMP DOES NOT OWN   00287000
*                                      SALLOC                        */ 00288000
         NI    SUMFRRSL(R8),B'11011111'                            0202 00289000
*         END;                      /* END OF FREE SALLOC LOCK       */ 00290000
*     END;                          /* END LOCK WAS OBTAINED OR    0204 00291000
*                                      ALREADY OWNED                 */ 00292000
*   ELSE                                                           0205 00293000
*     DO;                           /* SALLOC LOCK WAS NOT OBTAINED  */ 00294000
         B     @RC00153                                            0205 00295000
@RF00153 DS    0H                                                  0206 00296000
*       RTCTSDPR=PARTDUMP;          /* THIS MEANS A PARTIAL SVC DUMP */ 00297000
