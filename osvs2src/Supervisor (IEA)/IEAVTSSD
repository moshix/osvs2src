*             SMWKRSNM=ON;          /* INDICATE SOME STORAGE AREAS 0632 02303000
*                                      NOT MOVED                     */ 02304000
         OI    SMWKRSNM(R9),B'00010000'                            0632 02305000
*           END;                    /* END REAL BUFFER IS NOT FULL   */ 02306000
*       END;                        /* END OF OBTAIN NEXT REAL FRAME */ 02307000
@RC00621 DS    0H                                                  0635 02308000
*     RFY                                                          0635 02309000
*      (R4,                                                        0635 02310000
*       R5,                                                        0635 02311000
*       R11,                                                       0635 02312000
*       R2) RSTD;                                                  0635 02313000
@RF00619 DS    0H                                                  0636 02314000
*     IF SMWKSCBF>ZERO THEN         /* IF CURRENT FRAME HAS SPACE    */ 02315000
         LH    @00,SMWKSCBF(,R9)                                   0636 02316000
         LTR   @00,@00                                             0636 02317000
         BNP   @RF00636                                            0636 02318000
*       DO;                         /* MOVE VIRTUAL AREA TO FRAME    */ 02319000
*         R4=SMWKRFAD;              /* TO ADDRESS IS REAL FRAME    0638 02320000
*                                      ADDRESS                       */ 02321000
         L     R4,SMWKRFAD(,R9)                                    0638 02322000
*         IF R7>SMWKSCBF THEN       /* IF AREA TO MOVE IS LARGE THAN    02323000
