@EF00012 DS    0H                                                  0786 02871000
@ER00012 BR    @14                                                 0786 02872000
*                                                                  0787 02873000
*/* ******************************************************************/ 02874000
*/*    THIS SEGMENT IS THE SUMMARY DUMP STEAL BACK ROUTINE. IT       */ 02875000
*/*    IS A SECONDARY ENTRY POINT IN THE IEAVTSSD LOAD MODULE        */ 02876000
*/*    THIS ROUTINE IS CALLED BY RSM WHEN RSM FINDS IT HAS AN        */ 02877000
*/*    IMMEDIATE REAL STORAGE NEED AND A SUMMARY DUMP REAL STORAGE   */ 02878000
*/*    BUFFER EXISTS. RSM WILL CALL THIS ENTRY POINT, THUS           */ 02879000
*/*    INFORMING SUMDUMP THAT IT IS 'STEALING' BACK A REAL FRAME     */ 02880000
*/*    THIS SEGMENT WILL REARRANGE THE SUMDUMP AND UPDATE SUMDUMPS   */ 02881000
*/*    POINTERS TO LEAVE THE DATA WHICH REMAINS IN THE BUFFER IN     */ 02882000
*/*    A USABLE FORM. NOTE: THIS CODE RUNS DISABLED WITH THE         */ 02883000
*/*    SALLOC LOCK HELD.                                             */ 02884000
*/*    INPUT: R1 HAS REAL ADDRESS OF FRAM TO BE 'STOLEN' BACK        */ 02885000
*/*           SMWKRSIX HAS INDEX TO LAST FRAME IN USE                */ 02886000
*/*           R13 HAS REGISTER SAVE AREA ADDRESS                     */ 02887000
*/*           SALLOC LOCK IS HELD ON ENTRY                           */ 02888000
*/*                                                                  */ 02889000
*/*    OUTPUT: SUMDUMP REARRANGED AND COUNTERS AND POINTERS UPDATED  */ 02890000
*/*                                                                  */ 02891000
*/*    INCLUDED BY: TOP SEGMENT                                      */ 02892000
*/*                                                                  */ 02893000
*/* ******************************************************************/ 02894000
*                                                                  0787 02895000
*IEAVTSDS:                                                         0787 02896000
*   ENTRY;                                                         0787 02897000
@EP00787 DS    0H                                                  0788 02898000
*   R9=RTCTSDSW;                    /* ADDRESS THE SUMDUMP WORK AREA */ 02899000
         L     @02,PSAPTR                                          0788 02900000
         L     @02,CVTPTR(,@02)                                    0788 02901000
         L     @02,CVTRTMCT(,@02)                                  0788 02902000
         L     R9,RTCTSDSW(,@02)                                   0788 02903000
*   SMWKSTBK=ON;                    /* INDICATE A STEAL BACK HAS   0789 02904000
*                                      OCCURRED              @ZM45073*/ 02905000
         OI    SMWKSTBK(R9),B'00000100'                            0789 02906000
*   SMWKNSTB=SMWKNSTB+1;            /* INCREMENT STEAL BACK COUNTER     02907000
*                                                            @ZM45073*/ 02908000
         LA    @02,1                                               0790 02909000
         AH    @02,SMWKNSTB(,R9)                                   0790 02910000
         STH   @02,SMWKNSTB(,R9)                                   0790 02911000
*   RFY                                                            0791 02912000
*    (R10,                                                         0791 02913000
*     R11,                                                         0791 02914000
*     R8) RSTD;                                                    0791 02915000
*   RFY                                                            0792 02916000
*     SUMFRR6W BASED(R8);           /* MAP THE 6 WORD FRR WORK AREA  */ 02917000
*                                                                  0792 02918000
*   /*****************************************************************/ 02919000
*   /*                                                               */ 02920000
*   /* ESTABLISH A RECOVERY EXIT - FRR WORK AREA INIT BY RTM         */ 02921000
*   /*                                                               */ 02922000
*   /*****************************************************************/ 02923000
*                                                                  0793 02924000
*   GEN REFS(SUMFRRAD,PSA,PSACSTK,FRRS,FRRSCURR,FRRSLAST,PSALSFCC, 0793 02925000
*       FRRSFRRA,FRRSPARM,FRRSENTR);                               0793 02926000
         SETFRR    A,FRRAD=SUMFRRAD,                                   X02927000
               PARMAD=(R8),WRKREGS=(R10,R11)                            02928000
*   RFY                                                            0794 02929000
*    (R10,                                                         0794 02930000
*     R11) UNRSTD;                                                 0794 02931000
*   SUMFRRBR=R12;                   /* SAVE THE BASE REGISTER      0795 02932000
*                                      CONTENTS                      */ 02933000
         ST    R12,SUMFRRBR(,R8)                                   0795 02934000
*   SUMFRRSA=R13;                   /* SAVE THE CALLERS SAVE AREA  0796 02935000
*                                      ADDR                          */ 02936000
         ST    R13,SUMFRRSA(,R8)                                   0796 02937000
*   R15=NOLOCK;                     /* INITIALIZE FOR A BAD RETURN 0797 02938000
*                                      CODE                          */ 02939000
*                                                                  0797 02940000
         LA    R15,8                                               0797 02941000
*   /*****************************************************************/ 02942000
*   /*                                                               */ 02943000
*   /* THIS LOOP SEARCHES FOR THE TABLE ENTRY OF THE FRAME TO BE     */ 02944000
*   /* STOLEN                                                        */ 02945000
*   /*                                                               */ 02946000
*   /*****************************************************************/ 02947000
*                                                                  0798 02948000
*   DO SMWKIDX1=1 TO DIM(SMWKFRMA) WHILE(R1^=SMWKFRMA(SMWKIDX1));  0798 02949000
         LA    @02,1                                               0798 02950000
         STH   @02,SMWKIDX1(,R9)                                   0798 02951000
@DL00798 LH    @02,SMWKIDX1(,R9)                                   0798 02952000
         SLA   @02,2                                               0798 02953000
         C     R1,SMWKFRMA+192(@02,R9)                             0798 02954000
         BE    @DC00798                                            0798 02955000
*   END;                            /* END OF SEARCH FOR TABLE ENTRY */ 02956000
         LA    @02,1                                               0799 02957000
         AH    @02,SMWKIDX1(,R9)                                   0799 02958000
         STH   @02,SMWKIDX1(,R9)                                   0799 02959000
         C     @02,@CF01387                                        0799 02960000
         BNH   @DL00798                                            0799 02961000
@DC00798 DS    0H                                                  0800 02962000
*   IF SMWKIDX1<=DIM(SMWKFRMA) THEN /* IF THE STOLEN FRAME WAS FOUND */ 02963000
         LH    @02,SMWKIDX1(,R9)                                   0800 02964000
         C     @02,@CF01387                                        0800 02965000
         BH    @RF00800                                            0800 02966000
*     DO;                           /* REARRANGE FRAME CONTENTS      */ 02967000
*       SUMFRRDP=ON;                /* YES SO END LOOP               */ 02968000
         OI    SUMFRRDP(R8),B'00010000'                            0802 02969000
*       IF SMWKIDX1^=SMWKRSIX THEN  /* IS STOLEN BACK FRAME LAST     */ 02970000
         LH    @11,SMWKRSIX(,R9)                                   0803 02971000
         CR    @02,@11                                             0803 02972000
         BE    @RF00803                                            0803 02973000
*         DO;                       /* NOT LAST-MOVE IS NECESSARY    */ 02974000
*           SMWKPSW=SMWKPSW&&SMWKPSW;/* INIT PSW FOR LPSW USE        */ 02975000
         XC    SMWKPSW(8,R9),SMWKPSW(R9)                           0805 02976000
*           RFY                                                    0806 02977000
*            (R4,                                                  0806 02978000
*             R5,                                                  0806 02979000
*             R6,                                                  0806 02980000
*             R7,                                                  0806 02981000
*             R2,                                                  0806 02982000
*             R10) RSTD;                                           0806 02983000
*           R5=FOURK;               /* LENGTH TO AREA IS 4096        */ 02984000
         L     @00,@CF03187                                        0807 02985000
         LR    R5,@00                                              0807 02986000
*           R7=FOURK;               /* LENGTH OF FROM AREA IS 4096   */ 02987000
         LR    R7,@00                                              0808 02988000
*           R6=SMWKFRMA(SMWKIDX1);  /* STOLEN FRAME FROM ADDR        */ 02989000
         LH    @00,SMWKIDX1(,R9)                                   0809 02990000
         SLA   @00,2                                               0809 02991000
         LA    @11,196                                             0809 02992000
         ALR   @11,R9                                              0809 02993000
         LR    R6,@00                                              0809 02994000
         L     R6,SMWKFRMA-4(R6,@11)                               0809 02995000
*           R4=SMWKFRMA(SMWKRSIX);  /* CONTENTS MOVED TO LOWEST    0810 02996000
*                                      PRIORITY REAL FRAME           */ 02997000
*                                                                  0810 02998000
         LH    @00,SMWKRSIX(,R9)                                   0810 02999000
         SLA   @00,2                                               0810 03000000
         LR    R4,@00                                              0810 03001000
         L     R4,SMWKFRMA-4(R4,@11)                               0810 03002000
*           /*********************************************************/ 03003000
*           /*                                                       */ 03004000
*           /* THIS SECTION INITIALIZES THE MODEL PSW, TURNS DAT OFF,*/ 03005000
*           /* MOVES THE DATA FROM THE STOLER FRAME AND TURNS DAT ON */ 03006000
*           /*                                                       */ 03007000
*           /*********************************************************/ 03008000
*                                                                  0811 03009000
*           SMWKPSWK='0C'X;         /* PUT KEY VALUES IN MODEL PSW   */ 03010000
         MVI   SMWKPSWK(R9),X'0C'                                  0811 03011000
*           GEN REFS(SMWKPSW,SMWKPSWS,SMWKPSWK,SMWKPSWA);          0812 03012000
         STOSM SMWKPSWS(R9),X'00'  GET CURRENT SYSTEM MASK IN MODEL PSW 03013000
         NI    SMWKPSWS(9),X'FB'     TURN OFF DAT IN MODEL PSW          03014000
         LRA   R2,SMSTBGIN      GET REAL ADDR OF BEGIN OF DAT OFF       03015000
         ST    R2,SMWKPSWA(R9) PUT REAL ADDR IN MODEL PSW               03016000
         LA    R2,SMSTRTRN      GET VIRTUAL ADDRESS OF RETURN POINT     03017000
         LRA   R10,SMWKPSW(R9)  GET REAL ADDRESS OF MODEL PSW           03018000
         LPSW  SMWKPSW(R9)        TURN DAT OFF AND GO TO SMSTBGIN       03019000
SMSTBGIN MVCL  R4,R6              MOVE FRAME CONTENTS                   03020000
         OI    0(R10),X'04'       TURN DAT ON IN MODEL PSW              03021000
         ST    R2,4(R10)      PUT RETURN VIRTUAL ADDRESS IN MOD PSW     03022000
         LPSW  0(R10)             TURN DAT ON AND RETURN TO SMSTRTRN    03023000
SMSTRTRN EQU   *                  LABEL FOR RETURN ADDRESS              03024000
*           RFY                                                    0813 03025000
*            (R5,                                                  0813 03026000
*             R4,                                                  0813 03027000
*             R7,                                                  0813 03028000
*             R6,                                                  0813 03029000
*             R2,                                                  0813 03030000
*             R10) UNRSTD;                                         0813 03031000
*           SMWKFRMA(SMWKIDX1)=SMWKFRMA(SMWKRSIX);/* EXCHANGE ENTRY  */ 03032000
         LH    @02,SMWKIDX1(,R9)                                   0814 03033000
         SLA   @02,2                                               0814 03034000
         LA    @11,196                                             0814 03035000
         ALR   @11,R9                                              0814 03036000
         LH    @10,SMWKRSIX(,R9)                                   0814 03037000
         SLA   @10,2                                               0814 03038000
         L     @00,SMWKFRMA-4(@10,@11)                             0814 03039000
         ST    @00,SMWKFRMA-4(@02,@11)                             0814 03040000
*         END;                      /* END OF A MOVE IS NECESSARY    */ 03041000
*       SMWKFRMA(SMWKRSIX)=NULL;    /* ZERO BOTTOM LIST ENTRY        */ 03042000
@RF00803 LH    @02,SMWKRSIX(,R9)                                   0816 03043000
         LR    @11,@02                                             0816 03044000
         SLA   @11,2                                               0816 03045000
         LA    @10,196                                             0816 03046000
         ALR   @10,R9                                              0816 03047000
         SLR   @00,@00                                             0816 03048000
         ST    @00,SMWKFRMA-4(@11,@10)                             0816 03049000
*       SMWKFRUS=SMWKFRUS-ONE;      /* ONE LESS FRAME IN USE         */ 03050000
         LH    @00,SMWKFRUS(,R9)                                   0817 03051000
         BCTR  @00,0                                               0817 03052000
         STH   @00,SMWKFRUS(,R9)                                   0817 03053000
*       SMWKFRHD=SMWKFRHD-ONE;      /* ONE LESS FRAME HELD           */ 03054000
         LH    @00,SMWKFRHD(,@10)                                  0818 03055000
         N     @00,@CF03420                                        0818 03056000
         BCTR  @00,0                                               0818 03057000
         STH   @00,SMWKFRHD(,@10)                                  0818 03058000
*       SMWKRSIX=SMWKRSIX-ONE;      /* MOVE CURRENT LAST INDEX UP    */ 03059000
         BCTR  @02,0                                               0819 03060000
         STH   @02,SMWKRSIX(,R9)                                   0819 03061000
*       IF SMWKFRHD=ZERO THEN       /* WAS THIS THE LAST HELD FRAME  */ 03062000
         LTR   @00,@00                                             0820 03063000
         BNZ   @RF00820                                            0820 03064000
*         DO;                       /* YES DO LAST FRAME PROCESSING  */ 03065000
*           SMWKRBUF=OFF;           /* IND SUM DUMP DOESNT HOLD    0822 03066000
*                                      BUFFER                        */ 03067000
         NI    SMWKRBUF(R9),B'10111111'                            0822 03068000
*           RTCTSDRW=OFF;           /* IND NO SUM DUMP TO WRITE      */ 03069000
         L     @02,PSAPTR                                          0823 03070000
         L     @02,CVTPTR(,@02)                                    0823 03071000
         L     @02,CVTRTMCT(,@02)                                  0823 03072000
         NI    RTCTSDRW(@02),B'11111101'                           0823 03073000
*         END;                      /* END OF LAST FRAME PROCESSING  */ 03074000
*       R15=NULL;                   /* INDICATE STEAL BACK SUCCESSUFL*/ 03075000
@RF00820 SLR   R15,R15                                             0825 03076000
*     END;                          /* END OF REARRANGE TABLE ENTRIES*/ 03077000
*   RFY                                                            0827 03078000
*    (R10,                                                         0827 03079000
*     R11) RSTD;                                                   0827 03080000
*                                                                  0827 03081000
@RF00800 DS    0H                                                  0828 03082000
*   /*****************************************************************/ 03083000
*   /*                                                               */ 03084000
*   /* DELETE RECOVERY EXIT - FRR                                    */ 03085000
*   /*                                                               */ 03086000
*   /*****************************************************************/ 03087000
*                                                                  0828 03088000
*   GEN REFS(PSA,PSACSTK,FRRS,FRRSCURR,FRRSEMP,FRRSELEN);          0828 03089000
         SETFRR  D,WRKREGS=(R10,R11)                                    03090000
*   RFY                                                            0829 03091000
*    (R10,                                                         0829 03092000
*     R11) UNRSTD;                                                 0829 03093000
*   RETURN CODE(R15);               /* RETURN TO RSM                 */ 03094000
         L     @14,12(,@13)                                        0830 03095000
         LM    @00,@12,20(@13)                                     0830 03096000
         BR    @14                                                 0830 03097000
*   DCL                                                            0831 03098000
*     PATCHLOC FIXED(31) STATIC;                                   0831 03099000
