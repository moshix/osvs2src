*   SDWAHEX='1'B;                   /* VARIABLE RECORDING AREA IN HEX*/ 02804000
         OI    SDWAHEX(R1),B'10000000'                             0762 02805000
*   SDWAURAL=LENGTH(SUMFRR6W);      /* LENGTH OF VARIABLE AREA TO  0763 02806000
