*                                      INTERVAL                      */ 00369000
         STH   @02,SMWKNFRI(,R9)                                   0224 00370000
*   SMWKNFRE=ZERO;                  /* ZERO NUMBER OF FRAMES TO BE 0225 00371000
*                                      FREED                         */ 00372000
         STH   @02,SMWKNFRE(,R9)                                   0225 00373000
*   SMWKNDIE=ZERO;                  /* ZERO COUNTER TIMES ENTERED DIE*/ 00374000
         STH   @02,SMWKNDIE(,R9)                                   0226 00375000
*   SMWKNFRQ=ZERO;                  /* ZERO NUMBER FRAMES REQUESTED     00376000
