*     SDWACSCT=ENTNAME;             /* IEAVTSDS ENTRY POINT NAME FOR    02795000
*                                      RECORD                        */ 02796000
         MVC   SDWACSCT(8,R1),@CC03213                             0759 02797000
*   ELSE                                                           0760 02798000
*     SDWACSCT=CSECTNAM;            /* IEAVTSSD CSECT NAME FOR RECORD*/ 02799000
         B     @RC00758                                            0760 02800000
