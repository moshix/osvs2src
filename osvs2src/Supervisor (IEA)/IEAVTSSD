         AL    @00,SMWKRFAD(,R9)                                   0643 02344000
         ST    @00,SMWKRFAD(,R9)                                   0643 02345000
*         /***********************************************************/ 02346000
*         /*                                                         */ 02347000
*         /* THIS SECTION TURNS DAT OFF VIA LPSW LOGIC, MOVES THE    */ 02348000
*         /* DATA TO THE REAL STORAGE BUFFER, AND TURNS DAT ON USING */ 02349000
*         /* LPSW LOGIC                                              */ 02350000
*         /*                                                         */ 02351000
*         /***********************************************************/ 02352000
*                                                                  0644 02353000
*         GEN REFS(SMWKPSW,SMWKPSWS,SMWKPSWA);                     0644 02354000
         NI    SMWKPSWS(R9),X'FB'  TURN DAT OFF IN MODEL PSW            02355000
         LRA   R2,SMMVBGIN        GET REAL ADDRESS OF MVCL              02356000
         ST    R2,SMWKPSWA(R9)    PUT BEGIN ADDRESS IN PSW              02357000
         LRA   R2,SMWKPSW(R9)     GET REAL ADDRESS OF MODEL PSW         02358000
         LA    R11,SMMVRTRN         GET ADDRESS OF RETURN POINT         02359000
         LPSW  SMWKPSW(R9)            SHUT DAT OFF AND MOVE DATA        02360000
SMMVBGIN MVCL  R4,R6                  MOVE DATA TO THE BUFFER           02361000
         ST    R11,4(R2)        PUT RETURN ADDRESS IN MODEL PSW         02362000
         OI    0(R2),X'04'        TURN DAT ON IN THE MODEL PSW          02363000
         LPSW  0(R2)       TURN DAT ON - CONTROL TO SMMVRTRN            02364000
SMMVRTRN EQU   *                      LABEL FOR RETURN ADDRESS          02365000
*       END;                        /* END OF MOVE VIRTUAL AREA TO 0645 02366000
*                                      FRAME                         */ 02367000
*     RFY                                                          0646 02368000
*      (R4,                                                        0646 02369000
*       R5,                                                        0646 02370000
*       R11,                                                       0646 02371000
*       R2) UNRSTD;                                                0646 02372000
@RF00636 DS    0H                                                  0647 02373000
