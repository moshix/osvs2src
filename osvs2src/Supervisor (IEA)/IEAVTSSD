         L     @02,CVTPTR(,@02)                                    0208 00305000
         L     @02,CVTRTMCT(,@02)                                  0208 00306000
         OI    RTCTSDSC(@02),B'00000001'                           0208 00307000
*   R13=SUMFRRSA;                   /* RESTORE CALLERS SAVE AREA ADDR*/ 00308000
         L     R13,SUMFRRSA(,R8)                                   0209 00309000
*   RFY                                                            0210 00310000
*    (R10,                                                         0210 00311000
*     R11) RSTD;                                                   0210 00312000
*                                                                  0210 00313000
*   /*****************************************************************/ 00314000
*   /*                                                               */ 00315000
*   /* DELETE THE RECOVERY EXIT FRR                                  */ 00316000
*   /*                                                               */ 00317000
*   /*****************************************************************/ 00318000
*                                                                  0211 00319000
*   GEN REFS(PSA,PSACSTK,FRRS,FRRSCURR,FRRSEMP,FRRSELEN,FRRSCURR); 0211 00320000
    SETFRR    D,WRKREGS=(R10,R11)                                       00321000
*   RFY                                                            0212 00322000
*    (R10,                                                         0212 00323000
*     R11) UNRSTD;                                                 0212 00324000
*   RETURN CODE(0);                 /* RETURN TO CALLER              */ 00325000
         SLR   @15,@15                                             0213 00326000
         L     @14,12(,@13)                                        0213 00327000
         LM    @00,@12,20(@13)                                     0213 00328000
         BR    @14                                                 0213 00329000
*                                                                  0214 00330000
*/********************************************************************/ 00331000
*/*   THIS SEGMENT INITIALIZES KEY FIELDS OF THE SUMMARY DUMP WORK   */ 00332000
*/*   AREA TO ZERO.  THIS MAKES THE VALUE OF ALL OF THESE FIELDS     */ 00333000
*/*   KNOWN PRIOR TO ANY SUMMARY DUMP PROCESSING                     */ 00334000
*/*                                                                  */ 00335000
*/*   INPUT: UNINITIALIZED SUMMARY DUMP WORK AREA                    */ 00336000
*/*                                                                  */ 00337000
*/*   OUTPUT: INITIALIZED SUMMARY DUMP WORK AREA                     */ 00338000
*/*                                                                  */ 00339000
*/*   INCLUDED BY: TOP SEGMENT                                       */ 00340000
*/********************************************************************/ 00341000
*                                                                  0214 00342000
*SUMINIT:                                                          0214 00343000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0214 00344000
SUMINIT  DS    0H                                                  0215 00345000
*   SMWKPSW=SMWKPSW&&SMWKPSW;       /* ZERO MODULE PSW FOR LPSW      */ 00346000
         XC    SMWKPSW(8,R9),SMWKPSW(R9)                           0215 00347000
*   SMWKRSM(1:256)=SMWKRSM(1:256)&&SMWKRSM(1:256);/* ZERO FIRST PART    00348000
*                                      OF RSM PART OF WORK AREA      */ 00349000
         XC    SMWKRSM(256,R9),SMWKRSM(R9)                         0216 00350000
*   SMWKRSM(257:LENGTH(SMWKRSM))=SMWKRSM(257:LENGTH(SMWKRSM))&&SMWKRSM( 00351000
*       257:LENGTH(SMWKRSM));       /* ZERO REST OF RSM AREA         */ 00352000
         XC    SMWKRSM+256(160,R9),SMWKRSM+256(R9)                 0217 00353000
*   SMWKFLGS=SMWKFLGS&&SMWKFLGS;    /* ZERO FLAG BYTE AREAS          */ 00354000
         XC    SMWKFLGS(4,R9),SMWKFLGS(R9)                         0218 00355000
*   SMWKRSIX=ZERO;                  /* ZERO STORAGE INDEX FIELD      */ 00356000
         SLR   @02,@02                                             0219 00357000
         STH   @02,SMWKRSIX(,R9)                                   0219 00358000
*   SMWKRFAD=ZERO;                  /* ZERO REAL FRAME ADDRESS FIELD */ 00359000
         ST    @02,SMWKRFAD(,R9)                                   0220 00360000
*   SMWKNFAV=ZERO;                  /* ZERO NUMBER OF FRAMES AVAIL 0221 00361000
*                                      COUNT                         */ 00362000
         STH   @02,SMWKNFAV(,R9)                                   0221 00363000
*   SMWKFRUS=ZERO;                  /* ZERO NUMBER FRAMES USED COUNT */ 00364000
         STH   @02,SMWKFRUS(,R9)                                   0222 00365000
*   SMWKREGN=ZERO;                  /* ZERO NUMBER OF SAVED REGISTERS*/ 00366000
         STH   @02,SMWKREGN(,R9)                                   0223 00367000
