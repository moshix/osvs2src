         BZ    @RF00285                                            0285 00682000
         TM    SMWKRBFL(R9),B'00100000'                            0285 00683000
         BNZ   @RF00285                                            0285 00684000
*       DO;                         /* DUMP LCCA FOR THIS CPU        */ 00685000
*         SMDLRID=SMDLCCA;          /* PUT LCCA ID IN HEADER CPUS    */ 00686000
         LA    @07,656                                             0287 00687000
         ALR   @07,R9                                              0287 00688000
         MVC   SMDLRID(2,@07),@CH00107                             0287 00689000
*         SMDLRLEN=LENGTH(LCCA);    /* PUT LCCA LENGTH IN HEADER     */ 00690000
         MVC   SMDLRLEN(4,@07),@CF03415                            0288 00691000
*         SMDLRADR=LCCAT00P(SMWKIDX1);/* ADDR OF LCCA IN HEADER      */ 00692000
         ST    @02,SMDLRADR(,@07)                                  0289 00693000
*         CALL SUMDATA;             /* MOVE THIS LCCA TO THE BUFFER  */ 00694000
         BAL   @14,SUMDATA                                         0290 00695000
*       END;                        /* END OF DUMP LCCA FOR THIS CPU */ 00696000
*     RFY                                                          0292 00697000
*       PCCA BASED(PCCAT00P(SMWKIDX1));/* MAP THIS PCCA              */ 00698000
@RF00285 DS    0H                                                  0293 00699000
*     IF PCCAT00P(SMWKIDX1)^=NULL&  /* IF CURRENT PCCA EXISTS      0293 00700000
*                                                            @ZM44951*/ 00701000
*         PCCAPSAV^=NULL&           /* IF THIS IS NOT CURRENT PSA    */ 00702000
*         SMWKRBFL=OFF THEN         /* AND IF REAL BUFFER IS STILL 0293 00703000
*                                      NOT FULL                      */ 00704000
         SLR   @02,@02                                             0293 00705000
         LH    @07,SMWKIDX1(,R9)                                   0293 00706000
         SLA   @07,2                                               0293 00707000
         L     @06,PSAPTR                                          0293 00708000
         L     @06,CVTPTR(,@06)                                    0293 00709000
         L     @06,CVTPCCAT(,@06)                                  0293 00710000
         AL    @06,@CF03427                                        0293 00711000
         L     @07,PCCAT00P(@07,@06)                               0293 00712000
         CR    @07,@02                                             0293 00713000
         BE    @RF00293                                            0293 00714000
         L     @07,PCCAPSAV(,@07)                                  0293 00715000
         CR    @07,@02                                             0293 00716000
         BE    @RF00293                                            0293 00717000
         TM    SMWKRBFL(R9),B'00100000'                            0293 00718000
         BNZ   @RF00293                                            0293 00719000
*       DO;                         /* DUMP PSA FOR THIS CPU         */ 00720000
*         SMDLRID=SMDPSA;           /* PUT PSA ID IN HEADER          */ 00721000
         LA    @02,656                                             0295 00722000
         ALR   @02,R9                                              0295 00723000
         MVC   SMDLRID(2,@02),@CH00033                             0295 00724000
*         SMDLRLEN=LENGTH(PSA);     /* PUT PSA LENGTH IN HEADER      */ 00725000
         MVC   SMDLRLEN(4,@02),@CF03187                            0296 00726000
*         SMDLRADR=PCCAPSAV;        /* PUT ADDR THIS PSA IN HEADER   */ 00727000
         ST    @07,SMDLRADR(,@02)                                  0297 00728000
*         CALL SUMDATA;             /* MOVE PSA THIS CPU TO BUFFER   */ 00729000
         BAL   @14,SUMDATA                                         0298 00730000
*       END;                        /* END OF DUMP PSA FOR THIS CPU  */ 00731000
*     RFY                                                          0300 00732000
*       PCCA BASED(PCCAPTR);                                       0300 00733000
@RF00293 DS    0H                                                  0301 00734000
*   END;                            /* END OF MOVE PCCA,LCCA AND PSA    00735000
*                                      FOR ALL CPUS                  */ 00736000
         LA    @02,1                                               0301 00737000
         AH    @02,SMWKIDX1(,R9)                                   0301 00738000
         STH   @02,SMWKIDX1(,R9)                                   0301 00739000
         C     @02,@CF00126                                        0301 00740000
         BNH   @DL00277                                            0301 00741000
@DC00277 DS    0H                                                  0302 00742000
*SUMRTY20:                                                         0302 00743000
*   SUMFRRRT=ADDR(SUMRTY30);        /* GET NEXT RETRY LOCATION     0302 00744000
*                                      ADDRESS                       */ 00745000
SUMRTY20 LA    @02,SUMRTY30                                        0302 00746000
         ST    @02,SUMFRRRT(,R8)                                   0302 00747000
*                                                                  0303 00748000
*/********************************************************************/ 00749000
*/*   THIS SEGMENT MOVES THE SUPERVISOR TRACE TABLE, IF IT IS ACTIVE */ 00750000
*/*   TO THE REAL STORAGE BUFFER. THE TRACE TABLE IS NOT STOPPED     */ 00751000
*/*   BECAUSE THE MOVE IS VERY FAST AND ANY OTHER PROCESSOR IS       */ 00752000
*/*   PROBABLY STOPPED BY THIS ROUTINE HOLDING THE SALLOC LOCK.      */ 00753000
*/*                                                                  */ 00754000
*/*   INCLUDED BY: SUMDUMP                                           */ 00755000
*/*                                                                  */ 00756000
*/********************************************************************/ 00757000
*                                                                  0303 00758000
*   IF SMWKRBFL=OFF THEN            /* IF REAL STORAGE BUFFER IS NOT    00759000
*                                      FULL                          */ 00760000
         TM    SMWKRBFL(R9),B'00100000'                            0303 00761000
         BNZ   @RF00303                                            0303 00762000
*     DO;                           /* TRY TO DUMP THE TRACE TABLE   */ 00763000
*       IF CVTTRACE(2)='FA'X THEN   /* IF SUPERVISOR TRACE IS ACTIVE */ 00764000
         L     @02,PSAPTR                                          0305 00765000
         L     @07,CVTPTR(,@02)                                    0305 00766000
         CLI   CVTTRACE+1(@07),X'FA'                               0305 00767000
         BNE   @RF00305                                            0305 00768000
*         DO;                       /* MOVE THE TRACE TABLE TO THE 0306 00769000
*                                      BUFFER                        */ 00770000
*           SMDLRID=SMDTRT;         /* PUT TRACE TABLE ID IN HEADER  */ 00771000
         LA    @07,656                                             0307 00772000
         ALR   @07,R9                                              0307 00773000
         MVC   SMDLRID(2,@07),@CH00044                             0307 00774000
*           SMDLRLEN=TRTEND-FLCTRACE;/* PUT LENGTH OF TRACE TABLE IN    00775000
*                                      HEADER                @ZM44941*/ 00776000
