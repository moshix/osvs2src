         TITLE 'IEAVTSSD SVC DUMP SUMMARY DUMP PROCESSOR               *00001000
                        '                                               00002000
IEAVTSSD CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'IEAVTSSD  78.135'                                 0001 00009000
IEAVTSDS DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00787-IEAVTSDS)                                   00013000
         ENTRY IEAVTSDS                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00016000
         BALR  @12,0                                               0001 00017000
@PSTART  LA    @03,4095(,@12)                                      0001 00018000
         USING @PSTART,@12                                         0001 00019000
         USING @PSTART+4095,@03                                    0001 00020000
         AH    @15,4(,@15)                                         0001 00021000
         BR    @15                                                 0001 00022000
@EP00001 DS    0H                                                  0002 00023000
*   R9=RTCTSDSW;                    /* GET ADDRESS SUM DUMP WORK AREA*/ 00024000
         L     @02,PSAPTR                                          0140 00025000
         L     @02,CVTPTR(,@02)                                    0140 00026000
         L     @02,CVTRTMCT(,@02)                                  0140 00027000
         L     R9,RTCTSDSW(,@02)                                   0140 00028000
*   RFY                                                            0141 00029000
*     SMWKRSCB BASED(ADDR(SMWKRSM));/* MAP RSM PAR OF WORK AREA      */ 00030000
*   RFY                                                            0142 00031000
*     SMDLR BASED(ADDR(SMWKHEAD));  /* MAP SUM DUMP HEADER RECORD    */ 00032000
*                                                                  0142 00033000
*   /*****************************************************************/ 00034000
*   /*                                                               */ 00035000
*   /* ESTABLISH AN FRR RECOVERY EXIT                                */ 00036000
*   /*                                                               */ 00037000
*   /*****************************************************************/ 00038000
*                                                                  0143 00039000
*   GEN REFS(SUMFRRAD,PSA,PSACSTK,PSALSFCC,FRRS,FRRSCURR,FRRSFRRA, 0143 00040000
*       FRRSPARM,FRRSENTR);                                        0143 00041000
         SETFRR    A,FRRAD=SUMFRRAD,                                   X00042000
               PARMAD=(R8),WRKREGS=(R10,R11)                            00043000
*   RFY                                                            0144 00044000
*    (R10,                                                         0144 00045000
*     R11) UNRSTD;                                                 0144 00046000
*   RFY                                                            0145 00047000
*     SUMFRR6W BASED(R8);           /* MAP THE 6 WORD FRR WORK AREA  */ 00048000
*   SUMFRRBR=R12;                   /* SAVE THE BASE REGISTER        */ 00049000
         ST    R12,SUMFRRBR(,R8)                                   0146 00050000
*   SUMFRRB2=R3;                    /* SAVE SECOND BASE REGISTER     */ 00051000
         ST    R3,SUMFRRB2(,R8)                                    0147 00052000
*   SUMFRRSA=R13;                   /* SAVE CALLERS SAVE AREA ADDR   */ 00053000
         ST    R13,SUMFRRSA(,R8)                                   0148 00054000
*   RTCTSDSD=ON;                    /* INDICATE SUMMARY DUMP ENTERED */ 00055000
         L     @02,PSAPTR                                          0149 00056000
         L     @02,CVTPTR(,@02)                                    0149 00057000
         L     @02,CVTRTMCT(,@02)                                  0149 00058000
         OI    RTCTSDSD(@02),B'00000100'                           0149 00059000
*   SUMFRRRT=ADDR(SUMUNLCK);        /* NEXT RETRY LOCATION ADDRESS   */ 00060000
         LA    @02,SUMUNLCK                                        0150 00061000
         ST    @02,SUMFRRRT(,R8)                                   0150 00062000
*   CALL SUMLOCK;                   /* OBTAIN THE SALLOC LOCK        */ 00063000
         BAL   @14,SUMLOCK                                         0151 00064000
*   SUMFRRRT=ADDR(SUMFRFRM);        /* GET NEXT RETRY LOCATION     0152 00065000
*                                      ADDRESS                       */ 00066000
*                                                                  0152 00067000
         LA    @02,SUMFRFRM                                        0152 00068000
         ST    @02,SUMFRRRT(,R8)                                   0152 00069000
*   /*****************************************************************/ 00070000
*   /*                                                               */ 00071000
*   /* INITIALIZE THE WORK AREA AND CALL RSM TO OBTAIN REAL STORAGE  */ 00072000
*   /* FRAMES FOR THE BUFFER                                         */ 00073000
*   /*                                                               */ 00074000
*   /*****************************************************************/ 00075000
*                                                                  0153 00076000
*   IF R15=NULL THEN                /* WAS LOCK OBTAINED OR ALREADY     00077000
*                                      OWNED                         */ 00078000
         LTR   R15,R15                                             0153 00079000
         BNZ   @RF00153                                            0153 00080000
*     DO;                           /* YES CONTINUE DUMP PROCESSING  */ 00081000
*       CALL SUMINIT;               /* INIT KEY SUMDUMP WORKAREA   0155 00082000
*                                      VALUES                        */ 00083000
         BAL   @14,SUMINIT                                         0155 00084000
*       R13=ADDR(SMWKSAV1);         /* GET ADDRESS OF SAVE AREA FOR     00085000
*                                      RSM                           */ 00086000
         LA    R13,SMWKSAV1(,R9)                                   0156 00087000
*       RFY                                                        0157 00088000
*         SDUMP BASED(RTCTSDPL);    /* MAP CALLERS PARAMTER LIST   0157 00089000
*                                                            @ZM44941*/ 00090000
*       IF SDUSULST=ON THEN         /* IF SUMLIST IS SPECIFED GET  0158 00091000
*                                      LARGE NUMBER OF FRAMES      0158 00092000
*                                                            @ZM44941*/ 00093000
         L     @02,PSAPTR                                          0158 00094000
         L     @02,CVTPTR(,@02)                                    0158 00095000
         L     @02,CVTRTMCT(,@02)                                  0158 00096000
         L     @02,RTCTSDPL(,@02)                                  0158 00097000
         TM    SDUSULST(@02),B'00001000'                           0158 00098000
         BNO   @RF00158                                            0158 00099000
*         SMWKFRHD=NUMFROPT;        /* GET NUMBER OF FRAMES TO OBTAIN*/ 00100000
         MVC   SMWKFRHD+196(2,R9),@CH01387                         0159 00101000
*       ELSE                                                       0160 00102000
*         SMWKFRHD=DEFLTNUM;        /* NOT SUMLIST GET SMALL BUFFER     00103000
*                                                            @ZM44941*/ 00104000
         B     @RC00158                                            0160 00105000
@RF00158 MVC   SMWKFRHD+196(2,R9),@CH01336                         0160 00106000
*       SMWKNFRQ=SMWKFRHD;          /* OBTAIN NUMBER FRAMES REQUESTED   00107000
*                                                            @ZM45073*/ 00108000
@RC00158 LH    @02,SMWKFRHD+196(,R9)                               0161 00109000
         N     @02,@CF03420                                        0161 00110000
         STH   @02,SMWKNFRQ(,R9)                                   0161 00111000
*       R1=ADDR(SMWKRSM);           /* OBTAIN ADDRESS REAL STORAGE 0162 00112000
*                                      CONTROL BLOCK                 */ 00113000
         LA    R1,SMWKRSM(,R9)                                     0162 00114000
*       CALL IEAVPRSB;              /* OBTAIN THE REAL STORAGE FROM     00115000
*                                      RSM                           */ 00116000
         L     @02,PSAPTR                                          0163 00117000
         L     @02,CVTPTR(,@02)                                    0163 00118000
         L     @02,PVTPTR(,@02)                                    0163 00119000
         L     @15,IEAVPRSB(,@02)                                  0163 00120000
         BALR  @14,@15                                             0163 00121000
*       IF SMWKFRHD>ZERO THEN       /* IF SOME STORAGE WAS OBTAINED  */ 00122000
         LH    @02,SMWKFRHD+196(,R9)                               0164 00123000
         N     @02,@CF03420                                        0164 00124000
         LTR   @02,@02                                             0164 00125000
         BNP   @RF00164                                            0164 00126000
*         DO;                       /* CONTINUE WITH THE SUMMMARY  0165 00127000
*                                      DUMP                          */ 00128000
*           SUMFRRRF=ON;            /* INDICATE REAL STORAGE OBTAINED*/ 00129000
         OI    SUMFRRRF(R8),B'01000000'                            0166 00130000
*           SMWKNFAV=SMWKFRHD;      /* NUMBER OBTAINED IS NUMBER   0167 00131000
*                                      AVAIL                         */ 00132000
         STH   @02,SMWKNFAV(,R9)                                   0167 00133000
*           SMWKNFAQ=SMWKFRHD;      /* GET NUMBER OF FRAMES ACQUIRED    00134000
*                                                            @ZM45073*/ 00135000
         STH   @02,SMWKNFAQ(,R9)                                   0168 00136000
*                                                                  0169 00137000
*           /*********************************************************/ 00138000
*           /*                                                       */ 00139000
*           /* THIS SECTION BUILDS THE MODEL PSW USED BY SUMBUFF TO  */ 00140000
*           /* TURN DAT ON AND OFF. SUMBUFF USES LPSW LOGIC TO       */ 00141000
*           /* PERFORM THIS FUNCTION                                 */ 00142000
*           /*                                                       */ 00143000
*           /*********************************************************/ 00144000
*                                                                  0169 00145000
*           GEN REFS(SMWKPSWK,SMWKPSWS);                           0169 00146000
         STOSM SMWKPSWS(R9),X'00'  PUT CURRENT SYSTEM MASK IN MODEL PSW 00147000
*           SMWKPSWK='0C'X;         /* SET EC ON AND ENABLE FOR    0170 00148000
*                                      MACHINE CHECK INTERRUPTS      */ 00149000
*                                                                  0170 00150000
         MVI   SMWKPSWK(R9),X'0C'                                  0170 00151000
*           /*********************************************************/ 00152000
*           /*                                                       */ 00153000
*           /* MOVE STORAGE AREAS TO THE REAL STORAGE FRAMES         */ 00154000
*           /*                                                       */ 00155000
*           /*********************************************************/ 00156000
*                                                                  0171 00157000
*           CALL SUMDUMP;           /* MOVE REQUESTED AREAS TO BUFFER*/ 00158000
*                                                                  0171 00159000
         BAL   @14,SUMDUMP                                         0171 00160000
*           /*********************************************************/ 00161000
*           /*                                                       */ 00162000
*           /* FREE ANY UNUSED FRAMES                                */ 00163000
*           /*                                                       */ 00164000
*           /*********************************************************/ 00165000
*                                                                  0172 00166000
*SUMFRFRM:                                                         0172 00167000
*           SUMFRRRT=ADDR(SUMUNLCK);/* NEXT RETRY LOCATION ADDRESS   */ 00168000
SUMFRFRM LA    @02,SUMUNLCK                                        0172 00169000
         ST    @02,SUMFRRRT(,R8)                                   0172 00170000
*           SMWKNFUS=SMWKFRUS;      /* SAVE NUMBER FRAMES USED     0173 00171000
*                                                            @ZM45073*/ 00172000
         LH    @02,SMWKFRUS(,R9)                                   0173 00173000
         STH   @02,SMWKNFUS(,R9)                                   0173 00174000
*           IF SMWKNFAV^=ZERO THEN  /* IF ALL FRAMES NOT USED        */ 00175000
         LH    @02,SMWKNFAV(,R9)                                   0174 00176000
         LTR   @02,@02                                             0174 00177000
         BZ    @RF00174                                            0174 00178000
*             DO;                   /* RETURN ALL UNUSED FRAMES      */ 00179000
*               SUMFRRRT=ADDR(SUMUNLCK);/* NEXT RETRY LOCATION ADDR  */ 00180000
         LA    @02,SUMUNLCK                                        0176 00181000
         ST    @02,SUMFRRRT(,R8)                                   0176 00182000
*               SMWKRSIX=SMWKRSIX+ONE;/* GET INDEX 1ST FRAME FREED   */ 00183000
         LA    @02,1                                               0177 00184000
         AH    @02,SMWKRSIX(,R9)                                   0177 00185000
         STH   @02,SMWKRSIX(,R9)                                   0177 00186000
*               DO WHILE(SMWKNFAV>ZERO);/* FREE ALL UNUSED FRAMES    */ 00187000
         B     @DE00178                                            0178 00188000
@DL00178 DS    0H                                                  0179 00189000
*                 SMWKFRPT=SMWKFRMA(SMWKRSIX);/* GET FRAME ADDRESS   */ 00190000
         LA    @02,196                                             0179 00191000
         ALR   @02,R9                                              0179 00192000
         LH    @11,SMWKRSIX(,R9)                                   0179 00193000
         SLA   @11,2                                               0179 00194000
         L     @00,SMWKFRMA-4(@11,@02)                             0179 00195000
         ST    @00,SMWKFRPT(,@02)                                  0179 00196000
*                 SMWKNFAV=SMWKNFAV-ONE;/* DECREMENT AVAIL COUNT     */ 00197000
         LH    @00,SMWKNFAV(,R9)                                   0180 00198000
         BCTR  @00,0                                               0180 00199000
         STH   @00,SMWKNFAV(,R9)                                   0180 00200000
*                 SMWKFRHD=SMWKFRHD-ONE;/* DECREMENT NUMBER HELD     */ 00201000
         LH    @00,SMWKFRHD(,@02)                                  0181 00202000
         N     @00,@CF03420                                        0181 00203000
         BCTR  @00,0                                               0181 00204000
         STH   @00,SMWKFRHD(,@02)                                  0181 00205000
*                 IF SMWKFRHD=ZERO THEN/* IF THIS IS LAST FRAME      */ 00206000
         LTR   @00,@00                                             0182 00207000
         BNZ   @RF00182                                            0182 00208000
*                   SMWKEVRY=ON;    /* TURN ON EVERY BIT TO INSURE 0183 00209000
*                                      RSM CLEAN UP                  */ 00210000
         OI    SMWKEVRY(@02),B'10000000'                           0183 00211000
*                 SMWKFRMA(SMWKRSIX)=NULL;/* ZERO FRAME TABLE ENTRY  */ 00212000
@RF00182 LH    @02,SMWKRSIX(,R9)                                   0184 00213000
         LR    @11,@02                                             0184 00214000
         SLA   @11,2                                               0184 00215000
         LA    @10,196                                             0184 00216000
         ALR   @10,R9                                              0184 00217000
         SLR   @00,@00                                             0184 00218000
