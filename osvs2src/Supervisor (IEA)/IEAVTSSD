*   SDWAVRA=SUMFRR6W;               /* RECORD FRR 6 WORD WORK AREA   */ 02809000
         MVI   SDWAVRA+24(R1),C' '                                 0764 02810000
         MVC   SDWAVRA+25(83,R1),SDWAVRA+24(R1)                    0764 02811000
         MVC   SDWAVRA(24,R1),SUMFRR6W(R8)                         0764 02812000
*   SUMFRRRT=R14;                   /* SAVE RTM RETURN ADDRESS       */ 02813000
         ST    R14,SUMFRRRT(,R8)                                   0765 02814000
*   IF SUMFRRSL=ON THEN             /* IF SUMMARY DUMP OBTAINED    0766 02815000
*                                      SALLOC                        */ 02816000
         TM    SUMFRRSL(R8),B'00100000'                            0766 02817000
         BNO   @RF00766                                            0766 02818000
*     DO;                           /* TELL RTM TO FREE SALLOC LOCK  */ 02819000
*       DO;                         /* SETRP RECORD(YES)RC(0)FRELOCK(   02820000
*                                      SALLOC)                       */ 02821000
*         RESPECIFY                                                0769 02822000
*          (GPR00P,                                                0769 02823000
*           GPR01P,                                                0769 02824000
*           GPR14P,                                                0769 02825000
*           GPR15P) RSTD;                                          0769 02826000
*         GPR01P->SDWARCDE=0;       /* SAVE RC VALUE                 */ 02827000
