*           CALL SUMDATA;           /* MOVE TRACE TABLE TO THE BUFFER*/ 00784000
         BAL   @14,SUMDATA                                         0310 00785000
*         END;                      /* END OF MOVE TRACE TABLE TO THE   00786000
*                                      BUFFER                        */ 00787000
*     END;                          /* END OF REAL STORAGE BUFFER NOT   00788000
*                                      FULL                          */ 00789000
@RF00305 DS    0H                                                  0313 00790000
*SUMRTY30:                                                         0313 00791000
*   SUMFRRRT=ADDR(SUMRTY40);        /* GET NEXT RETRY LOCATION     0313 00792000
*                                      ADDRESS                       */ 00793000
@RF00303 DS    0H                                                  0313 00794000
SUMRTY30 LA    @02,SUMRTY40                                        0313 00795000
         ST    @02,SUMFRRRT(,R8)                                   0313 00796000
*                                                                  0314 00797000
*/* ******************************************************************/ 00798000
*/*    THIS SEGMENT HANDLES THE DUMPING OF THE FOLLOWING AREAS:      */ 00799000
*/*        - THE SUPER FRR STACKS (NOTE: THE NORMAL STACK IS         */ 00800000
*/*          DUMPED AS PART OF THE CURRENT PSA),                     */ 00801000
*/*        - THE CURRENT IHSA,                                       */ 00802000
*/*        - 2K PLUS AND MINUS THE UNIQUE REGISTER VALUES IN THE     */ 00803000
*/*          IHSA,                                                   */ 00804000
*/*        - THE SDWA IF ONE IS AVAILABLE,                           */ 00805000
*/*        - 2K PLUS AND MINUS THE UNIQUE REGISTER VALUES IN THE     */ 00806000
*/*          SDWA.                                                   */ 00807000
*/*    CURRENTLY THE SDWA REGISTER AREAS ARE ONLY DUMPED WHEN THE    */ 00808000
*/*    RT1TLPN FIELD IN THE RT1W POINTED TO BY PSACSTK, IS EQUAL TO  */ 00809000
*/*    X'FE'*. THIS WILL NOT WORK FOR A SLIP TRAP SINCE WHEN SLIP    */ 00810000
*/*    CALLS SDUMP,PSACSTK WILL NOT POINT TO THE INTERRUPTED STACK   */ 00811000
*/*    A SPECIAL INTERFACE TO SDUMP IS NEED FROM SLIP ALLOWING       */ 00812000
*/*    SLIP TO PASS US THE PROPER SDWA ADDRESS.                      */ 00813000
*/*                                                                  */ 00814000
*/*    INPUT: IHASA, RT1W, PSACSTK, SDWA, FRRS                       */ 00815000
*/*                                                                  */ 00816000
*/*    OUTPUT: AREAS FRR STACK, IHASA,SDWA AND 2K + AND - THE        */ 00817000
*/*            UNIQUE REGISTER CONTENTS ARE MOVED TO THE REAL        */ 00818000
*/*            STORAGE BUFFER                                        */ 00819000
*/*                                                                  */ 00820000
*/*    INCLUDED BY: SUMDUMP                                          */ 00821000
*/*                          *CHANGED BY @ZA31375                    */ 00822000
*/*                                                                  */ 00823000
*/* ******************************************************************/ 00824000
*                                                                  0314 00825000
*   /*****************************************************************/ 00826000
*   /*                                                               */ 00827000
*   /* THIS SECTION DUMPS THE IHSA                                   */ 00828000
*   /*                                                               */ 00829000
*   /*****************************************************************/ 00830000
*                                                                  0314 00831000
*   IF SMWKRBFL=OFF THEN            /* IF REAL STORAGE BUFFER NOT  0314 00832000
*                                      FULL                          */ 00833000
         TM    SMWKRBFL(R9),B'00100000'                            0314 00834000
         BNZ   @RF00314                                            0314 00835000
*     DO;                           /* DUMP IHSA FOR CURRENT ADDR  0315 00836000
*                                      SPACE                         */ 00837000
*       SMDLRID=SMDIHSA;            /* PUT IHSA ID IN HEADER         */ 00838000
         LA    @02,656                                             0316 00839000
         ALR   @02,R9                                              0316 00840000
         MVC   SMDLRID(2,@02),@CH01281                             0316 00841000
*       SMDLRLEN=LENGTH(IHSA);      /* GET LENGTH OF IHSA            */ 00842000
         MVC   SMDLRLEN(4,@02),@CF03416                            0317 00843000
*       SMDLRADR=ASXBIHSA;          /* PUT LOCAL IHSA ADDRESS IN   0318 00844000
*                                      HEADER                        */ 00845000
         L     @07,PSAPTR                                          0318 00846000
         L     @07,ASCBPTR(,@07)                                   0318 00847000
         L     @07,ASXBPTR(,@07)                                   0318 00848000
         L     @00,ASXBIHSA(,@07)                                  0318 00849000
         ST    @00,SMDLRADR(,@02)                                  0318 00850000
*       CALL SUMDATA;               /* MOVE LOCAL IHSA TO BUFFER     */ 00851000
         BAL   @14,SUMDATA                                         0319 00852000
*     END;                          /* END DUMP OF IHSA              */ 00853000
*                                                                  0321 00854000
*   /*****************************************************************/ 00855000
*   /*                                                               */ 00856000
*   /* THIS SECTION OBTAINS THE REGISTER VALUES FROM THE SDWA & IHSA */ 00857000
*   /*                                                               */ 00858000
*   /*****************************************************************/ 00859000
*                                                                  0321 00860000
*   RFY                                                            0321 00861000
*     IHSA BASED(ASXBIHSA);         /* MAP IHSA                      */ 00862000
@RF00314 DS    0H                                                  0322 00863000
*   R10=ADDR(IHSAGPRS);             /* GET ADDRESS OF IHSA REG SAVE     00864000
*                                      AREA                          */ 00865000
         L     @02,PSAPTR                                          0322 00866000
         L     @02,ASCBPTR(,@02)                                   0322 00867000
         L     @02,ASXBPTR(,@02)                                   0322 00868000
         L     R10,ASXBIHSA(,@02)                                  0322 00869000
         LA    R10,IHSAGPRS(,R10)                                  0322 00870000
*   CALL SUMREGS;                   /* SAVE UNIQUE REG CONTENTS IN 0323 00871000
*                                      IHSA                          */ 00872000
         BAL   @14,SUMREGS                                         0323 00873000
*   RFY                                                            0324 00874000
*     IHSA BASED;                                                  0324 00875000
*   RFY                                                            0325 00876000
*     FRRS BASED(PSACSTK);          /* MAP CURRENT FRR STACK         */ 00877000
*   RFY                                                            0326 00878000
*     RT1TRACK BASED(ADDR(RT1WLPTA));/* MAP TRACKING PART OF RT1W    */ 00879000
*   IF RT1TLPN=FRRCALL THEN         /* IF RTM1WA IS VALID - FROM FRR */ 00880000
         L     @02,PSAPTR                                          0327 00881000
         L     @02,PSACSTK(,@02)                                   0327 00882000
         CLI   RT1TLPN+16(@02),254                                 0327 00883000
         BNE   @RF00327                                            0327 00884000
*     DO;                           /* GET SDWA AND SAVE ITS REGS    */ 00885000
*       RFY                                                        0329 00886000
*         SDWA BASED(RT1WRTCA);     /* MAP CURRENT SDWA              */ 00887000
*       R10=ADDR(SDWAGRSV);         /* GET ADDR OF REG AT TIME OF  0330 00888000
*                                      ERROR                         */ 00889000
         L     R10,RT1WRTCA(,@02)                                  0330 00890000
         LA    R10,SDWAGRSV(,R10)                                  0330 00891000
*       CALL SUMREGS;               /* SAVE THE REGISTERS AT TIME  0331 00892000
*                                      ERROR                         */ 00893000
*                                                                  0331 00894000
         BAL   @14,SUMREGS                                         0331 00895000
*       /*************************************************************/ 00896000
*       /*                                                           */ 00897000
*       /* THIS SECTION DUMPS THE SDWA                               */ 00898000
*       /*                                                           */ 00899000
*       /*************************************************************/ 00900000
