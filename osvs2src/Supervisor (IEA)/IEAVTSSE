*         ERRRETRY=ADDR(SSERTY10);  /* GET NEXT RETRY LOCATION     0214 00628000
*                                      ADDRESS                       */ 00629000
         LA    @15,SSERTY10                                        0214 00630000
         ST    @15,ERRRETRY(,ERR6WPTR)                             0214 00631000
*         SMDLRLEN=ZERO;            /* GIVE ZERO TOTAL LENGTH        */ 00632000
         SLR   @15,@15                                             0215 00633000
