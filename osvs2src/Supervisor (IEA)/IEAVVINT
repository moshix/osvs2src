*         RESPECIFY                                                0480 01229000
*          (GPR15P) RESTRICTED;                                    0480 01230000
*         GPR15P=ADDR(I006);        /* ADDR OF LINK LIST             */ 01231000
         LA    GPR15P,I006                                         0481 01232000
*         SVC(6);                   /* ISSUE LINK SVC                */ 01233000
         SVC   6                                                   0482 01234000
*         RESPECIFY                                                0483 01235000
*          (GPR15P) UNRESTRICTED;                                  0483 01236000
*       END;                        /* LINK EP('IEECVGCI') LINK TO 0484 01237000
*                                      DIDOCS INIT                   */ 01238000
*       RFY                                                        0485 01239000
*         REG01 UNRSTD;             /* RELEASE REG 1 FOR USE         */ 01240000
*     END;                                                         0486 01241000
*   ELSE                                                           0487 01242000
*     ;                                                            0487 01243000
@RF00471 DS    0H                                                  0488 01244000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 01245000
*     GPR00F FIXED(31) REG(0),                                     0488 01246000
*     GPR01F FIXED(31) REG(1),                                     0488 01247000
*     GPR15F FIXED(31) REG(15),                                    0488 01248000
