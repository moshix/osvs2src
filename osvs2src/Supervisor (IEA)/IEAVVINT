         SVC   9                                                   0376 00773000
*     RESPECIFY                                                    0377 00774000
*      (GPR00P) UNRESTRICTED;                                      0377 00775000
*   END;                            /* DELETE EP('IEECVPRG') DELETE     00776000
*                                      TO REDUCE USE COUNT           */ 00777000
*   RFY                                                            0379 00778000
*     REG00 UNRSTD;                 /* RELEASE REG 0 TO COMPILER     */ 00779000
*                                                                  0379 00780000
*   /*****************************************************************/ 00781000
*   /*                                                               */ 00782000
*   /* THIS SEGMENT WILL BUILD THE INITIAL CELL POOLS FOR THE WQE'S  */ 00783000
*   /* AND ORE'S. THE SIZE OF THE INITIAL WQE CELLPOOL VARIES BETWEEN*/ 00784000
*   /* A MINIMUM 20 CELLS AND A MAXIMUM OF THE AMOUNT DEFINED BY THE */ 00785000
*   /* VARIABLE 'DEFLTWQE'.                                          */ 00786000
*   /*                                                               */ 00787000
*   /*****************************************************************/ 00788000
*                                                                  0380 00789000
*   NUMWQE=DEFLTWQE;                /* ASSUME MAX NUMBER WQE'S       */ 00790000
         LA    NUMWQE,36                                           0380 00791000
*   IF UCMWQLM1<NUMWQE              /* TEST ASSUMPTION OF MAX        */ 00792000
*     THEN                                                         0381 00793000
