         AL    UCMDVEPT,UCMVEZ(,UCMBASPT)                          0459 01144000
@DE00454 C     UCMDVEPT,UCMVEL(,UCMBASPT)                          0459 01145000
         BNH   @DL00454                                            0459 01146000
*   /*****************************************************************/ 01147000
*   /*                                                               */ 01148000
*   /* CALCULATE 60 AND 80 PERCENT OF WQE LIMITS SPECIFIED AT IPL AND*/ 01149000
*   /* STORE THEM IN THE UCM FIXED EXTENSION.                        */ 01150000
*   /*                                                               */ 01151000
*   /*****************************************************************/ 01152000
*                                                                  0460 01153000
*   UCMF60WQ=(UCMWQLM1*SIX)/TEN;    /* 60% OF WQE LIMIT      @ZA11392*/ 01154000
         LA    @08,10                                              0460 01155000
         LH    @15,UCMWQLM1(,UCMBASPT)                             0460 01156000
         LR    @01,@15                                             0460 01157000
         M     @00,@CF00060                                        0460 01158000
         DR    @00,@08                                             0460 01159000
         STH   @01,UCMF60WQ(,UCMFXPTR)                             0460 01160000
*   UCMF80WQ=(UCMWQLM1*EIGHT)/TEN;  /* 80% OF WQE LIMIT      @ZA11392*/ 01161000
         M     @14,@CF00035                                        0461 01162000
         DR    @14,@08                                             0461 01163000
         STH   @15,UCMF80WQ(,UCMFXPTR)                             0461 01164000
*/*** END OF UCMEXTBD SEGMENT ****************************************/ 01165000
*                                                                  0462 01166000
*                                                                  0462 01167000
*   /*****************************************************************/ 01168000
*   /*                                                               */ 01169000
*   /* IF THE HARDCOPY DEVICE IS NOT SYSLOG THEN POST COMMTASK THAT  */ 01170000
*   /* IEAVMWTL MAY BE CALLED TO HARDCOPY THE MESSAGES BUFFERED BY   */ 01171000
*   /* NIP.                                                          */ 01172000
*   /*                                                               */ 01173000
*   /*****************************************************************/ 01174000
*                                                                  0462 01175000
*   IF UCMSYSG=NO                   /* UCMSYSG IS FOR H.C. TO SYSLOG */ 01176000
*     THEN                                                         0462 01177000
         L     @10,UCMMCSPT                                        0462 01178000
         TM    UCMSYSG(@10),B'00000010'                            0462 01179000
         BNZ   @RF00462                                            0462 01180000
*     DO;                           /* POST (UCMNPECB)               */ 01181000
*       RESPECIFY                                                  0464 01182000
*        (GPR00F,                                                  0464 01183000
*         GPR01P) RESTRICTED;                                      0464 01184000
*       GPR00F=0;                   /* POST CODE IN R0               */ 01185000
         SLR   GPR00F,GPR00F                                       0465 01186000
*       GPR01P=ADDR(UCMNPECB);      /* ECB ADDRESS                   */ 01187000
         LA    GPR01P,UCMNPECB(,@10)                               0466 01188000
*       SVC(2);                     /* ISSUE POST SVC                */ 01189000
         SVC   2                                                   0467 01190000
*       RESPECIFY                                                  0468 01191000
*        (GPR00F,                                                  0468 01192000
*         GPR01P) UNRESTRICTED;                                    0468 01193000
*     END;                          /* POST (UCMNPECB) IF H.C. NOT 0469 01194000
*                                      SYSLOG, DO POST.              */ 01195000
*   ELSE                                                           0470 01196000
*     ;                                                            0470 01197000
*                                                                  0470 01198000
@RF00462 DS    0H                                                  0471 01199000
*   /*****************************************************************/ 01200000
*   /*                                                               */ 01201000
*   /* IF A GRAPHICS CONSOLE IS IN THE SYSTEM LINK TO THE DIDOCS     */ 01202000
*   /* INITIALIZATION ROUTINE, PASSING A POINTER TO THE DSAB FOR     */ 01203000
*   /* SYS1.DCMLIB IN REGISTER 1.                                    */ 01204000
*   /*                                                               */ 01205000
*   /*****************************************************************/ 01206000
*                                                                  0471 01207000
*   IF UCMSYSF=YES                  /* UCMSYSF IS FOR GRAPHICS       */ 01208000
*     THEN                                                         0471 01209000
         L     @10,UCMMCSPT                                        0471 01210000
         TM    UCMSYSF(@10),B'00000100'                            0471 01211000
         BNO   @RF00471                                            0471 01212000
*     DO;                                                          0472 01213000
*       RFY                                                        0473 01214000
*         REG01 RSTD;               /* RESTRICT REG 1 ACROSS LINK    */ 01215000
*       REG01=DIDOCSDS;             /* PASS PTR TO DCMLIB DSAB       */ 01216000
         L     REG01,DIDOCSDS                                      0474 01217000
*       DO;                         /* LINK EP('IEECVGCI')           */ 01218000
*         I00601='00'X;             /* EP/DE FLAG                    */ 01219000
         MVI   I00601,X'00'                                        0476 01220000
*         I00602=ADDR('IEECVGCI');  /* ADDR OF EP/DE                 */ 01221000
         LA    @10,@CC03423                                        0477 01222000
         STCM  @10,7,I00602                                        0477 01223000
*         I00604=0;                 /* DCB PTR                       */ 01224000
         SLR   @10,@10                                             0478 01225000
