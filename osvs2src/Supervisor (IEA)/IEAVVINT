         CR    @10,NUMWQE                                          0381 00795000
         BNL   @RF00381                                            0381 00796000
*     NUMWQE=UCMWQLM1;              /* IF WRONG, USE LOWER VALUE     */ 00797000
         LR    NUMWQE,@10                                          0382 00798000
*/*   CALCULATE NUMBER OF 1024 BYTE BLOCKS NEEDED FOR WQE'S          */ 00799000
*                                                                  0383 00800000
*   NUMWQBLK=(((NUMWQE*(LENGTH(WQE)+4))+32)+1023)/1024;            0383 00801000
@RF00381 LR    @00,NUMWQE                                          0383 00802000
         MH    @00,@CH03438                                        0383 00803000
         AL    @00,@CF03439                                        0383 00804000
         SRDA  @00,32                                              0383 00805000
         D     @00,@CF03414                                        0383 00806000
         LR    NUMWQBLK,@01                                        0383 00807000
*   RFY                                                            0384 00808000
*    (REG00,                                                       0384 00809000
*     REG02,                                                       0384 00810000
*     REG03) RSTD;                                                 0384 00811000
*   REG02=NUMWQBLK;                 /* SET REG 2 FOR BLDCPOOL        */ 00812000
         LR    REG02,NUMWQBLK                                      0385 00813000
*   REG03=LENGTH(WQE);              /* SET REG 3 FOR BLDCPOOL        */ 00814000
         LA    REG03,192                                           0386 00815000
*   GENERATE SETS(REG00,REG01,REG14,REG15) REFS(CVTBLDCP,CVTMAP,CVTPTR) 00816000
*       ;                                                          0387 00817000
          BLDCPOOL SP=231,POOLSIZ=(2),CSIZE=(3),BRANCH=YES              00818000
*   UCMWQECP=REG00;                 /* STORE WQE CELLPOOL ID IN UCM  */ 00819000
         ST    REG00,UCMWQECP(,UCMBASPT)                           0388 00820000
*   REG02=NUMORBLK;                 /* SET REG 2 FOR BLDCPOOL        */ 00821000
         LA    REG02,1                                             0389 00822000
