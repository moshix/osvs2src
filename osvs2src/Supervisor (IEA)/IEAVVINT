         TITLE 'IEAVVINT, COMM TASK INITIALIZATION ROUTINE             *00001000
                        '                                               00002000
IEAVVINT CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(24)                                             0001 00007000
         DC    C'IEAVVINT 78058  UZ17072 '                         0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @11,8(,@13)                                         0001 00020000
         LR    @13,@11                                             0001 00021000
JSCBDISP EQU 188                                                        00022000
JSCBAOS1 EQU  72                                                        00023000
JSCBAOS2 EQU 192                                                        00024000
RBPRFXLN EQU 64                                                         00025000
RBPRFLNA EQU 32                                                         00026000
PRBLEN   EQU 136                                                        00027000
SIRBLEN  EQU 200                                                        00028000
TIRBLEN  EQU 136                                                        00029000
IRBLEN   EQU 128                                                        00030000
SVRBLEN  EQU 224                                                        00031000
*                                                                  0147 00032000
*/********************************************************************/ 00033000
*/*                                                                  */ 00034000
*/*    GET A LOCAL COPY OF THE CVT POINTER THEN REBASE THE CVT ON IT */ 00035000
*/*                                                                  */ 00036000
*/********************************************************************/ 00037000
*                                                                  0147 00038000
*   LCVTPTR=CVTPTR;                 /* INITIALIZE LOCAL CVT BASE     */ 00039000
         L     LCVTPTR,CVTPTR                                      0147 00040000
*   RFY                                                            0148 00041000
*     CVTMAP BASED(LCVTPTR);        /* REBASE THE CVT ON LOCAL COPY  */ 00042000
*   UCMBASPT=CVTCUCB;               /* DEFINE THE UCM BASE   @G51APSS*/ 00043000
         L     UCMBASPT,CVTCUCB(,LCVTPTR)                          0149 00044000
*   UCMMCSPT=UCMPRFXP;              /* DEFINE THE UCM PREFIX @G51APSS*/ 00045000
*                                                                  0150 00046000
         LR    @10,UCMBASPT                                        0150 00047000
         AL    @10,@CF03443                                        0150 00048000
         L     @10,UCMPRFXP(,@10)                                  0150 00049000
         ST    @10,UCMMCSPT                                        0150 00050000
*   /*****************************************************************/ 00051000
*   /*                                                               */ 00052000
*   /* SET THE UCMCTID AND UCMASCB FIELDS WITH THE ASID AND ASCB     */ 00053000
*   /* ADDRESS OF THE COMM TASK (MASTER SCHEDULER'S) ADDRESS SPACE   */ 00054000
*   /*                                                               */ 00055000
*   /*****************************************************************/ 00056000
*                                                                  0151 00057000
*   UCMCTID=CTASID;                 /* SET ASID OF COMM TASK         */ 00058000
         LA    @08,1                                               0151 00059000
         STH   @08,UCMCTID(,UCMBASPT)                              0151 00060000
*   UCMASCB=ASVTENTY(CTASID);       /* SET ASCB ADDR OF COMM TASK    */ 00061000
*                                                                  0152 00062000
         L     @05,ASVTPTR(,LCVTPTR)                               0152 00063000
         L     @05,ASVTENTY(,@05)                                  0152 00064000
         ST    @05,UCMASCB(,UCMBASPT)                              0152 00065000
*   /*****************************************************************/ 00066000
*   /*                                                               */ 00067000
*   /* SEGMENT GETSPACE THIS SEGMENT CALCULATES THE AMOUNT OF SPACE  */ 00068000
*   /* REQUIRED FOR THE DSABS, TIOT, AND QDB, OBTAINS THE LOCAL LOCK,*/ 00069000
*   /* USES THE BRANCH ENTRY TO GETMAIN TO OBTAIN THE NEEDED STORAGE,*/ 00070000
*   /* THEN FREES THE LOCAL LOCK.                                    */ 00071000
*   /*                                                               */ 00072000
*   /*****************************************************************/ 00073000
*                                                                  0153 00074000
*   ALLOCCNT=(UCMVEL-UCMVEA+UCMVEZ)/UCMVEZ;/* COUNT UCMES            */ 00075000
         L     ALLOCCNT,UCMVEL(,UCMBASPT)                          0153 00076000
         SL    ALLOCCNT,UCMVEA(,UCMBASPT)                          0153 00077000
         L     @05,UCMVEZ(,UCMBASPT)                               0153 00078000
         ALR   ALLOCCNT,@05                                        0153 00079000
         SRDA  ALLOCCNT,32                                         0153 00080000
         DR    ALLOCCNT,@05                                        0153 00081000
         LR    ALLOCCNT,@03                                        0153 00082000
*   IF UCMSYSF=YES                  /* IF GRAPHICS CONSOLES EXIST    */ 00083000
*     THEN                                                         0154 00084000
         TM    UCMSYSF(@10),B'00000100'                            0154 00085000
         BNO   @RF00154                                            0154 00086000
*     ALLOCCNT=ALLOCCNT+1;          /* INCREMENT COUNT FOR DCMLIB    */ 00087000
         ALR   ALLOCCNT,@08                                        0155 00088000
*   DSABALEN=ALLOCCNT*LENGTH(DSAB); /* CALCULATE DSAB STORAGE        */ 00089000
@RF00154 LR    DSABALEN,ALLOCCNT                                   0156 00090000
         MH    DSABALEN,@CH02694                                   0156 00091000
*   TIOTALEN=(ALLOCCNT*LENTIOTE)+ENDTIOTE;/* STORAGE FOR TIOTES      */ 00092000
         LR    @10,ALLOCCNT                                        0157 00093000
         MH    @10,@CH00088                                        0157 00094000
         AL    @10,@CF00058                                        0157 00095000
         ST    @10,TIOTALEN                                        0157 00096000
*/* NOW ADD UP THE TOTAL STORAGE REQUIREMENTS OF THE CONTROL BLOCKS  */ 00097000
*                                                                  0158 00098000
*   STORGREQ=DSABALEN+LENGTH(QDB)+LENGTH(TIOT1)+TIOTALEN;          0158 00099000
         LA    @08,56                                              0158 00100000
         ALR   @08,DSABALEN                                        0158 00101000
         ALR   @08,@10                                             0158 00102000
         ST    @08,STORGREQ                                        0158 00103000
*   RFY                                                            0159 00104000
*    (REG00,                                                       0159 00105000
*     REG01,                                                       0159 00106000
*     REG15) RSTD;                  /* RESTRICT ACROSS GENERATE      */ 00107000
*   REG00=STORGREQ;                 /* PASS LENGTH FOR GETMAIN       */ 00108000
         LR    REG00,@08                                           0160 00109000
*   GENERATE SETS(REG00,REG01,REG15) REFS(REG00,REG01,REG02,REG03,REG04 00110000
*       ,REG05,REG06,REG07,REG08,REG09,REG10,REG11,REG12,REG13,REG14,   00111000
*       REG15,CVTCRMN,CVTCUCB,CVTMAP,CVTPTR,FLC,PSALITA,STORGREQ,UCM,   00112000
*       UCMPXA,UCMASCB) DEFS(VGETLOCK,VFRELOCK);                   0161 00113000
         SPACE 1                                                        00114000
*  OBTAIN THE LOCAL LOCKS, AND THEN USE THE BRANCH ENTRY TO             00115000
*  GETMAIN TO OBTAIN THE STORAGE FOR THE ALLOCATION CONTROL BLOCKS      00116000
*  IN THE SCHEDULERS STORAGE KEY.                                       00117000
*  (BRANCH ENTRY IS REQUIRED TO REQUEST A DIFFERENT KEY)                00118000
         SPACE 1                                                        00119000
         SAVE  (2,12)              SAVE REGS ACROSS LOCKED AREA         00120000
         LR    REG08,REG13         SAVE REG 13 ACROSS MACROS            00121000
         SPACE 1                                                        00122000
         LR    REG05,REG00         SET LENGTH OF 'TO' AREA FOR          00123000
*                                  LATER MVCL TO ZERO AREA.             00124000
         SPACE 1                                                        00125000
*  SET UP REGS FOR BRANCH ENTRY TO GETMAIN BEFORE DROPING BASES         00126000
         SPACE 1                                                        00127000
         L     REG15,CVTPTR        GET PTR TO CVT                       00128000
         L     REG15,CVTCUCB-CVTMAP(REG15) GET PTR TO UCM BASE          00129000
         L     REG04,UCMPXA-UCM(REG15)     ADDRESS OF COMM TASK TCB     00130000
         L     REG07,UCMASCB-UCM(REG15)    ADDRESS OF COMM TASK ASCB    00131000
         BALR  REG06,0              MAKE A TEMPORARY BASE REG           00132000
         USING *,REG06              TEMPORARILY BASE MODULE HERE        00133000
         SPACE 1                                                        00134000
*  GET THE LOCAL LOCK  (REQUIRED FOR BRANCH ENTRY TO GETMAIN)           00135000
         SPACE 1                                                        00136000
VGETLOCK SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        *00137000
               RELATED=(GETMAIN,IEAVVINT(VFRELOCK))                     00138000
         SPACE 1                                                        00139000
*  NOW, GET THE STORAGE FOR THE CONTROL BLOCKS                          00140000
         SPACE 1                                                        00141000
         GETMAIN  RU,LV=(0),BRANCH=YES,KEY=1,SP=230                     00142000
         SPACE 1                                                        00143000
*  FREE THE LOCAL LOCKS                                                 00144000
         SPACE 1                                                        00145000
VFRELOCK SETLOCK RELEASE,TYPE=LOCAL,                                   *00146000
               RELATED=(GETMAIN,IEAVVINT(VGETLOCK))                     00147000
         SPACE 2                                                        00148000
*  NOW CLEAR THE GOTTEN CORE                                            00149000
         SPACE 1                                                        00150000
         LR    REG04,REG01         SET THE 'TO' ADDRESS                 00151000
         LR    REG02,REG01         SET THE 'FROM' ADDRESS (= 'TO')      00152000
         SR    REG03,REG03         SET LENGTH OF 'FROM' AREA TO 0.      00153000
*                                  NOTE: THIS ALSO SETS THE PADDING     00154000
*                                        CHARACTER (BITS 0-7) TO X'00'  00155000
         MVCL  REG04,REG02         CLEAR THE GOTTEN STORAGE             00156000
         SPACE 1                                                        00157000
         LR    REG13,REG08         RESTORE SAVE AREA PTR                00158000
         LM    REG02,REG12,28(REG13)  RESTORE GENERAL REGS              00159000
         DROP  REG06               DROP TEMPORARY MODULE BASE           00160000
*   CBCOREPT=REG01;                 /* SAVE PTR TO GOTTEN STORAGE    */ 00161000
         LR    CBCOREPT,REG01                                      0162 00162000
*   RFY                                                            0163 00163000
*    (REG00,                                                       0163 00164000
*     REG01,                                                       0163 00165000
*     REG15) UNRSTD;                /* RELEASE REGS TO COMPILER      */ 00166000
*                                                                  0163 00167000
*   /*****************************************************************/ 00168000
*   /*                                                               */ 00169000
*   /* SET UP START ADDRESSES OF THE QDB, TIOT, TIOT ENTRIES, AND    */ 00170000
*   /* DSABS                                                         */ 00171000
*   /*                                                               */ 00172000
*   /*****************************************************************/ 00173000
*                                                                  0164 00174000
*   QDBPTR=CBCOREPT;                /* SET BASE FOR THE QDB          */ 00175000
         LR    QDBPTR,CBCOREPT                                     0164 00176000
*   TIOTPTR=QDBPTR+LENGTH(QDB);     /* TIOT STARTS AFTER THE QDB     */ 00177000
         LA    @08,32                                              0165 00178000
         ALR   @08,QDBPTR                                          0165 00179000
         ST    @08,TIOTPTR                                         0165 00180000
*   BASETIOE=TIOTPTR+LENGTH(TIOT1); /* SET PTR TO 1ST TIOT ENTRY     */ 00181000
         LA    BASETIOE,24                                         0166 00182000
         ALR   BASETIOE,@08                                        0166 00183000
*   DSABPTR=BASETIOE+TIOTALEN;      /* DSABS START AFTER TIOT ENTRYS */ 00184000
*                                                                  0167 00185000
         LR    DSABPTR,BASETIOE                                    0167 00186000
         AL    DSABPTR,TIOTALEN                                    0167 00187000
*   /*****************************************************************/ 00188000
*   /*                                                               */ 00189000
*   /* QUEUE FIRST DSAB TO THE QDB THEN FILL IN THE QDB              */ 00190000
*   /*                                                               */ 00191000
*   /*****************************************************************/ 00192000
*                                                                  0168 00193000
*   QDBFELMP=DSABPTR;               /* PTR TO FIRST DSAB             */ 00194000
         ST    DSABPTR,QDBFELMP(,QDBPTR)                           0168 00195000
*   QDBQDB=IDQDB;                   /* SET QDB IDENTIFIER            */ 00196000
         MVC   QDBQDB(4,QDBPTR),@CC00067                           0169 00197000
*   QDBNELMS=ALLOCCNT;              /* SET NUMBER OF CHAIN ELEMENTS  */ 00198000
         ST    ALLOCCNT,QDBNELMS(,QDBPTR)                          0170 00199000
*/* SET OFFSET TO FOREWARD CHAIN PTR OF DSAB IN QDB                  */ 00200000
*                                                                  0171 00201000
*   QDBFPTDS=ADDR(DSABFCHN)-ADDR(DSAB);                            0171 00202000
*                                                                  0171 00203000
         MVC   QDBFPTDS(2,QDBPTR),@CH00058                         0171 00204000
*/* SET OFFSET TO BACK CHAIN PTR OF DSAB IN QDB                      */ 00205000
*                                                                  0172 00206000
*   QDBBPTDS=ADDR(DSABBCHN)-ADDR(DSAB);                            0172 00207000
*                                                                  0172 00208000
         MVC   QDBBPTDS(2,QDBPTR),@CH00035                         0172 00209000
*   /*****************************************************************/ 00210000
*   /*                                                               */ 00211000
*   /* NEXT, FILL IN THE TIOT HEADER                                 */ 00212000
*   /*                                                               */ 00213000
*   /*****************************************************************/ 00214000
*                                                                  0173 00215000
*   TIOT1=HDRTIOT;                  /* INITIALIZE TIOT HEADER        */ 00216000
         MVC   TIOT1(24,@08),HDRTIOT                               0173 00217000
*   OLDDSAB=DSABPTR;                /* INITIALIZE PREVIOUS DSAB PTR  */ 00218000
         LR    OLDDSAB,DSABPTR                                     0174 00219000
*   OLDTIOT=BASETIOE;               /* INITIALIZE PREVIOUS TIOTE PTR */ 00220000
         ST    BASETIOE,OLDTIOT                                    0175 00221000
*   IF UCMSYSF=YES THEN             /* IF GRAPHICS IS IN SYSTEM      */ 00222000
         L     @08,UCMMCSPT                                        0176 00223000
         TM    UCMSYSF(@08),B'00000100'                            0176 00224000
         BNO   @RF00176                                            0176 00225000
*     DO;                           /* BUILD DCMLIB C.B.             */ 00226000
*                                                                  0177 00227000
*       /*************************************************************/ 00228000
*       /*                                                           */ 00229000
*       /* THIS SEGMENT BUILDS A DSAB AND TIOT ENTRY FOR SYS1.DCMLIB */ 00230000
*       /*                                                           */ 00231000
*       /*************************************************************/ 00232000
*                                                                  0178 00233000
*       DSABID=IDDSAB;              /* FILL IN IDENT FIELD           */ 00234000
         MVC   DSABID(4,DSABPTR),@CC00065                          0178 00235000
*       DSABPO=YES;                 /* MAKE DATA SET ORGANIZATION PO */ 00236000
         OI    DSABPO(DSABPTR),B'00000010'                         0179 00237000
*       DSABLNTH=LENGTH(DSAB);      /* FILL IN CONTROL BLOCK LENGTH  */ 00238000
         LA    @08,72                                              0180 00239000
         STH   @08,DSABLNTH(,DSABPTR)                              0180 00240000
*       DSABTIOT=BASETIOE;          /* PTR TO TIOT ENTRY FOR DSAB    */ 00241000
         ST    BASETIOE,DSABTIOT(,DSABPTR)                         0181 00242000
*       TIOEDDNM=DCMDDNAM;          /* DDNAME IN TIOT ENTRY          */ 00243000
         MVC   TIOEDDNM(8,BASETIOE),DCMDDNAM                       0182 00244000
*       TIOELNGH=LENTIOTE;          /* LENGTH OF THIS TIOT ENTRY     */ 00245000
         MVI   TIOELNGH(BASETIOE),X'14'                            0183 00246000
*       TIOEWTCT=DEVICENO;          /* SET NO. OF DEVICES THIS ENTRY */ 00247000
         MVI   TIOEWTCT(BASETIOE),X'01'                            0184 00248000
*       DIDOCSDS=DSABPTR;           /* SAVE PTR TO DSAB FOR DIDOCS   */ 00249000
*                                                                  0185 00250000
         ST    DSABPTR,DIDOCSDS                                    0185 00251000
*       /*************************************************************/ 00252000
*       /*                                                           */ 00253000
*       /* INCREMENT THE DSAB AND TIOT PTRS TO THE NEXT              */ 00254000
*       /*                                                           */ 00255000
*       /*************************************************************/ 00256000
*                                                                  0186 00257000
*       DSABPTR=DSABPTR+LENGTH(DSAB);/* DSAB PTR                     */ 00258000
         ALR   DSABPTR,@08                                         0186 00259000
*       BASETIOE=BASETIOE+LENTIOTE; /* TIOT ENTRY POINTER            */ 00260000
         AL    BASETIOE,@CF00088                                   0187 00261000
*       ENDTIOT=ZERO;               /* ZERO THE END OF TIOT MARKER   */ 00262000
         SLR   @08,@08                                             0188 00263000
         ST    @08,ENDTIOT(,BASETIOE)                              0188 00264000
*/*** END OF DCMLIBCB SEGMENT ****************************************/ 00265000
*                                                                  0189 00266000
*     END;                          /* END DCMLIB C.B. BUILD         */ 00267000
*   ELSE                                                           0190 00268000
*     ;                             /* ELSE SKIP DCMLIB C.B.         */ 00269000
@RF00176 DS    0H                                                  0191 00270000
*   DO CONSOLE=UCMVEA BY UCMVEZ TO UCMVEL;/* CONSOLE C.B. LOOP       */ 00271000
         L     CONSOLE,UCMVEA(,UCMBASPT)                           0191 00272000
         B     @DE00191                                            0191 00273000
@DL00191 DS    0H                                                  0192 00274000
*     RFY                                                          0192 00275000
*       UCMLIST BASED(CONSOLE);     /* REBASE THE UCME               */ 00276000
*/*** START OF CONSOLCB SEGMENT **************************************/ 00277000
*/*                                                                  */ 00278000
*/* SEGMENT   CONSOLCB  BUILDS A DSAB AND TIOT ENTRY FOR EACH        */ 00279000
*/*                     CONSOLE, AND LOADS THE ENTRY POINT OF THE    */ 00280000
*/*                     DEVICE PROCESSOR ROUTINE IN SVC 72.          */ 00281000
*/*                                                                  */ 00282000
*/* INPUT     CONSOLE   CONTAINS THE ADDR OF THE NEXT UCME           */ 00283000
*/*           DSABPTR   CONTAINS THE ADDR OF THE NEXT DSAB           */ 00284000
*/*           TIOENPTR  CONTAINS THE ADDR OF THE NEXT TIOT ENTRY     */ 00285000
*/*           OLDDSAB   CONTAINS THE ADDR OF THE PREVIOUS DSAB       */ 00286000
*/*           OLDTIOT   CONTAINS THE ADDR OF THE PREVIOUS TIOT ENTRY */ 00287000
*/*                                                                  */ 00288000
*/* OUTPUT    UCMSBR    CONTAINS THE ADDRESS OF THE DEVICE PROCESSOR */ 00289000
*/*                     FOR THIS CONSOLE TYPE.                       */ 00290000
*/*           A DSAB    FOR EACH CONSOLE SYSGENED INTO THE SYSTEM    */ 00291000
*/*           A TIOTE   FOR EACH CONSOLE SYSGENED INTO THE SYSTEM    */ 00292000
*/*                                                                  */ 00293000
*/********************************************************************/ 00294000
*                                                                  0193 00295000
*     IF OLDDSAB^=DSABPTR           /* IF THIS IS NOT THE 1ST DSAB   */ 00296000
*       THEN                                                       0193 00297000
         CR    OLDDSAB,DSABPTR                                     0193 00298000
         BE    @RF00193                                            0193 00299000
*       DO;                         /* THEN QUEUE NEW DSAB TO OLD    */ 00300000
*                                                                  0194 00301000
*         /***********************************************************/ 00302000
*         /*                                                         */ 00303000
*         /* FOREWARD AND BACKWARD CHAIN THE DSAB QUEUE.             */ 00304000
*         /*                                                         */ 00305000
*         /***********************************************************/ 00306000
*                                                                  0195 00307000
*         OLDDSAB->DSABFCHN=DSABPTR;/* FOREWARD CHAINING             */ 00308000
         ST    DSABPTR,DSABFCHN(,OLDDSAB)                          0195 00309000
*         DSABBCHN=OLDDSAB;         /* BACKWARD CHAINING             */ 00310000
         ST    OLDDSAB,DSABBCHN(,DSABPTR)                          0196 00311000
*       END;                        /* END CHAINING                  */ 00312000
*     ELSE                                                         0198 00313000
*       ;                                                          0198 00314000
@RF00193 DS    0H                                                  0199 00315000
*     DSABID=IDDSAB;                /* CONTROL BLOCK IDENT FOR DSAB  */ 00316000
         MVC   DSABID(4,DSABPTR),@CC00065                          0199 00317000
*     DSABPS=YES;                   /* DATA SET ORGANIZATION IS PS   */ 00318000
         OI    DSABPS(DSABPTR),B'01000000'                         0200 00319000
*     DSABLNTH=LENGTH(DSAB);        /* CONTROL BLOCK LENGTH          */ 00320000
         MVC   DSABLNTH(2,DSABPTR),@CH02694                        0201 00321000
*     DSABTIOT=BASETIOE;            /* PTR TO TIOT ENTRY FOR DSAB    */ 00322000
*                                                                  0202 00323000
         ST    BASETIOE,DSABTIOT(,DSABPTR)                         0202 00324000
*     /***************************************************************/ 00325000
*     /*                                                             */ 00326000
*     /* CONVERT THE CONSOLE ID TO EBCDIC AND CREATE THE CONSOLES    */ 00327000
*     /* DDNAME FROM THE PREFIX 'IEAVM' AND A SUFFIX OF 3 DECIMAL    */ 00328000
*     /* DIGITS CONTAINING THE CONSOLE ID.                           */ 00329000
*     /*                                                             */ 00330000
*     /***************************************************************/ 00331000
*                                                                  0203 00332000
*     CVD(IDCON,DBLWORK);           /* CONVERT THE ID TO PACKED FORM */ 00333000
         SLR   @08,@08                                             0203 00334000
         IC    @08,IDCON(,CONSOLE)                                 0203 00335000
         CVD   @08,DBLWORK                                         0203 00336000
*     UNPK(WORKID,DECID);           /* UNPACK THE ID TO ZONED        */ 00337000
         UNPK  WORKID(5),DECID(3)                                  0204 00338000
*     TIOEDDNM(1:5)=DDPREFIX;       /* SET UP DDNAME PREFIX          */ 00339000
         MVC   TIOEDDNM(5,BASETIOE),@CC00039                       0205 00340000
*     TIOEDDNM(6:8)=CONSID;         /* MOVE IN CONSOLE ID IN EBCDIC  */ 00341000
         MVC   TIOEDDNM+5(3,BASETIOE),CONSID                       0206 00342000
*     TIOELNGH=LENTIOTE;            /* LENGTH OF THIS TIOT ENTRY     */ 00343000
         MVI   TIOELNGH(BASETIOE),X'14'                            0207 00344000
*     TIOEFSRT=UCMUCB;              /* MOVE UCB ADDR TO TIOT ENTRY   */ 00345000
         MVC   TIOEFSRT(3,BASETIOE),UCMUCB+1(CONSOLE)              0208 00346000
*     TIOEWTCT=DEVICENO;            /* SET NO. OF DEVICES FOR TIOTE  */ 00347000
         MVI   TIOEWTCT(BASETIOE),X'01'                            0209 00348000
*     RFY                                                          0210 00349000
*       REG00 RSTD;                 /* NEEDED BY LOAD MACRO          */ 00350000
*     DO;                           /* LOAD EP(UCMNAME)              */ 00351000
*       RESPECIFY                                                  0212 00352000
*        (GPR00P,                                                  0212 00353000
*         GPR01P) RESTRICTED;                                      0212 00354000
*       GPR00P=ADDR(UCMNAME);       /* LOAD ENTRY POINT NAME ADDRESS */ 00355000
         LA    GPR00P,UCMNAME(,CONSOLE)                            0213 00356000
*       GPR01P=0;                   /* JOBLIB OR LINKLIB DCB         */ 00357000
         SLR   GPR01P,GPR01P                                       0214 00358000
*       SVC(8);                     /* ISSUE LOAD SVC                */ 00359000
         SVC   8                                                   0215 00360000
*       RESPECIFY                                                  0216 00361000
*        (GPR00P,                                                  0216 00362000
*         GPR01P) UNRESTRICTED;                                    0216 00363000
*     END;                          /* LOAD EP(UCMNAME) LOAD EP OF 0217 00364000
*                                      SVC 72 PROCESSOR              */ 00365000
*     UCMSBR=REG00;                 /* FILL IN PTR TO PROCESSOR      */ 00366000
         ST    REG00,UCMSBR(,CONSOLE)                              0218 00367000
*     DO;                           /* DELETE EP(UCMNAME)            */ 00368000
*       RESPECIFY                                                  0220 00369000
*        (GPR00P) RESTRICTED;                                      0220 00370000
*       GPR00P=ADDR(UCMNAME);       /* ->ENTRY NAME                  */ 00371000
         LA    GPR00P,UCMNAME(,CONSOLE)                            0221 00372000
*       SVC(9);                     /* DELETE SVC                    */ 00373000
         SVC   9                                                   0222 00374000
*       RESPECIFY                                                  0223 00375000
*        (GPR00P) UNRESTRICTED;                                    0223 00376000
*     END;                          /* DELETE EP(UCMNAME) REDUCE USE    00377000
*                                      COUNT ON MODNAME              */ 00378000
*     RFY                                                          0225 00379000
*       REG00 UNRSTD;               /* RELEASE REG 0 TO COMPILER     */ 00380000
*                                                                  0225 00381000
*     /***************************************************************/ 00382000
*     /*                                                             */ 00383000
*     /* NOW INCREMENT THE POINTERS TO THE DSAB AND TIOT ENTRY       */ 00384000
*     /*                                                             */ 00385000
*     /***************************************************************/ 00386000
*                                                                  0226 00387000
*     OLDDSAB=DSABPTR;              /* SAVE PTR TO THIS DSAB         */ 00388000
         LR    OLDDSAB,DSABPTR                                     0226 00389000
*     OLDTIOT=BASETIOE;             /* SAVE PTR TO THIS TIOT ENTRY   */ 00390000
         ST    BASETIOE,OLDTIOT                                    0227 00391000
*     DSABPTR=DSABPTR+LENGTH(DSAB); /* INCREMENT DSAB PTR            */ 00392000
         AL    DSABPTR,@CF02694                                    0228 00393000
*     BASETIOE=BASETIOE+LENTIOTE;   /* INCREMENT TIOT ENTRY POINTER  */ 00394000
         AL    BASETIOE,@CF00088                                   0229 00395000
*     ENDTIOT=ZERO;                 /* TIOT END MARKER IS ZERO       */ 00396000
         SLR   @08,@08                                             0230 00397000
         ST    @08,ENDTIOT(,BASETIOE)                              0230 00398000
*/*** END OF CONSOLCB SEGMENT ****************************************/ 00399000
*                                                                  0231 00400000
*     RFY                                                          0231 00401000
*       UCMLIST BASED(UCMDVEPT);    /* RESTORE ORIGINAL UCME BASE    */ 00402000
*   END;                            /* END CONSOLE C.B. LOOP         */ 00403000
         AL    CONSOLE,UCMVEZ(,UCMBASPT)                           0232 00404000
@DE00191 C     CONSOLE,UCMVEL(,UCMBASPT)                           0232 00405000
         BNH   @DL00191                                            0232 00406000
*   TCBTIO=TIOTPTR;                 /* POINT TCB TO COMMTASK TIOT    */ 00407000
         L     @08,SCVTSPTR(,LCVTPTR)                              0233 00408000
         L     @08,TCBPTR(,@08)                                    0233 00409000
         L     @05,TIOTPTR                                         0233 00410000
         ST    @05,TCBTIO(,@08)                                    0233 00411000
*   QDBLELMP=OLDDSAB;               /* CHAIN LAST DSAB TO QDB        */ 00412000
*                                                                  0234 00413000
         ST    OLDDSAB,QDBLELMP(,QDBPTR)                           0234 00414000
*   /*****************************************************************/ 00415000
*   /*                                                               */ 00416000
*   /* GET A JSCB USING THE GETJSCB MACRO, THEN INITIALIZE IT FOR    */ 00417000
*   /* COMMTASK                                                      */ 00418000
*   /*                                                               */ 00419000
*   /*****************************************************************/ 00420000
*                                                                  0235 00421000
*   RFY                                                            0235 00422000
*    (REG00,                                                       0235 00423000
*     REG01,                                                       0235 00424000
*     REG15) RSTD;                  /* RESTRICT GETJSCB REGS         */ 00425000
*   GEN(GETJSCB ,)                  /* GET A JSCB FOR COMM TASK      */ 00426000
*   SETS(REG00,REG01,REG15);                                       0236 00427000
         GETJSCB ,                                                      00428000
*   TCBJSCB=REG01;                  /* SET TCB PTR TO JSCB           */ 00429000
         L     @14,SCVTSPTR(,LCVTPTR)                              0237 00430000
         L     @14,TCBPTR(,@14)                                    0237 00431000
         ST    REG01,TCBJSCB(,@14)                                 0237 00432000
*   JSCBACT=REG01;                  /* MAKE THIS THE ACTIVE JSCB     */ 00433000
         L     @14,SCVTSPTR(,LCVTPTR)                              0238 00434000
         L     @14,TCBPTR(,@14)                                    0238 00435000
         L     @14,TCBJSCB(,@14)                                   0238 00436000
         ST    REG01,JSCBACT(,@14)                                 0238 00437000
*   RFY                                                            0239 00438000
*    (REG00,                                                       0239 00439000
*     REG01,                                                       0239 00440000
*     REG15) UNRSTD;                /* RELEASE GETJSCB REGS          */ 00441000
*   JSCBAUTH=YES;                   /* AUTHORIZE USE OF MODESET      */ 00442000
         OI    JSCBAUTH(@14),B'00000001'                           0240 00443000
*   JSCDSABQ=QDBPTR;                /* POINT JSCB AT QDB             */ 00444000
         ST    QDBPTR,JSCDSABQ(,@14)                               0241 00445000
*/*** START OF BLDSSIB SEGMENT ***************************************/ 00446000
*/*                                                                  */ 00447000
*/* SEGMENT   BLDSSIB   GETS STORAGE FOR AN SSIB CONTROL BLOCK AND   */ 00448000
*/*                     INITIALIZES IT. A POINTER TO THE SSIB IS     */ 00449000
*/*                     PLACED IN THE JSCB OF COMM TASK.             */ 00450000
*/*                                                                  */ 00451000
*/* INPUT     TCBJSCB   CONTAINS A PTR TO COMM TASK'S JSCB.          */ 00452000
*/*                                                                  */ 00453000
*/* OUTPUT    JSCBSSIB  CONTAINS A PTR TO THE SSIB                   */ 00454000
*/*           A SSIB    A COMPLETED SSIB CONTROL BLOCK               */ 00455000
*/*                                                                  */ 00456000
*/********************************************************************/ 00457000
*                                                                  0242 00458000
*   RFY                                                            0242 00459000
*    (REG00,                                                       0242 00460000
*     REG01,                                                       0242 00461000
*     REG15) RSTD;                  /* RESTRICT REGS FOR GETMAIN     */ 00462000
*   DO;                             /* GETMAIN (R)                 0243 00463000
*                                      LV(LENGTH(SSIB))SP(255)       */ 00464000
*     RESPECIFY                                                    0244 00465000
*      (GPR01F,                                                    0244 00466000
*       GPR00F) RESTRICTED;                                        0244 00467000
*     GPR00F=LENGTH(SSIB);          /* LENGTH REQUESTED              */ 00468000
         LA    GPR00F,36                                           0245 00469000
*     GPR01F=255;                   /* SP IN REG 1                   */ 00470000
         LA    GPR01F,255                                          0246 00471000
*     GEN(SLL   GPR01F,24)                                         0247 00472000
*     NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;                 0247 00473000
         SLL   GPR01F,24                                                00474000
*     GPR00F=GPR00F|GPR01F;         /* SP IN HIGH BYTE               */ 00475000
         OR    GPR00F,GPR01F                                       0248 00476000
*     GPR01F=-1;                    /* INDICATE A GETMAIN REQUEST    */ 00477000
         SLR   GPR01F,GPR01F                                       0249 00478000
         BCTR  GPR01F,0                                            0249 00479000
*     SVC(10);                      /* R-FORM OF GETMAIN             */ 00480000
         SVC   10                                                  0250 00481000
*     RESPECIFY                                                    0251 00482000
*      (GPR01F,                                                    0251 00483000
*       GPR00F) UNRESTRICTED;                                      0251 00484000
*   END;                            /* GETMAIN (R)                 0252 00485000
*                                      LV(LENGTH(SSIB))SP(255)     0252 00486000
*                                      GETMAIN AN SSIB               */ 00487000
*   JSCBSSIB=REG01;                 /* POINT TO SSIB FROM JSCB       */ 00488000
         L     @14,SCVTSPTR(,LCVTPTR)                              0253 00489000
         L     @14,TCBPTR(,@14)                                    0253 00490000
         L     @14,TCBJSCB(,@14)                                   0253 00491000
         ST    REG01,JSCBSSIB(,@14)                                0253 00492000
*   RFY                                                            0254 00493000
*    (REG00,                                                       0254 00494000
*     REG01,                                                       0254 00495000
*     REG15) UNRSTD;                /* RELEASE REGS TO COMPILER      */ 00496000
*   SSIB=(SSIB&&SSIB);              /* CLEAR THE SSIB TO ZERO        */ 00497000
         L     @10,SSIBPTR(,@14)                                   0255 00498000
         XC    SSIB(36,@10),SSIB(@10)                              0255 00499000
*   SSIBID=IDSSIB;                  /* CONTROL BLOCK IDENT FIELD     */ 00500000
         MVC   SSIBID(4,@10),@CC00069                              0256 00501000
*   SSIBLEN=LENGTH(SSIB);           /* CONTROL BLOCK LENGTH          */ 00502000
         MVC   SSIBLEN(2,@10),@CH00100                             0257 00503000
*   SSIBSSNM=IDMSTR;                /* SUB-SYSTEM NAME               */ 00504000
         MVC   SSIBSSNM(4,@10),@CC00071                            0258 00505000
*   SSIBJBID=IDMSTR;                /* JOB IDENTITY                  */ 00506000
         MVI   SSIBJBID+4(@10),C' '                                0259 00507000
         MVC   SSIBJBID+5(3,@10),SSIBJBID+4(@10)                   0259 00508000
         MVC   SSIBJBID(4,@10),@CC00071                            0259 00509000
*/*** END OF BLDSSIB SEGMENT *****************************************/ 00510000
*                                                                  0260 00511000
*   /*****************************************************************/ 00512000
*   /*                                                               */ 00513000
*   /* THIS SEGMENT RESOLVES MODULE ENTRY POINTS AND BUILDS THE      */ 00514000
*   /* INITIAL CELL POOLS FOR WQE'S AND ORE'S. THIS SEGMENT RESOLVES */ 00515000
*   /* MODULE ENTRY POINT FOR COMMTASK BY ISSUING LOAD MACRO. WHEN   */ 00516000
*   /* ALL ENTRY POINTS HAVE BEEN RESOLVED, DELETE MACROS ARE ISSUED */ 00517000
*   /* TO REDUCE THE MODULE USE COUNT.                               */ 00518000
*   /*                                                               */ 00519000
*   /*****************************************************************/ 00520000
*                                                                  0260 00521000
*   RFY                                                            0260 00522000
*     REG00 RSTD;                   /* NEEDED BY LOAD MACRO          */ 00523000
*   DO;                             /* LOAD EP('IEECMENQ')           */ 00524000
*     RESPECIFY                                                    0262 00525000
*      (GPR00P,                                                    0262 00526000
*       GPR01P) RESTRICTED;                                        0262 00527000
*     GPR00P=ADDR('IEECMENQ');      /* LOAD ENTRY POINT NAME ADDRESS */ 00528000
         LA    GPR00P,@CC03404                                     0263 00529000
*     GPR01P=0;                     /* JOBLIB OR LINKLIB DCB         */ 00530000
         SLR   GPR01P,GPR01P                                       0264 00531000
*     SVC(8);                       /* ISSUE LOAD SVC                */ 00532000
         SVC   8                                                   0265 00533000
*     RESPECIFY                                                    0266 00534000
*      (GPR00P,                                                    0266 00535000
*       GPR01P) UNRESTRICTED;                                      0266 00536000
*   END;                            /* LOAD EP('IEECMENQ') LOAD QUEUE   00537000
*                                      ROUTINE EP                    */ 00538000
*   UCMQRTN=REG00;                  /* FILL IN THE UCM PTR           */ 00539000
         L     @10,UCMMCSPT                                        0268 00540000
         ST    REG00,UCMQRTN(,@10)                                 0268 00541000
*   DO;                             /* LOAD EP('IEAVSTAA')           */ 00542000
*     RESPECIFY                                                    0270 00543000
*      (GPR00P,                                                    0270 00544000
*       GPR01P) RESTRICTED;                                        0270 00545000
