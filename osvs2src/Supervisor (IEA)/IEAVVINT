*     GPR15P PTR(31) REG(15);                                      0488 01251000
*   DCL                                                            0489 01252000
*     1 I006 DEF(ILIST),            /* DEFINE LIST                   */ 01253000
*      2 I00601 CHAR(1),            /* EP/DE FLAG                    */ 01254000
*      2 I00602 PTR(24),            /* EP/DE PTR                     */ 01255000
*      2 I00603 CHAR(1),            /* ERRET FLAG                    */ 01256000
*      2 I00604 PTR(24),            /* DCB PTR                       */ 01257000
*      2 I00605 PTR(31);            /* ERRET PTR                     */ 01258000
*   DECLARE                                                        0490 01259000
*     ILIST CHAR(12) BDY(DWORD);    /* STANDARD LIST                 */ 01260000
*   DECLARE                         /* COMMON VARIABLES              */ 01261000
*     I256C CHAR(256) BASED,                                       0491 01262000
*     I031F FIXED(31) BASED,                                       0491 01263000
*     I031P PTR(31) BASED,                                         0491 01264000
