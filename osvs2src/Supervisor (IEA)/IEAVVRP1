*                                      /* QUOTES.                    */ 01271000
*                                      /*                            */ 01272000
*                                      /******************************/ 01273000
*                                                                  0361 01274000
*                                      IF BUFTEXT(INDX:INDX+1)=''''''   01275000
*                                       THEN/* PAIR OF QUOTES ?      */ 01276000
         LR    @10,BUFPTR                                          0361 01277000
         ALR   @10,INDX                                            0361 01278000
         CLC   BUFTEXT-1(2,@10),@CC02066                           0361 01279000
         BNE   @RF00361                                            0361 01280000
*                                       DO;/* IF YES,                */ 01281000
*                                        INDX=INDX+1;/* POINT TO NEXT   01282000
*                                      CHAR                          */ 01283000
         LA    INDX,1(,INDX)                                       0363 01284000
*                                       END;/* END IF PAIR OF QUOTES */ 01285000
