*                           OREPTR=XAP;/* RESTORE PTR TO ORE         */ 01183000
         L     OREPTR,XAP(,R2)                                     0342 01184000
*                           OREOPBUF=PARMREG;/* ADDR OF TEMP. BUFFER    01185000
*                                      IN ORE                        */ 01186000
