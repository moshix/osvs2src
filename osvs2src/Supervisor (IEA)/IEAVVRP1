*                                    XAD=1;/* TURN BYPASS SWITCH ON  */ 00989000
@RC00285 MVC   XAD(4,R2),@CF00047                                  0297 00990000
*                                   END;/* END CHECKING END OF BUFFER*/ 00991000
*                                 ELSE/* IF NOT END OF BUFFER        */ 00992000
*                                   DO;/* TEST THIS CHAR             */ 00993000
*                                                                  0299 00994000
         B     @RC00283                                            0299 00995000
@RF00283 DS    0H                                                  0300 00996000
*                                    /********************************/ 00997000
*                                    /*                              */ 00998000
*                                    /* TEST TEXT CHARACTER FOR A    */ 00999000
*                                    /* QUOTE.                       */ 01000000
*                                    /*                              */ 01001000
*                                    /********************************/ 01002000
*                                                                  0300 01003000
*                                    IF BUFTEXT(INDX)='''' THEN/*  0300 01004000
*                                      THIS CHAR A QUOTE ?           */ 01005000
         LR    @10,BUFPTR                                          0300 01006000
         ALR   @10,INDX                                            0300 01007000
         CLI   BUFTEXT-1(@10),C''''                                0300 01008000
         BNE   @RF00300                                            0300 01009000
*                                     DO;/* IF YES,                  */ 01010000
*                                      DQSWIT=DQSWIT+1;/* INDICATE 0302 01011000
*                                      QUOTE FOUND                   */ 01012000
*                                                                  0302 01013000
         LH    @10,DQSWIT                                          0302 01014000
         LA    @10,1(,@10)                                         0302 01015000
         STH   @10,DQSWIT                                          0302 01016000
*                                      /******************************/ 01017000
*                                      /*                            */ 01018000
*                                      /* TEST FOR CONSECUTIVE       */ 01019000
*                                      /* QUOTES.                    */ 01020000
*                                      /*                            */ 01021000
*                                      /******************************/ 01022000
*                                                                  0303 01023000
*                                      IF DQSWIT=2 THEN/* SECOND   0303 01024000
*                                      CONSECUTIVE QUOTE ?           */ 01025000
         C     @10,@CF00109                                        0303 01026000
         BNE   @RF00303                                            0303 01027000
*                                       DQSWIT=0;/* RESET QUOTE    0304 01028000
*                                      INDICATOR                     */ 01029000
         SLR   @10,@10                                             0304 01030000
