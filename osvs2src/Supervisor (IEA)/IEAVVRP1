*                               /*************************************/ 01222000
*                                                                  0353 01223000
*                               IF BUFTEXT(1)='''' THEN/* TEXT     0353 01224000
*                                      EMBEDED IN QUOTES ?           */ 01225000
         CLI   BUFTEXT(BUFPTR),C''''                               0353 01226000
         BNE   @RF00353                                            0353 01227000
*                                 DO;/* IF YES,                      */ 01228000
*                                                                  0354 01229000
*                                   /*********************************/ 01230000
*                                   /*                               */ 01231000
*                                   /* TEST TO DETERMINE IF DOUBLE   */ 01232000
*                                   /* QUOTES WERE FOUND.            */ 01233000
*                                   /*                               */ 01234000
*                                   /*********************************/ 01235000
*                                                                  0355 01236000
*                                   IF DQPAIRS=0 THEN/* ANY DOUBLE 0355 01237000
*                                      QUOTES FOUND ?                */ 01238000
         LH    @10,DQPAIRS                                         0355 01239000
         LTR   @10,@10                                             0355 01240000
