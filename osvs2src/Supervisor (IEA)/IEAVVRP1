*                                      /* MOVE CHAR OF TEXT          */ 01292000
*                                      INDX1=INDX1+1;/* INCREASE TEMP   01293000
*                                      TEXT POINTER                  */ 01294000
         LA    INDX1,1(,INDX1)                                     0366 01295000
*                                     END;/* END DO LOOP             */ 01296000
         LA    INDX,1(,INDX)                                       0367 01297000
@DE00360 LR    @10,LNGREG                                          0367 01298000
         AH    @10,DQPAIRS                                         0367 01299000
         AL    @10,@CF00047                                        0367 01300000
         CR    INDX,@10                                            0367 01301000
         BNH   @DL00360                                            0367 01302000
*                                    END;/* END IF DOUBLE QUOTES   0368 01303000
*                                      FOUND                         */ 01304000
*                                 END;/* END IF TEXT IS IN QOUTES    */ 01305000
*                               ELSE/* IF TEXT IS NOT IN QUOTES      */ 01306000
*                                 DO;/* MOVE TEXT CHARACTER          */ 01307000
         B     @RC00353                                            0370 01308000
