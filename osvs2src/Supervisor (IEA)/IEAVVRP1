@RC00202 C     ENDSEG,@CF00047                                     0235 00647000
         BE    @RF00235                                            0235 00648000
*                         DO;       /* IF NO, CONTINUE               */ 00649000
*                                                                  0236 00650000
*                           /*****************************************/ 00651000
*                           /*                                       */ 00652000
*                           /* AUTHORITY CHECKING SEGMENT            */ 00653000
*                           /* CONTINUATION TEST TO DETERMINE IF THE */ 00654000
*                           /* WTOR WAS ISSUED TO ALL CONSOLES       */ 00655000
*                           /*                                       */ 00656000
*                           /*****************************************/ 00657000
*                                                                  0237 00658000
*                           IF WQEMCSFF^='1'B THEN/* BROADCAST TO ALL   00659000
*                                      CONSOLES ?                    */ 00660000
         TM    WQEMCSFF(WQEPTR),B'00000100'                        0237 00661000
         BO    @RF00237                                            0237 00662000
*                             DO;   /* IF NO,                        */ 00663000
*                                                                  0238 00664000
*                               /*************************************/ 00665000
*                               /*                                   */ 00666000
*                               /* TEST FOR WTOR ISSUED BY MESSAGE   */ 00667000
*                               /* TYPE                              */ 00668000
*                               /*                                   */ 00669000
*                               /*************************************/ 00670000
*                                                                  0239 00671000
*                               IF WQEMCSD^='1'B THEN/* MESSAGE TYPE    00672000
*                                      FIELDS EXIST ?                */ 00673000
         TM    WQEMCSD(WQEPTR),B'00010000'                         0239 00674000
         BO    @RF00239                                            0239 00675000
*                                 DO;/* IF NO,                       */ 00676000
*                                                                  0240 00677000
*                                   /*********************************/ 00678000
*                                   /*                               */ 00679000
*                                   /* IF NO MESSAGE TYPE FIELDS     */ 00680000
*                                   /* EXIST, CHECK FOR ROUT CODE    */ 00681000
*                                   /* MATCH.                        */ 00682000
*                                   /*                               */ 00683000
*                                   /*********************************/ 00684000
*                                                                  0241 00685000
*                                   IF(RTCDTST&WQEROUT)=0 THEN/* ROUT   00686000
*                                      CODE MATCH ?                  */ 00687000
         LH    @10,WQEROUT(,WQEPTR)                                0241 00688000
         N     @10,@CF02085                                        0241 00689000
         LH    @09,RTCDTST                                         0241 00690000
         NR    @10,@09                                             0241 00691000
         LTR   @10,@10                                             0241 00692000
         BNZ   @RF00241                                            0241 00693000
*                                    DO;/* IF NO,                    */ 00694000
*                                     ERRCDE=3;/* INDICATE ERROR MSG3*/ 00695000
         LA    ERRCDE,3                                            0243 00696000
*                                     CALL ERRORTN;/* CALL ERROR RTN */ 00697000
         BAL   @14,ERRORTN                                         0244 00698000
*                                    END;/* END IF NO ROUT CODE MATCH*/ 00699000
*                                 END;/* END IF NO MSG TYPE          */ 00700000
*                               ELSE/* IF MSGTYPE FIELD EXIST        */ 00701000
*                                 DO;/* TEST FOR MSGTYPE MATCH       */ 00702000
         B     @RC00239                                            0247 00703000
@RF00239 DS    0H                                                  0248 00704000
*                                   MSGTTST=UCMMSG;/* LOAD UCME    0248 00705000
*                                      MSGTYPE                       */ 00706000
*                                                                  0248 00707000
         LH    MSGTTST,UCMMSG(,UCMDVEPT)                           0248 00708000
         N     MSGTTST,@CF02085                                    0248 00709000
*                                   /*********************************/ 00710000
*                                   /*                               */ 00711000
*                                   /* TEST FOR MESSAGE TYPE MATCH   */ 00712000
*                                   /*                               */ 00713000
*                                   /*********************************/ 00714000
*                                                                  0249 00715000
*                                   IF(MSGTTST&WQEMSGTP)=0 THEN/*  0249 00716000
*                                      MESSAGE TYPE MATCH ?          */ 00717000
         LH    @10,WQEMSGTP(,WQEPTR)                               0249 00718000
         N     @10,@CF02085                                        0249 00719000
         NR    @10,MSGTTST                                         0249 00720000
         LTR   @10,@10                                             0249 00721000
         BNZ   @RF00249                                            0249 00722000
*                                    DO;/* IF NO,                    */ 00723000
*                                     ERRCDE=3;/* INDICATE ERROR MSG3*/ 00724000
         LA    ERRCDE,3                                            0251 00725000
*                                     CALL ERRORTN;/* CALL ERROR RTN */ 00726000
         BAL   @14,ERRORTN                                         0252 00727000
*                                    END;/* END TESTING MSGTYPE MATCH*/ 00728000
*                                 END;/* END TESTING MSGTYPE         */ 00729000
@RF00249 DS    0H                                                  0255 00730000
*                             END;  /* END ALL CONSOLE BROADCAST     */ 00731000
*                                                                  0255 00732000
@RC00239 DS    0H                                                  0256 00733000
