         TITLE 'IEAVVRP1 - REPLY PROCESSOR - STAGE 1-                  *00001000
                        '                                               00002000
IEAVVRP1 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(19)                                             0001 00007000
         DC    C'IEAVVRP1R0300076163'                              0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         L     @00,@SIZDATD                                        0001 00013000
         GETMAIN  R,LV=(0)                                              00014000
         LR    @11,@01                                             0001 00015000
         USING @DATD,@11                                           0001 00016000
*                                      TABLE                         */ 00017000
*/********************************************************************/ 00018000
*/*           SETLOCKS   SEGMENT                                     */ 00019000
*/*                                                                  */ 00020000
*/* IN ORDER TO SERIALIZE THE USE OF RESOURCES ACROSS REPLY          */ 00021000
*/* PROCESSING, THE FOLLOWING SEGMENT ISSUES SETLOCKS FOR THE        */ 00022000
*/* LOCAL AND CMS LOCKS.                                             */ 00023000
*/*                                                                  */ 00024000
*/********************************************************************/ 00025000
*                                                                  0099 00026000
*   RECEXADD=UCMFRRAD;              /* SET ADDR OF RECOVERY EXIT     */ 00027000
         L     @10,CVTPTR                                          0099 00028000
         L     @10,UCMBASPT(,@10)                                  0099 00029000
         L     @10,UCMFRRAD(,@10)                                  0099 00030000
         ST    @10,RECEXADD                                        0099 00031000
*   XAV(5:8)=RTNREG;                /* SAVE RETURN ADDR              */ 00032000
         STCM  RTNREG,15,XAV+4(R2)                                 0100 00033000
*   XAD=BASREG;                     /* SAVE BASE REGISTER            */ 00034000
         ST    BASREG,XAD(,R2)                                     0101 00035000
*   WQEPTR=R11;                     /* SAVE DATA REGISTER            */ 00036000
         LR    WQEPTR,R11                                          0102 00037000
*   GEN REFS(PSALITA,FLC);                                         0103 00038000
        SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        XX00039000
               RELATED=(ORE,IEAVVRP1(RELEASE))                          00040000
        SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,                          XX00041000
               RELATED=(ORE,IEAVVRP1(RELEASE))                          00042000
*   BASREG=XAD;                     /* RESTORE BASE REG              */ 00043000
         L     BASREG,XAD(,R2)                                     0104 00044000
*   R11=WQEPTR;                     /* RESTORE DATA REGISTER         */ 00045000
         LR    R11,WQEPTR                                          0105 00046000
*   GEN REFS(PSACSTK,FRRSCURR,FRRSLAST,FRRSELEN,PSALSFCC,FRRSPARM, 0106 00047000
*       FRRSENTR,FRRSFRRA,RECEXADD);                               0106 00048000
*                                             /*ISSUE SET FRR       */  00049000
        SETFRR A,FRRAD=RECEXADD,WRKREGS=(8,9),PARMAD=(8)                00050000
*   PARMPTR=SRBPTR;                 /* BASE FOR PARM LIST            */ 00051000
         LR    PARMPTR,SRBPTR                                      0107 00052000
*   PARMID='VRP1';                  /* SET MODULE ID                 */ 00053000
         MVC   PARMID(4,PARMPTR),@CC02055                          0108 00054000
*   ERRCDE=0;                       /* INIT ERROR CODE TO ZERO       */ 00055000
         SLR   ERRCDE,ERRCDE                                       0109 00056000
*   UCMMCSPT=UCMPRFXP;              /* ADDR OF MCS PREFIX            */ 00057000
         L     @10,CVTPTR                                          0110 00058000
         L     @10,UCMBASPT(,@10)                                  0110 00059000
         LR    @09,@10                                             0110 00060000
         AL    @09,@CF02083                                        0110 00061000
         L     @09,UCMPRFXP(,@09)                                  0110 00062000
         ST    @09,UCMMCSPT                                        0110 00063000
*   UCMEXTPT=UCM2PTR;               /* ADDR OF UCM EXTENSION         */ 00064000
         AL    @10,@CF02084                                        0111 00065000
         L     UCMEXTPT,UCM2PTR(,@10)                              0111 00066000
*   BUFPTR=XAL;                     /* SET PTR TO INPUT BUFFER       */ 00067000
         L     BUFPTR,XAL-1(,R2)                                   0112 00068000
         LA    BUFPTR,0(,BUFPTR)                                   0112 00069000
*   IDSAV=BUFID;                    /* SAVE REPLY ID                 */ 00070000
         MVC   IDSAV+32(2,R2),BUFID(BUFPTR)                        0113 00071000
*   TCBREG=PSATOLD;                 /* SET ADDR OF TCB       @ZA08774*/ 00072000
         L     TCBREG,PSATOLD                                      0114 00073000
*   ZEROREG=288;                    /* SET LEN OF GETMAIN    @ZA08774*/ 00074000
         LA    ZEROREG,288                                         0115 00075000
*   ASCBREG=PSAAOLD;                /* SET ADDR OF ASCB      @ZA08774*/ 00076000
         L     ASCBREG,PSAAOLD                                     0116 00077000
*   PARMREG=0;                      /* GETMAIN INTERFACE     @ZA08774*/ 00078000
*                                                                  0117 00079000
         SLR   PARMREG,PARMREG                                     0117 00080000
*   /*****************************************************************/ 00081000
*   /*                                                               */ 00082000
*   /* *************************************************     @ZA08774*/ 00083000
*   /* THE FOLLOWING GETMAIN IS ISSUED TO GET TWO 144        @ZA08774*/ 00084000
*   /* BYTE AREAS TO CONTAIN A HARDCOPY MESSAGE AND          @ZA08774*/ 00085000
*   /* AN ACCEPTED REPLY MESSAGE.                            @ZA08774*/ 00086000
*   /* *************************************************     @ZA08774*/ 00087000
*   /*                                                               */ 00088000
*   /*****************************************************************/ 00089000
*                                                                  0118 00090000
*   GEN REFS(CVTCRMR);                                             0118 00091000
*                           /*GET BUFFER FOR MSG            @ZA08774*/  00092000
        GETMAIN RU,LV=(0),SP=231,BRANCH=YES GET CORE          @ZA08774  00093000
*   XASAVLOC(2)=PARMREG;            /* SAVE ADDR OF MSG CORE @ZA08774*/ 00094000
*                                                                  0119 00095000
         ST    PARMREG,XASAVLOC+4(,R2)                             0119 00096000
*   /*****************************************************************/ 00097000
*   /*                                                               */ 00098000
*   /* TEST TO DETERMINE IF THE ADDRESS OF AN OPERAND FIELD WAS      */ 00099000
*   /* PASSED IN THE XSA.                                            */ 00100000
*   /*                                                               */ 00101000
*   /*****************************************************************/ 00102000
*                                                                  0120 00103000
*   IF XAL=0 THEN                   /* OPERAND ADDRESS MISSING ?     */ 00104000
         L     @10,XAL-1(,R2)                                      0120 00105000
         LA    @10,0(,@10)                                         0120 00106000
         LTR   @10,@10                                             0120 00107000
         BNZ   @RF00120                                            0120 00108000
*     DO;                           /* IF YES,                       */ 00109000
*       ERRCDE=1;                   /* SET ERROR INDICATOR           */ 00110000
         LA    ERRCDE,1                                            0122 00111000
*       XAE='0B'X;                  /* SET IND. FOR MSG MODULE       */ 00112000
         MVI   XAE(R2),X'0B'                                       0123 00113000
*/********************************************************************/ 00114000
*/*         RELEASE                    SEGMENT                       */ 00115000
*/*                                                                  */ 00116000
*/* THE FOLLOWING SEGMENT WILL RELEASE ALL LOCKS CURRENTLY           */ 00117000
*/* HELD BY THIS MODULE BEFORE ISSUEING A WTOR FOR ACCEPTED          */ 00118000
*/* REPLY.                                                           */ 00119000
*/********************************************************************/ 00120000
*                                                                  0124 00121000
*       XAD=BASREG;                 /* SAVE BASE REGISTER            */ 00122000
         ST    BASREG,XAD(,R2)                                     0124 00123000
*       OREPTR=R11;                 /* SAVE DATA REGISTER            */ 00124000
         LR    OREPTR,R11                                          0125 00125000
*       GEN REFS(PSALITA,FLC,PSACSTK,FRRSCURR,FRRSEMP,FRRSELEN);   0126 00126000
*                                     /*RELEASE LOCKS               */  00127000
        SETFRR  D,WRKREGS=(8,9)                                         00128000
        SETLOCK  RELEASE,TYPE=CMS,RELATED=(ORE,IEAVVRP1(SETLOCKS))      00129000
        SETLOCK  RELEASE,TYPE=LOCAL,RELATED=(ORE,IEAVVRP1(SETLOCKS))    00130000
*       BASREG=XAD;                 /* RESTORE BASE REGISTER         */ 00131000
         L     BASREG,XAD(,R2)                                     0127 00132000
*       R11=OREPTR;                 /* RESTORE DATA REGISTER         */ 00133000
         LR    R11,OREPTR                                          0128 00134000
*       RTNREG=XAV(5:8);            /* SET RETURN REGISTER           */ 00135000
         ICM   RTNREG,15,XAV+4(R2)                                 0129 00136000
*       XAV='REPLY   ';             /* SET REPLY VERB                */ 00137000
         MVC   XAV(8,R2),@CC02059                                  0130 00138000
*       R15=ADDR(IEE0503D);         /* SET ADDR OF MSG MODULE        */ 00139000
         L     R15,@CV00204                                        0131 00140000
*       GEN(BR  R15);               /* BRANCH TO MSG MODULE          */ 00141000
         BR  R15                                                        00142000
*     END;                          /* END IF NO OPERAND FIELD       */ 00143000
*   ELSE                            /* IF OPERAND FIELD SPECIFIED    */ 00144000
*     DO;                           /* CONTINUE PROCESSING           */ 00145000
*                                                                  0134 00146000
         B     @RC00120                                            0134 00147000
@RF00120 DS    0H                                                  0135 00148000
*       /*************************************************************/ 00149000
*       /*                                                           */ 00150000
*       /* TEST UCM TO DETERMINE IF THERE ARE ANY ORE'S ON THE CHAIN */ 00151000
*       /*                                                           */ 00152000
*       /*************************************************************/ 00153000
*                                                                  0135 00154000
*       IF UCMRPYQ=0 THEN           /* IS ORE CHAIN EMPTY ?          */ 00155000
         L     @10,CVTPTR                                          0135 00156000
         L     @10,UCMBASPT(,@10)                                  0135 00157000
         L     @10,UCMRPYQ(,@10)                                   0135 00158000
         LTR   @10,@10                                             0135 00159000
         BNZ   @RF00135                                            0135 00160000
*         DO;                       /* IF YES,                       */ 00161000
*           ERRCDE=1;               /* INDICATE ERROR MSG 1          */ 00162000
         LA    ERRCDE,1                                            0137 00163000
*           CALL ERRORTN;           /* CALL ERROR ROUTINE            */ 00164000
         BAL   @14,ERRORTN                                         0138 00165000
*         END;                      /* END - NO ORE'S                */ 00166000
*       ELSE                                                       0140 00167000
*         DO;                                                      0140 00168000
*                                                                  0140 00169000
         B     @RC00135                                            0140 00170000
@RF00135 DS    0H                                                  0141 00171000
*/********************************************************************/ 00172000
*/*           VALIDITY  CHECK  SEGMENT                               */ 00173000
*/*                                                                  */ 00174000
*/* THE FOLLOWING SEGMENT OF CODE WILL VALIDATE THE REPLY ID. THE    */ 00175000
*/* REPLY ID CAN BE A ONE OR TWO DIGIT DECIMAL NUMBER FOLLOWED BY    */ 00176000
*/* A BLANK OR COMMA.IF THE ID IS INVALID, AN ERROR MESSAGE WILL BE  */ 00177000
*/* ISSUED TO THE OPERATOR. IF VALID, REPLY PROCESSING WILL CONTINUE */ 00178000
*/* NORMALLY.                                                        */ 00179000
*/*                                                                  */ 00180000
*/********************************************************************/ 00181000
*                                                                  0141 00182000
*           /*********************************************************/ 00183000
*           /*                                                       */ 00184000
*           /* TEST THE FIRST PART OF THE REPLY ID FOR DECIMAL       */ 00185000
*           /* CHARACTER                                             */ 00186000
*           /*                                                       */ 00187000
*           /*********************************************************/ 00188000
*                                                                  0141 00189000
*           IF BUFID1(1:4)^='1111'B THEN/* FIRST PART OF ID DECIMAL ?*/ 00190000
         TM    BUFID1(BUFPTR),B'11110000'                          0141 00191000
         BO    @RF00141                                            0141 00192000
*             DO;                   /* IF NO,                        */ 00193000
*               ERRCDE=2;           /* INDICATE ERROR MESSAGE 2      */ 00194000
         LA    ERRCDE,2                                            0143 00195000
*               CALL ERRORTN;       /* CALL ERROR ROUTINE            */ 00196000
         BAL   @14,ERRORTN                                         0144 00197000
*             END;                  /* END FIRST PART OF ID CHECK    */ 00198000
*           ELSE                                                   0146 00199000
*             DO;                                                  0146 00200000
*                                                                  0146 00201000
         B     @RC00141                                            0146 00202000
@RF00141 DS    0H                                                  0147 00203000
*               /*****************************************************/ 00204000
*               /*                                                   */ 00205000
*               /* TEST THE SECOND PART OF ID FOR DECIMAL CHARACTER  */ 00206000
*               /*                                                   */ 00207000
*               /*****************************************************/ 00208000
*                                                                  0147 00209000
*               IF BUFID2(1:4)^='1111'B THEN/* SECOND PART ID DEC    */ 00210000
         TM    BUFID2(BUFPTR),B'11110000'                          0147 00211000
         BO    @RF00147                                            0147 00212000
*                 DO;               /* IF NO,                        */ 00213000
*                   ID1='F0'X;      /* FIRST PART OF ID EQ DECIMAL   */ 00214000
         LA    @10,32                                              0149 00215000
         ALR   @10,R2                                              0149 00216000
         MVI   ID1(@10),X'F0'                                      0149 00217000
*                   ID2=BUFID1;     /* SAVE ID IN XSA                */ 00218000
         MVC   ID2(1,@10),BUFID1(BUFPTR)                           0150 00219000
*                   BUFPTR=BUFPTR-1;/* DECREASE ADDR OF ID BY 1      */ 00220000
         BCTR  BUFPTR,0                                            0151 00221000
*                 END;              /* END SECOND PART OF ID CHECK   */ 00222000
*                                                                  0152 00223000
*               /*****************************************************/ 00224000
*               /*                                                   */ 00225000
*               /* TEST FOR TEXT AFTER REPLY ID                      */ 00226000
*               /*                                                   */ 00227000
*               /*****************************************************/ 00228000
*                                                                  0153 00229000
*               IF BUFCOMMA^=',' THEN/* TEST FOR COMMA AFTER ID      */ 00230000
*                                                                  0153 00231000
@RF00147 CLI   BUFCOMMA(BUFPTR),C','                               0153 00232000
         BE    @RF00153                                            0153 00233000
*                 /***************************************************/ 00234000
*                 /*                                                 */ 00235000
*                 /* TEST FOR NULL REPLY                             */ 00236000
*                 /*                                                 */ 00237000
*                 /***************************************************/ 00238000
*                                                                  0154 00239000
*                 IF BUFCOMMA^=' ' THEN/* NULL REPLY ?               */ 00240000
         CLI   BUFCOMMA(BUFPTR),C' '                               0154 00241000
         BE    @RF00154                                            0154 00242000
*                   DO;             /* IF NO,                        */ 00243000
*                     ERRCDE=5;     /* INDICATE ERROR MESSAGE 5      */ 00244000
         LA    ERRCDE,5                                            0156 00245000
*                     CALL ERRORTN; /* CALL ERROR ROUTINE            */ 00246000
         BAL   @14,ERRORTN                                         0157 00247000
*                   END;            /* END CHECKING FOR TEXT         */ 00248000
*             END;                  /* END CHECKING REPLY ID         */ 00249000
*                                                                  0159 00250000
@RF00154 DS    0H                                                  0159 00251000
@RF00153 DS    0H                                                  0160 00252000
*           /*********************************************************/ 00253000
*           /*                                                       */ 00254000
*           /* TEST TO DETERMINE IF AN ERROR HAS BEEN DETECTED.      */ 00255000
*           /*                                                       */ 00256000
*           /*********************************************************/ 00257000
*                                                                  0160 00258000
*           IF ERRCDE=0 THEN        /* IS THE ERROR CODE SET ?       */ 00259000
@RC00141 LTR   ERRCDE,ERRCDE                                       0160 00260000
         BNZ   @RF00160                                            0160 00261000
*             DO;                   /* IF NO,                        */ 00262000
*/********************************************************************/ 00263000
*/*           SCAN  ID  SEGMENT                                      */ 00264000
*/*                                                                  */ 00265000
*/* THE FOLLOWING SEGMENT WILL SCAN THE ORE CHAIN FOR A REPLY ID     */ 00266000
*/* MATCH. IF ANY ONE OF THE FOLLOWING CONDITIONS ARE TRUE, THE      */ 00267000
*/* REPLY ID SPECIFIED IS NOT OUTSTANDING:                           */ 00268000
*/*     1) NO ORES ON CHAIN                                          */ 00269000
*/*     2) NO ID MATCH                                               */ 00270000
*/*     3) ORE MARKED TO BE DELETED                                  */ 00271000
*/*     4) ORE PARTIALLY PROCESSED                                   */ 00272000
*/*     5) NO ASSOCIATED WQE                                         */ 00273000
*/*     6) ASSOCIATED WQE MARKED SUSPENDED                           */ 00274000
*/********************************************************************/ 00275000
*                                                                  0162 00276000
*               RFY                                                0162 00277000
*                (BYPASSW) RSTD;    /* RESTRICT SWITCH REG           */ 00278000
*               BYPASSW=0;          /* TURN BYPASS SWITCH OFF        */ 00279000
         SLR   BYPASSW,BYPASSW                                     0163 00280000
*               OREPTR=UCMRPYQ;     /* SAVE ADDR OF ORE CHAIN        */ 00281000
*                                                                  0164 00282000
         L     @10,CVTPTR                                          0164 00283000
         L     @10,UCMBASPT(,@10)                                  0164 00284000
         L     OREPTR,UCMRPYQ(,@10)                                0164 00285000
*               /*****************************************************/ 00286000
*               /*                                                   */ 00287000
*               /* SCAN ORE CHAIN FOR REPLY ID MATCH                 */ 00288000
*               /*                                                   */ 00289000
*               /*****************************************************/ 00290000
*                                                                  0165 00291000
*               DO WHILE(OREPTR^=0&BYPASSW^=1);/* SCAN FOR ID MATCH  */ 00292000
*                                                                  0165 00293000
         B     @DE00165                                            0165 00294000
@DL00165 DS    0H                                                  0166 00295000
*                 /***************************************************/ 00296000
*                 /*                                                 */ 00297000
*                 /* CHECK REPLY ID WIYH ORE ID                      */ 00298000
*                 /*                                                 */ 00299000
*                 /***************************************************/ 00300000
*                                                                  0166 00301000
*                 IF IDSAV=OREID THEN/* IDS MATCH ?                  */ 00302000
         CLC   IDSAV+32(2,R2),OREID(OREPTR)                        0166 00303000
         BNE   @RF00166                                            0166 00304000
*                   DO;             /* IF YES,                       */ 00305000
*                                                                  0167 00306000
*                     /***********************************************/ 00307000
*                     /*                                             */ 00308000
*                     /* HAS THIS ORE BEEN MARKED TO BE DELETED ?    */ 00309000
*                     /*                                             */ 00310000
*                     /***********************************************/ 00311000
*                                                                  0168 00312000
*                     IF OREBUFC^='1'B THEN/* DELETE ORE ?           */ 00313000
         TM    OREBUFC(OREPTR),B'00100000'                         0168 00314000
         BO    @RF00168                                            0168 00315000
*                       DO;         /* IF NO,                        */ 00316000
*                                                                  0169 00317000
*                         /*******************************************/ 00318000
*                         /*                                         */ 00319000
*                         /* HAS THIS ORE BEEN PROCESSED BY REPLY ?  */ 00320000
*                         /*                                         */ 00321000
*                         /*******************************************/ 00322000
*                                                                  0170 00323000
*                         IF OREOPBUF=0 THEN/* ORE PROCESSED ?       */ 00324000
         SLR   @10,@10                                             0170 00325000
         C     @10,OREOPBUF(,OREPTR)                               0170 00326000
         BNE   @RF00170                                            0170 00327000
*                           DO;     /* IF NO,                        */ 00328000
*                             WQEPTR=OREWQE;/* GET ADDR OF WQE       */ 00329000
*                                                                  0172 00330000
         L     WQEPTR,OREWQE(,OREPTR)                              0172 00331000
*                             /***************************************/ 00332000
*                             /*                                     */ 00333000
*                             /* TEST TO DETERMINE IF A WQE EXIST    */ 00334000
*                             /*                                     */ 00335000
*                             /***************************************/ 00336000
*                                                                  0173 00337000
*                             IF WQEPTR^=0 THEN/* WQE EXIST ?        */ 00338000
         CR    WQEPTR,@10                                          0173 00339000
         BE    @RF00173                                            0173 00340000
*                               DO; /* IF YES,                       */ 00341000
*                                                                  0174 00342000
*                                 /***********************************/ 00343000
*                                 /*                                 */ 00344000
*                                 /* WQE MARKED SUSPENDED ?          */ 00345000
*                                 /*                                 */ 00346000
*                                 /***********************************/ 00347000
*                                                                  0175 00348000
*                                 IF WQESUSP^='1'B THEN/* SUSPENDED ?*/ 00349000
         TM    WQESUSP(WQEPTR),B'00000010'                         0175 00350000
         BO    @RF00175                                            0175 00351000
*                                   BYPASSW=1;/* TURN BYPASS SWITCH     00352000
*                                      ON                            */ 00353000
         LA    BYPASSW,1                                           0176 00354000
*                                 ELSE/* SUSPEND NOT INDICATED       */ 00355000
*                                   OREPTR=0;/* GET OUT OF LOOP      */ 00356000
         B     @RC00175                                            0177 00357000
@RF00175 SLR   OREPTR,OREPTR                                       0177 00358000
*                               END;/* END WQE TEST                  */ 00359000
*                             ELSE  /* NO WQE EXIST                  */ 00360000
*                               OREPTR=0;/* GET OUT OF LOOP          */ 00361000
         B     @RC00173                                            0179 00362000
@RF00173 SLR   OREPTR,OREPTR                                       0179 00363000
*                           END;    /* END ORE PROCESS TEST          */ 00364000
*                         ELSE      /* ORE NOT PROCESSED             */ 00365000
*                           OREPTR=0;/* GET OUT OF LOOP              */ 00366000
         B     @RC00170                                            0181 00367000
@RF00170 SLR   OREPTR,OREPTR                                       0181 00368000
*                       END;        /* END DELETE ORE TEST           */ 00369000
*                     ELSE          /* IF ERROR WAS DETECTED         */ 00370000
*                       OREPTR=0;   /* SET CONDITION TO BYPASS LOOP  */ 00371000
         B     @RC00168                                            0183 00372000
@RF00168 SLR   OREPTR,OREPTR                                       0183 00373000
*                   END;            /* END ID MATCH                  */ 00374000
*                 ELSE              /* IF NO ID MATCH                */ 00375000
*                   OREPTR=ORELKP;  /* GET POINTER TO NEXT ORE       */ 00376000
         B     @RC00166                                            0185 00377000
@RF00166 L     OREPTR,ORELKP(,OREPTR)                              0185 00378000
*               END;                /* END DO WHILE LOOP             */ 00379000
*                                                                  0186 00380000
@RC00166 DS    0H                                                  0186 00381000
@DE00165 LTR   OREPTR,OREPTR                                       0186 00382000
         BZ    @DC00165                                            0186 00383000
         C     BYPASSW,@CF00047                                    0186 00384000
         BNE   @DL00165                                            0186 00385000
@DC00165 DS    0H                                                  0187 00386000
*               /*****************************************************/ 00387000
*               /*                                                   */ 00388000
*               /* TEST TO ISSUE ERROR CONDITION                     */ 00389000
*               /*                                                   */ 00390000
*               /*****************************************************/ 00391000
*                                                                  0187 00392000
*               IF BYPASSW=0 THEN   /* BYPASS SWITCH SET             */ 00393000
         LTR   BYPASSW,BYPASSW                                     0187 00394000
         BNZ   @RF00187                                            0187 00395000
*                 DO;               /* IF NO,                        */ 00396000
*                   ERRCDE=4;       /* INDICATE ERROR MESSAGE 4      */ 00397000
         LA    ERRCDE,4                                            0189 00398000
*                   CALL ERRORTN;   /* CALL ERROR ROUTINE            */ 00399000
         BAL   @14,ERRORTN                                         0190 00400000
*                 END;              /* END ERROR PROCESSING          */ 00401000
*               RFY                                                0192 00402000
*                (BYPASSW) UNRSTD;  /* MAKE REG AVAILABLE            */ 00403000
*                                                                  0192 00404000
@RF00187 DS    0H                                                  0193 00405000
*               /*****************************************************/ 00406000
*               /*                                                   */ 00407000
*               /* TEST TO DETERMINE IF AN ERROR HAS BEEN DETECTED.  */ 00408000
*               /*                                                   */ 00409000
*               /*****************************************************/ 00410000
*                                                                  0193 00411000
*               IF ERRCDE=0 THEN    /* IS ERROR CODE SET ?           */ 00412000
         LTR   ERRCDE,ERRCDE                                       0193 00413000
         BNZ   @RF00193                                            0193 00414000
*                 DO;               /* IF NO,                        */ 00415000
*                                                                  0194 00416000
*                   /*************************************************/ 00417000
*                   /*                                               */ 00418000
*                   /* TEST TO DETERMINE IF REPLY WAS ENTERED THROUGH*/ 00419000
*                   /* INPUT STREAM.                                 */ 00420000
*                   /*                                               */ 00421000
*                   /*************************************************/ 00422000
*                                                                  0195 00423000
*                   IF XAU^='00'X THEN/* REPLY FROM INPUT STREAM ?   */ 00424000
         CLI   XAU(R2),X'00'                                       0195 00425000
         BE    @RF00195                                            0195 00426000
*                     DO;           /* IF NO,                        */ 00427000
*/********************************************************************/ 00428000
*/*            AUTHORITY   CHECKING   SEGMENT                        */ 00429000
*/*                                                                  */ 00430000
*/* THE FOLLOWING CODE CHECKS THE AUTHORIZATION OF THE RESPONDING    */ 00431000
*/* CONSOLE TO ANSWER THE WTOR MESSAGE. ANY ONE OF THE FOLLOWING     */ 00432000
*/* CONDITIONS ENABLES THE CONSOLE TO ANSWER.                        */ 00433000
*/*                                                                  */ 00434000
*/* 1. THE RESPONDING CONSOLE IS A MASTER CONSOLE.                   */ 00435000
*/*                                                                  */ 00436000
*/* 2. THE RESPONDING CONSOLE'S UNIQUE UCM ENTRY ID MATCHES.         */ 00437000
*/*                                                                  */ 00438000
*/* 3. THE RESPONDING CONSOLE IS MARKED TO ACCEPT ONE OF THE ROUTING */ 00439000
*/*    CODES TO WHICH THE WTOR WAS ISSUED.                           */ 00440000
*/*                                                                  */ 00441000
*/* 4. THE WTOR WAS ISSUED TO ALL CONSOLES.                          */ 00442000
*/*                                                                  */ 00443000
*/* 5. THE WTOR WAS OF THE MSG TYPE WHICH THIS CONSOLE RECEIVES.     */ 00444000
*/*                                                                  */ 00445000
*/* INPUT:                                                           */ 00446000
*/*  WQE,UCME,XSA,UCM                                                */ 00447000
*/*                                                                  */ 00448000
*/* OUTPUT: ERRMSG3 OR CONTINUE PROCESSING                           */ 00449000
*/*  IEE703I REPLY ID NOT REQUESTED FROM THIS CONSOLE.               */ 00450000
*/*                                                                  */ 00451000
*/********************************************************************/ 00452000
*                                                                  0197 00453000
*                       ENDSEG=0;   /* INIT END INDICATOR            */ 00454000
         SLR   ENDSEG,ENDSEG                                       0197 00455000
*                       UCMDVEPT=UCMVEA;/* ADDR OF FIRST UCME        */ 00456000
*                                                                  0198 00457000
         L     @10,CVTPTR                                          0198 00458000
         L     @10,UCMBASPT(,@10)                                  0198 00459000
         L     UCMDVEPT,UCMVEA(,@10)                               0198 00460000
*                       /*********************************************/ 00461000
*                       /*                                           */ 00462000
*                       /* SCAN UCME CHAIN FOR MATCHING IDS.         */ 00463000
*                       /*                                           */ 00464000
*                       /*********************************************/ 00465000
*                                                                  0199 00466000
*                       DO WHILE(UCMDVEPT<=UCMVEL&UCMID^=XAU);     0199 00467000
         B     @DE00199                                            0199 00468000
@DL00199 DS    0H                                                  0200 00469000
*                         UCMDVEPT=UCMDVEPT+UCMVEZ;/* GET ADDR OF  0200 00470000
*                                      NEXT UCME                     */ 00471000
         L     @10,CVTPTR                                          0200 00472000
         L     @10,UCMBASPT(,@10)                                  0200 00473000
         AL    UCMDVEPT,UCMVEZ(,@10)                               0200 00474000
*                       END;        /* END UCME SCAN                 */ 00475000
*                                                                  0201 00476000
@DE00199 L     @10,CVTPTR                                          0201 00477000
