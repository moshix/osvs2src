         STH   LNGREG,TEMPLNT(,@10)                                0373 01320000
*                             END;  /* END MOVING OF TEXT            */ 01321000
*                                                                  0374 01322000
*                           /*****************************************/ 01323000
*                           /*                                       */ 01324000
*                           /* TEST WQE FOR SECURITY REPLY           */ 01325000
*                           /*                                       */ 01326000
*                           /*****************************************/ 01327000
*                                                                  0375 01328000
*                           IF WQEROUTI='1'B THEN/* SECURITY REPLY ? */ 01329000
@RC00347 TM    WQEROUTI(WQEPTR),B'10000000'                        0375 01330000
         BNO   @RF00375                                            0375 01331000
*                             DO;   /* IF YES,                       */ 01332000
*/********************************************************************/ 01333000
*/*                        SECURITY   SEGMENT                        */ 01334000
*/*                                                                  */ 01335000
*/* THE FOLLOWING CODE WILL OVERLAY THE TEXT PORTION OF THE          */ 01336000
*/* OPERATOR'S REPLY WITH THE WORD 'SUPPRESSED'. THIS                */ 01337000
*/* INDICATES THAT THE OPERATOR'S REPLY IS A SECURITY                */ 01338000
*/* RESPONSE AND THE TEXT IS NOT TO BE SHOWN.                        */ 01339000
*/*                                                                  */ 01340000
*/********************************************************************/ 01341000
*                                                                  0377 01342000
*                               LNGREG=LENGTH(SUPHEAD);/* SET NEW  0377 01343000
*                                      LENGTH OF TEXT                */ 01344000
         LA    @10,10                                              0377 01345000
         LR    LNGREG,@10                                          0377 01346000
*                               TXTLNG=(XAL-XAR)+(LENGTH(SUPHEAD)+ADDR( 01347000
*                                   BUFTEXT)-XAL);/* STORE NEW LENGTH   01348000
