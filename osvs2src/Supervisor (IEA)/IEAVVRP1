*                             END;  /* END IF TEXT IS IN QUOTES      */ 01089000
*                                                                  0320 01090000
*                           /*****************************************/ 01091000
*                           /*                                       */ 01092000
*                           /* TEST TO DETERMINE IF AN ERROR WAS     */ 01093000
*                           /* DETECTED.                             */ 01094000
*                           /*                                       */ 01095000
*                           /*****************************************/ 01096000
*                                                                  0321 01097000
