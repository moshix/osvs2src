*                           /*                                       */ 00735000
*                           /* *** END AUTHORITY SEGMENT (PART 2)    */ 00736000
*                           /*                                       */ 00737000
*                           /*****************************************/ 00738000
*                                                                  0256 00739000
*                         END;      /* END AUTHORITY CHECKING        */ 00740000
*                                                                  0256 00741000
@RF00237 DS    0H                                                  0257 00742000
*                       /*********************************************/ 00743000
*                       /*                                           */ 00744000
*                       /* ************ END AUTHORITY SEGMENT        */ 00745000
*                       /*                                           */ 00746000
*                       /*********************************************/ 00747000
*                                                                  0257 00748000
*                     END;          /* END TEST AUTHORITY            */ 00749000
*                                                                  0257 00750000
@RF00235 DS    0H                                                  0258 00751000
*                   /*************************************************/ 00752000
*                   /*                                               */ 00753000
*                   /* TEST TO DETERMINE IF AN ERROR HAS BEEN        */ 00754000
*                   /* DETECTED.                                     */ 00755000
*                   /*                                               */ 00756000
*                   /*************************************************/ 00757000
*                                                                  0258 00758000
*                   IF ERRCDE=0 THEN/* IS ERROR CODE SET ?           */ 00759000
@RF00195 LTR   ERRCDE,ERRCDE                                       0258 00760000
         BNZ   @RF00258                                            0258 00761000
*                     DO;           /* IF NO,                        */ 00762000
*                       LNGVAL=0;   /* INDICATE ZERO REPLY LENGTH    */ 00763000
*                                                                  0260 00764000
         SLR   LNGVAL,LNGVAL                                       0260 00765000
*                       /*********************************************/ 00766000
*                       /*                                           */ 00767000
*                       /* TEST TO DETERMINE IF THIS IS A NULL REPLY.*/ 00768000
*                       /*                                           */ 00769000
*                       /*********************************************/ 00770000
*                                                                  0261 00771000
*                       IF BUFCOMMA^=' ' THEN/* NULL REPLY ?         */ 00772000
         CLI   BUFCOMMA(BUFPTR),C' '                               0261 00773000
         BE    @RF00261                                            0261 00774000
*                         DO;       /* IF NO,                        */ 00775000
*/********************************************************************/ 00776000
*/*      SYNTAX  CHECKING  SEGMENT                                   */ 00777000
*/*                                                                  */ 00778000
*/* THE FOLLOWING SEGMENT SCANS THE TEXT PORTION OF THE REPLY BUFFER */ 00779000
*/* FOR PROPER SYNTAX. IF THE FIRST TEXT CHAR IS NOT A QUOTE, THE    */ 00780000
*/* TEXT IS SCANNED FOR THE LAST NON-BLANK CHAR TO DETERMINE THE     */ 00781000
*/* LENGTH OF THE TEXT. TRAILING BLANKS ARE EXCLUDED FROM THE LENGTH.*/ 00782000
*/* IF THE FIRST CHARACTER OF THE TEXT IS A QUOTE, THEN THE TEXT     */ 00783000
*/* IS ASSUMED TO BE EMBEDDED IN QUOTES. THE TEXT IS THEN SCANNED    */ 00784000
*/* LOOKING FOR PAIRED DOUBLE QUOTES KEEPING A COUNT OF THE NUMBER   */ 00785000
*/* OF CHARACTERS SCANNED (PAIRED DOUBLE QUOTES ARE COUNTED AS ONE   */ 00786000
*/* CHARACTER). THE SCAN CONTINUES UNTIL A SINGLE QUOTE IS FOUND OR  */ 00787000
*/* THE END OF THE BUFFER IS REACHED. IF THE END OF THE BUFFER IS    */ 00788000
*/* REACHED AND THE LAST CHARACTER IS NOT A QUOTE OR THE LAST        */ 00789000
*/* CHARACTER IS A QUOTE BUT IS PRECEDED BY AN ODD NUMBER OF QUOTES, */ 00790000
*/* AN ERROR MESSAGE (IMPROPER USE OF DELIMETERS) WILL BE ISSUED.    */ 00791000
*/*                                                                  */ 00792000
*/* IF THE LENGTH OF THE REPLY BUFFER IS GREATER THEN THE LENGTH     */ 00793000
*/* OF THE USER'S BUFFER, AN ERROR MESSAGE (REPLY TOO LONG) WILL BE  */ 00794000
*/* ISSUED.                                                          */ 00795000
*/*                                                                  */ 00796000
*/* INPUT:                                                           */ 00797000
*/*  XSA,ORE                                                         */ 00798000
*/*                                                                  */ 00799000
*/* OUTPUT:                                                          */ 00800000
*/*  ERROR - IEE702I REPLY ID IGNORED; IMPROPER USE OF DELIMETERS.   */ 00801000
*/*          IEE700I REPLY ID IGNORED; REPLY TOO LONG FOR REQUESTOR. */ 00802000
*/*  NORMAL- CONTINUE PROCESSING                                     */ 00803000
*/********************************************************************/ 00804000
*                                                                  0263 00805000
*                           /*****************************************/ 00806000
*                           /*                                       */ 00807000
*                           /* TEST FOR TEXT ENCLOSED IN QUOTES      */ 00808000
*                           /*                                       */ 00809000
*                           /*****************************************/ 00810000
*                                                                  0263 00811000
*                           IF BUFTEXT(1)^='''' THEN/* TEXT ENCLOSED    00812000
*                                      IN QUOTES ?                   */ 00813000
         CLI   BUFTEXT(BUFPTR),C''''                               0263 00814000
         BE    @RF00263                                            0263 00815000
*                             DO;   /* IF NO,                        */ 00816000
*                                                                  0264 00817000
*                               /*************************************/ 00818000
*                               /*                                   */ 00819000
*                               /* SYNTAX CHECKING SEGMENT           */ 00820000
*                               /* CONTINUATION                      */ 00821000
*                               /*                                   */ 00822000
*                               /*************************************/ 00823000
*                                                                  0265 00824000
*                               ENBUF=(XAR+TXTLNG)-ADDR(BUFTEXT);/*     00825000
*                                      POINT TO END OF BUFFER        */ 00826000
*                                                                  0265 00827000
         L     @10,XAR-1(,R2)                                      0265 00828000
         LA    @10,0(,@10)                                         0265 00829000
         LH    @09,TXTLNG(,@10)                                    0265 00830000
         ALR   @09,@10                                             0265 00831000
         LA    @10,BUFTEXT(,BUFPTR)                                0265 00832000
         SLR   @09,@10                                             0265 00833000
         ST    @09,ENBUF                                           0265 00834000
*                               /*************************************/ 00835000
*                               /*                                   */ 00836000
*                               /* SCAN INPUT TEXT FOR NON - BLANK   */ 00837000
*                               /* CHARACTER.                        */ 00838000
*                               /*                                   */ 00839000
*                               /*************************************/ 00840000
*                                                                  0266 00841000
*                               DO INDX=1 TO ENBUF BY 1;/* SCAN FOR     00842000
*                                      END OF BUFFER                 */ 00843000
*                                                                  0266 00844000
         LA    INDX,1                                              0266 00845000
         B     @DE00266                                            0266 00846000
@DL00266 DS    0H                                                  0267 00847000
*                                 /***********************************/ 00848000
*                                 /*                                 */ 00849000
*                                 /* SCAN CHARACTER FOR A BLANK.     */ 00850000
*                                 /*                                 */ 00851000
*                                 /***********************************/ 00852000
*                                                                  0267 00853000
*                                 IF BUFTEXT(INDX)^=' ' THEN/* BLANK    00854000
*                                      CHARACTER IN TEXT ?           */ 00855000
         LR    @10,BUFPTR                                          0267 00856000
         ALR   @10,INDX                                            0267 00857000
         CLI   BUFTEXT-1(@10),C' '                                 0267 00858000
         BE    @RF00267                                            0267 00859000
*                                   DO;/* IF NO,                     */ 00860000
*                                    LNGVAL=ADDR(BUFTEXT(INDX));/* 0269 00861000
*                                      SAVE ADDR OF NON-BLANK        */ 00862000
         LA    LNGVAL,BUFTEXT-1(INDX,BUFPTR)                       0269 00863000
*                                   END;/* END IF NON-BLANK FOUND    */ 00864000
*                               END;/* END SCAN OF BUFFER            */ 00865000
*                                                                  0271 00866000
@RF00267 AL    INDX,@CF00047                                       0271 00867000
@DE00266 C     INDX,ENBUF                                          0271 00868000
         BNH   @DL00266                                            0271 00869000
*                               /*************************************/ 00870000
*                               /*                                   */ 00871000
*                               /* TEST FOR NULL INPUT TEXT.         */ 00872000
*                               /*                                   */ 00873000
*                               /*************************************/ 00874000
*                                                                  0272 00875000
*                               IF LNGVAL^=0 THEN/* NULL INPUT TEXT ?*/ 00876000
         LTR   LNGVAL,LNGVAL                                       0272 00877000
         BZ    @RF00272                                            0272 00878000
*                                 DO;/* IF NO,                       */ 00879000
*                                   LNGVAL=LNGVAL-ADDR(BUFTEXT)+1;/*    00880000
*                                      CALCULATE LENGTH OF TEXT      */ 00881000
         LA    @10,BUFTEXT(,BUFPTR)                                0274 00882000
         LCR   @10,@10                                             0274 00883000
         ALR   @10,LNGVAL                                          0274 00884000
         AL    @10,@CF00047                                        0274 00885000
         LR    LNGVAL,@10                                          0274 00886000
*                                 END;/* END IF NO NULL INPUT        */ 00887000
*                                                                  0275 00888000
*                               /*************************************/ 00889000
*                               /*                                   */ 00890000
*                               /* ************ END SYNTAX CHECK     */ 00891000
*                               /* SEGMENT (PART 2)***********       */ 00892000
*                               /*                                   */ 00893000
*                               /*************************************/ 00894000
*                                                                  0276 00895000
*                             END;  /* END TEXT NOT IN QUOTES        */ 00896000
*                           ELSE    /* IF TEXT IS IN QUOTES          */ 00897000
*                             DO;   /* SCAN TEXT                     */ 00898000
         B     @RC00263                                            0277 00899000
@RF00263 DS    0H                                                  0278 00900000
*                               XAD=0;/* TURN BYPASS SWITCH OFF      */ 00901000
         SLR   @10,@10                                             0278 00902000
         ST    @10,XAD(,R2)                                        0278 00903000
*                               DQSWIT=0;/* INIT EVEN-ODD INDICATOR  */ 00904000
         STH   @10,DQSWIT                                          0279 00905000
*                               DQPAIRS=0;/* INDICATE NO DOUBLE    0280 00906000
*                                      QUOTES                        */ 00907000
         STH   @10,DQPAIRS                                         0280 00908000
*                               ENBUF=XAR+TXTLNG-1;/* POINT TO END OF   00909000
*                                      BUFFER -ONE                   */ 00910000
*                                                                  0281 00911000
         L     @10,XAR-1(,R2)                                      0281 00912000
         LA    @10,0(,@10)                                         0281 00913000
         LH    @09,TXTLNG(,@10)                                    0281 00914000
         ALR   @09,@10                                             0281 00915000
         BCTR  @09,0                                               0281 00916000
         ST    @09,ENBUF                                           0281 00917000
*                               /*************************************/ 00918000
*                               /*                                   */ 00919000
*                               /* SCAN BUFFER TEXT FOR PAIRED DOUBLE*/ 00920000
*                               /* QUOTES                            */ 00921000
*                               /*                                   */ 00922000
*                               /*************************************/ 00923000
*                                                                  0282 00924000
*                               DO INDX=2 BY 1 WHILE(XAD=0);       0282 00925000
*                                                                  0282 00926000
         LA    INDX,2                                              0282 00927000
         B     @DE00282                                            0282 00928000
@DL00282 DS    0H                                                  0283 00929000
*                                 /***********************************/ 00930000
*                                 /*                                 */ 00931000
*                                 /* CHECK FOR END OF BUFFER         */ 00932000
*                                 /*                                 */ 00933000
*                                 /***********************************/ 00934000
*                                                                  0283 00935000
*                                 IF ADDR(BUFTEXT(INDX))=ENBUF THEN/*   00936000
*                                      END OF BUFFER ?               */ 00937000
         LA    @10,BUFTEXT-1(INDX,BUFPTR)                          0283 00938000
         C     @10,ENBUF                                           0283 00939000
         BNE   @RF00283                                            0283 00940000
*                                   DO;/* IF YES,                    */ 00941000
*                                                                  0284 00942000
*                                    /********************************/ 00943000
*                                    /*                              */ 00944000
*                                    /* TEST LAST CHARACTER FOR A    */ 00945000
*                                    /* QUOTE.                       */ 00946000
*                                    /*                              */ 00947000
*                                    /********************************/ 00948000
*                                                                  0285 00949000
*                                    IF BUFTEXT(INDX)^='''' THEN/* 0285 00950000
*                                      LAST CHA A QUOTE ?            */ 00951000
         LR    @10,BUFPTR                                          0285 00952000
         ALR   @10,INDX                                            0285 00953000
         CLI   BUFTEXT-1(@10),C''''                                0285 00954000
         BE    @RF00285                                            0285 00955000
*                                     DO;/* IF NO,                   */ 00956000
*                                      ERRCDE=5;/* INDICATE DELIMETER   00957000
*                                      ERROR                         */ 00958000
         LA    ERRCDE,5                                            0287 00959000
*                                      CALL ERRORTN;/* CALL ERROR RTN*/ 00960000
         BAL   @14,ERRORTN                                         0288 00961000
*                                     END;/* END IF NO QUOTE         */ 00962000
*                                    ELSE/* IF LAST CHAR IS A QUOTE  */ 00963000
*                                     DO;/* CHECK FOR PAIRS          */ 00964000
*                                                                  0290 00965000
         B     @RC00285                                            0290 00966000
@RF00285 DS    0H                                                  0291 00967000
*                                      /******************************/ 00968000
*                                      /*                            */ 00969000
*                                      /* TEST FOR ODD NUMBER OF     */ 00970000
*                                      /* QUOTES.                    */ 00971000
*                                      /*                            */ 00972000
*                                      /******************************/ 00973000
*                                                                  0291 00974000
*                                      IF DQSWIT=1 THEN/* UNEVEN # OF   00975000
*                                      QUOTES ?                      */ 00976000
         CLC   DQSWIT(2),@CH00047                                  0291 00977000
         BNE   @RF00291                                            0291 00978000
*                                       DO;/* IF YES,                */ 00979000
*                                        ERRCDE=5;/* INDICATE      0293 00980000
*                                      DELIMETER ERROR               */ 00981000
         LA    ERRCDE,5                                            0293 00982000
*                                        CALL ERRORTN;/* CALL ERROR     00983000
*                                      RTN                           */ 00984000
         BAL   @14,ERRORTN                                         0294 00985000
*                                       END;/* END IF ODD QUOTES     */ 00986000
*                                     END;/* END LAST CHAR TESTING   */ 00987000
