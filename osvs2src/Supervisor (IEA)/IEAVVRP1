*                           WQEPTR=XAD;/* RESTORE PTR TO WQE         */ 01188000
         L     WQEPTR,XAD(,R2)                                     0344 01189000
*                           TEMPBUF(1:LNGREG)=' ';/* BLANK AREA      */ 01190000
         L     @10,OREOPBUF(,OREPTR)                               0345 01191000
