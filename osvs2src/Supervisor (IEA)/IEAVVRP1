         BNE   @RF00487                                            0487 01829000
*     DO;                           /* IF YES,                       */ 01830000
*       ERRORBUF(1:LENGTH(ERRMSG1))=/* STORE ERROR MESSAGE           */ 01831000
*           ERRMSG1;                /* IN BUFFER                     */ 01832000
         MVC   ERRORBUF(53,BUFPTR),ERRMSG1                         0489 01833000
*     END;                          /* END MSG 1                     */ 01834000
*   ELSE                            /* IF NOT MSG 1                  */ 01835000
*                                                                  0491 01836000
*     /***************************************************************/ 01837000
*     /*                                                             */ 01838000
*     /* TEST FOR ERROR MESSAGE 2                                    */ 01839000
*     /*                                                             */ 01840000
*     /***************************************************************/ 01841000
*                                                                  0491 01842000
*     IF ERRCDE=2 THEN              /* ERROR MESSAGE 2 ?             */ 01843000
         B     @RC00487                                            0491 01844000
@RF00487 C     ERRCDE,@CF00109                                     0491 01845000
         BNE   @RF00491                                            0491 01846000
*       DO;                         /* IF YES,                       */ 01847000
*         ERRORBUF(1:LENGTH(ERRMSG2))=/* STORE ERROR MESSAGE         */ 01848000
*             ERRMSG2;              /* IN BUFFER                     */ 01849000
