         BCTR  LNGREG,0                                            0346 01196000
*                           /*****************************************/ 01197000
*                           /*                                       */ 01198000
*                           /* TEST FOR NULL REPLY                   */ 01199000
*                           /*                                       */ 01200000
*                           /*****************************************/ 01201000
*                                                                  0347 01202000
*                           IF LNGREG=0 THEN/* NULL REPLY ?          */ 01203000
         SLR   @09,@09                                             0347 01204000
         CR    LNGREG,@09                                          0347 01205000
         BNE   @RF00347                                            0347 01206000
*                             DO;   /* IF YES,                       */ 01207000
*                               TEMPLNT=0;/* SET ZERO LENGTH         */ 01208000
         STH   @09,TEMPLNT(,@10)                                   0349 01209000
*                             END;  /* END IF NULL REPLY             */ 01210000
