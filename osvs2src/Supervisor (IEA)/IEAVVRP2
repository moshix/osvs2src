        SETLOCK  RELEASE,TYPE=CMS,RELATED=(ORE,IEAVVRP2(SETLOCKS))      00856000
        SETLOCK  RELEASE,TYPE=LOCAL,RELATED=(ORE,IEAVVRP2(SETLOCKS))    00857000
*   BASREG=R7;                      /* RESTORE BASE REGISTER         */ 00858000
