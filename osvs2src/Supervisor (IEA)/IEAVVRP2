        L     R10,SAVREG              SAVE RETURN REGISTER              00849000
        FREEMAIN RU,LV=(R2),A=(1),SP=229,BRANCH=YES            YM04721  00850000
*   R7=BASREG;                      /* SAVE BASE REGISTER            */ 00851000
         LR    R7,BASREG                                           0287 00852000
