*   MGPPRINT=ON;                    /* SET IND TO PUT MSG ON SYSOUT  */ 00026000
*   MGPHLDS=ON;                     /* MESSAGES TO LOB               */ 00027000
*                                                                  0209 00028000
         OI    MGPPRINT,B'10010000'                                0209 00029000
