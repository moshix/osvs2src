*   RFY                                                            0289 00373000
*     RTNCODE RSTD;                 /* RESTRICD CODE REG             */ 00374000
*   RVAEPRNM=SAVRVANM;              /* RESTORE RVA                   */ 00375000
         L     @14,CCARVAD(,CCAPTR)                                0290 00376000
         MVC   RVAEPRNM(8,@14),SAVRVANM                            0290 00377000
*   CVD(RTNCODE,DBLWRD);            /* CONVERT RETURN CODE TO DECIMAL*/ 00378000
         CVD   RTNCODE,DBLWRD                                      0291 00379000
*   MGPCLEAR=''B;                   /* CLEAR MSG PARMS               */ 00380000
