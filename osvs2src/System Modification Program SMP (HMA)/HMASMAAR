*   HIGHAAR=ZERO;                   /* INITIALIZE THE RC AREA        */ 00020000
         LH    HIGHAAR,ZERO                                        0205 00021000
*   SWITCH1=CLEAR;                  /* SET PROGRAM SWITCH TO ZERO    */ 00022000
