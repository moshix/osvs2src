*             MGPVARPT(2)=ADDR(THISVOL);/* VOLUME                    */ 00393000
         LA    @10,THISVOL                                         0326 00394000
         ST    @10,MGPVARPT+4                                      0326 00395000
*             CALL HMASMMSG(HMASMMGP);/* WRITE MSG                   */ 00396000
         L     @15,@CV03092                                        0327 00397000
         LA    @01,@AL00327                                        0327 00398000
         BALR  @14,@15                                             0327 00399000
*             ALCRC=STOPPTF;        /* SET RC=8 TO STOP PTF          */ 00400000
         LA    ALCRC,8                                             0328 00401000
