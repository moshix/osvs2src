         TITLE 'HMASMALC - DYNAMIC ALLOCATION PROCEEDURE'               00001000
HMASMALC CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMALC  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(8),0(@01)                                  0001 00017000
*   ALCRTNCD=ZERO;                  /*                               */ 00018000
         SLR   ALCRTNCD,ALCRTNCD                                   0214 00019000
*   IOPPTR=ADDR(PASSIOP);           /* SET UP IOP ADDRESSABILITY     */ 00020000
         L     IOPPTR,@PC00001                                     0215 00021000
*   JFCBPTR=ADDR(PASSJFCB);         /* SET UP JFCB ADDRESSABILITY    */ 00022000
         L     JFCBPTR,@PC00001+4                                  0216 00023000
*   HMASMMGP=''B;                   /* ZERO MSG PARM                 */ 00024000
         XC    HMASMMGP(24),HMASMMGP                               0217 00025000
*   MGPPRINT=ON;                    /* MSG TO PRINT                  */ 00026000
*   MGPHLDS=ON;                     /* AND LOG                       */ 00027000
         OI    MGPPRINT,B'10010000'                                0219 00028000
*   IF INITSW=OFF                   /* IF INITIAL ENTRY THEN         */ 00029000
*     THEN                          /*                               */ 00030000
         TM    INITSW,B'10000000'                                  0220 00031000
         BNZ   @RF00220                                            0220 00032000
*     DO;                           /* INITIALIZE LOCAL LISTS        */ 00033000
*       TCBPTR=CVTNTCB;             /* PICK UP CURRENT TCB PTR       */ 00034000
         L     @10,CVTPTR                                          0222 00035000
         L     @10,CVTTCBP(,@10)                                   0222 00036000
         L     TCBPTR,CVTNTCB(,@10)                                0222 00037000
*       IF JFCBVOLS(1)^=BLANK       /* IF VOLUME SPECIFIED           */ 00038000
*         THEN                      /* SAVE LIST AND GET UCBS        */ 00039000
         CLI   JFCBVOLS(JFCBPTR),C' '                              0223 00040000
         BE    @RF00223                                            0223 00041000
*         DO;                       /*                               */ 00042000
*           INITSW=ON;              /* INDICATE INTIALIZED           */ 00043000
         OI    INITSW,B'10000000'                                  0225 00044000
*           ORGVOLS=JFCBVOLS;       /* SAVE ORIGINAL VOLUME LIST     */ 00045000
         MVC   ORGVOLS(30),JFCBVOLS(JFCBPTR)                       0226 00046000
*           TIOTPTR=ADDR(TCBTIO->TIOT1);/* SET UP TIOT ADDRESS       */ 00047000
         L     TIOTPTR,TCBTIO(,TCBPTR)                             0227 00048000
*           L=TIOELNGH;             /* GET LENGTH OF FIRST ENTRY     */ 00049000
         SLR   L,L                                                 0228 00050000
         IC    L,TIOELNGH(,TIOTPTR)                                0228 00051000
*           DO WHILE TIOEDDNM^=SMPTLIB/* LOOK FOR SMPTLIB DD CARD OR */ 00052000
*                 &L>0;             /* END OF TIOT                   */ 00053000
         B     @DE00229                                            0229 00054000
@DL00229 DS    0H                                                  0230 00055000
*             TIOTPTR=TIOTPTR+L;    /* POINT TO NEXT DD ENTRY        */ 00056000
         ALR   TIOTPTR,L                                           0230 00057000
*             L=TIOELNGH;           /* GET LENGTH OF THIS ENTRY      */ 00058000
         SLR   L,L                                                 0231 00059000
         IC    L,TIOELNGH(,TIOTPTR)                                0231 00060000
*           END;                                                   0232 00061000
@DE00229 CLC   TIOEDDNM(8,TIOTPTR),SMPTLIB                         0232 00062000
         BE    @DC00229                                            0232 00063000
         LTR   L,L                                                 0232 00064000
         BP    @DL00229                                            0232 00065000
@DC00229 DS    0H                                                  0233 00066000
*           IF TIOEDDNM=SMPTLIB     /* IF SMPTLIB FOUND THEN         */ 00067000
*             THEN                  /*                               */ 00068000
         CLC   TIOEDDNM(8,TIOTPTR),SMPTLIB                         0233 00069000
         BNE   @RF00233                                            0233 00070000
*             DO;                   /*                               */ 00071000
*               LLCOUNT=0;          /* CLEAR LOCAL LIST COUNT        */ 00072000
         SLR   @10,@10                                             0235 00073000
         STH   @10,LLCOUNT                                         0235 00074000
*               L=(L-16)/4;         /* GET DEVICE ENTRY TOTAL        */ 00075000
         LR    @00,L                                               0236 00076000
         SH    @00,@CH00189                                        0236 00077000
         SRDA  @00,32                                              0236 00078000
         D     @00,@CF00039                                        0236 00079000
         LR    L,@01                                               0236 00080000
*               DO WHILE(           /* CHECK ALL DEVICE ENTRIES      */ 00081000
*                     L>0           /*                               */ 00082000
*                     &LLCOUNT<DIM(LLDATA)/* OR UP TO 5 VOLS FOUND   */ 00083000
*                     );            /*                               */ 00084000
         B     @DE00237                                            0237 00085000
@DL00237 DS    0H                                                  0238 00086000
*                 LLCOUNT=LLCOUNT+1;/* UPDATE LOCAL LIST COUNT       */ 00087000
         LA    @10,1                                               0238 00088000
         AH    @10,LLCOUNT                                         0238 00089000
         STH   @10,LLCOUNT                                         0238 00090000
*                 UCBPTR=TIOEFSRT;  /* SET UP THE UCB POINTER        */ 00091000
         SLR   @09,@09                                             0239 00092000
         ICM   @09,7,TIOEFSRT(TIOTPTR)                             0239 00093000
         ST    @09,UCBPTR                                          0239 00094000
*                 LLUCB(LLCOUNT)=UCBPTR;/* MOVE TO LOCAL LIST        */ 00095000
         MH    @10,@CH00177                                        0240 00096000
         ST    @09,LLUCB-12(@10)                                   0240 00097000
*                 LLVOL(LLCOUNT)=UCBVOLI;/* SET UP VOLUME ON THE   0241 00098000
*                                      DEVICE                        */ 00099000
         LA    @06,LLVOL-12(@10)                                   0241 00100000
         MVC   0(6,@06),UCBVOLI(@09)                               0241 00101000
*                 TIOTPTR=TIOTPTR+4;/* POINT TO THE NEXT DEVICE      */ 00102000
         AL    TIOTPTR,@CF00039                                    0242 00103000
*                 L=L-1;            /* SUBTRACT THE DEVICE           */ 00104000
         BCTR  L,0                                                 0243 00105000
*               END;                                               0244 00106000
@DE00237 LTR   L,L                                                 0244 00107000
         BNP   @DC00237                                            0244 00108000
         LH    @10,LLCOUNT                                         0244 00109000
         CH    @10,@CH00130                                        0244 00110000
         BL    @DL00237                                            0244 00111000
@DC00237 DS    0H                                                  0245 00112000
*             END;                  /*                               */ 00113000
*           ELSE                    /*                               */ 00114000
*             ALCRTNCD=MAX(ALCRTNCD,STOPSMP);/* SET BAD RC           */ 00115000
         B     @RC00233                                            0246 00116000
@RF00233 LA    @10,16                                              0246 00117000
         CR    ALCRTNCD,@10                                        0246 00118000
         BNL   *+6                                                      00119000
         LR    ALCRTNCD,@10                                        0246 00120000
*         END;                      /* END VOLUME LIST SPECIFIED     */ 00121000
*       ELSE                        /* NO VOLUME LIST SPECIFIED      */ 00122000
*         DO;                       /*                               */ 00123000
         B     @RC00223                                            0248 00124000
@RF00223 DS    0H                                                  0249 00125000
*           MGPCLEAR=''B;           /*                               */ 00126000
         XC    MGPCLEAR(6),MGPCLEAR                                0249 00127000
*           IF IOPFUNCT=IOPALC      /* IF ALLOC OR                   */ 00128000
*               |IOPFUNCT=IOPDALC   /* OR DEALLOCATE                 */ 00129000
*             THEN                  /*                               */ 00130000
         CLI   IOPFUNCT(IOPPTR),13                                 0250 00131000
         BE    @RT00250                                            0250 00132000
         CLI   IOPFUNCT(IOPPTR),14                                 0250 00133000
         BNE   @RF00250                                            0250 00134000
@RT00250 DS    0H                                                  0251 00135000
*             MGPMGNO1=HMA353I;     /* ALLOCATE FAILED FOR X44X      */ 00136000
         MVC   MGPMGNO1(2),@CH03099                                0251 00137000
*           ELSE                    /*                               */ 00138000
*             MGPMGNO1=HMA377I;     /* SCRATCH FAILED FOR X44X       */ 00139000
         B     @RC00250                                            0252 00140000
@RF00250 MVC   MGPMGNO1(2),@CH03103                                0252 00141000
*           MGPMGNO2=PART117;       /* - NO VOLUMES SPECIFIED        */ 00142000
@RC00250 MVC   MGPMGNO2(2),@CH03115                                0253 00143000
*           MGPTYPE=MGPSEV;         /*                               */ 00144000
         MVI   MGPTYPE,X'04'                                       0254 00145000
*           MGPVARPT(1)=ADDR(IOPDSN);/* DATA SET NAME                */ 00146000
         LA    @10,IOPDSN(,IOPPTR)                                 0255 00147000
         ST    @10,MGPVARPT                                        0255 00148000
*           CALL HMASMMSG(HMASMMGP);/*                               */ 00149000
         L     @15,@CV03092                                        0256 00150000
         LA    @01,@AL00256                                        0256 00151000
         BALR  @14,@15                                             0256 00152000
*           ALCRTNCD=STOPFUNC;      /* STOP FUNCTION                 */ 00153000
         LA    ALCRTNCD,12                                         0257 00154000
*         END;                      /*                               */ 00155000
*     END;                                                         0259 00156000
@RC00223 DS    0H                                                  0260 00157000
         EJECT                                                          00158000
*                                                                  0260 00159000
*   /*****************************************************************/ 00160000
*   /*                                                               */ 00161000
*   /* IF NO PREVIOUS ERRORS THEN DETERMINE FUNCTION TO PERFORMED AND*/ 00162000
*   /* CHECK IF VALID SEQUENCE IS BEING FOLLOWED. I.E. ALLOCATE -    */ 00163000
*   /* FREE - DELETE.                                                */ 00164000
*   /*                                                               */ 00165000
*   /*****************************************************************/ 00166000
*                                                                  0260 00167000
*   IF ALCRTNCD=ZERO                /* IF NO PREVIOUS ERRORS         */ 00168000
*     THEN                          /*                               */ 00169000
@RF00220 CH    ALCRTNCD,@CH00253                                   0260 00170000
         BNE   @RF00260                                            0260 00171000
*     DO;                           /*                               */ 00172000
*       IF IOPFUNCT=IOPALC          /* IF ALLOCATE REQUEST THEN      */ 00173000
*         THEN                      /*                               */ 00174000
         CLI   IOPFUNCT(IOPPTR),13                                 0262 00175000
         BNE   @RF00262                                            0262 00176000
*         DO;                       /* CHECK IF VALID REQUEST        */ 00177000
*           IF LRFINUSE=OFF         /* IF DSN NOT ALREADY ALLOCATED  */ 00178000
*             THEN                  /*                               */ 00179000
         TM    LRFINUSE,B'00100000'                                0264 00180000
         BNZ   @RF00264                                            0264 00181000
*             DO;                   /*                               */ 00182000
*               CALL ALCALC;        /* GO ALLOCATE DSN               */ 00183000
         BAL   @14,ALCALC                                          0266 00184000
*               ALCRTNCD=RTNCODE;   /* PASS HIGHEST RETURN CODE      */ 00185000
         LR    ALCRTNCD,RTNCODE                                    0267 00186000
*             END;                  /*                               */ 00187000
*           ELSE                    /* INTERNAL ERR IF LRF ALREADY   */ 00188000
*             ALCRTNCD=STOPSMP;     /* ALLOCATED                     */ 00189000
         B     @RC00264                                            0269 00190000
@RF00264 LA    ALCRTNCD,16                                         0269 00191000
*         END;                                                     0270 00192000
*       ELSE                        /* IF NOT ALLOCATE THEN          */ 00193000
*         IF IOPFUNCT=IOPDALC       /* IF DEALLOCATE REQUEST THEN    */ 00194000
*           THEN                    /*                               */ 00195000
         B     @RC00262                                            0271 00196000
@RF00262 CLI   IOPFUNCT(IOPPTR),14                                 0271 00197000
         BNE   @RF00271                                            0271 00198000
*           DO;                     /* CHECK IF VALID REQUEST        */ 00199000
*             IF LRFINUSE=ON        /* IF DSN ALLOCATED FOR THE      */ 00200000
*                 &IOPDSN=PREVDSN   /* DATASET SUPPLIED              */ 00201000
*               THEN                /*                               */ 00202000
         TM    LRFINUSE,B'00100000'                                0273 00203000
         BNO   @RF00273                                            0273 00204000
         CLC   IOPDSN(44,IOPPTR),PREVDSN                           0273 00205000
         BNE   @RF00273                                            0273 00206000
*               DO;                 /*                               */ 00207000
*                 LRFINUSE=OFF;     /* RESET THE IN USE SWITCH       */ 00208000
         NI    LRFINUSE,B'11011111'                                0275 00209000
*                 JFCRLSE='00'B;    /* INDICATE RELEASE STORAGE      */ 00210000
         NI    JFCRLSE(JFCBPTR),B'00111111'                        0276 00211000
*               END;                /*                               */ 00212000
*             ELSE                  /* INTERNAL ERR FOR DALC IF      */ 00213000
*               ALCRTNCD=STOPSMP;   /* LRF NOT ALREADY ALLOCATED     */ 00214000
         B     @RC00273                                            0278 00215000
@RF00273 LA    ALCRTNCD,16                                         0278 00216000
*           END;                                                   0279 00217000
*         ELSE                      /* IF NOT ALLOC OR DEL           */ 00218000
*           IF IOPFUNCT=IOPDEL      /* IF DELETE REQUEST AND         */ 00219000
*               &LRFINUSE=OFF       /* LRF IN NOT IN USE THEN        */ 00220000
*             THEN                  /*                               */ 00221000
         B     @RC00271                                            0280 00222000
@RF00271 CLI   IOPFUNCT(IOPPTR),15                                 0280 00223000
         BNE   @RF00280                                            0280 00224000
         TM    LRFINUSE,B'00100000'                                0280 00225000
         BNZ   @RF00280                                            0280 00226000
*             DO;                   /* GO PERFORM DELETE             */ 00227000
*               CALL ALCDEL;        /* CALL THE DELETE ROUTINE       */ 00228000
         BAL   @14,ALCDEL                                          0282 00229000
*               ALCRTNCD=RTNCODE;   /* PASS ALONG RETURN CODE        */ 00230000
         LR    ALCRTNCD,RTNCODE                                    0283 00231000
*             END;                                                 0284 00232000
*           ELSE                    /* IF IN USE OR INVALID REQUEST  */ 00233000
*             ALCRTNCD=STOPSMP;     /* MUST BE INTERNAL ERROR        */ 00234000
         B     @RC00280                                            0285 00235000
@RF00280 LA    ALCRTNCD,16                                         0285 00236000
*     END;                          /* END NO ERR IN PREV FUNCTIONS  */ 00237000
@RC00280 DS    0H                                                  0286 00238000
@RC00271 DS    0H                                                  0286 00239000
@RC00262 DS    0H                                                  0287 00240000
*   RETURN CODE(ALCRTNCD);          /*                               */ 00241000
@RF00260 LR    @15,ALCRTNCD                                        0287 00242000
         L     @13,4(,@13)                                         0287 00243000
         L     @14,12(,@13)                                        0287 00244000
         LM    @00,@12,20(@13)                                     0287 00245000
         BR    @14                                                 0287 00246000
         EJECT                                                          00247000
*                                                                  0288 00248000
*   /*****************************************************************/ 00249000
*   /*                                                               */ 00250000
*   /* ROUTINE CALLED TO ALLOCATE THE SMPTLIB DD CARD TO EITHER AN   */ 00251000
*   /* EXISTING DATASET OR ALLOCATE A NEW DATASET THEN ALLOCATE      */ 00252000
*   /* SMPTLIB DD CARD TO IT. DD CARD ALLOCATION DONE BY MODIFYING   */ 00253000
*   /* JFCB                                                          */ 00254000
*   /*                                                               */ 00255000
*   /*****************************************************************/ 00256000
*                                                                  0288 00257000
*ALCALC:                                                           0288 00258000
*   PROC;                                                          0288 00259000
ALCALC   STM   @14,@12,@SA00002                                    0288 00260000
*   DECLARE                         /*                               */ 00261000
*     ALCRC FIXED(8);               /*                               */ 00262000
*   RFY                             /*                               */ 00263000
*     HMASMIOP BASED(IOPPTR);       /*                               */ 00264000
*   ALCRC=ZERO;                     /*                               */ 00265000
*                                                                  0291 00266000
         SLR   ALCRC,ALCRC                                         0291 00267000
*   /*****************************************************************/ 00268000
*   /*                                                               */ 00269000
*   /* CHECK IF DATA SET ALREADY EXISTS                              */ 00270000
*   /*                                                               */ 00271000
*   /*****************************************************************/ 00272000
*                                                                  0292 00273000
*   THISDSN=IOPDSN;                 /* INITIALIZE DSN AREA           */ 00274000
         MVC   THISDSN(44),IOPDSN(IOPPTR)                          0292 00275000
*   DO I=1 TO LLCOUNT WHILE(        /* CHECK ALL VOLUMES FOR DSN     */ 00276000
*         ALCRC<STOPPTF             /* OR TILL ERROR                 */ 00277000
*         &LRFINUSE=OFF);           /* OR DATASET FOUND              */ 00278000
         LA    I,1                                                 0293 00279000
         B     @DE00293                                            0293 00280000
@DL00293 CH    ALCRC,@CH00112                                      0293 00281000
         BNL   @DC00293                                            0293 00282000
         TM    LRFINUSE,B'00100000'                                0293 00283000
         BNZ   @DC00293                                            0293 00284000
*     THISVOL=LLVOL(I);             /* SET UP VOLUME ID              */ 00285000
         LR    @10,I                                               0294 00286000
         MH    @10,@CH00177                                        0294 00287000
         LA    @06,LLVOL-12(@10)                                   0294 00288000
         MVC   THISVOL(6),0(@06)                                   0294 00289000
*     GEN(OBTAIN  OBTAINL)                                         0295 00290000
*     REFS(OBTAINL);                /* CHECK TO SEE IF IT IS ON THIS    00291000
*                                      VOLUME                        */ 00292000
         OBTAIN  OBTAINL                                                00293000
*     IF RTNCODE=0                  /* IF FOUND THEN                 */ 00294000
*       THEN                        /*                               */ 00295000
         LTR   RTNCODE,RTNCODE                                     0296 00296000
         BNZ   @RF00296                                            0296 00297000
*       DO;                         /* CHECK IF IT IS A PDS          */ 00298000
*         IF DS1DSORG(1)=POCODE     /* IF DATASET IS PDS             */ 00299000
*           THEN                    /*                               */ 00300000
         L     @10,F1PTR                                           0298 00301000
         CLI   DS1DSORG(@10),X'02'                                 0298 00302000
         BNE   @RF00298                                            0298 00303000
*           DO;                     /* THEN                          */ 00304000
*             MVSTRT=((I-1)*6)+1;   /* START OFFSET INTO ORGVOLS     */ 00305000
         LA    @10,1                                               0300 00306000
         LA    @06,6                                               0300 00307000
         LR    @01,I                                               0300 00308000
         BCTR  @01,0                                               0300 00309000
         MR    @00,@06                                             0300 00310000
         ALR   @01,@10                                             0300 00311000
         LR    MVSTRT,@01                                          0300 00312000
*             MVLEN=((LLCOUNT-I)+1)*6;/* LENGTH TO MOVE              */ 00313000
         LH    @01,LLCOUNT                                         0301 00314000
         SLR   @01,I                                               0301 00315000
         ALR   @01,@10                                             0301 00316000
         MR    @00,@06                                             0301 00317000
         LR    MVLEN,@01                                           0301 00318000
*             JFCBVOLS=ORGVOLS(MVSTRT:MVSTRT+MVLEN);/* MOVE PRIM VOL    00319000
*                                      PLUS ANY REMAINING VOLUMES IN    00320000
*                                      LIST TO FIRST POSITION IN JFCB*/ 00321000
         MVI   JFCBVOLS+1(JFCBPTR),C' '                            0302 00322000
         MVC   JFCBVOLS+2(28,JFCBPTR),JFCBVOLS+1(JFCBPTR)          0302 00323000
         LA    @10,ORGVOLS-1(MVSTRT)                               0302 00324000
         EX    MVLEN,@SM03197                                      0302 00325000
*             JFCBDSNM=IOPDSN;      /* PUT DATASET NAME IN JFCB      */ 00326000
         MVC   JFCBDSNM(44,JFCBPTR),IOPDSN(IOPPTR)                 0303 00327000
*             JFCRLSE='11'B;        /* RLSE SPACE ALFET OUTPUT OPEN  */ 00328000
         OI    JFCRLSE(JFCBPTR),B'11000000'                        0304 00329000
*             PREVDSN=IOPDSN;       /* SAVE DATASET NAME             */ 00330000
         MVC   PREVDSN(44),IOPDSN(IOPPTR)                          0305 00331000
*             LRFINUSE=ON;          /* POST AS IN USE                */ 00332000
*                                                                  0306 00333000
         OI    LRFINUSE,B'00100000'                                0306 00334000
*             /*******************************************************/ 00335000
*             /*                                                     */ 00336000
*             /* IF ATTEMPT TO ALLOCATE NEW DATASET BUT OLD ONE FOUND*/ 00337000
*             /* THEN TELL USER                                      */ 00338000
*             /*                                                     */ 00339000
*             /*******************************************************/ 00340000
*                                                                  0307 00341000
*             IF IOPSPACE^=NOSPACE  /* IF ATTEMPT TO ALLOCATE NEW    */ 00342000
*               THEN                /* DATASET BUT ACCEPTABLE OLD    */ 00343000
         CLC   IOPSPACE(6,IOPPTR),NOSPACE                          0307 00344000
         BE    @RF00307                                            0307 00345000
*               DO;                 /* ONE FOUND                     */ 00346000
*                 MGPCLEAR=''B;     /* CLEAR MSG PARM                */ 00347000
         XC    MGPCLEAR(6),MGPCLEAR                                0309 00348000
*                 MGPMGNO1=HMA352I; /* ALLOCATE SUCCESSFUL FOR X44X  */ 00349000
         MVC   MGPMGNO1(2),@CH03097                                0310 00350000
*                 MGPMGNO2=PART112; /* ON VOLUME X6X                 */ 00351000
         MVC   MGPMGNO2(2),@CH03105                                0311 00352000
*                 MGPMGNO3=PART113; /* EXISTING DATASET FOUND        */ 00353000
         MVC   MGPMGNO3(2),@CH03107                                0312 00354000
*                 MGPTYPE=MGPINFO;  /*                               */ 00355000
         MVI   MGPTYPE,X'01'                                       0313 00356000
*                 MGPVARPT(1)=ADDR(THISDSN);/* DATASET NAME          */ 00357000
         LA    @10,THISDSN                                         0314 00358000
         ST    @10,MGPVARPT                                        0314 00359000
*                 MGPVARPT(2)=ADDR(THISVOL);/* VOLUME                */ 00360000
         LA    @10,THISVOL                                         0315 00361000
         ST    @10,MGPVARPT+4                                      0315 00362000
*                 CALL HMASMMSG(HMASMMGP);/* WRITE MSG               */ 00363000
         L     @15,@CV03092                                        0316 00364000
         LA    @01,@AL00316                                        0316 00365000
         BALR  @14,@15                                             0316 00366000
*               END;                /*                               */ 00367000
*           END;                    /* END DATASET IS PDS            */ 00368000
*         ELSE                      /* IF DATASET IS PDS             */ 00369000
*           DO;                     /* ISSUE MESSAGE AND SET CODE    */ 00370000
*                                                                  0319 00371000
         B     @RC00298                                            0319 00372000
@RF00298 DS    0H                                                  0320 00373000
*             /*******************************************************/ 00374000
*             /*                                                     */ 00375000
*             /* ISSUE MESSAGE HMA353 - DATASET NOT A PDS            */ 00376000
*             /*                                                     */ 00377000
*             /*******************************************************/ 00378000
*                                                                  0320 00379000
*             MGPCLEAR=''B;         /* ZERO MSG PARM                 */ 00380000
         XC    MGPCLEAR(6),MGPCLEAR                                0320 00381000
*             MGPMGNO1=HMA353I;     /* ALLOCATE FAILED FOR X44X      */ 00382000
         MVC   MGPMGNO1(2),@CH03099                                0321 00383000
*             MGPMGNO2=PART112;     /* ON VOLUME X6X                 */ 00384000
         MVC   MGPMGNO2(2),@CH03105                                0322 00385000
*             MGPMGNO3=PART115;     /* - DATASET FOUND IS NOT A PDS  */ 00386000
         MVC   MGPMGNO3(2),@CH03111                                0323 00387000
*             MGPTYPE=MGPERR;       /*                               */ 00388000
         MVI   MGPTYPE,X'03'                                       0324 00389000
*             MGPVARPT(1)=ADDR(THISDSN);/* DATASET NAME              */ 00390000
         LA    @10,THISDSN                                         0325 00391000
