         TITLE 'HMASMAR1 - SYSMOD STATUS REPORT GENERATOR'              00001000
HMASMAR1 CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMAR1  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @09,0                                               0001 00010000
@PSTART  LA    @10,4095(,@09)                                      0001 00011000
         USING @PSTART,@09                                         0001 00012000
         USING @PSTART+4095,@10                                    0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
         MVC   @PC00001(4),0(@01)                                  0001 00018000
*                                                                  0216 00019000
*   /*****************************************************************/ 00020000
*   /*                                                               */ 00021000
*   /* THIS SEGMENT OF CODE WILL OUTPUT THE SYSMOD STATUS REPORT. THE*/ 00022000
*   /* REPORT IS CREATED IN TWO PHASES. THE FIRST PHASE WILL EXTRACT */ 00023000
*   /* THE DATA FROM THE PTF SECTION OF THE ICT AND SAVE THE         */ 00024000
*   /* EXTRACTED DATA IN THE GTA FILE. THE DATA MUST BE SAVED IN     */ 00025000
*   /* ORDER TO OUTPUT THE SYSMOD INFORMATION IN ALPHANUMERICAL      */ 00026000
*   /* SEQUENCE. THE SECOND PHASE WILL SEQUENTIALLY RETRIEVE GTA FILE*/ 00027000
*   /* RECORDS. EACH RECORD WILL BE FORMATTED AND PRINTED            */ 00028000
*   /*                                                               */ 00029000
*   /*****************************************************************/ 00030000
*                                                                  0216 00031000
*                                                                  0216 00032000
*   /*****************************************************************/ 00033000
*   /*                                                               */ 00034000
*   /* START OF HMASMAR1 CODE                                        */ 00035000
*   /*                                                               */ 00036000
*   /*****************************************************************/ 00037000
*                                                                  0216 00038000
*   AR1RTNCD=ZERO;                  /* INITIALIZE RETURN CODE        */ 00039000
         SLR   AR1RTNCD,AR1RTNCD                                   0216 00040000
*   CALL AR1INIT;                   /* INIT PARM LIST FOR HMASMGTA   */ 00041000
         BAL   @14,AR1INIT                                         0217 00042000
*   CALL AR1TBLD;                   /* PHASE ONE-EXTRACT AND SEQUENCE   00043000
*                                      DATA                          */ 00044000
         BAL   @14,AR1TBLD                                         0218 00045000
*   RFY                                                            0219 00046000
*     RTNCODE RSTD;                 /*                               */ 00047000
*   AR1RTNCD=RTNCODE;               /* SAVE RETURN CODE              */ 00048000
         LR    AR1RTNCD,RTNCODE                                    0220 00049000
*   RFY                                                            0221 00050000
*     RTNCODE UNRSTD;               /*                               */ 00051000
*   IF AR1RTNCD=ZERO                /* IF PHASE ONE SUCCESSFUL       */ 00052000
*     THEN                                                         0222 00053000
         LTR   AR1RTNCD,AR1RTNCD                                   0222 00054000
         BNZ   @RF00222                                            0222 00055000
*     DO;                           /* THEN DO                       */ 00056000
*       CALL AR1RPT;                /* PHASE TWO-RETRIEVE DATA,FORMAT   00057000
*                                      AND PRINT                     */ 00058000
         BAL   @14,AR1RPT                                          0224 00059000
*       RFY                                                        0225 00060000
*         RTNCODE RSTD;             /*                               */ 00061000
*       AR1RTNCD=RTNCODE;           /* SAVE RETURN CODE              */ 00062000
         LR    AR1RTNCD,RTNCODE                                    0226 00063000
*       RFY                                                        0227 00064000
*         RTNCODE UNRSTD;           /*                               */ 00065000
*     END;                          /*                               */ 00066000
*   CALL AR1CLEAN;                  /* FREE GOTTEN STORAGE AND RESET    00067000
*                                      HEADERS                       */ 00068000
@RF00222 BAL   @14,AR1CLEAN                                        0229 00069000
*   RFY                                                            0230 00070000
*     RTNCODE RSTD;                 /*                               */ 00071000
*   AR1RTNCD=MAX(AR1RTNCD,RTNCODE); /* SAVE MAXIMUM RETURN CODE      */ 00072000
         CR    AR1RTNCD,RTNCODE                                    0231 00073000
         BNL   *+6                                                      00074000
         LR    AR1RTNCD,RTNCODE                                    0231 00075000
*   RFY                                                            0232 00076000
*     RTNCODE UNRSTD;               /*                               */ 00077000
*   RETURN CODE(AR1RTNCD);          /* RETURN TO CALLER              */ 00078000
         LR    @15,AR1RTNCD                                        0233 00079000
         L     @13,4(,@13)                                         0233 00080000
         L     @14,12(,@13)                                        0233 00081000
         LM    @00,@12,20(@13)                                     0233 00082000
         BR    @14                                                 0233 00083000
         EJECT                                                          00084000
*AR1INIT:                                                          0234 00085000
*   PROC;                           /*                               */ 00086000
*                                                                  0234 00087000
AR1INIT  STM   @14,@12,12(@13)                                     0234 00088000
*   /*****************************************************************/ 00089000
*   /*                                                               */ 00090000
*   /* THIS SEGMENT OF CODE INITIALIZES THE PARAMETER LIST PASSED TO */ 00091000
*   /* HMASMGTA TO STORE AND SEQUENCE EXTRACTED SYSMOD DATA          */ 00092000
*   /*                                                               */ 00093000
*   /*****************************************************************/ 00094000
*                                                                  0235 00095000
*   HMASMGTP=''B;                   /* ZERO OUT THE GTP      @Z67SP00*/ 00096000
         XC    HMASMGTP(28),HMASMGTP                               0235 00097000
*   GTPKEYLN=LENGTH(GTAKEYLN);      /* GTA KEY LENGTH (15)   @Z67SP00*/ 00098000
         MVC   GTPKEYLN(2),@CH00117                                0236 00099000
*   GTPRCDLN=LENGTH(PTFBUFF);       /* SET GTA FILE RECORD LENGTH    */ 00100000
         MVC   GTPRCDLN(2),@CH00492                                0237 00101000
*   GTPBUFAD=ADDR(PTFBUFF);         /* INIT PTR TO GTA FILE RECORD 0238 00102000
*                                      BUFFER                        */ 00103000
         LA    @12,PTFBUFF                                         0238 00104000
         ST    @12,GTPBUFAD                                        0238 00105000
*   GTPWKAD=ZERO;                   /* SIGNAL NEW TABLE TO BE BUILT  */ 00106000
         SLR   @12,@12                                             0239 00107000
         ST    @12,GTPWKAD                                         0239 00108000
*   GTPPSIZE=GTP512B;               /* SET PAGE SIZE TO 512  @Z67SP00*/ 00109000
         NI    GTPPSIZE,B'11111001'                                0240 00110000
         OI    GTPPSIZE,B'00000001'                                0240 00111000
*   GTPFUNCT=GTPADD;                /* INIT FUNCTION TO ADD RECORDS  */ 00112000
         MVI   GTPFUNCT,X'04'                                      0241 00113000
*   RETURN;                         /* RETURN TO CALLER              */ 00114000
@EL00002 DS    0H                                                  0242 00115000
@EF00002 DS    0H                                                  0242 00116000
@ER00002 LM    @14,@12,12(@13)                                     0242 00117000
         BR    @14                                                 0242 00118000
*   END AR1INIT;                    /*                               */ 00119000
         EJECT                                                          00120000
*AR1TBLD:                                                          0244 00121000
*   PROC;                           /*                               */ 00122000
*                                                                  0244 00123000
AR1TBLD  STM   @14,@12,@SA00003                                    0244 00124000
*   /*****************************************************************/ 00125000
*   /*                                                               */ 00126000
*   /* THIS SEGMENT OF CODE WILL EXTRACT SYSMOD DATA TO BE REPORTED. */ 00127000
*   /* FOR EACH SYSMOD IN THE PTF SECTION OF THE ICT, A MAJOR SYSMOD */ 00128000
*   /* RECORD WILL BE CONSTUCTED AND STORED IN THE GTA FILE. IN      */ 00129000
*   /* ADDITION, A GTA FILE RECORD WILL BE CONSTRUCTED AND STORED FOR*/ 00130000
*   /* SYSMODS IN THE PTF IFREQ, REQ AND PRE LISTS FOR SYSMODS THAT  */ 00131000
*   /* ARE NOT DELETED IN THE PTF SECTION OF THE ICT. STORING THE    */ 00132000
*   /* RECORDS VIA HMASMGTA( GIVEN THE DESIGN OF THE SYSMOD RECORD   */ 00133000
*   /* KEY ) WILL SEQUENCE THE SYSMODS IN THE ICT IN ALPHANUMERICAL  */ 00134000
*   /* ORDER.                                                        */ 00135000
*   /*                                                               */ 00136000
*   /*****************************************************************/ 00137000
*                                                                  0245 00138000
*                                                                  0245 00139000
*   /*****************************************************************/ 00140000
*   /*                                                               */ 00141000
*   /* LOCAL DECLARES                                                */ 00142000
*   /*                                                               */ 00143000
*   /*****************************************************************/ 00144000
*                                                                  0245 00145000
*   DCL                                                            0245 00146000
*     TBLDRC FIXED(31);             /* INTERNAL RETURN CODE          */ 00147000
*   RFY                                                            0246 00148000
*     ICTPTF BASED(CCAICPTF);       /* ADDRESS PTF SECTION OF ICT    */ 00149000
*                                                                  0246 00150000
*   /*****************************************************************/ 00151000
*   /*                                                               */ 00152000
*   /* START OF AR1TBLD CODE                                         */ 00153000
*   /*                                                               */ 00154000
*   /*****************************************************************/ 00155000
*                                                                  0247 00156000
*   TBLDRC=ZERO;                    /* INITIALIZE RETURN CODE        */ 00157000
         SLR   TBLDRC,TBLDRC                                       0247 00158000
*   DO PTF=ONE BY ONE               /* LOOP THRU PTF SECTION         */ 00159000
*         WHILE(ICTPEND(PTF)^=ICTENDNT/* UNTIL END OF SYSMODS        */ 00160000
*         &TBLDRC=ZERO);            /* OR NO MORE SPACE FOR GTA FILE */ 00161000
         LA    PTF,1                                               0248 00162000
         B     @DE00248                                            0248 00163000
@DL00248 DS    0H                                                  0249 00164000
*     PTFBUFF=' ';                  /* INIT SYSMOD RECORD BUFFER     */ 00165000
         MVI   PTFBUFF+1,C' '                                      0249 00166000
         MVC   PTFBUFF+2(15),PTFBUFF+1                             0249 00167000
         MVI   PTFBUFF,C' '                                        0249 00168000
*     IF ICTPGELM(PTF)=OFF          /* NOT ELIMINATED, OR    @Z67SP00*/ 00169000
*         |(ICTPGELM(PTF)=ON        /* ELIMINATED AND IN THE @Z67SP00*/ 00170000
*         &ICTPSLST(PTF)=ON) THEN   /* SELECT LIST THEN--    @Z67SP00*/ 00171000
