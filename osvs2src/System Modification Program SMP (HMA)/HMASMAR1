         MH    @12,@CH01955                                        0250 00173000
         L     @08,CCAICPTF(,CCAPTR)                               0250 00174000
         ST    @12,@TF00001                                        0250 00175000
         ALR   @12,@08                                             0250 00176000
         AL    @12,@CF01966                                        0250 00177000
         TM    ICTPGELM-13(@12),B'00001000'                        0250 00178000
         BZ    @RT00250                                            0250 00179000
         LR    @12,@08                                             0250 00180000
         AL    @12,@TF00001                                        0250 00181000
         AL    @12,@CF01966                                        0250 00182000
         TM    ICTPGELM-13(@12),B'00001000'                        0250 00183000
         BNO   @RF00250                                            0250 00184000
         AL    @08,@TF00001                                        0250 00185000
         AL    @08,@CF01967                                        0250 00186000
         TM    ICTPSLST-8(@08),B'00000010'                         0250 00187000
         BNO   @RF00250                                            0250 00188000
@RT00250 DS    0H                                                  0251 00189000
*       DO;                         /* SAVE FOR REPORT       @Z67SP00*/ 00190000
*         PTFNUM=ICTPTFS(PTF);      /* INSERT SYSMOD NUM IN BUFFER   */ 00191000
         LR    @12,PTF                                             0252 00192000
         MH    @12,@CH01955                                        0252 00193000
         L     @08,CCAICPTF(,CCAPTR)                               0252 00194000
         ST    @12,@TF00001                                        0252 00195000
         ALR   @12,@08                                             0252 00196000
         AL    @12,@CF01968                                        0252 00197000
         MVC   PTFNUM(7),ICTPTFS(@12)                              0252 00198000
*         PTFDATA=ICTPFMID(PTF);    /* INSERT FMID IN BUFFER @Z67SP00*/ 00199000
*                                                                  0253 00200000
         LR    @12,@08                                             0253 00201000
         AL    @12,@TF00001                                        0253 00202000
         AL    @12,@CF01969                                        0253 00203000
         MVC   PTFDATA(7),ICTPFMID-22(@12)                         0253 00204000
*         /***********************************************************/ 00205000
*         /*                                                         */ 00206000
*         /* DETERMINE TYPE OF SYSMOD BEING SAVED IN GTA FILE        */ 00207000
*         /*                                                 @Z67SP00*/ 00208000
*         /*                                                         */ 00209000
*         /***********************************************************/ 00210000
*                                                                  0254 00211000
*         IF ICTPTYPE(PTF)=IOPPFUNC THEN/* SYSMOD A FUNCTION?      0254 00212000
*                                                            @Z67SP00*/ 00213000
