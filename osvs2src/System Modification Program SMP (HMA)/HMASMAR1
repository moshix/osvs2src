*                                      PRINTABLE FORMAT              */ 01086000
*       3 ACTTYPE CHAR(1) INIT(     /* REQUISITE SYSMOD TYPE         */ 01087000
*             '01'X,                /* IFREQ SYSMOD NUMBER           */ 01088000
*             '02'X,                /* REQ SYSMOD NUMBER             */ 01089000
*             '03'X),               /* PRE SYSMOD NUMBER             */ 01090000
*       3 FMTTYPE CHAR(5) INIT(     /* PRINTABLE REQUISITE SYSMOD TYP*/ 01091000
*             'IFREQ',              /* IFREQ SYSMOD                  */ 01092000
*             'REQ  ',              /* REQ SYSMOD                    */ 01093000
*             'PRE  ');             /* PRE SYSMOD                    */ 01094000
