*     INDXT FIXED(15);              /* TYPETAB LOOP VARIABLE         */ 01096000
*                                                                  0428 01097000
*   /*****************************************************************/ 01098000
*   /*                                                               */ 01099000
*   /* START OF AR1RPTF CODE                                         */ 01100000
*   /*                                                               */ 01101000
*   /*****************************************************************/ 01102000
*                                                                  0429 01103000
*   RPTFRC=ZERO;                    /* INITIALIZE RETURN CODE        */ 01104000
*                                                                  0429 01105000
         SLR   RPTFRC,RPTFRC                                       0429 01106000
*   /*****************************************************************/ 01107000
*   /*                                                               */ 01108000
*   /* IF REQUISITE SYSMOD TYPE IS DIFFERENT THAN TYPE OF THE LAST   */ 01109000
*   /* SYSMOD THAT WAS INSERTED IN THE REPORT OUTPUT BUFFER, THEN    */ 01110000
*   /* PRINTABLE FORM OF CURRENT REQUISITE SYSMOD TYPE MUST BE       */ 01111000
*   /* INSERTED IN OUTPUT BUFFER.                                    */ 01112000
*   /*                                                               */ 01113000
*   /*****************************************************************/ 01114000
*                                                                  0430 01115000
*   IF PTFIND^=CURIND               /* IF DIFFERENT TYPE THAN LAST 0430 01116000
