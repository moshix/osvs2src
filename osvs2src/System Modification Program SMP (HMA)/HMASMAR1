*         END;                      /*                               */ 01176000
*     END;                          /* END OF NEW REQUISITE SYSMOD   */ 01177000
*                                                                  0448 01178000
@RF00439 DS    0H                                                  0449 01179000
*   /*****************************************************************/ 01180000
*   /*                                                               */ 01181000
*   /* THE FOLLOWING CODE WILL INSERT THE REQUISITE SYSMOD NUMBER A  */ 01182000
*   /* STATUS INDICATOR FOR THE REQUISITE SYSMOD (EITHER - BLANK =   */ 01183000
*   /* GO, '-' = NOGO, '*' = BYPASS NOGO) IN THE REPORT OUTPUT BUFFER*/ 01184000
*   /*                                                               */ 01185000
*   /*****************************************************************/ 01186000
*                                                                  0449 01187000
*   IF RPTFRC=ZERO                  /* IF NO PRIOR IO ERRORS         */ 01188000
*     THEN                                                         0449 01189000
@RF00430 LTR   RPTFRC,RPTFRC                                       0449 01190000
         BNZ   @RF00449                                            0449 01191000
*     DO;                           /* THEN CONTINUE WITH REPORT     */ 01192000
*       RELPINDX=RELPINDX+ONE;      /* INCREMENT COUNT OF INSERTED 0451 01193000
*                                      SYSMODS                       */ 01194000
         LA    @12,1                                               0451 01195000
         AH    @12,RELPINDX                                        0451 01196000
         STH   @12,RELPINDX                                        0451 01197000
*       IF RELPINDX>DIM(OUTRELPS)   /* IF NO MORE ROOM IN BUFFER     */ 01198000
*         THEN                                                     0452 01199000
         CH    @12,@CH00248                                        0452 01200000
         BNH   @RF00452                                            0452 01201000
*         DO;                       /* THEN PRINT CURRENT CONTENTS   */ 01202000
*           CALL AR1PREC;           /* VIA CALL TO AR1PREC ROUTINE   */ 01203000
         BAL   @14,AR1PREC                                         0454 01204000
*           RFY                                                    0455 01205000
*             RTNCODE RSTD;         /*                               */ 01206000
*           RPTFRC=RTNCODE;         /* SAVE RETURN CODE              */ 01207000
         LR    RPTFRC,RTNCODE                                      0456 01208000
*           RFY                                                    0457 01209000
*             RTNCODE UNRSTD;       /*                               */ 01210000
*           RELPINDX=ONE;           /* SET INSRTED SYSMOD COUNT TO 1 */ 01211000
         MVC   RELPINDX(2),@CH00039                                0458 01212000
*         END;                      /* BUFFER CONTENTS PRINTED       */ 01213000
*       IF RPTFRC=ZERO              /* IF OK TO CONTINUE             */ 01214000
*         THEN                                                     0460 01215000
@RF00452 LTR   RPTFRC,RPTFRC                                       0460 01216000
