         AL    @12,@TF00001                                        0251 00271000
         AL    @12,@CF02042                                        0251 00272000
         TM    ICTMISEL-11(@12),B'10000000'                        0251 00273000
         BNO   @RF00251                                            0251 00274000
*                 DO;               /* DO FOR SELECTED       @Z67SP00*/ 00275000
*                   CUREFMID=ICTMFMID(ELEM);/* PUT FMID IN GTA FILE     00276000
*                                                            @Z67SP00*/ 00277000
         LR    @12,@08                                             0253 00278000
         AL    @12,@TF00001                                        0253 00279000
         AL    @12,@CF02044                                        0253 00280000
         MVC   CUREFMID(7),ICTMFMID-88(@12)                        0253 00281000
*                   CURERMID=ICTMRMID(ELEM);/* PUT RMID IN GTA FILE     00282000
*                                                            @Z67SP00*/ 00283000
         AL    @08,@TF00001                                        0254 00284000
         AL    @08,@CF02045                                        0254 00285000
         MVC   CURERMID(7),ICTMRMID-80(@08)                        0254 00286000
*                   CURESTAT=MODIDOK;/* YES- SET BYPASS STAT @Z67SP00*/ 00287000
         MVC   CURESTAT(8),@CC01893                                0255 00288000
*                 END;              /* END OF SELECTED       @Z67SP00*/ 00289000
*               ELSE                                               0257 00290000
*                 CURESTAT=MODIDERR;/* NO- SET MODID ER STAT @Z67SP00*/ 00291000
         B     @RC00251                                            0257 00292000
@RF00251 MVC   CURESTAT(8),@CC01891                                0257 00293000
*             END;                  /* END OF MODID ERROR    @Z67SP00*/ 00294000
*           ELSE                                                   0259 00295000
*             IF ICTMSELM(ELEM)=ON THEN/* IS ELEMENT ELIMINATED, AND    00296000
