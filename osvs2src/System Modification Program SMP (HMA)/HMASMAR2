         TITLE 'HMASMAR2 - ELEMENT SUMMARY REPORT GENERATOR'            00001000
HMASMAR2 CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMAR2  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @09,0                                               0001 00010000
@PSTART  LA    @10,4095(,@09)                                      0001 00011000
         USING @PSTART,@09                                         0001 00012000
         USING @PSTART+4095,@10                                    0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
         MVC   @PC00001(4),0(@01)                                  0001 00018000
         EJECT                                                          00019000
*                                                                  0203 00020000
*   /*****************************************************************/ 00021000
*   /*                                                               */ 00022000
*   /* THIS SEGMENT OF CODE WILL CONTROL THE EXTRACTION AND REPORTING*/ 00023000
*   /* OF THE ICT DATA                                               */ 00024000
*   /*                                                               */ 00025000
*   /*****************************************************************/ 00026000
*                                                                  0203 00027000
*   AR2RTNCD=ZERO;                  /* INITIALIZE RETURN CODE        */ 00028000
         SLR   AR2RTNCD,AR2RTNCD                                   0203 00029000
*   SAVAREA=' ';                    /* INITIALIZE SAVE AREA FOR LAST    00030000
*                                      RECORD PRINTED        @Z67SP00*/ 00031000
*                                                                  0204 00032000
         MVI   SAVAREA+1,C' '                                      0204 00033000
         MVC   SAVAREA+2(128),SAVAREA+1                            0204 00034000
         MVI   SAVAREA,C' '                                        0204 00035000
*   /*****************************************************************/ 00036000
*   /*                                                               */ 00037000
*   /* INITIALIZE PARAMETER FOR HMASMGTA TO ADD RECORDS TO REPORT    */ 00038000
*   /* TABLE ( SEE AR2TBLD PROCEDURE FOR DESCRIPTION OF TABLE ).     */ 00039000
*   /*                                                               */ 00040000
*   /*****************************************************************/ 00041000
*                                                                  0205 00042000
*   HMASMGTP=''B;                   /* ZERO PARAMETER AREA   @Z67SP00*/ 00043000
         XC    HMASMGTP(28),HMASMGTP                               0205 00044000
*   GTPKEYLN=LENGTH(CURKEY);        /* SET REPORT TABLE KEY LENGTH   */ 00045000
         MVC   GTPKEYLN(2),@CH01723                                0206 00046000
*   GTPRCDLN=LENGTH(CURBUFF);       /* REPORT TABLE RECORD LENGTH  0207 00047000
*                                                            @Z67SP00*/ 00048000
         MVC   GTPRCDLN(2),@CH01721                                0207 00049000
*   GTPWKAD=ZERO;                   /* SIGNAL NEW TABLE REQUEST      */ 00050000
         SLR   @12,@12                                             0208 00051000
         ST    @12,GTPWKAD                                         0208 00052000
*   GTPBUFAD=ADDR(CURBUFF);         /* PASS BUFFER ADDRESS           */ 00053000
         LA    @12,CURBUFF                                         0209 00054000
         ST    @12,GTPBUFAD                                        0209 00055000
*   GTPFUNCT=GTPADD;                /* SET ADD FUNCTION              */ 00056000
*                                                                  0210 00057000
         MVI   GTPFUNCT,X'04'                                      0210 00058000
*   /*****************************************************************/ 00059000
*   /*                                                               */ 00060000
*   /* EXTRACT AND SEQUENCE REPORT DATA                              */ 00061000
*   /*                                                               */ 00062000
*   /*****************************************************************/ 00063000
*                                                                  0211 00064000
*   CALL AR2TBLD;                   /* EXTRACT REPORT DATA FROM ICT     00065000
*                                      AND SAVE DATA IN REPORT TABLE */ 00066000
         BAL   @14,AR2TBLD                                         0211 00067000
*   RFY                                                            0212 00068000
*     RTNCODE RSTD;                 /*                               */ 00069000
*   AR2RTNCD=RTNCODE;               /* SAVE RETURN CODE              */ 00070000
         LR    AR2RTNCD,RTNCODE                                    0213 00071000
*   RFY                                                            0214 00072000
*     RTNCODE UNRSTD;               /*                               */ 00073000
*                                                                  0214 00074000
*   /*****************************************************************/ 00075000
*   /*                                                               */ 00076000
*   /* WRITE OUT ELEMENT SUMMARY REPORT                              */ 00077000
*   /*                                                               */ 00078000
*   /*****************************************************************/ 00079000
*                                                                  0215 00080000
*   IF AR2RTNCD=ZERO                /* IF REPORT TABLE COMPLETE      */ 00081000
*     THEN                                                         0215 00082000
         LTR   AR2RTNCD,AR2RTNCD                                   0215 00083000
         BNZ   @RF00215                                            0215 00084000
*     DO;                           /* THEN                          */ 00085000
*       CALL AR2RPT;                /* ACCESS REPORT DATA AND UNPACK    00086000
*                                      TABLE RECORDS INTO REPORT   0217 00087000
*                                      FORMAT                        */ 00088000
         BAL   @14,AR2RPT                                          0217 00089000
*       RFY                                                        0218 00090000
*         RTNCODE RSTD;             /*                               */ 00091000
*       AR2RTNCD=RTNCODE;           /* SAVE RETURN CODE              */ 00092000
         LR    AR2RTNCD,RTNCODE                                    0219 00093000
*       RFY                                                        0220 00094000
*         RTNCODE UNRSTD;           /*                               */ 00095000
*     END;                          /* END OF REPORT GENERATION      */ 00096000
*                                                                  0221 00097000
*   /*****************************************************************/ 00098000
*   /*                                                               */ 00099000
*   /* EXECUTE TERMINATION CODE                                      */ 00100000
*   /*                                                               */ 00101000
*   /*****************************************************************/ 00102000
*                                                                  0222 00103000
*   CALL AR2CLEAN;                  /* FREE ANY GOTTEN STORAGE       */ 00104000
@RF00215 BAL   @14,AR2CLEAN                                        0222 00105000
*   RFY                                                            0223 00106000
*     RTNCODE RSTD;                 /*                               */ 00107000
*   AR2RTNCD=MAX(AR2RTNCD,RTNCODE); /* SAVE MAXIMUM RETURN CODE      */ 00108000
         CR    AR2RTNCD,RTNCODE                                    0224 00109000
         BNL   *+6                                                      00110000
         LR    AR2RTNCD,RTNCODE                                    0224 00111000
*   RFY                                                            0225 00112000
*     RTNCODE UNRSTD;               /*                               */ 00113000
*   RETURN CODE(AR2RTNCD);          /* RETURN TO CALLER              */ 00114000
         LR    @15,AR2RTNCD                                        0226 00115000
         L     @13,4(,@13)                                         0226 00116000
         L     @14,12(,@13)                                        0226 00117000
         LM    @00,@12,20(@13)                                     0226 00118000
         BR    @14                                                 0226 00119000
         EJECT                                                          00120000
*AR2TBLD:                                                          0227 00121000
*   PROC;                           /* EXTRACT AND SAVE REPORT DATA  */ 00122000
*                                                                  0227 00123000
AR2TBLD  STM   @14,@12,@SA00002                                    0227 00124000
*   /*****************************************************************/ 00125000
*   /*                                                               */ 00126000
*   /* THIS SEGMENT OF CODE WILL EXTRACT REPORT DATA AS DESCRIBED IN */ 00127000
*   /* THE FUNCTION SECTION OF THE PROLOGUE. FOR EACH ELEMENT IN THE */ 00128000
*   /* MODULE SECTION OF THE ICT A RECORD(AS DEFINED IN THE CURBUFF  */ 00129000
*   /* MAPPING) WILL BE CREATED AND ADDED TO THE REPORT TABLE. NOTE  */ 00130000
*   /* THAT AN ELEMENT CONTAINED IN MORE THAN ONE LOAD MODULE WILL   */ 00131000
*   /* RESULT IN MORE THAN ONE RECORD BEING CREATED AND ADDED TO THE */ 00132000
*   /* REPORT TABLE. IN FACT, THERE WILL BE AS MANY RECORDS AS THERE */ 00133000
*   /* ARE AFFECTED LOAD MODULES                                     */ 00134000
*   /*                                                               */ 00135000
*   /*****************************************************************/ 00136000
*                                                                  0228 00137000
*   RFY                                                            0228 00138000
*     ICTMOD BASED(CCAICMOD);       /* PTR TO MOD SECTION OF ICT   0228 00139000
*                                      RETRIEVED FROM CCA            */ 00140000
*   RFY                                                            0229 00141000
*     ICTASMLS BASED(ICTMACHN(ELEM));/* RETRIEVE ELEMENT ASSEMBLY  0229 00142000
*                                      LIST POINTER                  */ 00143000
*   DCL                                                            0230 00144000
*     TBLDRC FIXED(31);             /* INTERNAL RETURN CODE          */ 00145000
*   DCL                                                            0231 00146000
*     TYPESAV BIT(8);               /* SAVE MASKED TYPE              */ 00147000
*                                                                  0231 00148000
*   /*****************************************************************/ 00149000
*   /*                                                               */ 00150000
*   /* START OF AR2TBLD CODE                                         */ 00151000
*   /*                                                               */ 00152000
*   /*****************************************************************/ 00153000
*                                                                  0232 00154000
*   TBLDRC=ZERO;                    /* INITIALIZE RETURN CODE        */ 00155000
*                                                                  0232 00156000
         SLR   TBLDRC,TBLDRC                                       0232 00157000
*   /*****************************************************************/ 00158000
*   /*                                                               */ 00159000
*   /* LOOP THROUGH ENTRIES IN MODULE SECTION OF ICT                 */ 00160000
*   /*                                                               */ 00161000
*   /*****************************************************************/ 00162000
*                                                                  0233 00163000
*   DO ELEM=ONE BY ONE WHILE(ICTMEND(ELEM)^=ICTENDNT/* LOOP WHILE NOT   00164000
*                                      AT END OF MODS                */ 00165000
*         &TBLDRC=ZERO);            /* AND SPACE REMAINS IN REPORT 0233 00166000
*                                      TABLE                         */ 00167000
         LA    ELEM,1                                              0233 00168000
         B     @DE00233                                            0233 00169000
@DL00233 DS    0H                                                  0234 00170000
*     IF ICTMASM(ELEM)=OFF          /* IF NOT AN ASSEMBLY AS RESULT     00171000
*                                      OR MAC OR SRC UPDATE          */ 00172000
*         &ICTMXUPD(ELEM)=OFF       /* NOT AN EXTRA SOURCE ENTRY THEN*/ 00173000
*       THEN                                                       0234 00174000
         LR    @12,ELEM                                            0234 00175000
         MH    @12,@CH02028                                        0234 00176000
         L     @08,CCAICMOD(,CCAPTR)                               0234 00177000
         ST    @12,@TF00001                                        0234 00178000
         ALR   @12,@08                                             0234 00179000
         AL    @12,@CF02038                                        0234 00180000
         TM    ICTMASM-9(@12),B'01000000'                          0234 00181000
         BNZ   @RF00234                                            0234 00182000
         AL    @08,@TF00001                                        0234 00183000
         AL    @08,@CF02039                                        0234 00184000
         TM    ICTMXUPD-17(@08),B'00001000'                        0234 00185000
         BNZ   @RF00234                                            0234 00186000
*       DO;                         /* CONTINUE EXTRACTING DATA      */ 00187000
*         CURBUFF='';               /* RESET REPORT TABLE RECORD   0236 00188000
*                                      BUFFER TO BLANKS              */ 00189000
         MVI   CURBUFF,C' '                                        0236 00190000
         MVC   CURBUFF+1(101),CURBUFF                              0236 00191000
*         TYPEFND=OFF;              /* INIT TYPE FOUND SWITCH        */ 00192000
         NI    TYPEFND,B'01111111'                                 0237 00193000
*         DO MTYPINDX=ONE TO DIM(MTYPE);/* FIND TYPE IN TYPE       0238 00194000
*                                      STRUCTURE                     */ 00195000
         LA    MTYPINDX,1                                          0238 00196000
@DL00238 DS    0H                                                  0239 00197000
*           TYPESAV=ICTMFLG1(ELEM)&ACTTYPE(MTYPINDX);/* SEE IF TYPE     00198000
*                                      SET                           */ 00199000
         LR    @12,ELEM                                            0239 00200000
         MH    @12,@CH02028                                        0239 00201000
         L     @08,CCAICMOD(,CCAPTR)                               0239 00202000
         LR    @05,MTYPINDX                                        0239 00203000
         SLA   @05,3                                               0239 00204000
         ALR   @08,@12                                             0239 00205000
         AL    @08,@CF02038                                        0239 00206000
         MVC   TYPESAV(1),ICTMFLG1-9(@08)                          0239 00207000
         LA    @12,ACTTYPE-8(@05)                                  0239 00208000
         NC    TYPESAV(1),0(@12)                                   0239 00209000
*           IF TYPESAV=ACTTYPE(MTYPINDX)/* IF MATCH THEN SET         */ 00210000
*             THEN                  /*                               */ 00211000
         LA    @12,ACTTYPE-8(@05)                                  0240 00212000
         CLC   TYPESAV(1),0(@12)                                   0240 00213000
         BNE   @RF00240                                            0240 00214000
*             DO;                   /*                               */ 00215000
*               CURTYPE=GENTYPE(MTYPINDX);/* EQUIVALENT SEQUENCE TYPE*/ 00216000
         LA    @12,GENTYPE-8(@05)                                  0242 00217000
         MVC   CURTYPE(1),0(@12)                                   0242 00218000
*               CURRPTYP=FMTTYPE(MTYPINDX);/* EQUIVALENT REPORT TYPE    00219000
*                                                            @Z67SP00*/ 00220000
         LA    @12,FMTTYPE-8(@05)                                  0243 00221000
         MVC   CURRPTYP(6),0(@12)                                  0243 00222000
*             END;                  /*                               */ 00223000
*         END;                      /* END OF TYPE SEARCH            */ 00224000
@RF00240 AH    MTYPINDX,@CH00039                                   0245 00225000
         CH    MTYPINDX,@CH00087                                   0245 00226000
         BNH   @DL00238                                            0245 00227000
*         CURELEM=ICTMNAME(ELEM);   /* SET SECOND PART OF RECORD KEY */ 00228000
         LR    @12,ELEM                                            0246 00229000
         MH    @12,@CH02028                                        0246 00230000
         L     @08,CCAICMOD(,CCAPTR)                               0246 00231000
         ST    @12,@TF00001                                        0246 00232000
         ALR   @12,@08                                             0246 00233000
         AL    @12,@CF02041                                        0246 00234000
         MVC   CURELEM(8),ICTMNAME(@12)                            0246 00235000
*         IF ICTMISEL(ELEM)=ON&ICTMCPL(ELEM)=OFF/* IS ELEMENT      0247 00236000
*                                      SELECTED, NOT COMPLETE AND  0247 00237000
*                                                            @Z67SP00*/ 00238000
*             &ARLCHECK=OFF THEN    /* NOT A CHECK RUN?      @Z67SP00*/ 00239000
