         CR    ARLRTNCD,RTNCODE                                    0027 00056000
         BNL   *+6                                                      00057000
         LR    ARLRTNCD,RTNCODE                                    0027 00058000
*   RFY                                                            0028 00059000
*     RTNCODE UNRSTD;               /*                               */ 00060000
*   IF ARLRTNCD<STOPFUNC            /* IF PREVIOUS FUNCTIONS OK      */ 00061000
*     THEN                          /*                               */ 00062000
         CH    ARLRTNCD,@CH00224                                   0029 00063000
         BNL   @RF00029                                            0029 00064000
*     DO;                           /* CALL NEXT REPORTER            */ 00065000
*       CALL HMASMAR2(ARLPARM);     /* CALL ELEMENT SUMMARY REPORT 0031 00066000
*                                                            @Z67SP00*/ 00067000
