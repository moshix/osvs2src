*   /*****************************************************************/ 00210000
*                                                                  0190 00211000
*   RFY                                                            0190 00212000
*     RTNCODE UNRSTD;               /*                               */ 00213000
@RF00183 DS    0H                                                  0191 00214000
*   RETURN CODE(BUERTNCD);          /*                               */ 00215000
         LR    @15,BUERTNCD                                        0191 00216000
         L     @13,4(,@13)                                         0191 00217000
         L     @14,12(,@13)                                        0191 00218000
         LM    @00,@12,20(@13)                                     0191 00219000
         BR    @14                                                 0191 00220000
         EJECT                                                          00221000
*                                                                  0192 00222000
*   /*****************************************************************/ 00223000
*   /*                                                               */ 00224000
*   /* ROUTINE CALLED TO CHECK TO SEE IF A SECOND IOP IS REQUIRED AND*/ 00225000
*   /* IF SO TO GETMAIN ONE                                          */ 00226000
*   /*                                                               */ 00227000
