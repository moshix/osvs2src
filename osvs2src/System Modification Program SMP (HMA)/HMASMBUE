         L     IOPPTR,BUPIOPAD(,@10)                               0154 00032000
*   HMASMMGP=''B;                   /* INIT MSG PARM FOR ALL MSG     */ 00033000
         XC    HMASMMGP(28),HMASMMGP                               0155 00034000
*   MGPPRINT=ON;                    /* TO SMPOUT AND SMPLOG          */ 00035000
*   MGPHLDS=ON;                     /*                               */ 00036000
