         BE    @RF00224                                            0224 00320000
*       GMRC=STOPSMP;               /* AT GETMAIN TIME               */ 00321000
*                                                                  0225 00322000
         LA    GMRC,16                                             0225 00323000
*   /*****************************************************************/ 00324000
*   /*                                                               */ 00325000
*   /* RETURN TO CALLER                                              */ 00326000
*   /*                                                               */ 00327000
*   /*****************************************************************/ 00328000
*                                                                  0226 00329000
*   RETURN CODE(GMRC);              /*                               */ 00330000
@RF00224 DS    0H                                                  0226 00331000
@RC00197 LR    @15,GMRC                                            0226 00332000
         L     @14,@SA00002                                        0226 00333000
         LM    @00,@12,@SA00002+8                                  0226 00334000
         BR    @14                                                 0226 00335000
*   END BUEGMIOP;                   /*                               */ 00336000
         EJECT                                                          00337000
*                                                                  0228 00338000
*   /*****************************************************************/ 00339000
*   /*                                                               */ 00340000
*   /* IF IOP GETMAINED OK AND THIS WAS REQUEST TO BACKUP ANOTHER    */ 00341000
*   /* SUBENTRY FOR AN EXISTING SCDS SYSMOD THEN LOCATE THE SYSMOD ON*/ 00342000
*   /* THE SCDS                                                      */ 00343000
*   /*                                                               */ 00344000
