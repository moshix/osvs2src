         BNZ   @RF00183                                            0183 00182000
*     DO;                           /*                               */ 00183000
*       CALL BUESTOW;               /*                               */ 00184000
         BAL   @14,BUESTOW                                         0185 00185000
*       BUERTNCD=MAX(BUERTNCD,RTNCODE);/*                            */ 00186000
*                                                                  0186 00187000
         CR    BUERTNCD,RTNCODE                                    0186 00188000
         BNL   *+6                                                      00189000
         LR    BUERTNCD,RTNCODE                                    0186 00190000
*       /*************************************************************/ 00191000
*       /*                                                           */ 00192000
*       /* IF ANY ERRORS IN PROCESS OR IF FUNCTION WAS STOW SCDS     */ 00193000
*       /* SYSMOD THEN FREE UP THE SCDS SYSMOD IOP                   */ 00194000
*       /*                                                           */ 00195000
*       /*************************************************************/ 00196000
*                                                                  0187 00197000
*       CALL BUEFMIOP;              /*                               */ 00198000
         BAL   @14,BUEFMIOP                                        0187 00199000
