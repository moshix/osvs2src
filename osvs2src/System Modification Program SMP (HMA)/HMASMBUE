*                                                                  0192 00229000
*BUEGMIOP:                          /*                               */ 00230000
*   PROCEDURE;                      /*                               */ 00231000
BUEGMIOP STM   @14,@12,@SA00002                                    0192 00232000
*   DECLARE                         /*                               */ 00233000
*     GMRC FIXED(8);                /*                               */ 00234000
*   RFY                                                            0194 00235000
*     HMASMIOP BASED(IOPPTR2);      /* IOP FOR SCDS SYSMOD           */ 00236000
*   GMRC=ZERO;                      /*                               */ 00237000
         SLR   GMRC,GMRC                                           0195 00238000
*   GMDONE=OFF;                     /* DID NOT DO GETMAIN            */ 00239000
*                                                                  0196 00240000
         NI    GMDONE,B'01111111'                                  0196 00241000
*   /*****************************************************************/ 00242000
*   /*                                                               */ 00243000
*   /* IF IOP NOT YET OBTAINED THEN CALL SUB TO GET ONE              */ 00244000
*   /*                                                               */ 00245000
*   /*****************************************************************/ 00246000
*                                                                  0197 00247000
*   IF IOPPTR2=ZERO                 /* IF SYSMOD IP NOT GETMAINED YET*/ 00248000
*     THEN                          /*                               */ 00249000
         ICM   @10,15,IOPPTR2                                      0197 00250000
         BNZ   @RF00197                                            0197 00251000
*     DO;                           /*                               */ 00252000
*       GMDONE=ON;                  /* DID GETMAIN                   */ 00253000
         OI    GMDONE,B'10000000'                                  0199 00254000
*       HMASMSPL=''B;               /* ZERO PARM                     */ 00255000
         XC    HMASMSPL(5),HMASMSPL                                0200 00256000
*       SPLFUNCT=SPLGMIOP;          /*                               */ 00257000
         MVI   SPLFUNCT,X'03'                                      0201 00258000
*       CALL HMASMSUB(HMASMSPL);    /*                               */ 00259000
         L     @15,@CV01285                                        0202 00260000
         LA    @01,@AL00202                                        0202 00261000
         BALR  @14,@15                                             0202 00262000
*       RFY                                                        0203 00263000
*         RTNCODE RSTD;             /*                               */ 00264000
*       GMRC=RTNCODE;               /*                               */ 00265000
         LR    GMRC,RTNCODE                                        0204 00266000
*       RFY                                                        0205 00267000
*         RTNCODE UNRSTD;           /*                               */ 00268000
*                                                                  0205 00269000
*       /*************************************************************/ 00270000
*       /*                                                           */ 00271000
*       /* IF IOP GEMAINED OK THEN INIT PTR AND INIT IOP             */ 00272000
*       /*                                                           */ 00273000
*       /*************************************************************/ 00274000
*                                                                  0206 00275000
*       IF GMRC=ZERO                /* IF IOP OBTAINED               */ 00276000
*         THEN                      /*                               */ 00277000
         CH    GMRC,@CH00270                                       0206 00278000
         BNE   @RF00206                                            0206 00279000
*         DO;                       /*                               */ 00280000
*           IOPPTR2=SPLPMAD;        /*                               */ 00281000
         L     @10,SPLPMAD                                         0208 00282000
         ST    @10,IOPPTR2                                         0208 00283000
*           HMASMIOP=''B;           /* INIT IOP                      */ 00284000
         XC    HMASMIOP(68,@10),HMASMIOP(@10)                      0209 00285000
*           IOPTYPE=IOPSMD;         /* SYSMOD TYPE IOP               */ 00286000
         MVI   IOPTYPE(@10),X'0B'                                  0210 00287000
*           IOPNAM17=BUPSYSMD;      /* SYSMOD NUMBER                 */ 00288000
         L     @03,@PC00001                                        0211 00289000
         MVC   IOPNAM17(7,@10),BUPSYSMD(@03)                       0211 00290000
*           IOPPTFNM=BUPSYSMD;      /* SYSMOD NUMBER FOR SCDS        */ 00291000
         MVC   IOPPTFNM(7,@10),BUPSYSMD(@03)                       0212 00292000
*           IOPPTFNT=''B;           /*                               */ 00293000
         XC    IOPPTFNT(104,@10),IOPPTFNT(@10)                     0213 00294000
*           IOPNTLVL=IOPPTFLC;      /* ENTRY LEVEL CODE              */ 00295000
         MVI   IOPNTLVL(@10),X'02'                                 0214 00296000
*           IOPPTYPE=BUPSTYPE;      /* SYSMOD TYPE                   */ 00297000
         MVC   IOPPTYPE(1,@10),BUPSTYPE(@03)                       0215 00298000
*           IOPDBYNO=BLANK7;        /*                               */ 00299000
         MVC   IOPDBYNO(7,@10),@CC01289                            0216 00300000
*           IOPSBYNO=BLANK7;        /*                               */ 00301000
         MVC   IOPSBYNO(7,@10),@CC01289                            0217 00302000
*           IOPBUSMD=BUPSYSMD;      /*                               */ 00303000
         MVC   IOPBUSMD(7,@10),BUPSYSMD(@03)                       0218 00304000
*           IOPBUTYP=IOPBUADD;      /*                               */ 00305000
         MVI   IOPBUTYP(@10),X'01'                                 0219 00306000
*           IOPPTFND(1)=IOPEOLST;   /* END OF LIST MARKER            */ 00307000
         MVI   IOPPTFND(@10),X'FF'                                 0220 00308000
*           SUBNTCNT=ONE;           /* NO SUBENTRIES IN SYSMOD       */ 00309000
         MVC   SUBNTCNT(4),@CF00040                                0221 00310000
*         END;                      /*                               */ 00311000
*     END;                          /*                               */ 00312000
*   ELSE                            /* IF SCDS IOP ALREADY GETMAINED */ 00313000
*     IF IOPNAM17^=BUPSYSMD         /* INSURE THAT THE CURRENT       */ 00314000
*       THEN                        /* SYSMOD IS SAME AS THE ONE     */ 00315000
         B     @RC00197                                            0224 00316000
@RF00197 L     @10,@PC00001                                        0224 00317000
