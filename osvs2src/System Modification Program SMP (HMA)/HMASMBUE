*BUELOC:                            /*                               */ 00347000
*   PROCEDURE;                      /*                               */ 00348000
BUELOC   STM   @14,@12,@SA00003                                    0228 00349000
*   DECLARE                         /*                               */ 00350000
*     LOCRC FIXED(8);               /*                               */ 00351000
*   RFY                                                            0230 00352000
*     HMASMIOP BASED(IOPPTR2);      /* IOPWITH SCDS SYSMOD ENTRY     */ 00353000
*   LOCRC=ZERO;                     /*                               */ 00354000
         SLR   LOCRC,LOCRC                                         0231 00355000
*   IF(BUPFUNCT=BUPEBKUP            /* BACKUP EXISING ENTRY          */ 00356000
*       |(BUPFUNCT=BUPABKUP         /* OR ADD NEW ENTRY BUT          */ 00357000
*       &BUPREDO=ON))               /* SYSMOD BEING REPROCESSED THEN    00358000
*                                      DO NOT SAVE ANY MEMBERS THAT     00359000
*                                      WERE SAVED FIRST TIME THRU    */ 00360000
*       &GMDONE=ON                  /* AND GETMAIN DONE THIS TIME    */ 00361000
*     THEN                          /*                               */ 00362000
         L     @10,@PC00001                                        0232 00363000
         CLI   BUPFUNCT(@10),2                                     0232 00364000
         BE    @GL00010                                            0232 00365000
         CLI   BUPFUNCT(@10),1                                     0232 00366000
         BNE   @RF00232                                            0232 00367000
         TM    BUPREDO(@10),B'10000000'                            0232 00368000
         BNO   @RF00232                                            0232 00369000
@GL00010 TM    GMDONE,B'10000000'                                  0232 00370000
         BNO   @RF00232                                            0232 00371000
*     DO;                           /*                               */ 00372000
*       IOPFUNCT=IOPLOC;            /* LOCATE ENTRY                  */ 00373000
         L     @10,IOPPTR2                                         0234 00374000
         MVI   IOPFUNCT(@10),X'03'                                 0234 00375000
*       IOPDSID=IOPSCDS;            /* ON SCDS                       */ 00376000
         MVI   IOPDSID(@10),X'21'                                  0235 00377000
*       CALL HMASMIO(HMASMIOP);     /*                               */ 00378000
         ST    @10,@AL00001                                        0236 00379000
         L     @15,@CV01283                                        0236 00380000
         LA    @01,@AL00001                                        0236 00381000
         BALR  @14,@15                                             0236 00382000
*       IF IOPRETRN=IOPGOOD         /* IF SYSMOD FOUND               */ 00383000
*         THEN                      /*                               */ 00384000
         L     @10,IOPPTR2                                         0237 00385000
         CLI   IOPRETRN(@10),0                                     0237 00386000
         BNE   @RF00237                                            0237 00387000
*         DO;                       /*                               */ 00388000
*           DO SUBNTCNT=1 BY 1 WHILE/* LOOP THRU SYSMOD SUBENTRIES   */ 00389000
*                 (IOPPTFND(SUBNTCNT)^=IOPEOLST);/* TILL END         */ 00390000
         LA    @10,1                                               0239 00391000
         ST    @10,SUBNTCNT                                        0239 00392000
         B     @DE00239                                            0239 00393000
@DL00239 DS    0H                                                  0240 00394000
*           END;                    /*                               */ 00395000
         LA    @10,1                                               0240 00396000
         AL    @10,SUBNTCNT                                        0240 00397000
         ST    @10,SUBNTCNT                                        0240 00398000
@DE00239 L     @10,SUBNTCNT                                        0240 00399000
         MH    @10,@CH00453                                        0240 00400000
         L     @03,IOPPTR2                                         0240 00401000
         SLR   @15,@15                                             0240 00402000
         IC    @15,IOPPTFND-9(@10,@03)                             0240 00403000
         CH    @15,@CH00642                                        0240 00404000
         BNE   @DL00239                                            0240 00405000
*         END;                      /*                               */ 00406000
*       ELSE                        /*                               */ 00407000
*         IF IOPRETRN>IOPNTFND      /* IF ERR IN LOCATE              */ 00408000
*           THEN                    /*                               */ 00409000
         B     @RC00237                                            0242 00410000
@RF00237 L     @10,IOPPTR2                                         0242 00411000
         CLI   IOPRETRN(@10),4                                     0242 00412000
         BNH   @RF00242                                            0242 00413000
*           LOCRC=IOPRETRN;         /* PASS ALONG RC                 */ 00414000
         SLR   LOCRC,LOCRC                                         0243 00415000
         IC    LOCRC,IOPRETRN(,@10)                                0243 00416000
*     END;                          /*                               */ 00417000
*                                                                  0244 00418000
@RF00242 DS    0H                                                  0244 00419000
@RC00237 DS    0H                                                  0245 00420000
*   /*****************************************************************/ 00421000
*   /*                                                               */ 00422000
*   /* RETURN TO CALLER                                              */ 00423000
*   /*                                                               */ 00424000
*   /*****************************************************************/ 00425000
*                                                                  0245 00426000
*   RETURN CODE(LOCRC);             /*                               */ 00427000
@RF00232 LR    @15,LOCRC                                           0245 00428000
         L     @14,@SA00003                                        0245 00429000
         LM    @00,@12,@SA00003+8                                  0245 00430000
         BR    @14                                                 0245 00431000
*   END BUELOC;                     /*                               */ 00432000
         EJECT                                                          00433000
*                                                                  0247 00434000
*   /*****************************************************************/ 00435000
*   /*                                                               */ 00436000
*   /* ROUTINE CALLED TO UPDATE SCDS SYSMOD ENTRY WITH ANOTHER       */ 00437000
