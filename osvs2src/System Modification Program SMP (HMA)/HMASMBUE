@RT00178 DS    0H                                                  0179 00149000
*     DO;                           /*                               */ 00150000
*       CALL BUEUPDAT;              /* UPDATE JCLIN FIELDS           */ 00151000
         BAL   @14,BUEUPDAT                                        0180 00152000
*       BUERTNCD=MAX(BUERTNCD,RTNCODE);/* SAVE RC                    */ 00153000
         CR    BUERTNCD,RTNCODE                                    0181 00154000
         BNL   *+6                                                      00155000
         LR    BUERTNCD,RTNCODE                                    0181 00156000
*     END;                          /*                               */ 00157000
*                                                                  0182 00158000
*   /*****************************************************************/ 00159000
*   /*                                                               */ 00160000
*   /* IF PREVIOUS FUNCTIONS HAD ANY ERROR OR IF FUNCTION WAS TO STOW*/ 00161000
*   /* SYSMOD THEN STOW IT NOW. STOWING SYSMOD AFTER ERROR CONDITION */ 00162000
*   /* ENABLES ANY BACKUP ENTRIES CREATED TO BE RESTORED VIA A CALL  */ 00163000
*   /* TO HMASMBUR BY THE CALLING PROGRAM                            */ 00164000
*   /*                                                               */ 00165000
*   /*****************************************************************/ 00166000
*                                                                  0183 00167000
*   IF(BUERTNCD>=STOPPTF            /*                               */ 00168000
*       |BUPFUNCT=BUPSTOW)          /*                               */ 00169000
*       &BUPSYSMD^=TXTJCLIN         /* AND NOT JCLIN FUNCTION        */ 00170000
*       &CCACHECK=OFF               /* AND NOT CHECK MODE            */ 00171000
*     THEN                          /*                               */ 00172000
@RF00178 CH    BUERTNCD,@CH00035                                   0183 00173000
         BNL   @GL00008                                            0183 00174000
         L     @14,@PC00001                                        0183 00175000
         CLI   BUPFUNCT(@14),3                                     0183 00176000
         BNE   @RF00183                                            0183 00177000
@GL00008 L     @14,@PC00001                                        0183 00178000
