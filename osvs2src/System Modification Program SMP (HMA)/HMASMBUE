*   RFY                                                            0158 00038000
*     HMASMIOP BASED(IOPPTR);       /* IOP WITH CDS ELEMENT ENTRY    */ 00039000
*   RFY                                                            0159 00040000
*     RTNCODE RSTD;                 /*                               */ 00041000
*                                                                  0159 00042000
*   /*****************************************************************/ 00043000
*   /*                                                               */ 00044000
*   /* IF IN CHECK MODE THEN NO PROCESSSING EXCEPT SETTING LAST      */ 00045000
*   /* UPDATE FIELDS IS REQUIRED                                     */ 00046000
*   /*                                                               */ 00047000
*   /*****************************************************************/ 00048000
*                                                                  0160 00049000
*   IF CCACHECK=OFF                 /* PROCESS ONLY IF NOT CHECK     */ 00050000
*     THEN                          /*                               */ 00051000
         TM    CCACHECK(CCAPTR),B'00010000'                        0160 00052000
         BNZ   @RF00160                                            0160 00053000
*     DO;                           /*                               */ 00054000
*                                                                  0161 00055000
*       /*************************************************************/ 00056000
*       /*                                                           */ 00057000
*       /* CHECK TO SEE IF ANOTHER IOP IS REQUIRED TO BUILD THE      */ 00058000
*       /* SYSMOD ENTRY                                              */ 00059000
*       /*                                                           */ 00060000
*       /*************************************************************/ 00061000
*                                                                  0162 00062000
*       IF BUERTNCD=ZERO            /* IF PREV FUNCTIONS OK          */ 00063000
*           &BUPSYSMD^=TXTJCLIN     /* AND NOT JCLIN FUNCTION        */ 00064000
*         THEN                      /*                               */ 00065000
         CH    BUERTNCD,@CH00270                                   0162 00066000
         BNE   @RF00162                                            0162 00067000
         CLC   BUPSYSMD(7,@10),@CC01292                            0162 00068000
         BE    @RF00162                                            0162 00069000
*         DO;                       /*                               */ 00070000
*           CALL BUEGMIOP;          /*                               */ 00071000
         BAL   @14,BUEGMIOP                                        0164 00072000
*           BUERTNCD=MAX(BUERTNCD,RTNCODE);/*                        */ 00073000
         CR    BUERTNCD,RTNCODE                                    0165 00074000
         BNL   *+6                                                      00075000
         LR    BUERTNCD,RTNCODE                                    0165 00076000
*         END;                      /*                               */ 00077000
*                                                                  0166 00078000
*       /*************************************************************/ 00079000
*       /*                                                           */ 00080000
*       /* CHECK TO SEE IF THE SYSMOD MUST BE LOCATED ON SCDS BEFORE */ 00081000
*       /* ADDING ANOTHER SUBENTRY TO IT                             */ 00082000
*       /*                                                           */ 00083000
*       /*************************************************************/ 00084000
*                                                                  0167 00085000
*       IF BUERTNCD=ZERO            /* IF PREVIOUS FUNCTION OK       */ 00086000
*           &BUPSYSMD^=TXTJCLIN     /* AND NOT JCLIN FUNCTION        */ 00087000
*         THEN                      /*                               */ 00088000
