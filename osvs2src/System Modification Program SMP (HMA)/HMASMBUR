*   /*                                                               */ 00345000
*   /* ROUTINE CALLED TO RESTORE CDS ENTRIES FROM SCDS ENTRIES       */ 00346000
*   /*                                                               */ 00347000
*   /*****************************************************************/ 00348000
*                                                                  0250 00349000
*BURREST:                           /*                               */ 00350000
*   PROCEDURE;                      /*                               */ 00351000
BURREST  STM   @14,@12,@SA00003                                    0250 00352000
*   DECLARE                         /*                               */ 00353000
*     RSTX FIXED(31);               /* LOOP THRU SCDS SYSMOD         */ 00354000
*   DECLARE                         /*                               */ 00355000
*     RESTRC FIXED(8);              /* PROC RETURN CODE              */ 00356000
*   RFY                                                            0253 00357000
*     HMASMIOP BASED(IOPPTR);       /* IOP WITH SCDS SYSMOD          */ 00358000
*                                                                  0253 00359000
*   /*****************************************************************/ 00360000
*   /*                                                               */ 00361000
*   /* LOOP THRU SCDS SYSMOD SUBENTRIES AND FOR EACH SUBENTRY DO     */ 00362000
*   /* PROCESSING NECESSARY TO GET SCDS BACK TO CDS                  */ 00363000
*   /*                                                               */ 00364000
*   /*****************************************************************/ 00365000
*                                                                  0254 00366000
*   RESTRC=ZERO;                    /* INIT RC                       */ 00367000
         SLR   RESTRC,RESTRC                                       0254 00368000
*   DO RSTX=1 BY 1 WHILE(           /* LOOP THRU SCDS SYSMOD         */ 00369000
*         IOPPTFND(RSTX)^=IOPEOLST  /* TILL END                      */ 00370000
*         &RESTRC<STOPPTF);         /* AND NO SERIOUS ERRORS         */ 00371000
         LA    @15,1                                               0255 00372000
         ST    @15,RSTX                                            0255 00373000
         B     @DE00255                                            0255 00374000
@DL00255 DS    0H                                                  0256 00375000
*     RESTADD=OFF;                  /* NOT DELETED TYPE ENTRY        */ 00376000
*                                                                  0256 00377000
         NI    RESTADD,B'11110111'                                 0256 00378000
*     /***************************************************************/ 00379000
*     /*                                                             */ 00380000
*     /* CHECK SUBENTRY TYPE TO SET TYPE CODE FOR CDS ENTRY THEN TO  */ 00381000
*     /* SEE IF ENTRY WAS DELETED/UPDATED OR ADDED                   */ 00382000
*     /*                                                             */ 00383000
*     /***************************************************************/ 00384000
*                                                                  0257 00385000
*     IF IOPPIND(RSTX)=IOPSAASM     /* IF UPDATED OR                 */ 00386000
*         |IOPPIND(RSTX)=IOPSUASM   /* OR DELETED ASSEM              */ 00387000
*         |IOPPIND(RSTX)=IOPSDASM   /* OR DELETED ASSEM              */ 00388000
*       THEN                        /*                               */ 00389000
         L     @15,RSTX                                            0257 00390000
         MH    @15,@CH00453                                        0257 00391000
         SLR   @14,@14                                             0257 00392000
         IC    @14,IOPPIND-9(@15,IOPPTR)                           0257 00393000
         CH    @14,@CH01027                                        0257 00394000
         BE    @RT00257                                            0257 00395000
         CH    @14,@CH01047                                        0257 00396000
         BE    @RT00257                                            0257 00397000
         CH    @14,@CH00825                                        0257 00398000
         BNE   @RF00257                                            0257 00399000
@RT00257 DS    0H                                                  0258 00400000
*       SAVTYPE=IOPASM;             /* MUST DELETE                   */ 00401000
         MVI   SAVTYPE,X'01'                                       0258 00402000
*     ELSE                          /*                               */ 00403000
*       IF IOPPIND(RSTX)=IOPSALMD   /*                               */ 00404000
*           |IOPPIND(RSTX)=IOPSULMD /*                               */ 00405000
*           |IOPPIND(RSTX)=IOPSDLMD /*                               */ 00406000
*         THEN                      /*                               */ 00407000
         B     @RC00257                                            0259 00408000
@RF00257 L     @15,RSTX                                            0259 00409000
         MH    @15,@CH00453                                        0259 00410000
         SLR   @14,@14                                             0259 00411000
         IC    @14,IOPPIND-9(@15,IOPPTR)                           0259 00412000
         CH    @14,@CH01029                                        0259 00413000
         BE    @RT00259                                            0259 00414000
         CH    @14,@CH01049                                        0259 00415000
         BE    @RT00259                                            0259 00416000
         CH    @14,@CH01039                                        0259 00417000
         BNE   @RF00259                                            0259 00418000
@RT00259 DS    0H                                                  0260 00419000
*         SAVTYPE=IOPLMD;           /*                               */ 00420000
         MVI   SAVTYPE,X'02'                                       0260 00421000
*       ELSE                        /*                               */ 00422000
*         IF IOPPIND(RSTX)=IOPSAMAC /*                               */ 00423000
*             |IOPPIND(RSTX)=IOPSUMAC/*                              */ 00424000
*             |IOPPIND(RSTX)=IOPSDMAC/*                              */ 00425000
*           THEN                    /*                               */ 00426000
         B     @RC00259                                            0261 00427000
@RF00259 L     @15,RSTX                                            0261 00428000
         MH    @15,@CH00453                                        0261 00429000
         SLR   @14,@14                                             0261 00430000
         IC    @14,IOPPIND-9(@15,IOPPTR)                           0261 00431000
         CH    @14,@CH00725                                        0261 00432000
         BE    @RT00261                                            0261 00433000
         CH    @14,@CH01051                                        0261 00434000
         BE    @RT00261                                            0261 00435000
         CH    @14,@CH00269                                        0261 00436000
         BNE   @RF00261                                            0261 00437000
@RT00261 DS    0H                                                  0262 00438000
*           SAVTYPE=IOPMAC;         /*                               */ 00439000
