*   /*****************************************************************/ 00817000
*   /*                                                               */ 00818000
*   /* ROUTINE TO LOCATE SCDS ELEMENT ENTRY                          */ 00819000
*   /*                                                               */ 00820000
*   /*****************************************************************/ 00821000
*                                                                  0328 00822000
*BURSCDSL:                          /*                               */ 00823000
*   PROCEDURE(SCDLOCX,SCDLOCNM,SCDLOCTP);/*                          */ 00824000
BURSCDSL STM   @14,@12,@SA00004                                    0328 00825000
         MVC   @PC00004(12),0(@01)                                 0328 00826000
*   DECLARE                         /*                               */ 00827000
*     SCDLOCX FIXED(31),            /* OFFSET INTO PTF LIST          */ 00828000
*     SCDLOCNM CHAR(8),             /* ELEMENT NAME                  */ 00829000
*     SCDLOCTP FIXED(8);            /* ELEMENT TYPE CODE             */ 00830000
*   DECLARE                         /*                               */ 00831000
*     SCDLOCRC FIXED(8);            /*                               */ 00832000
