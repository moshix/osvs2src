         TITLE 'HMASMCIL, RELFILE PRE-PROCESSOR'                        00001000
HMASMCIL CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMCIL  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(8),0(@01)                                  0001 00017000
         EJECT                                                          00018000
*                                                                  0198 00019000
*   /*****************************************************************/ 00020000
*   /*                                                               */ 00021000
*   /* INITIALIZE ERROR CODES SWITCHES, ETC.                         */ 00022000
*   /*                                                               */ 00023000
*   /*****************************************************************/ 00024000
*                                                                  0198 00025000
*   HIGHCIL=ZERO;                   /*                               */ 00026000
         SLR   HIGHCIL,HIGHCIL                                     0198 00027000
*   SWITCHES=''B;                   /*                               */ 00028000
         MVI   SWITCHES,X'00'                                      0199 00029000
*   IOPPTR=CCAIOPTR;                /* GENERAL USE IOP               */ 00030000
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0200 00031000
*   SPLPMAD=ADDR(CILPDSN);          /*                               */ 00032000
         LA    @10,CILPDSN                                         0201 00033000
         ST    @10,SPLPMAD                                         0201 00034000
*   SPLFUNCT=SPLBDDSN;              /* SUBROUTINE FUNCTION           */ 00035000
         MVI   SPLFUNCT,X'05'                                      0202 00036000
*   CILODSN=BLANK;                  /* INIT RELFILE SAVE AREA        */ 00037000
         MVI   CILODSN+1,C' '                                      0203 00038000
         MVC   CILODSN+2(42),CILODSN+1                             0203 00039000
         MVI   CILODSN,C' '                                        0203 00040000
*   CILPDSN=BLANK;                  /* INIT RELFILE FOR SUB          */ 00041000
         MVI   CILPDSN+1,C' '                                      0204 00042000
         MVC   CILPDSN+2(42),CILPDSN+1                             0204 00043000
         MVI   CILPDSN,C' '                                        0204 00044000
*   CILTLIB=BLANK;                  /* INIT RELFILE DSN FOR SMPTLIB  */ 00045000
         MVI   CILTLIB+1,C' '                                      0205 00046000
         MVC   CILTLIB+2(42),CILTLIB+1                             0205 00047000
         MVI   CILTLIB,C' '                                        0205 00048000
*   RFY                                                            0206 00049000
*     RVAPARM BASED(CCARVAD);       /* BASE FOR STAE PARM            */ 00050000
*                                                                  0206 00051000
*   /*****************************************************************/ 00052000
*   /*                                                               */ 00053000
*   /* SEARCH THE ICT AND DETERMINE WHICH ELEMENTS SHOULD BE         */ 00054000
*   /* TRANSFERED TO SMPWRK5                                         */ 00055000
*   /*                                                               */ 00056000
*   /*****************************************************************/ 00057000
*                                                                  0207 00058000
*   CALL CILCHK;                    /*                               */ 00059000
*                                                                  0207 00060000
         BAL   @14,CILCHK                                          0207 00061000
*   /*****************************************************************/ 00062000
*   /*                                                               */ 00063000
*   /* BUILD IEBCOPY CONTROL STMTS FOR ALL ELEMENTS TO BE TRANSFERED */ 00064000
*   /* AND INVOKE IEBCOPY.                                           */ 00065000
*   /*                                                               */ 00066000
*   /*****************************************************************/ 00067000
*                                                                  0208 00068000
*   CALL CILCOPY;                   /*                               */ 00069000
*                                                                  0208 00070000
         BAL   @14,CILCOPY                                         0208 00071000
*   /*****************************************************************/ 00072000
*   /*                                                               */ 00073000
*   /* RETURN TO CALLER. ANY IEBCOPY ERROR WILL TERMINATE THE        */ 00074000
*   /* FUNCTION.                                                     */ 00075000
*   /*                                                               */ 00076000
*   /*****************************************************************/ 00077000
*                                                                  0209 00078000
*   RETURN CODE(HIGHCIL);           /*                               */ 00079000
         LR    @15,HIGHCIL                                         0209 00080000
         L     @13,4(,@13)                                         0209 00081000
         L     @14,12(,@13)                                        0209 00082000
         LM    @00,@12,20(@13)                                     0209 00083000
         BR    @14                                                 0209 00084000
         EJECT                                                          00085000
*                                                                  0210 00086000
*   /*****************************************************************/ 00087000
*   /*                                                               */ 00088000
*   /* LOOK AT EACH LMOD IN THE ICT AND FOR EACH WHICH WILL BE       */ 00089000
*   /* LINKEDITED SEARCH CHECK ITS CHAINED MODS TO DETERMINE WHICH   */ 00090000
*   /* ELEMENTS SHOULD BE TRANSFERED TO SMPWRK5. NO TRANSFERS WILL BE*/ 00091000
*   /* DONE UNLESS MORE THAN ONE SMPTLIB DATA SET IS PRESENT. ONLY   */ 00092000
*   /* SELECTED MODULE REPLACEMENTS WHICH ARE TO BE LINKEDITED AND   */ 00093000
*   /* FROM OTHER THAN THE FIRST SMPTLIB WILL BE MARKED FOR          */ 00094000
*   /* PROCESSING .                                                  */ 00095000
*   /*                                                               */ 00096000
*   /*****************************************************************/ 00097000
*                                                                  0210 00098000
*CILCHK:                                                           0210 00099000
*   PROC;                           /*                               */ 00100000
@EL00001 L     @13,4(,@13)                                         0210 00101000
@EF00001 DS    0H                                                  0210 00102000
@ER00001 LM    @14,@12,12(@13)                                     0210 00103000
         BR    @14                                                 0210 00104000
CILCHK   STM   @14,@12,@SA00002                                    0210 00105000
*   RFY                                                            0211 00106000
*     ICTLMOD BASED(CCAICLMD);      /* ICT LMOD BASE                 */ 00107000
*   RFY                                                            0212 00108000
*     ICTIXLF BASED(ICTLCHN(L));    /* CAIN PTRS TO MODS             */ 00109000
*   RFY                                                            0213 00110000
*     ICTMOD BASED(ICTIXL(A));      /* MODS BASED ON LMOD CHAIN      */ 00111000
*   RFY                                                            0214 00112000
*     ICTPTF BASED(ICTPPTR(1));     /* SYSMOD OWNER OF THE MOD       */ 00113000
*   DECLARE                                                        0215 00114000
*     A FIXED(31);                  /* LOOP COUNTER                  */ 00115000
*   DECLARE                                                        0216 00116000
*     L FIXED(31);                  /* LOOP COUNTER                  */ 00117000
*                                                                  0216 00118000
*   /*****************************************************************/ 00119000
*   /*                                                               */ 00120000
*   /* LOOK AT ICT LMOD ENTRIES. IF THE LMOD IS TO BE LINKEDITED THEN*/ 00121000
*   /* CHECK ITS MODS.                                               */ 00122000
*   /*                                                               */ 00123000
*   /*****************************************************************/ 00124000
*                                                                  0217 00125000
*   DO L=1 BY 1 WHILE ICTLEND(L)^=ICTENDNT;/* LMOD LOOP              */ 00126000
         LA    L,1                                                 0217 00127000
         B     @DE00217                                            0217 00128000
@DL00217 DS    0H                                                  0218 00129000
*     IF ICTLLKD(L)=ON              /* LOAD MOD WILL BE LINKEDITED   */ 00130000
*         &ICTLCPL(L)=OFF THEN      /* AND NOT COMPLETED             */ 00131000
*                                                                  0218 00132000
         LR    @10,L                                               0218 00133000
         MH    @10,@CH01010                                        0218 00134000
         L     @02,CCAICLMD(,CCAPTR)                               0218 00135000
         ST    @10,@TF00001                                        0218 00136000
         ALR   @10,@02                                             0218 00137000
         AL    @10,@CF01828                                        0218 00138000
         TM    ICTLLKD-14(@10),B'00001000'                         0218 00139000
         BNO   @RF00218                                            0218 00140000
         AL    @02,@TF00001                                        0218 00141000
         AL    @02,@CF01829                                        0218 00142000
         TM    ICTLCPL-8(@02),B'00100000'                          0218 00143000
         BNZ   @RF00218                                            0218 00144000
*       /*************************************************************/ 00145000
*       /*                                                           */ 00146000
*       /* LOOK AT MODS FOR THIS LMOD                                */ 00147000
*       /*                                                           */ 00148000
*       /*************************************************************/ 00149000
*                                                                  0219 00150000
*       DO A=1 BY 1 WHILE ICTIXL(A)^=ICTENDIX;/*                     */ 00151000
*                                                                  0219 00152000
         LA    A,1                                                 0219 00153000
         B     @DE00219                                            0219 00154000
@DL00219 DS    0H                                                  0220 00155000
*         /***********************************************************/ 00156000
*         /*                                                         */ 00157000
*         /* CHECK FOR ONLY MOD REPLACEMENTS FROM RELFILES(SMPPTLIB) */ 00158000
*         /* WHICH ARE TO BE LINKED AND ARE SELECTED.                */ 00159000
*         /*                                                         */ 00160000
*         /***********************************************************/ 00161000
*                                                                  0220 00162000
*         IF ICTMMOD(1)=ON          /* ELEMENT IS REPLACENET         */ 00163000
*             &ICTMRELF(1)=ON       /* ELEMENT IS FROM RELFILE       */ 00164000
*             &ICTMISEL(1)=ON       /* SELECTED                      */ 00165000
*             &ICTMCPL(1)=OFF THEN  /* NOT COMPLETED                 */ 00166000
         LR    @10,A                                               0220 00167000
         SLA   @10,2                                               0220 00168000
         LR    @05,L                                               0220 00169000
         MH    @05,@CH01010                                        0220 00170000
         L     @02,CCAICLMD(,CCAPTR)                               0220 00171000
         AL    @02,@CF01830                                        0220 00172000
         L     @05,ICTLCHN-34(@05,@02)                             0220 00173000
         AL    @05,@CF01831                                        0220 00174000
         L     @10,ICTIXL(@10,@05)                                 0220 00175000
         TM    ICTMMOD(@10),B'10000000'                            0220 00176000
         BNO   @RF00220                                            0220 00177000
         TM    ICTMRELF(@10),B'00100000'                           0220 00178000
         BNO   @RF00220                                            0220 00179000
         TM    ICTMISEL(@10),B'10000000'                           0220 00180000
         BNO   @RF00220                                            0220 00181000
         TM    ICTMCPL(@10),B'00100000'                            0220 00182000
         BNZ   @RF00220                                            0220 00183000
*           DO;                     /*                               */ 00184000
*             SPLIPFX=ICTPTPFX(1);  /* RELFILE DSN PREFIX            */ 00185000
         L     @05,SPLPMAD                                         0222 00186000
         L     @02,ICTPPTR(,@10)                                   0222 00187000
         MVC   SPLIPFX(26,@05),ICTPTPFX(@02)                       0222 00188000
*             SPLISMD=ICTPTFS(1);   /*                               */ 00189000
         MVC   SPLISMD(7,@05),ICTPTFS(@02)                         0223 00190000
*             SPLIFIL=ICTMRELN(1);  /*                               */ 00191000
         MVC   SPLIFIL(2,@05),ICTMRELN(@10)                        0224 00192000
*             CALL HMASMSUB(HMASMSPL);/*                             */ 00193000
*                                                                  0225 00194000
         L     @15,@CV01681                                        0225 00195000
         LA    @01,@AL00225                                        0225 00196000
         BALR  @14,@15                                             0225 00197000
*             /*******************************************************/ 00198000
*             /*                                                     */ 00199000
*             /* DETERMINE FIRST RELFILE DATA SET (SMPTLIB). ELEMENTS*/ 00200000
*             /* IN OTHER THAN THIS ONE WILL BE TRANSFERED.          */ 00201000
*             /*                                                     */ 00202000
*             /*******************************************************/ 00203000
*                                                                  0226 00204000
*             IF CILTLIB(1)=BLANK   /* FIRST SMPTLIB NOT FOUND YET   */ 00205000
*               THEN                /*                               */ 00206000
         CLI   CILTLIB,C' '                                        0226 00207000
         BNE   @RF00226                                            0226 00208000
*               DO;                 /*                               */ 00209000
*                 CILTLIB=SPLODSN;  /* THIS IS THE FIRST, SAVE IT    */ 00210000
         L     @10,SPLPMAD                                         0228 00211000
         MVC   CILTLIB(44),SPLODSN(@10)                            0228 00212000
*                 CILPTFNO=ICTPTFS(1);/* SAVE PTF NUMBER             */ 00213000
         L     @05,@PC00001                                        0229 00214000
         LR    @02,A                                               0229 00215000
         SLA   @02,2                                               0229 00216000
         LR    @15,L                                               0229 00217000
         MH    @15,@CH01010                                        0229 00218000
         L     @01,CCAICLMD(,CCAPTR)                               0229 00219000
         AL    @01,@CF01830                                        0229 00220000
         L     @01,ICTLCHN-34(@15,@01)                             0229 00221000
         AL    @01,@CF01831                                        0229 00222000
         L     @02,ICTIXL(@02,@01)                                 0229 00223000
         L     @02,ICTPPTR(,@02)                                   0229 00224000
         MVC   CILPTFNO(7,@05),ICTPTFS(@02)                        0229 00225000
*                 CILPTFDS=SPLODSN; /* AND DSN                       */ 00226000
         L     @05,@PC00001+4                                      0230 00227000
         MVC   CILPTFDS(44,@05),SPLODSN(@10)                       0230 00228000
*               END;                /*                               */ 00229000
*                                                                  0231 00230000
*             /*******************************************************/ 00231000
*             /*                                                     */ 00232000
*             /* IF RELFILE (SMPTLIB) NAME OF THIS ELEMENT IS NOT THE*/ 00233000
*             /* ONE SAVED THEN THIS ELEMENT IS MARKED FOR TRANSFER. */ 00234000
*             /*                                                     */ 00235000
*             /*******************************************************/ 00236000
*                                                                  0232 00237000
*             IF SPLODSN^=CILTLIB THEN/*                             */ 00238000
@RF00226 L     @10,SPLPMAD                                         0232 00239000
         CLC   SPLODSN(44,@10),CILTLIB                             0232 00240000
         BE    @RF00232                                            0232 00241000
*               DO;                 /*                               */ 00242000
*                 ICTFMLIB(1)=SMPWRK5;/* FOR LINKEDIT INCLUDE        */ 00243000
         LR    @10,A                                               0234 00244000
         SLA   @10,2                                               0234 00245000
         LR    @05,L                                               0234 00246000
         MH    @05,@CH01010                                        0234 00247000
         L     @02,CCAICLMD(,CCAPTR)                               0234 00248000
         AL    @02,@CF01830                                        0234 00249000
         L     @05,ICTLCHN-34(@05,@02)                             0234 00250000
         AL    @05,@CF01831                                        0234 00251000
         L     @10,ICTIXL(@10,@05)                                 0234 00252000
         MVC   ICTFMLIB(8,@10),@CC01721                            0234 00253000
*                 ICTMTRX(1)=ON;    /* MOVE THIS TO SMPWRK5          */ 00254000
         OI    ICTMTRX(@10),B'00010000'                            0235 00255000
*               END;                /*                               */ 00256000
*           END;                    /*                               */ 00257000
@RF00232 DS    0H                                                  0238 00258000
*       END;                        /* END ICT MOD LOOP              */ 00259000
@RF00220 AH    A,@CH00089                                          0238 00260000
@DE00219 LR    @10,A                                               0238 00261000
         SLA   @10,2                                               0238 00262000
         LR    @05,L                                               0238 00263000
         MH    @05,@CH01010                                        0238 00264000
         L     @02,CCAICLMD(,CCAPTR)                               0238 00265000
         AL    @02,@CF01830                                        0238 00266000
         L     @05,ICTLCHN-34(@05,@02)                             0238 00267000
         AL    @05,@CF01831                                        0238 00268000
         L     @10,ICTIXL(@10,@05)                                 0238 00269000
         CL    @10,ICTENDIX                                        0238 00270000
         BNE   @DL00219                                            0238 00271000
*   END;                            /* END LMOD LOOP                 */ 00272000
@RF00218 AH    L,@CH00089                                          0239 00273000
@DE00217 LR    @10,L                                               0239 00274000
         MH    @10,@CH01010                                        0239 00275000
         L     @02,CCAICLMD(,CCAPTR)                               0239 00276000
         AL    @02,@CF01833                                        0239 00277000
         L     @10,ICTLEND(@10,@02)                                0239 00278000
         CL    @10,ICTENDNT                                        0239 00279000
         BNE   @DL00217                                            0239 00280000
*   END CILCHK;                     /*                               */ 00281000
@EL00002 DS    0H                                                  0240 00282000
@EF00002 DS    0H                                                  0240 00283000
@ER00002 LM    @14,@12,@SA00002                                    0240 00284000
         BR    @14                                                 0240 00285000
         EJECT                                                          00286000
*                                                                  0241 00287000
*   /*****************************************************************/ 00288000
*   /*                                                               */ 00289000
*   /* SEARCH THRU ICT MOD SECTION FOR THOSE SELECTED FOR TRANSFER.  */ 00290000
*   /* SET UP THE SMPTLIB DD NAME WITH THE RELFILE DATA SET NAME.    */ 00291000
*   /* WRITE THE IEBCOPY 'COPY' STATEMENT AND MEMBER 'SELECT'        */ 00292000
*   /* STATEMENTS. INVOKE IEBCOPY FOR EACH RELFILE DATA SET WHICH HAS*/ 00293000
*   /* ELEMEMTS TO BE TRANSFERED.                                    */ 00294000
*   /*                                                               */ 00295000
*   /*****************************************************************/ 00296000
*                                                                  0241 00297000
*CILCOPY:                                                          0241 00298000
*   PROC;                           /*                               */ 00299000
CILCOPY  STM   @14,@04,@SA00003                                    0241 00300000
         STM   @06,@12,@SA00003+28                                 0241 00301000
*   RFY                                                            0242 00302000
*     ICTMOD BASED(CCAICMOD);       /*                               */ 00303000
*   DECLARE                                                        0243 00304000
*     B FIXED(31);                  /* LOOP COUNTER                  */ 00305000
*   DECLARE                                                        0244 00306000
*     SAVENDX FIXED(31);            /* INDEX FOR MOD REFERENCE       */ 00307000
*                                                                  0244 00308000
*   /*****************************************************************/ 00309000
*   /*                                                               */ 00310000
*   /* THE PROCESSING IS REPEATED UNTIL ALL RELFILE DATA SETS HAVING */ 00311000
*   /* MEMEBERS TO TRANSFER HAVE BEEN PROCESSED.                     */ 00312000
*   /*                                                               */ 00313000
*   /*****************************************************************/ 00314000
*                                                                  0245 00315000
*   DOCOPY=ON;                      /* INDICATES NEED FOR COPIES     */ 00316000
         OI    DOCOPY,B'00100000'                                  0245 00317000
*   DO WHILE DOCOPY=ON&HIGHCIL=ZERO;/*                               */ 00318000
         B     @DE00246                                            0246 00319000
@DL00246 DS    0H                                                  0247 00320000
*     SAVENDX=ZERO;                 /* INIT INDEX FOR REFERENCE      */ 00321000
         SLR   SAVENDX,SAVENDX                                     0247 00322000
*     DOCOPY=OFF;                   /* RESET COTROL SWITCH           */ 00323000
         NI    DOCOPY,B'11011111'                                  0248 00324000
*     CILODSN=BLANK;                /* CURRENT RELFILE               */ 00325000
         MVI   CILODSN+1,C' '                                      0249 00326000
         MVC   CILODSN+2(42),CILODSN+1                             0249 00327000
         MVI   CILODSN,C' '                                        0249 00328000
*     FIRSTSW=ON;                   /* FIRST PASS FOR RELFILE        */ 00329000
*                                                                  0250 00330000
         OI    FIRSTSW,B'10000000'                                 0250 00331000
*     /***************************************************************/ 00332000
*     /*                                                             */ 00333000
*     /* CHECK FOR ELIGIBLE ELEMENTS IN THE ICT MOD SECTION AND WRITE*/ 00334000
*     /* IEBCOPY STATEMENTS.                                         */ 00335000
*     /*                                                             */ 00336000
*     /***************************************************************/ 00337000
*                                                                  0251 00338000
*     DO B=1 BY 1 WHILE ICTMEND(B)^=ICTENDNT/* LOOP THRU MODS        */ 00339000
*           &HIGHCIL=ZERO;          /*                               */ 00340000
         LA    B,1                                                 0251 00341000
         B     @DE00251                                            0251 00342000
@DL00251 DS    0H                                                  0252 00343000
*       IF ICTMTRX(B)=ON THEN       /* TO BE TRANSFERED              */ 00344000
         LR    @10,B                                               0252 00345000
         MH    @10,@CH01826                                        0252 00346000
         L     @07,CCAICMOD(,CCAPTR)                               0252 00347000
         ST    @10,@TF00001                                        0252 00348000
         ALR   @10,@07                                             0252 00349000
         AL    @10,@CF01837                                        0252 00350000
         TM    ICTMTRX-10(@10),B'00010000'                         0252 00351000
         BNO   @RF00252                                            0252 00352000
*         DO;                       /*                               */ 00353000
*           SELSW=OFF;              /* INIT ELEMENT SELECT SW        */ 00354000
*                                                                  0254 00355000
         NI    SELSW,B'10111111'                                   0254 00356000
*           /*********************************************************/ 00357000
*           /*                                                       */ 00358000
*           /* CHECK THIS ELEMENTS RELFILE. IF FROM ONE BEING        */ 00359000
*           /* PROCESSED IN THIS PASS THEN BUILD STATEMENT FOR IT. IF*/ 00360000
*           /* SELSW IS SET ON THEN THE ELEMENT GETS SELECTED.       */ 00361000
*           /*                                                       */ 00362000
*           /*********************************************************/ 00363000
*                                                                  0255 00364000
*           CALL NAMECMPR(ADDR(ICTMNAME(B)));/*                      */ 00365000
         L     @10,@TF00001                                        0255 00366000
         AL    @07,@CF01839                                        0255 00367000
         LA    @10,ICTMNAME(@10,@07)                               0255 00368000
         ST    @10,@AFTEMPS                                        0255 00369000
         LA    @01,@AL00255                                        0255 00370000
         BAL   @14,NAMECMPR                                        0255 00371000
*           IF SELSW=ON THEN        /* SAME DATASET                  */ 00372000
         TM    SELSW,B'01000000'                                   0256 00373000
         BNO   @RF00256                                            0256 00374000
*             DO;                   /*                               */ 00375000
*               CALL STATEMNT(ADDR(ICTMNAME(B)));/* WRITE STMTS      */ 00376000
         LR    @10,B                                               0258 00377000
         MH    @10,@CH01826                                        0258 00378000
         L     @02,CCAICMOD(,CCAPTR)                               0258 00379000
         AL    @02,@CF01839                                        0258 00380000
         LA    @10,ICTMNAME(@10,@02)                               0258 00381000
         ST    @10,@AFTEMPS                                        0258 00382000
         LA    @01,@AL00258                                        0258 00383000
         BAL   @14,STATEMNT                                        0258 00384000
*               FIRSTSW=OFF;        /* FIRST HAS BEEN PROCESSED      */ 00385000
*                                                                  0259 00386000
         NI    FIRSTSW,B'01111111'                                 0259 00387000
*               /*****************************************************/ 00388000
*               /*                                                   */ 00389000
*               /* WRITE SELECT STMSTS FOR ALIAS NAMES IF ANY        */ 00390000
*               /*                                                   */ 00391000
*               /*****************************************************/ 00392000
*                                                                  0260 00393000
*               CALL ALIAS(ICTMTCHN(B));/* PTR TO ALIAS CHAIN IS PARM*/ 00394000
         LR    @10,B                                               0260 00395000
         MH    @10,@CH01826                                        0260 00396000
         L     @02,CCAICMOD(,CCAPTR)                               0260 00397000
         AL    @02,@CF01841                                        0260 00398000
         LA    @10,ICTMTCHN-116(@10,@02)                           0260 00399000
         ST    @10,@AL00001                                        0260 00400000
         LA    @01,@AL00001                                        0260 00401000
         BAL   @14,ALIAS                                           0260 00402000
*               ICTMTRX(B)=OFF;     /* THIS ELEMENT PROCESSED        */ 00403000
         LR    @10,B                                               0261 00404000
         MH    @10,@CH01826                                        0261 00405000
         L     @07,CCAICMOD(,CCAPTR)                               0261 00406000
         ALR   @07,@10                                             0261 00407000
         AL    @07,@CF01837                                        0261 00408000
         NI    ICTMTRX-10(@07),B'11101111'                         0261 00409000
*               DOCOPY=ON;          /* INDICATE ELEMENTS TO COPY     */ 00410000
         OI    DOCOPY,B'00100000'                                  0262 00411000
*               SAVENDX=B;          /* FOR ELEMENT REFERENCE         */ 00412000
         LR    SAVENDX,B                                           0263 00413000
*             END;                  /*                               */ 00414000
*         END;                      /* ELIGIBLE MOD DO               */ 00415000
@RF00256 DS    0H                                                  0266 00416000
*     END;                          /* ICT MOD LOOP                  */ 00417000
@RF00252 AH    B,@CH00089                                          0266 00418000
@DE00251 LR    @10,B                                               0266 00419000
         MH    @10,@CH01826                                        0266 00420000
         L     @07,CCAICMOD(,CCAPTR)                               0266 00421000
         AL    @07,@CF01839                                        0266 00422000
         L     @10,ICTMEND(@10,@07)                                0266 00423000
         CL    @10,ICTENDNT                                        0266 00424000
         BE    @DC00251                                            0266 00425000
