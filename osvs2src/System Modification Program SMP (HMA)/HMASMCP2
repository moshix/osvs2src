         TITLE 'HMASMCP2 CHECK A SYSMOD FOR GROUP-COMPLETE PROCESSING'  00001000
HMASMCP2 CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMCP2  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         L     @00,@SIZDATD                                        0001 00013000
         GETMAIN  R,LV=(0)                                              00014000
         LR    @10,@01                                             0001 00015000
         USING @DATD,@10                                           0001 00016000
         ST    @13,@SA00001+4                                      0001 00017000
         LM    @00,@01,20(@13)                                     0001 00018000
         ST    @10,8(,@13)                                         0001 00019000
         LR    @13,@10                                             0001 00020000
         XC    @ZTEMPS(@ZLEN),@ZTEMPS                                   00021000
         MVC   @PC00001(4),0(@01)                                  0001 00022000
*                                      ICT PTF SECTION ENTRY PASSED     00023000
*                                      BY THE CALLER                 */ 00024000
*   PTFPTR=ADDR(PTFENTRY);          /* SET BASE FOR ICT PTF          */ 00025000
         L     PTFPTR,@PC00001                                     0056 00026000
*   CP2RETRN=NOTCOMPL;              /* INITIALIZE LOCAL RETURN CODE     00027000
*                                      TO "NOT COMPLETE"             */ 00028000
         LA    CP2RETRN,4                                          0057 00029000
*   IF ICTPGCPL(1)=ON               /* SYSMOD ALREADY MARKED "GROUP     00030000
*                                      COMPLETE"                     */ 00031000
*     THEN                                                         0058 00032000
         TM    ICTPGCPL(PTFPTR),B'10000000'                        0058 00033000
         BNO   @RF00058                                            0058 00034000
*     CP2RETRN=COMPLETE;            /* SET REQISISTES "COMPLETE"     */ 00035000
         SLR   CP2RETRN,CP2RETRN                                   0059 00036000
*   CP2FLGS=''B;                    /* CLEAR LOCAL PROCESSING FLAGS  */ 00037000
@RF00058 MVI   CP2FLGS,X'00'                                       0060 00038000
*   IF ICTPPASS(1)=ON THEN          /* CHK PASS FLAG IN ENTRY PASSED */ 00039000
         TM    ICTPPASS(PTFPTR),B'10000000'                        0061 00040000
         BNO   @RF00061                                            0061 00041000
*     LOCALCP2=ON;                  /* REMEMBER THAT CP2 PROCESSING     00042000
*                                      FLAG WAS SET UPON ENTRY       */ 00043000
         OI    LOCALCP2,B'10000000'                                0062 00044000
*   ELSE                                                           0063 00045000
*     ICTPPASS(1)=ON;               /* OTHERWISE SET CP2 PROCESSING     00046000
*                                      FLAG IN CURRENT PTF ENTRY     */ 00047000
         B     @RC00061                                            0063 00048000
@RF00061 OI    ICTPPASS(PTFPTR),B'10000000'                        0063 00049000
*                                                                  0064 00050000
*   /*****************************************************************/ 00051000
*   /*                                                               */ 00052000
*   /* IF SYSMOD NOT ALREADY COMPLETED AND ALL ELEMENTS ARE COMPLETE */ 00053000
*   /*                                                               */ 00054000
*   /*****************************************************************/ 00055000
*                                                                  0064 00056000
*   IF CP2RETRN=NOTCOMPL            /* IF REQUISITE NOT COMPLETE     */ 00057000
*       &ICTPECPL(1)=ON             /* AND ALL ELEMENTS ARE COMPLETE */ 00058000
*     THEN                          /*                               */ 00059000
@RC00061 CH    CP2RETRN,@CH00037                                   0064 00060000
         BNE   @RF00064                                            0064 00061000
         TM    ICTPECPL(PTFPTR),B'01000000'                        0064 00062000
         BNO   @RF00064                                            0064 00063000
*     DO;                           /*                               */ 00064000
*       CP2RETRN=COMPLETE;          /* RE-INITIALIZE RETURN CODE TO     00065000
*                                      REQUISITES "COMPLETE"         */ 00066000
*                                                                  0066 00067000
         SLR   CP2RETRN,CP2RETRN                                   0066 00068000
*       /*************************************************************/ 00069000
*       /*                                                           */ 00070000
*       /* APPLY OR ACCEPT PROCESSING CHECKS. ALL PRE, REQ, AND IFREQ*/ 00071000
*       /* SYSMODS MUST ALSO BE COMPLETED                            */ 00072000
*       /*                                                           */ 00073000
*       /*************************************************************/ 00074000
*                                                                  0067 00075000
*       IF(CCAAPPLY=ON|CCAACCPT=ON) /* IF APPLY OR ACCEPT            */ 00076000
*         THEN                                                     0067 00077000
         TM    CCAAPPLY(CCAPTR),B'01100000'                        0067 00078000
         BZ    @RF00067                                            0067 00079000
*         DO;                                                      0068 00080000
*           RFY                     /*                               */ 00081000
*             RTNCODE RSTD;         /*                               */ 00082000
*           CALL REQCHK(ICTPPCHN(1),PRE);/* CHECK PRES               */ 00083000
         LA    @14,ICTPPCHN(,PTFPTR)                               0070 00084000
         ST    @14,@AL00001                                        0070 00085000
         LA    @14,@CH00085+1                                      0070 00086000
         ST    @14,@AL00001+4                                      0070 00087000
         LA    @01,@AL00001                                        0070 00088000
         BAL   @14,REQCHK                                          0070 00089000
*           CP2RETRN=RTNCODE;       /*                               */ 00090000
         LR    CP2RETRN,RTNCODE                                    0071 00091000
*           IF CP2RETRN=COMPLETE    /* IF SYSMOD STILL COMPLETE      */ 00092000
*             THEN                  /*                               */ 00093000
         LTR   CP2RETRN,CP2RETRN                                   0072 00094000
         BNZ   @RF00072                                            0072 00095000
*             DO;                   /*                               */ 00096000
*               CALL REQCHK(ICTPRCHN(1),REQ);/* CHECK REQS           */ 00097000
         LA    @14,ICTPRCHN(,PTFPTR)                               0074 00098000
         ST    @14,@AL00001                                        0074 00099000
         LA    @14,@CH00182+1                                      0074 00100000
         ST    @14,@AL00001+4                                      0074 00101000
         LA    @01,@AL00001                                        0074 00102000
         BAL   @14,REQCHK                                          0074 00103000
*               CP2RETRN=RTNCODE;   /*                               */ 00104000
         LR    CP2RETRN,RTNCODE                                    0075 00105000
*             END;                  /*                               */ 00106000
*           IF CP2RETRN<NOTCOMPL    /* IF SYSMOD STILL COMPLETE      */ 00107000
*             THEN                  /*                               */ 00108000
@RF00072 CH    CP2RETRN,@CH00037                                   0077 00109000
         BNL   @RF00077                                            0077 00110000
*             DO;                   /*                               */ 00111000
*               CALL REQCHK(ICTPICHN(1),IRQ);/* CHECK IFREQS         */ 00112000
         LA    @14,ICTPICHN(,PTFPTR)                               0079 00113000
         ST    @14,@AL00001                                        0079 00114000
         LA    @14,@CH00234+1                                      0079 00115000
         ST    @14,@AL00001+4                                      0079 00116000
         LA    @01,@AL00001                                        0079 00117000
         BAL   @14,REQCHK                                          0079 00118000
*               CP2RETRN=RTNCODE;   /*                               */ 00119000
         LR    CP2RETRN,RTNCODE                                    0080 00120000
*             END;                  /*                               */ 00121000
*           RFY                     /*                               */ 00122000
*             RTNCODE UNRSTD;       /*                               */ 00123000
@RF00077 DS    0H                                                  0083 00124000
*         END;                      /* END APPLY/ACCEPT GROUP        */ 00125000
*                                                                  0083 00126000
*       /*************************************************************/ 00127000
*       /*                                                           */ 00128000
*       /* IF FUNCTION IS RESTORE THEN ALL THE SYSMODS THAT PRE, REQ,*/ 00129000
*       /* IFREQ, OR SUP THIS SYSMOD MUSTBE COMPLETE                 */ 00130000
*       /*                                                           */ 00131000
*       /*************************************************************/ 00132000
*                                                                  0084 00133000
*       ELSE                                                       0084 00134000
*         DO;                       /*                               */ 00135000
         B     @RC00067                                            0084 00136000
@RF00067 DS    0H                                                  0085 00137000
*           RFY                     /*                               */ 00138000
*             RTNCODE RSTD;         /*                               */ 00139000
*           CALL RESREQ1(PRE);      /* CHECK ALL THAT PRE CURRENT    */ 00140000
         LA    @01,@AL00086                                        0086 00141000
         BAL   @14,RESREQ1                                         0086 00142000
*           CP2RETRN=RTNCODE;       /*                               */ 00143000
         LR    CP2RETRN,RTNCODE                                    0087 00144000
*           IF CP2RETRN=COMPLETE    /* IF SYSMOD STILL COMPLETE      */ 00145000
*             THEN                  /*                               */ 00146000
         LTR   CP2RETRN,CP2RETRN                                   0088 00147000
         BNZ   @RF00088                                            0088 00148000
*             DO;                   /*                               */ 00149000
*               CALL RESREQ1(REQ);  /* CHECK ALL THAT REQ CURRENT    */ 00150000
         LA    @01,@AL00090                                        0090 00151000
         BAL   @14,RESREQ1                                         0090 00152000
*               CP2RETRN=RTNCODE;   /*                               */ 00153000
         LR    CP2RETRN,RTNCODE                                    0091 00154000
*             END;                  /*                               */ 00155000
*           IF CP2RETRN=COMPLETE    /* IF SYSMOD STILL COMPLETE      */ 00156000
*             THEN                  /*                               */ 00157000
@RF00088 LTR   CP2RETRN,CP2RETRN                                   0093 00158000
         BNZ   @RF00093                                            0093 00159000
*             DO;                   /*                               */ 00160000
*               CALL RESREQ1(IRQ);  /* CHECK ALL THAT IFREQ CURRENT  */ 00161000
         LA    @01,@AL00095                                        0095 00162000
         BAL   @14,RESREQ1                                         0095 00163000
*               CP2RETRN=RTNCODE;   /*                               */ 00164000
         LR    CP2RETRN,RTNCODE                                    0096 00165000
*             END;                  /*                               */ 00166000
*           IF CP2RETRN=COMPLETE    /* IF SYSMOD STILL COMPLETE      */ 00167000
*             THEN                  /*                               */ 00168000
@RF00093 LTR   CP2RETRN,CP2RETRN                                   0098 00169000
         BNZ   @RF00098                                            0098 00170000
*             DO;                   /*                               */ 00171000
*               CALL RESREQ1(SUP);  /* CHECK ALL THAT SUP CURRENT    */ 00172000
         LA    @01,@AL00100                                        0100 00173000
         BAL   @14,RESREQ1                                         0100 00174000
*               CP2RETRN=RTNCODE;   /*                               */ 00175000
         LR    CP2RETRN,RTNCODE                                    0101 00176000
*             END;                  /*                               */ 00177000
*           RFY                     /*                               */ 00178000
*             RTNCODE UNRSTD;       /*                               */ 00179000
@RF00098 DS    0H                                                  0104 00180000
*         END;                      /*                               */ 00181000
*     END;                          /* END NOT CPL/ELE CPL           */ 00182000
*                                                                  0105 00183000
@RC00067 DS    0H                                                  0106 00184000
*   /*****************************************************************/ 00185000
*   /*                                                               */ 00186000
*   /* IF THIS IS RECURSION NOT RECURSION PASS THRU CP2 THEN RESET   */ 00187000
*   /* ICT BIT INDICATING POSSIBLE RECURSION                         */ 00188000
*   /*                                                               */ 00189000
*   /*****************************************************************/ 00190000
*                                                                  0106 00191000
*   IF LOCALCP2=OFF                 /* RECURSION FLAG NOT SET ON   0106 00192000
*                                      ENTRY                         */ 00193000
*     THEN                                                         0106 00194000
@RF00064 TM    LOCALCP2,B'10000000'                                0106 00195000
         BNZ   @RF00106                                            0106 00196000
*     ICTPPASS(1)=OFF;              /* TURN OFF CP2 RECURSION FLAG   */ 00197000
         NI    ICTPPASS(PTFPTR),B'01111111'                        0107 00198000
*   RETURN CODE(CP2RETRN);          /* RETURN WITH APPROPRIATE CODE  */ 00199000
@RF00106 L     @13,4(,@13)                                         0108 00200000
         L     @00,@SIZDATD                                        0108 00201000
         LR    @01,@10                                             0108 00202000
         FREEMAIN R,LV=(0),A=(1)                                        00203000
         LR    @15,@02                                             0108 00204000
         L     @14,12(,@13)                                        0108 00205000
         LM    @00,@12,20(@13)                                     0108 00206000
         BR    @14                                                 0108 00207000
         EJECT                                                          00208000
*                                                                  0109 00209000
*/********************************************************************/ 00210000
*/*                                                                  */ 00211000
*/*  REQCHK   - CHECK THE REQUISITES OF A SYSMOD FOR PROCESSING      */ 00212000
*/*             COMPLETENESS                                         */ 00213000
*/*                                                                  */ 00214000
*/*     * CONDITIONS UPON EXIT                                       */ 00215000
*/*                                                                  */ 00216000
*/*           - CP2RETRN UNCHANGED IF ALL REQUISITES ARE FOUND TO    */ 00217000
*/*             BE "GROUP COMPLETE".                                 */ 00218000
*/*                                                                  */ 00219000
*/*           - CP2RETRN SET TO 4 ("NOTCOMPL") IF ANY REQUISITE IS   */ 00220000
*/*             FOUND TO BE NOT "GROUP COMPLETE".                    */ 00221000
*/*                                                                  */ 00222000
*/********************************************************************/ 00223000
*                                                                  0109 00224000
*REQCHK:                                                           0109 00225000
*   PROC(RLISTPTR,REQTYPE);                                        0109 00226000
REQCHK   STM   @14,@12,@SA00002                                    0109 00227000
         MVC   @PC00002(8),0(@01)                                  0109 00228000
*   DECLARE                                                        0110 00229000
*     RLISTPTR PTR(31);             /* POINTER TO REQUISITE LIST     */ 00230000
*   DECLARE                                                        0111 00231000
*     REQTYPE FIXED(8);             /* REQ TYPE (PRE,REQ,IRQ)        */ 00232000
*                                                                  0111 00233000
*   /*****************************************************************/ 00234000
*   /*                                                               */ 00235000
*   /* LOCAL VARIABLES                                               */ 00236000
*   /*                                                               */ 00237000
*   /*****************************************************************/ 00238000
*                                                                  0112 00239000
*   DECLARE                                                        0112 00240000
*     I FIXED(31);                  /* REQ LIST ENTRY INDEX          */ 00241000
*   DECLARE                                                        0113 00242000
*     REQPTR PTR(31);               /* PTR TO ICT PTF SECTION ENTRY     00243000
*                                      FOR REQ (IF FOUND IN ICT)     */ 00244000
*   DECLARE                                                        0114 00245000
*     SPTR PTR(31);                 /* PTR TO ICT PTF SECTION ENTRY     00246000
*                                      FOR SUPERSEDER OF REQ (IF   0114 00247000
*                                      FOUND IN ICT)                 */ 00248000
*   DECLARE                                                        0115 00249000
*     INDXSTEP FIXED(31);           /* INDEX STEP INCREMENT          */ 00250000
*                                                                  0116 00251000
*   /*****************************************************************/ 00252000
*   /*                                                               */ 00253000
*   /* SET UP ADDRESSIBILITY TO THE REQUISITE LIST PASSED            */ 00254000
*   /*                                                               */ 00255000
*   /*****************************************************************/ 00256000
*                                                                  0116 00257000
*   RFY                                                            0116 00258000
*     ICTENTLS BASED(RLISTPTR);     /* "GENERAL" REQUISISTE LIST   0116 00259000
*                                      ENTRY                         */ 00260000
*   RFY                                                            0117 00261000
*     ICTPRELS BASED(RLISTPTR);     /* PREREQ REQUISISTE LIST ENTRY  */ 00262000
*   RFY                                                            0118 00263000
*     ICTREQLS BASED(RLISTPTR);     /* REQ/MREQ REQUISISTE LIST ENTRY*/ 00264000
*   RFY                                                            0119 00265000
*     ICTIRQLS BASED(RLISTPTR);     /* IREQ REQUISITE LIST ENTRY     */ 00266000
*                                                                  0119 00267000
*   /*****************************************************************/ 00268000
*   /*                                                               */ 00269000
*   /* SET UP LOOP INDEX INCREMENT BASED UPON THE TYPE OF REQUISITE  */ 00270000
*   /* LIST ENTRY BEING DEALT WITH                                   */ 00271000
*   /*                                                               */ 00272000
*   /*****************************************************************/ 00273000
*                                                                  0120 00274000
*   IF REQTYPE=IRQ THEN                                            0120 00275000
         L     @02,@PC00002+4                                      0120 00276000
         CLI   REQTYPE(@02),3                                      0120 00277000
         BNE   @RF00120                                            0120 00278000
*     INDXSTEP=2;                   /* INDEX STEP FOR IREQ ENTRIES IS   00279000
*                                      2 ... IREQ ENTRIES ARE MADE UP   00280000
*                                      OF TWO "GENERAL" ENTRIES      */ 00281000
         LA    INDXSTEP,2                                          0121 00282000
*   ELSE                                                           0122 00283000
*     INDXSTEP=1;                   /* INDEX STEP FOR ALL OTHERS IS 1*/ 00284000
         B     @RC00120                                            0122 00285000
@RF00120 LA    INDXSTEP,1                                          0122 00286000
*                                                                  0123 00287000
*   /*****************************************************************/ 00288000
*   /*                                                               */ 00289000
*   /* GO THRU ALL REQUISITE LIST ENTRIES                            */ 00290000
*   /*                                                               */ 00291000
*   /*****************************************************************/ 00292000
*                                                                  0123 00293000
*   LOCREQC=ON;                     /* SET REQUISITES COMPLETE       */ 00294000
@RC00120 OI    LOCREQC,B'01000000'                                 0123 00295000
*   DO I=1 BY INDXSTEP WHILE ICTENTND(I)^=ICTENDIX/* NOT END         */ 00296000
*         &LOCREQC=ON;              /* REQUISITES COMPLETE           */ 00297000
         LA    I,1                                                 0124 00298000
         B     @DE00124                                            0124 00299000
@DL00124 DS    0H                                                  0125 00300000
*     LOCREQC=OFF;                  /* SET REQUISITES INCOMPLETE     */ 00301000
         NI    LOCREQC,B'10111111'                                 0125 00302000
*     IF ICTEREQT(I)=ON             /* REQ ON TARGET SYSTEM          */ 00303000
*         &ICTEREQI(I)=OFF          /* NOT REQUIRED TO BE IN ICT     */ 00304000
*       THEN                                                       0126 00305000
         LR    @05,I                                               0126 00306000
         SLA   @05,3                                               0126 00307000
         L     @15,@PC00002                                        0126 00308000
         L     @01,RLISTPTR(,@15)                                  0126 00309000
         ALR   @01,@05                                             0126 00310000
         BCTR  @01,0                                               0126 00311000
         TM    ICTEREQT-7(@01),B'00000010'                         0126 00312000
         BNO   @RF00126                                            0126 00313000
         TM    ICTEREQI-7(@01),B'00000100'                         0126 00314000
         BNZ   @RF00126                                            0126 00315000
*       LOCREQC=ON;                 /* SET REQUISITES COMPLETE       */ 00316000
         OI    LOCREQC,B'01000000'                                 0127 00317000
*     ELSE                                                         0128 00318000
*       IF ICTEREQB(I)=ON           /* REQ WAS BYPASSED              */ 00319000
*         THEN                                                     0128 00320000
         B     @RC00126                                            0128 00321000
@RF00126 LR    @05,I                                               0128 00322000
         SLA   @05,3                                               0128 00323000
         L     @15,@PC00002                                        0128 00324000
         L     @01,RLISTPTR(,@15)                                  0128 00325000
         ALR   @01,@05                                             0128 00326000
         BCTR  @01,0                                               0128 00327000
         TM    ICTEREQB-7(@01),B'00001000'                         0128 00328000
         BNO   @RF00128                                            0128 00329000
*         LOCREQC=ON;               /* SET REQUISITES COMPLETE       */ 00330000
         OI    LOCREQC,B'01000000'                                 0129 00331000
*     IF LOCREQC=OFF THEN                                          0130 00332000
*                                                                  0130 00333000
@RF00128 DS    0H                                                  0130 00334000
@RC00126 TM    LOCREQC,B'01000000'                                 0130 00335000
         BNZ   @RF00130                                            0130 00336000
*       /*************************************************************/ 00337000
*       /*                                                           */ 00338000
*       /* CHECK THE ICT FOR THIS REQ                                */ 00339000
*       /*                                                           */ 00340000
*       /*************************************************************/ 00341000
*                                                                  0131 00342000
*       CALL CKICTREQ;              /* CHECK FOR REQ IN ICT          */ 00343000
         BAL   @14,CKICTREQ                                        0131 00344000
*     IF LOCREQC=OFF THEN                                          0132 00345000
*                                                                  0132 00346000
@RF00130 TM    LOCREQC,B'01000000'                                 0132 00347000
         BNZ   @RF00132                                            0132 00348000
*       /*************************************************************/ 00349000
*       /*                                                           */ 00350000
*       /* CHECK THE ICT FOR SYSMODS WHICH SUPERSEDE THIS REQ        */ 00351000
*       /*                                                           */ 00352000
*       /*************************************************************/ 00353000
*                                                                  0133 00354000
*       CALL CKICTSUP;              /* CHECK FOR REQ SUPD IN ICT     */ 00355000
         BAL   @14,CKICTSUP                                        0133 00356000
*   END;                            /* END OF LOOP THRU ALL        0134 00357000
*                                      REQUISITES IN CURRENT REQ LIST*/ 00358000
*                                                                  0134 00359000
@RF00132 ALR   I,INDXSTEP                                          0134 00360000
@DE00124 LR    @05,I                                               0134 00361000
         SLA   @05,3                                               0134 00362000
         L     @15,@PC00002                                        0134 00363000
         L     @01,RLISTPTR(,@15)                                  0134 00364000
         AL    @01,@CF00840                                        0134 00365000
         L     @05,ICTENTND(@05,@01)                               0134 00366000
         CL    @05,ICTENDIX                                        0134 00367000
         BE    @DC00124                                            0134 00368000
         TM    LOCREQC,B'01000000'                                 0134 00369000
         BO    @DL00124                                            0134 00370000
@DC00124 DS    0H                                                  0135 00371000
*   /*****************************************************************/ 00372000
*   /*                                                               */ 00373000
*   /* ALL REQS IN THE LIST HAVE BEEN CHECKED (IF LOCREQC IS "ON")   */ 00374000
*   /*                                                               */ 00375000
*   /*****************************************************************/ 00376000
*                                                                  0135 00377000
*   IF LOCREQC=OFF THEN             /*                               */ 00378000
         TM    LOCREQC,B'01000000'                                 0135 00379000
         BNZ   @RF00135                                            0135 00380000
*     RETURN CODE(NOTCOMPL);        /* AN INCOMPLETE REQUISITE FOUND */ 00381000
         LA    @15,4                                               0136 00382000
         L     @14,@SA00002                                        0136 00383000
         LM    @00,@12,@SA00002+8                                  0136 00384000
         BR    @14                                                 0136 00385000
*   ELSE                            /*                               */ 00386000
*     RETURN CODE(COMPLETE);                                       0137 00387000
@RF00135 SLR   @15,@15                                             0137 00388000
         L     @14,@SA00002                                        0137 00389000
         LM    @00,@12,@SA00002+8                                  0137 00390000
         BR    @14                                                 0137 00391000
         EJECT                                                          00392000
*                                                                  0138 00393000
*/********************************************************************/ 00394000
*/*                                                                  */ 00395000
*/*      CKICTREQ - CHECK ICT FOR THE CURRENT REQUISITE              */ 00396000
*/*                                                                  */ 00397000
*/*         RLISTPTR - POINTS TO THE REQUISITE LIST BEING CHECKED    */ 00398000
*/*                                                                  */ 00399000
*/*         I        - INDEX (IN REQUISITE LIST) OF THE REQUISITE    */ 00400000
*/*                    TO BE CHECKED                                 */ 00401000
*/*                                                                  */ 00402000
*/*        NOTE - LOCREQC IS EXPECTED TO BE "OFF" ON ENTRY           */ 00403000
*/*                                                                  */ 00404000
*/*             - LOCREQC WILL BE SET "ON" IF THE REQUISITE IS       */ 00405000
*/*               FOUND TO BE COMPLETE.                              */ 00406000
*/*                                                                  */ 00407000
*/*        NOTE - THIS INTERNAL PROCEDURE IS PART OF REQCHK          */ 00408000
*/*                                                                  */ 00409000
*/********************************************************************/ 00410000
*                                                                  0138 00411000
*CKICTREQ:                                                         0138 00412000
*   PROC;                                                          0138 00413000
CKICTREQ STM   @14,@12,@SA00003                                    0138 00414000
*   CALL CHKICT(ICTENTNM(I),REQPTR);/* LOOK FOR REQ IN ICT           */ 00415000
         LR    @03,I                                               0139 00416000
         SLA   @03,3                                               0139 00417000
         L     @15,@PC00002                                        0139 00418000
         L     @01,RLISTPTR(,@15)                                  0139 00419000
         AL    @01,@CF00840                                        0139 00420000
         LA    @03,ICTENTNM(@03,@01)                               0139 00421000
         ST    @03,@AL00001                                        0139 00422000
         LA    @03,REQPTR                                          0139 00423000
         ST    @03,@AL00001+4                                      0139 00424000
         LA    @01,@AL00001                                        0139 00425000
         BAL   @14,CHKICT                                          0139 00426000
*   IF REQPTR^=0                    /* REQUISITE FOUND IN ICT        */ 00427000
*     THEN                                                         0140 00428000
         L     @03,REQPTR                                          0140 00429000
         LTR   @03,@03                                             0140 00430000
         BZ    @RF00140                                            0140 00431000
*     DO;                                                          0141 00432000
*                                                                  0141 00433000
*       /*************************************************************/ 00434000
*       /*                                                           */ 00435000
*       /* CHECK THE ACTUAL REQUISITE FOUND IN THE ICT               */ 00436000
*       /*                                                           */ 00437000
*       /*************************************************************/ 00438000
*                                                                  0142 00439000
*       IF REQPTR->ICTPPASS(1)=ON   /* CP2 RECURSION FLAG SET        */ 00440000
*           |REQPTR->ICTPGCPL(1)=ON /* REQ IS "GROUP" COMPLETE       */ 00441000
*         THEN                                                     0142 00442000
         TM    ICTPPASS(@03),B'10000000'                           0142 00443000
         BO    @RT00142                                            0142 00444000
         TM    ICTPGCPL(@03),B'10000000'                           0142 00445000
         BNO   @RF00142                                            0142 00446000
@RT00142 DS    0H                                                  0143 00447000
*         LOCREQC=ON;               /* SET REQ COMPLETE FLAG         */ 00448000
         OI    LOCREQC,B'01000000'                                 0143 00449000
*       ELSE                                                       0144 00450000
*         DO;                                                      0144 00451000
*                                                                  0144 00452000
         B     @RC00142                                            0144 00453000
@RF00142 DS    0H                                                  0145 00454000
*           /*********************************************************/ 00455000
*           /*                                                       */ 00456000
*           /* THE REQUISITE FOUND IN THE ICT IS NOT YET MARKED      */ 00457000
*           /* "GROUP COMPLETE" ... CHECK IT BY CALLING HMASMCP2     */ 00458000
*           /*                                                       */ 00459000
*           /*********************************************************/ 00460000
*                                                                  0145 00461000
*           CALL HMASMCP2(REQPTR->ICTPTF(1));/* CHECK IT             */ 00462000
         L     @03,REQPTR                                          0145 00463000
         ST    @03,@AL00001                                        0145 00464000
         LA    @01,@AL00001                                        0145 00465000
         L     @15,@CA00028                                        0145 00466000
         BALR  @14,@15                                             0145 00467000
*           RFY                                                    0146 00468000
*             RTNCODE RSTD;                                        0146 00469000
*           IF RTNCODE=COMPLETE THEN                               0147 00470000
         LTR   RTNCODE,RTNCODE                                     0147 00471000
         BNZ   @RF00147                                            0147 00472000
*             LOCREQC=ON;           /* SET REQUISITE "GROUP COMPL."  */ 00473000
         OI    LOCREQC,B'01000000'                                 0148 00474000
*           RFY                                                    0149 00475000
*             RTNCODE UNRSTD;                                      0149 00476000
@RF00147 DS    0H                                                  0150 00477000
*         END;                                                     0150 00478000
*     END;                          /* END OF CODE PROCESSING      0151 00479000
*                                      REQUISITE FOUND IN ICT        */ 00480000
*   RETURN;                                                        0152 00481000
@EL00003 DS    0H                                                  0152 00482000
@EF00003 DS    0H                                                  0152 00483000
@ER00003 LM    @14,@12,@SA00003                                    0152 00484000
         BR    @14                                                 0152 00485000
*   END CKICTREQ;                                                  0153 00486000
         B     @EL00003                                            0153 00487000
         EJECT                                                          00488000
*                                                                  0154 00489000
*/********************************************************************/ 00490000
*/*                                                                  */ 00491000
*/*    CKICTSUP - CHECK ICT TO SEE IF THE CURRENT REQUISITE IS SUPD  */ 00492000
*/*               ... IF SO, SEE IF THE SUPERSEDING SYSMOD IS        */ 00493000
*/*                   COMPLETE                                       */ 00494000
*/*                                                                  */ 00495000
*/*         RLISTPTR - POINTS TO THE REQUISITE LIST BEING CHECKED    */ 00496000
*/*                                                                  */ 00497000
*/*         I        - INDEX (IN REQUISITE LIST) OF THE REQUISITE    */ 00498000
*/*                    TO BE CHECKED                                 */ 00499000
*/*                                                                  */ 00500000
*/*        NOTE - LOCREQC IS EXPECTED TO BE "OFF" ON ENTRY           */ 00501000
*/*                                                                  */ 00502000
*/*             - LOCREQC WILL BE SET "ON" IF THE REQUISITE IS       */ 00503000
*/*               FOUND TO BE COMPLETE.                              */ 00504000
*/*                                                                  */ 00505000
*/*        NOTE - THIS INTERNAL PROCEDURE IS PART OF REQCHK          */ 00506000
*/*                                                                  */ 00507000
*/********************************************************************/ 00508000
*                                                                  0154 00509000
*CKICTSUP:                                                         0154 00510000
*   PROC;                                                          0154 00511000
CKICTSUP STM   @14,@12,@SA00004                                    0154 00512000
*   SPTR=0;                         /* INITIALIZE PTR SO SUPICT PROC    00513000
*                                      STARTS LOOKING FOR SUPS AT THE   00514000
*                                      BEGINNING OF THE ICT.         */ 00515000
         SLR   @15,@15                                             0155 00516000
         ST    @15,SPTR                                            0155 00517000
*   CALL SUPICT(ICTENTNM(I),SPTR);  /* CHECK ICT FOR SUPING PTF      */ 00518000
*                                                                  0156 00519000
         LR    @15,I                                               0156 00520000
         SLA   @15,3                                               0156 00521000
         L     @14,@PC00002                                        0156 00522000
         L     @01,RLISTPTR(,@14)                                  0156 00523000
         AL    @01,@CF00840                                        0156 00524000
         LA    @15,ICTENTNM(@15,@01)                               0156 00525000
         ST    @15,@AL00001                                        0156 00526000
         LA    @15,SPTR                                            0156 00527000
         ST    @15,@AL00001+4                                      0156 00528000
         LA    @01,@AL00001                                        0156 00529000
         BAL   @14,SUPICT                                          0156 00530000
*   /*****************************************************************/ 00531000
*   /*                                                               */ 00532000
*   /* EXAMINE ALL SYSMODS IN THE ICT SUPERSEDING THE REQUISITE UNTIL*/ 00533000
*   /* A "GROUP COMPLETE" ONE IS FOUND                               */ 00534000
*   /*                                                               */ 00535000
*   /*****************************************************************/ 00536000
*                                                                  0157 00537000
*   DO WHILE LOCREQC=OFF            /* SYSMOD NOT CONSIDERED COMPLETE*/ 00538000
*         &SPTR^=0;                 /* THERE IS A SUPERSEDING SYSMOD */ 00539000
*                                                                  0157 00540000
         B     @DE00157                                            0157 00541000
@DL00157 DS    0H                                                  0158 00542000
*     /***************************************************************/ 00543000
*     /*                                                             */ 00544000
*     /* CHECK THE SYSMOD SUPERSEDING THE REQUISITE (SPTR POINTS TO  */ 00545000
*     /* IT)                                                         */ 00546000
*     /*                                                             */ 00547000
*     /***************************************************************/ 00548000
*                                                                  0158 00549000
*     IF REQTYPE=IRQ                /* IFREQ REQUISITE               */ 00550000
*         &ICTENTNM(I+1)=ICTPTFS(1) /* CAUSED BY "SELF"              */ 00551000
*         &SPTR=PTFPTR              /* SUPERSEDED BY "SELF"          */ 00552000
*       THEN                                                       0158 00553000
         L     @15,@PC00002+4                                      0158 00554000
         CLI   REQTYPE(@15),3                                      0158 00555000
         BNE   @RF00158                                            0158 00556000
         LR    @15,I                                               0158 00557000
         SLA   @15,3                                               0158 00558000
         L     @14,@PC00002                                        0158 00559000
         L     @01,RLISTPTR(,@14)                                  0158 00560000
         ALR   @01,@15                                             0158 00561000
         CLC   ICTENTNM(7,@01),ICTPTFS(PTFPTR)                     0158 00562000
         BNE   @RF00158                                            0158 00563000
         C     PTFPTR,SPTR                                         0158 00564000
         BE    @RT00158                                            0158 00565000
*       ;                           /* DO NOT USE "SELF" ... THERE IS   00566000
*                                      ANOTHER SUPERSEDER IN ICT     */ 00567000
*     ELSE                                                         0160 00568000
*       DO;                                                        0160 00569000
@RF00158 DS    0H                                                  0161 00570000
*         IF SPTR->ICTPGCPL(1)=ON   /* SUPERSEDER IS "GROUP COMPLETE"*/ 00571000
*             |SPTR->ICTPPASS(1)=ON /* CP2 RECURSION FLAG SET        */ 00572000
*             |SPTR=PTFPTR          /* SUPERSEDER IS "SELF"          */ 00573000
*           THEN                                                   0161 00574000
         L     @15,SPTR                                            0161 00575000
         TM    ICTPGCPL(@15),B'10000000'                           0161 00576000
         BO    @RT00161                                            0161 00577000
         TM    ICTPPASS(@15),B'10000000'                           0161 00578000
         BO    @RT00161                                            0161 00579000
         CR    @15,PTFPTR                                          0161 00580000
         BNE   @RF00161                                            0161 00581000
@RT00161 DS    0H                                                  0162 00582000
*           LOCREQC=ON;             /* SET REQ COMPLETE              */ 00583000
         OI    LOCREQC,B'01000000'                                 0162 00584000
*         ELSE                                                     0163 00585000
*           DO;                                                    0163 00586000
*                                                                  0163 00587000
         B     @RC00161                                            0163 00588000
@RF00161 DS    0H                                                  0164 00589000
*             /*******************************************************/ 00590000
*             /*                                                     */ 00591000
*             /* SUPERSEDING SYSMOD IN THE ICT IS NOT YET MARKED     */ 00592000
*             /* "GROUP" COMPLETE ... CHECK IT BY CALLING HMASMCP2   */ 00593000
*             /*                                                     */ 00594000
*             /*******************************************************/ 00595000
*                                                                  0164 00596000
*             CALL HMASMCP2(SPTR->ICTPTF(1));                      0164 00597000
         L     @01,SPTR                                            0164 00598000
         ST    @01,@AL00001                                        0164 00599000
         LA    @01,@AL00001                                        0164 00600000
         L     @15,@CA00028                                        0164 00601000
         BALR  @14,@15                                             0164 00602000
*             RFY                                                  0165 00603000
*               RTNCODE RSTD;                                      0165 00604000
*             IF RTNCODE=COMPLETE THEN                             0166 00605000
         LTR   RTNCODE,RTNCODE                                     0166 00606000
         BNZ   @RF00166                                            0166 00607000
*               LOCREQC=ON;         /* SET REQ COMPLETE FLAG         */ 00608000
         OI    LOCREQC,B'01000000'                                 0167 00609000
*             RFY                                                  0168 00610000
*               RTNCODE UNRSTD;                                    0168 00611000
@RF00166 DS    0H                                                  0169 00612000
*           END;                                                   0169 00613000
*       END;                                                       0170 00614000
@RC00161 DS    0H                                                  0171 00615000
*     IF LOCREQC=OFF THEN                                          0171 00616000
*                                                                  0171 00617000
@RT00158 TM    LOCREQC,B'01000000'                                 0171 00618000
         BNZ   @RF00171                                            0171 00619000
*       /*************************************************************/ 00620000
*       /*                                                           */ 00621000
*       /* THE SUPERSEDER DID NOT CHECK SUCCESSFULLY. LOOK FOR       */ 00622000
*       /* ANOTHER SUPERSEDER IN THE ICT.                            */ 00623000
*       /*                                                           */ 00624000
*       /*************************************************************/ 00625000
*                                                                  0172 00626000
*       CALL SUPICT(ICTENTNM(I),SPTR);/* GET ANOTHER SUP             */ 00627000
         LR    @15,I                                               0172 00628000
         SLA   @15,3                                               0172 00629000
         L     @14,@PC00002                                        0172 00630000
         L     @01,RLISTPTR(,@14)                                  0172 00631000
         AL    @01,@CF00840                                        0172 00632000
         LA    @15,ICTENTNM(@15,@01)                               0172 00633000
         ST    @15,@AL00001                                        0172 00634000
         LA    @15,SPTR                                            0172 00635000
         ST    @15,@AL00001+4                                      0172 00636000
         LA    @01,@AL00001                                        0172 00637000
         BAL   @14,SUPICT                                          0172 00638000
*   END;                            /* END OF LOOP FOR ALL         0173 00639000
*                                      SUPERSEDING SYSMODS IN ICT    */ 00640000
@RF00171 DS    0H                                                  0173 00641000
@DE00157 TM    LOCREQC,B'01000000'                                 0173 00642000
         BNZ   @DC00157                                            0173 00643000
         L     @15,SPTR                                            0173 00644000
         LTR   @15,@15                                             0173 00645000
         BNZ   @DL00157                                            0173 00646000
@DC00157 DS    0H                                                  0174 00647000
*   RETURN;                                                        0174 00648000
@EL00004 DS    0H                                                  0174 00649000
@EF00004 DS    0H                                                  0174 00650000
@ER00004 LM    @14,@12,@SA00004                                    0174 00651000
         BR    @14                                                 0174 00652000
*   END CKICTSUP;                                                  0175 00653000
*   END REQCHK;                                                    0176 00654000
         EJECT                                                          00655000
*                                                                  0177 00656000
*/********************************************************************/ 00657000
*/*                                                                  */ 00658000
*/*      CHKICT   - CHECK THE ICT FOR A NON-NOGO, NON-DELETED, NON-  */ 00659000
*/*                 SUPERSEDED ICT PTF SECTION ENTRY REPRESENTING    */ 00660000
*/*                 THE SYSMOD NAME PASSED BY THE CALLER.            */ 00661000
*/*                                                                  */ 00662000
*/*               - IF SUCH AN ENTRY IS FOUND, RETURN A POINTER TO   */ 00663000
*/*                 IT.                                              */ 00664000
*/*                                                                  */ 00665000
*/*                 IF SUCH AN ENTRY IS NOT FOUND, RETURN THE POINTER*/ 00666000
*/*                 AS ZERO.                                         */ 00667000
*/*                                                                  */ 00668000
*/********************************************************************/ 00669000
*                                                                  0177 00670000
*CHKICT:                                                           0177 00671000
*   PROC(SYSMNAME,SRCHPTR);                                        0177 00672000
CHKICT   STM   @14,@12,12(@13)                                     0177 00673000
         MVC   @PC00005(8),0(@01)                                  0177 00674000
*   DECLARE                                                        0178 00675000
*     SYSMNAME CHAR(7);             /* SYSMOD TO BE FOUND IN ICT     */ 00676000
*   DECLARE                                                        0179 00677000
*     SRCHPTR PTR(31);              /* RETURNS PTR TO SYSMOD FOUND   */ 00678000
*   DECLARE                                                        0180 00679000
*     P2PTR PTR(31);                /* PTR FOR ICT PTF LOOP          */ 00680000
*   RFY                                                            0181 00681000
*     ICTPTF BASED(P2PTR);          /* PTF BASED ON PTR              */ 00682000
*   SRCHPTR=0;                      /* INITIALIZE TO ZERO WHICH    0182 00683000
*                                      IMPLIES "NOT FOUND"           */ 00684000
*                                                                  0182 00685000
         L     @15,@PC00005+4                                      0182 00686000
         SLR   @14,@14                                             0182 00687000
         ST    @14,SRCHPTR(,@15)                                   0182 00688000
*   /*****************************************************************/ 00689000
*   /*                                                               */ 00690000
*   /* GO THRU THE ICT PTF SECTION ENTIRES LOOKING FOR "SYSMNAME"    */ 00691000
*   /*                                                               */ 00692000
*   /*****************************************************************/ 00693000
*                                                                  0183 00694000
*   DO P2PTR=CCAICPTF BY LENGTH(ICTPTF(1)) WHILE(/* ICT PTF LOOP     */ 00695000
*         ICTPEND(1)^=ICTENDNT      /* TILL END                      */ 00696000
*         &SRCHPTR=0);              /* AND SYSMOD NOT FOUND          */ 00697000
         L     P2PTR,CCAICPTF(,CCAPTR)                             0183 00698000
         B     @DE00183                                            0183 00699000
@DL00183 DS    0H                                                  0184 00700000
*     IF ICTPTFS(1)=SYSMNAME        /* IF EQUAL SYSMOD FOUND         */ 00701000
*         &ICTPNOGO(1)=OFF          /* NOT NOGO                      */ 00702000
*         &ICTPDEXP(1)=OFF          /* NOT EXPLICITLY DELETED        */ 00703000
*         &ICTPDIMP(1)=OFF          /* NOT IMPLICITLY DELETED        */ 00704000
*         &ICTPSUPN(1)=0            /* NOT SUPERSEDED                */ 00705000
*       THEN                                                       0184 00706000
         L     @03,@PC00005                                        0184 00707000
         CLC   ICTPTFS(7,P2PTR),SYSMNAME(@03)                      0184 00708000
         BNE   @RF00184                                            0184 00709000
         TM    ICTPNOGO(P2PTR),B'01000000'                         0184 00710000
         BNZ   @RF00184                                            0184 00711000
         TM    ICTPDEXP(P2PTR),B'00110000'                         0184 00712000
         BNZ   @RF00184                                            0184 00713000
         MVC   @ZT00002+2(2),ICTPSUPN(P2PTR)                       0184 00714000
         L     @03,@ZT00002                                        0184 00715000
         LTR   @03,@03                                             0184 00716000
         BNZ   @RF00184                                            0184 00717000
*       SRCHPTR=P2PTR;              /* RETURN POINTER TO CALLER      */ 00718000
         L     @03,@PC00005+4                                      0185 00719000
         ST    P2PTR,SRCHPTR(,@03)                                 0185 00720000
*   END;                                                           0186 00721000
@RF00184 AH    P2PTR,@CH00832                                      0186 00722000
@DE00183 CLC   ICTPEND(4,P2PTR),ICTENDNT                           0186 00723000
         BE    @DC00183                                            0186 00724000
         L     @03,@PC00005+4                                      0186 00725000
         L     @03,SRCHPTR(,@03)                                   0186 00726000
         LTR   @03,@03                                             0186 00727000
         BZ    @DL00183                                            0186 00728000
@DC00183 DS    0H                                                  0187 00729000
*   RETURN;                                                        0187 00730000
@EL00005 DS    0H                                                  0187 00731000
@EF00005 DS    0H                                                  0187 00732000
@ER00005 LM    @14,@12,12(@13)                                     0187 00733000
         BR    @14                                                 0187 00734000
*   END CHKICT;                                                    0188 00735000
         EJECT                                                          00736000
*                                                                  0189 00737000
*/********************************************************************/ 00738000
*/*                                                                  */ 00739000
*/*      SUPICT   - CHECK THE ICT FOR A NON-NOGO, NON-DELETED, NON-  */ 00740000
*/*                 SUPERSEDED SYSMOD WHICH SUPERSEDES THE SYSMOD    */ 00741000
*/*                 PASSED BY THE CALLER.                            */ 00742000
*/*                                                                  */ 00743000
*/*               - THE SERACH THRU THE ICT PTF SECTION IS STARTED   */ 00744000
*/*                 FROM THE PTF SECTION ENTRY AFTER THE ENTRY       */ 00745000
*/*                 POINTED TO BY THE PARAMETER, "SUPPTR".  IF       */ 00746000
*/*                 "SUPPTR" IS ZERO UPON ENTRY, THE SEARCH WILL     */ 00747000
*/*                 START AT THE BEGINNING OF THE ICT PTF SECTION.   */ 00748000
*/*                                                                  */ 00749000
*/*               - ON EXIT, THE PARAMETER "SUPPTR" WILL POINT TO    */ 00750000
*/*                 THE NEXT SYSMOD FOUND IN THE ICT WHICH SUPERSEDES*/ 00751000
*/*                 THE SYSMOD PASSED.  IF "SUPPTR" IS ZERO ON EXIT, */ 00752000
*/*                 NO OTHER SYSMOD WAS FOUND IN THE ICT WHICH       */ 00753000
*/*                 SUPERSEDED THE SYSMOD PASSED.                    */ 00754000
*/*                                                                  */ 00755000
*/********************************************************************/ 00756000
*                                                                  0189 00757000
*SUPICT:                                                           0189 00758000
*   PROC(SUPNAME,SUPPTR);                                          0189 00759000
SUPICT   STM   @14,@12,12(@13)                                     0189 00760000
         MVC   @PC00006(8),0(@01)                                  0189 00761000
*   DECLARE                                                        0190 00762000
*     SUPNAME CHAR(7);              /* SYSMOD TO BE FOUND AS       0190 00763000
*                                      SUPERSEDED IN ICT             */ 00764000
*   DECLARE                                                        0191 00765000
*     SUPPTR PTR(31);               /* UPON ENTRY THIS PARAMETER IS     00766000
*                                      EITHER 0 OR POINTS TO AN ICT     00767000
*                                      PTF SECTION ENTRY. .........     00768000
*                                      UPON EXIT THIS PARAMETER IS 0191 00769000
*                                      SET TO EITHER 0 OR POINTS TO     00770000
*                                      THE ICT PTF SECTION ENTRY FOR    00771000
*                                      A SYSMOD WHICH SUPERSEDES   0191 00772000
*                                      "SUPNAME".                    */ 00773000
*   DECLARE                                                        0192 00774000
*     P3PTR PTR(31);                /* PTR FOR ICT PTF LOOP          */ 00775000
*   DECLARE                                                        0193 00776000
*     UPTR PTR(31);                 /* PTR TO SUPERSEDING SYSMOD     */ 00777000
*   DECLARE                                                        0194 00778000
*     L FIXED(31);                  /* INDEX - SUP LIST ENTRIES      */ 00779000
*   RFY                                                            0195 00780000
*     ICTPTF BASED(SUPPTR);         /* ADDRESS PTF SECT ENTRY PASSED */ 00781000
*   UPTR=0;                         /* SUPERSEDING SYSMOD NOT FOUND  */ 00782000
         SLR   UPTR,UPTR                                           0196 00783000
*   IF SUPPTR=0 THEN                                               0197 00784000
         L     @02,@PC00006+4                                      0197 00785000
         L     @15,SUPPTR(,@02)                                    0197 00786000
         LTR   @15,@15                                             0197 00787000
         BNZ   @RF00197                                            0197 00788000
*     SUPPTR=CCAICPTF;              /* START WITH THE FIRST PTF    0198 00789000
*                                      SECTION ENTRY.                */ 00790000
         MVC   SUPPTR(4,@02),CCAICPTF(CCAPTR)                      0198 00791000
*   ELSE                                                           0199 00792000
*     SUPPTR=ADDR(ICTPTF(2));       /* START WITH THE NEXT PTF     0199 00793000
*                                      SECTION ENTRY.                */ 00794000
         B     @RC00197                                            0199 00795000
@RF00197 L     @02,@PC00006+4                                      0199 00796000
         L     @01,SUPPTR(,@02)                                    0199 00797000
         LA    @15,ICTPTF+116(,@01)                                0199 00798000
         ST    @15,SUPPTR(,@02)                                    0199 00799000
*                                                                  0200 00800000
*   /*****************************************************************/ 00801000
*   /*                                                               */ 00802000
*   /* GO THRU ALL REMAINING PTF SECTION ENTRIES                     */ 00803000
*   /*                                                               */ 00804000
*   /*****************************************************************/ 00805000
*                                                                  0200 00806000
*   RFY                             /*                               */ 00807000
*     ICTPTF BASED(P3PTR);          /* NEW ICT PTF BASED             */ 00808000
@RC00197 DS    0H                                                  0201 00809000
*   DO P3PTR=SUPPTR BY LENGTH(ICTPTF(1)) WHILE(/* LOOP THRU REMAINING   00810000
*                                      ICT PTFS                      */ 00811000
*         ICTPEND(1)^=ICTENDNT      /* TILL END                      */ 00812000
*         &UPTR=0);                 /* OR SUPING SYSMOD FOUND        */ 00813000
         L     @15,@PC00006+4                                      0201 00814000
         L     P3PTR,SUPPTR(,@15)                                  0201 00815000
         B     @DE00201                                            0201 00816000
@DL00201 DS    0H                                                  0202 00817000
*     IF ICTPNOGO(1)=OFF            /* NOT NOGO                      */ 00818000
*         &ICTPDEXP(1)=OFF          /* NOT EXPLICITLY DELETED        */ 00819000
*         &ICTPDIMP(1)=OFF          /* NOT IMPLICITLY DELETED        */ 00820000
*         &ICTPSUPN(1)=0            /* NOT SUPERSEDED                */ 00821000
*       THEN                                                       0202 00822000
         TM    ICTPNOGO(P3PTR),B'01000000'                         0202 00823000
         BNZ   @RF00202                                            0202 00824000
         TM    ICTPDEXP(P3PTR),B'00110000'                         0202 00825000
         BNZ   @RF00202                                            0202 00826000
         MVC   @ZT00002+2(2),ICTPSUPN(P3PTR)                       0202 00827000
         L     @15,@ZT00002                                        0202 00828000
         LTR   @15,@15                                             0202 00829000
         BNZ   @RF00202                                            0202 00830000
*       DO;                                                        0203 00831000
*                                                                  0203 00832000
*         /***********************************************************/ 00833000
*         /*                                                         */ 00834000
*         /* GO THRU THE LIST OF SUPERSEDED SYSMODS IN THE CURRENT   */ 00835000
*         /* PTF SECTION ENTRY                                       */ 00836000
*         /*                                                         */ 00837000
*         /***********************************************************/ 00838000
*                                                                  0204 00839000
*         DO L=1 BY 1 WHILE ICTPSCHN(1)->ICTSUPND(L)^=ICTENDIX&UPTR=0;  00840000
         LA    L,1                                                 0204 00841000
         B     @DE00204                                            0204 00842000
@DL00204 DS    0H                                                  0205 00843000
*                                   /* SUPERSEDING SYSMOD NOT FOUND  */ 00844000
*           IF ICTPSCHN(1)->ICTSUPNM(L)=SUPNAME THEN               0205 00845000
         LR    @05,L                                               0205 00846000
         SLA   @05,3                                               0205 00847000
         MVC   @TF00001(4),ICTPSCHN(P3PTR)                         0205 00848000
         L     @01,@TF00001                                        0205 00849000
         L     @15,@PC00006                                        0205 00850000
         ALR   @01,@05                                             0205 00851000
         AL    @01,@CF00840                                        0205 00852000
         CLC   ICTSUPNM(7,@01),SUPNAME(@15)                        0205 00853000
         BNE   @RF00205                                            0205 00854000
*             UPTR=ADDR(ICTPTF(1)); /* POINT TO SUPERSEDER           */ 00855000
         LR    UPTR,P3PTR                                          0206 00856000
*         END;                                                     0207 00857000
@RF00205 AH    L,@CH00085                                          0207 00858000
@DE00204 LR    @05,L                                               0207 00859000
         SLA   @05,3                                               0207 00860000
         MVC   @TF00001(4),ICTPSCHN(P3PTR)                         0207 00861000
         L     @01,@TF00001                                        0207 00862000
         AL    @01,@CF00840                                        0207 00863000
         L     @05,ICTSUPND(@05,@01)                               0207 00864000
         CL    @05,ICTENDIX                                        0207 00865000
         BE    @DC00204                                            0207 00866000
         LTR   UPTR,UPTR                                           0207 00867000
         BZ    @DL00204                                            0207 00868000
@DC00204 DS    0H                                                  0208 00869000
*       END;                                                       0208 00870000
*   END;                                                           0209 00871000
@RF00202 AH    P3PTR,@CH00832                                      0209 00872000
@DE00201 CLC   ICTPEND(4,P3PTR),ICTENDNT                           0209 00873000
         BE    @DC00201                                            0209 00874000
         LTR   UPTR,UPTR                                           0209 00875000
         BZ    @DL00201                                            0209 00876000
@DC00201 DS    0H                                                  0210 00877000
*   SUPPTR=UPTR;                    /* RETURN POINTER TO CALLER      */ 00878000
         L     @02,@PC00006+4                                      0210 00879000
         ST    UPTR,SUPPTR(,@02)                                   0210 00880000
*   RETURN;                                                        0211 00881000
@EL00006 DS    0H                                                  0211 00882000
@EF00006 DS    0H                                                  0211 00883000
@ER00006 LM    @14,@12,12(@13)                                     0211 00884000
         BR    @14                                                 0211 00885000
*   END SUPICT;                                                    0212 00886000
         EJECT                                                          00887000
*                                                                  0213 00888000
*   /*****************************************************************/ 00889000
*   /*                                                               */ 00890000
*   /* THIS ROUTINE IS CALLED FOR THE RESTORE FUNCTION TO INSURE ALL */ 00891000
*   /* SYSMODS THAT PRE, REQ, IFREQ, OR SUP THE PASSED SYSMOD ARE    */ 00892000
*   /* COMPLETE. IF SO THEN PASSED SYSMOD IS COMPLETE ELSE PASSED    */ 00893000
*   /* SYSMOD IS NOT COMPLETE                                        */ 00894000
*   /*                                                               */ 00895000
*   /*****************************************************************/ 00896000
*                                                                  0213 00897000
*RESREQ1:                           /*                               */ 00898000
*   PROCEDURE(RESTYPE);             /*                               */ 00899000
RESREQ1  STM   @14,@12,@SA00007                                    0213 00900000
         MVC   @PC00007(4),0(@01)                                  0213 00901000
*   DECLARE                         /*                               */ 00902000
*     RESTYPE FIXED(8);             /*                               */ 00903000
*   DECLARE                         /*                               */ 00904000
*     RESRC FIXED(8);               /*                               */ 00905000
*   DECLARE                         /*                               */ 00906000
*     RESINCR FIXED(31);            /* INCR THRU VARIABLE LIST       */ 00907000
*   DECLARE                         /*                               */ 00908000
*     RESX FIXED(31);               /* INDEX THRU VARIABLE LISTS     */ 00909000
*   DECLARE                         /*                               */ 00910000
*     P1PTR PTR(31);                /* PTR FOR ICT PTF LOOP          */ 00911000
*   DECLARE                         /*                               */ 00912000
*     RESVRPTR PTR(31);             /* BASE FOR LISTS                */ 00913000
*   RFY                             /*                               */ 00914000
*     ICTPTF BASED(P1PTR);          /* BASE FOR ICT PTFS             */ 00915000
*   RFY                             /*                               */ 00916000
*     ICTENTLS BASED(RESVRPTR);     /*                               */ 00917000
*   RESRC=COMPLETE;                 /*                               */ 00918000
*                                                                  0222 00919000
         SLR   RESRC,RESRC                                         0222 00920000
*   /*****************************************************************/ 00921000
*   /*                                                               */ 00922000
*   /* LOOP THRU ALL ICT PTFS UNTIL END OF ICT OR TILL SYSMOD THAT IS*/ 00923000
*   /* RELATED TO CURRENT IS FOUND THAT IS NOT COMPLETE              */ 00924000
*   /*                                                               */ 00925000
*   /*****************************************************************/ 00926000
*                                                                  0223 00927000
*   DO P1PTR=CCAICPTF BY LENGTH(ICTPTF(1)) WHILE(/* ICT PTF LOOP     */ 00928000
*         ICTPEND(1)^=ICTENDNT      /* TILL END                      */ 00929000
*         &RESRC=COMPLETE);         /* AND CURRENT SYSMOD COMPLETE   */ 00930000
*                                                                  0223 00931000
         L     P1PTR,CCAICPTF(,CCAPTR)                             0223 00932000
         B     @DE00223                                            0223 00933000
@DL00223 DS    0H                                                  0224 00934000
*     /***************************************************************/ 00935000
*     /*                                                             */ 00936000
*     /* DETERMINE CORRECT LIST TO LOOP THRU BASED ON TYPE PASSED    */ 00937000
*     /*                                                             */ 00938000
*     /***************************************************************/ 00939000
*                                                                  0224 00940000
*     RESINCR=1;                    /* DEFAULT INCR IS 1             */ 00941000
         LA    RESINCR,1                                           0224 00942000
*     IF RESTYPE=PRE                /*                               */ 00943000
*       THEN                        /*                               */ 00944000
         L     @07,@PC00007                                        0225 00945000
         CLI   RESTYPE(@07),1                                      0225 00946000
         BNE   @RF00225                                            0225 00947000
*       RESVRPTR=ICTPPCHN(1);       /* ADDR OF PRE LIST              */ 00948000
         MVC   @TF00001(4),ICTPPCHN(P1PTR)                         0226 00949000
         L     RESVRPTR,@TF00001                                   0226 00950000
*     ELSE                          /*                               */ 00951000
*       IF RESTYPE=REQ              /*                               */ 00952000
*         THEN                      /*                               */ 00953000
         B     @RC00225                                            0227 00954000
@RF00225 L     @07,@PC00007                                        0227 00955000
         CLI   RESTYPE(@07),2                                      0227 00956000
         BNE   @RF00227                                            0227 00957000
*         RESVRPTR=ICTPRCHN(1);     /* ADDR OF REQ LIST              */ 00958000
         MVC   @TF00001(4),ICTPRCHN(P1PTR)                         0228 00959000
         L     RESVRPTR,@TF00001                                   0228 00960000
*       ELSE                        /*                               */ 00961000
*         IF RESTYPE=IRQ            /*                               */ 00962000
*           THEN                    /*                               */ 00963000
         B     @RC00227                                            0229 00964000
@RF00227 L     @07,@PC00007                                        0229 00965000
         CLI   RESTYPE(@07),3                                      0229 00966000
         BNE   @RF00229                                            0229 00967000
*           DO;                     /*                               */ 00968000
*             RESVRPTR=ICTPICHN(1); /* ADDR OF IFREQ LIST            */ 00969000
         MVC   @TF00001(4),ICTPICHN(P1PTR)                         0231 00970000
         L     RESVRPTR,@TF00001                                   0231 00971000
*             RESINCR=2;            /* INCR FOR LOOP                 */ 00972000
         LA    RESINCR,2                                           0232 00973000
*           END;                    /*                               */ 00974000
*         ELSE                      /*                               */ 00975000
*           RESVRPTR=ICTPSCHN(1);   /* ADDR OF SUP LIST              */ 00976000
*                                                                  0234 00977000
         B     @RC00229                                            0234 00978000
@RF00229 MVC   @TF00001(4),ICTPSCHN(P1PTR)                         0234 00979000
         L     RESVRPTR,@TF00001                                   0234 00980000
*     /***************************************************************/ 00981000
*     /*                                                             */ 00982000
*     /* NOW LOOP THRU APPROPRIATE LIST TO SEE IF ELEMENT IN LIST IS */ 00983000
*     /* EQUAL TO CURRENT SYSMOD                                     */ 00984000
*     /*                                                             */ 00985000
*     /***************************************************************/ 00986000
*                                                                  0235 00987000
*     DO RESX=1 BY RESINCR WHILE(   /* LOOP THRU LIST                */ 00988000
*           ICTENTND(RESX)^=ICTENDIX/* TILL END                      */ 00989000
*           &RESRC=COMPLETE);       /* AND CURRENT STILL COMPLETE    */ 00990000
*                                                                  0235 00991000
@RC00229 DS    0H                                                  0235 00992000
@RC00227 DS    0H                                                  0235 00993000
@RC00225 LA    RESX,1                                              0235 00994000
         B     @DE00235                                            0235 00995000
@DL00235 DS    0H                                                  0236 00996000
*       /*************************************************************/ 00997000
*       /*                                                           */ 00998000
*       /* IF ENTRY IN LIST IS EQUAL TO THE SYSMOD PASSED THEN CHECK */ 00999000
*       /* TO SEE THIS SYSMOD IS COMPLETE. IF NOT COMPLETE THEN SET  */ 01000000
*       /* RETURN CODE TO STOP LOOP AND STOP PROCESS FOR SYSMOD      */ 01001000
*       /* PASSED                                                    */ 01002000
*       /*                                                           */ 01003000
*       /*************************************************************/ 01004000
*                                                                  0236 01005000
*       IF ICTENTNM(RESX)=PTFPTR->ICTPTFS(1)/* IF CURRENT RELATED    */ 01006000
*           &ICTPPASS(1)=OFF        /* AND FIRST LOOK AT THIS SYSMOD */ 01007000
*         THEN                      /*                               */ 01008000
         LR    @15,RESX                                            0236 01009000
         SLA   @15,3                                               0236 01010000
         ALR   @15,RESVRPTR                                        0236 01011000
         AL    @15,@CF00840                                        0236 01012000
         CLC   ICTENTNM(7,@15),ICTPTFS(PTFPTR)                     0236 01013000
         BNE   @RF00236                                            0236 01014000
         TM    ICTPPASS(P1PTR),B'10000000'                         0236 01015000
         BNZ   @RF00236                                            0236 01016000
*         DO;                       /*                               */ 01017000
*           CALL HMASMCP2(ICTPTF(1));/* CHECK THIS PTF FOR COMPLETE  */ 01018000
         ST    P1PTR,@AL00001                                      0238 01019000
         LA    @01,@AL00001                                        0238 01020000
         L     @15,@CA00028                                        0238 01021000
         BALR  @14,@15                                             0238 01022000
*           RFY                     /*                               */ 01023000
*             RTNCODE RSTD;         /*                               */ 01024000
*           IF RTNCODE^=ZERO        /* IF NOT COMPLETE               */ 01025000
*             THEN                  /* THEN                          */ 01026000
         LTR   RTNCODE,RTNCODE                                     0240 01027000
         BZ    @RF00240                                            0240 01028000
*             RESRC=NOTCOMPL;       /* CURRENT SYSMOD NOT COMPLETE   */ 01029000
         LA    RESRC,4                                             0241 01030000
*           RFY                     /*                               */ 01031000
*             RTNCODE UNRSTD;       /*                               */ 01032000
@RF00240 DS    0H                                                  0243 01033000
*         END;                      /* END SYSMOD RELATED            */ 01034000
*     END;                          /* END LOOP THRU LIST            */ 01035000
@RF00236 ALR   RESX,RESINCR                                        0244 01036000
@DE00235 LR    @15,RESX                                            0244 01037000
         SLA   @15,3                                               0244 01038000
         AL    @15,@CF00840                                        0244 01039000
         L     @15,ICTENTND(@15,RESVRPTR)                          0244 01040000
         CL    @15,ICTENDIX                                        0244 01041000
         BE    @DC00235                                            0244 01042000
         CH    RESRC,@CH00251                                      0244 01043000
         BE    @DL00235                                            0244 01044000
@DC00235 DS    0H                                                  0245 01045000
*   END;                            /* END ICT PTF LOOP              */ 01046000
         AH    P1PTR,@CH00832                                      0245 01047000
@DE00223 CLC   ICTPEND(4,P1PTR),ICTENDNT                           0245 01048000
         BE    @DC00223                                            0245 01049000
         CH    RESRC,@CH00251                                      0245 01050000
         BE    @DL00223                                            0245 01051000
@DC00223 DS    0H                                                  0246 01052000
*   RETURN CODE(RESRC);             /*                               */ 01053000
         LR    @15,RESRC                                           0246 01054000
         L     @14,@SA00007                                        0246 01055000
         LM    @00,@12,@SA00007+8                                  0246 01056000
         BR    @14                                                 0246 01057000
*   END RESREQ1;                    /*                               */ 01058000
*   END HMASMCP2                                                   0248 01059000
