*   IOPPTR=CCAIOPTR;                /* SET ADDRESS OF THE IOP        */ 00018000
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0167 00019000
*   SCPPTR=ADDR(CPYSCP);            /* LOCAL SCP             @Z67SP00*/ 00020000
         LA    @10,CPYSCP                                          0168 00021000
         ST    @10,SCPPTR                                          0168 00022000
*   HMASMSCP=CRPSCPAD->HMASMSCP;    /* COPY PASSED SCP       @Z67SP00*/ 00023000
