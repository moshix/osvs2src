         ST    @08,SCPCHAR(,@10)                                   0172 00034000
*   SCPIORTN=ZERO;                  /* ZERO THE I/O ROUTINE ADDR     */ 00035000
         SLR   @08,@08                                             0173 00036000
         ST    @08,SCPIORTN(,@10)                                  0173 00037000
*   SCPINLN=LRECL;                  /* SET RECORD LENGTH             */ 00038000
         MVC   SCPINLN(2,@10),@CH00117                             0174 00039000
*   SCPEOR=ZERO;                    /* ZERO THE FLAGS        @YA07506*/ 00040000
         MVI   SCPEOR(@10),X'00'                                   0175 00041000
*   BUPFUNCT=BUPABKUP;              /* ADD SCDS BACKUP ENTRIES     0176 00042000
*                                                            @Z67SP00*/ 00043000
