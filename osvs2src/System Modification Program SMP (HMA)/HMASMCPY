         BNE   @RF00195                                            0195 00120000
         CLI   CRPINDS(@10),6                                      0195 00121000
         BNE   @RF00195                                            0195 00122000
@RT00195 DS    0H                                                  0196 00123000
*     EOFSW=ON;                     /* REMEMBER IT           @YA07506*/ 00124000
         OI    EOFSW,B'00001000'                                   0196 00125000
*   IF CRPRETRN>CRPEOF              /* I/O ERROR?            @Z67SP00*/ 00126000
*     THEN                          /*                       @SA72421*/ 00127000
@RF00195 L     @10,@PC00001+4                                      0197 00128000
