         L     @15,@CV01340                                        0194 00109000
         LA    @01,@AL00001                                        0194 00110000
         BALR  @14,@15                                             0194 00111000
*   IF CRPRETRN=CRPEOF|             /* EOF ON READ?          @Z67SP00*/ 00112000
*       (CRPBUFFR(1:2)=PLUSPLUS&    /* IS COL 1:2 = ++ FOR   @Z67SP00*/ 00113000
*       CRPINDS=IOPPTS)             /* INPUT DATASET OF PTS? @Z67SP00*/ 00114000
*     THEN                          /* YES                   @YA07506*/ 00115000
