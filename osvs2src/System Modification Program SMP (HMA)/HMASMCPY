         ST    @10,BUFADSAV                                        0178 00051000
*   SWITCHES=ZERO;                  /* INIT SWITCHES         @YA07506*/ 00052000
         MVI   SWITCHES,X'00'                                      0179 00053000
*COPYSC10:                                                         0180 00054000
*   CALL HMASMSCN(HMASMSCP);        /* SCAN IEBCOPY STMT             */ 00055000
COPYSC10 L     @10,SCPPTR                                          0180 00056000
         ST    @10,@AL00001                                        0180 00057000
         L     @15,@CV01337                                        0180 00058000
         LA    @01,@AL00001                                        0180 00059000
         BALR  @14,@15                                             0180 00060000
*   OUTDDSW=OFF;                    /* RESET COPY OUTDD SWITCH     0181 00061000
*                                                            @ZA01999*/ 00062000
*   INDDSW=OFF;                     /* AND COPY INDD SWITCH  @ZA01999*/ 00063000
         NI    OUTDDSW,B'10011111'                                 0182 00064000
*   IF SCPRET^=SCPGOOD              /* RETURN GOOD?          @YA07506*/ 00065000
*     THEN                          /* NO - CHECK TYPE ERR   @YA05690*/ 00066000
         L     @10,SCPPTR                                          0183 00067000
         CLI   SCPRET(@10),0                                       0183 00068000
         BE    @RF00183                                            0183 00069000
*     IF EXCERR=ON                  /* IOP EXCEEDED?         @YA05690*/ 00070000
*       THEN                        /* YES                   @YA05690*/ 00071000
         TM    EXCERR,B'00010000'                                  0184 00072000
         BNO   @RF00184                                            0184 00073000
*       RETURN CODE(IOPEXC);        /* SET RETURN CODE       @YA05690*/ 00074000
         LA    @15,8                                               0185 00075000
         L     @13,4(,@13)                                         0185 00076000
         L     @14,12(,@13)                                        0185 00077000
         LM    @00,@12,20(@13)                                     0185 00078000
         BR    @14                                                 0185 00079000
*     ELSE                          /* MUST BE SYNTAX        @YA05690*/ 00080000
*       IF SCPRET>IOPNTCLS          /* I/O ERR?              @YA07506*/ 00081000
*         THEN                      /* YES                   @YA07506*/ 00082000
@RF00184 L     @10,SCPPTR                                          0186 00083000
         CLI   SCPRET(@10),12                                      0186 00084000
         BH    @RT00186                                            0186 00085000
*         GO TO IOOR;               /*                       @YA07506*/ 00086000
*       ELSE                        /*                       @YA07506*/ 00087000
*         DO;                       /*                       @SA72421*/ 00088000
*           LPTR=ADDR(CRPBUFFR);    /* POINT TO LINE IN ERR  @Z67SP00*/ 00089000
