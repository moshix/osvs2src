         TITLE 'HMASMCRD CONTROL CARD READ ROUTINE'                     00001000
HMASMCRD CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMCRD  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(4),0(@01)                                  0001 00017000
         EJECT                                                          00018000
*                                                                  0152 00019000
*   /*****************************************************************/ 00020000
*   /*                                                               */ 00021000
*   /* MAIN MODULE SEGMENT                                           */ 00022000
*   /*                                                               */ 00023000
*   /*****************************************************************/ 00024000
*                                                                  0152 00025000
*   CRDRTNCD=ZERO;                  /* INIT RETURN CODE              */ 00026000
         SLR   CRDRTNCD,CRDRTNCD                                   0152 00027000
*   IOPPTR=ADDR(CRDIOP);            /* INIT PTR TO LOCAL IOP         */ 00028000
         LA    IOPPTR,CRDIOP                                       0153 00029000
*   SCPPTR=CRPSCPAD;                /* PTR TO SCP                    */ 00030000
         L     @10,@PC00001                                        0154 00031000
         L     SCPPTR,CRPSCPAD(,@10)                               0154 00032000
*   RFY                                                            0155 00033000
*     HMASMSCP BASED(SCPPTR);       /*                               */ 00034000
*                                                                  0155 00035000
*   /*****************************************************************/ 00036000
*   /*                                                               */ 00037000
*   /* READ RECORD INTO BUFFER                                       */ 00038000
*   /*                                                               */ 00039000
*   /*****************************************************************/ 00040000
*                                                                  0156 00041000
*   IF CRDRTNCD=ZERO                /* IF PREV FUNCTIONS OK          */ 00042000
*     THEN                          /*                               */ 00043000
         CH    CRDRTNCD,@CH00092                                   0156 00044000
         BNE   @RF00156                                            0156 00045000
*     DO;                           /*                               */ 00046000
*       CALL CRDGET;                /* GET NEXT RCD INTO BUFFER      */ 00047000
         BAL   @14,CRDGET                                          0158 00048000
*       RFY                                                        0159 00049000
*         RTNCODE RSTD;             /*                               */ 00050000
*       CRDRTNCD=MAX(CRDRTNCD,RTNCODE);/* SAVE RETURN CODE           */ 00051000
         CR    CRDRTNCD,RTNCODE                                    0160 00052000
         BNL   *+6                                                      00053000
         LR    CRDRTNCD,RTNCODE                                    0160 00054000
*       RFY                                                        0161 00055000
*         RTNCODE UNRSTD;           /*                               */ 00056000
*     END;                          /*                               */ 00057000
*                                                                  0162 00058000
*   /*****************************************************************/ 00059000
*   /*                                                               */ 00060000
*   /* CALL USER EXIT ROUTINE                                        */ 00061000
*   /*                                                               */ 00062000
*   /*****************************************************************/ 00063000
*                                                                  0163 00064000
*   IF CRDRTNCD=ZERO                /* IF PREV FUNCTIONS OK          */ 00065000
*     THEN                          /*                               */ 00066000
@RF00156 CH    CRDRTNCD,@CH00092                                   0163 00067000
         BNE   @RF00163                                            0163 00068000
*     DO;                           /*                               */ 00069000
*       CALL CRDEXIT;               /* GET NEXT RCD INTO BUFFER      */ 00070000
         BAL   @14,CRDEXIT                                         0165 00071000
*       RFY                                                        0166 00072000
*         RTNCODE RSTD;             /*                               */ 00073000
*       CRDRTNCD=MAX(CRDRTNCD,RTNCODE);/* SAVE RETURN CODE           */ 00074000
         CR    CRDRTNCD,RTNCODE                                    0167 00075000
         BNL   *+6                                                      00076000
         LR    CRDRTNCD,RTNCODE                                    0167 00077000
*       RFY                                                        0168 00078000
*         RTNCODE UNRSTD;           /*                               */ 00079000
*     END;                          /*                               */ 00080000
*                                                                  0170 00081000
*   /*****************************************************************/ 00082000
*   /*                                                               */ 00083000
*   /* WRITE RECORD TO SPECIFIED OTPUT DATASET(S)                    */ 00084000
*   /*                                                               */ 00085000
*   /*****************************************************************/ 00086000
*                                                                  0170 00087000
*   IF CRDRTNCD=ZERO                /* IF PREV FUNCTIONS OK          */ 00088000
*       &(CRPBUFFR(1:2)^='++'       /* AND NOT SMP CNTL CARD         */ 00089000
*       |CRPNOWPP=OFF)              /* OR USER INDICATED WRITE       */ 00090000
*     THEN                          /*                               */ 00091000
@RF00163 CH    CRDRTNCD,@CH00092                                   0170 00092000
         BNE   @RF00170                                            0170 00093000
         L     @10,@PC00001                                        0170 00094000
         CLC   CRPBUFFR(2,@10),@CC01343                            0170 00095000
         BNE   @RT00170                                            0170 00096000
         TM    CRPNOWPP(@10),B'00100000'                           0170 00097000
         BNZ   @RF00170                                            0170 00098000
@RT00170 DS    0H                                                  0171 00099000
*     DO;                           /*                               */ 00100000
*       CALL CRDPUT;                /* GET NEXT RCD INTO BUFFER      */ 00101000
         BAL   @14,CRDPUT                                          0172 00102000
*       RFY                                                        0173 00103000
*         RTNCODE RSTD;             /*                               */ 00104000
*       CRDRTNCD=MAX(CRDRTNCD,RTNCODE);/* SAVE RETURN CODE           */ 00105000
         CR    CRDRTNCD,RTNCODE                                    0174 00106000
         BNL   *+6                                                      00107000
         LR    CRDRTNCD,RTNCODE                                    0174 00108000
*       RFY                                                        0175 00109000
*         RTNCODE UNRSTD;           /*                               */ 00110000
*     END;                          /*                               */ 00111000
*                                                                  0176 00112000
*   /*****************************************************************/ 00113000
*   /*                                                               */ 00114000
*   /* RESET VARIABLES IN SCAN PARAMETER LIST                        */ 00115000
*   /*                                                               */ 00116000
*   /*****************************************************************/ 00117000
*                                                                  0177 00118000
*   IF CRPSCPAD^=ZERO               /* IF SCP PASSED                 */ 00119000
*     THEN                          /*                               */ 00120000
@RF00170 SLR   @10,@10                                             0177 00121000
         L     @05,@PC00001                                        0177 00122000
         C     @10,CRPSCPAD(,@05)                                  0177 00123000
         BE    @RF00177                                            0177 00124000
*     DO;                           /*                               */ 00125000
*       SCPCHAR=ADDR(CRPBUFFR);     /* SET PTR TO BUFFER             */ 00126000
         LA    @05,CRPBUFFR(,@05)                                  0179 00127000
         ST    @05,SCPCHAR(,SCPPTR)                                0179 00128000
*       IF CRDRTNCD=ZERO            /* IF RCD PROCESSED OK           */ 00129000
*         THEN                      /*                               */ 00130000
         CR    CRDRTNCD,@10                                        0180 00131000
         BNE   @RF00180                                            0180 00132000
*         SCPRET=SCPGOOD;           /* SET RETURN CODE OK            */ 00133000
