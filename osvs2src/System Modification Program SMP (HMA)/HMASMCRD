         B     @RC00180                                            0182 00137000
@RF00180 STC   CRDRTNCD,SCPRETRN(,SCPPTR)                          0182 00138000
*     END;                          /*                               */ 00139000
*                                                                  0183 00140000
@RC00180 DS    0H                                                  0184 00141000
*   /*****************************************************************/ 00142000
*   /*                                                               */ 00143000
*   /* RETURN WITH CODE SET UP DURING PROCESSING                     */ 00144000
*   /*                                                               */ 00145000
*   /*****************************************************************/ 00146000
*                                                                  0184 00147000
*   CRPRETRN=CRDRTNCD;              /* MOVE RC TO PARM LIST          */ 00148000
@RF00177 L     @10,@PC00001                                        0184 00149000
         STC   CRDRTNCD,CRPRETRN(,@10)                             0184 00150000
*   RETURN CODE(CRDRTNCD);          /*                               */ 00151000
         LR    @15,CRDRTNCD                                        0185 00152000
         L     @13,4(,@13)                                         0185 00153000
         L     @14,12(,@13)                                        0185 00154000
         LM    @00,@12,20(@13)                                     0185 00155000
         BR    @14                                                 0185 00156000
         EJECT                                                          00157000
*                                                                  0186 00158000
*   /*****************************************************************/ 00159000
*   /*                                                               */ 00160000
*   /* THIS PROCEDURE IS CALLED TO READ THE NEXT RECORD INTO THE     */ 00161000
*   /* INPUT BUFFER                                                  */ 00162000
*   /*                                                               */ 00163000
*   /*****************************************************************/ 00164000
*                                                                  0186 00165000
*CRDGET:                            /*                               */ 00166000
*   PROC;                           /*                               */ 00167000
CRDGET   STM   @14,@12,@SA00002                                    0186 00168000
*   DECLARE                         /*                               */ 00169000
*     GETRTNCD FIXED(8);            /*                               */ 00170000
*   GETRTNCD=ZERO;                  /*                               */ 00171000
         SLR   GETRTNCD,GETRTNCD                                   0188 00172000
*   IF CRPBFPRM=OFF                 /* IF BUFFER ALREADY PRIMED      */ 00173000
*       &CRPEXADD=OFF               /* AND EXIT NOT TO ADD RCDS      */ 00174000
*       &CRPEOFSW=OFF               /* AND NOT EOF YET               */ 00175000
*     THEN                          /*                               */ 00176000
         L     @10,@PC00001                                        0189 00177000
         TM    CRPBFPRM(@10),B'10000000'                           0189 00178000
         BNZ   @RF00189                                            0189 00179000
         TM    CRPEXADD(@10),B'11000000'                           0189 00180000
         BNZ   @RF00189                                            0189 00181000
*     DO;                           /*                               */ 00182000
*       IOPDSID=CRPINDS;            /* SPECIFY INPUT DSID            */ 00183000
         MVC   IOPDSID(1,IOPPTR),CRPINDS(@10)                      0191 00184000
*       IF CRPINDS=IOPANYDD         /* NON-SMP DATASET               */ 00185000
*           |CRPINDS=IOPTXTDD       /* NON-SMP DATASET               */ 00186000
*           |CRPINDS=IOPDDNM        /* NON-SMP DATASET               */ 00187000
*         THEN                                                     0192 00188000
         CLI   CRPINDS(@10),24                                     0192 00189000
         BE    @RT00192                                            0192 00190000
         CLI   CRPINDS(@10),24                                     0192 00191000
         BE    @RT00192                                            0192 00192000
         CLI   CRPINDS(@10),14                                     0192 00193000
         BNE   @RF00192                                            0192 00194000
@RT00192 DS    0H                                                  0193 00195000
*         IOPDDNAM=CRPDDNAM;        /* INITIALIZE DATASET DDNAME     */ 00196000
         L     @10,@PC00001                                        0193 00197000
         MVC   IOPDDNAM(8,IOPPTR),CRPDDNAM(@10)                    0193 00198000
*       IOPFUNCT=IOPREAD;           /* INDICATE INPUT OPERATION      */ 00199000
@RF00192 MVI   IOPFUNCT(IOPPTR),X'01'                              0194 00200000
*       IOPBUFAD=ADDR(CRPBUFFR);    /* TO PARM BUFFER                */ 00201000
         L     @10,@PC00001                                        0195 00202000
         LA    @10,CRPBUFFR(,@10)                                  0195 00203000
         ST    @10,IOPBUFAD(,IOPPTR)                               0195 00204000
*       CALL HMASMIO(HMASMIOP);     /* READ A RECORD                 */ 00205000
         ST    IOPPTR,@AL00001                                     0196 00206000
         L     @15,@CV01324                                        0196 00207000
         LA    @01,@AL00001                                        0196 00208000
         BALR  @14,@15                                             0196 00209000
*       IF IOPRETRN=IOPEOF          /*                               */ 00210000
*         THEN                      /*                               */ 00211000
         CLI   IOPRETRN(IOPPTR),4                                  0197 00212000
         BNE   @RF00197                                            0197 00213000
*         DO;                       /*                               */ 00214000
*           IF CRPUXNUM=ZERO        /* IF NO USER EXIT TO ADD MORE   */ 00215000
*             THEN                  /* RECORDS THEN SET EOF SW IN    */ 00216000
         L     @10,@PC00001                                        0199 00217000
         CLI   CRPUXNUM(@10),0                                     0199 00218000
         BNE   @RF00199                                            0199 00219000
*             DO;                   /*                               */ 00220000
*               GETRTNCD=CRPEOF;    /* SET EOF RC                    */ 00221000
         LA    GETRTNCD,4                                          0201 00222000
*               CRPEOFEN=ON;        /* PARAMETER                     */ 00223000
         OI    CRPEOFEN(@10),B'10000000'                           0202 00224000
*             END;                  /*                               */ 00225000
*           CRPEOFSW=ON;            /* SET EOF SW                    */ 00226000
@RF00199 L     @10,@PC00001                                        0204 00227000
         OI    CRPEOFSW(@10),B'10000000'                           0204 00228000
*         END;                      /*                               */ 00229000
*       ELSE                        /*                               */ 00230000
*         GETRTNCD=IOPRETRN;        /* SAVE RETURN CODE              */ 00231000
*                                                                  0206 00232000
         B     @RC00197                                            0206 00233000
@RF00197 SLR   GETRTNCD,GETRTNCD                                   0206 00234000
         IC    GETRTNCD,IOPRETRN(,IOPPTR)                          0206 00235000
*       /*************************************************************/ 00236000
*       /*                                                           */ 00237000
*       /* RESET CCA TIME FIELDS                                     */ 00238000
*       /*                                                           */ 00239000
*       /*************************************************************/ 00240000
*                                                                  0207 00241000
*       IF CRPSCNCL=OFF             /* IF CALL FROM DRV              */ 00242000
*           &CRPINDS=IOPCONTR       /* AND INPUT IS SMPCNTL          */ 00243000
*         THEN                      /* THEN GET NEW TIME             */ 00244000
@RC00197 L     @10,@PC00001                                        0207 00245000
         TM    CRPSCNCL(@10),B'00000001'                           0207 00246000
         BNZ   @RF00207                                            0207 00247000
         CLI   CRPINDS(@10),9                                      0207 00248000
         BNE   @RF00207                                            0207 00249000
*         DO;                       /* FOR HEADINGS                  */ 00250000
*           GEN(TIME DEC);          /* GET TIME                      */ 00251000
         TIME DEC                                                       00252000
*           RFY                                                    0210 00253000
*             R0 RSTD;                                             0210 00254000
*           TIMETEMP=R0;            /* SAVE TIME                     */ 00255000
         ST    R0,TIMETEMP                                         0211 00256000
*           RFY                                                    0212 00257000
*             R0 UNRSTD;                                           0212 00258000
*           CCATIME=TIMETEMP(1:3);  /* SAVE TIME IN CCA              */ 00259000
         L     @10,@CA00099                                        0213 00260000
         MVC   CCATIME-HMASMCCA(3,@10),TIMETEMP                    0213 00261000
*         END;                      /*                               */ 00262000
*     END;                          /*                               */ 00263000
@RF00207 DS    0H                                                  0216 00264000
*   RETURN CODE(GETRTNCD);          /*                               */ 00265000
@RF00189 LR    @15,GETRTNCD                                        0216 00266000
         L     @14,@SA00002                                        0216 00267000
         LM    @00,@12,@SA00002+8                                  0216 00268000
         BR    @14                                                 0216 00269000
*   END CRDGET;                     /*                               */ 00270000
         EJECT                                                          00271000
*                                                                  0218 00272000
*/********************************************************************/ 00273000
*/*                                                                  */ 00274000
*/* CRDEXIT - CALL TO USER EXIT ROUTINE                              */ 00275000
*/*                                                                  */ 00276000
*/********************************************************************/ 00277000
*/*                                                                  */ 00278000
*/* USER INTERFACE - USERREG (REG 1) CONTAINS ADDR OF 80-BYTE BUFFER */ 00279000
*/*    OF PTF RECORD. IF THE HIGH-ORDER BYTE IS ON, EOF WAS FOUND    */ 00280000
*/*    ON PTF FILE. IF USER WANTS TO SUBSTITUTE HIS PTF RECORD FOR   */ 00281000
*/*    ONE READ BY ROUTINE, HE MUST MOVE INTO OUR BUFFER HIS RECORD  */ 00282000
*/*    AND NOT SUBSTITUTE BUFFER ADDRESSES.                          */ 00283000
*/*                                                                  */ 00284000
*/* OUTPUT - PTF RECORD IN BUFFER POINTED TO BY IOPBUFAD.            */ 00285000
*/*    RETURN CODES- (FROM USER EXIT TO READ ROUTINE)                */ 00286000
*/*           0 - NORMAL RETURN                                      */ 00287000
*/*           4 - UNUSED                                             */ 00288000
*/*           8 - REQUEST TO TERMINATE THIS SYSMOD                   */ 00289000
*/*          12 - REQUEST TO TERMINATE THIS FUNCTION                 */ 00290000
*/*          16 - REQUEST TO TERMINATE SMP                           */ 00291000
*/*          20 - NORMAL RETURN, PROCESS RCD IN BUFFER               */ 00292000
*/*               BYPASS READ ON NEXT REQUEST. USER WILL ADD RCD     */ 00293000
*/*          24 - USER DELETED RCD IN BURRDER.                       */ 00294000
*/*               READ NEXT RCD AND RETURN TO USER EXIT.             */ 00295000
*/*                                                                  */ 00296000
*/********************************************************************/ 00297000
*                                                                  0218 00298000
*CRDEXIT:                                                          0218 00299000
*   PROC;                                                          0218 00300000
CRDEXIT  STM   @14,@12,@SA00003                                    0218 00301000
*   DECLARE                         /*                               */ 00302000
*     EXTRTNCD FIXED(8);            /*                               */ 00303000
*   DECLARE                         /*                               */ 00304000
*     SAVRTNCD FIXED(31);           /* RTNCODE SAVE AREA             */ 00305000
*   DECLARE                         /*                               */ 00306000
*     1 EXTSW BIT(8),               /*                               */ 00307000
*       3 STOP BIT(1),              /* LOOP CONTROL                  */ 00308000
*       3 * BIT(7);                 /* UNUSED                        */ 00309000
*   EXTRTNCD=ZERO;                  /*                               */ 00310000
         SLR   EXTRTNCD,EXTRTNCD                                   0222 00311000
*   STOP=OFF;                       /* INIT LOOP CONTROL             */ 00312000
         NI    STOP,B'01111111'                                    0223 00313000
*   DO WHILE(                       /* LOOP                          */ 00314000
*         CRPUXNUM^=ZERO            /* WHILE EXIT SPECIFIED          */ 00315000
*         &CRPBFPRM=OFF             /* AND BUFFER NOT PRIMED         */ 00316000
*         &STOP=OFF);               /* AND SW SET                    */ 00317000
         B     @DE00224                                            0224 00318000
@DL00224 DS    0H                                                  0225 00319000
*     STOP=ON;                      /* SET SW TO STOP LOOP           */ 00320000
         OI    STOP,B'10000000'                                    0225 00321000
*     IF CRPEXADD=ON                /* IF PREV CALL TO EXIT INDICATED   00322000
*                                      ADD                           */ 00323000
*         |CRPEOFSW=OFF             /* OR EOF NOT ENCOUNTERED YET    */ 00324000
*         |CRPEOF1=OFF              /* OR IF EOF THIS IS FIRST TIME     00325000
*                                      THRU AFTER EOF                */ 00326000
*       THEN                        /* THEN CALL USER EXIT           */ 00327000
         L     @10,@PC00001                                        0226 00328000
         TM    CRPEXADD(@10),B'01000000'                           0226 00329000
         BNZ   @RT00226                                            0226 00330000
         TM    CRPEOFSW(@10),B'10010000'                           0226 00331000
         BO    @RF00226                                            0226 00332000
@RT00226 DS    0H                                                  0227 00333000
*       DO;                         /*                               */ 00334000
*CALLEXIT:                          /*                               */ 00335000
*         UX001PRM=''B;             /* ZERO 001 PARM                 */ 00336000
CALLEXIT XC    UX001PRM(81),UX001PRM                               0228 00337000
*         IF CRPEOFSW=ON            /* IF EOF ENCOUNTERED            */ 00338000
*           THEN                    /*                               */ 00339000
         L     @10,@PC00001                                        0229 00340000
         TM    CRPEOFSW(@10),B'10000000'                           0229 00341000
         BNO   @RF00229                                            0229 00342000
*           UX001RC=UX001EOF;       /* SET EOF RC                    */ 00343000
         MVI   UX001RC,X'04'                                       0230 00344000
*         UX001RCD=CRPBUFFR;        /* MOVE RCD TO BUFFER            */ 00345000
@RF00229 L     @10,@PC00001                                        0231 00346000
         MVC   UX001RCD(80),CRPBUFFR(@10)                          0231 00347000
*         CALL HMASMUXC(UXPUX001,UX001PRM);/* CALL USER EXIT INTF    */ 00348000
         L     @15,@CV01325                                        0232 00349000
         LA    @01,@AL00232                                        0232 00350000
         BALR  @14,@15                                             0232 00351000
*         RFY                                                      0233 00352000
*           RTNCODE RSTD;                                          0233 00353000
*                                                                  0233 00354000
*         /***********************************************************/ 00355000
*         /*                                                         */ 00356000
*         /* IF NORMAL RETURN CODE                                   */ 00357000
*         /*                                                         */ 00358000
*         /***********************************************************/ 00359000
*                                                                  0234 00360000
*         IF RTNCODE=UXPGOOD        /* IF USE INPUT BUFFER           */ 00361000
*           THEN                    /*                               */ 00362000
         LTR   RTNCODE,RTNCODE                                     0234 00363000
         BNZ   @RF00234                                            0234 00364000
*           DO;                     /*                               */ 00365000
*             CRPBUFFR=UX001RCD;    /* MOVE RCD TO PARM              */ 00366000
         L     @14,@PC00001                                        0236 00367000
         MVC   CRPBUFFR(80,@14),UX001RCD                           0236 00368000
*             IF CRPEOFSW=ON        /* IF EOF HIT ALREADY            */ 00369000
*               THEN                /*                               */ 00370000
         TM    CRPEOFSW(@14),B'10000000'                           0237 00371000
         BNO   @RF00237                                            0237 00372000
*               DO;                 /*                               */ 00373000
*                 CRPEOFEN=ON;      /* END OF FILE SW                */ 00374000
         OI    CRPEOFEN(@14),B'10000000'                           0239 00375000
*                 EXTRTNCD=CRPEOF;  /* RESET RC TO EOF               */ 00376000
         LA    EXTRTNCD,4                                          0240 00377000
*               END;                /*                               */ 00378000
*             ELSE                  /* IF NOT EOF                    */ 00379000
*               IF CRPEXADD=ON      /* IF USER ADDED LAST RCD        */ 00380000
*                 THEN              /*                               */ 00381000
         B     @RC00237                                            0242 00382000
@RF00237 L     @14,@PC00001                                        0242 00383000
         TM    CRPEXADD(@14),B'01000000'                           0242 00384000
         BNO   @RF00242                                            0242 00385000
*                 DO;               /*                               */ 00386000
*                   CRPEXADD=OFF;   /* TURN OFF ADD SW               */ 00387000
         NI    CRPEXADD(@14),B'10111111'                           0244 00388000
*                   SAVRTNCD=RTNCODE;/* SAVE RC                      */ 00389000
         LR    SAVRTNCD,RTNCODE                                    0245 00390000
*                   CALL CRDGET;    /* GET NEXT RCD                  */ 00391000
         BAL   @14,CRDGET                                          0246 00392000
*                   IF RTNCODE<=CRPEOF/* IF NO ERROR IN GET          */ 00393000
*                     THEN          /*                               */ 00394000
         CH    RTNCODE,@CH00094                                    0247 00395000
         BH    @RF00247                                            0247 00396000
*                     DO;           /*                               */ 00397000
*                       STOP=OFF;   /*                               */ 00398000
         NI    STOP,B'01111111'                                    0249 00399000
*                       RTNCODE=SAVRTNCD;/* RESET RC                 */ 00400000
         LR    RTNCODE,SAVRTNCD                                    0250 00401000
*                     END;          /*                               */ 00402000
*                   ELSE            /* IF ERR IN GET                 */ 00403000
*                     EXTRTNCD=RTNCODE;/* SAVE RC                    */ 00404000
         B     @RC00247                                            0252 00405000
@RF00247 LR    EXTRTNCD,RTNCODE                                    0252 00406000
*                 END;              /*                               */ 00407000
*           END;                    /*                               */ 00408000
*                                                                  0254 00409000
*         /***********************************************************/ 00410000
*         /*                                                         */ 00411000
*         /* IF REQUEST TO ADD RECORD AFTER THIS ONE                 */ 00412000
*         /*                                                         */ 00413000
*         /***********************************************************/ 00414000
*                                                                  0255 00415000
*         ELSE                      /*                               */ 00416000
*           DO;                     /*                               */ 00417000
         B     @RC00234                                            0255 00418000
@RF00234 DS    0H                                                  0256 00419000
*             CRPEXADD=OFF;         /* NO EXTRA ADD                  */ 00420000
         L     @14,@PC00001                                        0256 00421000
         NI    CRPEXADD(@14),B'10111111'                           0256 00422000
*             IF RTNCODE=UXPADD     /* IF ADD REQUEST                */ 00423000
*               THEN                /*                               */ 00424000
         CH    RTNCODE,@CH00411                                    0257 00425000
         BNE   @RF00257                                            0257 00426000
*               DO;                 /*                               */ 00427000
*                 CRPEXADD=ON;      /* SET TO ADD RECORD NEXT TIME   */ 00428000
         OI    CRPEXADD(@14),B'01000000'                           0259 00429000
*                 CRPBUFFR=UX001RCD;/* MOVE RCD TO BUFFER            */ 00430000
         MVC   CRPBUFFR(80,@14),UX001RCD                           0260 00431000
*                 IF CRPEOFSW=ON    /* IF EOF HIT                    */ 00432000
*                     &CRPEOF1=OFF  /* AND THIS IS FIRST TIME AFTER  */ 00433000
*                   THEN            /* EOF THEN                      */ 00434000
         TM    CRPEOFSW(@14),B'10000000'                           0261 00435000
         BNO   @RF00261                                            0261 00436000
         TM    CRPEOF1(@14),B'00010000'                            0261 00437000
         BNZ   @RF00261                                            0261 00438000
*                   DO;             /*                               */ 00439000
*                     CRPEOF1=ON;   /* SET SW TO PREVENT AGAIN       */ 00440000
         OI    CRPEOF1(@14),B'00010000'                            0263 00441000
*                     GO TO CALLEXIT;/* GO BACK TO EXIT ONCE         */ 00442000
         B     CALLEXIT                                            0264 00443000
*                   END;            /*                               */ 00444000
*               END;                /*                               */ 00445000
*                                                                  0266 00446000
*             /*******************************************************/ 00447000
*             /*                                                     */ 00448000
*             /* IF REQUEST TO DELETE CURRENT RECORD                 */ 00449000
*             /*                                                     */ 00450000
*             /*******************************************************/ 00451000
*                                                                  0267 00452000
*             ELSE                  /*                               */ 00453000
*               IF RTNCODE=UXPDEL   /* IF DEL CURRENT RCD            */ 00454000
*                 THEN              /*                               */ 00455000
         B     @RC00257                                            0267 00456000
@RF00257 CH    RTNCODE,@CH00221                                    0267 00457000
         BNE   @RF00267                                            0267 00458000
*                 DO;               /*                               */ 00459000
*                   IF CRPEOFSW=ON  /* IF AT EOF                     */ 00460000
*                     THEN          /*                               */ 00461000
         L     @14,@PC00001                                        0269 00462000
         TM    CRPEOFSW(@14),B'10000000'                           0269 00463000
         BNO   @RF00269                                            0269 00464000
*                     EXTRTNCD=CRPEOF;/* SET RC TO EOF               */ 00465000
         LA    EXTRTNCD,4                                          0270 00466000
*                   ELSE            /*                               */ 00467000
*                     DO;           /*                               */ 00468000
         B     @RC00269                                            0271 00469000
@RF00269 DS    0H                                                  0272 00470000
*                       SAVRTNCD=RTNCODE;/* SAVE RC ACROSS CALL      */ 00471000
         LR    SAVRTNCD,RTNCODE                                    0272 00472000
*                       CALL CRDGET;/* GET NEXT RECORD               */ 00473000
         BAL   @14,CRDGET                                          0273 00474000
*                       IF RTNCODE>CRPEOF/* IF ERROR IN GET          */ 00475000
*                         THEN      /*                               */ 00476000
         CH    RTNCODE,@CH00094                                    0274 00477000
