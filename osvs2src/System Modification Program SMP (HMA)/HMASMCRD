*                         EXTRTNCD=RTNCODE;/* PASS ALONG CODE        */ 00479000
         LR    EXTRTNCD,RTNCODE                                    0275 00480000
*                       STOP=OFF;   /* SET SW TO CONTINUE EXIT CALL  */ 00481000
@RF00274 NI    STOP,B'01111111'                                    0276 00482000
*                       RTNCODE=SAVRTNCD;/* RELOAD RC                */ 00483000
         LR    RTNCODE,SAVRTNCD                                    0277 00484000
*                     END;          /*                               */ 00485000
*                 END;              /*                               */ 00486000
*                                                                  0279 00487000
*             /*******************************************************/ 00488000
*             /*                                                     */ 00489000
*             /* IF ANY OTHER RETURN CODE THEN PASS IT ALONG         */ 00490000
*             /*                                                     */ 00491000
*             /*******************************************************/ 00492000
*                                                                  0280 00493000
*               ELSE                /*                               */ 00494000
*                 DO;               /*                               */ 00495000
         B     @RC00267                                            0280 00496000
@RF00267 DS    0H                                                  0281 00497000
*                   EXTRTNCD=RTNCODE;/* PASS ALONG RC                */ 00498000
         LR    EXTRTNCD,RTNCODE                                    0281 00499000
*                   CRPTRMEX=ON;    /* TERMINATED BY USER EXIT       */ 00500000
         L     @14,@PC00001                                        0282 00501000
         OI    CRPTRMEX(@14),B'01000000'                           0282 00502000
*                 END;              /*                               */ 00503000
*           END;                    /* END NOT ZERO RC               */ 00504000
@RC00267 DS    0H                                                  0284 00505000
@RC00257 DS    0H                                                  0285 00506000
*         RFY                                                      0285 00507000
*           RTNCODE UNRSTD;         /*                               */ 00508000
@RC00234 DS    0H                                                  0286 00509000
*       END;                        /* END NOT EOF OR ADD REQUESTED  */ 00510000
*   END;                            /* END EXIT CALL LOOP            */ 00511000
@RF00226 DS    0H                                                  0287 00512000
@DE00224 L     @10,@PC00001                                        0287 00513000
