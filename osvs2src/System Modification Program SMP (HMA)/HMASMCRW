@RF00262 DS    0H                                                  0272 00521000
*           /*********************************************************/ 00522000
*           /*                                                       */ 00523000
*           /* A FMID CHANGE HAS OCCURRED IN THE GTA FILE RECORDS.   */ 00524000
*           /* EACH FMID REQUIRES A SEPARATE RECORD IN THE MEMBER, SO*/ 00525000
*           /* WRITE THE CURRENT RECORD FOR THIS FMID, THEN BUILD THE*/ 00526000
*           /* NEXT FMID RECORD                                      */ 00527000
*           /*                                                       */ 00528000
*           /*********************************************************/ 00529000
*                                                                  0272 00530000
*           IOPFUNCT=IOPWRITE;      /* SET FUNCTION TO WRITE         */ 00531000
         MVI   IOPFUNCT(IOPPTR),X'51'                              0272 00532000
*           IOPTYPE=IOPCSR;         /* ... TO CAUSER MEMBER          */ 00533000
         MVI   IOPTYPE(IOPPTR),X'0F'                               0273 00534000
*           IOPNAM17=CAUSER;        /* ... CAUSER MEMBER NAME        */ 00535000
         L     @10,@PC00001                                        0274 00536000
         MVC   IOPNAM17(7,IOPPTR),CAUSER(@10)                      0274 00537000
*           CALL HMASMIO(HMASMIOP); /* CALL IO TO WRITE DATA RECORD  */ 00538000
         ST    IOPPTR,@AL00001                                     0275 00539000
         L     @15,@CV01289                                        0275 00540000
         LA    @01,@AL00001                                        0275 00541000
         BALR  @14,@15                                             0275 00542000
*           IF IOPRETRN=IOPGOOD THEN/* WAS WRITE OK?                 */ 00543000
         CLI   IOPRETRN(IOPPTR),0                                  0276 00544000
         BNE   @RF00276                                            0276 00545000
*             DO;                   /* YES- THEN                     */ 00546000
*                                                                  0277 00547000
*               /*****************************************************/ 00548000
*               /*                                                   */ 00549000
*               /* BUILD NEW FMID RECORD WITH REQUISITES FOR CAUSER  */ 00550000
*               /* MEMBER                                            */ 00551000
*               /*                                                   */ 00552000
*               /*****************************************************/ 00553000
*                                                                  0278 00554000
*               LASTFMID=TPIFMID;   /* SET UP FMID WITH CURRENT FMID */ 00555000
