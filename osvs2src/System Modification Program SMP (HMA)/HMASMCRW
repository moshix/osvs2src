*           IOPQFMND(1)=IOPEOLST;   /* SET END OF VARIABLE LIST      */ 00407000
         MVI   IOPQFMND(IOPPTR),X'FF'                              0239 00408000
*           IOPRETRN=IOPGOOD;       /* RESET RC=0 TO CONTINUE        */ 00409000
         MVI   IOPRETRN(IOPPTR),X'00'                              0240 00410000
*         END;                      /* END OF BUILD ONE LOOP         */ 00411000
*       CRWRETRN=MAX(CRWRETRN,IOPRETRN);/* PICK UP HIGHEST RC        */ 00412000
