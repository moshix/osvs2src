         TITLE 'HMASMCRW - WRITE ''IF DATA'' TO (A)CRQ'                 00001000
HMASMCRW CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMCRW  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(4),0(@01)                                  0001 00017000
*   IOPPTR=CCAIOPTR;                /* SET BASE TO GENERAL IOP       */ 00018000
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0155 00019000
*   CRWRETRN=ZERO;                  /* INIT RETURN CODE COLLECTOR    */ 00020000
         SLR   CRWRETRN,CRWRETRN                                   0156 00021000
*   LASTFMID=' ';                   /* INIT SAVED FMID ENVIRONMENT   */ 00022000
*                                                                  0157 00023000
         MVI   LASTFMID+1,C' '                                     0157 00024000
         MVC   LASTFMID+2(5),LASTFMID+1                            0157 00025000
         MVI   LASTFMID,C' '                                       0157 00026000
*   /*****************************************************************/ 00027000
*   /*                                                               */ 00028000
*   /* DETERMINE FUNCTION SO THAT DATASET CAN BE SET                 */ 00029000
*   /*                                                               */ 00030000
*   /*****************************************************************/ 00031000
*                                                                  0158 00032000
*   IF CCAACCPT=ON THEN             /* IF FUNCTION IS ACCEPT THEN    */ 00033000
         TM    CCAACCPT(CCAPTR),B'00100000'                        0158 00034000
         BNO   @RF00158                                            0158 00035000
*     IOPDSID=IOPACRQ;              /* USE ACRQ AS DATASET,          */ 00036000
         MVI   IOPDSID(IOPPTR),X'1E'                               0159 00037000
*   ELSE                                                           0160 00038000
*     IOPDSID=IOPCRQ;               /* OTHERWISE USE CRQ AS DATASET  */ 00039000
*                                                                  0160 00040000
         B     @RC00158                                            0160 00041000
@RF00158 MVI   IOPDSID(IOPPTR),X'1C'                               0160 00042000
*   /*****************************************************************/ 00043000
*   /*                                                               */ 00044000
*   /* INITIALIZE PARAMETERS FOR HMASMVLU                            */ 00045000
*   /*                                                               */ 00046000
*   /*****************************************************************/ 00047000
*                                                                  0161 00048000
*   HMASMVLP=''B;                   /*                               */ 00049000
@RC00158 XC    HMASMVLP(52),HMASMVLP                               0161 00050000
*   VLPFUNCT=VLPADD;                /* SET FUNCTION TO ADD ENTRIES   */ 00051000
         MVI   VLPFUNCT,X'01'                                      0162 00052000
*   VLPFMSG=OFF;                    /* NO MESSAGES (ERROR/WARNING)   */ 00053000
         NI    VLPFMSG,B'01111111'                                 0163 00054000
*   VLPLPTR=ADDR(IOPQFMLS(1));      /* SET POINTER TO FIRST ENTRY    */ 00055000
*                                                                  0164 00056000
         LA    @10,IOPQFMLS(,IOPPTR)                               0164 00057000
         ST    @10,VLPLPTR                                         0164 00058000
*   /*****************************************************************/ 00059000
*   /*                                                               */ 00060000
*   /* INITIALIZE PARAMETERS FOR HMASMGTA                            */ 00061000
*   /*                                                               */ 00062000
*   /*****************************************************************/ 00063000
*                                                                  0165 00064000
*   GTPRCDLN=LENGTH(TPIFRECD);      /* MAXIMUM RECORD LENGTH         */ 00065000
         L     @10,CCAIFGTP(,CCAPTR)                               0165 00066000
         MVC   GTPRCDLN(2,@10),@CH00456                            0165 00067000
*                                                                  0166 00068000
*   /*****************************************************************/ 00069000
*   /*                                                               */ 00070000
*   /* READ THE GTA FILE TWICE. FIRST TIME FOR THE ENVIRONMENT ENTRY,*/ 00071000
*   /* THE SECOND TIME FOR THE CAUSER ENTRIES.                       */ 00072000
*   /*                                                               */ 00073000
*   /*****************************************************************/ 00074000
*                                                                  0166 00075000
*   DO X=1 BY 1 TO 2 WHILE CRWRETRN=GTPGOOD;/* READ TWICE WHILE RC=0 */ 00076000
*                                                                  0166 00077000
         LA    X,1                                                 0166 00078000
@DL00166 LTR   CRWRETRN,CRWRETRN                                   0166 00079000
         BNZ   @DC00166                                            0166 00080000
*     /***************************************************************/ 00081000
*     /*                                                             */ 00082000
*     /* LOCATE FIRST RECORD FOR "CAUSER" IN GTA FILE                */ 00083000
*     /*                                                             */ 00084000
*     /***************************************************************/ 00085000
*                                                                  0167 00086000
*     TPIKEY=''B;                   /* CLEAR KEY                     */ 00087000
         L     @10,CCAIFGTP(,CCAPTR)                               0167 00088000
         L     @05,GTPBUFAD(,@10)                                  0167 00089000
         XC    TPIKEY(21,@05),TPIKEY(@05)                          0167 00090000
*     TPICSR=CAUSER;                /* PUT SYSMOD-ID PASSED IN KEY   */ 00091000
         L     @15,@PC00001                                        0168 00092000
         MVC   TPICSR(7,@05),CAUSER(@15)                           0168 00093000
*     GTPFUNCT=GTPLOC;              /* SET FUNCTION TO LOCATE        */ 00094000
         MVI   GTPFUNCT(@10),X'03'                                 0169 00095000
*     GTPKEYLN=LENGTH(TPIKEY);      /* SET FULL KEY LENGTH           */ 00096000
         MVC   GTPKEYLN(2,@10),@CH00454                            0170 00097000
*     CALL HMASMGTA(HMASMGTP);      /* CALL GTA TO LOCATE RECORD     */ 00098000
         ST    @10,@AL00001                                        0171 00099000
         L     @15,@CV01291                                        0171 00100000
         LA    @01,@AL00001                                        0171 00101000
         BALR  @14,@15                                             0171 00102000
*     GTPKEYLN=LENGTH(TPICSR);      /* SET UP FOR PARTIAL KEY        */ 00103000
         L     @10,CCAIFGTP(,CCAPTR)                               0172 00104000
         MVC   GTPKEYLN(2,@10),@CH00034                            0172 00105000
*     CALL HMASMGTA(HMASMGTP);      /* CALL GTA TO LOCATE RECORD     */ 00106000
         ST    @10,@AL00001                                        0173 00107000
         L     @15,@CV01291                                        0173 00108000
         LA    @01,@AL00001                                        0173 00109000
         BALR  @14,@15                                             0173 00110000
*     IF GTPRETRN=GTPGOOD THEN      /* WAS FUNCTION OK?              */ 00111000
         L     @10,CCAIFGTP(,CCAPTR)                               0174 00112000
         CLI   GTPRETRN(@10),0                                     0174 00113000
         BNE   @RF00174                                            0174 00114000
*       DO;                         /* YES-THEN                      */ 00115000
*         GTPFUNCT=GTPSTRTN;        /* SET FUNCTION TO RESTART GETN  */ 00116000
         MVI   GTPFUNCT(@10),X'06'                                 0176 00117000
*         CALL HMASMGTA(HMASMGTP);  /* CALL GTA TO RESTART GETN      */ 00118000
         ST    @10,@AL00001                                        0177 00119000
         L     @15,@CV01291                                        0177 00120000
         LA    @01,@AL00001                                        0177 00121000
         BALR  @14,@15                                             0177 00122000
*         IF GTPRETRN=GTPGOOD THEN  /* WAS FUNCTION OK?              */ 00123000
         L     @10,CCAIFGTP(,CCAPTR)                               0178 00124000
         CLI   GTPRETRN(@10),0                                     0178 00125000
         BNE   @RF00178                                            0178 00126000
*           DO;                     /* YES-THEN                      */ 00127000
*                                                                  0179 00128000
*             /*******************************************************/ 00129000
*             /*                                                     */ 00130000
*             /* LOOP THROUGH THE ENTRIES IN THE GTA FILE UNTIL      */ 00131000
*             /* EITHER AN END OF FILE OR A CHANGE IN KEY IS FOUND.  */ 00132000
*             /*                                                     */ 00133000
*             /*******************************************************/ 00134000
*                                                                  0180 00135000
*             DO WHILE GTPRETRN=GTPGOOD;/* TILL EOF/CHANGE           */ 00136000
         B     @DE00180                                            0180 00137000
@DL00180 DS    0H                                                  0181 00138000
*               GTPFUNCT=GTPGETN;   /* SET FUNCTION TO GETNEXT       */ 00139000
         L     @10,CCAIFGTP(,CCAPTR)                               0181 00140000
         MVI   GTPFUNCT(@10),X'08'                                 0181 00141000
*               CALL HMASMGTA(HMASMGTP);/* CALL GTA TO DO GETNEXT    */ 00142000
         ST    @10,@AL00001                                        0182 00143000
         L     @15,@CV01291                                        0182 00144000
         LA    @01,@AL00001                                        0182 00145000
         BALR  @14,@15                                             0182 00146000
*               IF GTPRETRN=GTPGOOD THEN/* NEXT RECORD FOUND?        */ 00147000
         L     @10,CCAIFGTP(,CCAPTR)                               0183 00148000
         CLI   GTPRETRN(@10),0                                     0183 00149000
         BNE   @RF00183                                            0183 00150000
*                 DO;               /* YES-THEN,                     */ 00151000
*                   IF TPICSR=CAUSER THEN/* IS IT THE SAME KEY?      */ 00152000
         L     @10,GTPBUFAD(,@10)                                  0185 00153000
         L     @05,@PC00001                                        0185 00154000
         CLC   TPICSR(7,@10),CAUSER(@05)                           0185 00155000
         BNE   @RF00185                                            0185 00156000
*                     DO;           /* YES THEN                      */ 00157000
*                                                                  0186 00158000
*                       /*********************************************/ 00159000
*                       /*                                           */ 00160000
*                       /* DETERMINE WHICH PASS OF THE GTA FILE IS   */ 00161000
*                       /* BEING DONE. FOR THE FIRST, CALL CRWENVIR  */ 00162000
*                       /* TO PROCESS THE ENVIRONMENT RECORDS ON THE */ 00163000
*                       /* (A)CRQ DATASET, IF THE FMID FOR THIS      */ 00164000
*                       /* CAUSER HAS NOT ALREADY BEEN PROCESSED. IF */ 00165000
*                       /* THE SECOND PASS OF THE GTA, THEN CALL     */ 00166000
*                       /* CRWCAUSR TO PROCESS THE CAUSER RECORDS ON */ 00167000
*                       /* THE (A)CRQ.                               */ 00168000
*                       /*                                           */ 00169000
*                       /*********************************************/ 00170000
*                                                                  0187 00171000
*                       IF X=1&TPIFMID^=LASTFMID THEN/* PASS FOR   0187 00172000
*                                      ENVIRONMENT ENTRY, AND THIS 0187 00173000
*                                      FMID HAS NOT ALREADY BEEN   0187 00174000
*                                      PROCESSED FOR THIS CAUSER   0187 00175000
*                                      SYSMOD, THEN PROCESS IT       */ 00176000
         CH    X,@CH00086                                          0187 00177000
         BNE   @RF00187                                            0187 00178000
         CLC   TPIFMID(7,@10),LASTFMID                             0187 00179000
         BE    @RF00187                                            0187 00180000
*                         CALL CRWENVIR;/* CALL CRWENVIR             */ 00181000
         BAL   @14,CRWENVIR                                        0188 00182000
*                       IF X=2 THEN /* SECOND PASS FOR CAUSER?       */ 00183000
@RF00187 CH    X,@CH00183                                          0189 00184000
         BNE   @RF00189                                            0189 00185000
*                         CALL CRWCAUSR;/* YES- CALL CRWCAUSR        */ 00186000
         BAL   @14,CRWCAUSR                                        0190 00187000
*                     END;          /* END OF SAME KEY LOOP          */ 00188000
*                   ELSE                                           0192 00189000
*                     GTPRETRN=GTPEOF;/* KEY HAS CHANGED SO SET END     00190000
*                                      OF FILE RC                    */ 00191000
         B     @RC00185                                            0192 00192000
@RF00185 L     @10,CCAIFGTP(,CCAPTR)                               0192 00193000
         MVI   GTPRETRN(@10),X'04'                                 0192 00194000
*                 END;              /* END OF NEXT RECORD FOUND LOOP */ 00195000
@RC00185 DS    0H                                                  0194 00196000
*             END;                  /* END OF FILE/KEY CHANGED LOOP  */ 00197000
*                                                                  0194 00198000
@RF00183 DS    0H                                                  0194 00199000
@DE00180 L     @10,CCAIFGTP(,CCAPTR)                               0194 00200000
         CLI   GTPRETRN(@10),0                                     0194 00201000
         BE    @DL00180                                            0194 00202000
*             /*******************************************************/ 00203000
*             /*                                                     */ 00204000
*             /* CHECK FOR X=1 AND DO STOW OF ENVIRONMENT RECORD IN  */ 00205000
*             /* BUFFER, WHEN END OF FILE/KEY CHANGE IS ENCOUNTERED  */ 00206000
*             /* AND PEMAX WAS NOT EXCEEDED IN HMASMVLU.             */ 00207000
*             /*                                                     */ 00208000
*             /*******************************************************/ 00209000
*                                                                  0195 00210000
*             IF GTPRETRN=GTPEOF    /* END OF FILE/KEY CHANGE HIT,   */ 00211000
*                 &X=1              /* AND ENVIRONMENT ENTRY LOOP,   */ 00212000
*                 &CRWRETRN<VLPPEEXC THEN/* AND PEMAX NOT EXCEEDED,  */ 00213000
         CLI   GTPRETRN(@10),4                                     0195 00214000
         BNE   @RF00195                                            0195 00215000
         CH    X,@CH00086                                          0195 00216000
         BNE   @RF00195                                            0195 00217000
         CH    CRWRETRN,@CH00111                                   0195 00218000
         BNL   @RF00195                                            0195 00219000
*               DO;                 /* THEN STOWR THE ONLY/LAST ENTRY*/ 00220000
*                 IOPFUNCT=IOPSTOWR;/* SET FOR STOW REPLACE OPER     */ 00221000
         MVI   IOPFUNCT(IOPPTR),X'54'                              0197 00222000
*                 CALL HMASMIO(HMASMIOP);/* CALL IO TO DO STOWR      */ 00223000
         ST    IOPPTR,@AL00001                                     0198 00224000
         L     @15,@CV01289                                        0198 00225000
         LA    @01,@AL00001                                        0198 00226000
         BALR  @14,@15                                             0198 00227000
*                 CRWRETRN=MAX(CRWRETRN,IOPRETRN);                 0199 00228000
         SLR   @10,@10                                             0199 00229000
         IC    @10,IOPRETRN(,IOPPTR)                               0199 00230000
         CR    CRWRETRN,@10                                        0199 00231000
         BNL   *+6                                                      00232000
         LR    CRWRETRN,@10                                        0199 00233000
*               END;                /* END OF STOWR LOOP             */ 00234000
*                                                                  0200 00235000
*             /*******************************************************/ 00236000
*             /*                                                     */ 00237000
*             /* CHECK FOR X=2 AND DO THE WRITE OF THE LAST RECORD IN*/ 00238000
*             /* THE BUFFER AND THEN DO THE BUILD AND STOW OF        */ 00239000
*             /* DIRECTORY ENTRY FOR THIS CAUSER WHEN END OF FILE/KEY*/ 00240000
*             /* CHANGE IS ENCOUNTERED.                              */ 00241000
*             /*                                                     */ 00242000
*             /*******************************************************/ 00243000
*                                                                  0201 00244000
*             IF GTPRETRN=GTPEOF    /* END OF FILE/KEY CHANGE HIT,   */ 00245000
*                 &X=2 THEN         /* AND CAUSER ENTRY LOOP,        */ 00246000
@RF00195 L     @10,CCAIFGTP(,CCAPTR)                               0201 00247000
         CLI   GTPRETRN(@10),4                                     0201 00248000
         BNE   @RF00201                                            0201 00249000
         CH    X,@CH00183                                          0201 00250000
         BNE   @RF00201                                            0201 00251000
*               DO;                 /* THEN WRITE LAST RECORD IN BUFR*/ 00252000
*                 IOPFUNCT=IOPWRITE;/* SET FOR WRITE OPERATION       */ 00253000
         MVI   IOPFUNCT(IOPPTR),X'51'                              0203 00254000
*                 IOPTYPE=IOPCSR;   /* ... TO CAUSER MEMBER          */ 00255000
         MVI   IOPTYPE(IOPPTR),X'0F'                               0204 00256000
*                 IOPNAM17=CAUSER;  /* ... CAUSER MEMBER NAME        */ 00257000
         L     @10,@PC00001                                        0205 00258000
         MVC   IOPNAM17(7,IOPPTR),CAUSER(@10)                      0205 00259000
*                 CALL HMASMIO(HMASMIOP);/* CALL IO TO DO WRITE      */ 00260000
         ST    IOPPTR,@AL00001                                     0206 00261000
         L     @15,@CV01289                                        0206 00262000
         LA    @01,@AL00001                                        0206 00263000
         BALR  @14,@15                                             0206 00264000
*                 IF IOPRETRN=IOPGOOD THEN/* WAS WRITE OPER OK?      */ 00265000
         CLI   IOPRETRN(IOPPTR),0                                  0207 00266000
         BNE   @RF00207                                            0207 00267000
*                   DO;             /* YES- THEN BUILD/STOWR DIR     */ 00268000
*                     IOPNAM17=CAUSER;/* CAUSER SYSMOD NAME          */ 00269000
         L     @10,@PC00001                                        0209 00270000
         MVC   IOPNAM17(7,IOPPTR),CAUSER(@10)                      0209 00271000
*                     IOPTYPE=IOPCSR;/* CAUSER TYPE ENTRY            */ 00272000
         MVI   IOPTYPE(IOPPTR),X'0F'                               0210 00273000
*                     IOPQSMNT=''B; /* ZERO OUT FIELD                */ 00274000
         XC    IOPQSMNT(2,IOPPTR),IOPQSMNT(IOPPTR)                 0211 00275000
*                     IOPFUNCT=IOPSTOWR;/* SET FOR STOW REPLACE OPER */ 00276000
         MVI   IOPFUNCT(IOPPTR),X'54'                              0212 00277000
*                     CALL HMASMIO(HMASMIOP);/* CALL IO TO DO STOWR  */ 00278000
         ST    IOPPTR,@AL00001                                     0213 00279000
         L     @15,@CV01289                                        0213 00280000
         LA    @01,@AL00001                                        0213 00281000
         BALR  @14,@15                                             0213 00282000
*                   END;            /* END OF BUILD/STOWR LOOP       */ 00283000
*                 CRWRETRN=MAX(CRWRETRN,IOPRETRN);                 0215 00284000
@RF00207 SLR   @10,@10                                             0215 00285000
         IC    @10,IOPRETRN(,IOPPTR)                               0215 00286000
         CR    CRWRETRN,@10                                        0215 00287000
         BNL   *+6                                                      00288000
         LR    CRWRETRN,@10                                        0215 00289000
*               END;                /* END OF WRITE LAST RECORD LOOP */ 00290000
*           END;                    /* END OF GOOD RESET GETN LOOP   */ 00291000
@RF00201 DS    0H                                                  0218 00292000
*       END;                        /* END OF GOOD LOCATE LOOP       */ 00293000
@RF00178 DS    0H                                                  0219 00294000
*     LASTFMID=' ';                 /* RESET FMID SAVE FOR PASS 2    */ 00295000
@RF00174 MVI   LASTFMID+1,C' '                                     0219 00296000
         MVC   LASTFMID+2(5),LASTFMID+1                            0219 00297000
         MVI   LASTFMID,C' '                                       0219 00298000
*   END;                            /* END OF READ GTA TWICE LOOP    */ 00299000
         AH    X,@CH00086                                          0220 00300000
         CH    X,@CH00183                                          0220 00301000
         BNH   @DL00166                                            0220 00302000
@DC00166 DS    0H                                                  0221 00303000
*   RETURN CODE(CRWRETRN);          /* RETURN TO CALLER WITH HIGHEST    00304000
*                                      RETURN CODE ENCOUNTERED       */ 00305000
         LR    @15,CRWRETRN                                        0221 00306000
         L     @13,4(,@13)                                         0221 00307000
         L     @14,12(,@13)                                        0221 00308000
         LM    @00,@12,20(@13)                                     0221 00309000
         BR    @14                                                 0221 00310000
*                                                                  0222 00311000
*   /*****************************************************************/ 00312000
*   /*                                                               */ 00313000
*   /* NOTE: THIS PROCEDURE IS ENTERED ONLY ONCE PER FMID.           */ 00314000
*   /*                                                               */ 00315000
*   /*****************************************************************/ 00316000
*                                                                  0222 00317000
*CRWENVIR:                                                         0222 00318000
*   PROC;                           /*                               */ 00319000
*                                                                  0222 00320000
@EL00001 L     @13,4(,@13)                                         0222 00321000
@EF00001 DS    0H                                                  0222 00322000
@ER00001 LM    @14,@12,12(@13)                                     0222 00323000
         BR    @14                                                 0222 00324000
CRWENVIR STM   @14,@02,@SA00002                                    0222 00325000
         STM   @04,@12,@SA00002+20                                 0222 00326000
*   /*****************************************************************/ 00327000
*   /*                                                               */ 00328000
*   /* THIS PROCEDURE HANDLES THE FIRST PASS OF THE GTA FILE BY      */ 00329000
*   /* EXTRACTING THE ENVIRONMENTS MENTIONED IN THE CAUSER SYSMOD    */ 00330000
*   /* PASSED, AND ADDING THAT CAUSER TO AN EXISTING ENVIRONMENT TYPE*/ 00331000
*   /* (A)CRQ RECORD, OR CREATING ONE IF NONE EXISTS.                */ 00332000
*   /*                                                               */ 00333000
*   /*****************************************************************/ 00334000
*                                                                  0223 00335000
*   IF LASTFMID^=BLANKS THEN        /* IS THIS FIRST PASS?           */ 00336000
*                                                                  0223 00337000
         CLC   LASTFMID(7),@CC01293                                0223 00338000
         BE    @RF00223                                            0223 00339000
*     /***************************************************************/ 00340000
*     /*                                                             */ 00341000
*     /* FOR THE FIRST PASS THROUGH THIS ROUTINE, THERE WILL BE NO   */ 00342000
*     /* ENVIRONMENT ENTRY IN THE BUFFER TO STOWR, SO BYPASS THE     */ 00343000
*     /* STOW. IF NOT THE FIRST PASS, THEN THE FMID HAS CHANGED, SO  */ 00344000
*     /* DO A STOWR OF THE PREVIOUS ENVIRONMENT ENTRY THAT IS IN THE */ 00345000
*     /* BUFFER AND THEN PROCESS THE CURRENT ENVIRONMENT ENTRY.      */ 00346000
*     /*                                                             */ 00347000
*     /***************************************************************/ 00348000
*                                                                  0224 00349000
*     DO;                           /* NO- THEN ENTRY IN BUFFER      */ 00350000
*       IOPFUNCT=IOPSTOWR;          /* SET FUNCTION TO STOWR         */ 00351000
         MVI   IOPFUNCT(IOPPTR),X'54'                              0225 00352000
*       CALL HMASMIO(HMASMIOP);     /* CALL IO TO DO STOWR OPER      */ 00353000
         ST    IOPPTR,@AL00001                                     0226 00354000
         L     @15,@CV01289                                        0226 00355000
         LA    @01,@AL00001                                        0226 00356000
         BALR  @14,@15                                             0226 00357000
*       CRWRETRN=MAX(CRWRETRN,IOPRETRN);/* PICK UP THE HIGHEST RC    */ 00358000
         SLR   @10,@10                                             0227 00359000
         IC    @10,IOPRETRN(,IOPPTR)                               0227 00360000
         CR    CRWRETRN,@10                                        0227 00361000
         BNL   *+6                                                      00362000
         LR    CRWRETRN,@10                                        0227 00363000
*     END;                          /* END OF ENTRY IN BUFFER LOOP   */ 00364000
*                                                                  0228 00365000
*   /*****************************************************************/ 00366000
*   /*                                                               */ 00367000
*   /* IF THE RETURN CODE COLLECTOR IS OK, THEN ATTEMPT TO FIND AN   */ 00368000
*   /* EXISTING ENVIRONMENT ENTRY ON THE (A)CRQ.                     */ 00369000
*   /*                                                               */ 00370000
*   /*****************************************************************/ 00371000
*                                                                  0229 00372000
*   IF CRWRETRN=IOPGOOD THEN        /* IS RC COLLECTOR OK?           */ 00373000
@RF00223 LTR   CRWRETRN,CRWRETRN                                   0229 00374000
         BNZ   @RF00229                                            0229 00375000
*     DO;                           /* YES- THEN                     */ 00376000
*       LASTFMID=TPIFMID;           /* SAVE CURRENT FMID             */ 00377000
         L     @10,CCAIFGTP(,CCAPTR)                               0231 00378000
         L     @10,GTPBUFAD(,@10)                                  0231 00379000
         MVC   LASTFMID(7),TPIFMID(@10)                            0231 00380000
*       IOPFUNCT=IOPLOC;            /* SET FUNCTION TO LOCATE        */ 00381000
         MVI   IOPFUNCT(IOPPTR),X'03'                              0232 00382000
*       IOPTYPE=IOPFMD;             /* ENVIRONMENT TYPE ENTRY        */ 00383000
         MVI   IOPTYPE(IOPPTR),X'0E'                               0233 00384000
*       IOPNAM17=TPIFMID;           /* FMID ENVIRONMENT SYSMOD NAME  */ 00385000
         MVC   IOPNAM17(7,IOPPTR),TPIFMID(@10)                     0234 00386000
*       CALL HMASMIO(HMASMIOP);     /* CALL IO TO DO THE LOCATE      */ 00387000
         ST    IOPPTR,@AL00001                                     0235 00388000
         L     @15,@CV01289                                        0235 00389000
         LA    @01,@AL00001                                        0235 00390000
         BALR  @14,@15                                             0235 00391000
*       IF IOPRETRN=IOPNTFND THEN   /* DOES AN ENTRY ALREADY EXIST?  */ 00392000
         CLI   IOPRETRN(IOPPTR),4                                  0236 00393000
         BNE   @RF00236                                            0236 00394000
*         DO;                       /* NO- THEN SET UP TO BUILD ONE  */ 00395000
*                                                                  0237 00396000
*           /*********************************************************/ 00397000
*           /*                                                       */ 00398000
*           /* NO ENTRY EXISTS, SO BUILD A END OF LIST VARIABLE      */ 00399000
*           /* ENTRY. THE DIRECTORY DATA REQUIRED FOR A STOWR ALREADY*/ 00400000
*           /* FILLED IN.                                            */ 00401000
*           /*                                                       */ 00402000
*           /*********************************************************/ 00403000
*                                                                  0238 00404000
*           IOPQFMNT=''B;           /* ZERO OUT FIELD                */ 00405000
