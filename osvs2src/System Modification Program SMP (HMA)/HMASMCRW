*         CRWRETRN=VLPGOOD;         /* NO- RESET RC=0                */ 00460000
         SLR   CRWRETRN,CRWRETRN                                   0255 00461000
*       ELSE                                                       0256 00462000
*         CRWRETRN=MAX(CRWRETRN,VLPRETRN);/* YES- PICK UP PEMAX RC   */ 00463000
