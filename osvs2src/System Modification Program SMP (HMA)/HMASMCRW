         BR    @14                                                 0258 00476000
*CRWCAUSR:                                                         0259 00477000
*   PROC;                           /*                               */ 00478000
*                                                                  0259 00479000
CRWCAUSR STM   @14,@02,@SA00003                                    0259 00480000
         STM   @04,@12,@SA00003+20                                 0259 00481000
*   /*****************************************************************/ 00482000
*   /*                                                               */ 00483000
*   /* THIS PROCEDURE HANDLES THE SECOND PASS OF THE GTA FILE BY     */ 00484000
*   /* EXTRACTING THE REQUISITE SYSMODS REQUIRED SHOULD THIS FMID BE */ 00485000
*   /* INSTALLED, AND WRITING THE RECORDS TO REFLECT THE FMID AND    */ 00486000
*   /* REQUISITE IN THE DATA RECORDS FOR THE CAUSER ENTRY. IF A      */ 00487000
*   /* MEMBER EXISTS ALREADY, IT IS IGNORED. A NEW MEMBER WILL BE    */ 00488000
*   /* WRITTEN AND STOWED TO REPLACE THE CURRENT MEMBER.             */ 00489000
*   /*                                                               */ 00490000
*   /*****************************************************************/ 00491000
*                                                                  0260 00492000
*   IF LASTFMID^=TPIFMID THEN       /* FMID NOT THE SAME AS LAST?    */ 00493000
         L     @10,CCAIFGTP(,CCAPTR)                               0260 00494000
         L     @10,GTPBUFAD(,@10)                                  0260 00495000
         CLC   LASTFMID(7),TPIFMID(@10)                            0260 00496000
         BE    @RF00260                                            0260 00497000
*     DO;                           /* NO- THEN                      */ 00498000
*       IF LASTFMID=BLANKS THEN     /* FIRST PASS-I.E.- EMPTY IOPBUFR*/ 00499000
         CLC   LASTFMID(7),@CC01293                                0262 00500000
         BNE   @RF00262                                            0262 00501000
*         DO;                       /* YES- THEN                     */ 00502000
*           LASTFMID=TPIFMID;       /* SET UP FMID WITH CURRENT FMID */ 00503000
         MVC   LASTFMID(7),TPIFMID(@10)                            0264 00504000
*           IOPQSMIF=''B;           /* ZERO FMID & VARIABLE ENTRIES  */ 00505000
         L     @15,IOPBUFAD(,IOPPTR)                               0265 00506000
         XC    IOPQSMIF(80,@15),IOPQSMIF(@15)                      0265 00507000
*           IOPQSMFM=TPIFMID;       /* FMID TO FMID FIXED FIELD      */ 00508000
         MVC   IOPQSMFM(7,@15),TPIFMID(@10)                        0266 00509000
*           IOPQSMNM(1)=TPIREQ;     /* REQUISITE SYSMOD TO VARIABLE  */ 00510000
