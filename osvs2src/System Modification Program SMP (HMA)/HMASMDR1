*DR1FNCST:                          /*                               */ 00310000
*   PROC;                           /*                               */ 00311000
DR1FNCST STM   @14,@12,12(@13)                                     0228 00312000
*   DECLARE                         /*                               */ 00313000
*     FNCRC FIXED(8);               /*                               */ 00314000
*   FNCRC=ZERO;                     /*                               */ 00315000
         SLR   FNCRC,FNCRC                                         0230 00316000
*   IF CCAEOSMP=ON                  /* IF END OF SMP INDICATED       */ 00317000
