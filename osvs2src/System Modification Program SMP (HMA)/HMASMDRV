*   ENTRY;                          /*                       @Z40SP00*/ 03148000
MAINRCRT DS    0H                                                  1030 03149000
*   RCENTRD=ON;                     /* SET SW TO INDICATE THAT RC  1030 03150000
*                                      PARM WAS ENTERED AND THAT USER   03151000
*                                      NOW COMPLETE CONTROL OVER   1030 03152000
*                                      RETURN CODE CHECKING TO     1030 03153000
*                                      DETERMINE FLUSH CONDITIONS  1030 03154000
*                                                            @Z40SP00*/ 03155000
         OI    RCENTRD,B'00100000'                                 1030 03156000
*   IF SCPPMLN=1                    /* IF ONLY ONE CHAR      @Z40SP00*/ 03157000
*     THEN                          /* ENTERED FOR RC VALUE  @Z40SP00*/ 03158000
         CLC   SCPPMLN(2),@CH00091                                 1031 03159000
         BNE   @RF01031                                            1031 03160000
*     DO;                           /* INIT CONVERSION AREA  @Z40SP00*/ 03161000
*       CNVRCCD=TWOCHAR0;           /* AND THEN MOVE IN 1    @Z40SP00*/ 03162000
