         TITLE 'HMASMDRV - SMP DRIVER ROUTINE'                          00001000
HMASMDRV CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMDRV  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  LA    @10,4095(,@12)                                      0001 00011000
         LA    @09,4095(,@10)                                      0001 00012000
         USING @PSTART,@12                                         0001 00013000
         USING @PSTART+4095,@10                                    0001 00014000
         USING @PSTART+8190,@09                                    0001 00015000
         ST    @13,@SA00001+4                                      0001 00016000
         LA    @14,@SA00001                                        0001 00017000
         ST    @14,8(,@13)                                         0001 00018000
         LR    @13,@14                                             0001 00019000
         MVC   @PC00001(4),0(@01)                                  0001 00020000
*   DRVRTNCD=ZERO;                  /*                       @Z67SP00*/ 00021000
         MVI   DRVRTNCD,X'00'                                      0254 00022000
*   IOPPTR=ADDR(DRVIOP);            /* PTR TO LOCAL IOP      @Z67SP00*/ 00023000
         LA    IOPPTR,DRVIOP                                       0255 00024000
*   CCAPTR=ADDR(HMASMCCA);          /* SET ADDR OF CCA IN REG        */ 00025000
         L     CCAPTR,@CA00042                                     0256 00026000
*   DO I=1 TO LENGTH(HMASMCCA);     /* ZERO CCA                      */ 00027000
         LA    @08,1                                               0257 00028000
         ST    @08,I                                               0257 00029000
@DL00257 DS    0H                                                  0258 00030000
*     HMASMCCA(I)=ZERO;             /*                               */ 00031000
         L     @06,@CA00042                                        0258 00032000
         SLR   @15,@15                                             0258 00033000
         BCTR  @06,0                                               0258 00034000
         STC   @15,HMASMCCA-HMASMCCA(@08,@06)                      0258 00035000
*   END;                            /*                               */ 00036000
         AH    @08,@CH00091                                        0259 00037000
         ST    @08,I                                               0259 00038000
         CH    @08,@CH02556                                        0259 00039000
         BNH   @DL00257                                            0259 00040000
*   CCAIOPCS=ADDR(CDSSYS);          /* CDS SYSTEM IOP        @Z67SP00*/ 00041000
         L     @08,@CA00042                                        0260 00042000
         LA    @06,CDSSYS                                          0260 00043000
         ST    @06,CCAIOPCS-HMASMCCA(,@08)                         0260 00044000
*   CCAIOPCS->IOPBUFAD=ADDR(BUFFRIOP);/* BUFFER AREA         @Z67SP00*/ 00045000
         LA    @15,BUFFRIOP                                        0261 00046000
         ST    @15,IOPBUFAD(,@06)                                  0261 00047000
*   CCAIOPAS=ADDR(ACDSSYS);         /* ACDS YSSTEM IOP       @Z67SP00*/ 00048000
         LA    @06,ACDSSYS                                         0262 00049000
         ST    @06,CCAIOPAS-HMASMCCA(,@08)                         0262 00050000
*   CCAIOPAS->IOPBUFAD=ADDR(BUFFRIOP);/* BUFFER AREA         @Z67SP00*/ 00051000
         ST    @15,IOPBUFAD(,@06)                                  0263 00052000
*   CCAIOPTR=ADDR(DRVIOP);          /* TEMP IOP              @Z67SP00*/ 00053000
         LA    @06,DRVIOP                                          0264 00054000
         ST    @06,CCAIOPTR-HMASMCCA(,@08)                         0264 00055000
*   CCAIOPTR->IOPBUFAD=ADDR(BUFFRIOP);/* BUFFER AREA         @Z67SP00*/ 00056000
         ST    @15,IOPBUFAD(,@06)                                  0265 00057000
*   SMPRCS=''B;                     /* ZERO RC AREA          @Z67SP00*/ 00058000
         XC    SMPRCS(11),SMPRCS                                   0266 00059000
*   HMASMMGP=''B;                   /* CLEAR MGP             @Z67SP00*/ 00060000
         XC    HMASMMGP(24),HMASMMGP                               0267 00061000
*   MGPPRINT=ON;                    /* ALL MSG TO PRINTER    @OS64458*/ 00062000
*   MGPHLDS=ON;                     /* AND TO LOG            @SA74333*/ 00063000
         OI    MGPPRINT,B'10010000'                                0269 00064000
*   HMASMCRP=''B;                   /* ZERO CRD PARM         @Z67SP00*/ 00065000
         XC    HMASMCRP(106),HMASMCRP                              0270 00066000
*   CRPINDS=IOPCONTR;               /* INPUT FROM SMPCNTL    @Z67SP00*/ 00067000
         MVI   CRPINDS,X'09'                                       0271 00068000
*   CRPPRINT=ON;                    /* WRITE TO SMPOUT       @Z67SP00*/ 00069000
         OI    CRPPRINT,B'00001000'                                0272 00070000
*   HMASMSCP=''B;                   /* ZERO SCP FIELDS       @Z67SP00*/ 00071000
         XC    HMASMSCP(26),HMASMSCP                               0273 00072000
*   HMASMSET=''B;                   /* INIT SET ENTRY        @Z67SP00*/ 00073000
         XC    HMASMSET(12),HMASMSET                               0274 00074000
*   NOJCLGTP=''B;                   /* ZERO NOJCLIN GTP      @Z67SP00*/ 00075000
         XC    NOJCLGTP(28),NOJCLGTP                               0275 00076000
*   DRVFLGS1=ZERO;                  /* INIT SMP FLAGS        @ZD00003*/ 00077000
         SLR   @08,@08                                             0276 00078000
         STH   @08,DRVFLGS1                                        0276 00079000
*   CALL HMASMDSU(DRVPARM);         /* DO SETUP PROCESSING   @Z67SP00*/ 00080000
         L     @08,@PC00001                                        0277 00081000
         ST    @08,@AL00001                                        0277 00082000
         L     @15,@CV01805                                        0277 00083000
         LA    @01,@AL00001                                        0277 00084000
         BALR  @14,@15                                             0277 00085000
*   RFY                                                            0278 00086000
*     RTNCODE RSTD;                 /*                       @Z67SP00*/ 00087000
*   IF CCAIOPTR^=ZERO               /* IF NEW IOP OBTAINED   @Z67SP00*/ 00088000
*     THEN                          /*                       @Z67SP00*/ 00089000
         L     @14,@CA00042                                        0279 00090000
         L     @14,CCAIOPTR-HMASMCCA(,@14)                         0279 00091000
         LTR   @14,@14                                             0279 00092000
         BZ    @RF00279                                            0279 00093000
*     IOPPTR=CCAIOPTR;              /* USE IT                @Z67SP00*/ 00094000
         LR    IOPPTR,@14                                          0280 00095000
*   ELSE                            /* IF NOT OBTAINED       @Z67SP00*/ 00096000
*     IOPPTR=ADDR(DRVIOP);          /* USE FIXED IOP         @Z67SP00*/ 00097000
         B     @RC00279                                            0281 00098000
@RF00279 LA    IOPPTR,DRVIOP                                       0281 00099000
*   RCSMP=MAX(RCSMP,RTNCODE);       /*                       @Z67SP00*/ 00100000
@RC00279 SLR   @14,@14                                             0282 00101000
         IC    @14,RCSMP                                           0282 00102000
         CR    @14,RTNCODE                                         0282 00103000
         BNL   *+6                                                      00104000
         LR    @14,RTNCODE                                         0282 00105000
         STC   @14,RCSMP                                           0282 00106000
*   IF RTNCODE>PTFWARN              /* IF NOT OK             @Z67SP00*/ 00107000
*     THEN                          /*                       @Z67SP00*/ 00108000
         CH    RTNCODE,@CH00044                                    0283 00109000
         BH    @RT00283                                            0283 00110000
*     GO TO FINAL;                  /*                       @Z67SP00*/ 00111000
*   RFY                                                            0285 00112000
*     RTNCODE UNRSTD;               /*                       @Z67SP00*/ 00113000
*   CCATBLPA=ADDR(TBLPARM);         /* PUT TBL PARM ADDR     @Z67SP00*/ 00114000
*                                                                  0286 00115000
         L     @08,@CA00042                                        0286 00116000
         LA    @06,TBLPARM                                         0286 00117000
         ST    @06,CCATBLPA-HMASMCCA(,@08)                         0286 00118000
*   /*****************************************************************/ 00119000
*   /*                                                               */ 00120000
*   /* CALL HMASMSTA TO SETUP A STAE ENVIRONMENT             @Z67SP00*/ 00121000
*   /*                                                               */ 00122000
*   /*****************************************************************/ 00123000
*                                                                  0287 00124000
*   CCARVEST=ON;                    /* ESTABLISH STAE        @Z67SP00*/ 00125000
         OI    CCARVEST-HMASMCCA(@08),B'00001000'                  0287 00126000
*   CALL HMASMSTA;                  /*                       @Z67SP00*/ 00127000
         L     @15,@CV01812                                        0288 00128000
         BALR  @14,@15                                             0288 00129000
*   RFY                                                            0289 00130000
*     RTNCODE RSTD;                 /*                       @Z67SP00*/ 00131000
*   CCARVEST=OFF;                   /* RESET SW              @Z67SP00*/ 00132000
         L     @14,@CA00042                                        0290 00133000
         NI    CCARVEST-HMASMCCA(@14),B'11110111'                  0290 00134000
*   IF RTNCODE>STOPPTF              /* IF STA FAILED         @Z67SP00*/ 00135000
*     THEN                          /*                       @Z67SP00*/ 00136000
         CH    RTNCODE,@CH00116                                    0291 00137000
         BNH   @RF00291                                            0291 00138000
*     DO;                           /*                       @Z67SP00*/ 00139000
*       RCSMP=RTNCODE;              /* SET SMP RC            @Z67SP00*/ 00140000
         STC   RTNCODE,RCSMP                                       0293 00141000
*       GO TO FINAL;                /*                       @Z67SP00*/ 00142000
         B     FINAL                                               0294 00143000
*     END;                          /*                       @Z67SP00*/ 00144000
*   RFY                                                            0296 00145000
*     RTNCODE UNRSTD;               /*                       @Z67SP00*/ 00146000
@RF00291 DS    0H                                                  0297 00147000
*                                                                  0297 00148000
*   /*****************************************************************/ 00149000
*   /*                                                               */ 00150000
*   /* READ AND SCAN CONTROL CARDS TO DETERMINE OPERATION SCAN       */ 00151000
*   /* RETURNS AT SUBROUTINES TO INVOKE THE VARIOUS SMP VERB         */ 00152000
*   /* PROCESSORS                                                    */ 00153000
*   /*                                                               */ 00154000
*   /*****************************************************************/ 00155000
*                                                                  0297 00156000
*DRVSCAN:                                                          0297 00157000
*   SCPIORTN=ADDR(HMASMCRD);        /* SET PTR TO I/O ROUTINE        */ 00158000
DRVSCAN  L     @08,@CV01802                                        0297 00159000
         ST    @08,SCPIORTN                                        0297 00160000
*   SCPIOPAD=ADDR(HMASMCRP);        /*                       @Z67SP00*/ 00161000
         LA    @08,HMASMCRP                                        0298 00162000
         ST    @08,SCPIOPAD                                        0298 00163000
*   SCPINLN=LENGTH(CRPBFCTL);       /* SET INPUT RECORD LENGTH       */ 00164000
         MVC   SCPINLN(2),@CH00836                                 0299 00165000
*   SCPEOR=ZERO;                    /* ZERO THE EOR FLAGS            */ 00166000
         MVI   SCPEOR,X'00'                                        0300 00167000
*   SCPCOMNT=ON;                    /* INDICATE COMMENTS OCCUR       */ 00168000
         OI    SCPCOMNT,B'00100000'                                0301 00169000
*   SCPSRCH=ADCTLSCN;               /* FIRST DSCAN           @Z40SP00*/ 00170000
         MVC   SCPSRCH(4),ADCTLSCN                                 0302 00171000
*   CCAFUNCT=UNKWN;                 /* FUNCTION UNKNOWN YET  @Z67SP00*/ 00172000
         L     @08,@CA00042                                        0303 00173000
         MVC   CCAFUNCT-HMASMCCA(8,@08),@CC01847                   0303 00174000
*   SAVECODE=ZERO;                  /* INIT FUNCTION RTN CD  @Z40SP00*/ 00175000
         SLR   @08,@08                                             0304 00176000
         ST    @08,SAVECODE                                        0304 00177000
*   DRVFLGS2=OFF;                   /* INIT PROCESS FLAGS    @Z67SP00*/ 00178000
         MVI   DRVFLGS2+1,X'00'                                    0305 00179000
         MVI   DRVFLGS2,B'00000000'                                0305 00180000
*   TBLPARM=''B;                    /* INIT PROCESSOR PARM   @Z67SP00*/ 00181000
         XC    TBLPARM(16),TBLPARM                                 0306 00182000
*   TBLMASS=ON;                     /* DEFAULT MODE IS MASS  @Z67SP00*/ 00183000
         OI    TBLMASS,B'00000001'                                 0307 00184000
*   LISTPARM=''B;                   /* INIT LIST PARM        @Z67SP00*/ 00185000
         XC    LISTPARM(19),LISTPARM                               0308 00186000
*   CRPSCPAD=ADDR(HMASMSCP);        /* PTR TO SCP            @Z67SP00*/ 00187000
         LA    @08,HMASMSCP                                        0309 00188000
         ST    @08,CRPSCPAD                                        0309 00189000
*   CRPLOG=ON;                      /* WRITE RCD TO LOG      @Z67SP00*/ 00190000
         OI    CRPLOG,B'00010000'                                  0310 00191000
*   CRPLIST=OFF;                    /* NO WRITE TO SMPLIST   @Z67SP00*/ 00192000
*   CRPRPT=OFF;                     /* NO WRITE TO SMPLIST   @Z67SP00*/ 00193000
         NI    CRPLIST,B'11100111'                                 0312 00194000
*   CRPSCNCL=OFF;                   /* SET FOR CALL FROM DRV @Z67SP00*/ 00195000
*   CRPLOGCK=ON;                    /* CHECK FOR LOG STMT    @Z67SP00*/ 00196000
         OI    CRPLOGCK,B'01000000'                                0314 00197000
         NI    CRPSCNCL,B'11111110'                                0314 00198000
*   CALL HMASMCRD(HMASMCRP);        /* READ AND PRINT A STATEMENT    */ 00199000
         L     @15,@CV01802                                        0315 00200000
         LA    @01,@AL00315                                        0315 00201000
         BALR  @14,@15                                             0315 00202000
*   CRPSCNCL=ON;                    /* SET FOR NOT FROM DRV  @Z67SP00*/ 00203000
*   CRPLOGCK=OFF;                   /* DONT CHECK FOR LOG    @Z67SP00*/ 00204000
         OI    CRPSCNCL,B'00000001'                                0317 00205000
         NI    CRPLOGCK,B'10111111'                                0317 00206000
*   IF CRPRETRN^=CRPGOOD            /* IF NEXT RCD NOT READ  @Z67SP00*/ 00207000
*     THEN                          /* THEN                  @Z40SP00*/ 00208000
         CLI   CRPRETRN,0                                          0318 00209000
         BE    @RF00318                                            0318 00210000
*     DO;                           /* CHECK TYPE RC         @Z67SP00*/ 00211000
*       IF CRPRETRN=CRPEOF          /* IF END OF FILE        @Z67SP00*/ 00212000
*         THEN                      /* THEN GO TO            @Z67SP00*/ 00213000
         CLI   CRPRETRN,4                                          0320 00214000
         BE    @RT00320                                            0320 00215000
*         GO TO CLEANUP;            /* CLEANUP PROCESS       @Z67SP00*/ 00216000
*       RCSMP=CRPRETRN;             /* MUST BE I/O OR DD MISSING ERR.   00217000
*                                      SET RC                @Z67SP00*/ 00218000
         MVC   RCSMP(1),CRPRETRN                                   0322 00219000
*       GO TO FINAL;                /* TERMINATE SMP         @Z67SP00*/ 00220000
         B     FINAL                                               0323 00221000
*     END;                          /*                       @YA07506*/ 00222000
*   CALL HMASMSCN(HMASMSCP);        /* SCAN CONTROL STATEMENT        */ 00223000
@RF00318 L     @15,@CV01817                                        0325 00224000
         LA    @01,@AL00325                                        0325 00225000
         BALR  @14,@15                                             0325 00226000
*   CRPSCNCL=OFF;                   /* SET FOR CALL FROM DRV. IN CASE   00227000
*                                      CHANGED IN ROUT       @Z67SP00*/ 00228000
         NI    CRPSCNCL,B'11111110'                                0326 00229000
*                                                                  0327 00230000
*   /*****************************************************************/ 00231000
*   /*                                                               */ 00232000
*   /* CHECK FOR INCOMPLETE CONTROL STATEMENT                @Z67SP00*/ 00233000
*   /*                                                               */ 00234000
*   /*****************************************************************/ 00235000
*                                                                  0327 00236000
*   IF SCPRET=SCPGOOD               /* NOT SYNTAX BUT HIT END OF     */ 00237000
*       &CRPEOFEN=ON                /* FILE BEFORE END OF STATEMENT  */ 00238000
*     THEN                          /* OK TO CONTINUE                */ 00239000
         CLI   SCPRET,0                                            0327 00240000
         BNE   @RF00327                                            0327 00241000
         TM    CRPEOFEN,B'10000000'                                0327 00242000
         BNO   @RF00327                                            0327 00243000
*     DO;                           /* ERROR - INCOMPLETE VERB       */ 00244000
*       INCPLSW=ON;                 /* INCOMPLETE STMT       @Z40SP00*/ 00245000
         OI    INCPLSW,B'01000000'                                 0329 00246000
*       MGPCLEAR=MGPCLEAR&&MGPCLEAR;/* ZERO MSG PARM         @Z40SP00*/ 00247000
         XC    MGPCLEAR(6),MGPCLEAR                                0330 00248000
*       MGPTYPE=MGPTERM;            /* SET SEVERITY          @ZD00001*/ 00249000
         MVI   MGPTYPE,X'05'                                       0331 00250000
*       MGPMGNO1=HMA252I;           /* INDICATE INCOMPLETE STMT      */ 00251000
         MVC   MGPMGNO1(2),@CH01774                                0332 00252000
*       CALL HMASMMSG(HMASMMGP);    /* WRITE MESSAGE                 */ 00253000
         L     @15,@CV01809                                        0333 00254000
         LA    @01,@AL00333                                        0333 00255000
         BALR  @14,@15                                             0333 00256000
*       SAVECODE=STOPSMP;           /* INDICATE ERROR        @ZD00009*/ 00257000
         MVC   SAVECODE(4),@CF00192                                0334 00258000
*       GO TO CLEANUP;              /* GO PUT OUT MSG FOR PROCESSING    00259000
*                                      COMPLETE AND STOP SMP @Z40SP00*/ 00260000
         B     CLEANUP                                             0335 00261000
*     END;                                                         0336 00262000
*                                                                  0336 00263000
*   /*****************************************************************/ 00264000
*   /*                                                               */ 00265000
*   /* CHECK FOR OUT OF STORAGE CONDITION                    @Z67SP00*/ 00266000
*   /*                                                               */ 00267000
*   /*****************************************************************/ 00268000
*                                                                  0337 00269000
*   IF M202SW=ON                    /*                       @Z67SP00*/ 00270000
*     THEN                          /*                       @Z67SP00*/ 00271000
@RF00327 TM    M202SW,B'00000010'                                  0337 00272000
         BNO   @RF00337                                            0337 00273000
*     DO;                           /*                       @Z67SP00*/ 00274000
*       HMASMMGP=''B;               /* INIT PARM             @Z67SP00*/ 00275000
         XC    HMASMMGP(24),HMASMMGP                               0339 00276000
*       MGPMGNO1=HMA202I;           /* UNABLE TO OBTAIN STORAGE FOR     00277000
*                                      WORK AREAS            @Z67SP00*/ 00278000
         MVC   MGPMGNO1(2),@CH00239                                0340 00279000
*       MGPTYPE=MGPSEV;             /*                       @Z67SP00*/ 00280000
         MVI   MGPTYPE,X'04'                                       0341 00281000
*       CALL HMASMMSG(HMASMMGP);    /*                       @Z67SP00*/ 00282000
         L     @15,@CV01809                                        0342 00283000
         LA    @01,@AL00342                                        0342 00284000
         BALR  @14,@15                                             0342 00285000
*       SAVECODE=STOPFUNC;          /*                       @Z67SP00*/ 00286000
         MVC   SAVECODE(4),@CF00033                                0343 00287000
*       GO TO CLEANUP;              /*                       @Z67SP00*/ 00288000
         B     CLEANUP                                             0344 00289000
*     END;                          /*                       @Z67SP00*/ 00290000
*                                                                  0346 00291000
*   /*****************************************************************/ 00292000
*   /*                                                               */ 00293000
*   /* IF ERROR IN SCANNING CARDS OR INVALID OPTIONS SPECIFIED THEN  */ 00294000
*   /* PUT OUT MSG IF NOT ALREADY DONE BY SCAN ROUT          @Z40SP00*/ 00295000
*   /*                                                               */ 00296000
*   /*****************************************************************/ 00297000
*                                                                  0346 00298000
*   IF SCPRET^=SCPGOOD              /* IS THERE AN ERROR?    @Z40SP00*/ 00299000
*     THEN                          /* YES - CHECK IT OUT            */ 00300000
@RF00337 CLI   SCPRET,0                                            0346 00301000
         BE    @RF00346                                            0346 00302000
*     DO;                                                          0347 00303000
*                                                                  0347 00304000
*       /*************************************************************/ 00305000
*       /*                                                           */ 00306000
*       /* AN ERROR HAS OCCURRED. IT MAY BE SYNTAX ERROR OR I/O ERROR*/ 00307000
*       /* IF RTNCODE HAS A VALUE, THEN THE CAUSE OF THE ERROR HAS   */ 00308000
*       /* ALREADY BEEN DETERMINED, OTHERWISE IT IS A SYNTAX ERROR IN*/ 00309000
*       /* A CONTROL STATEMENT.                                      */ 00310000
*       /*                                                           */ 00311000
*       /*************************************************************/ 00312000
*                                                                  0348 00313000
*       IF RTNERR=ON                /* HAS ERROR OCCURRED IN ROUT?   */ 00314000
*           |SCPRET=STOPSMP         /* OR TERMINATE          @YA07506*/ 00315000
*         THEN                      /* YES - NOT SYNTAX ERROR        */ 00316000
         TM    RTNERR,B'10000000'                                  0348 00317000
         BO    @RT00348                                            0348 00318000
         CLI   SCPRET,16                                           0348 00319000
         BNE   @RF00348                                            0348 00320000
@RT00348 DS    0H                                                  0349 00321000
*         DO;                                                      0349 00322000
*           RTNERR=OFF;             /* RESET ROUTINE SWITCH          */ 00323000
         NI    RTNERR,B'01111111'                                  0350 00324000
*           SAVECODE=MAX(STOPFUNC,SCPRET);/* SET RC FOR      @Z40SP00*/ 00325000
         SLR   @08,@08                                             0351 00326000
         IC    @08,SCPRET                                          0351 00327000
         LA    @06,12                                              0351 00328000
         CR    @08,@06                                             0351 00329000
         BNL   *+6                                                      00330000
         LR    @08,@06                                             0351 00331000
         ST    @08,SAVECODE                                        0351 00332000
*         END;                                                     0352 00333000
*       ELSE                        /*                       @Z67SP00*/ 00334000
*         DO;                       /*                       @Z67SP00*/ 00335000
         B     @RC00348                                            0353 00336000
@RF00348 DS    0H                                                  0354 00337000
*           ERRCOL=SCPCHAR-ADDR(CRPBUFFR)+1;/* CALC COLUMN FOR ERROR    00338000
*                                                            @Z67SP00*/ 00339000
         LA    @08,CRPBUFFR                                        0354 00340000
         LCR   @08,@08                                             0354 00341000
         AL    @08,SCPCHAR                                         0354 00342000
         AH    @08,@CH00091                                        0354 00343000
         STH   @08,ERRCOL                                          0354 00344000
*           CVD(ERRCOL,DBLWRD);     /* MAKE DECIMAL                  */ 00345000
         CVD   @08,DBLWRD                                          0355 00346000
*           UNPK(CHARCOL,DBLWRD);   /* MAKE EBCDIC                   */ 00347000
         UNPK  CHARCOL(2),DBLWRD(8)                                0356 00348000
*           LASTDIG=LASTDIG|MAKEPRNT;/* MAKE PRINTABLE               */ 00349000
         OI    LASTDIG,X'F0'                                       0357 00350000
*           MGPCLEAR=MGPCLEAR&&MGPCLEAR;/* ZERO MSG PARM     @Z40SP00*/ 00351000
         XC    MGPCLEAR(6),MGPCLEAR                                0358 00352000
*           MGPTYPE=MGPSEV;         /* SET SEVERITY          @ZD00001*/ 00353000
         MVI   MGPTYPE,X'04'                                       0359 00354000
*           MGPMGNO1=HMA203I;       /* INDICATE SYNTAX ERROR         */ 00355000
         MVC   MGPMGNO1(2),@CH00044                                0360 00356000
*           MGPMGNO2=PART021;       /* INDICATE DRIVER               */ 00357000
         MVC   MGPMGNO2(2),@CH00456                                0361 00358000
*           MGPMGNO3=PART026;       /* ADD IN COLUMN PART            */ 00359000
         MVC   MGPMGNO3(2),@CH00466                                0362 00360000
*           MGPVARPT(1)=ADDR(CHARCOL);/* POINT TO VARIABLE MESSAGE   */ 00361000
         LA    @08,CHARCOL                                         0363 00362000
         ST    @08,MGPVARPT                                        0363 00363000
*           CALL HMASMMSG(HMASMMGP);/* ISSUE MESSAGE                 */ 00364000
         L     @15,@CV01809                                        0364 00365000
         LA    @01,@AL00364                                        0364 00366000
         BALR  @14,@15                                             0364 00367000
*           IF ERRAFTPR=OFF         /* IF SYNTAX ERR AFTER THE PERIOD   00368000
*                                      IN CNTL CARD THEN NO  @Z40SP00*/ 00369000
*             THEN                  /* NEED TO SCAN FOR .    @Z40SP00*/ 00370000
         TM    ERRAFTPR,B'00001000'                                0365 00371000
         BNZ   @RF00365                                            0365 00372000
*             DO;                   /* ASSUMED TO BE ON NEXT LINE  0366 00373000
*                                      ELSE SYNTAX ERR AGAIN @Z40SP00*/ 00374000
*               SCPSRCH=ADSYNSCN;   /* PTR TO DSCAN TO SCAN FOR NEXT    00375000
*                                      PERIOD                @Z40SP00*/ 00376000
         MVC   SCPSRCH(4),ADSYNSCN                                 0367 00377000
*               SCPCHAR=ADDR(CRPBUFFR);/* RESET PTR TO START OF    0368 00378000
*                                      STRING IN CASE SYNTAX AT END     00379000
