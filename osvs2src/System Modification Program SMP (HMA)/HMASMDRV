*   IF RCENT>=STOPSMP               /* VALUE SPECIFIED GREATER OR  1039 03180000
*                                      EQUAL 16 THEN SYNTAX ERR    1039 03181000
*                                                            @Z67SP00*/ 03182000
*     THEN                          /* SO STOP SCAN          @YA07506*/ 03183000
         CLI   RCENT,16                                            1039 03184000
         BL    @RF01039                                            1039 03185000
*BADRCRT:                           /*                       @Z40SP00*/ 03186000
*     SCPRET=SCPSTOP;               /* STOP SCAN             @Z40SP00*/ 03187000
