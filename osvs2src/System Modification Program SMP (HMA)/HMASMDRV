*                                                                  0813 02245000
*   /*****************************************************************/ 02246000
*   /*                                                               */ 02247000
*   /* THE REST OF THE HMASMDRV MODULE CONSISTS OF DSCAN ROUTS. THESE*/ 02248000
*   /* ROUTS WILL BE CALLED FROM HMASMSCN WHENEVER THE KEY IS FOUND  */ 02249000
*   /* AND THE ROUT IS SPECIFIED. REGISTER ONE WILL BE RESTRICTED    */ 02250000
*   /* SINCE HMASMSCN DEPENDS UPON REGISTER ONE NOT BEING CHANGED. IT*/ 02251000
*   /* KEEPS THE ADDRESS OF ITS SAVE AREA IN AN AREA IN THE HMASMSCP */ 02252000
*   /* AND REGISTER ONE POINTS TO THE HMASMSCP               @Z40SP00*/ 02253000
*   /* THE RETURN REG(REG14) IS ALSO RESTRICTED SO THAT SAVE AREAS   */ 02254000
*   /* WILL NOT BE NEEDED FOR EACH PROC                      @Z40SP00*/ 02255000
*   /*                                                               */ 02256000
*   /*****************************************************************/ 02257000
*                                                                  0813 02258000
*   RFY                                                            0813 02259000
*     R1 RSTD;                      /*                       @Z40SP00*/ 02260000
*   RFY                                                            0814 02261000
*     RTNREG RSTD;                  /*                       @Z40SP00*/ 02262000
*                                                                  0815 02263000
*   /*****************************************************************/ 02264000
*   /*                                                               */ 02265000
*   /* THE FOLLOWING ROUTINES ARE INVOKED WHEN SCAN ENCOUNTERS THE   */ 02266000
*   /* ASSOCIATED CONTROL VERB IN THE INPUT STREAM.                  */ 02267000
*   /*                                                               */ 02268000
*   /*****************************************************************/ 02269000
*                                                                  0815 02270000
*VERBSETS:                                                         0815 02271000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @Z40SP00*/ 02272000
VERBSETS DS    0H                                                  0816 02273000
*   DECLARE                         /*                       @Z67SP00*/ 02274000
*     SAVCHAR PTR(31);              /* SAVE FOR SCPCHAR      @Z67SP00*/ 02275000
*                                                                  0816 02276000
*   /*****************************************************************/ 02277000
*   /*                                                               */ 02278000
*   /* THIS ROUTINE IS INVOKED FOR RECEIVE OPERATIONS                */ 02279000
*   /*                                                               */ 02280000
*   /*****************************************************************/ 02281000
*                                                                  0817 02282000
*RECSAV:                                                           0817 02283000
*   ENTRY;                                                         0817 02284000
         B     @EC00817                                            0817 02285000
RECSAV   DS    0H                                                  0817 02286000
@EC00817 DS    0H                                                  0818 02287000
*   CCAREC=ON;                      /* INDICATE RECEIVE VERB         */ 02288000
         L     @15,@CA00042                                        0818 02289000
         OI    CCAREC-HMASMCCA(@15),B'10000000'                    0818 02290000
*   CCAFUNCT=TXTREC;                /* PUT FUNCTION ID IN    @Z67SP00*/ 02291000
         MVC   CCAFUNCT-HMASMCCA(8,@15),@CC01833                   0819 02292000
*   GO TO VERBEXIT;                 /* RETURN                        */ 02293000
*                                                                  0820 02294000
         B     VERBEXIT                                            0820 02295000
*   /*****************************************************************/ 02296000
*   /*                                                               */ 02297000
*   /* THIS ROUTINE IS INVOKED FOR THE APPLY VERB                    */ 02298000
*   /*                                                               */ 02299000
