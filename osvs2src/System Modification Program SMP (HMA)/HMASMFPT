*FPTCNV:                            /*                               */ 01210000
*   PROCEDURE(CNVDATE,CNVTIME);     /*                               */ 01211000
FPTCNV   STM   @14,@12,@SA00010                                    0454 01212000
         MVC   @PC00010(8),0(@01)                                  0454 01213000
*   DECLARE                         /*                               */ 01214000
*     CNVDATE CHAR(3),              /*                               */ 01215000
*     CNVTIME CHAR(3);              /*                               */ 01216000
*   DECLARE                         /*                               */ 01217000
*     NULL CHAR(3) INIT('000000'X); /*                               */ 01218000
*   DECLARE                         /*                               */ 01219000
*     1 DATEWORK CHAR(5),           /* TO UNPACK DATE                */ 01220000
