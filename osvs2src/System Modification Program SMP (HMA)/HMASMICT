*/*                                   IOPPFUNC FUNCTION      @Z67SP00*/ 00622314
                                                                        00623314
       2 ICTPONUM FIXED(16) BDY(HWORD),/* PTF PROCESSING ORDER #        00624514
                                                             @Z67SP00*/ 00625114
       2 ICTPSUPN FIXED(16) BDY(HWORD),/* COUNT OF PTF ENTRIES THAT     00625714
                                       SUP THIS ENTRY        @Z67SP00*/ 00626314
       2 ICTPFMID CHAR(7),          /* FMID FOR THIS PTF     @Z67SP00*/ 00626914
       2 ICTPVERN FIXED(8),         /* NUMBER OF VERIFY STATEMENT       00627514
                                       USED TO PROCESS THIS SYSMOD      00627614
                                                             @Z67SP00*/ 00627714
       2 ICTPRECD CHAR(3),          /* PTF RECIEVED DATE     @Z67SP00*/ 00627814
       2 ICTPRECT CHAR(3),          /* PTF RECIEVED TIME     @Z67SP00*/ 00627914
       2 ICTPRELN FIXED(15) BDY(BYTE), /* NUMBER OF RELFILES @Z67SP00*/ 00628014
       2 ICTPTPFX CHAR(26),/* RELFILE DSN PREFIX             @Z67SP00*/ 00628614
       2 ICTPCHN PTR(31) BDY(WORD), /* CHAIN PTR TO INDEX EXTENSION  */ 00628714
       2 ICTPRCHN PTR(31) BDY(BYTE),/* PTR TO REQ/MREQ LIST  @ZA14010*/ 00629314
