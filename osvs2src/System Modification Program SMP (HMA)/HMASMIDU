         TITLE 'HMASMIDU - HMASMP MODID UPDATE ROUTINE'                 00001000
HMASMIDU CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMIDU  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @09,0                                               0001 00010000
@PSTART  LA    @10,4095(,@09)                                      0001 00011000
         USING @PSTART,@09                                         0001 00012000
         USING @PSTART+4095,@10                                    0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
         MVC   @PC00001(4),0(@01)                                  0001 00018000
*                                                                  0198 00019000
*   /*****************************************************************/ 00020000
*   /*                                                               */ 00021000
*   /* BEGIN MODID UPDATE PROCESSING                                 */ 00022000
*   /*                                                               */ 00023000
*   /*****************************************************************/ 00024000
*                                                                  0199 00025000
*   MAINRC=0;                       /* INITIALIZE RETURN CODE VALUE  */ 00026000
         SLR   @12,@12                                             0199 00027000
         ST    @12,MAINRC                                          0199 00028000
*   HMASMMGP=''B;                   /* CLEAR MSG PARM LIST   @Z67SP00*/ 00029000
*                                                                  0200 00030000
         XC    HMASMMGP(48),HMASMMGP                               0200 00031000
*   /*****************************************************************/ 00032000
*   /*                                                               */ 00033000
*   /* INITAILIZE IOP FOR ACCESS TO CDS OR ACDS ELEMENT ENTRIES      */ 00034000
*   /*                                                       @Z67SP00*/ 00035000
*   /*                                                               */ 00036000
*   /*****************************************************************/ 00037000
*                                                                  0201 00038000
*   IOPPTR=CCAIOPTR;                /* USE IOP FROM CCA              */ 00039000
         L     @12,CCAIOPTR(,CCAPTR)                               0201 00040000
         ST    @12,IOPPTR                                          0201 00041000
*   IF CCAAPPLY=ON                  /* APPLY                 @Z67SP00*/ 00042000
*       |CCARES=ON                  /* RESTORE               @Z67SP00*/ 00043000
*     THEN                                                         0202 00044000
         TM    CCAAPPLY(CCAPTR),B'01001000'                        0202 00045000
         BZ    @RF00202                                            0202 00046000
*     IOPDSID=IOPCDS;               /* ... UPDATE CDS        @Z67SP00*/ 00047000
         MVI   IOPDSID(@12),X'02'                                  0203 00048000
*   ELSE                                                           0204 00049000
*     IF CCAACCPT=ON                /* ACCEPT                @Z67SP00*/ 00050000
*       THEN                                                       0204 00051000
         B     @RC00202                                            0204 00052000
@RF00202 TM    CCAACCPT(CCAPTR),B'00100000'                        0204 00053000
         BNO   @RF00204                                            0204 00054000
*       IOPDSID=IOPACDS;            /* ... UPDATE ACDS       @Z67SP00*/ 00055000
         L     @12,IOPPTR                                          0205 00056000
         MVI   IOPDSID(@12),X'0C'                                  0205 00057000
*     ELSE                                                         0206 00058000
*       RETURN CODE(0);             /* ... RETURN TO CALLER  @Z67SP00*/ 00059000
         B     @RC00204                                            0206 00060000
@RF00204 SLR   @15,@15                                             0206 00061000
         L     @13,4(,@13)                                         0206 00062000
         L     @14,12(,@13)                                        0206 00063000
         LM    @00,@12,20(@13)                                     0206 00064000
         BR    @14                                                 0206 00065000
*                                                                  0207 00066000
*   /*****************************************************************/ 00067000
*   /*                                                               */ 00068000
*   /* PROCESS ALL ELEMENTS FOR PTF. ADDRESSES OF ICTMOD ENTRIES ARE */ 00069000
*   /* IN ICTPTF ENTRY PASSED TO ROUTINE                             */ 00070000
*   /*                                                               */ 00071000
*   /*****************************************************************/ 00072000
*                                                                  0207 00073000
*   DO J=1 BY 1 WHILE(MAINRC<STOPPTF/* LOOP IF NOT BAD RETURN CODE   */ 00074000
*         &ICTIXP(J)^=ICTENDIX);    /* AND MORE ELEMENTS             */ 00075000
@RC00204 DS    0H                                                  0207 00076000
@RC00202 LA    @12,1                                               0207 00077000
         ST    @12,J                                               0207 00078000
         B     @DE00207                                            0207 00079000
@DL00207 DS    0H                                                  0208 00080000
*     IF ICTPPTR(1)->ICTPTFS(1)=ICTPTFS(1)/* MOD BELONGS TO SYSMOD 0208 00081000
*                                      PASSED                        */ 00082000
*         &ICTMXUPD(1)=OFF          /* AND NOT EXTRA UPDATE ?        */ 00083000
*         &ICTMFLG1(1)^='00000000'B /* AND NOT DUMMY ENTRY ?         */ 00084000
*       THEN                                                       0208 00085000
         L     @12,J                                               0208 00086000
         SLA   @12,2                                               0208 00087000
         L     @08,@PC00001                                        0208 00088000
         L     @01,ICTPCHN(,@08)                                   0208 00089000
         AL    @01,@CF02006                                        0208 00090000
         L     @12,ICTIXP(@12,@01)                                 0208 00091000
         L     @01,ICTPPTR(,@12)                                   0208 00092000
         CLC   ICTPTFS(7,@01),ICTPTFS(@08)                         0208 00093000
         BNE   @RF00208                                            0208 00094000
         TM    ICTMXUPD(@12),B'00001000'                           0208 00095000
         BNZ   @RF00208                                            0208 00096000
         CLI   ICTMFLG1(@12),B'00000000'                           0208 00097000
         BE    @RF00208                                            0208 00098000
*       DO;                         /* OKAY TO PROCESS               */ 00099000
*                                                                  0209 00100000
*         /***********************************************************/ 00101000
*         /*                                                         */ 00102000
*         /* INITALIZE FOR PROCESSING THIS ELEMENT           @Z67SP00*/ 00103000
*         /*                                                         */ 00104000
*         /***********************************************************/ 00105000
*                                                                  0210 00106000
*         ENTICT@=ADDR(ICTMOD(1));  /* SET ADDR OF MOD ENTRY @Z67SP00*/ 00107000
         ST    @12,ENTICT@                                         0210 00108000
*         CALL ELEMINIT(ENTICT@);   /* INIT ELEM. PROCESS    @Z67SP00*/ 00109000
         LA    @01,@AL00211                                        0211 00110000
         BAL   @14,ELEMINIT                                        0211 00111000
*         IF ENTETYPE^=IDUNONE      /* VALID ELEMENT TYPE    @Z67SP00*/ 00112000
*           THEN                                                   0212 00113000
         CLI   ENTETYPE,C'N'                                       0212 00114000
         BE    @RF00212                                            0212 00115000
*           DO;                                                    0213 00116000
*             IF ICTMISEL(1)=ON     /* ELEMENT SELECTED      @Z67SP00*/ 00117000
*               THEN                                               0214 00118000
         L     @12,J                                               0214 00119000
         SLA   @12,2                                               0214 00120000
         L     @08,@PC00001                                        0214 00121000
         L     @08,ICTPCHN(,@08)                                   0214 00122000
         LA    @01,0(@12,@08)                                      0214 00123000
         AL    @01,@CF02006                                        0214 00124000
         L     @01,ICTIXP(,@01)                                    0214 00125000
         TM    ICTMISEL(@01),B'10000000'                           0214 00126000
         BNO   @RF00214                                            0214 00127000
*               DO;                                                0215 00128000
*                                                                  0215 00129000
*                 /***************************************************/ 00130000
*                 /*                                                 */ 00131000
*                 /* BASED ON FUNCTION, CALL APPROPRIATE PROCEDURE   */ 00132000
*                 /*                                                 */ 00133000
*                 /***************************************************/ 00134000
*                                                                  0216 00135000
*                 IF CCARES=ON      /* RESTORE ?                     */ 00136000
*                   THEN                                           0216 00137000
         TM    CCARES(CCAPTR),B'00001000'                          0216 00138000
         BNO   @RF00216                                            0216 00139000
*                   CALL IDURSTRE(ICTIXP(J));/* CALL RESTORE       0217 00140000
*                                      PROCESSOR                     */ 00141000
         AL    @08,@CF02006                                        0217 00142000
         LA    @12,ICTIXP(@12,@08)                                 0217 00143000
         ST    @12,@AL00001                                        0217 00144000
         LA    @01,@AL00001                                        0217 00145000
         BAL   @14,IDURSTRE                                        0217 00146000
*                 ELSE                                             0218 00147000
*                   CALL IDUUPDAT(ICTIXP(J));/* CALL APPLY & ACCEPT     00148000
*                                      PROCESSOR                     */ 00149000
         B     @RC00216                                            0218 00150000
@RF00216 L     @12,J                                               0218 00151000
         SLA   @12,2                                               0218 00152000
         L     @08,@PC00001                                        0218 00153000
         L     @08,ICTPCHN(,@08)                                   0218 00154000
         AL    @08,@CF02006                                        0218 00155000
         LA    @12,ICTIXP(@12,@08)                                 0218 00156000
         ST    @12,@AL00001                                        0218 00157000
         LA    @01,@AL00001                                        0218 00158000
         BAL   @14,IDUUPDAT                                        0218 00159000
*                 RFY                                              0219 00160000
*                   RTNCODE RSTD;                                  0219 00161000
@RC00216 DS    0H                                                  0220 00162000
*                 MAINRC=MAX(MAINRC,RTNCODE);/* SAVE MAXIMUM RC VALUE*/ 00163000
         L     @14,MAINRC                                          0220 00164000
         CR    @14,RTNCODE                                         0220 00165000
         BNL   *+6                                                      00166000
         LR    @14,RTNCODE                                         0220 00167000
         ST    @14,MAINRC                                          0220 00168000
*                 RFY                                              0221 00169000
*                   RTNCODE UNRSTD;                                0221 00170000
*               END;                                               0222 00171000
*           END;                                                   0223 00172000
@RF00214 DS    0H                                                  0224 00173000
*       END;                        /* END OKAY TO PROCESS ENTRY     */ 00174000
@RF00212 DS    0H                                                  0225 00175000
*   END;                            /* END OF PTF ELEMENT LIST       */ 00176000
@RF00208 LA    @12,1                                               0225 00177000
         AL    @12,J                                               0225 00178000
         ST    @12,J                                               0225 00179000
@DE00207 L     @12,MAINRC                                          0225 00180000
         CH    @12,@CH00112                                        0225 00181000
         BNL   @DC00207                                            0225 00182000
         L     @12,J                                               0225 00183000
         SLA   @12,2                                               0225 00184000
         L     @08,@PC00001                                        0225 00185000
         L     @08,ICTPCHN(,@08)                                   0225 00186000
         AL    @08,@CF02006                                        0225 00187000
         L     @12,ICTIXP(@12,@08)                                 0225 00188000
         CL    @12,ICTENDIX                                        0225 00189000
         BNE   @DL00207                                            0225 00190000
@DC00207 DS    0H                                                  0226 00191000
*   RETURN CODE(MAINRC);            /* RETURN TO CALLING MODULE      */ 00192000
         L     @15,MAINRC                                          0226 00193000
         L     @13,4(,@13)                                         0226 00194000
         L     @14,12(,@13)                                        0226 00195000
         LM    @00,@12,20(@13)                                     0226 00196000
         BR    @14                                                 0226 00197000
*                                                                  0227 00198000
*/********************************************************************/ 00199000
*/*                                                                  */ 00200000
*/*  IDURSTRE  - RESTORE MODID PROCESSOR                             */ 00201000
*/*                                                                  */ 00202000
*/*                                                                  */ 00203000
*/********************************************************************/ 00204000
*                                                                  0227 00205000
*IDURSTRE:                                                         0227 00206000
*   PROC(NPTR);                                                    0227 00207000
IDURSTRE STM   @14,@12,@SA00002                                    0227 00208000
         MVC   @PC00002(4),0(@01)                                  0227 00209000
*   DECLARE                                                        0228 00210000
*     NPTR PTR(31);                 /* POINTER TO ICTMOD ENTRY       */ 00211000
*   DECLARE                                                        0229 00212000
*     RSTRERC FIXED(31);            /* PROCEDURE'S RETURN CODE       */ 00213000
*   RFY                                                            0230 00214000
*     ICTMOD BASED(NPTR);           /* ADDRESS MOD ENTRY PASSED      */ 00215000
*   RFY                                                            0231 00216000
*     ICTPTF BASED(ICTPPTR(1));     /* ADDRESS PTF ENTRY ASSOCIATED     00217000
*                                      WITH MOD ENTRY PASSED         */ 00218000
*   RFY                                                            0232 00219000
*     ICTENTLS BASED(ICTMICHA(1));  /* ACDS MODID LIST               */ 00220000
*   RFY                                                            0233 00221000
*     ICTMIDLS BASED(ICTMICHN(1));  /* CDS MODID LIST                */ 00222000
*                                                                  0233 00223000
*   /*****************************************************************/ 00224000
*   /*                                                               */ 00225000
*   /* BEGIN RESTORE MODID UPDATE PROCESSING                         */ 00226000
*   /*                                                               */ 00227000
*   /*****************************************************************/ 00228000
*                                                                  0234 00229000
*   RSTRERC=0;                      /* INITIALIZE PROCEDURE'S RETURN    00230000
*                                      CODE                          */ 00231000
         SLR   @12,@12                                             0234 00232000
         ST    @12,RSTRERC                                         0234 00233000
*   IF ICTMZAP(1)=ON                /* ZAP BEING RESTORED    @Z67SP00*/ 00234000
*     THEN                          /*                       @Z67SP00*/ 00235000
         L     @08,@PC00002                                        0235 00236000
         L     @08,NPTR(,@08)                                      0235 00237000
         TM    ICTMZAP(@08),B'00100000'                            0235 00238000
         BNO   @RF00235                                            0235 00239000
*     RETURN CODE(RSTRERC);         /* ... IGNORE IT         @Z67SP00*/ 00240000
*                                                                  0236 00241000
         LR    @15,@12                                             0236 00242000
         L     @14,@SA00002                                        0236 00243000
         LM    @00,@12,@SA00002+8                                  0236 00244000
         BR    @14                                                 0236 00245000
*   /*****************************************************************/ 00246000
*   /*                                                               */ 00247000
*   /* RESET ANY REGRESSED ELEMENT SUB-ENTRIES (FOR THIS ELEMENT) IN */ 00248000
*   /* CDS SYSMOD ENTRIES WHICH ARE NO LONGER REGRESSED AS A RESULT  */ 00249000
*   /* OF THIS RESTORE                                       @Z67SP00*/ 00250000
*   /*                                                               */ 00251000
*   /*****************************************************************/ 00252000
*                                                                  0237 00253000
*   IF ICTMIDEL(1)=OFF              /* NOT A DELETED ELEMENT @Z67SP00*/ 00254000
*     THEN                                                         0237 00255000
@RF00235 L     @12,@PC00002                                        0237 00256000
         L     @12,NPTR(,@12)                                      0237 00257000
         TM    ICTMIDEL(@12),B'01000000'                           0237 00258000
         BNZ   @RF00237                                            0237 00259000
*     CALL RESTREG;                 /*                       @Z67SP00*/ 00260000
         BAL   @14,RESTREG                                         0238 00261000
*   IF RSTRERC<STOPPTF              /* RETURN CODE OKAY ?            */ 00262000
*     THEN                                                         0239 00263000
@RF00237 L     @12,RSTRERC                                         0239 00264000
         CH    @12,@CH00112                                        0239 00265000
         BNL   @RF00239                                            0239 00266000
*     DO;                           /* OKAY TO CONTINUE RESTORE    0240 00267000
*                                      PROCESSING                    */ 00268000
*                                                                  0240 00269000
*       /*************************************************************/ 00270000
*       /*                                                           */ 00271000
*       /* ACCESS CDS ELEMENT ENTRY TO UPDATE MODIDS         @Z67SP00*/ 00272000
*       /*                                                           */ 00273000
*       /*************************************************************/ 00274000
*                                                                  0241 00275000
*       IOPFUNCT=IOPLOC;            /* LOCATE ELEMENT ENTRY  @Z67SP00*/ 00276000
         L     @12,IOPPTR                                          0241 00277000
         MVI   IOPFUNCT(@12),X'03'                                 0241 00278000
*       IOPTYPE=ENTTYPE;            /* ENTRY TYPE AS SAVED           */ 00279000
         MVC   IOPTYPE(1,@12),ENTTYPE                              0242 00280000
*       IOPNAME=ICTMNAME(1);        /* ELEMENT NAME                  */ 00281000
         L     @08,@PC00002                                        0243 00282000
         L     @08,NPTR(,@08)                                      0243 00283000
         MVC   IOPNAME(8,@12),ICTMNAME(@08)                        0243 00284000
*       CALL HMASMIO(HMASMIOP);     /* LOCATE ELEMENT ENTRY ON CDS   */ 00285000
         ST    @12,@AL00001                                        0244 00286000
         L     @15,@CV01721                                        0244 00287000
         LA    @01,@AL00001                                        0244 00288000
         BALR  @14,@15                                             0244 00289000
*       IF IOPRETRN>IOPNOTFD        /* TERMINATING ERROR     @Z67SP00*/ 00290000
*         THEN                                                     0245 00291000
         L     @12,IOPPTR                                          0245 00292000
         CLI   IOPRETRN(@12),4                                     0245 00293000
         BNH   @RF00245                                            0245 00294000
*         RSTRERC=MAX(RSTRERC,IOPRETRN);/* SAVE MAXIMUM RETURN VALUE */ 00295000
         L     @08,RSTRERC                                         0246 00296000
         SLR   @15,@15                                             0246 00297000
         IC    @15,IOPRETRN(,@12)                                  0246 00298000
         CR    @08,@15                                             0246 00299000
         BNL   *+6                                                      00300000
         LR    @08,@15                                             0246 00301000
         ST    @08,RSTRERC                                         0246 00302000
*       IF IOPRETRN=IOPGOOD         /* LOCATE OKAY ?                 */ 00303000
*         THEN                                                     0247 00304000
@RF00245 L     @12,IOPPTR                                          0247 00305000
         CLI   IOPRETRN(@12),0                                     0247 00306000
         BNE   @RF00247                                            0247 00307000
*         DO;                       /* UPDATE MODIDS FOR RESTORE     */ 00308000
*                                                                  0248 00309000
*           /*********************************************************/ 00310000
*           /*                                                       */ 00311000
*           /* DELETE MODID AND ALIAS SUB-ENTRIES FROM CDS ELEMENT   */ 00312000
*           /* ENTRY IOP                                     @Z67SP00*/ 00313000
*           /*                                                       */ 00314000
*           /*********************************************************/ 00315000
*                                                                  0249 00316000
*           REWRITE=OFF;            /* ENTIRE ENTRY NEED NOT BE    0249 00317000
*                                      RE-WRITTEN            @Z67SP00*/ 00318000
         NI    REWRITE,B'11101111'                                 0249 00319000
*           Q=1;                    /* RESET NEW SUBENTRY INDEX      */ 00320000
         LA    @12,1                                               0250 00321000
         ST    @12,Q                                               0250 00322000
*           DO P=1 BY 1 WHILE(SENTEND(P)^=IOPEOLST);/* LOOP IF     0251 00323000
*                                      SUBENTRY                      */ 00324000
         ST    @12,P                                               0251 00325000
         B     @DE00251                                            0251 00326000
@DL00251 DS    0H                                                  0252 00327000
*             IF SETYPIND(P)=IOPUMID/* UMID SUBENTRY         @Z67SP00*/ 00328000
*                 |SETYPIND(P)=ENTALIAS/* ALIAS SUBENTRY     @Z67SP00*/ 00329000
*               THEN                                               0252 00330000
         L     @12,P                                               0252 00331000
         MH    @12,@CH00033                                        0252 00332000
         L     @08,SEPTR                                           0252 00333000
         BCTR  @08,0                                               0252 00334000
         SLR   @15,@15                                             0252 00335000
         IC    @15,SETYPIND-8(@12,@08)                             0252 00336000
         CH    @15,@CH00818                                        0252 00337000
         BE    @RT00252                                            0252 00338000
         SLR   @12,@12                                             0252 00339000
         IC    @12,ENTALIAS                                        0252 00340000
         CR    @15,@12                                             0252 00341000
         BNE   @RF00252                                            0252 00342000
@RT00252 DS    0H                                                  0253 00343000
*               REWRITE=ON;         /* SUB-ENTRY OVERLAID    @Z67SP00*/ 00344000
         OI    REWRITE,B'00010000'                                 0253 00345000
*             ELSE                  /*                       @Z67SP00*/ 00346000
*               DO;                 /* NOT MODID | ALIAS SUB-ENTRY   */ 00347000
         B     @RC00252                                            0254 00348000
@RF00252 DS    0H                                                  0255 00349000
*                 SENTRY(Q)=SENTRY(P);/* MOVE SUBENTRY TO NEW      0255 00350000
*                                      POSITION                      */ 00351000
         L     @12,SEPTR                                           0255 00352000
         L     @08,Q                                               0255 00353000
         LA    @15,9                                               0255 00354000
         LR    @01,@08                                             0255 00355000
         MR    @00,@15                                             0255 00356000
         ALR   @01,@12                                             0255 00357000
         AL    @01,@CF02009                                        0255 00358000
         M     @14,P                                               0255 00359000
         ALR   @12,@15                                             0255 00360000
         AL    @12,@CF02009                                        0255 00361000
         MVC   @TS00001(9),SENTRY(@12)                             0255 00362000
         MVC   SENTRY(9,@01),@TS00001                              0255 00363000
*                 Q=Q+1;            /* INCREMENT NEW SUBENTRY INDEX  */ 00364000
         AH    @08,@CH00087                                        0256 00365000
         ST    @08,Q                                               0256 00366000
*               END;                                               0257 00367000
*           END;                    /* LOOP THRU ELEMENT SUB-ENTRIES */ 00368000
@RC00252 LA    @12,1                                               0258 00369000
         AL    @12,P                                               0258 00370000
         ST    @12,P                                               0258 00371000
@DE00251 LA    @12,255                                             0258 00372000
         L     @08,SEPTR                                           0258 00373000
         LA    @15,9                                               0258 00374000
         L     @01,P                                               0258 00375000
         MR    @00,@15                                             0258 00376000
         AL    @01,@CF02009                                        0258 00377000
         SLR   @00,@00                                             0258 00378000
         IC    @00,SENTEND(@01,@08)                                0258 00379000
         CR    @12,@00                                             0258 00380000
         BNE   @DL00251                                            0258 00381000
*           SENTEND(Q)=IOPEOLST;    /* SET END OF SUBENTRIES       0259 00382000
*                                      INDICATOR IN IOP              */ 00383000
         M     @14,Q                                               0259 00384000
         AL    @08,@CF02009                                        0259 00385000
         STC   @12,SENTEND(@15,@08)                                0259 00386000
*                                                                  0260 00387000
*           /*********************************************************/ 00388000
*           /*                                                       */ 00389000
*           /* GET MODIDS FROM THE ACDS MODID LIST AND SET THE FMID, */ 00390000
*           /* RMID FIELDS AND UMID SUB-ENTRIES IN CDS ELEMENT IOP   */ 00391000
*           /*                                               @Z67SP00*/ 00392000
*           /*                                                       */ 00393000
*           /*********************************************************/ 00394000
*                                                                  0260 00395000
*           IOPRMID=' ';            /* CLEAR RMID            @Z67SP00*/ 00396000
         L     @12,IOPPTR                                          0260 00397000
         MVI   IOPRMID+1(@12),C' '                                 0260 00398000
         MVC   IOPRMID+2(5,@12),IOPRMID+1(@12)                     0260 00399000
         MVI   IOPRMID(@12),C' '                                   0260 00400000
*           IOPRMST=''B;            /* CLEAR RMID STATUS     @Z67SP00*/ 00401000
         MVI   IOPRMST(@12),X'00'                                  0261 00402000
*           IOPFMID=' ';            /* CLEAR FMID            @Z67SP00*/ 00403000
         MVI   IOPFMID+1(@12),C' '                                 0262 00404000
         MVC   IOPFMID+2(5,@12),IOPFMID+1(@12)                     0262 00405000
         MVI   IOPFMID(@12),C' '                                   0262 00406000
*           IOPFMST=''B;            /* CLEAR RMID STATUS     @Z67SP00*/ 00407000
         MVI   IOPFMST(@12),X'00'                                  0263 00408000
*           RFY                                                    0264 00409000
*             ICTMIDLS BASED(ICTMICHA(1));/* ADDRESS ACDS MODID LIST */ 00410000
*           IF ICTMIDEL(1)=OFF      /* NOT A DELETED ELEMENT @Z67SP00*/ 00411000
*             THEN                                                 0265 00412000
         L     @12,@PC00002                                        0265 00413000
         L     @12,NPTR(,@12)                                      0265 00414000
         TM    ICTMIDEL(@12),B'01000000'                           0265 00415000
         BNZ   @RF00265                                            0265 00416000
*             DO;                                                  0266 00417000
*                                                                  0266 00418000
*               /*****************************************************/ 00419000
*               /*                                                   */ 00420000
*               /* GO THRU ICT ENTRY ACDS MODID LIST         @Z67SP00*/ 00421000
*               /*                                                   */ 00422000
*               /*****************************************************/ 00423000
*                                                                  0267 00424000
*               DO N=1 BY 1 WHILE(ICTMIDND(N)^=ICTENDIX);          0267 00425000
         LA    @12,1                                               0267 00426000
         ST    @12,N                                               0267 00427000
         B     @DE00267                                            0267 00428000
