*                   END;            /* REGRESSION MESSAGE            */ 01613000
*               END;                /* FOUND MATCHING ENTRY          */ 01614000
@RF00445 DS    0H                                                  0461 01615000
*           END;                    /* FOUND SAME ELEMENT NAME       */ 01616000
@RF00430 DS    0H                                                  0462 01617000
*       END;                        /* PTF SUBENTRY LOOP             */ 01618000
@RF00428 LA    @12,1                                               0462 01619000
         AL    @12,P                                               0462 01620000
         ST    @12,P                                               0462 01621000
@DE00427 L     @12,P                                               0462 01622000
         MH    @12,@CH00033                                        0462 01623000
         L     @08,IOPPTR                                          0462 01624000
         SLR   @15,@15                                             0462 01625000
