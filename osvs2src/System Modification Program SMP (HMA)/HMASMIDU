         TM    ELEMHIT,B'10000000'                                 0462 01629000
         BZ    @DL00427                                            0462 01630000
@DC00427 DS    0H                                                  0463 01631000
*     END;                          /* FOUND PTF ENTRY FOR MODID     */ 01632000
*   RETURN CODE(UPPTFRC);           /* RETURN WITH MAXIMUM VALUE     */ 01633000
@RF00424 L     @15,UPPTFRC                                         0464 01634000
         L     @14,@SA00004                                        0464 01635000
         LM    @00,@12,@SA00004+8                                  0464 01636000
         BR    @14                                                 0464 01637000
*   END IDUUPPTF;                   /* END OF PROCEDURE              */ 01638000
