*/*  IDUUPPTF  - REGRESSED ELEMENT PROCESSOR                         */ 01260000
*/*                                                                  */ 01261000
*/*                                                                  */ 01262000
*/********************************************************************/ 01263000
*                                                                  0406 01264000
*IDUUPPTF:                                                         0406 01265000
*   PROC(MODPTR,MIDLSPTR,MIDIX,REGACT);                            0406 01266000
*                                                                  0406 01267000
IDUUPPTF STM   @14,@12,@SA00004                                    0406 01268000
         MVC   @PC00004(16),0(@01)                                 0406 01269000
*   /*****************************************************************/ 01270000
*   /*                                                               */ 01271000
*   /* THIS PROCEDURE WILL ACCESS THE PTF ENTRY FOR THE MODID        */ 01272000
*   /* INDEXED. THE DATA SET WILL BE EITHER THE SMPCDS OR SMPACDS AND*/ 01273000
*   /* IS SET IN THE IOP BEFORE THE CALL. IF THE REGRESSION ACTION   */ 01274000
*   /* INDICATOR (REGACT) IS ZERO, THE ELEMENT SUBENTRY TYPE IN THE  */ 01275000
*   /* PTF ENTRY IS SET TO INDICATE THE CORRECT MODIFICATION CONTROL */ 01276000
*   /* STATEMENT TYPE IN NONREGRESSED STATE. IF THE REGRESSION ACTION*/ 01277000
*   /* INDICATOR IS ONE, THE ELEMENT SUBENTRY TYPE IS SET TO INDICATE*/ 01278000
*   /* REGRESSED STATE AND A WARNING MESSAGE ISSUED NOTIFYING USER OF*/ 01279000
*   /* REGRESSION OF A PREVIOUSLY APPLIED OR ACCEPTED PTF.           */ 01280000
*   /*                                                               */ 01281000
*   /*****************************************************************/ 01282000
*                                                                  0407 01283000
*   DECLARE                                                        0407 01284000
*     MODPTR PTR(31);               /* POINTER TO ICTMOD ENTRY       */ 01285000
*   DECLARE                                                        0408 01286000
*     MIDLSPTR PTR(31);             /* POINTER TO MODID LIST         */ 01287000
*   DECLARE                                                        0409 01288000
*     MIDIX FIXED(31);              /* INDEX TO ENTRY IN MODID LIST  */ 01289000
*   DECLARE                                                        0410 01290000
*     REGACT FIXED(8);              /* REGRESSION ACTION INDICATOR   */ 01291000
*   DECLARE                                                        0411 01292000
*     UPPTFRC FIXED(31);            /* PROCEDURE'S RETURN CODE       */ 01293000
*   DCL                                                            0412 01294000
*     1 UPPTFFLG BIT(8),            /* LOCAL FLAGS           @Z67SP00*/ 01295000
*      2 ELEMHIT BIT(1),            /* ELEM FOUND IN SYSMOD  @Z67SP00*/ 01296000
*      2 UPPTFWRT BIT(1),           /* RE-WRITE SYSMOD ENTRY @Z67SP00*/ 01297000
*      2 * BIT(6);                  /* UN-USED               @Z67SP00*/ 01298000
*   RFY                                                            0413 01299000
*     ICTMOD BASED(MODPTR);                                        0413 01300000
*   RFY                                                            0414 01301000
*     ICTPTF BASED(ICTPPTR(1));                                    0414 01302000
*   RFY                                                            0415 01303000
*     ICTMIDLS BASED(MIDLSPTR);                                    0415 01304000
*   UPPTFRC=0;                      /* INITIALIZE PROCEDURE'S RETURN    01305000
*                                      CODE                          */ 01306000
*                                                                  0416 01307000
         SLR   @12,@12                                             0416 01308000
         ST    @12,UPPTFRC                                         0416 01309000
*   /*****************************************************************/ 01310000
*   /*                                                               */ 01311000
*   /* ACCESS CDS SYSMOD ENTRY FOR SYSMOD CORRESPONDING TO THE MODID */ 01312000
*   /* PASSED                                                @Z67SP00*/ 01313000
*   /*                                                               */ 01314000
*   /*****************************************************************/ 01315000
*                                                                  0417 01316000
*   UPPTFFLG=''B;                   /* CLEAR LOCAL FLAGS     @Z67SP00*/ 01317000
         MVI   UPPTFFLG,X'00'                                      0417 01318000
*   IOPFUNCT=IOPLOC;                /* SET FUNCTION TO LOCATE        */ 01319000
         L     @12,IOPPTR                                          0418 01320000
         MVI   IOPFUNCT(@12),X'03'                                 0418 01321000
*   IOPTYPE=IOPSMD;                 /* SET TYPE OF ENTRY TO PTF      */ 01322000
         MVI   IOPTYPE(@12),X'0B'                                  0419 01323000
*   IOPNAM17=ICTMODID(MIDIX);       /* MODID ENTRY NAME IS PTF NUMBER*/ 01324000
         L     @08,@PC00004+8                                      0420 01325000
         L     @08,MIDIX(,@08)                                     0420 01326000
         SLA   @08,3                                               0420 01327000
         L     @15,@PC00004+4                                      0420 01328000
         L     @01,MIDLSPTR(,@15)                                  0420 01329000
         ALR   @01,@08                                             0420 01330000
         AL    @01,@CF02010                                        0420 01331000
         MVC   IOPNAM17(7,@12),ICTMODID(@01)                       0420 01332000
*   CALL HMASMIO(HMASMIOP);         /* LOCATE PTF ENTRY FOR MODID    */ 01333000
         ST    @12,@AL00001                                        0421 01334000
         L     @15,@CV01721                                        0421 01335000
         LA    @01,@AL00001                                        0421 01336000
         BALR  @14,@15                                             0421 01337000
*   IF IOPRETRN>IOPNOTFD            /* TERMINATING ERROR     @Z67SP00*/ 01338000
*     THEN                                                         0422 01339000
         L     @12,IOPPTR                                          0422 01340000
         CLI   IOPRETRN(@12),4                                     0422 01341000
         BNH   @RF00422                                            0422 01342000
*     UPPTFRC=IOPRETRN;             /* SAVE RETURN CODE FROM HMASMIO */ 01343000
         SLR   @08,@08                                             0423 01344000
         IC    @08,IOPRETRN(,@12)                                  0423 01345000
         ST    @08,UPPTFRC                                         0423 01346000
*   IF IOPRETRN=IOPGOOD             /* LOCATE OKAY ?                 */ 01347000
*     THEN                                                         0424 01348000
@RF00422 L     @12,IOPPTR                                          0424 01349000
         CLI   IOPRETRN(@12),0                                     0424 01350000
         BNE   @RF00424                                            0424 01351000
*     DO;                           /* FOUND PTF ENTRY FOR MODID     */ 01352000
*       ELEMHIT=OFF;                /* RESET LOOP CONTROL SWITCH     */ 01353000
         NI    ELEMHIT,B'01111111'                                 0426 01354000
*/*   LOOK FOR MATCHING ELEMENT IN SYSMOD ENTRY              @Z67SP00*/ 01355000
*                                                                  0427 01356000
*       DO P=1 BY 1 WHILE(IOPPTFND(P)^=IOPEOLST/* LOOP IF SUBENTRIES */ 01357000
*             &ELEMHIT=OFF);        /* AND MATCHING NOT FOUND        */ 01358000
         LA    @12,1                                               0427 01359000
         ST    @12,P                                               0427 01360000
         B     @DE00427                                            0427 01361000
@DL00427 DS    0H                                                  0428 01362000
*         IF IOPPMODS(P)=ICTMNAME(1)/* FOUND ELEMENT NAME    @ZA24813*/ 01363000
*           THEN                                                   0428 01364000
         L     @12,IOPPTR                                          0428 01365000
         L     @08,P                                               0428 01366000
         MH    @08,@CH00033                                        0428 01367000
         L     @15,@PC00004                                        0428 01368000
         L     @15,MODPTR(,@15)                                    0428 01369000
         ST    @08,@TF00001                                        0428 01370000
         ALR   @08,@12                                             0428 01371000
         CLC   IOPPMODS-9(8,@08),ICTMNAME(@15)                     0428 01372000
         BNE   @RF00428                                            0428 01373000
*           DO;                     /* FOUND SAME ELEMENT NAME       */ 01374000
*             IF((ICTMMOD(1)=ON     /* ---- MOD REPL ------          */ 01375000
*                 |ICTMZAP(1)=ON    /* ---- MOD UPDATE ----          */ 01376000
*                 |ICTMXPND(1)=ON)  /* ---- MOD UPDATE ----          */ 01377000
*                 &(IOPPIND(P)=IOPPMOD/* ++MOD                       */ 01378000
*                 |IOPPIND(P)=IOPPRMOD/* ++MOD REGRESSED             */ 01379000
*                 |IOPPIND(P)=IOPPZAP/* ++ZAP                        */ 01380000
*                 |IOPPIND(P)=IOPPRZAP/* ++ZAP REGRESSED             */ 01381000
*                 |IOPPIND(P)=IOPPXPD/* ++XZAP                       */ 01382000
*                 |IOPPIND(P)=IOPPRXPD))/* ++XZAP REGRESSED          */ 01383000
*                 |((ICTMMACR(1)=ON /* ---- MAC REPL ------          */ 01384000
*                 |ICTMMACU(1)=ON)  /* ---- MAC UPDATE ----          */ 01385000
*                 &(IOPPIND(P)=IOPPMACR/* ++MAC                      */ 01386000
*                 |IOPPIND(P)=IOPPRMCR/* ++MAC REGRESSED             */ 01387000
*                 |IOPPIND(P)=IOPPMACU/* ++MACUPD                    */ 01388000
*                 |IOPPIND(P)=IOPPRMCU))/* ++MACUPD REGRESSED        */ 01389000
*                 |((ICTMSRCR(1)=ON /* ---- SRC REPL ------          */ 01390000
*                 |ICTMSRCU(1)=ON)  /* ---- SRC UPDATE ----          */ 01391000
*                 &(IOPPIND(P)=IOPPSRCR/* ++SRC                      */ 01392000
*                 |IOPPIND(P)=IOPPRSCR/* ++SRC REGRESSED             */ 01393000
*                 |IOPPIND(P)=IOPPSRCU/* ++SRCUPD                    */ 01394000
*                 |IOPPIND(P)=IOPPRSCU))/* ++SRCUPD REGRESSED        */ 01395000
*               THEN                                               0430 01396000
         TM    ICTMMOD(@15),B'10110000'                            0430 01397000
         BZ    @GL00015                                            0430 01398000
         L     @08,@TF00001                                        0430 01399000
         SLR   @15,@15                                             0430 01400000
         IC    @15,IOPPIND-9(@08,@12)                              0430 01401000
         CH    @15,@CH00087                                        0430 01402000
         BE    @RT00430                                            0430 01403000
         CH    @15,@CH00601                                        0430 01404000
         BE    @RT00430                                            0430 01405000
         CH    @15,@CH00184                                        0430 01406000
         BE    @RT00430                                            0430 01407000
         CH    @15,@CH00603                                        0430 01408000
         BE    @RT00430                                            0430 01409000
         CH    @15,@CH00236                                        0430 01410000
         BE    @RT00430                                            0430 01411000
         CH    @15,@CH00605                                        0430 01412000
         BE    @RT00430                                            0430 01413000
@GL00015 L     @12,@PC00004                                        0430 01414000
         L     @12,MODPTR(,@12)                                    0430 01415000
         TM    ICTMMACR(@12),B'00001100'                           0430 01416000
         BZ    @GL00014                                            0430 01417000
         L     @12,P                                               0430 01418000
         MH    @12,@CH00033                                        0430 01419000
         L     @08,IOPPTR                                          0430 01420000
         SLR   @15,@15                                             0430 01421000
         IC    @15,IOPPIND-9(@12,@08)                              0430 01422000
         CH    @15,@CH00039                                        0430 01423000
         BE    @RT00430                                            0430 01424000
         CH    @15,@CH00607                                        0430 01425000
         BE    @RT00430                                            0430 01426000
         CH    @15,@CH00130                                        0430 01427000
         BE    @RT00430                                            0430 01428000
         CH    @15,@CH00609                                        0430 01429000
         BE    @RT00430                                            0430 01430000
@GL00014 L     @12,@PC00004                                        0430 01431000
         L     @12,MODPTR(,@12)                                    0430 01432000
         TM    ICTMSRCR(@12),B'00000011'                           0430 01433000
         BZ    @RF00430                                            0430 01434000
         L     @12,P                                               0430 01435000
         MH    @12,@CH00033                                        0430 01436000
         L     @08,IOPPTR                                          0430 01437000
         SLR   @15,@15                                             0430 01438000
         IC    @15,IOPPIND-9(@12,@08)                              0430 01439000
         CH    @15,@CH00201                                        0430 01440000
         BE    @RT00430                                            0430 01441000
         CH    @15,@CH00611                                        0430 01442000
         BE    @RT00430                                            0430 01443000
         CH    @15,@CH00241                                        0430 01444000
         BE    @RT00430                                            0430 01445000
         CH    @15,@CH00613                                        0430 01446000
         BNE   @RF00430                                            0430 01447000
@RT00430 DS    0H                                                  0431 01448000
*               DO;                 /* FOUND MATCHING ENTRY          */ 01449000
*                 ELEMHIT=ON;       /* SET LOOP CONTROL SWITCH TO  0432 01450000
*                                      QUIT                          */ 01451000
         OI    ELEMHIT,B'10000000'                                 0432 01452000
*                 IF(IOPPIND(P)>IOPPREG/* REGRESSED SUBTYPE ?        */ 01453000
*                     &REGACT=0)    /* AND SET REGRESSION OFF ?      */ 01454000
*                     |(IOPPIND(P)<IOPPREG/* OR NONREGRESSED SUBTYPE    01455000
*                                      ?                             */ 01456000
*                     &REGACT=1)    /* AND SET REGRESSION ON ?       */ 01457000
*                   THEN                                           0433 01458000
         L     @12,P                                               0433 01459000
         MH    @12,@CH00033                                        0433 01460000
         L     @08,IOPPTR                                          0433 01461000
         SLR   @15,@15                                             0433 01462000
         IC    @15,IOPPIND-9(@12,@08)                              0433 01463000
         CH    @15,@CH00599                                        0433 01464000
         BNH   @GL00030                                            0433 01465000
         L     @12,@PC00004+12                                     0433 01466000
         CLI   REGACT(@12),0                                       0433 01467000
         BE    @RT00433                                            0433 01468000
@GL00030 L     @12,P                                               0433 01469000
         MH    @12,@CH00033                                        0433 01470000
         L     @08,IOPPTR                                          0433 01471000
         SLR   @15,@15                                             0433 01472000
         IC    @15,IOPPIND-9(@12,@08)                              0433 01473000
         CH    @15,@CH00599                                        0433 01474000
         BNL   @RF00433                                            0433 01475000
         L     @12,@PC00004+12                                     0433 01476000
         CLI   REGACT(@12),1                                       0433 01477000
         BNE   @RF00433                                            0433 01478000
@RT00433 DS    0H                                                  0434 01479000
*                   UPPTFWRT=ON;    /* SYSMOD ENTRY MUST BE UPDATED     01480000
*                                      AND RE-WRITTEN        @Z67SP00*/ 01481000
*                                                                  0434 01482000
         OI    UPPTFWRT,B'01000000'                                0434 01483000
*                 /***************************************************/ 01484000
*                 /*                                                 */ 01485000
*                 /* SET SUBENTRY TYPE AS SPECIFIED BY ACTION        */ 01486000
*                 /* INDICATOR AND REPLACE PTF ENTRY                 */ 01487000
*                 /*                                                 */ 01488000
*                 /***************************************************/ 01489000
*                                                                  0435 01490000
*                 IF REGACT=0       /* SET TO NON-REGRESSED  @Z67SP00*/ 01491000
*                   THEN            /*                       @Z67SP00*/ 01492000
@RF00433 L     @12,@PC00004+12                                     0435 01493000
         CLI   REGACT(@12),0                                       0435 01494000
         BNE   @RF00435                                            0435 01495000
*                   IOPPIND(P)=IOPPIND(P)-IOPPREG;/* CHANGE TO     0436 01496000
*                                      NONREGRESSED                  */ 01497000
         L     @12,IOPPTR                                          0436 01498000
         L     @08,P                                               0436 01499000
         MH    @08,@CH00033                                        0436 01500000
         SLR   @15,@15                                             0436 01501000
         IC    @15,IOPPIND-9(@08,@12)                              0436 01502000
         SH    @15,@CH00599                                        0436 01503000
         STC   @15,IOPPIND-9(@08,@12)                              0436 01504000
*                 ELSE                                             0437 01505000
*                   IF IOPPIND(P)<IOPPREG/* NOT ALREADY REGRESSED  0437 01506000
*                                                            @Z67SP00*/ 01507000
*                     THEN                                         0437 01508000
         B     @RC00435                                            0437 01509000
@RF00435 LA    @12,80                                              0437 01510000
         L     @08,IOPPTR                                          0437 01511000
         L     @15,P                                               0437 01512000
         MH    @15,@CH00033                                        0437 01513000
         SLR   @14,@14                                             0437 01514000
         IC    @14,IOPPIND-9(@15,@08)                              0437 01515000
         CR    @14,@12                                             0437 01516000
         BNL   @RF00437                                            0437 01517000
*                     IOPPIND(P)=IOPPIND(P)+IOPPREG;/* CHANGE TO   0438 01518000
*                                      REGRESSED                     */ 01519000
         ALR   @14,@12                                             0438 01520000
         STC   @14,IOPPIND-9(@15,@08)                              0438 01521000
*                 IOPFUNCT=IOPSTOWR;/* SET FUNCTION TO STOW ENTRY    */ 01522000
@RF00437 DS    0H                                                  0439 01523000
@RC00435 L     @12,IOPPTR                                          0439 01524000
         MVI   IOPFUNCT(@12),X'54'                                 0439 01525000
*                 IF UPPTFWRT=ON    /* RE-WRITE SYSMOD ENTRY @Z67SP00*/ 01526000
*                   THEN                                           0440 01527000
         TM    UPPTFWRT,B'01000000'                                0440 01528000
         BNO   @RF00440                                            0440 01529000
*                   DO;             /*                       @Z67SP00*/ 01530000
*                     CALL HMASMIO(HMASMIOP);/* REPLACE PTF ENTRY    */ 01531000
         ST    @12,@AL00001                                        0442 01532000
         L     @15,@CV01721                                        0442 01533000
         LA    @01,@AL00001                                        0442 01534000
         BALR  @14,@15                                             0442 01535000
*                     UPPTFRC=MAX(UPPTFRC,IOPRETRN);/* SAVE MAXIMUM     01536000
*                                      VALUE                         */ 01537000
         L     @12,UPPTFRC                                         0443 01538000
         L     @08,IOPPTR                                          0443 01539000
         SLR   @15,@15                                             0443 01540000
         IC    @15,IOPRETRN(,@08)                                  0443 01541000
         CR    @12,@15                                             0443 01542000
         BNL   *+6                                                      01543000
         LR    @12,@15                                             0443 01544000
         ST    @12,UPPTFRC                                         0443 01545000
*                   END;            /*                       @Z67SP00*/ 01546000
*                 IF UPPTFRC=ZERO   /* EVERYTHING OK         @Z67SP00*/ 01547000
*                     &REGACT=1     /* AND REGRESSION ON ?           */ 01548000
*                   THEN                                           0445 01549000
@RF00440 ICM   @12,15,UPPTFRC                                      0445 01550000
         BNZ   @RF00445                                            0445 01551000
         L     @12,@PC00004+12                                     0445 01552000
         CLI   REGACT(@12),1                                       0445 01553000
         BNE   @RF00445                                            0445 01554000
*                   DO;             /* REGRESSION MESSAGE            */ 01555000
*                                                                  0446 01556000
*                     /***********************************************/ 01557000
*                     /*                                             */ 01558000
*                     /* ISSUE MESSAGE - HMA3241 EEEEEE YYYYYYYY     */ 01559000
*                     /* SUBENTRY IN PTF MMMMMMM REGRESSED BY PTF    */ 01560000
*                     /* NNNNNNN                                     */ 01561000
*                     /*                                             */ 01562000
*                     /***********************************************/ 01563000
*                                                                  0447 01564000
*                     MGPCLEAR=''B; /* CLEAR MSG NUMBERS AND FLAGS   */ 01565000
         XC    MGPCLEAR(6),MGPCLEAR                                0447 01566000
*                     MGPTYPE=MGPWARN;/* SEVERITY IS WARNING         */ 01567000
         MVI   MGPTYPE,X'02'                                       0448 01568000
*                     MGPMGNO1=HMA324I;/* VALUE FOR MESSAGE HMA324I  */ 01569000
         MVC   MGPMGNO1(2),@CH01745                                0449 01570000
*                     MGPPRINT=ON;  /* PRINT MESSAGE                 */ 01571000
*                     MGPHLDS=ON;   /* LOG MESSAGE                   */ 01572000
         OI    MGPPRINT,B'10010000'                                0451 01573000
*                     MTYPE=IOPPIND(P)-IOPPREG;/* CHANGE TYPE TO   0452 01574000
*                                      NON-REGRESSED TYPE VALUE    0452 01575000
*                                                            @Z67SP00*/ 01576000
         L     @12,IOPPTR                                          0452 01577000
         L     @08,P                                               0452 01578000
         MH    @08,@CH00033                                        0452 01579000
         SLR   @15,@15                                             0452 01580000
         IC    @15,IOPPIND-9(@08,@12)                              0452 01581000
         SH    @15,@CH00599                                        0452 01582000
         STC   @15,MTYPE                                           0452 01583000
*                     MGPVARPT(1)=ADDR(MTYPEVAR(MTYPE));/* ELEMENT 0453 01584000
*                                      TYPE                          */ 01585000
         MH    @15,@CH00201                                        0453 01586000
         LA    @08,MTYPEVAR-6(@15)                                 0453 01587000
         ST    @08,MGPVARPT                                        0453 01588000
*                     MGPVARPT(2)=ADDR(ICTMNAME(1));/* ELEMENT NAME  */ 01589000
         L     @08,@PC00004                                        0454 01590000
         L     @08,MODPTR(,@08)                                    0454 01591000
         ST    @08,MGPVARPT+4                                      0454 01592000
*                     MGPVARPT(3)=ADDR(IOPNAM17);/* PREVIOUS PTF   0455 01593000
*                                      NUMBER                        */ 01594000
         LA    @12,IOPNAM17(,@12)                                  0455 01595000
         ST    @12,MGPVARPT+8                                      0455 01596000
*                     MGPVARPT(4)=ADDR(ICTPTFS(1));/* CURRENT PTF  0456 01597000
*                                      NUMBER                        */ 01598000
         L     @12,ICTPPTR(,@08)                                   0456 01599000
         ST    @12,MGPVARPT+12                                     0456 01600000
*                     CALL HMASMMSG(HMASMMGP);/* ISSUE MESSAGE       */ 01601000
         L     @15,@CV01723                                        0457 01602000
         LA    @01,@AL00457                                        0457 01603000
