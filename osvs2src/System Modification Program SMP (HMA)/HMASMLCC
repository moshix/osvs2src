*                                                                  0394 00748000
*   /*****************************************************************/ 00749000
*   /*                                                               */ 00750000
*   /* SORT ROUTINE FOR A FULL LIST USING A GTA FILE                 */ 00751000
*   /*                                                               */ 00752000
*   /*****************************************************************/ 00753000
*                                                                  0394 00754000
*LCCSORT:                                                          0394 00755000
*   PROCEDURE;                      /* SORT PROC FOR FULL LIST       */ 00756000
LCCSORT  STM   @14,@12,@SA00007                                    0394 00757000
*   DCL                                                            0395 00758000
*     SORTRC FIXED(8);              /*                               */ 00759000
*   DCL                                                            0396 00760000
*     LCCIOP CHAR(LENGTH(HMASMIOP)+LENGTH(IOPCQMAP));              0396 00761000
*   IOPPTR2=ADDR(LCCIOP);           /*                               */ 00762000
         LA    IOPPTR2,LCCIOP                                      0397 00763000
*   RFY                                                            0398 00764000
*     HMASMIOP BASED(IOPPTR2);      /*                               */ 00765000
*   RFY                                                            0399 00766000
*     HMASMGTP BASED(GTPPTR1);      /*                               */ 00767000
*   SORTRC=ZERO;                    /*                               */ 00768000
         SLR   SORTRC,SORTRC                                       0400 00769000
*   RESETSW=OFF;                    /*                               */ 00770000
         NI    RESETSW,B'01111111'                                 0401 00771000
*   HMASMGTP=''B;                   /* ZERO OUT THE GTP              */ 00772000
         XC    HMASMGTP(28,GTPPTR1),HMASMGTP(GTPPTR1)              0402 00773000
*   GTPBUFAD=ADDR(SMDRCD);          /* SETUP THE GTP TO ADD RECORDS  */ 00774000
         LA    @10,SMDRCD                                          0403 00775000
         ST    @10,GTPBUFAD(,GTPPTR1)                              0403 00776000
*   GTPRCDLN=LENGTH(SMDRCD);        /*                               */ 00777000
         LA    @10,14                                              0404 00778000
         STH   @10,GTPRCDLN(,GTPPTR1)                              0404 00779000
*   GTPKEYLN=LENGTH(SMDRCD);        /*                               */ 00780000
         STH   @10,GTPKEYLN(,GTPPTR1)                              0405 00781000
*   GTPFUNCT=GTPADD;                /*                               */ 00782000
         MVI   GTPFUNCT(GTPPTR1),X'04'                             0406 00783000
*   HMASMIOP=''B;                   /* ZERO OUT THE NEW IOP          */ 00784000
         XC    HMASMIOP(68,IOPPTR2),HMASMIOP(IOPPTR2)              0407 00785000
*   IOPBUFAD=IOPPTR->IOPBUFAD;                                     0408 00786000
         MVC   IOPBUFAD(4,IOPPTR2),IOPBUFAD(IOPPTR)                0408 00787000
*   IOPDSID=IOPPTR->IOPDSID;        /* READ FROM DATASET IN PASSIOP  */ 00788000
         MVC   IOPDSID(1,IOPPTR2),IOPDSID(IOPPTR)                  0409 00789000
*   IOPTYPE=IOPCSR;                 /* SET THE TYPE TO SYSMOD        */ 00790000
         MVI   IOPTYPE(IOPPTR2),X'0F'                              0410 00791000
*   IOPFUNCT=IOPLOC;                /* L O C A T E                   */ 00792000
         MVI   IOPFUNCT(IOPPTR2),X'03'                             0411 00793000
*   DO G=1 BY 1 WHILE IOPPTR->IOPQFMND(G)^=IOPEOLST&IOPRETRN<=IOPEOF&   00794000
*         SORTRC=ZERO;              /*                               */ 00795000
         LA    G,1                                                 0412 00796000
         B     @DE00412                                            0412 00797000
@DL00412 DS    0H                                                  0413 00798000
*     IF RESETSW=ON THEN            /*                               */ 00799000
         TM    RESETSW,B'10000000'                                 0413 00800000
         BNO   @RF00413                                            0413 00801000
*       DO;                         /* THIS SWITCH IS USED TO STOP A */ 00802000
*         RESETSW=OFF;              /* DOWHILE LOOP IN SUBSEQUENT  0415 00803000
*                                      CODE                          */ 00804000
         NI    RESETSW,B'01111111'                                 0415 00805000
*         IOPTYPE=IOPCSR;           /* IF THE SWITCH IS ON RESET IT  */ 00806000
         MVI   IOPTYPE(IOPPTR2),X'0F'                              0416 00807000
*         IOPFUNCT=IOPLOC;          /* ALSO RESET THE IOP TO PERFORM    00808000
*                                      A                             */ 00809000
