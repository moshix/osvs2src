         MVC   TXT1(7),@CC01353                                    0374 00683000
*             END;                  /*                               */ 00684000
*           ELSE                    /*                               */ 00685000
*             NUM1(A)=SMDNUM;       /* FILL IN THE LINE              */ 00686000
         B     @RC00362                                            0376 00687000
@RF00362 LR    @10,A                                               0376 00688000
         MH    @10,@CH00383                                        0376 00689000
         LA    @01,NUM1-10(@10)                                    0376 00690000
         MVC   0(7,@01),SMDNUM                                     0376 00691000
*         END;                      /*                               */ 00692000
@RC00362 DS    0H                                                  0378 00693000
*     END;                          /*                               */ 00694000
@RF00360 AH    A,@CH00041                                          0378 00695000
         CH    A,@CH00084                                          0378 00696000
         BNH   @DL00358                                            0378 00697000
@DC00358 DS    0H                                                  0379 00698000
*     IF A>DIM(REQNUM1)|GTPRETRN=GTPEOF THEN                       0379 00699000
