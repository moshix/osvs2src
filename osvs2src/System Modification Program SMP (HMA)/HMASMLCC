         BH    @RT00379                                            0379 00701000
         CLI   GTPRETRN(GTPPTR1),4                                 0379 00702000
         BNE   @RF00379                                            0379 00703000
@RT00379 DS    0H                                                  0380 00704000
*       DO;                         /* A WILL BE BUMPED TO NINE      */ 00705000
*         IF SWITCH=OFF THEN        /* IF SWITCH IS STILL OFF THEN 0381 00706000
*                                      ITS                           */ 00707000
         TM    SWITCH,B'10000000'                                  0381 00708000
         BNZ   @RF00381                                            0381 00709000
*           DO;                     /* A FLUSH                       */ 00710000
