         SLA   @10,2                                               0215 00230000
         C     @14,HEADPTR-4(@10)                                  0215 00231000
         BNE   @RF00215                                            0215 00232000
*       DO;                                                        0216 00233000
*         PRLBLANK=ON;              /* INDICATE BLANK HEADING        */ 00234000
         L     @10,IOPBUFAD(,@15)                                  0217 00235000
         OI    PRLBLANK(@10),B'00100000'                           0217 00236000
*         PRLHDADR=ZERO;            /* CLEAR HEADING ADDRESS         */ 00237000
         STCM  @14,7,PRLHDADR(@10)                                 0218 00238000
*       END;                                                       0219 00239000
*     ELSE                                                         0220 00240000
*       DO;                         /* NO PASS A HEADING ADDRESS     */ 00241000
         B     @RC00215                                            0220 00242000
@RF00215 DS    0H                                                  0221 00243000
*         PRLBLANK=OFF;             /* NOT A BLANK LINE              */ 00244000
         L     @10,IOPPTR                                          0221 00245000
         L     @10,IOPBUFAD(,@10)                                  0221 00246000
         NI    PRLBLANK(@10),B'11011111'                           0221 00247000
*         PRLHDADR=HEADPTR(HDINDX); /* INSERT THE HEAD ADDR          */ 00248000
         L     @15,HDINDX                                          0222 00249000
         SLA   @15,2                                               0222 00250000
         L     @15,HEADPTR-4(@15)                                  0222 00251000
         STCM  @15,7,PRLHDADR(@10)                                 0222 00252000
*       END;                                                       0223 00253000
*     CALL HMASMIO(HMASMIOP);       /* PASS TO I/O ROUTINE           */ 00254000
@RC00215 L     @10,IOPPTR                                          0224 00255000
         ST    @10,@AL00001                                        0224 00256000
         L     @15,@CV01395                                        0224 00257000
         LA    @01,@AL00001                                        0224 00258000
