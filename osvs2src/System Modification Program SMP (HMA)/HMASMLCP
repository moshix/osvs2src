         L     @10,IOPBUFAD(,@10)                                  0211 00214000
         MVC   PRLRLEN(2,@10),@CH01414                             0211 00215000
*   PRLHEADO=OFF;                   /* KILL THE RESET SWITCH         */ 00216000
*   PRLHEAD=ON;                     /* SET IN THE NEW HEADING        */ 00217000
         OI    PRLHEAD(@10),B'10000000'                            0213 00218000
         NI    PRLHEADO(@10),B'10111111'                           0213 00219000
*   DO HDINDX=1 TO DIM(HEADPTR)     /* LOOP USING ALL THE HD PTRS    */ 00220000
*         WHILE(IOPRETRN=IOPGOOD);  /*                               */ 00221000
         LA    @10,1                                               0214 00222000
         ST    @10,HDINDX                                          0214 00223000
@DL00214 L     @15,IOPPTR                                          0214 00224000
         CLI   IOPRETRN(@15),0                                     0214 00225000
         BNE   @DC00214                                            0214 00226000
*     IF HEADPTR(HDINDX)=ZERO       /* BLANK HEADING?                */ 00227000
*       THEN                                                       0215 00228000
