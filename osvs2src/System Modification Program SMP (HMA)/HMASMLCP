         LA    @14,HMASMSCP                                        0243 00329000
         ST    @14,CRPSCPAD                                        0243 00330000
*   CRPINDS=IOPPTS;                 /* INPUT FROM PTS                */ 00331000
         MVI   CRPINDS,X'06'                                       0244 00332000
*   HDSUBHD=TXTMCSHD;               /*                               */ 00333000
