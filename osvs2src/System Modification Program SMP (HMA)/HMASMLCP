@RF00183 L     @14,IOPPTR                                          0185 00077000
         CLI   IOPTYPE(@14),16                                     0185 00078000
         BNE   @RF00185                                            0185 00079000
*         CALL LCPMCS;              /* FORMAT MCS ENTRY              */ 00080000
         BAL   @14,LCPMCS                                          0186 00081000
*       ELSE                        /* IF NOT MCS ENTRY              */ 00082000
*         IF IOPTYPE=IOPSYS         /* IF SYSTEM ENTRY               */ 00083000
*           THEN                    /*                               */ 00084000
