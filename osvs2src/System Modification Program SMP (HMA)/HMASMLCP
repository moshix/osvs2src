         ST    @10,IOPPTR                                          0176 00027000
*   SAVBUFAD=IOPBUFAD;              /*                               */ 00028000
         L     @15,IOPBUFAD(,@10)                                  0177 00029000
         ST    @15,SAVBUFAD                                        0177 00030000
*   RFY                                                            0178 00031000
*     RTNCODE RSTD;                 /*                               */ 00032000
*   RTNCODE=ZERO;                   /* INIT RETURN CODE              */ 00033000
         SLR   RTNCODE,RTNCODE                                     0179 00034000
*   IF IOPTYPE^=LASTTYPE            /*                               */ 00035000
*       &IOPTYPE^=IOPSMD            /*                               */ 00036000
*       &RTNCODE=ZERO               /*                               */ 00037000
*     THEN                          /*                               */ 00038000
