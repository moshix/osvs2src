         CLI   IOPRETRN(@14),0                                     0252 00362000
         BNE   @RF00252                                            0252 00363000
*         DO;                       /*                               */ 00364000
*           CALL LCPIORTN;          /* READ AND PRINT FIRST CARD     */ 00365000
         BAL   @14,LCPIORTN                                        0254 00366000
*           CRPRETRN=RTNCODE;       /* SET RC                        */ 00367000
         STC   RTNCODE,CRPRETRN                                    0255 00368000
*           DO WHILE(               /* LOOP READING AND PRINTING     */ 00369000
*                 CRPRETRN=CRPGOOD);/* TILL EOF OR ERR               */ 00370000
         B     @DE00256                                            0256 00371000
@DL00256 DS    0H                                                  0257 00372000
*             CALL HMASMSCN(HMASMSCP);/* SCAN CONTROL CARD SO THAT 0257 00373000
*                                      CONTINUATION CARDS WILL RESULT   00374000
*                                      IN IO EXIT BEING CALLED SO  0257 00375000
*                                      THEY CAN ALSO BE PRINTED      */ 00376000
         L     @15,@CV01396                                        0257 00377000
         LA    @01,@AL00257                                        0257 00378000
