         L     @01,IOPBUFAD(,@10)                                  0226 00269000
         MVI   PRLFLGS(@01),B'00000000'                            0226 00270000
*   IOPBUFAD=SAVBUFAD;              /* RESTORE BUFFER ADDRESS        */ 00271000
         L     @15,SAVBUFAD                                        0227 00272000
         ST    @15,IOPBUFAD(,@10)                                  0227 00273000
*   IOPDSID=SETDSID;                /*                               */ 00274000
         IC    @15,SETDSID                                         0228 00275000
         STC   @15,IOPDSID(,@10)                                   0228 00276000
*   RETURN CODE(IOPRETRN);          /*                               */ 00277000
         SLR   @15,@15                                             0229 00278000
         IC    @15,IOPRETRN(,@10)                                  0229 00279000
         L     @14,@SA00002                                        0229 00280000
         LM    @00,@12,@SA00002+8                                  0229 00281000
         BR    @14                                                 0229 00282000
*   END LCPSETHD;                   /* RETURN TO CALLER              */ 00283000
         EJECT                                                          00284000
*                                                                  0231 00285000
*   /*****************************************************************/ 00286000
*   /*                                                               */ 00287000
*   /* ROUTINE CALLED TO FORMAT PTS MCS ENTRIES                      */ 00288000
*   /*                                                               */ 00289000
*   /*****************************************************************/ 00290000
*                                                                  0231 00291000
*LCPMCS:                            /*                               */ 00292000
*   PROCEDURE;                      /*                               */ 00293000
LCPMCS   STM   @14,@12,@SA00003                                    0231 00294000
*   RFY                                                            0232 00295000
*     HMASMIOP BASED(IOPPTR);       /*                               */ 00296000
*   RFY                                                            0233 00297000
*     RTNCODE RSTD;                 /*                               */ 00298000
*   RTNCODE=ZERO;                   /*                               */ 00299000
*                                                                  0234 00300000
         SLR   RTNCODE,RTNCODE                                     0234 00301000
*   /*****************************************************************/ 00302000
*   /*                                                               */ 00303000
*   /* SET UP SCP AND CRP PARAMETER LISTS                            */ 00304000
*   /*                                                               */ 00305000
*   /*****************************************************************/ 00306000
*                                                                  0235 00307000
*   HMASMSCP=''B;                   /*                               */ 00308000
         XC    HMASMSCP(26),HMASMSCP                               0235 00309000
*   SCPCHAR=ADDR(CRPBUFFR);         /* BUFFER ADDR                   */ 00310000
         LA    @14,CRPBUFFR                                        0236 00311000
         ST    @14,SCPCHAR                                         0236 00312000
*   SCPSRCH=ADDR(KPER1);            /* FIRST DSCAN ADDR              */ 00313000
         LA    @14,KPER1                                           0237 00314000
         ST    @14,SCPSRCH                                         0237 00315000
*   SCPIORTN=ADDR(LCPIORTN);        /* IO ROUTINE                    */ 00316000
         LA    @14,LCPIORTN                                        0238 00317000
         ST    @14,SCPIORTN                                        0238 00318000
*   SCPIOPAD=ADDR(HMASMCRP);        /* PARM TO IO ROUTINE            */ 00319000
         LA    @14,HMASMCRP                                        0239 00320000
         ST    @14,SCPIOPAD                                        0239 00321000
*   SCPINLN=LENGTH(CRPBFDTA);       /* LENGTH OF DATA IN BUFFER      */ 00322000
         MVC   SCPINLN(2),@CH01094                                 0240 00323000
*   SCPCOMNT=ON;                    /* SMP COMMENTS VALID            */ 00324000
         OI    SCPCOMNT,B'00100000'                                0241 00325000
*   HMASMCRP=''B;                   /*                               */ 00326000
