*       IOPFUNCT=IOPPOSI;           /*                               */ 00352000
         MVI   IOPFUNCT(@14),X'02'                                 0250 00353000
*       CALL HMASMIO(HMASMIOP);     /* POSITION FOR INPUT            */ 00354000
         ST    @14,@AL00001                                        0251 00355000
         L     @15,@CV01395                                        0251 00356000
         LA    @01,@AL00001                                        0251 00357000
         BALR  @14,@15                                             0251 00358000
