         AL    @10,HDINDX                                          0225 00262000
         ST    @10,HDINDX                                          0225 00263000
         C     @10,@CF00066                                        0225 00264000
         BNH   @DL00214                                            0225 00265000
@DC00214 DS    0H                                                  0226 00266000
*   PRLFLGS=OFF;                    /* RESET FLAGS                   */ 00267000
