*             CRPRETRN=SCPRETRN;    /* SAVE RC                       */ 00380000
         IC    @14,SCPRETRN                                        0258 00381000
         STC   @14,CRPRETRN                                        0258 00382000
*             DO UNTIL(             /* NOW READ ALL DATA CARDS       */ 00383000
*                   CRPRETRN^=CRPGOOD/* BETWEEN CONTROL CARDS        */ 00384000
*                   |CRPBUFFR(1:2)=PLUSPLUS);/*                      */ 00385000
@DL00259 DS    0H                                                  0260 00386000
*               CALL HMASMCRD(HMASMCRP);/* READ CARD                 */ 00387000
         L     @15,@CV01392                                        0260 00388000
         LA    @01,@AL00260                                        0260 00389000
         BALR  @14,@15                                             0260 00390000
*             END;                  /*                               */ 00391000
@DE00259 CLI   CRPRETRN,0                                          0261 00392000
         BNE   @DC00259                                            0261 00393000
         CLC   CRPBUFFR(2),@CC01400                                0261 00394000
         BNE   @DL00259                                            0261 00395000
@DC00259 DS    0H                                                  0262 00396000
*             IF CRPRETRN=CRPGOOD   /* IF NEXT CARD READ OK          */ 00397000
*                 &CRPBFDTA(1:2)=PLUSPLUS/* IF CONTROL CARD (NOT EOF)*/ 00398000
*               THEN                /* THEN                          */ 00399000
         CLI   CRPRETRN,0                                          0262 00400000
         BNE   @RF00262                                            0262 00401000
         CLC   CRPBFDTA(2),@CC01400                                0262 00402000
         BNE   @RF00262                                            0262 00403000
*               DO;                 /*                               */ 00404000
*                 HDMCS=CRPBUFFR;   /* WRITE IT OUT                  */ 00405000
         MVC   HDMCS(80),CRPBUFFR                                  0264 00406000
*                 CALL LCPWRITE;    /*                               */ 00407000
         BAL   @14,LCPWRITE                                        0265 00408000
*                 CRPRETRN=RTNCODE; /* SAVE RC FOR LOOP CNTL         */ 00409000
         STC   RTNCODE,CRPRETRN                                    0266 00410000
*               END;                /*                               */ 00411000
*           END;                    /* END PTS READ LOOP             */ 00412000
@RF00262 DS    0H                                                  0268 00413000
@DE00256 CLI   CRPRETRN,0                                          0268 00414000
         BE    @DL00256                                            0268 00415000
*           IF CRPRETRN<=CRPEOF     /* IF NO ERROR                   */ 00416000
*             THEN                  /*                               */ 00417000
         CLI   CRPRETRN,4                                          0269 00418000
         BH    @RF00269                                            0269 00419000
*             RTNCODE=ZERO;         /* SET RC                        */ 00420000
         SLR   RTNCODE,RTNCODE                                     0270 00421000
*           ELSE                    /*                               */ 00422000
*             RTNCODE=CRPRETRN;     /* PASS ALONG RC                 */ 00423000
         B     @RC00269                                            0271 00424000
@RF00269 SLR   RTNCODE,RTNCODE                                     0271 00425000
         IC    RTNCODE,CRPRETRN                                    0271 00426000
*         END;                      /*                               */ 00427000
*       ELSE                        /*                               */ 00428000
*         RTNCODE=IOPRETRN;         /* PASS ALONG RC                 */ 00429000
         B     @RC00252                                            0273 00430000
@RF00252 L     @10,IOPPTR                                          0273 00431000
