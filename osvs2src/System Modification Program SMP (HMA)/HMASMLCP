*       HDTYPE=TXTMCSHD;            /*                               */ 00171000
*                                                                  0202 00172000
         MVC   HDTYPE(19),TXTMCSHD                                 0202 00173000
*   /*****************************************************************/ 00174000
*   /*                                                               */ 00175000
*   /* SET UP IOP TO DELETE EXISTING HEADINGS AND CALL IO TO DELETE  */ 00176000
*   /*                                                               */ 00177000
*   /*****************************************************************/ 00178000
*                                                                  0203 00179000
*   IOPBUFAD=ADDR(OUTAREA);         /* AND AIM AT THE PRINT LINE     */ 00180000
@RF00201 DS    0H                                                  0203 00181000
@RC00199 L     @10,IOPPTR                                          0203 00182000
         LA    @15,OUTAREA                                         0203 00183000
         ST    @15,IOPBUFAD(,@10)                                  0203 00184000
*   IOPDSID=CCALSTDS;               /* INDICATE PRINTER DATA SET     */ 00185000
