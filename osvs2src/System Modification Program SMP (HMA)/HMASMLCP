*   PRLRLEN=ZERO;                   /* INDICATE ZERO RECORD LENGTH 0207 00193000
*                                      USED TO RESET HEADINGS        */ 00194000
         STCM  @14,3,PRLRLEN(@15)                                  0207 00195000
*   PRLFLGS=OFF;                    /* RESET ALL HEADINGS            */ 00196000
