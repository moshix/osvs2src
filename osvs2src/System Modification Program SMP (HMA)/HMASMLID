         TITLE 'HMASMLID - SMP LIST DRIVER ROUTINE'                     00001000
HMASMLID CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMLID  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  LA    @10,4095(,@12)                                      0001 00011000
         USING @PSTART,@12                                         0001 00012000
         USING @PSTART+4095,@10                                    0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
         MVC   @PC00001(4),0(@01)                                  0001 00018000
*                                                                  0173 00019000
*   /*****************************************************************/ 00020000
*   /*                                                               */ 00021000
*   /* IF LIST LOG REQUEST CALL HMASMLOG ELSE PROCESS OTHER DATASET  */ 00022000
*   /*                                                               */ 00023000
*   /*****************************************************************/ 00024000
*                                                                  0173 00025000
*   LIDRTNCD=ZERO;                  /*                               */ 00026000
         SLR   LIDRTNCD,LIDRTNCD                                   0173 00027000
*   IOPPTR=CCAIOPTR;                /* INITIAL SETUP                 */ 00028000
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0174 00029000
*   SAVBUFAD=IOPBUFAD;              /*                               */ 00030000
         L     SAVBUFAD,IOPBUFAD(,IOPPTR)                          0175 00031000
*   SETPTR=CCASETAD;                                               0176 00032000
