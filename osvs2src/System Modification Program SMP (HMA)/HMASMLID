         LA    @01,@AL00001                                        0204 00164000
         BALR  @14,@15                                             0204 00165000
*             RFY                                                  0205 00166000
*               RTNCODE RSTD;       /* CALL LC1 TO PEFORM            */ 00167000
*             LIDRC=MAX(LIDRC,RTNCODE);/* A CHECKING FUNCTION        */ 00168000
         CR    LIDRC,RTNCODE                                       0206 00169000
         BNL   *+6                                                      00170000
         LR    LIDRC,RTNCODE                                       0206 00171000
*             RFY                                                  0207 00172000
*               RTNCODE UNRSTD;     /* AND RETURN HERE WITH          */ 00173000
*             IF LIDRC=FOUR THEN                                   0208 00174000
         CH    LIDRC,@CH00098                                      0208 00175000
         BNE   @RF00208                                            0208 00176000
*               DO;                 /* A RC TO CHECK                 */ 00177000
*                 LIDRC=ZERO;       /* RC=0 GOOD                     */ 00178000
