         L     GTPPTR1,CCAGTP1(,CCAPTR)                            0177 00035000
*   IF LISTLOG=ON THEN                                             0178 00036000
         L     @07,@PC00001                                        0178 00037000
         TM    LISTLOG(@07),B'01000000'                            0178 00038000
         BNO   @RF00178                                            0178 00039000
*     DO;                           /* CHECK IF THE LISTLOG          */ 00040000
*       CALL HMASMLOG(LISTPARM);    /* FUNCT WAS REQUESTED           */ 00041000
         ST    @07,@AL00001                                        0180 00042000
         L     @15,@CV01384                                        0180 00043000
         LA    @01,@AL00001                                        0180 00044000
         BALR  @14,@15                                             0180 00045000
*       RFY                                                        0181 00046000
*         RTNCODE RSTD;             /* IF YES, PROCESS AND           */ 00047000
*       LIDRTNCD=MAX(LIDRTNCD,RTNCODE);/* RETURN TO THE CALLER       */ 00048000
         CR    LIDRTNCD,RTNCODE                                    0182 00049000
         BNL   *+6                                                      00050000
         LR    LIDRTNCD,RTNCODE                                    0182 00051000
*       RFY                                                        0183 00052000
*         RTNCODE UNRSTD;           /*                               */ 00053000
*     END;                          /* AFTER PROCESSING THE LISTLOG     00054000
*                                      REQUEST SKIP                  */ 00055000
*                                                                  0185 00056000
*   /*****************************************************************/ 00057000
*   /*                                                               */ 00058000
*   /* IF NOT LIST LOG REQUEST THEN PROCESS IT HERE                  */ 00059000
*   /*                                                               */ 00060000
*   /*****************************************************************/ 00061000
*                                                                  0185 00062000
*   ELSE                                                           0185 00063000
*     DO;                           /* THE REST OF THE CODE AND    0185 00064000
*                                      CLEANUP BEFORE EXIT           */ 00065000
         B     @RC00178                                            0185 00066000
@RF00178 DS    0H                                                  0186 00067000
*       RFY                                                        0186 00068000
*         HMASMGTP BASED(CCAGTP1);  /* ITS NOT THE LOG FUNCT         */ 00069000
*       CALL LIDINIT;               /* START SETTING UP FOR MASS OR     00070000
*                                      SELECT MODE                   */ 00071000
         BAL   @14,LIDINIT                                         0187 00072000
*       RFY                                                        0188 00073000
*         RTNCODE RSTD;             /*                               */ 00074000
*       LIDRTNCD=MAX(LIDRTNCD,RTNCODE);/* SAVE THE RETURN CODE       */ 00075000
         CR    LIDRTNCD,RTNCODE                                    0189 00076000
         BNL   *+6                                                      00077000
         LR    LIDRTNCD,RTNCODE                                    0189 00078000
*       RFY                                                        0190 00079000
*         RTNCODE UNRSTD;           /* FROM LIDINIT                  */ 00080000
*                                                                  0190 00081000
*       /*************************************************************/ 00082000
*       /*                                                           */ 00083000
*       /* LOOP READING RECORDS FROM EITHER SPECIFIED DATASET OF SET */ 00084000
*       /* FILE TILL END OF FILE OR ERROR OCCURS                     */ 00085000
*       /*                                                           */ 00086000
*       /*************************************************************/ 00087000
*                                                                  0191 00088000
*       DO WHILE(                   /*                               */ 00089000
*             LIDRTNCD<STOPPTF      /* WHILE RETURN OK               */ 00090000
*             &EOFSW=OFF);          /* AND NOT END OF FILE           */ 00091000
         B     @DE00191                                            0191 00092000
@DL00191 DS    0H                                                  0192 00093000
*         LIDRC=ZERO;               /* INIT RC FOR THIS ENTRY        */ 00094000
         SLR   LIDRC,LIDRC                                         0192 00095000
*         SKIPSW=OFF;               /* RESET THE SKIP SWITCH         */ 00096000
         NI    SKIPSW,B'11011111'                                  0193 00097000
*         RFY                       /*                               */ 00098000
*           RTNCODE RSTD;           /*                               */ 00099000
*                                                                  0194 00100000
*         /***********************************************************/ 00101000
*         /*                                                         */ 00102000
*         /* GET NEXT RECORD TO FOMAT BASED ON MODE OF REQUEST       */ 00103000
*         /* (EITHER MASS OR SELECT)                                 */ 00104000
*         /*                                                         */ 00105000
*         /***********************************************************/ 00106000
*                                                                  0195 00107000
*         IF LISTSEL=OFF            /* IF MASS MODE REQUEST          */ 00108000
*             |LISTSCDS=ON          /* OR LIST SCDS                  */ 00109000
*           THEN                    /*                               */ 00110000
         L     @14,@PC00001                                        0195 00111000
         TM    LISTSEL(@14),B'10000000'                            0195 00112000
         BZ    @RT00195                                            0195 00113000
         TM    LISTSCDS(@14),B'00010000'                           0195 00114000
         BNO   @RF00195                                            0195 00115000
@RT00195 DS    0H                                                  0196 00116000
*           CALL LIDMASS;           /* CALL MASS MODE RTN AND PROCESS   00117000
*                                      REQ                           */ 00118000
         BAL   @14,LIDMASS                                         0196 00119000
*         ELSE                      /* IF SELECT MODE REQ            */ 00120000
*           CALL LIDSELCT;          /* CALL SELECT MODE RTN AND    0197 00121000
*                                      PROCESS REQ                   */ 00122000
         B     @RC00195                                            0197 00123000
@RF00195 BAL   @14,LIDSELCT                                        0197 00124000
*         LIDRC=MAX(LIDRC,RTNCODE); /*                               */ 00125000
@RC00195 CR    LIDRC,RTNCODE                                       0198 00126000
         BNL   *+6                                                      00127000
         LR    LIDRC,RTNCODE                                       0198 00128000
*         RFY                                                      0199 00129000
*           RTNCODE UNRSTD;         /*                               */ 00130000
*         IF LIDRC=ZERO THEN        /*                               */ 00131000
         CH    LIDRC,@CH00304                                      0200 00132000
         BNE   @RF00200                                            0200 00133000
*           HOLDTYPE=IOPTYPE;       /* HOLD THE TYPE UNTIL           */ 00134000
*                                                                  0201 00135000
         MVC   HOLDTYPE(1),IOPTYPE(IOPPTR)                         0201 00136000
*         /***********************************************************/ 00137000
*         /*                                                         */ 00138000
*         /* IF NOAPPLY OR NOACCEPT SPECIFIED CALL HMASMLC1 TO DO    */ 00139000
*         /* CROSS CDS/ACDS CHECKING                                 */ 00140000
*         /*                                                         */ 00141000
*         /***********************************************************/ 00142000
*                                                                  0202 00143000
*         IF LIDRC=ZERO             /* IF ALL OK                     */ 00144000
*             &EOFSW=OFF            /* AND NOT END OF FILE           */ 00145000
*             &IOPTYPE=IOPSMD       /* AND SYSMOD TYPE RECORD        */ 00146000
*             &(LISTNOAP=ON|LISTNOAC=ON)/* AND NOAPPLY OR NOACCEPT   */ 00147000
*           THEN                    /*                               */ 00148000
@RF00200 CH    LIDRC,@CH00304                                      0202 00149000
         BNE   @RF00202                                            0202 00150000
         TM    EOFSW,B'10000000'                                   0202 00151000
         BNZ   @RF00202                                            0202 00152000
