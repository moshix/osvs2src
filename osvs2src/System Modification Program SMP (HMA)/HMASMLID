         BNE   @RF00202                                            0202 00154000
         L     @07,@PC00001                                        0202 00155000
         TM    LISTNOAP(@07),B'01100000'                           0202 00156000
         BZ    @RF00202                                            0202 00157000
*           DO;                                                    0203 00158000
*             CALL HMASMLC1(LISTPARM,HMASMIOP);/* IF CONDITIONS ARE     00159000
*                                      MET                           */ 00160000
