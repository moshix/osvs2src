         TITLE 'HMASMMPE - MODIFICATION ELEMENT PARSE ROUTINE'          00001000
HMASMMPE CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMMPE  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  LA    @10,4095(,@12)                                      0001 00011000
         LA    @09,4095(,@10)                                      0001 00012000
         USING @PSTART,@12                                         0001 00013000
         USING @PSTART+4095,@10                                    0001 00014000
         USING @PSTART+8190,@09                                    0001 00015000
         ST    @13,@SA00001+4                                      0001 00016000
         LA    @14,@SA00001                                        0001 00017000
         ST    @14,8(,@13)                                         0001 00018000
         LR    @13,@14                                             0001 00019000
         MVC   @PC00001(4),0(@01)                                  0001 00020000
*                                                                  0223 00021000
*   /*****************************************************************/ 00022000
*   /*                                                               */ 00023000
*   /* START OF HMASMMPE FUNCTION. INITIALIZE HMASMSCN & HMASMMSG    */ 00024000
*   /* PARMS, THE MCB AND LOCAL DECLARES                             */ 00025000
*   /*                                                               */ 00026000
*   /*****************************************************************/ 00027000
*                                                                  0224 00028000
*   CALL MPESETUP;                  /* SETUP AND INITIALIZE PROC     */ 00029000
*                                                                  0224 00030000
         BAL   @14,MPESETUP                                        0224 00031000
*   /*****************************************************************/ 00032000
*   /*                                                               */ 00033000
*   /* SCAN THE ELEMENT MODIFICATION CONTROL STATEMENTS OF THE PTF   */ 00034000
*   /* THE PROGRAM LOOPS BACK TO HERE WHEN THE PROCESSING FOR THE    */ 00035000
*   /* ELEMENT HAS BEEN COMPLETED AND THE MCB FOR THAT ELEMENT HAS   */ 00036000
*   /* BEEN BUILT                                                    */ 00037000
*   /*                                                               */ 00038000
*   /*****************************************************************/ 00039000
*                                                                  0225 00040000
*   CALL MPESCAN;                   /* SCAN THE ELEMENT CONTROL    0225 00041000
*                                      STATEMENTS                    */ 00042000
*                                                                  0225 00043000
         BAL   @14,MPESCAN                                         0225 00044000
*   /*****************************************************************/ 00045000
*   /*                                                               */ 00046000
*   /* IF SCAN COMPLETED SUCCESSFULLY & IF IOP ADDRESS PASSED THEN   */ 00047000
*   /* BUILD THE IOPPNTRY FOR THIS ELEMENT FROM DATA IN THE MCB      */ 00048000
*   /*                                                               */ 00049000
*   /*****************************************************************/ 00050000
*                                                                  0226 00051000
*   IF MPECODE=SCPGOOD              /* WAS SCAN PROCESS OK           */ 00052000
*       &MPPIOPAD^=ZERO             /* WAS IOP ADDRESS PASSED        */ 00053000
*     THEN                          /* YES                           */ 00054000
         SLR   @08,@08                                             0226 00055000
         C     @08,MPECODE                                         0226 00056000
         BNE   @RF00226                                            0226 00057000
         L     @15,@PC00001                                        0226 00058000
         C     @08,MPPIOPAD(,@15)                                  0226 00059000
         BE    @RF00226                                            0226 00060000
*     CALL MPEIOPBD;                /* CALL IOPPNTRY BUILD PROCEDURE */ 00061000
         BAL   @14,MPEIOPBD                                        0227 00062000
*   GOTO ENDOFMPE;                  /* GO END HMASMMPE PROCESSING    */ 00063000
         B     ENDOFMPE                                            0228 00064000
*                                                                  0229 00065000
*   /*****************************************************************/ 00066000
*   /*                                                               */ 00067000
*   /* MPESETUP - SETUP SUBROUTINE PARAMETER LISTS AND INITIALIZE    */ 00068000
*   /* LOCAL FIELDS AND THE MCB                                      */ 00069000
*   /*                                                               */ 00070000
*   /*****************************************************************/ 00071000
*                                                                  0229 00072000
*MPESETUP:                                                         0229 00073000
*   PROC;                                                          0229 00074000
*                                                                  0229 00075000
MPESETUP STM   @14,@12,12(@13)                                     0229 00076000
*   /*****************************************************************/ 00077000
*   /*                                                               */ 00078000
*   /* CLEAR THE MCB AND INITIALIZE LOCAL VARIABLES                  */ 00079000
*   /*                                                               */ 00080000
*   /*****************************************************************/ 00081000
*                                                                  0230 00082000
*   MPECODE=ZERO;                   /* INIT RETURN CODE              */ 00083000
         SLR   @08,@08                                             0230 00084000
         ST    @08,MPECODE                                         0230 00085000
*   ASMPGMLN=ZERO;                  /* INIT ASM PGM LEN ON JCLIN     */ 00086000
         MVI   ASMPGMLN,X'00'                                      0231 00087000
*   CPYPGMLN=ZERO;                  /* INIT CPY PGM LEN ON JCLIN     */ 00088000
         MVI   CPYPGMLN,X'00'                                      0232 00089000
*   LKEPGMLN=ZERO;                  /* INIT LKE PGM LEN ON JCLIN     */ 00090000
         MVI   LKEPGMLN,X'00'                                      0233 00091000
*   UPDPGMLN=ZERO;                  /* INIT UPD PGM LEN ON JCLIN     */ 00092000
         MVI   UPDPGMLN,X'00'                                      0234 00093000
*   LOCALBIT=''B;                   /* INIT KEYWORD BITS             */ 00094000
         XC    LOCALBIT(4),LOCALBIT                                0235 00095000
*   XTRABITS=''B;                   /* INIT KEYWORD BITS             */ 00096000
         MVI   XTRABITS,X'00'                                      0236 00097000
*   MCBELEMP='';                    /* INITIALIZE MCB TO BLANKS      */ 00098000
         L     @08,@PC00001                                        0237 00099000
         L     @15,MPPMCBAD(,@08)                                  0237 00100000
         MVI   MCBELEMP(@15),C' '                                  0237 00101000
         MVC   MCBELEMP+1(140,@15),MCBELEMP(@15)                   0237 00102000
*   MCBELASM=ZERO;                  /* ZERO JCLIN ASM NAME LENGTH    */ 00103000
         MVI   MCBELASM(@15),X'00'                                 0238 00104000
*   MCBELCPY=ZERO;                  /* ZERO JCLIN COPY NAME LENGTH   */ 00105000
         MVI   MCBELCPY(@15),X'00'                                 0239 00106000
*   MCBELUPI=ZERO;                  /* ZERO JCLIN UPDATE NAME LENGTH */ 00107000
         MVI   MCBELUPI(@15),X'00'                                 0240 00108000
*   MCBELLKD=ZERO;                  /* ZERO JCLIN LKED NAME LENGTH   */ 00109000
         MVI   MCBELLKD(@15),X'00'                                 0241 00110000
*   MCBELEPM=''B;                   /* TURN OFF BITS                 */ 00111000
         XC    MCBELEPM(4,@15),MCBELEPM(@15)                       0242 00112000
*   MCBEFLG5=''B;                   /* TURN OFF BITS                 */ 00113000
         MVI   MCBEFLG5(@15),X'00'                                 0243 00114000
*   MCBEND(1)=IOPEOLST;             /* INIT END OF MCB               */ 00115000
*                                                                  0244 00116000
         MVI   MCBEND(@15),X'FF'                                   0244 00117000
*   /*****************************************************************/ 00118000
*   /*                                                               */ 00119000
*   /* HMASMSCN PARAMETER INITIALIZATION                             */ 00120000
*   /*                                                               */ 00121000
*   /*****************************************************************/ 00122000
*                                                                  0245 00123000
*   SCPCHAR=ADDR(CRPBUFFR);         /* INPUT BUFFER                  */ 00124000
         L     @15,MPPCRDPM(,@08)                                  0245 00125000
         LA    @14,CRPBUFFR(,@15)                                  0245 00126000
         ST    @14,SCPCHAR                                         0245 00127000
*   SCPSRCH=AMODSCAN;               /* SCANS FOR MODULE SEARCH       */ 00128000
         MVC   SCPSRCH(4),AMODSCAN                                 0246 00129000
*   SCPIORTN=MPPCRDAD;              /* FOR ADDITIONAL RECORDS DURING    00130000
*                                      SCAN GO HERE                  */ 00131000
         MVC   SCPIORTN(4),MPPCRDAD(@08)                           0247 00132000
*   SCPIOPAD=MPPCRDPM;              /* READ ROUTINE PARAMETER LIST   */ 00133000
         ST    @15,SCPIOPAD                                        0248 00134000
*   SCPINLN=LENGTH(CRPBFCTL);       /* SET LENGTH OF INPUT BUFFER=72 */ 00135000
         MVC   SCPINLN(2),@CH00839                                 0249 00136000
*   SCPEOR=ZERO;                    /* CLEAR END OF RECORD FLAG      */ 00137000
         MVI   SCPEOR,X'00'                                        0250 00138000
*   SCPCOMNT=ON;                    /* SET SKIP-BY COMMENTS DURING 0251 00139000
*                                      SCAN                          */ 00140000
*   SCPCONT=ON;                     /* SET READ CONTINUATION RECORD  */ 00141000
*   SCPRTERR=OFF;                   /* SET OFF ROUT ERROR MSG SWITCH */ 00142000
         OI    SCPCOMNT,B'10100000'                                0253 00143000
         NI    SCPRTERR,B'11101111'                                0253 00144000
*   CRPSCPAD=ADDR(HMASMSCP);        /* ADDRESS OF SCAN PARM FOR    0254 00145000
*                                      HMASMCRD                      */ 00146000
*                                                                  0254 00147000
         LA    @08,HMASMSCP                                        0254 00148000
         ST    @08,CRPSCPAD(,@15)                                  0254 00149000
*   /*****************************************************************/ 00150000
*   /*                                                               */ 00151000
*   /* HMASMMSG PARAMETER INITIALIZATION                             */ 00152000
*   /*                                                               */ 00153000
*   /*****************************************************************/ 00154000
*                                                                  0255 00155000
*   HMASMMGP=''B;                   /* CLEAR MGP                     */ 00156000
         XC    HMASMMGP(28),HMASMMGP                               0255 00157000
*   MGPHLDS=ON;                     /* MESSAGES TO LOG               */ 00158000
*   MGPPRINT=ON;                    /* MESSAGES TO PRINTER           */ 00159000
         OI    MGPHLDS,B'10010000'                                 0257 00160000
*   END MPESETUP;                                                  0258 00161000
@EL00002 DS    0H                                                  0258 00162000
@EF00002 DS    0H                                                  0258 00163000
@ER00002 LM    @14,@12,12(@13)                                     0258 00164000
         BR    @14                                                 0258 00165000
*                                                                  0259 00166000
*   /*****************************************************************/ 00167000
*   /*                                                               */ 00168000
*   /* SCAN - SCAN THE ELEMENT MODIFICATION CONTROL STATEMENTS OF THE*/ 00169000
*   /* SYSMOD FOR SYNTAX, MUTUALLY EXCLUSIVE KEYWORDS OR ANY         */ 00170000
*   /* COMBINATION OF INVALID CONDITIONS. THE ELEMENT TYPES ARE:     */ 00171000
*   /* ++MOD ++MAC ++ZAP ++SRC ++SRCUPD ++UPDTE ++MACUPD+ ++JCLIN.   */ 00172000
*   /* HMASMSCN WILL CALL THE PROPER DSCAN ROUT TO PROCESS THE       */ 00173000
*   /* KEYWORDS AND SAVE THEIR VALUES IN THE MCB BEING BUILT.        */ 00174000
*   /*                                                               */ 00175000
*   /*****************************************************************/ 00176000
*                                                                  0259 00177000
*MPESCAN:                                                          0259 00178000
*   PROC;                                                          0259 00179000
MPESCAN  STM   @14,@12,@SA00003                                    0259 00180000
*   CALL HMASMSCN(HMASMSCP);        /* SCAN MODULE HEADER            */ 00181000
*                                                                  0260 00182000
         L     @15,@CV01419                                        0260 00183000
         LA    @01,@AL00260                                        0260 00184000
         BALR  @14,@15                                             0260 00185000
*   /*****************************************************************/ 00186000
*   /*                                                               */ 00187000
*   /* DETERMINE IF HMASMSCN & HMASMCRD (OPTIONALLY) WERE SUCCESSFULL*/ 00188000
*   /*                                                               */ 00189000
*   /*****************************************************************/ 00190000
*                                                                  0261 00191000
*   IF SCPRET=SCPGOOD THEN          /* IF SCAN SUCCESSFUL            */ 00192000
         CLI   SCPRET,0                                            0261 00193000
         BNE   @RF00261                                            0261 00194000
*     DO;                                                          0262 00195000
*       IF CRPRETRN^=CRPGOOD THEN   /* IF UNUSAL READ CONDITION      */ 00196000
         L     @08,@PC00001                                        0263 00197000
         L     @08,MPPCRDPM(,@08)                                  0263 00198000
         CLI   CRPRETRN(@08),0                                     0263 00199000
         BE    @RF00263                                            0263 00200000
*         DO;                                                      0264 00201000
*           IF CRPRETRN=CRPEOF THEN /* UNEXPECTED FILE END           */ 00202000
         CLI   CRPRETRN(@08),4                                     0265 00203000
         BNE   @RF00265                                            0265 00204000
*             DO;                                                  0266 00205000
*               CRPRETRN=CRPTPTF;   /* STOP THE PTF                  */ 00206000
         MVI   CRPRETRN(@08),X'08'                                 0267 00207000
*               CALL UCRDEOF;       /* INCOMPLETE CNTL STMT MSG      */ 00208000
         BAL   @14,UCRDEOF                                         0268 00209000
*             END;                                                 0269 00210000
*         END;                                                     0270 00211000
*     END;                                                         0271 00212000
*   ELSE                                                           0272 00213000
*     DO;                           /* IF SCAN WAS UNSUCCESSFUL      */ 00214000
         B     @RC00261                                            0272 00215000
@RF00261 DS    0H                                                  0273 00216000
*       IF SCPRTERR=OFF THEN        /* IF ERROR MESSAGE NOT SENT     */ 00217000
         TM    SCPRTERR,B'00010000'                                0273 00218000
         BNZ   @RF00273                                            0273 00219000
*         CALL SYNTXMSG;            /* GENERATE SYNTAX MESSAGE       */ 00220000
         BAL   @14,SYNTXMSG                                        0274 00221000
*     END;                          /* END SCAN UNSUCCESSFULL        */ 00222000
@RF00273 DS    0H                                                  0276 00223000
*   MPECODE=MAX(MPECODE,SCPRET,CRPRETRN);/* HIGHEST RC IS IN MPECODE */ 00224000
@RC00261 L     @08,MPECODE                                         0276 00225000
         SLR   @15,@15                                             0276 00226000
         IC    @15,SCPRET                                          0276 00227000
         CR    @08,@15                                             0276 00228000
         BNL   *+6                                                      00229000
         LR    @08,@15                                             0276 00230000
         L     @15,@PC00001                                        0276 00231000
         L     @01,MPPCRDPM(,@15)                                  0276 00232000
         SLR   @00,@00                                             0276 00233000
         IC    @00,CRPRETRN(,@01)                                  0276 00234000
         CR    @08,@00                                             0276 00235000
         BNL   *+6                                                      00236000
         LR    @08,@00                                             0276 00237000
         ST    @08,MPECODE                                         0276 00238000
*   END MPESCAN;                                                   0277 00239000
@EL00003 DS    0H                                                  0277 00240000
@EF00003 DS    0H                                                  0277 00241000
@ER00003 LM    @14,@12,@SA00003                                    0277 00242000
         BR    @14                                                 0277 00243000
*                                                                  0278 00244000
*   /*****************************************************************/ 00245000
*   /*                                                               */ 00246000
*   /* PUT ELEMENT NAME AND TYPE INDICATOR INTO THE CURRENT PTF IOP  */ 00247000
*   /* VARIABLE ELEMENT LIST (EXCEPT IF ++JCLIN).                    */ 00248000
*   /*                                                               */ 00249000
*   /*****************************************************************/ 00250000
*                                                                  0278 00251000
*MPEIOPBD:                                                         0278 00252000
*   PROC;                           /*                               */ 00253000
*                                                                  0278 00254000
MPEIOPBD STM   @14,@12,@SA00004                                    0278 00255000
*   /*****************************************************************/ 00256000
*   /*                                                               */ 00257000
*   /* IF NO ERRORS DETECTED IN THE SCAN OF THE CONTROL STATEMENTS,  */ 00258000
*   /* THEN CONSTRUCT THE IOPPNTRY (EXCEPT FOR ++JCLIN).             */ 00259000
*   /*                                                               */ 00260000
*   /*****************************************************************/ 00261000
*                                                                  0279 00262000
*   IF JCL=ON THEN                  /* IS TYPE JCLIN                 */ 00263000
         TM    JCL,B'00000010'                                     0279 00264000
         BNO   @RF00279                                            0279 00265000
*     DO;                                                          0280 00266000
*                                                                  0280 00267000
*       /*************************************************************/ 00268000
*       /*                                                           */ 00269000
*       /* THIS ROUTINE FOR ++JCLIN ONLY                             */ 00270000
*       /*                                                           */ 00271000
*       /*************************************************************/ 00272000
*                                                                  0281 00273000
*       IF IOPJCLP=ON THEN                                         0281 00274000
         L     @08,@PC00001                                        0281 00275000
         L     @08,MPPIOPAD(,@08)                                  0281 00276000
         TM    IOPJCLP(@08),B'00000010'                            0281 00277000
         BNO   @RF00281                                            0281 00278000
*         DO;                       /* IF THIS IOP HAS JCLIN ALREAD  */ 00279000
*           MGPVARPT(1)=ADDR(CHJCLIN);/* FILL IN JCLIN IN MSG        */ 00280000
         LA    @08,@CC01449                                        0283 00281000
         ST    @08,MGPVARPT                                        0283 00282000
*           MGPVARPT(2)=ADDR(CHSYSMOD);/* FILL SYSMOD IN MSG         */ 00283000
         LA    @08,@CC01517                                        0284 00284000
         ST    @08,MGPVARPT+4                                      0284 00285000
*           CALL DUPKYWRD;          /* SEND MSG, SET RC=8, AND TERM  */ 00286000
         BAL   @14,DUPKYWRD                                        0285 00287000
*         END;                                                     0286 00288000
*       ELSE                                                       0287 00289000
*         IOPJCLP=ON;               /* INDICATE JCLIN FOR THIS IOP   */ 00290000
         B     @RC00281                                            0287 00291000
@RF00281 L     @08,@PC00001                                        0287 00292000
         L     @08,MPPIOPAD(,@08)                                  0287 00293000
         OI    IOPJCLP(@08),B'00000010'                            0287 00294000
*     END;                                                         0288 00295000
*   ELSE                                                           0289 00296000
*     DO;                                                          0289 00297000
*                                                                  0289 00298000
         B     @RC00279                                            0289 00299000
@RF00279 DS    0H                                                  0290 00300000
*       /*************************************************************/ 00301000
*       /*                                                           */ 00302000
*       /* DO THIS ROUTINE FOR ALL OTHER ELEMENT CONTROL STATEMENTS  */ 00303000
*       /*                                                           */ 00304000
*       /*************************************************************/ 00305000
*                                                                  0290 00306000
*                                                                  0290 00307000
*       /*************************************************************/ 00308000
*       /*                                                           */ 00309000
*       /* LOOP TILL EITHER THE END OF LIST (IOPEOLST) , A MATCHING  */ 00310000
*       /* ELEMENT NAME (DUPELEM) , PEMAX SIZE IS EXHAUSTED (PEMAX). */ 00311000
*       /*                                                           */ 00312000
*       /*************************************************************/ 00313000
*                                                                  0290 00314000
*       DO INDX=1 BY 1 TO CCAPEMAX WHILE IOPPTFND(INDX)^=IOPEOLST& 0290 00315000
*             MPECODE<STOPPTF;                                     0290 00316000
*                                                                  0290 00317000
         LA    @08,1                                               0290 00318000
         ST    @08,INDX                                            0290 00319000
         B     @DE00290                                            0290 00320000
@DL00290 MH    @08,@CH00035                                        0290 00321000
         L     @15,@PC00001                                        0290 00322000
         L     @14,MPPIOPAD(,@15)                                  0290 00323000
         SLR   @07,@07                                             0290 00324000
         IC    @07,IOPPTFND-9(@08,@14)                             0290 00325000
         CH    @07,@CH00628                                        0290 00326000
         BE    @DC00290                                            0290 00327000
         L     @07,MPECODE                                         0290 00328000
         CH    @07,@CH00119                                        0290 00329000
         BNL   @DC00290                                            0290 00330000
*         /***********************************************************/ 00331000
*         /*                                                         */ 00332000
*         /* CHECK FOR DUPLICATE ELEMENTS IN SYSMOD                  */ 00333000
*         /*                                                         */ 00334000
*         /***********************************************************/ 00335000
*                                                                  0291 00336000
*         IF IOPPMODS(INDX)=MCBENAME THEN/* TEST DUP ELEM            */ 00337000
         L     @01,MPPMCBAD(,@15)                                  0291 00338000
         ST    @08,@TF00001                                        0291 00339000
         ALR   @08,@14                                             0291 00340000
         CLC   IOPPMODS-9(8,@08),MCBENAME(@01)                     0291 00341000
         BNE   @RF00291                                            0291 00342000
*           DO;                                                    0292 00343000
*             IF MOD=ON&IOPPIND(INDX)=IOPPMOD THEN                 0293 00344000
         TM    MOD,B'10000000'                                     0293 00345000
         BNO   @RF00293                                            0293 00346000
         L     @08,@TF00001                                        0293 00347000
         SLR   @15,@15                                             0293 00348000
         IC    @15,IOPPIND-9(@08,@14)                              0293 00349000
         CH    @15,@CH00094                                        0293 00350000
         BNE   @RF00293                                            0293 00351000
*               CALL DUPELEM;                                      0294 00352000
         BAL   @14,DUPELEM                                         0294 00353000
*             IF MOD=ON&IOPPIND(INDX)=IOPPZAP THEN                 0295 00354000
@RF00293 TM    MOD,B'10000000'                                     0295 00355000
         BNO   @RF00295                                            0295 00356000
         L     @08,INDX                                            0295 00357000
         MH    @08,@CH00035                                        0295 00358000
         L     @15,@PC00001                                        0295 00359000
         L     @01,MPPIOPAD(,@15)                                  0295 00360000
         SLR   @00,@00                                             0295 00361000
         IC    @00,IOPPIND-9(@08,@01)                              0295 00362000
         CH    @00,@CH00190                                        0295 00363000
         BNE   @RF00295                                            0295 00364000
*               CALL DUPELEM;                                      0296 00365000
         BAL   @14,DUPELEM                                         0296 00366000
*             IF MAC=ON&IOPPIND(INDX)=IOPPMCR THEN                 0297 00367000
@RF00295 TM    MAC,B'01000000'                                     0297 00368000
         BNO   @RF00297                                            0297 00369000
         L     @08,INDX                                            0297 00370000
         MH    @08,@CH00035                                        0297 00371000
         L     @15,@PC00001                                        0297 00372000
         L     @01,MPPIOPAD(,@15)                                  0297 00373000
         SLR   @00,@00                                             0297 00374000
         IC    @00,IOPPIND-9(@08,@01)                              0297 00375000
         CH    @00,@CH00047                                        0297 00376000
         BNE   @RF00297                                            0297 00377000
*               CALL DUPELEM;                                      0298 00378000
         BAL   @14,DUPELEM                                         0298 00379000
*             IF MAC=ON&IOPPIND(INDX)=IOPPMCU THEN                 0299 00380000
@RF00297 TM    MAC,B'01000000'                                     0299 00381000
         BNO   @RF00299                                            0299 00382000
         L     @08,INDX                                            0299 00383000
         MH    @08,@CH00035                                        0299 00384000
         L     @15,@PC00001                                        0299 00385000
         L     @01,MPPIOPAD(,@15)                                  0299 00386000
         SLR   @00,@00                                             0299 00387000
         IC    @00,IOPPIND-9(@08,@01)                              0299 00388000
         CH    @00,@CH00137                                        0299 00389000
         BNE   @RF00299                                            0299 00390000
*               CALL DUPELEM;                                      0300 00391000
         BAL   @14,DUPELEM                                         0300 00392000
*             IF SRC=ON&IOPPIND(INDX)=IOPPSCR THEN                 0301 00393000
@RF00299 TM    SRC,B'00001000'                                     0301 00394000
         BNO   @RF00301                                            0301 00395000
         L     @08,INDX                                            0301 00396000
         MH    @08,@CH00035                                        0301 00397000
         L     @15,@PC00001                                        0301 00398000
         L     @01,MPPIOPAD(,@15)                                  0301 00399000
         SLR   @00,@00                                             0301 00400000
         IC    @00,IOPPIND-9(@08,@01)                              0301 00401000
         CH    @00,@CH00207                                        0301 00402000
         BNE   @RF00301                                            0301 00403000
*               CALL DUPELEM;                                      0302 00404000
         BAL   @14,DUPELEM                                         0302 00405000
*             IF SRC=ON&IOPPIND(INDX)=IOPPSCU THEN                 0303 00406000
@RF00301 TM    SRC,B'00001000'                                     0303 00407000
         BNO   @RF00303                                            0303 00408000
         L     @08,INDX                                            0303 00409000
         MH    @08,@CH00035                                        0303 00410000
         L     @15,@PC00001                                        0303 00411000
         L     @01,MPPIOPAD(,@15)                                  0303 00412000
         SLR   @00,@00                                             0303 00413000
         IC    @00,IOPPIND-9(@08,@01)                              0303 00414000
         CH    @00,@CH00043                                        0303 00415000
         BNE   @RF00303                                            0303 00416000
*               CALL DUPELEM;                                      0304 00417000
         BAL   @14,DUPELEM                                         0304 00418000
*           END;                                                   0305 00419000
@RF00303 DS    0H                                                  0306 00420000
*       END;                                                       0306 00421000
@RF00291 LA    @08,1                                               0306 00422000
         AL    @08,INDX                                            0306 00423000
