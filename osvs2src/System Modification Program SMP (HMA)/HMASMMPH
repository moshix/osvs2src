         TITLE 'HMASMMPH HEADER PARSE ROUTINE'                          00001000
HMASMMPH CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMMPH  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(4),0(@01)                                  0001 00017000
*                                      PTF ENTRY IF PTF DATA TO BE 0167 00018000
*                                      FILLED IN ELSE 0              */ 00019000
*   MPHRTNCD=ZERO;                  /* TO BE RETURNED IF NO ERRORS   */ 00020000
*                                                                  0168 00021000
         SLR   @10,@10                                             0168 00022000
         ST    @10,MPHRTNCD                                        0168 00023000
*   /*****************************************************************/ 00024000
*   /*                                                               */ 00025000
*   /* INITIALIZE CRP AND MCB                                        */ 00026000
*   /*                                                               */ 00027000
*   /*****************************************************************/ 00028000
*                                                                  0169 00029000
*   CRPSCPAD=ADDR(HMASMSCP);        /* INIT. CRD PARM. LIST          */ 00030000
         L     @10,@PC00001                                        0169 00031000
         L     @15,MPPCRDPM(,@10)                                  0169 00032000
         LA    @14,HMASMSCP                                        0169 00033000
         ST    @14,CRPSCPAD(,@15)                                  0169 00034000
*   MCBPNO=NULLPTF#;                /* CLEAR SYSMOD-ID FIELD         */ 00035000
         L     @14,MPPMCBAD(,@10)                                  0170 00036000
         MVC   MCBPNO(7,@14),@CC01438                              0170 00037000
*   MCBTYPE=MCBPTFTP;               /* SET CONSTANT PTF TYPE RECORD     00038000
*                                      IN MCB                        */ 00039000
         MVI   MCBTYPE(@14),X'01'                                  0171 00040000
*   MCBPRELN=''B;                   /* INIT. FILES BINARY 0'S        */ 00041000
*                                                                  0172 00042000
         SLR   @09,@09                                             0172 00043000
         STH   @09,MCBPRELN(,@14)                                  0172 00044000
*   /*****************************************************************/ 00045000
*   /*                                                               */ 00046000
*   /* INITIALIZE SCAN PARMS                                         */ 00047000
*   /*                                                               */ 00048000
*   /*****************************************************************/ 00049000
*                                                                  0173 00050000
*   SCPRET=SCPGOOD;                 /* GOOD RETURN CODE              */ 00051000
         MVI   SCPRET,X'00'                                        0173 00052000
*   SCPCHAR=ADDR(CRPBUFFR);         /* CHAR STRING TO BE SCANNED     */ 00053000
         LA    @14,CRPBUFFR(,@15)                                  0174 00054000
         ST    @14,SCPCHAR                                         0174 00055000
*   SCPSRCH=ADDR(KPLPL);            /* FIRST DSCAN MACRO USED IN SCAN*/ 00056000
         LA    @14,KPLPL                                           0175 00057000
         ST    @14,SCPSRCH                                         0175 00058000
*   SCPIORTN=MPPCRDAD;              /* ADDR OF USER I/O ROUTINE      */ 00059000
         MVC   SCPIORTN(4),MPPCRDAD(@10)                           0176 00060000
*   SCPIOPAD=MPPCRDPM;              /* ADDR OF PARM I/O ROUTINE      */ 00061000
         ST    @15,SCPIOPAD                                        0177 00062000
*   SCPINLN=LENGTH(CRPBFCTL);       /* INPUT RECORD LENGTH 72 COLUMNS*/ 00063000
         MVC   SCPINLN(2),@CH00839                                 0178 00064000
*   SCPEOR=ZERO;                    /* FLAG BYTE                     */ 00065000
         MVI   SCPEOR,X'00'                                        0179 00066000
*   SCPCOMNT=ON;                    /* CHECK FOR COMMENT DELIMITERS  */ 00067000
*   SCPCONT=ON;                     /* GO TO NEXT CARD               */ 00068000
*                                                                  0181 00069000
         OI    SCPCOMNT,B'10100000'                                0181 00070000
*   /*****************************************************************/ 00071000
*   /*                                                               */ 00072000
*   /* INITIALIZE MESSAGE PARMS                                      */ 00073000
*   /*                                                               */ 00074000
*   /*****************************************************************/ 00075000
*                                                                  0182 00076000
*   HMASMMGP=''B;                   /* CLEAR MGP                     */ 00077000
         XC    HMASMMGP(24),HMASMMGP                               0182 00078000
*   MGPHLDS=ON;                     /* MESSAGES TO LOG               */ 00079000
*   MGPPRINT=ON;                    /* SET FOR MSG TO PRINTER        */ 00080000
         OI    MGPHLDS,B'10010000'                                 0184 00081000
*                                                                  0185 00082000
*   /*****************************************************************/ 00083000
*   /*                                                               */ 00084000
*   /* SCAN CONTROL CARD FOR TYPE SYSMOD                             */ 00085000
*   /*                                                               */ 00086000
*   /*****************************************************************/ 00087000
*                                                                  0185 00088000
*   CALL HMASMSCN(HMASMSCP);                                       0185 00089000
*                                                                  0185 00090000
         L     @15,@CV01421                                        0185 00091000
         LA    @01,@AL00185                                        0185 00092000
         BALR  @14,@15                                             0185 00093000
*   /*****************************************************************/ 00094000
*   /*                                                               */ 00095000
*   /* IF END OF FILE BEFORE END OF COMPLETE STATEMENT CALL PROC TO  */ 00096000
*   /* ISSUE MSG HMA252I RETRN 8 TO CALLER                           */ 00097000
*   /*                                                               */ 00098000
*   /*****************************************************************/ 00099000
*                                                                  0186 00100000
*   IF CRPRETRN=CRPEOF              /*                               */ 00101000
*     THEN                          /*                               */ 00102000
         L     @10,@PC00001                                        0186 00103000
         L     @10,MPPCRDPM(,@10)                                  0186 00104000
         CLI   CRPRETRN(@10),4                                     0186 00105000
         BNE   @RF00186                                            0186 00106000
*     DO;                           /*                               */ 00107000
*       CALL INCOMPMG;              /*                               */ 00108000
         BAL   @14,INCOMPMG                                        0188 00109000
*       MPHRTNCD=STOPPTF;           /*                               */ 00110000
         MVC   MPHRTNCD(4),@CF00116                                0189 00111000
*     END;                          /*                               */ 00112000
*                                                                  0190 00113000
*   /*****************************************************************/ 00114000
*   /*                                                               */ 00115000
*   /* IF SYNTAX ERROR PUT OUT MESSAGE. IF SEVERE ERROR JUST SET RC  */ 00116000
*   /*                                                               */ 00117000
*   /*****************************************************************/ 00118000
*                                                                  0191 00119000
*   ELSE                            /*                               */ 00120000
*     IF SCPRET>=SCPBAD             /* IF ERROR IN SCAN              */ 00121000
*       THEN                        /*                               */ 00122000
         B     @RC00186                                            0191 00123000
@RF00186 CLI   SCPRET,4                                            0191 00124000
         BL    @RF00191                                            0191 00125000
*       DO;                         /*                               */ 00126000
*         IF SCPRET=SCPBAD          /*                               */ 00127000
*           THEN                    /*                               */ 00128000
         CLI   SCPRET,4                                            0193 00129000
         BNE   @RF00193                                            0193 00130000
*           CALL SYNTAXMG;          /*                               */ 00131000
         BAL   @14,SYNTAXMG                                        0194 00132000
*         MPHRTNCD=MAX(SCPRET,STOPPTF,MPHRTNCD);                   0195 00133000
@RF00193 SLR   @10,@10                                             0195 00134000
         IC    @10,SCPRET                                          0195 00135000
         LA    @15,8                                               0195 00136000
         CR    @10,@15                                             0195 00137000
         BNL   *+6                                                      00138000
         LR    @10,@15                                             0195 00139000
         L     @15,MPHRTNCD                                        0195 00140000
         CR    @10,@15                                             0195 00141000
         BNL   *+6                                                      00142000
         LR    @10,@15                                             0195 00143000
         ST    @10,MPHRTNCD                                        0195 00144000
*       END;                                                       0196 00145000
*   RETURN CODE(MPHRTNCD);                                         0197 00146000
@RF00191 DS    0H                                                  0197 00147000
@RC00186 L     @15,MPHRTNCD                                        0197 00148000
         L     @13,4(,@13)                                         0197 00149000
         L     @14,12(,@13)                                        0197 00150000
         LM    @00,@12,20(@13)                                     0197 00151000
         BR    @14                                                 0197 00152000
         EJECT                                                          00153000
*   RFY                                                            0198 00154000
*    (RTNCODE,                                                     0198 00155000
*     REG1,                                                        0198 00156000
*     RTNREG) RSTD;                                                0198 00157000
*                                                                  0198 00158000
*   /*****************************************************************/ 00159000
*   /*                                                               */ 00160000
*   /* MCB BIT SETTING FOR PTF                                       */ 00161000
*   /*                                                               */ 00162000
*   /*****************************************************************/ 00163000
*                                                                  0199 00164000
*RPTF:                                                             0199 00165000
*   MCBPTP=IOPPPTF;                 /* SET PTF BIT IN MCB            */ 00166000
RPTF     L     @10,@PC00001                                        0199 00167000
         L     @10,MPPMCBAD(,@10)                                  0199 00168000
         MVI   MCBPTP(@10),X'03'                                   0199 00169000
*   GO TO GOODSCN1;                 /* RETURN NORMALLY               */ 00170000
*                                                                  0200 00171000
         B     GOODSCN1                                            0200 00172000
*   /*****************************************************************/ 00173000
*   /*                                                               */ 00174000
*   /* MCB BIT SETTING FOR FUNCTION                                  */ 00175000
*   /*                                                               */ 00176000
*   /*****************************************************************/ 00177000
*                                                                  0201 00178000
*RFUNCT:                                                           0201 00179000
*   MCBPTP=IOPPFUNC;                /* SET FUNC BIT IN MCB           */ 00180000
RFUNCT   L     @10,@PC00001                                        0201 00181000
         L     @10,MPPMCBAD(,@10)                                  0201 00182000
         MVI   MCBPTP(@10),X'04'                                   0201 00183000
*   GO TO GOODSCN1;                 /* RETURN NORMALLY               */ 00184000
*                                                                  0202 00185000
         B     GOODSCN1                                            0202 00186000
*   /*****************************************************************/ 00187000
*   /*                                                               */ 00188000
*   /* MCB BIT SETTING FOR USERMOD                                   */ 00189000
*   /*                                                               */ 00190000
*   /*****************************************************************/ 00191000
*                                                                  0203 00192000
*RUSER:                                                            0203 00193000
*   MCBPTP=IOPPUSER;                /* SET USERMOD BIT IN MCB        */ 00194000
RUSER    L     @10,@PC00001                                        0203 00195000
         L     @10,MPPMCBAD(,@10)                                  0203 00196000
         MVI   MCBPTP(@10),X'01'                                   0203 00197000
*   GO TO GOODSCN1;                 /* RETURN NORMALLY               */ 00198000
*                                                                  0204 00199000
         B     GOODSCN1                                            0204 00200000
*   /*****************************************************************/ 00201000
*   /*                                                               */ 00202000
*   /* MCB BIT SETTING FOR APAR                                      */ 00203000
*   /*                                                               */ 00204000
*   /*****************************************************************/ 00205000
*                                                                  0205 00206000
*RAPAR:                                                            0205 00207000
*   MCBPTP=IOPPAPAR;                /* SET APAR BIT IN MCB           */ 00208000
RAPAR    L     @10,@PC00001                                        0205 00209000
         L     @10,MPPMCBAD(,@10)                                  0205 00210000
         MVI   MCBPTP(@10),X'02'                                   0205 00211000
*   GO TO GOODSCN1;                 /* RETURN NORMALLY               */ 00212000
         B     GOODSCN1                                            0206 00213000
         EJECT                                                          00214000
*                                                                  0207 00215000
*   /*****************************************************************/ 00216000
*   /*                                                               */ 00217000
*   /* PICK UP SYSMOD ID WHICH IS 7 ALPHA NUMERIC CHARS              */ 00218000
*   /*                                                               */ 00219000
*   /*****************************************************************/ 00220000
*                                                                  0207 00221000
*RSMDNO:                                                           0207 00222000
*                                                                  0207 00223000
*   /*****************************************************************/ 00224000
*   /*                                                               */ 00225000
*   /* CHECK FOR IOP, IF PASSED FILL IN IOP TYPE BIT AND PTF #       */ 00226000
*   /*                                                               */ 00227000
*   /*****************************************************************/ 00228000
*                                                                  0207 00229000
*   IF MPPIOPAD^=0 THEN                                            0207 00230000
RSMDNO   L     @10,@PC00001                                        0207 00231000
         L     @09,MPPIOPAD(,@10)                                  0207 00232000
         LTR   @09,@09                                             0207 00233000
         BZ    @RF00207                                            0207 00234000
*     DO;                                                          0208 00235000
*       IOPPTYPE=MCBPTP;                                           0209 00236000
         L     @10,MPPMCBAD(,@10)                                  0209 00237000
         MVC   IOPPTYPE(1,@09),MCBPTP(@10)                         0209 00238000
*       IOPNAM17=SCPSTRNG(1:LENGTH(IOPNAM17));/*                     */ 00239000
         L     @10,SCPCHAR                                         0210 00240000
         MVC   IOPNAM17(7,@09),SCPSTRNG(@10)                       0210 00241000
*     END;                                                         0211 00242000
*                                                                  0211 00243000
*   /*****************************************************************/ 00244000
*   /*                                                               */ 00245000
*   /* FILL IN PTF# FOR MCB                                          */ 00246000
*   /*                                                               */ 00247000
*   /*****************************************************************/ 00248000
*                                                                  0212 00249000
*   MCBPNO=SCPSTRNG(1:LENGTH(MCBPNO));/* MOVE SYSMOD NUMBER          */ 00250000
@RF00207 L     @10,@PC00001                                        0212 00251000
         L     @10,MPPMCBAD(,@10)                                  0212 00252000
         L     @09,SCPCHAR                                         0212 00253000
         MVC   MCBPNO(7,@10),SCPSTRNG(@09)                         0212 00254000
*   GO TO GOODSCN1;                 /* RETURN NORMALLY               */ 00255000
         B     GOODSCN1                                            0213 00256000
         EJECT                                                          00257000
*                                                                  0214 00258000
*   /*****************************************************************/ 00259000
*   /*                                                               */ 00260000
*   /* PUT NUM OF FILES IN MCB AND IOP IF PASSED                     */ 00261000
*   /*                                                               */ 00262000
*   /*****************************************************************/ 00263000
*                                                                  0214 00264000
*RRELFNO:                                                          0214 00265000
*   DECLARE                                                        0214 00266000
*     PKWKAREA CHAR(8) BDY(DWORD);  /* CONVERT CHAR FILES# TO BINARY */ 00267000
RRELFNO  DS    0H                                                  0215 00268000
*   DECLARE                                                        0215 00269000
*     FILEWKA CHAR(8);                                             0215 00270000
*   DECLARE                                                        0216 00271000
*     FILESNO FIXED(15);                                           0216 00272000
*   DECLARE                                                        0217 00273000
*     FOPERAND CHAR(*) BASED(SCPCHAR);                             0217 00274000
*   DECLARE                                                        0218 00275000
*     INVALID CHAR(1) INIT('0');                                   0218 00276000
*                                                                  0218 00277000
*   /*****************************************************************/ 00278000
*   /*                                                               */ 00279000
*   /* CHECK FOR INVALID 0 PRECEEDING FILES NUMBER                   */ 00280000
*   /*                                                               */ 00281000
*   /*****************************************************************/ 00282000
*                                                                  0219 00283000
*   IF FOPERAND(1:1)=INVALID THEN                                  0219 00284000
         L     @10,SCPCHAR                                         0219 00285000
         CLC   FOPERAND(1,@10),INVALID                             0219 00286000
         BNE   @RF00219                                            0219 00287000
*     DO;                           /* ZERO PRECEDING FILES          */ 00288000
*       SCPRET=SCPSTOP;                                            0221 00289000
         MVI   SCPRET,X'08'                                        0221 00290000
*     END;                                                         0222 00291000
*                                                                  0222 00292000
*   /*****************************************************************/ 00293000
*   /*                                                               */ 00294000
*   /* CONVERT TO BINERY FILES NUMBER MOVE FILE # IN MCB AND IOP IF  */ 00295000
*   /* PASSED                                                        */ 00296000
*   /*                                                               */ 00297000
*   /*****************************************************************/ 00298000
*                                                                  0223 00299000
*   ELSE                                                           0223 00300000
*     DO;                                                          0223 00301000
         B     @RC00219                                            0223 00302000
@RF00219 DS    0H                                                  0224 00303000
*       FILEWKA=' ';                                               0224 00304000
         MVI   FILEWKA+1,C' '                                      0224 00305000
         MVC   FILEWKA+2(6),FILEWKA+1                              0224 00306000
         MVI   FILEWKA,C' '                                        0224 00307000
*       FILEWKA((9-SCPPMLN):8)=FOPERAND(1:SCPPMLN);                0225 00308000
         LA    @10,9                                               0225 00309000
         SH    @10,SCPPMLN                                         0225 00310000
         LA    @09,FILEWKA-1(@10)                                  0225 00311000
         LCR   @10,@10                                             0225 00312000
         AL    @10,@CF00116                                        0225 00313000
         L     @08,SCPCHAR                                         0225 00314000
         EX    @10,@SM01488                                        0225 00315000
*       PACK(PKWKAREA,FILEWKA);                                    0226 00316000
         PACK  PKWKAREA(8),FILEWKA(8)                              0226 00317000
*       CVB(FILESNO,PKWKAREA);                                     0227 00318000
         CVB   @10,PKWKAREA                                        0227 00319000
         STH   @10,FILESNO                                         0227 00320000
*       MCBPRELN=FILESNO;           /* NUM FILES TO MCB              */ 00321000
         L     @09,@PC00001                                        0228 00322000
         L     @08,MPPMCBAD(,@09)                                  0228 00323000
         STH   @10,MCBPRELN(,@08)                                  0228 00324000
*       IF MPPIOPAD^=0 THEN                                        0229 00325000
         L     @09,MPPIOPAD(,@09)                                  0229 00326000
         LTR   @09,@09                                             0229 00327000
         BZ    @RF00229                                            0229 00328000
*         DO;                       /*                               */ 00329000
*           IOPPRELN=MCBPRELN;      /* NUM FILES TO IOP              */ 00330000
