*           IOPLRFP=ON;             /*                               */ 00332000
         OI    IOPLRFP(@09),B'00000001'                            0232 00333000
*         END;                      /*                               */ 00334000
*     END;                                                         0234 00335000
*   GO TO GOODSCN1;                 /* RETURN NORMALLY               */ 00336000
         B     GOODSCN1                                            0235 00337000
*                                                                  0236 00338000
*   /*****************************************************************/ 00339000
*   /*                                                               */ 00340000
*   /* RETURN TO SCAN WITH NORMAL RETURN CODE                        */ 00341000
*   /*                                                               */ 00342000
