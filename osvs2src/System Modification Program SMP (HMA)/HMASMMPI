K2PLUS   DSCAN KEY='++',SUCC=KIF                                        00604000
KIF      DSCAN KEY='IF',SUCC=KFMID                                      00605000
*                                                                       00606000
*   FMID KEYWORD MUST BE FIRST, ONLY ONE VALUE ALLOWED                  00607000
*                                                                       00608000
KFMID    DSCAN KEY='FMID',SUCC=KFMIDLP                                  00609000
KFMIDLP  DSCAN KEY='(',SUCC=KFMIDV                                      00610000
KFMIDV   DSCAN MINLEN=7,MAXLEN=7,SUCC=KFMIDRP,ROUT=RFMIDV               00611000
KFMIDRP  DSCAN KEY=')',SUCC=KTHEN,COMMA=YES                             00612000
*                                                                       00613000
*   THEN KEYWORK IS OPTIONAL, MUST FOLLOW FMID                          00614000
*                                                                       00615000
KTHEN    DSCAN KEY='THEN',ALT=KREQ,SUCC=KREQ                            00616000
*                                                                       00617000
*    REQ KEYWORD MUST BE LAST ON STATEMENT, LIST OF VALUES ALLOWED      00618000
*                                                                       00619000
KREQ     DSCAN KEY='REQ',SUCC=KREQLP                                    00620000
KREQLP   DSCAN KEY='(',SUCC=KREQV                                       00621000
KREQV    DSCAN MINLEN=7,MAXLEN=7,SUCC=KREQRP,ROUT=RREQV                 00622000
KREQRP   DSCAN KEY=')',ALT=KREQV,SUCC=KPERIOD,COMMA=NO                  00623000
*                                                                       00624000
*  CONCLUDING DSCANS FOR IF     STATEMENT                               00625000
*                                                                       00626000
KPERIOD  DSCAN KEY='.',SUCC=KXTRA                                       00627000
KXTRA    DSCAN MINLEN=0,ROUT=RXTRA,IO=NO                                00628000
