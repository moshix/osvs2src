*   VLPDATA(8)=''B;                 /*                               */ 00390000
         MVI   VLPDATA+7,X'00'                                     0189 00391000
*   VLPTYPE=MCBIFREQ;               /* SET ENTRY TYPE                */ 00392000
         MVI   VLPTYPE,X'01'                                       0190 00393000
*   VLPDLEN=LENGTH(MCBINUM);        /* LEN OF KEY IN VLPDATA         */ 00394000
         MVI   VLPDLEN,X'07'                                       0191 00395000
