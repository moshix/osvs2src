*SYNTAXMG:                                                         0161 00281000
*   PROC;                                                          0161 00282000
SYNTAXMG STM   @14,@12,@SA00004                                    0161 00283000
*   DECLARE                                                        0162 00284000
*     HEXNO FIXED(8);               /* HEXADECIMAL NUMBER            */ 00285000
*   DECLARE                                                        0163 00286000
*     1 DECNO BDY(DWORD),           /* CONVERT DECIMAL NUMBER        */ 00287000
*       3 DECNO1 CHAR(8),           /* ACTUAL DECIMAL NUMBER         */ 00288000
*       3 * CHAR(1);                /* EXTRA FOR UNPACK              */ 00289000
*   DECLARE                                                        0164 00290000
*     1 BCDNO BDY(WORD) CHAR(5),    /* EBCDIC NUMBER                 */ 00291000
*       3 * CHAR(1),                /* PADDING                       */ 00292000
*       3 BCDCOL CHAR(2),           /* COLUMN NUMBER                 */ 00293000
*       3 * CHAR(2);                /* FOR SIGN                      */ 00294000
*   DECLARE                                                        0165 00295000
*     COLSTR FIXED(8) CONSTANT(7);  /* START FOR UNPACK              */ 00296000
*   DECLARE                                                        0166 00297000
*     COLSTP FIXED(8) CONSTANT(9);  /* END FOR UNPACK                */ 00298000
*   HEXNO=SCPCHAR-ADDR(CRPBUFFR)+1; /* GET COLUMN NUMBER             */ 00299000
         L     @10,@PC00001                                        0167 00300000
         L     @10,MPPCRDPM(,@10)                                  0167 00301000
         LA    @10,CRPBUFFR(,@10)                                  0167 00302000
         LCR   @10,@10                                             0167 00303000
         AL    @10,SCPCHAR                                         0167 00304000
         LA    @10,1(,@10)                                         0167 00305000
         STC   @10,HEXNO                                           0167 00306000
*   CVD(HEXNO,DECNO1);              /* CONVERT NO. TO DECIMAL        */ 00307000
         CVD   @10,DECNO1                                          0168 00308000
*   UNPK(BCDNO,DECNO(COLSTR:COLSTP));/* UNPACK NO.                   */ 00309000
         UNPK  BCDNO(5),DECNO+6(3)                                 0169 00310000
*   MGPCLEAR=''B;                   /* CLEAR ALL MESSAGE NUMBERS     */ 00311000
         XC    MGPCLEAR(6),MGPCLEAR                                0170 00312000
*   MGPMGNO1=HMA203I;               /* SYNTAX ERROR IN               */ 00313000
         MVC   MGPMGNO1(2),@CH00044                                0171 00314000
*   MGPMGNO2=PART021;               /* CONTROL STATMENT              */ 00315000
         MVC   MGPMGNO2(2),@CH00416                                0172 00316000
*   MGPMGNO3=PART026;               /* AT COL                        */ 00317000
         MVC   MGPMGNO3(2),@CH00426                                0173 00318000
*   MGPVARPT(1)=ADDR(BCDCOL);       /* MESSAGE TEXT = COLUMN NUMBER  */ 00319000
         LA    @10,BCDCOL                                          0174 00320000
         ST    @10,MGPVARPT                                        0174 00321000
*   MGPTYPE=MGPERR;                 /* MESSAGE SEVERITY              */ 00322000
         MVI   MGPTYPE,X'03'                                       0175 00323000
*   CALL HMASMMSG(HMASMMGP);        /* CALL MESSAGE ROUTINE          */ 00324000
         L     @15,@CV00780                                        0176 00325000
         LA    @01,@AL00176                                        0176 00326000
         BALR  @14,@15                                             0176 00327000
*   RETURN CODE(ZERO);                                             0177 00328000
