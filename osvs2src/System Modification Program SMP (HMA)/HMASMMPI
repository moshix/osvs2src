         BNL   *+6                                                      00227000
         LR    @14,RTNCODE                                         0143 00228000
         STC   @14,SCNRC                                           0143 00229000
*           RFY                     /*                               */ 00230000
*             RTNCODE UNRSTD;       /*                               */ 00231000
*         END;                      /*                               */ 00232000
*     END;                          /*                               */ 00233000
@RF00139 DS    0H                                                  0147 00234000
*   IF CRPRETRN^=CRPGOOD THEN       /* ERROR IN CONTINUATION READ    */ 00235000
