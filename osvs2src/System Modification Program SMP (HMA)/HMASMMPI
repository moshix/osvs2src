*                                                                  0136 00207000
*   IF SCPRET=SCPBAD THEN           /* SCN INDICATED AN ERROR        */ 00208000
         CLI   SCPRET,4                                            0136 00209000
         BNE   @RF00136                                            0136 00210000
*     DO;                           /*                               */ 00211000
*       SCNRC=STOPPTF;              /* FLUSH THE SYSMOD              */ 00212000
         MVI   SCNRC,X'08'                                         0138 00213000
*       IF SCPRTERR=OFF             /* SCAN FOUND SYNTAX ERROR       */ 00214000
*         THEN                      /*                               */ 00215000
         TM    SCPRTERR,B'00010000'                                0139 00216000
         BNZ   @RF00139                                            0139 00217000
*         DO;                       /*                               */ 00218000
*           CALL SYNTAXMG;          /* ISSUE MESSAGE                 */ 00219000
         BAL   @14,SYNTAXMG                                        0141 00220000
*           RFY                     /*                               */ 00221000
*             RTNCODE RSTD;         /*                               */ 00222000
*           SCNRC=MAX(SCNRC,RTNCODE);/*                              */ 00223000
