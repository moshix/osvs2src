*SYNTAXMG:                                                         0252 00346000
*   PROC;                                                          0252 00347000
SYNTAXMG STM   @14,@12,@SA00004                                    0252 00348000
*   DECLARE                                                        0253 00349000
*     HEXNO FIXED(8);               /* HEXADECIMAL NUMBER            */ 00350000
*   DECLARE                                                        0254 00351000
*     1 DECNO BDY(DWORD),           /* CONVERT DECIMAL NUMBER        */ 00352000
*       3 DECNO1 CHAR(8),           /* ACTUAL DECIMAL NUMBER         */ 00353000
*       3 * CHAR(1);                /* EXTRA FOR UNPACK              */ 00354000
*   DECLARE                                                        0255 00355000
*     1 BCDNO BDY(WORD) CHAR(5),    /* EBCDIC NUMBER                 */ 00356000
*       3 * CHAR(1),                /* PADDING                       */ 00357000
*       3 BCDCOL CHAR(2),           /* COLUMN NUMBER                 */ 00358000
*       3 * CHAR(2);                /* FOR SIGN                      */ 00359000
*   DECLARE                                                        0256 00360000
*     COLSTR FIXED(8) CONSTANT(7);  /* START FOR UNPACK              */ 00361000
*   DECLARE                                                        0257 00362000
*     COLSTP FIXED(8) CONSTANT(9);  /* END FOR UNPACK                */ 00363000
*   HEXNO=SCPCHAR-ADDR(CRPBUFFR)+1; /* GET COLUMN NUMBER             */ 00364000
         L     @10,@PC00001                                        0258 00365000
         L     @10,MPPCRDPM(,@10)                                  0258 00366000
