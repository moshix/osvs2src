*   CVD(HEXNO,DECNO1);              /* CONVERT NO. TO DECIMAL        */ 00372000
         CVD   @10,DECNO1                                          0259 00373000
*   UNPK(BCDNO,DECNO(COLSTR:COLSTP));/* UNPACK NO.                   */ 00374000
         UNPK  BCDNO(5),DECNO+6(3)                                 0260 00375000
*   MGPMGNO1=HMA203I;               /* SYNTAX ERROR IN               */ 00376000
         MVC   MGPMGNO1(2),@CH00044                                0261 00377000
*   MGPMGNO2=PART021;               /* CONTROL STATMENT              */ 00378000
         MVC   MGPMGNO2(2),@CH00458                                0262 00379000
*   MGPMGNO3=PART026;               /* AT COL                        */ 00380000
