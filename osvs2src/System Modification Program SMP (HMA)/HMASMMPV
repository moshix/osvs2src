*   MGPVARPT(1)=ADDR(BCDCOL);       /* MESSAGE TEXT = COLUMN NUMBER  */ 00382000
         LA    @10,BCDCOL                                          0264 00383000
         ST    @10,MGPVARPT                                        0264 00384000
*   MGPTYPE=MGPERR;                 /* MESSAGE SEVERITY              */ 00385000
