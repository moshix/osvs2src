         TITLE 'HMASMPGC - PURGE CRQ/ACRQ PROCEDURE'                    00001000
HMASMPGC CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMPGC  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(4),0(@01)                                  0001 00017000
*                                      PASSED                        */ 00018000
*   HIGHPGC=ZERO;                   /* CLEAR RETURN CODE COLLECTOR   */ 00019000
         SLR   HIGHPGC,HIGHPGC                                     0178 00020000
*   IOPPTR=CCAIOPTR;                /* SET POINTER TO CCA IOP        */ 00021000
*                                                                  0179 00022000
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0179 00023000
*   /*****************************************************************/ 00024000
*   /*                                                               */ 00025000
*   /* DETERMINE WHAT FUNCTION IS BEING EXECUTED SO THAT THE IOPDSID */ 00026000
*   /* IS SET TO THE PROPER DATASET. (APPLY/RESTORE=CRQ,ACCEPT=ACRQ) */ 00027000
*   /*                                                               */ 00028000
*   /*****************************************************************/ 00029000
*                                                                  0180 00030000
*   IF CCAACCPT=ON THEN             /* FOR ACCEPT MODE SET           */ 00031000
         TM    CCAACCPT(CCAPTR),B'00100000'                        0180 00032000
         BNO   @RF00180                                            0180 00033000
*     IOPDSID=IOPACRQ;              /* IOPDSID = ACRQ                */ 00034000
         MVI   IOPDSID(IOPPTR),X'1E'                               0181 00035000
*   ELSE                            /* ELSE DEFAULT TO APPLY/RESTORE */ 00036000
*     IOPDSID=IOPCRQ;               /* AND SET IOPDSID = CRQ         */ 00037000
*                                                                  0182 00038000
         B     @RC00180                                            0182 00039000
@RF00180 MVI   IOPDSID(IOPPTR),X'1C'                               0182 00040000
*   /*****************************************************************/ 00041000
*   /*                                                               */ 00042000
*   /* FOR APPLY/ACCEPT PROCESSING CALL INTERNAL PROCEDURE, PGCAPP,  */ 00043000
*   /* FOR EXPLICITLY DELETED SYSMODS AND RETURN TO CALLER           */ 00044000
*   /*                                                               */ 00045000
*   /*****************************************************************/ 00046000
*                                                                  0183 00047000
*   IF CCAAPPLY=ON                  /* APPLY                         */ 00048000
*       |CCAACCPT=ON                /* ACCEPT                        */ 00049000
*     THEN                                                         0183 00050000
@RC00180 TM    CCAAPPLY(CCAPTR),B'01100000'                        0183 00051000
