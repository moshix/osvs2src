         TITLE 'HMASMRCF - RECEIVE SYSMOD FLUSH PROCEDURE'              00001000
HMASMRCF CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMRCF  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(4),0(@01)                                  0001 00017000
*                                                                  0164 00018000
*   /*****************************************************************/ 00019000
*   /*                                                               */ 00020000
*   /* INITIALIZE THE HMASMMGP PARAMETER                             */ 00021000
*   /*                                                               */ 00022000
*   /*****************************************************************/ 00023000
*                                                                  0164 00024000
*   HMASMMGP=''B;                   /* CLEAR OUT THE MGP             */ 00025000
*                                                                  0164 00026000
         XC    HMASMMGP(24),HMASMMGP                               0164 00027000
*   /*****************************************************************/ 00028000
*   /*                                                               */ 00029000
*   /* INITIALIZE FOR SYSMOD COMPLETION PROCESSING                   */ 00030000
*   /*                                                               */ 00031000
*   /*****************************************************************/ 00032000
*                                                                  0165 00033000
*   HIGHRCF=0;                      /* CLEAR RETURN CODE COLLECTOR   */ 00034000
         MVI   HIGHRCF,X'00'                                       0165 00035000
*   HMASMMGP=''B;                   /* CLEAR HMASMMSG PARM BLOCK     */ 00036000
         XC    HMASMMGP(24),HMASMMGP                               0166 00037000
*   RESPECIFY                                                      0167 00038000
*     HMASMGTP BASED(RECXGSMP);     /* SUMMARY GTP                   */ 00039000
*   RESPECIFY                                                      0168 00040000
*     GSMREC BASED(GTPBUFAD);       /* SUMMARY RECORD                */ 00041000
*   DATA1=GSMS#;                    /* PUT SYSMOD NAME FROM GTA    0169 00042000
*                                      SUMMARY RECORD INTO DATA AREA    00043000
*                                      FOR MESSAGE VARIABLE PART     */ 00044000
