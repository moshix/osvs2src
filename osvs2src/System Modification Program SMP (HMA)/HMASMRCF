         L     @01,RECXGSMP(,@10)                                  0169 00046000
         L     @15,GTPBUFAD(,@01)                                  0169 00047000
         MVC   DATA1(7),GSMS#(@15)                                 0169 00048000
*                                                                  0170 00049000
*   /*****************************************************************/ 00050000
*   /*                                                               */ 00051000
*   /* CHECK FOR SYSMOD CONSTRUCTION ERRORS                          */ 00052000
*   /*                                                               */ 00053000
*   /*****************************************************************/ 00054000
*                                                                  0170 00055000
*   IF GSMEXCL=OFF                  /* SYSMOD NOT EXCLUDED           */ 00056000
*       &GSMRCVD=OFF                /* SYSMOD NOT ALREADY RECEIVED   */ 00057000
*       &RECXVERF=OFF               /* NO ++VER FOUND                */ 00058000
*     THEN                                                         0170 00059000
         TM    GSMEXCL(@15),B'00110000'                            0170 00060000
         BNZ   @RF00170                                            0170 00061000
         TM    RECXVERF(@10),B'10000000'                           0170 00062000
         BNZ   @RF00170                                            0170 00063000
*     DO;                                                          0171 00064000
*                                                                  0171 00065000
*       /*************************************************************/ 00066000
*       /*                                                           */ 00067000
*       /* NO ++VER FOUND - HMA396I ISSUED:                          */ 00068000
*       /*                                                           */ 00069000
*       /*************************************************************/ 00070000
*                                                                  0172 00071000
*       DO;                         /* HMASMPMG M1(HMA396I)TYPE(ERR)T   00072000
*                                      ARGET(LOG,SYSOUT)VAR(DATA1)   */ 00073000
*         HMASMMGP=HMASMMGP&&HMASMMGP;/* SET PARMS TO ZEROS          */ 00074000
