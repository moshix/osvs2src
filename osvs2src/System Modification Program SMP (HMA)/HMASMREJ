         TITLE 'HMASMREJ - SMP REJECT ROUTINE'                          00001000
HMASMREJ CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMREJ  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(4),0(@01)                                  0001 00017000
*   REJRTNCD=ZERO;                  /* INITIALIZE RETURN CODE        */ 00018000
         SLR   REJRTNCD,REJRTNCD                                   0165 00019000
*   REJBITS=''B;                    /* INITIALIZE THE REJECT BITS    */ 00020000
         MVI   REJBITS,X'00'                                       0166 00021000
*   IOPPTR=CCAIOPTR;                /* INITIALIZE IOP POINTER        */ 00022000
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0167 00023000
*   REJRCD=''B;                     /* INITIALIZE THE REJRCD         */ 00024000
         XC    REJRCD(38),REJRCD                                   0168 00025000
*   RLFPRE='';                      /* INITIALIZE RELFILE PREFIX     */ 00026000
         MVI   RLFPRE,C' '                                         0169 00027000
         MVC   RLFPRE+1(25),RLFPRE                                 0169 00028000
*   FMIDDEL=N;                      /* INITIALIZE FMID DELETE        */ 00029000
         MVC   FMIDDEL(1),N                                        0170 00030000
*   REJGTP=''B;                     /* ZERO THE REJECT GTP PARM      */ 00031000
         XC    REJGTP(28),REJGTP                                   0171 00032000
*   HMASMMGP=''B;                   /* ZERO THE MESSAGE PARM         */ 00033000
         XC    HMASMMGP(28),HMASMMGP                               0172 00034000
*   MGPPRINT=ON;                    /* ROUTE MESSAGES TO THE PRINTER */ 00035000
*   MGPHLDS=ON;                     /* ROUTE MESSAGES TO THE SMPLOG  */ 00036000
*                                                                  0174 00037000
         OI    MGPPRINT,B'10010000'                                0174 00038000
*   /*****************************************************************/ 00039000
*   /*                                                               */ 00040000
*   /* IF IN MASS OR EXCLUDE MODE CLOSE THE SMPPTS DCB BY CALLING    */ 00041000
*   /* HMASMIO. THIS IS DONE FOR THE UPCOMING SEQUENTIAL READ        */ 00042000
*   /* OPERATION                                                     */ 00043000
*   /*                                                               */ 00044000
*   /*****************************************************************/ 00045000
*                                                                  0175 00046000
*   IF TBLMASS=ON|TBLEXC=ON THEN                                   0175 00047000
         L     @10,@PC00001                                        0175 00048000
         TM    TBLMASS(@10),B'01000001'                            0175 00049000
         BZ    @RF00175                                            0175 00050000
*     DO;                           /*                               */ 00051000
*       IOPDSID=IOPPTS;             /*                               */ 00052000
         MVI   IOPDSID(IOPPTR),X'06'                               0177 00053000
*       IOPFUNCT=IOPCLOSN;          /*                               */ 00054000
         MVI   IOPFUNCT(IOPPTR),X'0C'                              0178 00055000
*       CALL HMASMIO(HMASMIOP);     /* CLOSE SMPPTS DCB FOR        0179 00056000
*                                      SEQUENTIAL DIRECTORY READ     */ 00057000
         ST    IOPPTR,@AL00001                                     0179 00058000
         L     @15,@CV01386                                        0179 00059000
         LA    @01,@AL00001                                        0179 00060000
         BALR  @14,@15                                             0179 00061000
*       REJRTNCD=MAX(REJRTNCD,IOPRETRN);/* SAVE THE RETURN CODE      */ 00062000
         SLR   @10,@10                                             0180 00063000
         IC    @10,IOPRETRN(,IOPPTR)                               0180 00064000
         CR    REJRTNCD,@10                                        0180 00065000
         BNL   *+6                                                      00066000
         LR    REJRTNCD,@10                                        0180 00067000
*     END;                          /*                               */ 00068000
*                                                                  0181 00069000
*   /*****************************************************************/ 00070000
*   /*                                                               */ 00071000
*   /* IF IN SELECT MODE HMASMGTA IS CALLED TO PREPARE THE SET GTA   */ 00072000
*   /* FILE FOR SEQUENTIAL READ                                      */ 00073000
*   /*                                                               */ 00074000
*   /*****************************************************************/ 00075000
*                                                                  0182 00076000
*   ELSE                                                           0182 00077000
*     DO;                           /* IS THE SELECT BIT ON??        */ 00078000
