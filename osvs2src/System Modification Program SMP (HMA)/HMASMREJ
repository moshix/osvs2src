         CR    REJRTNCD,@05                                        0186 00096000
         BNL   *+6                                                      00097000
         LR    REJRTNCD,@05                                        0186 00098000
*     END;                          /*                               */ 00099000
*                                                                  0188 00100000
*   /*****************************************************************/ 00101000
*   /*                                                               */ 00102000
*   /* CALL ROUTINE TO DO SYSMOD SELECTION AND BUILD GTA FIKE USED TO*/ 00103000
*   /* CONTROL ACTUAL REJECT PROCES                                  */ 00104000
*   /*                                                               */ 00105000
*   /*****************************************************************/ 00106000
*                                                                  0188 00107000
*   IF REJRTNCD=ZERO                /*                               */ 00108000
*     THEN                          /*                               */ 00109000
@RC00175 CH    REJRTNCD,@CH00297                                   0188 00110000
         BNE   @RF00188                                            0188 00111000
*     DO;                           /*                               */ 00112000
*       CALL REJSEL;                /*                               */ 00113000
