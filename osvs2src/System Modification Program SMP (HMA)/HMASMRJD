         TITLE 'HMASMRJD REJECT DELETE ROUTINE'                         00001000
HMASMRJD CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMRJD  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(12),0(@01)                                 0001 00017000
*                                      DELETED                       */ 00018000
         EJECT                                                          00019000
*                                                                  0149 00020000
*   /*****************************************************************/ 00021000
*   /*                                                               */ 00022000
*   /* INITIALIZATION AND SETUP FOR MAINLINE CODE                    */ 00023000
*   /*                                                               */ 00024000
*   /*****************************************************************/ 00025000
*                                                                  0149 00026000
*   RJDRTNCD=ZERO;                  /* INITIALIZE RETURN CODE        */ 00027000
         SLR   RJDRTNCD,RJDRTNCD                                   0149 00028000
*   IOPPTR=ADDR(RJDIOP);            /* INITIALIZE IOPPTR TO LOCAL IOP*/ 00029000
         LA    IOPPTR,RJDIOP                                       0150 00030000
*   HMASMIOP=''B;                   /* CLEAR THE IOP                 */ 00031000
         XC    HMASMIOP(68,IOPPTR),HMASMIOP(IOPPTR)                0151 00032000
*   DSPREFIX=RLFDSPRE;              /* SAVE DATASET NAME PREFIX      */ 00033000
         L     @10,@PC00001+4                                      0152 00034000
         MVC   DSPREFIX(26),RLFDSPRE(@10)                          0152 00035000
*   RELFILE#=NORLFS;                /* SAVE # OF RELFILES TO DELETE  */ 00036000
*                                                                  0153 00037000
         L     @10,@PC00001+8                                      0153 00038000
         MVC   RELFILE#(2),NORLFS(@10)                             0153 00039000
*   /*****************************************************************/ 00040000
*   /*                                                               */ 00041000
*   /* CALL RJDRLFD TO SCRATCH ANY LOADED RELFILE DATASETS           */ 00042000
*   /*                                                               */ 00043000
*   /*****************************************************************/ 00044000
*                                                                  0154 00045000
*   CALL PTSLOC;                    /* GET PTS SYSMOD ENTRY          */ 00046000
         BAL   @14,PTSLOC                                          0154 00047000
*   IF RELFILE#^=ZERO               /* RELFILES WERE LOADED AT REC   */ 00048000
*     THEN                                                         0155 00049000
         ICM   @10,3,RELFILE#                                      0155 00050000
         BZ    @RF00155                                            0155 00051000
*     DO;                                                          0156 00052000
*       CALL RJDRLFD;               /* PROCEDURE TO CREATE AND DELETE   00053000
*                                      ANY LOADED RELFILES           */ 00054000
         BAL   @14,RJDRLFD                                         0157 00055000
*       RFY                                                        0158 00056000
*         RTNCODE RSTD;             /* CHECK THE RETURN CODE FROM    */ 00057000
*       RJDRTNCD=MAX(RJDRTNCD,RTNCODE);/* RJDRLFD AND SET IT IN      */ 00058000
         CR    RJDRTNCD,RTNCODE                                    0159 00059000
         BNL   *+6                                                      00060000
         LR    RJDRTNCD,RTNCODE                                    0159 00061000
*       RFY                                                        0160 00062000
*         RTNCODE UNRSTD;           /* RJDRTNCD                      */ 00063000
*     END;                                                         0161 00064000
*                                                                  0161 00065000
*   /*****************************************************************/ 00066000
*   /*                                                               */ 00067000
*   /* CALL PROCEDURE TO DELETE ANY PTS CONTROL ENTRIES              */ 00068000
*   /*                                                               */ 00069000
*   /*****************************************************************/ 00070000
*                                                                  0162 00071000
*   CALL RJDPTSD;                   /* PROCEDURE TO DELETE THE PTS 0162 00072000
*                                      CONTROL ENTRY                 */ 00073000
@RF00155 BAL   @14,RJDPTSD                                         0162 00074000
*   RFY                                                            0163 00075000
*     RTNCODE RSTD;                 /* CHECK THE RETURN CODE FROM    */ 00076000
*   RJDRTNCD=MAX(RJDRTNCD,RTNCODE); /* RJDPTSD AND SET IT IN         */ 00077000
         CR    RJDRTNCD,RTNCODE                                    0164 00078000
         BNL   *+6                                                      00079000
         LR    RJDRTNCD,RTNCODE                                    0164 00080000
*   RFY                                                            0165 00081000
*     RTNCODE UNRSTD;               /* RJDRTNCD                      */ 00082000
*                                                                  0165 00083000
*   /*****************************************************************/ 00084000
*   /*                                                               */ 00085000
*   /* RETURN TO CALLER WITH THE HIGHEST RETURN CODE ENCOUNTERED     */ 00086000
*   /*                                                               */ 00087000
*   /*****************************************************************/ 00088000
*                                                                  0166 00089000
*   RETURN CODE(RJDRTNCD);          /* REGISTER 15 RETURN CODE       */ 00090000
         LR    @15,RJDRTNCD                                        0166 00091000
         L     @13,4(,@13)                                         0166 00092000
         L     @14,12(,@13)                                        0166 00093000
         LM    @00,@12,20(@13)                                     0166 00094000
         BR    @14                                                 0166 00095000
         EJECT                                                          00096000
*                                                                  0167 00097000
*   /*****************************************************************/ 00098000
*   /*                                                               */ 00099000
*   /* THIS IS THE RELFILE DELETE ROUTINE. IT WILL ENTERED ONLY IF   */ 00100000
*   /* THE NUMBER OF RELFILES PASSED TO THIS MODULE IS NOT ZERO. IT  */ 00101000
*   /* WILL LOOP FROM 1 TO THE NUMBER OF FILES PASSED AND FOR EACH   */ 00102000
*   /* POSSIBLE FILE PASSED IT WILL DO: (A) CALL HMASMSUB TO BUILD   */ 00103000
*   /* THE RELFILE DATASET NAME (B) CALL HMASMIO TO SCRATCH THE      */ 00104000
*   /* DATASET (C) EXIT IF THE RETURN CODE IS BAD (D) CONTINUE ON    */ 00105000
*   /* WHEN ALL MEMBERS HAS BEEN PROCESSED                           */ 00106000
*   /*                                                               */ 00107000
*   /*****************************************************************/ 00108000
*                                                                  0167 00109000
*RJDRLFD:                                                          0167 00110000
*   PROCEDURE;                                                     0167 00111000
RJDRLFD  STM   @14,@12,@SA00002                                    0167 00112000
*   DECLARE                         /*                               */ 00113000
*     I FIXED(31);                  /* RELFILE NUMBER INDEX          */ 00114000
*   DECLARE                         /*                               */ 00115000
*     RLFDELRC FIXED(8);            /* RETURN CODE AREA FOR PROCEDURE*/ 00116000
*   RLFDELRC=ZERO;                  /* CLEAR OUT THE RETURN CODE     */ 00117000
         SLR   RLFDELRC,RLFDELRC                                   0170 00118000
*   HMASMSPL=''B;                   /* CLEAR THE SUP PARM            */ 00119000
*                                                                  0171 00120000
         XC    HMASMSPL(5),HMASMSPL                                0171 00121000
*   /*****************************************************************/ 00122000
*   /*                                                               */ 00123000
*   /* LOOP THRU PROCESSING ALL RELFILES                             */ 00124000
*   /*                                                               */ 00125000
*   /*****************************************************************/ 00126000
*                                                                  0172 00127000
*   DO I=1 BY 1 TO RELFILE# WHILE(  /* LOOP TILL SERIOUS ERROR       */ 00128000
*         RLFDELRC<STOPFUNC);       /*                               */ 00129000
*                                                                  0172 00130000
         LA    I,1                                                 0172 00131000
         B     @DE00172                                            0172 00132000
@DL00172 CH    RLFDELRC,@CH00181                                   0172 00133000
         BNL   @DC00172                                            0172 00134000
*     /***************************************************************/ 00135000
*     /*                                                             */ 00136000
*     /* CALL HMASMSUB TO BUILD THE RELFILE DATASET NAME FROM THE    */ 00137000
*     /* PASSED PARAMETERS (THE RELFILE DATASET NAME PREFIX, THE     */ 00138000
*     /* SYSMOD NUMBER,AND THE FILE NUMBER.                          */ 00139000
*     /*                                                             */ 00140000
*     /***************************************************************/ 00141000
*                                                                  0173 00142000
*     SPLPMAD=ADDR(BLDRLFDS);       /* ADDR OF AREA MAPPED BY SPLPDSN*/ 00143000
         LA    @10,BLDRLFDS                                        0173 00144000
         ST    @10,SPLPMAD                                         0173 00145000
*     SPLIPFX=DSPREFIX;             /* LRF DSN PREFIX                */ 00146000
         MVC   SPLIPFX(26,@10),DSPREFIX                            0174 00147000
*     SPLISMD=SYSMODNO;             /* SYSMOD NUMBER                 */ 00148000
         L     @05,@PC00001                                        0175 00149000
         MVC   SPLISMD(7,@10),SYSMODNO(@05)                        0175 00150000
*     SPLIFIL=I;                    /* THE FILE NUMBER THAT THE LOOP    00151000
*                                      COUNTER IS POINTING TO        */ 00152000
         STH   I,SPLIFIL(,@10)                                     0176 00153000
*     SPLFUNCT=SPLBDDSN;            /* BUILD RELFILE DATASET NAME    */ 00154000
         MVI   SPLFUNCT,X'05'                                      0177 00155000
*     CALL HMASMSUB(HMASMSPL);      /*                               */ 00156000
         L     @15,@CV01266                                        0178 00157000
         LA    @01,@AL00178                                        0178 00158000
         BALR  @14,@15                                             0178 00159000
*     RFY                                                          0179 00160000
*       RTNCODE RSTD;               /*                               */ 00161000
*     RLFDELRC=RTNCODE;             /* SAVE RETURN CODE FROM RELFILE    00162000
*                                      BUILD                         */ 00163000
         LR    RLFDELRC,RTNCODE                                    0180 00164000
*     RFY                                                          0181 00165000
*       RTNCODE UNRSTD;             /*                               */ 00166000
*     IF RLFDELRC=ZERO THEN         /*                               */ 00167000
         CH    RLFDELRC,@CH00256                                   0182 00168000
         BNE   @RF00182                                            0182 00169000
*       DO;                         /* IF THE RETURN CODE FROM     0183 00170000
*                                      RELFILE BUILD IS ZERO         */ 00171000
*                                                                  0183 00172000
*         /***********************************************************/ 00173000
*         /*                                                         */ 00174000
*         /* CALL HMASMIO TO SCRATCH RELFILE                         */ 00175000
*         /*                                                         */ 00176000
*         /***********************************************************/ 00177000
*                                                                  0184 00178000
*         IOPDSID=IOPLRF;           /* DATASET ID IS SET TO IOPLRF   */ 00179000
         MVI   IOPDSID(IOPPTR),X'20'                               0184 00180000
*         IOPDSN=SPLODSN;           /* THE DATASET NAME TO BE DELETED   00181000
*                                      IS THE ONE MAPPED IN SPLPDSN  */ 00182000
         L     @10,SPLPMAD                                         0185 00183000
         MVC   IOPDSN(44,IOPPTR),SPLODSN(@10)                      0185 00184000
*         IOPFUNCT=IOPDEL;          /* DO AN IOPDEL FUNCTION         */ 00185000
         MVI   IOPFUNCT(IOPPTR),X'0F'                              0186 00186000
*         CALL HMASMIO(HMASMIOP);   /*                               */ 00187000
         ST    IOPPTR,@AL00001                                     0187 00188000
         L     @15,@CV01265                                        0187 00189000
         LA    @01,@AL00001                                        0187 00190000
         BALR  @14,@15                                             0187 00191000
*         RLFDELRC=MAX(RLFDELRC,IOPRETRN);/* SAVE RCN CODE           */ 00192000
         SLR   @10,@10                                             0188 00193000
         IC    @10,IOPRETRN(,IOPPTR)                               0188 00194000
         CR    RLFDELRC,@10                                        0188 00195000
         BNL   *+6                                                      00196000
         LR    RLFDELRC,@10                                        0188 00197000
*       END;                        /* END IO PROCESSING             */ 00198000
*   END;                            /* END LOOP PROCESSING           */ 00199000
@RF00182 AH    I,@CH00091                                          0190 00200000
@DE00172 CH    I,RELFILE#                                          0190 00201000
         BNH   @DL00172                                            0190 00202000
@DC00172 DS    0H                                                  0191 00203000
*   RETURN CODE(RLFDELRC);          /* RETURN WITH HIGHEST RETURN  0191 00204000
*                                      ENCOUNTERED DURING RJDRLFD  0191 00205000
*                                      PROCESSING                    */ 00206000
         LR    @15,RLFDELRC                                        0191 00207000
         L     @14,@SA00002                                        0191 00208000
         LM    @00,@12,@SA00002+8                                  0191 00209000
         BR    @14                                                 0191 00210000
*   END RJDRLFD;                    /* END OF PROCEDURE              */ 00211000
         EJECT                                                          00212000
*                                                                  0193 00213000
*   /*****************************************************************/ 00214000
*   /*                                                               */ 00215000
*   /* THIS ROUTINE WILL BE CALLED TO DELETE THE PTS MCS AND SYSMOD  */ 00216000
*   /* ENTRIES                                                       */ 00217000
*   /*                                                               */ 00218000
*   /*****************************************************************/ 00219000
*                                                                  0193 00220000
*RJDPTSD:                           /*                               */ 00221000
*   PROCEDURE;                      /*                               */ 00222000
RJDPTSD  STM   @14,@12,@SA00003                                    0193 00223000
*   DECLARE                         /*                               */ 00224000
*     PTSDELRC FIXED(8);            /* RETURN CODE FOR RJDPTSD       */ 00225000
*   PTSDELRC=ZERO;                  /* CLEAR OUT THE RETURN CODE     */ 00226000
*                                                                  0195 00227000
         SLR   PTSDELRC,PTSDELRC                                   0195 00228000
*   /*****************************************************************/ 00229000
*   /*                                                               */ 00230000
*   /* CALL HMASMIO TO DELETE PTS MCS ENTRY                          */ 00231000
*   /*                                                               */ 00232000
*   /*****************************************************************/ 00233000
*                                                                  0196 00234000
*   IOPDSID=IOPPTS;                 /* THE DATASET ID IS THE PTS DSN */ 00235000
         MVI   IOPDSID(IOPPTR),X'06'                               0196 00236000
*   IOPNAME=SYSMODNO;               /* THE SYSMOD N0. TO BE DELETED  */ 00237000
         MVI   IOPNAME+7(IOPPTR),C' '                              0197 00238000
         L     @10,@PC00001                                        0197 00239000
         MVC   IOPNAME(7,IOPPTR),SYSMODNO(@10)                     0197 00240000
*   IOPTYPE=IOPSMD;                 /* THE PTS CONTROL ENTRY         */ 00241000
         MVI   IOPTYPE(IOPPTR),X'0B'                               0198 00242000
*   IOPFUNCT=IOPSTOWD;              /* DO STOW DELETE OF THE ENTRY   */ 00243000
         MVI   IOPFUNCT(IOPPTR),X'53'                              0199 00244000
*   CALL HMASMIO(HMASMIOP);         /*                               */ 00245000
         ST    IOPPTR,@AL00001                                     0200 00246000
         L     @15,@CV01265                                        0200 00247000
         LA    @01,@AL00001                                        0200 00248000
         BALR  @14,@15                                             0200 00249000
*   PTSDELRC=IOPRETRN;              /* SAVE THE RETURN CODE FROM IO  */ 00250000
*                                                                  0201 00251000
         SLR   PTSDELRC,PTSDELRC                                   0201 00252000
         IC    PTSDELRC,IOPRETRN(,IOPPTR)                          0201 00253000
*   /*****************************************************************/ 00254000
*   /*                                                               */ 00255000
*   /* CALL HMASMIO TO DELETE PTS SYSMOD ENTRY                       */ 00256000
*   /*                                                               */ 00257000
*   /*****************************************************************/ 00258000
*                                                                  0202 00259000
*   IOPTYPE=IOPMCS;                 /* THE PTS SYSMOD NUMBER         */ 00260000
         MVI   IOPTYPE(IOPPTR),X'10'                               0202 00261000
*   CALL HMASMIO(HMASMIOP);         /*                               */ 00262000
         ST    IOPPTR,@AL00001                                     0203 00263000
         L     @15,@CV01265                                        0203 00264000
         LA    @01,@AL00001                                        0203 00265000
         BALR  @14,@15                                             0203 00266000
*   PTSDELRC=MAX(PTSDELRC,IOPRETRN);/* SAVE THE RETURN CODE FROM IO  */ 00267000
*                                                                  0204 00268000
         SLR   @10,@10                                             0204 00269000
         IC    @10,IOPRETRN(,IOPPTR)                               0204 00270000
         CR    PTSDELRC,@10                                        0204 00271000
         BNL   *+6                                                      00272000
         LR    PTSDELRC,@10                                        0204 00273000
*   /*****************************************************************/ 00274000
*   /*                                                               */ 00275000
*   /* SETUP FOR DELETION OF FUNCTION ID FROM THE SMPPTS VIA CALL TO */ 00276000
*   /* HMASMVLU, IF A FUNCTION AND RESTORE IS BEING RUN              */ 00277000
*   /*                                                               */ 00278000
*   /*****************************************************************/ 00279000
*                                                                  0205 00280000
*   IF PTSDELRC=IOPGOOD             /* NO ERRORS,                    */ 00281000
*       &CCARES=ON THEN             /* AND RESTORE FUNCTION          */ 00282000
         CH    PTSDELRC,@CH00256                                   0205 00283000
         BNE   @RF00205                                            0205 00284000
         TM    CCARES(CCAPTR),B'00001000'                          0205 00285000
         BNO   @RF00205                                            0205 00286000
*     DO;                           /*                               */ 00287000
*       IOPPTR=CCAIOPTR;            /* USE FULL IOP                  */ 00288000
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0207 00289000
*       IOPDSID=IOPPTS;             /* PTS DATASET                   */ 00290000
         MVI   IOPDSID(IOPPTR),X'06'                               0208 00291000
*       IOPTYPE=IOPSYS;             /* SYSTEM ENTRY                  */ 00292000
         MVI   IOPTYPE(IOPPTR),X'07'                               0209 00293000
*       IOPFUNCT=IOPLOC;            /* LOCATE FUNCTION               */ 00294000
         MVI   IOPFUNCT(IOPPTR),X'03'                              0210 00295000
*       CALL HMASMIO(HMASMIOP);     /* DO LOCATE OF PTS SYSTEM ENTRY */ 00296000
         ST    IOPPTR,@AL00001                                     0211 00297000
         L     @15,@CV01265                                        0211 00298000
         LA    @01,@AL00001                                        0211 00299000
         BALR  @14,@15                                             0211 00300000
*       PTSDELRC=IOPRETRN;          /* PICK UP RETURN CODE           */ 00301000
         SLR   PTSDELRC,PTSDELRC                                   0212 00302000
         IC    PTSDELRC,IOPRETRN(,IOPPTR)                          0212 00303000
*       IF PTSDELRC=IOPGOOD         /* SUCCESSFULL LOCATE OF PTS SYS */ 00304000
*           &IOPPTSNJ=OFF           /* AND REJECT AT RESTORE         */ 00305000
*         THEN                      /*                               */ 00306000
         CH    PTSDELRC,@CH00256                                   0213 00307000
         BNE   @RF00213                                            0213 00308000
         TM    IOPPTSNJ(IOPPTR),B'01000000'                        0213 00309000
         BNZ   @RF00213                                            0213 00310000
*         DO;                       /*                               */ 00311000
*           HMASMVLP=''B;           /*                               */ 00312000
         XC    HMASMVLP(52),HMASMVLP                               0215 00313000
*           VLPLPTR=ADDR(IOPPTNTR(1));/* IOP LIST TO BE DELETED      */ 00314000
         LA    @10,IOPPTNTR(,IOPPTR)                               0216 00315000
         ST    @10,VLPLPTR                                         0216 00316000
*           VLPFMSG=OFF;            /* NO MESSAGE                    */ 00317000
         NI    VLPFMSG,B'01111111'                                 0217 00318000
*           VLPDATA(1:LENGTH(IOPPTFMD))=SYSMODNO;/* FMID SYSMOD      */ 00319000
         L     @10,@PC00001                                        0218 00320000
         MVC   VLPDATA(7),SYSMODNO(@10)                            0218 00321000
*           VLPTYPE=IOPPTFM;        /* FMID TYPE TO BE DELETED       */ 00322000
         MVI   VLPTYPE,X'01'                                       0219 00323000
*           VLPDLEN=LENGTH(IOPPTFMD);/* LEN OF KEY IN VLPDATA        */ 00324000
         MVI   VLPDLEN,X'07'                                       0220 00325000
*           VLPFUNCT=VLPDEL;        /* DELETE ELEMENT FROM LIST      */ 00326000
         MVI   VLPFUNCT,X'02'                                      0221 00327000
*           VLPMTYPE=CCADSIDA(IOPPTS);/* SMPPTS                      */ 00328000
         L     @10,CCADSAD(,CCAPTR)                                0222 00329000
         MVC   VLPMTYPE(8),CCADSIDA+40(@10)                        0222 00330000
*           VLPMNAME=CCATYPEA(IOPSYS);/* SYSTEM                      */ 00331000
         L     @10,CCATPAD(,CCAPTR)                                0223 00332000
         MVC   VLPMNAME(8),CCATYPEA+48(@10)                        0223 00333000
*           VLPSTYPE=CCATYPEA(IOPFMD);/*                             */ 00334000
         MVI   VLPSTYPE+8,C' '                                     0224 00335000
         MVC   VLPSTYPE+9(7),VLPSTYPE+8                            0224 00336000
         MVC   VLPSTYPE(8),CCATYPEA+104(@10)                       0224 00337000
*           CALL HMASMVLU(HMASMVLP);/*                               */ 00338000
         L     @15,@CV01267                                        0225 00339000
         LA    @01,@AL00225                                        0225 00340000
         BALR  @14,@15                                             0225 00341000
*           IF VLPRETRN=VLPGOOD     /* IF SYSMOD WAS FMID AND DELETED*/ 00342000
*             THEN                  /*                               */ 00343000
         CLI   VLPRETRN,0                                          0226 00344000
         BNE   @RF00226                                            0226 00345000
*             DO;                   /*                               */ 00346000
*               IOPFUNCT=IOPSTOWR;  /* REPLACE ENTRY                 */ 00347000
         MVI   IOPFUNCT(IOPPTR),X'54'                              0228 00348000
*               CALL HMASMIO(HMASMIOP);/*                            */ 00349000
         ST    IOPPTR,@AL00001                                     0229 00350000
         L     @15,@CV01265                                        0229 00351000
         LA    @01,@AL00001                                        0229 00352000
         BALR  @14,@15                                             0229 00353000
*               PTSDELRC=MAX(PTSDELRC,IOPRETRN);/*                   */ 00354000
         SLR   @10,@10                                             0230 00355000
         IC    @10,IOPRETRN(,IOPPTR)                               0230 00356000
         CR    PTSDELRC,@10                                        0230 00357000
         BNL   *+6                                                      00358000
         LR    PTSDELRC,@10                                        0230 00359000
*             END;                  /*                               */ 00360000
*         END;                      /*                               */ 00361000
@RF00226 DS    0H                                                  0233 00362000
*     END;                          /*                               */ 00363000
@RF00213 DS    0H                                                  0234 00364000
*   IOPPTR=ADDR(RJDIOP);            /* RESTORE TO LOCAL IOP          */ 00365000
@RF00205 LA    IOPPTR,RJDIOP                                       0234 00366000
*   RETURN CODE(PTSDELRC);          /* RETURN TO CALL WITH MAX RETURN   00367000
*                                      CODE FOUND                    */ 00368000
         LR    @15,PTSDELRC                                        0235 00369000
         L     @14,@SA00003                                        0235 00370000
         LM    @00,@12,@SA00003+8                                  0235 00371000
         BR    @14                                                 0235 00372000
*   END RJDPTSD;                    /* END RJDMCSD PROCESSING        */ 00373000
*                                                                  0237 00374000
*   /*****************************************************************/ 00375000
*   /*                                                               */ 00376000
*   /* PTSLOC - LOCATE PTS SYSMOD ENTRY FOR THE SYSMOD BEING         */ 00377000
*   /* PROCESSED AND SET RELFILE# AND DSPREFIX ... APPLY, ACCEPT AND */ 00378000
*   /* RESTORE PROCESSING ONLY                                       */ 00379000
*   /*                                                               */ 00380000
*   /*****************************************************************/ 00381000
*                                                                  0237 00382000
*PTSLOC:                                                           0237 00383000
*   PROC;                                                          0237 00384000
PTSLOC   STM   @14,@01,@SA00004                                    0237 00385000
         STM   @03,@12,@SA00004+16                                 0237 00386000
*   IOPPTR=CCAIOPTR;                /* POINT TO FULL LENGTH IOP      */ 00387000
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0238 00388000
*   IF CCAAPPLY=ON                  /* APPLY PROCESSING              */ 00389000
*       |CCAACCPT=ON                /* ACCEPT PROCESSING             */ 00390000
*       |CCARES=ON                  /* RESTORE PROCESSING            */ 00391000
*     THEN                                                         0239 00392000
         TM    CCAAPPLY(CCAPTR),B'01101000'                        0239 00393000
         BZ    @RF00239                                            0239 00394000
*     DO;                                                          0240 00395000
*                                                                  0240 00396000
*       /*************************************************************/ 00397000
*       /*                                                           */ 00398000
*       /* CLEAR RELFILE# (NUMBER OF RELFILES) AND DSPREFIX (DATASET */ 00399000
*       /* NAME PREFIX) FIELDS                                       */ 00400000
*       /*                                                           */ 00401000
*       /*************************************************************/ 00402000
*                                                                  0241 00403000
*       RELFILE#=0;                 /* CLEAR NO. OF RELFILES         */ 00404000
         SLR   @10,@10                                             0241 00405000
         STH   @10,RELFILE#                                        0241 00406000
*       DSPREFIX=' ';               /* BLANK DATASET PREFIX          */ 00407000
         MVI   DSPREFIX+1,C' '                                     0242 00408000
         MVC   DSPREFIX+2(24),DSPREFIX+1                           0242 00409000
         MVI   DSPREFIX,C' '                                       0242 00410000
*       IOPFUNCT=IOPLOC;            /* LOCATE                        */ 00411000
         MVI   IOPFUNCT(IOPPTR),X'03'                              0243 00412000
