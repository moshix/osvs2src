         TITLE 'HMASMSEC - CREATE (A)CDS SYSMOD FROM ICT SYSMOD ENTRY'  00001000
HMASMSEC CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMSEC  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(8),0(@01)                                  0001 00017000
*                                                                  0169 00018000
*   /*****************************************************************/ 00019000
*   /*                                                               */ 00020000
*   /* MAIN MODULE SEGMENT FOR HMASMSEC                              */ 00021000
*   /*                                                               */ 00022000
*   /*****************************************************************/ 00023000
*                                                                  0169 00024000
*   SECRETCD=ZERO;                  /* INIT RETURN CODE COLLECTOR    */ 00025000
         SLR   SECRETCD,SECRETCD                                   0169 00026000
*   SECBITS=''B;                    /* INIT CONTROL BITS             */ 00027000
         MVI   SECBITS,X'00'                                       0170 00028000
*   RFY                                                            0171 00029000
*     ICTPTF BASED(ADDR(CURSYM));   /* BASE ON CURRENT ICT SYSMOD    */ 00030000
*   RFY                                                            0172 00031000
*     HMASMIOP BASED(CURIOP);       /* BASE ON PASSED IOP TO USE     */ 00032000
*                                                                  0172 00033000
*   /*****************************************************************/ 00034000
*   /*                                                               */ 00035000
*   /* IF THE 'IN PROCESS' BIT IS ON, AND AN ENTRY EXISTS ON THE     */ 00036000
*   /* (A)CDS, THEN SET UP TO RETURN TO THE CALLER WITH RC=0         */ 00037000
*   /*                                                               */ 00038000
*   /*****************************************************************/ 00039000
*                                                                  0173 00040000
*   IF ICTPROCS(1)=ON               /* IS THE 'IN PROCESS' BIT ON AND*/ 00041000
*       &ICTPCDSE(1)=ON THEN        /* IS AN ENTRY ON (A)CDS?        */ 00042000
         L     @10,@PC00001                                        0173 00043000
         TM    ICTPROCS(@10),B'10000000'                           0173 00044000
         BNO   @RF00173                                            0173 00045000
         TM    ICTPCDSE(@10),B'10000000'                           0173 00046000
         BNO   @RF00173                                            0173 00047000
*     ALLDONE=ON;                   /* SET UP TO RETURN WITH RC=0    */ 00048000
         OI    ALLDONE,B'01000000'                                 0174 00049000
*   ELSE                                                           0175 00050000
*     DO;                           /*                               */ 00051000
         B     @RC00173                                            0175 00052000
@RF00173 DS    0H                                                  0176 00053000
*       ICTERROR(1)=ON;             /* SET ERROR BIT ON              */ 00054000
*                                                                  0176 00055000
         L     @10,@PC00001                                        0176 00056000
         OI    ICTERROR(@10),B'00010000'                           0176 00057000
*       /*************************************************************/ 00058000
*       /*                                                           */ 00059000
*       /* IF THE ICT SYSMOD IS MARKED FOR DELETE (EITHER EXPLICIT OR*/ 00060000
*       /* IMPLICIT) THEN CALL THE DELETE PROCESSING SUBROUTINE AND  */ 00061000
*       /* SET UP TO RETURN TO THE CALLER WITHOUT BUILDING A ENTRY.  */ 00062000
*       /*                                                           */ 00063000
*       /*************************************************************/ 00064000
*                                                                  0177 00065000
*       IF ICTPDEXP(1)=ON           /* PTF ENTRY EXPLICITLY DELETED, */ 00066000
*           |ICTPDIMP(1)=ON THEN    /* PTF ENTRY IMPLICITLY DELETED? */ 00067000
         TM    ICTPDEXP(@10),B'00110000'                           0177 00068000
         BZ    @RF00177                                            0177 00069000
*         DO;                       /* YES- THEN DO DELETE PROCESS   */ 00070000
*           CALL SECDELET;          /* ROUTINE TO HANDLE DELETE      */ 00071000
         BAL   @14,SECDELET                                        0179 00072000
*           RFY                                                    0180 00073000
*             RTNCODE RSTD;         /* RESTRICT REG 15               */ 00074000
*           SECRETCD=MAX(SECRETCD,RTNCODE);/* PICK UP HIGHEST RETURN    00075000
*                                      CODE                          */ 00076000
         CR    SECRETCD,RTNCODE                                    0181 00077000
         BNL   *+6                                                      00078000
         LR    SECRETCD,RTNCODE                                    0181 00079000
*           RFY                                                    0182 00080000
*             RTNCODE UNRSTD;       /* RELEASE REG 15                */ 00081000
*           ALLDONE=ON;             /* SET UP TO RETURN TO CALLER    */ 00082000
         OI    ALLDONE,B'01000000'                                 0183 00083000
*         END;                      /* END OF THE DELETE PROCESS     */ 00084000
*     END;                          /* END OF ELSE DO                */ 00085000
@RF00177 DS    0H                                                  0186 00086000
*                                                                  0186 00087000
*   /*****************************************************************/ 00088000
*   /*                                                               */ 00089000
*   /* IF NOT ALL COMPLETED ALREADY THEN CALL APPROPRIATE SUBROUTINE */ 00090000
*   /* BASED ON FUNCTION BEING EXECUTED.                             */ 00091000
*   /*                                                               */ 00092000
*   /*****************************************************************/ 00093000
*                                                                  0186 00094000
*   IF ALLDONE=OFF THEN             /* RETURN NOT DESIRED?           */ 00095000
@RC00173 TM    ALLDONE,B'01000000'                                 0186 00096000
         BNZ   @RF00186                                            0186 00097000
*     DO;                           /*                               */ 00098000
*       RFY                                                        0188 00099000
*         RTNCODE RSTD;             /* RESTRICT REG 15               */ 00100000
*       IF CCAAPPLY=ON              /* IS FUNCTION APPLY, OR         */ 00101000
*           |CCAACCPT=ON            /* ACCEPT?                       */ 00102000
*         THEN                      /*                               */ 00103000
         TM    CCAAPPLY(CCAPTR),B'01100000'                        0189 00104000
         BZ    @RF00189                                            0189 00105000
*         CALL SECAPPAC;            /* YES- CALL APPLY/ACCEPT PROCESS*/ 00106000
         BAL   @14,SECAPPAC                                        0190 00107000
*       ELSE                        /*                               */ 00108000
*         CALL SECREST;             /* YES- CALL RESTORE PROCESS     */ 00109000
         B     @RC00189                                            0191 00110000
@RF00189 BAL   @14,SECREST                                         0191 00111000
*       SECRETCD=MAX(SECRETCD,RTNCODE);/* PICK UP HIGHEST RETURN CODE*/ 00112000
@RC00189 CR    SECRETCD,RTNCODE                                    0192 00113000
         BNL   *+6                                                      00114000
         LR    SECRETCD,RTNCODE                                    0192 00115000
*       RFY                         /*                               */ 00116000
*         RTNCODE UNRSTD;           /*                               */ 00117000
*     END;                          /*                               */ 00118000
*                                                                  0194 00119000
*   /*****************************************************************/ 00120000
*   /*                                                               */ 00121000
*   /* RETURN TO THE CALLER WITH THE HIGHEST RETURN CODE ENCOUNTERED */ 00122000
*   /*                                                               */ 00123000
*   /*****************************************************************/ 00124000
*                                                                  0195 00125000
*   RETURN CODE(SECRETCD);          /* RETURN TO CALLER WITH MAX RC  */ 00126000
@RF00186 LR    @15,SECRETCD                                        0195 00127000
         L     @13,4(,@13)                                         0195 00128000
         L     @14,12(,@13)                                        0195 00129000
         LM    @00,@12,20(@13)                                     0195 00130000
         BR    @14                                                 0195 00131000
         EJECT                                                          00132000
*                                                                  0196 00133000
*   /*****************************************************************/ 00134000
*   /*                                                               */ 00135000
*   /* DELETE SYSMOD PROCESSING ROUTINE                              */ 00136000
*   /*                                                               */ 00137000
*   /*****************************************************************/ 00138000
*                                                                  0196 00139000
*SECDELET:                                                         0196 00140000
*   PROC;                           /*                               */ 00141000
SECDELET STM   @14,@12,@SA00002                                    0196 00142000
*   DECLARE                                                        0197 00143000
*     SECDELRC FIXED(31);           /* RETURN CODE HOLDER            */ 00144000
*   SECDELRC=0;                     /* CLEAR RETURN CODE             */ 00145000
*                                                                  0198 00146000
         SLR   SECDELRC,SECDELRC                                   0198 00147000
*   /*****************************************************************/ 00148000
*   /*                                                               */ 00149000
*   /* DO A BLDL FOR THE SYSMOD ENTRY ON THE (A)CDS                  */ 00150000
*   /*                                                               */ 00151000
*   /*****************************************************************/ 00152000
*                                                                  0199 00153000
*   IF CCAAPPLY=ON THEN             /* IS FUNCTION APPLY?            */ 00154000
         TM    CCAAPPLY(CCAPTR),B'01000000'                        0199 00155000
         BNO   @RF00199                                            0199 00156000
*     IOPDSID=IOPCDS;               /* INDICATE CDS DATASET          */ 00157000
         L     @10,@PC00001+4                                      0200 00158000
         L     @10,CURIOP(,@10)                                    0200 00159000
         MVI   IOPDSID(@10),X'02'                                  0200 00160000
*   ELSE                            /* MUST BE ACCEPT FUNCTION       */ 00161000
*     IOPDSID=IOPACDS;              /* INDICATE ACDS DATASET         */ 00162000
         B     @RC00199                                            0201 00163000
@RF00199 L     @10,@PC00001+4                                      0201 00164000
         L     @10,CURIOP(,@10)                                    0201 00165000
         MVI   IOPDSID(@10),X'0C'                                  0201 00166000
*   IOPFUNCT=IOPBLDL;               /* INDICATE BLDL OPERATION       */ 00167000
@RC00199 L     @10,@PC00001+4                                      0202 00168000
         L     @10,CURIOP(,@10)                                    0202 00169000
         MVI   IOPFUNCT(@10),X'04'                                 0202 00170000
*   IOPNAME=ICTPTFS(1);             /* SYSMOD BEING PROCESSED        */ 00171000
         MVI   IOPNAME+7(@10),C' '                                 0203 00172000
         L     @03,@PC00001                                        0203 00173000
         MVC   IOPNAME(7,@10),ICTPTFS(@03)                         0203 00174000
*   IOPTYPE=IOPSMD;                 /* SYSMOD TYPE ENTRY DESIRED     */ 00175000
         MVI   IOPTYPE(@10),X'0B'                                  0204 00176000
*   CALL HMASMIO(HMASMIOP);         /* CALL SMP IO INTERFACE ROUTINE */ 00177000
         ST    @10,@AL00001                                        0205 00178000
         L     @15,@CV01624                                        0205 00179000
         LA    @01,@AL00001                                        0205 00180000
         BALR  @14,@15                                             0205 00181000
*   IF IOPRETRN>IOPNOTFD            /* ERRORS ENCOUNTERED            */ 00182000
*     THEN                                                         0206 00183000
         L     @10,@PC00001+4                                      0206 00184000
         L     @10,CURIOP(,@10)                                    0206 00185000
         CLI   IOPRETRN(@10),4                                     0206 00186000
         BNH   @RF00206                                            0206 00187000
*     SECDELRC=IOPRETRN;            /* SAVE RETURN CODE FROM I/O     */ 00188000
         SLR   SECDELRC,SECDELRC                                   0207 00189000
         IC    SECDELRC,IOPRETRN(,@10)                             0207 00190000
*   IF IOPRETRN=IOPGOOD THEN        /* WAS THE SYSMOD ENTRY FOUND?   */ 00191000
@RF00206 L     @10,@PC00001+4                                      0208 00192000
         L     @10,CURIOP(,@10)                                    0208 00193000
         CLI   IOPRETRN(@10),0                                     0208 00194000
         BNE   @RF00208                                            0208 00195000
*     DO;                           /* YES-                          */ 00196000
*                                                                  0209 00197000
*       /*************************************************************/ 00198000
*       /*                                                           */ 00199000
*       /* SET UP TO UPDATE THE DIRECTORY ONLY OPERATION WITH THE    */ 00200000
*       /* ERROR FLAG SET ON (A)CDS                                  */ 00201000
*       /*                                                           */ 00202000
*       /*************************************************************/ 00203000
*                                                                  0210 00204000
*       IOPERROR=ON;                /* TURN ON THE ERROR FLAG        */ 00205000
         OI    IOPERROR(@10),B'00010000'                           0210 00206000
*       IOPFUNCT=IOPSTOWR;          /* ADD ENTRY                     */ 00207000
         MVI   IOPFUNCT(@10),X'54'                                 0211 00208000
*       CALL HMASMIO(HMASMIOP);     /* CALL SMP IO INTERFACE ROUTINE */ 00209000
         ST    @10,@AL00001                                        0212 00210000
         L     @15,@CV01624                                        0212 00211000
         LA    @01,@AL00001                                        0212 00212000
         BALR  @14,@15                                             0212 00213000
*       SECDELRC=IOPRETRN;          /* SAVE I/O RETURN CODE          */ 00214000
         L     @10,@PC00001+4                                      0213 00215000
         L     @10,CURIOP(,@10)                                    0213 00216000
         SLR   SECDELRC,SECDELRC                                   0213 00217000
         IC    SECDELRC,IOPRETRN(,@10)                             0213 00218000
*     END;                          /* END SYSMOD ENTRY ON (A)CDS    */ 00219000
*   RETURN CODE(SECDELRC);          /* RETURN WITH RETURN CODE FROM     00220000
*                                      HMASMIO BLDL/UPDATE DIR OPER  */ 00221000
@RF00208 LR    @15,SECDELRC                                        0215 00222000
         L     @14,@SA00002                                        0215 00223000
         LM    @00,@12,@SA00002+8                                  0215 00224000
         BR    @14                                                 0215 00225000
*   END SECDELET;                   /*                               */ 00226000
         EJECT                                                          00227000
*                                                                  0217 00228000
*   /*****************************************************************/ 00229000
*   /*                                                               */ 00230000
*   /* RESTORE PROCESS SUBROUTINE                                    */ 00231000
*   /*                                                               */ 00232000
*   /*****************************************************************/ 00233000
*                                                                  0217 00234000
*SECREST:                                                          0217 00235000
*   PROC;                           /*                               */ 00236000
*                                                                  0217 00237000
SECREST  STM   @14,@12,@SA00003                                    0217 00238000
*   /*****************************************************************/ 00239000
*   /*                                                               */ 00240000
*   /* LOCATE THE SYSMOD ENTRY ON THE ACDS                           */ 00241000
*   /*                                                               */ 00242000
*   /*****************************************************************/ 00243000
*                                                                  0218 00244000
*   IOPFUNCT=IOPLOC;                /* INDICATE LOCATE OPERATION     */ 00245000
         L     @10,@PC00001+4                                      0218 00246000
         L     @10,CURIOP(,@10)                                    0218 00247000
         MVI   IOPFUNCT(@10),X'03'                                 0218 00248000
*   IOPDSID=IOPCDS;                 /* CDS DATASET FOR THE RESTORE   */ 00249000
         MVI   IOPDSID(@10),X'02'                                  0219 00250000
*   IOPNAME=ICTPTFS(1);             /* SYSMOD BEING PROCESSED        */ 00251000
         MVI   IOPNAME+7(@10),C' '                                 0220 00252000
         L     @15,@PC00001                                        0220 00253000
         MVC   IOPNAME(7,@10),ICTPTFS(@15)                         0220 00254000
*   IOPTYPE=IOPSMD;                 /* SYSMOD TYPE ENTRY DESIRED     */ 00255000
         MVI   IOPTYPE(@10),X'0B'                                  0221 00256000
*   CALL HMASMIO(HMASMIOP);         /* CALL SMP IO INTERFACE ROUTINE */ 00257000
         ST    @10,@AL00001                                        0222 00258000
         L     @15,@CV01624                                        0222 00259000
         LA    @01,@AL00001                                        0222 00260000
         BALR  @14,@15                                             0222 00261000
*   IF IOPRETRN=IOPGOOD THEN        /* WAS THE SYSMOD ENTRY FOUND?   */ 00262000
         L     @10,@PC00001+4                                      0223 00263000
         L     @10,CURIOP(,@10)                                    0223 00264000
         CLI   IOPRETRN(@10),0                                     0223 00265000
         BNE   @RF00223                                            0223 00266000
*     DO;                           /* YES-                          */ 00267000
*                                                                  0224 00268000
*       /*************************************************************/ 00269000
*       /*                                                           */ 00270000
*       /* SET UP TO STOW THE SYSMOD WITH THE ERROR FLAG SET ON THE  */ 00271000
*       /* CDS                                                       */ 00272000
*       /*                                                           */ 00273000
*       /*************************************************************/ 00274000
*                                                                  0225 00275000
*       IOPERROR=ON;                /* TURN ON THE ERROR FLAG        */ 00276000
         OI    IOPERROR(@10),B'00010000'                           0225 00277000
*       IF ICTBYP(1)=ON             /* BYPASS USED IN PROCESSING     */ 00278000
*         THEN                                                     0226 00279000
         L     @15,@PC00001                                        0226 00280000
         TM    ICTBYP(@15),B'00000010'                             0226 00281000
         BNO   @RF00226                                            0226 00282000
*         IOPBYP=ON;                /* SET BYPASS FLAG IN IOP        */ 00283000
         OI    IOPBYP(@10),B'00000010'                             0227 00284000
*       IOPRES=ON;                  /* TURN ON THE RESTORE FLAG      */ 00285000
@RF00226 L     @10,@PC00001+4                                      0228 00286000
         L     @10,CURIOP(,@10)                                    0228 00287000
         OI    IOPRES(@10),B'01000000'                             0228 00288000
*       IOPRSTD=CCADATE;            /* SET CURRENT DATE              */ 00289000
         MVC   IOPRSTD(3,@10),CCADATE(CCAPTR)                      0229 00290000
*       IOPRSTT=CCATIME;            /* SET CURRENT TIME              */ 00291000
         MVC   IOPRSTT(3,@10),CCATIME(CCAPTR)                      0230 00292000
*       IOPDSID=IOPCDS;             /* STOW TO THE CDS DATASET       */ 00293000
         MVI   IOPDSID(@10),X'02'                                  0231 00294000
*       IOPFUNCT=IOPSTOWR;          /* INDICATE STOW REPLACE OPER    */ 00295000
         MVI   IOPFUNCT(@10),X'54'                                 0232 00296000
*       CALL HMASMIO(HMASMIOP);     /* CALL SMP IO INTERFACE ROUTINE */ 00297000
         ST    @10,@AL00001                                        0233 00298000
         L     @15,@CV01624                                        0233 00299000
         LA    @01,@AL00001                                        0233 00300000
         BALR  @14,@15                                             0233 00301000
*     END;                          /*                               */ 00302000
*   RETURN CODE(IOPRETRN);          /* RETURN WITH RETURN CODE FROM     00303000
*                                      HMASMIO LOCATE/STOWR OPER     */ 00304000
@RF00223 L     @10,@PC00001+4                                      0235 00305000
         L     @10,CURIOP(,@10)                                    0235 00306000
         SLR   @15,@15                                             0235 00307000
         IC    @15,IOPRETRN(,@10)                                  0235 00308000
         L     @14,@SA00003                                        0235 00309000
         LM    @00,@12,@SA00003+8                                  0235 00310000
         BR    @14                                                 0235 00311000
*   END SECREST;                    /*                               */ 00312000
         EJECT                                                          00313000
*                                                                  0237 00314000
*   /*****************************************************************/ 00315000
*   /*                                                               */ 00316000
*   /* APPLY OR ACCEPT PROCESS SUBROUTINE                            */ 00317000
*   /*                                                               */ 00318000
*   /*****************************************************************/ 00319000
*                                                                  0237 00320000
*SECAPPAC:                                                         0237 00321000
*   PROC;                           /*                               */ 00322000
SECAPPAC STM   @14,@12,@SA00004                                    0237 00323000
*   DECLARE                         /*                               */ 00324000
*     M1 FIXED(31),                 /* LOOP INDEX                    */ 00325000
*     M2 FIXED(31);                 /* LOOP INDEX                    */ 00326000
*   DECLARE                         /*                               */ 00327000
*     APACRC FIXED(8);              /*                               */ 00328000
*   APACRC=ZERO;                    /*                               */ 00329000
         SLR   APACRC,APACRC                                       0240 00330000
*   IOPPTFDR=''B;                   /* CLEAR THE IOP FOR BUILD       */ 00331000
         L     @10,@PC00001+4                                      0241 00332000
         L     @10,CURIOP(,@10)                                    0241 00333000
         XC    IOPPTFDR(24,@10),IOPPTFDR(@10)                      0241 00334000
*   IOPPTFFX=''B;                   /* CLEAR THE IOP FOR BUILD       */ 00335000
*                                                                  0242 00336000
         XC    IOPPTFFX(80,@10),IOPPTFFX(@10)                      0242 00337000
*   /*****************************************************************/ 00338000
*   /*                                                               */ 00339000
*   /* DETERMINE THE FUNCTION AND SET UP THE PROPER DATASET, INSERT  */ 00340000
*   /* THE CURRENT DATE AND TIME IN THE IOP BEING BUILT.             */ 00341000
*   /*                                                               */ 00342000
*   /*****************************************************************/ 00343000
*                                                                  0243 00344000
*   IF CCAAPPLY=ON THEN             /* IS FUNCTION APPLY?            */ 00345000
         TM    CCAAPPLY(CCAPTR),B'01000000'                        0243 00346000
         BNO   @RF00243                                            0243 00347000
*     DO;                           /* YES-                          */ 00348000
*                                                                  0244 00349000
*       /*************************************************************/ 00350000
*       /*                                                           */ 00351000
*       /* APPLY BEING EXECUTED                                      */ 00352000
*       /*                                                           */ 00353000
*       /*************************************************************/ 00354000
*                                                                  0245 00355000
*       IOPDSID=IOPCDS;             /* INDICATE CDS DATASET          */ 00356000
         MVI   IOPDSID(@10),X'02'                                  0245 00357000
*       IOPAPP=ON;                  /* INDICATE APPLY FUNCTION       */ 00358000
         OI    IOPAPP(@10),B'10000000'                             0246 00359000
*       IOPAPPD=CCADATE;            /* FILL IN APPLY DATE            */ 00360000
         MVC   IOPAPPD(3,@10),CCADATE(CCAPTR)                      0247 00361000
*       IOPAPPT=CCATIME;            /* FILL IN APPLY TIME            */ 00362000
         MVC   IOPAPPT(3,@10),CCATIME(CCAPTR)                      0248 00363000
*     END;                          /*                               */ 00364000
*   ELSE                            /* MUST BE ACCEPT FUNCTION       */ 00365000
*     DO;                           /*                               */ 00366000
*                                                                  0250 00367000
         B     @RC00243                                            0250 00368000
@RF00243 DS    0H                                                  0251 00369000
*       /*************************************************************/ 00370000
*       /*                                                           */ 00371000
*       /* ACCEPT BEING EXECUTED                                     */ 00372000
*       /*                                                           */ 00373000
*       /*************************************************************/ 00374000
*                                                                  0251 00375000
*       IOPDSID=IOPACDS;            /* INDICATE ACDS DATASET         */ 00376000
         L     @10,@PC00001+4                                      0251 00377000
         L     @10,CURIOP(,@10)                                    0251 00378000
         MVI   IOPDSID(@10),X'0C'                                  0251 00379000
*       IOPACC=ON;                  /* INDICATE ACCEPT FUNCTION      */ 00380000
         OI    IOPACC(@10),B'00100000'                             0252 00381000
*       IOPACCD=CCADATE;            /* FILL IN ACCEPT DATE           */ 00382000
         MVC   IOPACCD(3,@10),CCADATE(CCAPTR)                      0253 00383000
*       IOPACCT=CCATIME;            /* FILL IN ACCEPT TIME           */ 00384000
         MVC   IOPACCT(3,@10),CCATIME(CCAPTR)                      0254 00385000
*     END;                          /*                               */ 00386000
*                                                                  0256 00387000
*   /*****************************************************************/ 00388000
*   /*                                                               */ 00389000
*   /* FILL IN VALUES FROM THE ICT PTF ENTRY FOR THE CURRENT SYSMOD  */ 00390000
*   /*                                                               */ 00391000
*   /*****************************************************************/ 00392000
*                                                                  0256 00393000
*   IOPNTLVL=IOPPTFLC;              /* CURRENT LEVEL                 */ 00394000
@RC00243 L     @10,@PC00001+4                                      0256 00395000
         L     @10,CURIOP(,@10)                                    0256 00396000
         MVI   IOPNTLVL(@10),X'02'                                 0256 00397000
*   IOPERROR=ON;                    /* SET ON ERROR FLAG             */ 00398000
         OI    IOPERROR(@10),B'00010000'                           0257 00399000
*   IF ICTBYP(1)=ON                 /* BYPASS USED IN PROCESSING     */ 00400000
*     THEN                                                         0258 00401000
         L     @05,@PC00001                                        0258 00402000
         TM    ICTBYP(@05),B'00000010'                             0258 00403000
         BNO   @RF00258                                            0258 00404000
*     IOPBYP=ON;                    /* SET BYPASS FLAG IN IOP        */ 00405000
         OI    IOPBYP(@10),B'00000010'                             0259 00406000
*   IOPPTYPE=ICTPTYPE(1);           /* PICK UP SYSMOD TYPE - APAR,PTF   00407000
*                                      USERMOD OR FUNCTION           */ 00408000
@RF00258 L     @10,@PC00001+4                                      0260 00409000
         L     @10,CURIOP(,@10)                                    0260 00410000
         L     @05,@PC00001                                        0260 00411000
         MVC   IOPPTYPE(1,@10),ICTPTYPE(@05)                       0260 00412000
*   IOPPTFF2=ICTPFLG5(1);           /* COPY FLAG BYTE                */ 00413000
         MVC   IOPPTFF2(2,@10),ICTPFLG5(@05)                       0261 00414000
*   IOPPTFF3=ICTPFLG6(1);           /* COPY FLAG BYTE                */ 00415000
         MVC   IOPPTFF3(1,@10),ICTPFLG6(@05)                       0262 00416000
