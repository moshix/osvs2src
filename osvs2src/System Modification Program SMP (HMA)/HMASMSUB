         TITLE 'HMASMSUB - HMASMP SUBROUTINES'                          00001000
HMASMSUB CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMSUB  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(4),0(@01)                                  0001 00017000
*                                                                  0160 00018000
*   /*****************************************************************/ 00019000
*   /*                                                               */ 00020000
*   /* INITIALIZE RETURN CODE AND CALL PROPER SUBROUTINE FOR FUNCTION*/ 00021000
*   /* REQUESTED IN SPLFUNCT THEN RETURN                             */ 00022000
*   /*                                                               */ 00023000
*   /*****************************************************************/ 00024000
*                                                                  0160 00025000
*   SUBRTNCD=SPLGOOD;               /* SET GOOD RETURN CODE          */ 00026000
         SLR   SUBRTNCD,SUBRTNCD                                   0160 00027000
*   CCAPTR=ADDR(HMASMCCA);          /*                       @Z67SP00*/ 00028000
         L     CCAPTR,@CA00071                                     0161 00029000
*   CALL SUBRTN;                    /* CALL REQUESTED SUBROUTINE     */ 00030000
         L     @10,@PC00001                                        0162 00031000
         SLR   @05,@05                                             0162 00032000
         IC    @05,SPLFUNCT(,@10)                                  0162 00033000
         SLA   @05,2                                               0162 00034000
         L     @15,SUBRTNS-4(@05)                                  0162 00035000
         BALR  @14,@15                                             0162 00036000
*   RFY                                                            0163 00037000
*     RTNCODE RSTD;                 /*                       @Z67SP00*/ 00038000
*   SUBRTNCD=MAX(SUBRTNCD,RTNCODE); /*                       @Z67SP00*/ 00039000
         CR    SUBRTNCD,RTNCODE                                    0164 00040000
         BNL   *+6                                                      00041000
         LR    SUBRTNCD,RTNCODE                                    0164 00042000
*   RFY                                                            0165 00043000
*     RTNCODE UNRSTD;               /*                       @Z67SP00*/ 00044000
*   RETURN CODE(SUBRTNCD);          /* RETURN WITH CODE IN PARM AREA */ 00045000
         LR    @15,SUBRTNCD                                        0166 00046000
         L     @13,4(,@13)                                         0166 00047000
         L     @14,12(,@13)                                        0166 00048000
         LM    @00,@12,20(@13)                                     0166 00049000
         BR    @14                                                 0166 00050000
         EJECT                                                          00051000
*                                                                  0167 00052000
*   /*****************************************************************/ 00053000
*   /*                                                               */ 00054000
*   /* ENTRY POINT TO PACK 8 CHARACTER DLIB OR SOURCE NAMES INTO 6   */ 00055000
*   /* CHARACTERS                                                    */ 00056000
*   /*                                                               */ 00057000
*   /*****************************************************************/ 00058000
*                                                                  0167 00059000
*PACKSRTN:                          /* SMP 8 TO 6 CHAR DLIB PACK     */ 00060000
*   PROCEDURE;                      /* ROUTINE                       */ 00061000
PACKSRTN STM   @14,@12,12(@13)                                     0167 00062000
*   CNVDLIB8=CNVPRM8;               /* MOVE 8 CHAR NAME              */ 00063000
         L     @10,@PC00001                                        0168 00064000
         L     @10,SPLPMAD(,@10)                                   0168 00065000
         MVC   CNVDLIB8(8),CNVPRM8(@10)                            0168 00066000
*   TR(CNVDLIB8,TRTABLE1);          /* CONVERT TO INT. FMT.  @Z67SP00*/ 00067000
*                                                                  0169 00068000
         TR    CNVDLIB8(8),TRTABLE1                                0169 00069000
*   /*****************************************************************/ 00070000
*   /*                                                               */ 00071000
*   /* LOOP THRU THE 8 CHARACTER NAME IN GROUPS OF 4 CHARACTERS      */ 00072000
*   /*                                                               */ 00073000
*   /*****************************************************************/ 00074000
*                                                                  0170 00075000
*   DO CNVCNT1=1 TO DIM(DLIB8WD);   /* PACK ONE WORD AT A TIME       */ 00076000
         LA    CNVCNT1,1                                           0170 00077000
@DL00170 DS    0H                                                  0171 00078000
*     CNVCHAR4=DLIB8WD(CNVCNT1);    /* PICK UP 4 CHARACTERS          */ 00079000
*                                                                  0171 00080000
         LR    @10,CNVCNT1                                         0171 00081000
         SLA   @10,2                                               0171 00082000
         L     @10,DLIB8WD-4(@10)                                  0171 00083000
         ST    @10,CNVCHAR4                                        0171 00084000
*     /***************************************************************/ 00085000
*     /*                                                             */ 00086000
*     /* LOOP THRU THE 4 CHARACTERS. THE HIGH ORDER BITS ARE SHIFTED */ 00087000
*     /* OUT. THEN THE REMAINING 6 BITS ARE SHIFTED LEFT INTO THE    */ 00088000
*     /* ADJOINING REGISTER. AT THE END OF THIS LOOP THE LOWER THREE */ 00089000
*     /* BYTES OF THE ADJOINING REGISTER WILL CONTAIN THE PACKED     */ 00090000
*     /* REPRSENTATION OF THE FOUR BYTES PROCESSED.                  */ 00091000
*     /*                                                             */ 00092000
*     /***************************************************************/ 00093000
*                                                                  0172 00094000
*     RFY                                                          0172 00095000
*      (CNVREG2,                                                   0172 00096000
*       CNVREG3) RSTD;              /*                       @PK00006*/ 00097000
*     CNVREG3=CNVCHAR4;             /* PICK UP 4 CHAR TO BE PACKED   */ 00098000
         LR    CNVREG3,@10                                         0173 00099000
*     CNVREG2=0;                    /* ZERO OUT REG TO HOLD PACKED 0174 00100000
*                                      CHARACTER                     */ 00101000
         SLR   CNVREG2,CNVREG2                                     0174 00102000
*     DO CNVCNT2=1 TO LENGTH(DLIB8CH);/* LOOP THRU 4 CHAR PER WRD    */ 00103000
         LA    CNVCNT2,1                                           0175 00104000
@DL00175 DS    0H                                                  0176 00105000
*       SLL(CNVREG3,2);             /* SHIFT OUT HIGH 2 BITS OF THE     00106000
*                                      LEFTMOST CHAR IN CNVREG3      */ 00107000
         SLL   CNVREG3,2                                           0176 00108000
*       SLDL(CNVREG2,6);            /* SHIFT THE REMAINING LOWER 6 0177 00109000
*                                      BITS INTO THE OTHER CNV REG   */ 00110000
         SLDL  CNVREG2,6                                           0177 00111000
*     END;                          /* END 4 CHAR SHIFT GROUP        */ 00112000
         AH    CNVCNT2,@CH00039                                    0178 00113000
         CH    CNVCNT2,@CH00045                                    0178 00114000
         BNH   @DL00175                                            0178 00115000
*     DLIB6WD(CNVCNT1)=CNVREG2;     /* STORE THE CONVERTED 4 CHAR IN    00116000
*                                      THE 6 CHAR CONVERSION AREA    */ 00117000
         LR    @10,CNVCNT1                                         0179 00118000
         SLA   @10,2                                               0179 00119000
         ST    CNVREG2,DLIB6WD-4(@10)                              0179 00120000
*     RFY                                                          0180 00121000
*      (CNVREG2,                                                   0180 00122000
*       CNVREG3) UNRSTD;            /*                       @PK00006*/ 00123000
*   END;                            /* END 2 WORD CONVERSION GROUP   */ 00124000
*                                                                  0181 00125000
         AH    CNVCNT1,@CH00039                                    0181 00126000
         CH    CNVCNT1,@CH00041                                    0181 00127000
         BNH   @DL00170                                            0181 00128000
*   /*****************************************************************/ 00129000
*   /*                                                               */ 00130000
*   /* SHIFT THE CONVERTED CHARACTERS INTO THE CORRECT SPOT IN THE   */ 00131000
*   /* RETURN AREA, SET THE SPECIAL CHARACTER INDICATING THAT THE    */ 00132000
*   /* NAME HAS BEEN PACKED, AND RETURN. THE NAME WILL BE IN THE     */ 00133000
*   /* SECOND THRU THE EIGTH CHAR OF THE WORK AREA                   */ 00134000
*   /*                                                               */ 00135000
*   /*****************************************************************/ 00136000
*                                                                  0182 00137000
*   DLB6AREA(3:5)=DLB6AREA(2:4);    /* SHIFT THE FIRST CONVERTED WORD   00138000
*                                      OVER 8 BITS SO THAT IT WILL BE   00139000
*                                      POSITIONED NEXT TO THE OTHER     00140000
*                                      CHAR                          */ 00141000
         MVC   @TS00001(3),DLB6AREA+1                              0182 00142000
         MVC   DLB6AREA+2(3),@TS00001                              0182 00143000
*   CNVPRM6=DLB6AREA(3:8);          /* SET RETURN NAME               */ 00144000
         L     @10,@PC00001                                        0183 00145000
         L     @10,SPLPMAD(,@10)                                   0183 00146000
         MVC   CNVPRM6(6,@10),DLB6AREA+2                           0183 00147000
*   RETURN CODE(SPLGOOD);           /*                       @Z67SP00*/ 00148000
         SLR   @15,@15                                             0184 00149000
         L     @14,12(,@13)                                        0184 00150000
         LM    @00,@12,20(@13)                                     0184 00151000
         BR    @14                                                 0184 00152000
*   END PACKSRTN;                   /* RETURN TO CALLING POINT       */ 00153000
         EJECT                                                          00154000
*                                                                  0186 00155000
*   /*****************************************************************/ 00156000
*   /*                                                               */ 00157000
*   /* ENTRY POINT TO UNPACK 6 CHARACTER DLIB OR SOURCE NAMES INTO 8 */ 00158000
*   /* CHARACTER RETURN AREA                                         */ 00159000
*   /*                                                               */ 00160000
*   /*****************************************************************/ 00161000
*                                                                  0186 00162000
*UNPKRTN:                           /* SMP 7 TO 8 CHAR DLIB UNPACK   */ 00163000
*   PROCEDURE;                      /* ROUTINE                       */ 00164000
UNPKRTN  STM   @14,@12,12(@13)                                     0186 00165000
*   DLB6AREA(3:8)=CNVPRM6;          /* MOVE 7 CHAR DLIB NAME         */ 00166000
*                                                                  0187 00167000
         L     @10,@PC00001                                        0187 00168000
         L     @10,SPLPMAD(,@10)                                   0187 00169000
         MVC   DLB6AREA+2(6),CNVPRM6(@10)                          0187 00170000
*   /*****************************************************************/ 00171000
*   /*                                                               */ 00172000
*   /* THE NAME IS IN PACKED FORMAT AND MUST THEREFORE BE UNPACKED.  */ 00173000
*   /* PROCESS THREE BYTES AT A TIME, REPRESENTING FOUR CHARACTERS.  */ 00174000
*   /*                                                               */ 00175000
*   /*****************************************************************/ 00176000
*                                                                  0188 00177000
*   DLB6AREA(2:4)=DLB6AREA(3:5);    /* SHIFT TO ALIGN FOR LOAD       */ 00178000
         MVC   @TS00001(3),DLB6AREA+2                              0188 00179000
         MVC   DLB6AREA+1(3),@TS00001                              0188 00180000
*   DO CNVCNT1=1 TO DIM(DLIB6WD);   /* DO 2 WORD OF CHAR             */ 00181000
         LA    CNVCNT1,1                                           0189 00182000
@DL00189 DS    0H                                                  0190 00183000
*     RFY                                                          0190 00184000
*      (CNVREG2,                                                   0190 00185000
*       CNVREG3) RSTD;              /*                       @PK00006*/ 00186000
*     CNVREG3=DLIB6WD(CNVCNT1);     /* PICK UP WORD OF PACKED BITS   */ 00187000
         LR    @10,CNVCNT1                                         0191 00188000
         SLA   @10,2                                               0191 00189000
         L     CNVREG3,DLIB6WD-4(@10)                              0191 00190000
*     SLL(CNVREG3,8);               /* SHIFT OUT HIGH ORDER BYTE     */ 00191000
*                                                                  0192 00192000
         SLL   CNVREG3,8                                           0192 00193000
*     /***************************************************************/ 00194000
*     /*                                                             */ 00195000
*     /* LOOP THRU SHIFTING 6 BITS AT A TIME INTO THE ADJECENT REG   */ 00196000
*     /*                                                             */ 00197000
*     /***************************************************************/ 00198000
*                                                                  0193 00199000
*     DO CNVCNT2=1 TO LENGTH(DLIB8CH);/* DO 4 CHAR PER WORD          */ 00200000
         LA    CNVCNT2,1                                           0193 00201000
@DL00193 DS    0H                                                  0194 00202000
*       CNVREG2=0;                  /* INITIALIZE REG TO HOLD CHAR   */ 00203000
         SLR   CNVREG2,CNVREG2                                     0194 00204000
*       SLDL(CNVREG2,6);            /* SHIFT 6 BITS TO CNV REG THE 0195 00205000
*                                      REGISTER                      */ 00206000
         SLDL  CNVREG2,6                                           0195 00207000
*       DLIB8CH(CNVCNT1,CNVCNT2)=CNVREG2;/* STORE CHAR IN CONVERTED     00208000
*                                      AREA                          */ 00209000
         LR    @10,CNVCNT1                                         0196 00210000
         SLA   @10,2                                               0196 00211000
         ALR   @10,CNVCNT2                                         0196 00212000
         STC   CNVREG2,DLIB8CH-5(@10)                              0196 00213000
*     END;                          /* END 4 CHAR GROUP              */ 00214000
         AH    CNVCNT2,@CH00039                                    0197 00215000
         CH    CNVCNT2,@CH00045                                    0197 00216000
         BNH   @DL00193                                            0197 00217000
*     RFY                                                          0198 00218000
*      (CNVREG2,                                                   0198 00219000
*       CNVREG3) UNRSTD;            /*                       @PK00006*/ 00220000
*   END;                            /* END 2 WORD GROUP              */ 00221000
         AH    CNVCNT1,@CH00039                                    0199 00222000
         CH    CNVCNT1,@CH00041                                    0199 00223000
         BNH   @DL00189                                            0199 00224000
*   TR(CNVDLIB8,TRTABLE2);          /* CONVERT TO APLH-NUMER @Z67SP00*/ 00225000
         TR    CNVDLIB8(8),TRTABLE2                                0200 00226000
*   CNVPRM8=CNVDLIB8;               /* RETURN CONVERTED NAME         */ 00227000
         L     @10,@PC00001                                        0201 00228000
         L     @10,SPLPMAD(,@10)                                   0201 00229000
         MVC   CNVPRM8(8,@10),CNVDLIB8                             0201 00230000
*   RFY                                                            0202 00231000
*    (CNVREG2,                                                     0202 00232000
*     CNVREG3) UNRSTD;                                             0202 00233000
*   RETURN CODE(SPLGOOD);           /*                       @Z67SP00*/ 00234000
         SLR   @15,@15                                             0203 00235000
         L     @14,12(,@13)                                        0203 00236000
         LM    @00,@12,20(@13)                                     0203 00237000
         BR    @14                                                 0203 00238000
*   END UNPKRTN;                    /* RETURN TO CALLER              */ 00239000
         EJECT                                                          00240000
*                                                                  0205 00241000
*   /*****************************************************************/ 00242000
*   /*                                                               */ 00243000
*   /* GETMAIN IOP SUBROTINE                                 @Z67SP00*/ 00244000
*   /*                                                               */ 00245000
