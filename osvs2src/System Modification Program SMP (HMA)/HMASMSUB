*                                                                  0241 00360000
*FMIOPRTN:                          /*                       @Z67SP00*/ 00361000
*   PROCEDURE;                      /*                       @Z67SP00*/ 00362000
FMIOPRTN STM   @14,@12,@SA00005                                    0241 00363000
*   GMADDR=SPLPMAD;                 /* ADDR OF IOP TO FREE   @Z67SP00*/ 00364000
         L     @14,@PC00001                                        0242 00365000
         MVC   GMADDR(4),SPLPMAD(@14)                              0242 00366000
*   RFY                                                            0243 00367000
*     SIZE RSTD;                    /*                       @Z67SP00*/ 00368000
*   SIZE=CCAPESIZ+LENGTH(HMASMIOP); /* IOP SIZE TO FREE      @Z67SP00*/ 00369000
         L     @14,@CA00071                                        0244 00370000
         LA    SIZE,68                                             0244 00371000
         AL    SIZE,CCAPESIZ-HMASMCCA(,@14)                        0244 00372000
*   GEN(FREEMAIN E,LV=(SIZE),A=GMADDR)                             0245 00373000
*   REFS(SIZE,GMADDR);              /*                       @Z67SP00*/ 00374000
         FREEMAIN E,LV=(SIZE),A=GMADDR                                  00375000
*   RFY                                                            0246 00376000
