*                                                                  0205 00247000
*GMIOPRTN:                          /*                       @Z67SP00*/ 00248000
*   PROCEDURE;                      /*                       @Z67SP00*/ 00249000
GMIOPRTN STM   @14,@12,@SA00004                                    0205 00250000
*   DECLARE                         /*                       @Z67SP00*/ 00251000
*     GMRTNCD FIXED(8);             /*                       @Z67SP00*/ 00252000
*   GMRTNCD=SPLGOOD;                /*                       @Z67SP00*/ 00253000
         SLR   GMRTNCD,GMRTNCD                                     0207 00254000
*   RFY                                                            0208 00255000
*     SIZE RSTD;                    /*                       @Z67SP00*/ 00256000
*   SIZE=LENGTH(IOPPTSYS)           /* LEN OF FIXED SIZE OF LARGEST     00257000
*                                      ENTRY IN ANY DATASET  @Z67SP00*/ 00258000
*       +LENGTH(IOPPNTRY)*(CCAPEMAX+1)/* VARIABLE SIZE OF    @Z67SP00*/ 00259000
*       +1;                         /* FOR EOLST             @Z67SP00*/ 00260000
         L     @10,@CA00071                                        0209 00261000
         LA    SIZE,1                                              0209 00262000
         AH    SIZE,CCAPEMAX-HMASMCCA(,@10)                        0209 00263000
         MH    SIZE,@CH00455                                       0209 00264000
         AH    SIZE,@CH01376                                       0209 00265000
*   REMAINDR=(SIZE-LENGTH(IOPDIRCT))//LRECL;/* ROUND TO 80   @Z67SP00*/ 00266000
         LA    @10,80                                              0210 00267000
         LR    @00,SIZE                                            0210 00268000
         SH    @00,@CH00191                                        0210 00269000
         SRDA  @00,32                                              0210 00270000
         DR    @00,@10                                             0210 00271000
         LR    REMAINDR,@00                                        0210 00272000
*   IF REMAINDR^=ZERO               /* 80 IF NOT ALREADY     @Z67SP00*/ 00273000
*     THEN                          /*                       @Z67SP00*/ 00274000
         LTR   REMAINDR,REMAINDR                                   0211 00275000
         BZ    @RF00211                                            0211 00276000
*     REMAINDR=LRECL-REMAINDR;      /*                       @Z67SP00*/ 00277000
         SLR   @10,REMAINDR                                        0212 00278000
         LR    REMAINDR,@10                                        0212 00279000
*   SIZE=SIZE+REMAINDR;             /*                       @Z67SP00*/ 00280000
@RF00211 ALR   SIZE,REMAINDR                                       0213 00281000
*   SIZE=SIZE+LENGTH(HMASMIOP);     /*                       @Z67SP00*/ 00282000
         AH    SIZE,@CH01052                                       0214 00283000
*   GEN(GETMAIN  EC,LV=(SIZE),A=GMADDR)                            0215 00284000
*   REFS(SIZE,GMADDR);              /*                       @Z67SP00*/ 00285000
         GETMAIN  EC,LV=(SIZE),A=GMADDR                                 00286000
*   RFY                                                            0216 00287000
*     RTNCODE RSTD;                 /*                       @Z67SP00*/ 00288000
*   IF RTNCODE=ZERO                 /*                       @Z67SP00*/ 00289000
*     THEN                          /*                       @Z67SP00*/ 00290000
         LTR   RTNCODE,RTNCODE                                     0217 00291000
         BNZ   @RF00217                                            0217 00292000
*     DO;                           /*                       @Z67SP00*/ 00293000
*       CCAPESIZ=SIZE-LENGTH(HMASMIOP);/*                    @Z67SP00*/ 00294000
         L     @14,@CA00071                                        0219 00295000
         LR    @10,SIZE                                            0219 00296000
         SH    @10,@CH01052                                        0219 00297000
         ST    @10,CCAPESIZ-HMASMCCA(,@14)                         0219 00298000
*       SPLPMAD=GMADDR;             /* RETURN ADDR           @Z67SP00*/ 00299000
         L     @14,@PC00001                                        0220 00300000
         MVC   SPLPMAD(4,@14),GMADDR                               0220 00301000
*     END;                          /*                       @Z67SP00*/ 00302000
*   ELSE                            /*                       @Z67SP00*/ 00303000
*     DO;                           /*                       @Z67SP00*/ 00304000
         B     @RC00217                                            0222 00305000
@RF00217 DS    0H                                                  0223 00306000
*       HMASMMGP=''B;               /* ZERO MSG PARM         @Z67SP00*/ 00307000
         XC    HMASMMGP(16),HMASMMGP                               0223 00308000
*       MGPPRINT=ON;                /*                       @Z67SP00*/ 00309000
*       MGPHLDS=ON;                 /*                       @Z67SP00*/ 00310000
         OI    MGPPRINT,B'10010000'                                0225 00311000
*       MGPMGNO1=HMA202I;           /* UNABL TO OBTAIN STORAGE FOR 0226 00312000
*                                      WORK AREAS            @Z67SP00*/ 00313000
         MVC   MGPMGNO1(2),@CH00043                                0226 00314000
*       IF CCAUPDU=ON               /* IF IN UCLIN PROCESS   @Z67SP00*/ 00315000
*           |CCAFLAG1=ZERO          /* OR IN INITIALIZATION  @Z67SP00*/ 00316000
*         THEN                      /*                       @Z67SP00*/ 00317000
         L     @14,@CA00071                                        0227 00318000
         TM    CCAUPDU-HMASMCCA(@14),B'00000010'                   0227 00319000
         BO    @RT00227                                            0227 00320000
         ICM   @10,7,CCAFLAG1-HMASMCCA(@14)                        0227 00321000
         BNZ   @RF00227                                            0227 00322000
@RT00227 DS    0H                                                  0228 00323000
*         DO;                       /*                       @Z67SP00*/ 00324000
*           GMRTNCD=SPLTSMP;        /* TERMINATE SMP         @Z67SP00*/ 00325000
         LA    GMRTNCD,16                                          0229 00326000
*           MGPTYPE=MGPTERM;        /*                       @Z67SP00*/ 00327000
         MVI   MGPTYPE,X'05'                                       0230 00328000
*         END;                      /*                       @Z67SP00*/ 00329000
*       ELSE                        /*                       @Z67SP00*/ 00330000
*         DO;                       /*                       @Z67SP00*/ 00331000
         B     @RC00227                                            0232 00332000
@RF00227 DS    0H                                                  0233 00333000
*           GMRTNCD=SPLSPACE;       /* TERMINATE FUNCTION    @Z67SP00*/ 00334000
         LA    GMRTNCD,12                                          0233 00335000
*           MGPTYPE=MGPSEV;         /*                       @Z67SP00*/ 00336000
         MVI   MGPTYPE,X'04'                                       0234 00337000
*         END;                      /*                       @Z67SP00*/ 00338000
*       CALL HMASMMSG(HMASMMGP);    /*                       @Z67SP00*/ 00339000
@RC00227 L     @15,@CV01263                                        0236 00340000
         LA    @01,@AL00236                                        0236 00341000
         BALR  @14,@15                                             0236 00342000
*     END;                          /*                       @Z67SP00*/ 00343000
*   RFY                                                            0238 00344000
*     SIZE UNRSTD;                  /*                       @Z67SP00*/ 00345000
@RC00217 DS    0H                                                  0239 00346000
*   RETURN CODE(GMRTNCD);           /*                       @Z67SP00*/ 00347000
         LR    @15,GMRTNCD                                         0239 00348000
         L     @14,@SA00004                                        0239 00349000
         LM    @00,@12,@SA00004+8                                  0239 00350000
         BR    @14                                                 0239 00351000
*   END GMIOPRTN;                   /*                       @Z67SP00*/ 00352000
         EJECT                                                          00353000
*                                                                  0241 00354000
*   /*****************************************************************/ 00355000
*   /*                                                               */ 00356000
*   /* FREEMAIN IOP ROUTINE                                  @Z67SP00*/ 00357000
