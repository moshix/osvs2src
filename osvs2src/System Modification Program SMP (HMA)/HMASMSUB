         LM    @00,@12,@SA00005+8                                  0247 00381000
         BR    @14                                                 0247 00382000
*   END FMIOPRTN;                   /*                       @Z67SP00*/ 00383000
         EJECT                                                          00384000
*                                                                  0249 00385000
*   /*****************************************************************/ 00386000
*   /*                                                               */ 00387000
*   /* ROUTINE TO CONSTRUCT THE RELFILE DSN FROM THE THREE PARTS THAT*/ 00388000
*   /* DETERMINE NAME                                        @Z67SP00*/ 00389000
*   /*                                                               */ 00390000
*   /*****************************************************************/ 00391000
*                                                                  0249 00392000
*BDDSNRTN:                          /*                       @Z67SP00*/ 00393000
*   PROCEDURE;                      /*                       @Z67SP00*/ 00394000
BDDSNRTN STM   @14,@12,12(@13)                                     0249 00395000
*   DECLARE                         /*                       @Z67SP00*/ 00396000
*     SAVPFX CHAR(26),              /* SAVE PREFIX AREA      @Z67SP00*/ 00397000
*     SAVSMD CHAR(7),               /* SAVE SYSMOD AREA      @Z67SP00*/ 00398000
*     SAVFIL FIXED(15);             /* SAVE FILE AREA        @Z67SP00*/ 00399000
*   DECLARE                         /*                       @Z67SP00*/ 00400000
*     DSNX FIXED(31);               /* LOOP CONTROL          @Z67SP00*/ 00401000
*   DECLARE                         /*                       @Z67SP00*/ 00402000
*     DBLWORD CHAR(8) BDY(DWORD),   /* FOR CVD               @Z67SP00*/ 00403000
*     FTEMP CHAR(4) BDY(WORD);      /* FOR UNPACK            @Z67SP00*/ 00404000
*   DECLARE                         /*                       @Z67SP00*/ 00405000
*     THREE0 CHAR(3) CONSTANT('000'),/* FOR COMPARE          @Z67SP00*/ 00406000
