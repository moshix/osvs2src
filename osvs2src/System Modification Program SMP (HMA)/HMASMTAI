         TITLE 'HMASMTAI - ADD AN INDEX ENTRY TO TABLE'                 00001000
HMASMTAI CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMTAI  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(16),0(@01)                                 0001 00017000
*                                                            @Z67SP00*/ 00018000
*                                                                  0069 00019000
*   /*****************************************************************/ 00020000
*   /*                                                               */ 00021000
*   /* HMASMTAI << MAINLINE                                          */ 00022000
*   /*                                                               */ 00023000
*   /*****************************************************************/ 00024000
*                                                                  0069 00025000
*   TAIRC=ZERO;                     /* CLEAR THE RETURN CODE         */ 00026000
         SLR   TAIRC,TAIRC                                         0069 00027000
*   IF CHAIN=ZERO                   /* STARTING A NEW INDEX ENTRY?   */ 00028000
*       |CHAIN=TBLXNULL THEN        /* OR NULL ENTRY?                */ 00029000
         L     @15,@PC00001+4                                      0070 00030000
         L     @15,CHAIN(,@15)                                     0070 00031000
         LTR   @15,@15                                             0070 00032000
         BZ    @RT00070                                            0070 00033000
         L     @14,@PC00001                                        0070 00034000
         C     @15,TBLXNULL(,@14)                                  0070 00035000
         BNE   @RF00070                                            0070 00036000
@RT00070 DS    0H                                                  0071 00037000
*     DO;                           /* YES,                          */ 00038000
*       TBLXCUR=TBLXCUR+LENGTH(ICTENDCH);/* ADVANCE PAST END OF INDEX   00039000
*                                      INDICATOR                     */ 00040000
         L     @15,@PC00001                                        0072 00041000
         LA    @14,4                                               0072 00042000
         AL    @14,TBLXCUR(,@15)                                   0072 00043000
         ST    @14,TBLXCUR(,@15)                                   0072 00044000
*       CHAIN=TBLXCUR;              /* START A NEW INDEX LIST        */ 00045000
         L     @15,@PC00001+4                                      0073 00046000
         ST    @14,CHAIN(,@15)                                     0073 00047000
*     END;                          /*                               */ 00048000
*   ELSE                                                           0075 00049000
*     ;                             /* ADDING TO AN EXISTING ENTRY   */ 00050000
@RF00070 DS    0H                                                  0076 00051000
*   IXLEN=((DATALEN+3)/4)*4;        /* LENGTH OF ENTRY IS DATA LENGTH   00052000
*                                      ROUNDED TO A WORD BOUNDRY     */ 00053000
         LA    @15,4                                               0076 00054000
         LA    @14,3                                               0076 00055000
         L     @09,@PC00001+12                                     0076 00056000
         L     @00,DATALEN(,@09)                                   0076 00057000
         ALR   @00,@14                                             0076 00058000
         SRDA  @00,32                                              0076 00059000
         DR    @00,@15                                             0076 00060000
         SLA   @01,2                                               0076 00061000
         LR    IXLEN,@01                                           0076 00062000
*   IXPTR=TBLXCUR;                  /* ADDRESS OF ENTRY IN INDEX     */ 00063000
         L     @09,@PC00001                                        0077 00064000
         L     IXPTR,TBLXCUR(,@09)                                 0077 00065000
*   NEWCUR=IXPTR+IXLEN;             /* NEXT AVAILABLE ENTRY          */ 00066000
         LR    NEWCUR,IXPTR                                        0078 00067000
         ALR   NEWCUR,IXLEN                                        0078 00068000
*   IF NEWCUR+LENGTH(ICTENDCH)=>TBLXEND THEN/* WILL ADDING THIS ENTRY   00069000
*                                      OVERFLOW THE END OF INDEX     */ 00070000
         LR    @05,NEWCUR                                          0079 00071000
         ALR   @05,@15                                             0079 00072000
         C     @05,TBLXEND(,@09)                                   0079 00073000
         BL    @RF00079                                            0079 00074000
*     DO;                           /*                               */ 00075000
*       NEWSTRT=TBLXSTRT-512;       /* BACK-UP START OF INDEX        */ 00076000
         L     @05,@PC00001                                        0081 00077000
         L     NEWSTRT,TBLXSTRT(,@05)                              0081 00078000
         SH    NEWSTRT,@CH00922                                    0081 00079000
*       IF NEWSTRT<TBLXTOP+LENGTH(ICTENDCH) THEN/* WILL BACKING UP 0082 00080000
*                                      OVERRUN THE FIXED PORTION OF     00081000
*                                      THE ICT?                      */ 00082000
         AL    @15,TBLXTOP(,@05)                                   0082 00083000
         CR    NEWSTRT,@15                                         0082 00084000
         BNL   @RF00082                                            0082 00085000
*         DO;                       /* YES,                          */ 00086000
*           TAIRC=STOPFUNC;         /* STOP THE FUNCTION             */ 00087000
*                                                                  0084 00088000
         LA    TAIRC,12                                            0084 00089000
*           /*********************************************************/ 00090000
*           /*                                                       */ 00091000
*           /* ISSUE MESSAGE HMA202I                                 */ 00092000
*           /*                                                       */ 00093000
*           /*********************************************************/ 00094000
*                                                                  0085 00095000
*           DO;                     /* HMASMPMG M1(HMA202I)TYPE(SEV)T   00096000
*                                      ARGET(SYSOUT,LOG)             */ 00097000
*             HMASMMGP=HMASMMGP&&HMASMMGP;/* SET PARMS TO ZEROS      */ 00098000
         XC    HMASMMGP(24),HMASMMGP                               0086 00099000
*             MGPMGNO1=HMA202I;     /* SET UP MSG 1 CODE             */ 00100000
         STH   @14,MGPMGNO1                                        0087 00101000
*             MGPTYPE=MGPSEV;       /* SET MESSAGE TYPE              */ 00102000
         MVI   MGPTYPE,X'04'                                       0088 00103000
*             MGPPRINT='1'B;        /* SET TARGET FLAG               */ 00104000
*             MGPHLDS='1'B;         /* SET TARGET FLAG               */ 00105000
         OI    MGPPRINT,B'10010000'                                0090 00106000
*             CALL HMASMMSG(HMASMMGP);/* HAVE MESSAGE ISSUED         */ 00107000
         L     @15,@CV00903                                        0091 00108000
         LA    @01,@AL00091                                        0091 00109000
         BALR  @14,@15                                             0091 00110000
*           END;                                                   0092 00111000
*         END;                                                     0093 00112000
*       ELSE                        /* CAN BACK-UP                   */ 00113000
*         DO;                                                      0094 00114000
         B     @RC00082                                            0094 00115000
@RF00082 DS    0H                                                  0095 00116000
*           NEWEND=TBLXEND;         /* ASSUME END DOES NOT MOVE      */ 00117000
         L     @15,@PC00001                                        0095 00118000
         L     NEWEND,TBLXEND(,@15)                                0095 00119000
*           IF CHAIN^=TBLXSTRT THEN /* BACKED UP BEFORE FOR THIS   0096 00120000
*                                      INDEX LIST?                   */ 00121000
         L     @06,@PC00001+4                                      0096 00122000
         L     @15,TBLXSTRT(,@15)                                  0096 00123000
         C     @15,CHAIN(,@06)                                     0096 00124000
         BE    @RF00096                                            0096 00125000
*             DO;                   /* NO,                           */ 00126000
*               NEWEND=TBLXSTRT;    /* NEW END IS OLD START          */ 00127000
         LR    NEWEND,@15                                          0098 00128000
*             END;                                                 0099 00129000
*           ELSE                                                   0100 00130000
*             ;                     /*                               */ 00131000
*                                                                  0100 00132000
@RF00096 DS    0H                                                  0101 00133000
*           /*********************************************************/ 00134000
*           /*                                                       */ 00135000
*           /* MOVE INDEX LIST DOWN                                  */ 00136000
*           /*                                                       */ 00137000
*           /*********************************************************/ 00138000
*                                                                  0101 00139000
*           RFY                                                    0101 00140000
*            (R0,                                                  0101 00141000
*             R1,                                                  0101 00142000
*             R14,                                                 0101 00143000
*             R15) RSTD;            /*                               */ 00144000
*           R1=TBLXCUR-CHAIN;       /* TO LENGTH                     */ 00145000
         L     @05,@PC00001                                        0102 00146000
         L     @04,@PC00001+4                                      0102 00147000
         L     @04,CHAIN(,@04)                                     0102 00148000
         L     R1,TBLXCUR(,@05)                                    0102 00149000
         SLR   R1,@04                                              0102 00150000
*           R0=NEWSTRT;             /* TO START                      */ 00151000
         LR    R0,NEWSTRT                                          0103 00152000
*           R14=CHAIN;              /* FROM ADDRESS                  */ 00153000
         LR    R14,@04                                             0104 00154000
*           R15=R1;                 /* FROM LENGTH                   */ 00155000
         LR    R15,R1                                              0105 00156000
*           IXPTR=NEWSTRT+R1;       /* BACK UP CURRENT PTR           */ 00157000
         LR    IXPTR,NEWSTRT                                       0106 00158000
         ALR   IXPTR,R1                                            0106 00159000
*           MVCL(R0,R14);                                          0107 00160000
         MVCL  R0,R14                                              0107 00161000
*           RFY                                                    0108 00162000
*            (R0,                                                  0108 00163000
*             R1,                                                  0108 00164000
*             R14,                                                 0108 00165000
*             R15) UNRSTD;          /*                               */ 00166000
*           TBLXSTRT=NEWSTRT;       /* RESET TBLX PTRS               */ 00167000
         L     @15,@PC00001                                        0109 00168000
         ST    NEWSTRT,TBLXSTRT(,@15)                              0109 00169000
*           TBLXEND=NEWEND;         /* ...                           */ 00170000
         ST    NEWEND,TBLXEND(,@15)                                0110 00171000
*           NEWCUR=IXPTR+IXLEN;     /* NEW CURRENT LIST ENTRY        */ 00172000
         LR    NEWCUR,IXPTR                                        0111 00173000
         ALR   NEWCUR,IXLEN                                        0111 00174000
*           CHAIN=NEWSTRT;          /* RESET INDEX LIST POINTER      */ 00175000
         L     @15,@PC00001+4                                      0112 00176000
         ST    NEWSTRT,CHAIN(,@15)                                 0112 00177000
*         END;                                                     0113 00178000
*     END;                                                         0114 00179000
*   ELSE                                                           0115 00180000
*     ;                             /* CAN ADD ENTRY WITH OUT      0115 00181000
*                                      OVERFLOW                      */ 00182000
@RF00079 DS    0H                                                  0116 00183000
*   IF TAIRC=ZERO THEN              /* NO ERROR SO FAR               */ 00184000
@RC00079 LTR   TAIRC,TAIRC                                         0116 00185000
         BNZ   @RF00116                                            0116 00186000
*     DO;                           /* YES,                          */ 00187000
*                                                                  0117 00188000
*       /*************************************************************/ 00189000
*       /*                                                           */ 00190000
*       /* PUT ENTRY IN INDEX LIST                                   */ 00191000
*       /*                                                           */ 00192000
*       /*************************************************************/ 00193000
*                                                                  0118 00194000
*       RFY                                                        0118 00195000
*         IXENT BASED(IXPTR);                                      0118 00196000
*       IXENT(1:IXLEN)=             /* CLEAR INDEX ENTRY             */ 00197000
*           IXENT(1:IXLEN)&&IXENT(1:IXLEN);/*                        */ 00198000
         LR    @09,IXLEN                                           0119 00199000
         BCTR  @09,0                                               0119 00200000
         EX    @09,@SX00929                                        0119 00201000
*       IXENT(1:DATALEN)=DATAITEM(1:DATALEN);/* SET ENTRY            */ 00202000
         L     @15,@PC00001+12                                     0120 00203000
         L     @15,DATALEN(,@15)                                   0120 00204000
         BCTR  @15,0                                               0120 00205000
         L     @14,@PC00001+8                                      0120 00206000
         L     @01,DATAPTR(,@14)                                   0120 00207000
         EX    @15,@SM00931                                        0120 00208000
*       RFY                                                        0121 00209000
*         IXENT BASED;                                             0121 00210000
*       RFY                                                        0122 00211000
*         IXENT BASED(NEWCUR);                                     0122 00212000
*       IXENT(1:LENGTH(ICTENDCH))=ICTENDCH;/* SET END OF INDEX LIST  */ 00213000
         MVC   IXENT(4,NEWCUR),ICTENDCH                            0123 00214000
*       RFY                                                        0124 00215000
*         IXENT BASED;                                             0124 00216000
*       TBLXCUR=NEWCUR;             /* NEXT INDEX ENTRY PTR          */ 00217000
         L     @06,@PC00001                                        0125 00218000
         ST    NEWCUR,TBLXCUR(,@06)                                0125 00219000
*     END;                                                         0126 00220000
*   ELSE                                                           0127 00221000
*     ;                             /* AN ERROR OCCURRED             */ 00222000
@RF00116 DS    0H                                                  0128 00223000
*   RETURN CODE(TAIRC);             /*                               */ 00224000
         LR    @15,TAIRC                                           0128 00225000
         L     @13,4(,@13)                                         0128 00226000
         L     @14,12(,@13)                                        0128 00227000
         LM    @00,@12,20(@13)                                     0128 00228000
         BR    @14                                                 0128 00229000
*   END HMASMTAI                    /*                               */ 00230000
