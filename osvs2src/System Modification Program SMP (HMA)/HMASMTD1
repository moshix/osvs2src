*             DELENT@->ICTPDEXP(1)=ON;/* MARK IT EXPLICITLY DELETED  */ 00139000
         L     @10,DELENT@                                         0230 00140000
         OI    ICTPDEXP(@10),B'00100000'                           0230 00141000
*             DELENT@->ICTPUSR2(1)=ON;/* DELETED "THIS PASS"         */ 00142000
         OI    ICTPUSR2(@10),B'01000000'                           0231 00143000
*           END;                                                   0232 00144000
*       END;                                                       0233 00145000
*     ELSE                                                         0234 00146000
*       DO;                                                        0234 00147000
*                                                                  0234 00148000
         B     @RC00224                                            0234 00149000
@RF00224 DS    0H                                                  0235 00150000
*         /***********************************************************/ 00151000
*         /*                                                         */ 00152000
*         /* ICT PTF SECTION ENTRY ALREADY EXISTS FOR SYSMOD         */ 00153000
*         /*                                                         */ 00154000
*         /***********************************************************/ 00155000
*                                                                  0235 00156000
*         DELENT@->ICTPDIMP(1)=OFF; /* MAKE SURE IMPLICIT DELETE IS     00157000
*                                      NOT ON                        */ 00158000
*         DELENT@->ICTPDEXP(1)=ON;  /* MAKE SURE EXPLICIT DELETE IS     00159000
*                                      ON                            */ 00160000
         L     @10,DELENT@                                         0236 00161000
         OI    ICTPDEXP(@10),B'00100000'                           0236 00162000
         NI    ICTPDIMP(@10),B'11101111'                           0236 00163000
*         DELENT@->ICTPUSR2(1)=ON;  /* DELETED "THIS PASS"           */ 00164000
         OI    ICTPUSR2(@10),B'01000000'                           0237 00165000
*       END;                                                       0238 00166000
*   END;                            /* LOOP THRU DELETE LIST         */ 00167000
*                                                                  0239 00168000
@RC00224 AH    II,@CH00065                                         0239 00169000
@DE00222 LR    @10,II                                              0239 00170000
