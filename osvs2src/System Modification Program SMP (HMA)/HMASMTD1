         LA    @01,@AL00001                                        0226 00129000
         BAL   @14,BLDICTP                                         0226 00130000
*         IF HIGHTD1<STOPPTF        /* ENTRY SUCCESSFULLY BUILT      */ 00131000
*           THEN                                                   0227 00132000
         L     @10,HIGHTD1                                         0227 00133000
         CH    @10,@CH00057                                        0227 00134000
         BNL   @RF00227                                            0227 00135000
*           DO;                                                    0228 00136000
*             ADDED=YES;            /* A SYSMOD WAS ADDED TO ICT     */ 00137000
