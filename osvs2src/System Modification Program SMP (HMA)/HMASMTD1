         BNE   @DL00213                                            0215 00059000
*   ADDED=NO;                       /* NO SYSMODS ADDED TO ICT       */ 00060000
         MVI   ADDED,C'N'                                          0216 00061000
*   IMPADD=NO;                      /* NO IMPLICITLY DELETED SYSMODS    00062000
*                                      TO ADD TO DELETE LIST         */ 00063000
         MVI   IMPADD,C'N'                                         0217 00064000
*   RESPECIFY                                                      0218 00065000
*     ICTPTF BASED(SYSMOD@);        /* ADDRESS PTF SECT ENTRY PASSED */ 00066000
*   RESPECIFY                                                      0219 00067000
*     ICTDELLS BASED(ICTPDCHN(1));  /* ADDRESS DELETE LIST FOR PTF 0219 00068000
*                                      SECT ENTRY PASSED             */ 00069000
