         L     @10,@PC00001+4                                      0158 00248000
         L     @10,SMDICTP(,@10)                                   0158 00249000
         LA    @10,ICTPRCHN(,@10)                                  0158 00250000
         ST    @10,@AL00001+8                                      0158 00251000
         LA    @01,@AL00001                                        0158 00252000
         BAL   @14,ENTCHAIN                                        0158 00253000
*               RFY                                                0159 00254000
*                 RTNCODE RSTD;                                    0159 00255000
*               TECRC=MAX(TECRC,RTNCODE);/* SAVE THE RETURN CODE     */ 00256000
         CR    TECRC,RTNCODE                                       0160 00257000
