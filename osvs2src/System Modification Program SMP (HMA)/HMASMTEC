*           /*                                                       */ 00235000
*           /*********************************************************/ 00236000
*                                                                  0156 00237000
*           IF TECRC<STOPPTF THEN   /* NON FAILING SYSMOD?           */ 00238000
@RF00144 CH    TECRC,@CH00059                                      0156 00239000
         BNL   @RF00156                                            0156 00240000
*             DO;                   /* YES,                          */ 00241000
*               CALL ENTCHAIN(IOPPREQ,IOPPTFVR,ICTPRCHN(1),/*        */ 00242000
*                   ZERO,LCLVERNO,ZERO);                           0158 00243000
