         TITLE 'HMASMTEC - BUILD ICT ENTRY FROM SYSMOD IOP'             00001000
HMASMTEC CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMTEC  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(16),0(@01)                                 0001 00017000
*   TECRC=ZERO;                     /* CLEAR THE RETURN CODE         */ 00018000
         SLR   TECRC,TECRC                                         0096 00019000
*   TECFLAGS=''B;                   /* CLEAR LOCAL CONTROL FLAGS     */ 00020000
         MVI   TECFLAGS,X'00'                                      0097 00021000
*   SYSMOD8=' ';                    /* CLEAR SYSMOD NAME             */ 00022000
         MVI   SYSMOD8+1,C' '                                      0098 00023000
         MVC   SYSMOD8+2(6),SYSMOD8+1                              0098 00024000
         MVI   SYSMOD8,C' '                                        0098 00025000
*   SYSMOD=IOPNAM17;                /* SAVE THE SYSMOD NAME          */ 00026000
         L     @10,@PC00001+8                                      0099 00027000
         MVC   SYSMOD(7),IOPNAM17(@10)                             0099 00028000
*   LCLVERNO=VERNDX;                /* SET THE LOCAL ++VER NO FROM 0100 00029000
*                                      PARM LIST                     */ 00030000
         L     @02,@PC00001+12                                     0100 00031000
         MVC   LCLVERNO(1),VERNDX(@02)                             0100 00032000
*   IF IOPDSID=IOPPTS               /* IOP IS FROM PTS ...           */ 00033000
*       &LCLVERNO=ZERO THEN         /* ... BUT NO VER NUMBER PASSED? */ 00034000
         CLI   IOPDSID(@10),6                                      0101 00035000
         BNE   @RF00101                                            0101 00036000
         CLI   LCLVERNO,0                                          0101 00037000
         BNE   @RF00101                                            0101 00038000
*     DO;                           /* YES,                          */ 00039000
*       SUPENTRY=ON;                /* SUP'ED ENTRY IS BEING BUILT   */ 00040000
         OI    SUPENTRY,B'00100000'                                0103 00041000
*     END;                                                         0104 00042000
*                                                                  0104 00043000
*   /*****************************************************************/ 00044000
*   /*                                                               */ 00045000
*   /* INITIALIZE ICT PTF ENTRY IF NONE WAS SUPPLIED BY CALLER       */ 00046000
*   /*                                                               */ 00047000
*   /*****************************************************************/ 00048000
*                                                                  0105 00049000
*   IF TECRC=ZERO THEN              /* ALL IS WELL?                  */ 00050000
@RF00101 LTR   TECRC,TECRC                                         0105 00051000
         BNZ   @RF00105                                            0105 00052000
*     DO;                           /* YES,                          */ 00053000
*                                                                  0106 00054000
*       /*************************************************************/ 00055000
*       /*                                                           */ 00056000
*       /* COPY FIELDS FROM IOP TO THE ICT                           */ 00057000
*       /*                                                           */ 00058000
*       /*************************************************************/ 00059000
*                                                                  0107 00060000
*       ICTPRECD(1)=IOPRECD;        /* RECEIVE DATE                  */ 00061000
         L     @10,@PC00001+4                                      0107 00062000
         L     @10,SMDICTP(,@10)                                   0107 00063000
         L     @02,@PC00001+8                                      0107 00064000
         MVC   ICTPRECD(3,@10),IOPRECD(@02)                        0107 00065000
*       ICTPRECT(1)=IOPRECT;        /* RECEIVE TIME                  */ 00066000
         MVC   ICTPRECT(3,@10),IOPRECT(@02)                        0108 00067000
*       ICTPFLG1(1)=IOPPTFF1;       /* COPY IOP STATE FLAGS          */ 00068000
         MVC   ICTPFLG1(1,@10),IOPPTFF1(@02)                       0109 00069000
*       ICTPFLG5(1)=IOPPTFF2;       /* COPY IOP FLAGS 2              */ 00070000
         MVC   ICTPFLG5(2,@10),IOPPTFF2(@02)                       0110 00071000
*       ICTPFLG6(1)=IOPPTFF3;       /* COPY IOP FLAGS 3              */ 00072000
         MVC   ICTPFLG6(1,@10),IOPPTFF3(@02)                       0111 00073000
*       ICTPVERN(1)=LCLVERNO;       /* SET THE ++VER NUMBER          */ 00074000
         MVC   ICTPVERN(1,@10),LCLVERNO                            0112 00075000
*       ICTPTPFX(1)=IOPPTPFX;       /* COPY THE RELFILE DSN PREFIX   */ 00076000
         MVC   ICTPTPFX(26,@10),IOPPTPFX(@02)                      0113 00077000
*       ICTPRELN(1)=IOPPRELN;       /* COPY THE RELFILE DSN PREFIX   */ 00078000
*                                                                  0114 00079000
         MVC   ICTPRELN(2,@10),IOPPRELN(@02)                       0114 00080000
*       /*************************************************************/ 00081000
*       /*                                                           */ 00082000
*       /* SET SYSMOD TYPE AND FMID OWNERSHIP                        */ 00083000
*       /*                                                           */ 00084000
*       /*************************************************************/ 00085000
*                                                                  0115 00086000
*       ICTPTYPE(1)=IOPPTYPE;       /* GET THE TYPE FROM THE PTS   0115 00087000
*                                      SYSMOD ENTRY                  */ 00088000
         MVC   ICTPTYPE(1,@10),IOPPTYPE(@02)                       0115 00089000
*       IF IOPFLF=ON THEN           /* 1ST LEVEL FUNCTION?           */ 00090000
         TM    IOPFLF(@02),B'00100000'                             0116 00091000
         BNO   @RF00116                                            0116 00092000
*         DO;                       /* YES,                          */ 00093000
*           ICTFLF(1)=ON;           /* SYSMOD IS 1ST LEVEL FUNCTION  */ 00094000
         OI    ICTFLF(@10),B'00100000'                             0118 00095000
*           ICTPFMID(1)=IOPNAM17;   /* FMID IS ITSELF                */ 00096000
         MVC   ICTPFMID(7,@10),IOPNAM17(@02)                       0119 00097000
*         END;                                                     0120 00098000
*       IF SUPENTRY=OFF THEN        /* NOT A SUP'ED ENTRY?           */ 00099000
@RF00116 TM    SUPENTRY,B'00100000'                                0121 00100000
         BNZ   @RF00121                                            0121 00101000
*         DO;                       /* YES, THEN CAN DETERMINE FMID  */ 00102000
*           IF IOPDSID=IOPPTS THEN  /* SYSMOD ENTRY FROM PTS?        */ 00103000
         L     @10,@PC00001+8                                      0123 00104000
         CLI   IOPDSID(@10),6                                      0123 00105000
         BNE   @RF00123                                            0123 00106000
*             DO;                   /* YES,                          */ 00107000
*               FMIDFND=OFF;        /* FMID NOT YES FOUND            */ 00108000
         NI    FMIDFND,B'10111111'                                 0125 00109000
*               DO NDX1=1 BY 1      /* SEARCH SUB-ENTRIES FOR FMID   */ 00110000
*                     WHILE(IOPPMODS(NDX1,1)^=IOPEOLST/*             */ 00111000
*                     &FMIDFND=OFF);                               0126 00112000
         LA    NDX1,1                                              0126 00113000
         B     @DE00126                                            0126 00114000
@DL00126 DS    0H                                                  0127 00115000
*                 IF IOPPIND(NDX1)=IOPPFID/* FMID SUB-ENTRY          */ 00116000
*                     &IOPFIDST(NDX1)=LCLVERNO THEN/* AND FOR THE  0127 00117000
*                                      ++VER?                        */ 00118000
         LR    @10,NDX1                                            0127 00119000
         MH    @10,@CH00483                                        0127 00120000
         L     @15,@PC00001+8                                      0127 00121000
         SLR   @14,@14                                             0127 00122000
         IC    @14,IOPPIND-9(@10,@15)                              0127 00123000
         CH    @14,@CH00801                                        0127 00124000
         BNE   @RF00127                                            0127 00125000
         SLR   @14,@14                                             0127 00126000
         IC    @14,IOPFIDST-9(@10,@15)                             0127 00127000
         SLR   @09,@09                                             0127 00128000
         IC    @09,LCLVERNO                                        0127 00129000
         CR    @14,@09                                             0127 00130000
         BNE   @RF00127                                            0127 00131000
*                   DO;             /* YES,                          */ 00132000
*                     FMIDFND=ON;   /* FMID FOUND                    */ 00133000
         OI    FMIDFND,B'01000000'                                 0129 00134000
*                     ICTPFMID(1)=IOPFIDNM(NDX1);/* SET THE FMID     */ 00135000
         L     @14,@PC00001+4                                      0130 00136000
         L     @01,SMDICTP(,@14)                                   0130 00137000
         ALR   @15,@10                                             0130 00138000
         MVC   ICTPFMID(7,@01),IOPFIDNM-9(@15)                     0130 00139000
*                   END;                                           0131 00140000
*               END;                /* END OF FMID SEARCH LOOP       */ 00141000
@RF00127 AH    NDX1,@CH00067                                       0132 00142000
@DE00126 LR    @10,NDX1                                            0132 00143000
         MH    @10,@CH00483                                        0132 00144000
         L     @15,@PC00001+8                                      0132 00145000
         SLR   @14,@14                                             0132 00146000
         IC    @14,IOPPMODS-9(@10,@15)                             0132 00147000
         CH    @14,@CH00616                                        0132 00148000
         BE    @DC00126                                            0132 00149000
         TM    FMIDFND,B'01000000'                                 0132 00150000
         BZ    @DL00126                                            0132 00151000
@DC00126 DS    0H                                                  0133 00152000
*               IF FMIDFND=OFF      /* FMID NOT FOUND ...            */ 00153000
*                   &CCAFMID^=BLANKS(1:LENGTH(CCAFMID))/* ... AND  0133 00154000
*                                      NULL FMID?                    */ 00155000
*                 THEN                                             0133 00156000
         TM    FMIDFND,B'01000000'                                 0133 00157000
         BNZ   @RF00133                                            0133 00158000
         CLC   CCAFMID(7,CCAPTR),BLANKS                            0133 00159000
         BE    @RF00133                                            0133 00160000
*                 DO;               /* YES,                          */ 00161000
*                   IF ICTPFMID(1)=BLANKS(1:LENGTH(ICTPFMID(1)))/* IS   00162000
*                                      FMID BLANK                    */ 00163000
*                     THEN                                         0135 00164000
         L     @10,@PC00001+4                                      0135 00165000
         L     @10,SMDICTP(,@10)                                   0135 00166000
         CLC   ICTPFMID(7,@10),BLANKS                              0135 00167000
         BNE   @RF00135                                            0135 00168000
*                     DO;           /* YES                           */ 00169000
*                       ICTPFMID(1)=CCAFMID;/* USE NULL FMID VALUE IN   00170000
*                                      ICT ENT                       */ 00171000
         MVC   ICTPFMID(7,@10),CCAFMID(CCAPTR)                     0137 00172000
*                     END;                                         0138 00173000
*                 END;                                             0139 00174000
*             END;                                                 0140 00175000
*           ELSE                    /* SYSMOD ENTRY FROM (A)CDS      */ 00176000
*             DO;                                                  0141 00177000
         B     @RC00123                                            0141 00178000
@RF00123 DS    0H                                                  0142 00179000
*               ICTPFMID(1)=IOPPFMID;/* COPY FMID TO THE ICT ENTRY   */ 00180000
         L     @10,@PC00001+4                                      0142 00181000
         L     @10,SMDICTP(,@10)                                   0142 00182000
         L     @02,@PC00001+8                                      0142 00183000
         MVC   ICTPFMID(7,@10),IOPPFMID(@02)                       0142 00184000
*             END;                                                 0143 00185000
*                                                                  0143 00186000
*           /*********************************************************/ 00187000
*           /*                                                       */ 00188000
*           /* BUILD SUP LIST                                        */ 00189000
*           /*                                                       */ 00190000
*           /*********************************************************/ 00191000
*                                                                  0144 00192000
*           IF TECRC<STOPPTF THEN   /* NON FAILING SYSMOD?           */ 00193000
@RC00123 CH    TECRC,@CH00059                                      0144 00194000
         BNL   @RF00144                                            0144 00195000
*             DO;                   /* YES,                          */ 00196000
*               CALL ENTCHAIN(IOPPSUPV,IOPPTFVR,ICTPSCHN(1),/*       */ 00197000
*                   ZERO,LCLVERNO,ZERO);                           0146 00198000
         MVC   @AL00001(24),@AL00146                               0146 00199000
         L     @10,@PC00001+8                                      0146 00200000
         LA    @10,IOPPTFVR(,@10)                                  0146 00201000
         ST    @10,@AL00001+4                                      0146 00202000
         L     @10,@PC00001+4                                      0146 00203000
         L     @10,SMDICTP(,@10)                                   0146 00204000
         LA    @10,ICTPSCHN(,@10)                                  0146 00205000
         ST    @10,@AL00001+8                                      0146 00206000
         LA    @01,@AL00001                                        0146 00207000
         BAL   @14,ENTCHAIN                                        0146 00208000
*               RFY                                                0147 00209000
*                 RTNCODE RSTD;                                    0147 00210000
*               TECRC=MAX(TECRC,RTNCODE);/* SAVE THE RETURN CODE     */ 00211000
         CR    TECRC,RTNCODE                                       0148 00212000
         BNL   *+6                                                      00213000
         LR    TECRC,RTNCODE                                       0148 00214000
*               RFY                                                0149 00215000
*                 RTNCODE UNRSTD;                                  0149 00216000
*               IF ICTPSCHN(1)^=TBLXNULL THEN/* LIST IS NON-NULL?    */ 00217000
         L     @10,@PC00001+4                                      0150 00218000
         L     @10,SMDICTP(,@10)                                   0150 00219000
         L     @02,@PC00001                                        0150 00220000
         CLC   ICTPSCHN(4,@10),TBLXNULL(@02)                       0150 00221000
         BE    @RF00150                                            0150 00222000
*                 DO;               /* YES,                          */ 00223000
*                   ICTPSUPP(1)=ON; /* SUP'S PRESENT FOR SYSMOD      */ 00224000
         OI    ICTPSUPP(@10),B'00000100'                           0152 00225000
*                 END;                                             0153 00226000
*               ELSE                                               0154 00227000
*                 ;                 /* LIST IS NULL                  */ 00228000
@RF00150 DS    0H                                                  0155 00229000
*             END;                                                 0155 00230000
*                                                                  0155 00231000
*           /*********************************************************/ 00232000
*           /*                                                       */ 00233000
