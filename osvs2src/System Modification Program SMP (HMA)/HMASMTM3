*           /*                                                       */ 00742000
*           /*********************************************************/ 00743000
*                                                                  0357 00744000
*           CALL HMASMIO(HMASMIOP); /* CALL SMP IO INTERFACE MODULE  */ 00745000
         ST    IOPPTR,@AL00001                                     0357 00746000
         L     @15,@CV01854                                        0357 00747000
         LA    @01,@AL00001                                        0357 00748000
         BALR  @14,@15                                             0357 00749000
*           IF IOPRETRN=IOPGOOD THEN/* IS OWNER APPLIED ON CDS?      */ 00750000
         CLI   IOPRETRN(IOPPTR),0                                  0358 00751000
