*               IF IOPRETRN=IOPGOOD THEN/* IS ELEMENT ALSO ON CDS?   */ 00777000
*                                                                  0365 00778000
         CLI   IOPRETRN(IOPPTR),0                                  0365 00779000
         BNE   @RF00365                                            0365 00780000
*                 /***************************************************/ 00781000
*                 /*                                                 */ 00782000
*                 /* CHECK IF SYSMOD OWNING ELEMENT IS FMID/RMID/UMID*/ 00783000
*                 /* IN ENTRY                                        */ 00784000
*                 /*                                                 */ 00785000
*                 /***************************************************/ 00786000
*                                                                  0366 00787000
*                 DO;               /*                               */ 00788000
*                   IF IOPFMID=ICTPTFS(1)/* DOES FMID MATCH OWNER, OR*/ 00789000
