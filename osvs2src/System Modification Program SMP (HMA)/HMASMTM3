         BNE   @RF00320                                            0320 00577000
*     DO;                           /* NO- THEN SEND ERROR MESSAGE   */ 00578000
*       ICTMCPL(1)=ON;              /* SET COMPLETE BIT ON           */ 00579000
*       ICTMNOGO(1)=ON;             /* SET NOGO BIT ON               */ 00580000
*       ICTMPROC(1)=ON;             /* SET INPROCESS                 */ 00581000
*       ICTMMSG(1)=ON;              /* SET MESSAGE SENT BIT ON       */ 00582000
*                                                                  0325 00583000
         OI    ICTMCPL(@15),B'11110000'                            0325 00584000
*       /*************************************************************/ 00585000
*       /*                                                           */ 00586000
*       /* GENERATE ERROR MSG HMA431                                 */ 00587000
*       /*                                                           */ 00588000
*       /*************************************************************/ 00589000
*                                                                  0326 00590000
*       ETYPE='SRC     ';           /* ELEMENT TYPE                  */ 00591000
         MVC   ETYPE(8),@CC01953                                   0326 00592000
*       MGPCLEAR=MGPCLEAR&&MGPCLEAR;/* ZERO FIELD                    */ 00593000
         XC    MGPCLEAR(6),MGPCLEAR                                0327 00594000
*       MGPTYPE=MGPERR;             /* SET FOR RETURN CODE=8         */ 00595000
