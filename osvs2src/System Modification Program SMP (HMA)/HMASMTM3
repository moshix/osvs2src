*                                                                  0364 00764000
*               /*****************************************************/ 00765000
*               /*                                                   */ 00766000
*               /* LOCATE ENTRY ON CDS DATASET                       */ 00767000
*               /*                                                   */ 00768000
*               /*****************************************************/ 00769000
*                                                                  0364 00770000
*               CALL HMASMIO(HMASMIOP);/* CALL SMP IO INTERFACE    0364 00771000
*                                      MODULE                        */ 00772000
         ST    IOPPTR,@AL00001                                     0364 00773000
         L     @15,@CV01854                                        0364 00774000
         LA    @01,@AL00001                                        0364 00775000
