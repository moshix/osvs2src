*       ICTMDSTE(1)=ON;             /* INDICATE A DLIB CONFLICT      */ 00658000
         OI    ICTMDSTE(@15),B'00000100'                           0339 00659000
*     END;                          /*                               */ 00660000
*   END DISTCHK;                    /*                               */ 00661000
@EL00005 DS    0H                                                  0341 00662000
@EF00005 DS    0H                                                  0341 00663000
