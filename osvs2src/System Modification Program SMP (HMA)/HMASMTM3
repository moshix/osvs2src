*       CALL HMASMMSG(HMASMMGP);    /* SEND ERROR MESSAGE            */ 00608000
*                                                                  0333 00609000
         L     @15,@CV01855                                        0333 00610000
         LA    @01,@AL00333                                        0333 00611000
         BALR  @14,@15                                             0333 00612000
*       /*************************************************************/ 00613000
*       /*                                                           */ 00614000
*       /* SET RETURN CODE TO STOPPTF (RC=8)                         */ 00615000
*       /*                                                           */ 00616000
*       /*************************************************************/ 00617000
*                                                                  0334 00618000
*       TM3HIRC=MAX(TM3HIRC,STOPPTF);/*                              */ 00619000
         LA    @15,8                                               0334 00620000
         CR    TM3HIRC,@15                                         0334 00621000
         BNL   *+6                                                      00622000
         LR    TM3HIRC,@15                                         0334 00623000
*     END;                          /*                               */ 00624000
*                                                                  0336 00625000
*   /*****************************************************************/ 00626000
*   /*                                                               */ 00627000
*   /* IF A DLIB VALUE WAS FOUND ON THE CDS, AND APPLY FUNCTION IS   */ 00628000
*   /* BEING EXECUTED, AND A MCB WAS PASSED, AND MCB DISTLIB VALUE   */ 00629000
*   /* WAS ENTERED, THEN COMPARE THE CDS DISTLIB AND THE MCB DISTLIB */ 00630000
*   /* VALUES. IF DIFFERENT DLIB VALUES ARE FOUND, THEN BLANK OUT THE*/ 00631000
*   /* ICTMOD ENTRY AND SET THE DISTLIB CONFLICT BIT (ICTMDSTE) ON.  */ 00632000
*   /*                                                               */ 00633000
*   /*****************************************************************/ 00634000
*                                                                  0336 00635000
*   IF DLIBBIT=ON                   /* DLIB FOUND IN (A)CDS SRC ENTRY*/ 00636000
