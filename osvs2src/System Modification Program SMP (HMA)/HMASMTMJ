*                                                                  0249 00200000
*   /*****************************************************************/ 00201000
*   /*                                                               */ 00202000
*   /* THIS ROUTINE IS CALLED TO SCAN THRU THE ICT SYSMODS LOOKING   */ 00203000
*   /* FOR THE NEXT SYSMOD TO BE PROCESSED. SELECTION IS BASED ON    */ 00204000
*   /* LOWEST ICTPONUM FOR APPLY AND HIGHEST ICTPONUM FOR RESTORE    */ 00205000
*   /*                                                               */ 00206000
*   /*****************************************************************/ 00207000
*                                                                  0249 00208000
*TMJNXSMD:                                                         0249 00209000
*   PROCEDURE(NXSMDX);              /*                               */ 00210000
TMJNXSMD STM   @14,@12,@SA00002                                    0249 00211000
         MVC   @PC00002(4),0(@01)                                  0249 00212000
*   DECLARE                         /*                               */ 00213000
*     NXSMDX FIXED(31);             /*                               */ 00214000
*   DECLARE                         /*                               */ 00215000
*     1 NXSMDSW BIT(8),             /*                               */ 00216000
*       3 NXSMDEOF BIT(1),          /*                               */ 00217000
*       3 NXSMDFND BIT(1),          /*                               */ 00218000
*       3 * BIT(6);                 /*                               */ 00219000
*   DECLARE                         /*                               */ 00220000
*     NXTX FIXED(31),               /*                               */ 00221000
*     NXTY FIXED(31);               /*                               */ 00222000
*   DECLARE                         /*                               */ 00223000
*     LON FIXED(15) INIT(0);        /*                               */ 00224000
