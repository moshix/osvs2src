@DL00261 DS    0H                                                  0262 00247000
*     NXSMDEOF=ON;                  /* NO SYSMODS FOUND              */ 00248000
         OI    NXSMDEOF,B'10000000'                                0262 00249000
*     DO NXTX=1 BY 1 WHILE(         /* LOOP THRU ICT                 */ 00250000
*           ICTPEND(NXTX)^=ICTENDNT);/* TILL END                     */ 00251000
*                                                                  0263 00252000
         LA    NXTX,1                                              0263 00253000
         B     @DE00263                                            0263 00254000
@DL00263 DS    0H                                                  0264 00255000
*       /*************************************************************/ 00256000
*       /*                                                           */ 00257000
*       /* CHECK FOR SYSMOD APPLICABILITY                            */ 00258000
*       /*                                                           */ 00259000
*       /*************************************************************/ 00260000
*                                                                  0264 00261000
*       IF ICTPCPL(NXTX)=OFF        /* IF NOT COMPLETE               */ 00262000
*           &ICTPDEXP(NXTX)=OFF     /* NOT DELETED                   */ 00263000
*           &ICTPDIMP(NXTX)=OFF     /*                               */ 00264000
*           &ICTPGELM(NXTX)=OFF     /* NOT ELIMINATED                */ 00265000
*           &ICTPSUPN(NXTX)=ZERO    /* NOT SUPED                     */ 00266000
*           &(ICTPJCLP(NXTX)=ON     /* IF IT HAS JCLIN               */ 00267000
*           |(CCARES=ON&ICTPMDLP(NXTX)=ON))/* OR RESTORE AND SYSMOD     00268000
*                                      HAD DELETES                   */ 00269000
*         THEN                      /*                               */ 00270000
         LR    @10,NXTX                                            0264 00271000
