*   RFY                                                            0255 00227000
*     ICTPTF BASED(CCAICPTF);       /*                               */ 00228000
*   NXSMDEOF=OFF;                   /*                               */ 00229000
*   NXSMDFND=OFF;                   /*                               */ 00230000
         NI    NXSMDEOF,B'00111111'                                0257 00231000
*   IF CCAAPPLY=ON|CCAACCPT=ON      /*                               */ 00232000
*     THEN                          /*                               */ 00233000
