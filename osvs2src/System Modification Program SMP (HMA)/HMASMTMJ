         STH   @10,LON                                             0268 00347000
*               NXSMDFND=ON;        /* SET FOUND SW                  */ 00348000
*               NXSMDEOF=OFF;       /* NO STOP AT EOF                */ 00349000
         OI    NXSMDFND,B'01000000'                                0270 00350000
         NI    NXSMDEOF,B'01111111'                                0270 00351000
*               NXTY=NXTX;          /* KEEP INDEX OF THIS ONE        */ 00352000
         ST    NXTX,NXTY                                           0271 00353000
*             END;                  /*                               */ 00354000
*         END;                      /*                               */ 00355000
@RF00266 DS    0H                                                  0274 00356000
*     END;                          /* END LOOP THRU ICT             */ 00357000
*                                                                  0274 00358000
@RF00264 AH    NXTX,@CH00061                                       0274 00359000
@DE00263 LA    @10,116                                             0274 00360000
         LR    @01,NXTX                                            0274 00361000
