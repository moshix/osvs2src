         BNE   @RF00247                                            0247 00216000
*       DO;                                                        0248 00217000
*         ENGEOF=ON;                /* END OF FILE                   */ 00218000
         OI    ENGEOF,B'10000000'                                  0249 00219000
*       END;                                                       0250 00220000
*     ELSE                          /* GTA FAILURE                   */ 00221000
*       DO;                                                        0251 00222000
         B     @RC00247                                            0251 00223000
@RF00247 DS    0H                                                  0252 00224000
*         ENVGTARC=MAX(ENVGTARC,GTPRETRN);/* SAVE RETURN CODE        */ 00225000
