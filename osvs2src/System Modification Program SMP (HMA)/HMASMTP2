*                                                                  0232 00141000
*ENVGTA:                                                           0232 00142000
*   PROC;                                                          0232 00143000
ENVGTA   STM   @14,@12,@SA00002                                    0232 00144000
*   RFY                                                            0233 00145000
*     ICTPTF BASED(ENTRYPTR);       /* ADDRESS ICT FOR SYSMOD PASSED */ 00146000
*   DCL                                                            0234 00147000
*     ENVGTARC FIXED(15);           /* ENVGTA RETURN CODE            */ 00148000
*   DCL                                                            0235 00149000
*     1 ENGFLAGS BIT(8),            /* ENVGTA CONTROL FLAGS          */ 00150000
*      2 ENGEOF BIT(1),             /* END OF FILE ON GTA FILE       */ 00151000
*      2 ENGCSROK BIT(1),           /* CAUSER IS IN CORRECT STATE TO    00152000
*                                      PROCESS ++IF DATA             */ 00153000
*      2 * BIT(6);                  /* UNUSED                        */ 00154000
*   RFY                                                            0236 00155000
*     RTNCODE RSTD;                                                0236 00156000
*   RFY                                                            0237 00157000
*     HMASMGTP BASED(TBLXIFGT);     /* ADDRESS ++IF GTA FILE         */ 00158000
*   RFY                                                            0238 00159000
