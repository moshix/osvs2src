         TITLE 'HMASMTP2 - BUILD IFREQ INDEX LIST'                      00001000
HMASMTP2 CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMTP2  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(12),0(@01)                                 0001 00017000
*   TP2RC=ZERO;                     /* CLEAR THE RETURN CODE         */ 00018000
         SLR   TP2RC,TP2RC                                         0202 00019000
*   TP2FLAGS=''B;                   /* CLEAR LOCAL FLAGS             */ 00020000
         MVI   TP2FLAGS,X'00'                                      0203 00021000
*   TAIFRST=ON;                     /* FIRST CALL TO ROUTINES        */ 00022000
         OI    TAIFRST,B'10000000'                                 0204 00023000
*   SYSMOD=ICTPTFS(1);              /* SAVE NAME OF CURRENT SYSMOD   */ 00024000
         L     @14,@PC00001+4                                      0205 00025000
         L     @14,ENTRYPTR(,@14)                                  0205 00026000
         MVC   SYSMOD(7),ICTPTFS(@14)                              0205 00027000
*   DSID=TBLXCRQI;                  /* PROCESS CRQ OR ACRQ AS      0206 00028000
*                                      SPECIFIED IN TBLX             */ 00029000
         L     @10,@PC00001                                        0206 00030000
         SLR   DSID,DSID                                           0206 00031000
         IC    DSID,TBLXCRQI(,@10)                                 0206 00032000
*   SAVBUFAD=IOPBUFAD;              /* SAVE IOPBUFAD                 */ 00033000
         L     @02,@PC00001+8                                      0207 00034000
         L     SAVBUFAD,IOPBUFAD(,@02)                             0207 00035000
*   IOPBUFAD=ADDR(CSRBUFFR);        /* POINT TO LOCAL BUFFER         */ 00036000
         LA    @09,CSRBUFFR                                        0208 00037000
         ST    @09,IOPBUFAD(,@02)                                  0208 00038000
*   TSLTBLXP=ADDR(TBLXPARM);        /* INITIALIZE HMASMTSB PARM LIST */ 00039000
         L     @02,TBLXTSLP(,@10)                                  0209 00040000
         ST    @10,TSLTBLXP(,@02)                                  0209 00041000
*   TSLICTSR=''B;                   /* CLEAR ICT SYSMOD SERACH PARMS */ 00042000
         LA    @02,ICTSR                                           0210 00043000
         XC    TSLICTSR(14,@02),TSLICTSR(@02)                      0210 00044000
*   TSLSICDS=''B;                   /* CLEAR CDS & ICT SEARCH PARMS  */ 00045000
         LA    @02,SICDS                                           0211 00046000
         XC    TSLSICDS(20,@02),TSLSICDS(@02)                      0211 00047000
*   ICTPICHN(1)=TBLXNULL;           /* INIT IREQ CHAIN POINTER       */ 00048000
         MVC   ICTPICHN(4,@14),TBLXNULL(@10)                       0212 00049000
*                                                                  0213 00050000
*   /*****************************************************************/ 00051000
*   /*                                                               */ 00052000
*   /* IF THE SYSMOD IS A FUNCTION THEN FIND CAUSERS WHICH REFERENCED*/ 00053000
*   /* IT AS AN FMID ON THEIR ++IF STATEMENTS.                       */ 00054000
*   /*                                                               */ 00055000
*   /*****************************************************************/ 00056000
*                                                                  0213 00057000
*   IF ICTPTYPE(1)=IOPPFUNC THEN    /* IS THIS A FUNCTION PTF ENTRY? */ 00058000
         CLI   ICTPTYPE(@14),4                                     0213 00059000
         BNE   @RF00213                                            0213 00060000
*     DO;                           /* YES,                          */ 00061000
*                                                                  0214 00062000
*       /*************************************************************/ 00063000
*       /*                                                           */ 00064000
*       /* SEARCH THE ++IF RECORDS FROM SYSMODS BEING PROCESSED.     */ 00065000
*       /*                                                           */ 00066000
*       /*************************************************************/ 00067000
*                                                                  0215 00068000
*       CALL ENVGTA;                /* PROCESS GTA FILE ++IF DATA  0215 00069000
*                                      FIRST                         */ 00070000
         BAL   @14,ENVGTA                                          0215 00071000
*       TP2RC=MAX(TP2RC,RTNCODE);                                  0216 00072000
         CR    TP2RC,RTNCODE                                       0216 00073000
         BNL   *+6                                                      00074000
         LR    TP2RC,RTNCODE                                       0216 00075000
*       IF TP2RC<STOPPTF THEN       /* ALL IS WELL?                  */ 00076000
         CH    TP2RC,@CH00057                                      0217 00077000
         BNL   @RF00217                                            0217 00078000
*         DO;                       /* YES,                          */ 00079000
*                                                                  0218 00080000
*           /*********************************************************/ 00081000
*           /*                                                       */ 00082000
*           /* SEARCH THE (A)CRQ FOR FMID RECORDS TO GET CAUSERS     */ 00083000
*           /* WHICH WILL LIST REQS.                                 */ 00084000
*           /*                                                       */ 00085000
*           /*********************************************************/ 00086000
*                                                                  0219 00087000
*           CALL ENVCRQ;            /* PROCESS CRQ EVIRONMENT RECORDS   00088000
*                                      NEXT                          */ 00089000
         BAL   @14,ENVCRQ                                          0219 00090000
*           TP2RC=MAX(TP2RC,RTNCODE);                              0220 00091000
         CR    TP2RC,RTNCODE                                       0220 00092000
         BNL   *+6                                                      00093000
         LR    TP2RC,RTNCODE                                       0220 00094000
*         END;                                                     0221 00095000
*     END;                                                         0222 00096000
*                                                                  0222 00097000
@RF00217 DS    0H                                                  0223 00098000
*   /*****************************************************************/ 00099000
*   /*                                                               */ 00100000
*   /* CHECK THE ++IF FROM THE SYSMODS BEING PROCESSED. LOCATE THIS  */ 00101000
*   /* SYSMOD AS A CAUSER AND GET REQS,                              */ 00102000
*   /*                                                               */ 00103000
*   /*****************************************************************/ 00104000
*                                                                  0223 00105000
*   IF TP2RC<STOPPTF THEN           /* NO ERROR YET?                 */ 00106000
@RF00213 CH    TP2RC,@CH00057                                      0223 00107000
         BNL   @RF00223                                            0223 00108000
*     DO;                           /* YES,                          */ 00109000
*       CALL CSRGTA;                /* PROCESS GTA FILE ++IF DATA  0225 00110000
*                                      FIRST                         */ 00111000
         BAL   @14,CSRGTA                                          0225 00112000
*       TP2RC=MAX(TP2RC,RTNCODE);                                  0226 00113000
         CR    TP2RC,RTNCODE                                       0226 00114000
         BNL   *+6                                                      00115000
         LR    TP2RC,RTNCODE                                       0226 00116000
*     END;                                                         0227 00117000
