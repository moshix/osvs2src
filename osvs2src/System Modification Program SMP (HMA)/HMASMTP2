         BNL   *+6                                                      00297000
         LR    ENVGTARC,@10                                        0262 00298000
*         IF TSLIOK=ON THEN         /* ICT ENTRY IS OK               */ 00299000
*                                                                  0263 00300000
@RF00261 LA    @14,ICTSR                                           0263 00301000
         TM    TSLIOK(@14),B'01000000'                             0263 00302000
         BNO   @RF00263                                            0263 00303000
*           /*********************************************************/ 00304000
*           /*                                                       */ 00305000
*           /* ADD THE REQ FROM THE GTA RECORD TO THE ICT IREQ LIST. */ 00306000
*           /*                                                       */ 00307000
*           /*********************************************************/ 00308000
*                                                                  0264 00309000
*           DO;                                                    0264 00310000
*             CALL REQADD(TPICSR,   /* CAUSER                        */ 00311000
*                 TPIREQ,           /* REQUISITE SYSMOD              */ 00312000
*                 ICTPTF(1));       /* ICT PTF SECT ENTRY TO ADD TO  */ 00313000
         L     @14,@PC00001                                        0265 00314000
         L     @14,TBLXIFGT(,@14)                                  0265 00315000
         L     @14,GTPBUFAD(,@14)                                  0265 00316000
         ST    @14,@AL00001                                        0265 00317000
