*   ENGFLAGS=''B;                                                  0239 00161000
         MVI   ENGFLAGS,X'00'                                      0239 00162000
*   ENVGTARC=0;                     /* CLEAR RETURN CODE             */ 00163000
*                                                                  0240 00164000
         SLR   ENVGTARC,ENVGTARC                                   0240 00165000
*   /*****************************************************************/ 00166000
*   /*                                                               */ 00167000
*   /* POSITION TO THE BEGINNING OF THE GTA FILE                     */ 00168000
*   /*                                                               */ 00169000
*   /*****************************************************************/ 00170000
*                                                                  0241 00171000
