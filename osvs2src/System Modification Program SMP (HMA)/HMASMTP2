*   RFY                                                            0272 00346000
*     HMASMGTP BASED;                                              0272 00347000
*   RFY                                                            0273 00348000
*     TPIFRECD BASED;                                              0273 00349000
*   END ENVGTA;                                                    0274 00350000
*                                                                  0275 00351000
*/********************************************************************/ 00352000
*/*                                                                  */ 00353000
*/*  CSRGTA - FIND CONDITIONAL REQUISITES SUPPLIED BY THE SYSMOD     */ 00354000
*/*           PASSED.                                                */ 00355000
*/*                                                                  */ 00356000
*/*           THOSE REQUISITES WHOSE FMID MATCHES AN FMID IN EITHER  */ 00357000
*/*           THE ICT OR ON THE (A)CDS ARE ATTACHED TO THE SYSMOD    */ 00358000
*/*           PASSED                                                 */ 00359000
*/*                                                                  */ 00360000
*/********************************************************************/ 00361000
*                                                                  0275 00362000
*CSRGTA:                                                           0275 00363000
*   PROC;                                                          0275 00364000
CSRGTA   STM   @14,@12,@SA00003                                    0275 00365000
*   RESPECIFY                                                      0276 00366000
*     ICTPTF BASED(ENTRYPTR);       /* ADDRESS SYSMOD PASSED         */ 00367000
*   DCL                                                            0277 00368000
*     CSRGTARC FIXED(15);           /* RETURN CODE                   */ 00369000
*   DCL                                                            0278 00370000
*     1 CSGFLAG BIT(8),             /* CONTROL FLAGS                 */ 00371000
*      2 CSGEOF BIT(1),             /* LOGICAL END OF FILE ON GTA CSR   00372000
*                                      SEARCH                        */ 00373000
*      2 * BIT(7);                  /* UNUSED                        */ 00374000
*   RFY                                                            0279 00375000
*     RTNCODE RSTD;                                                0279 00376000
*   RFY                                                            0280 00377000
*     HMASMGTP BASED(TBLXIFGT);                                    0280 00378000
*   RFY                                                            0281 00379000
*     TPIFRECD BASED(GTPBUFAD);                                    0281 00380000
*   CSRGTARC=ZERO;                  /* CLEAR RC                      */ 00381000
         SLR   CSRGTARC,CSRGTARC                                   0282 00382000
*                                                                  0283 00383000
*   /*****************************************************************/ 00384000
*   /*                                                               */ 00385000
*   /* SET UP ++IF DATA GTP TO ACCESS GTA FILE.                      */ 00386000
*   /*                                                               */ 00387000
*   /*****************************************************************/ 00388000
*                                                                  0283 00389000
*   GTPFUNCT=GTPOPENN;              /* FUNCTION IS OPEN              */ 00390000
         L     @14,@PC00001                                        0283 00391000
         L     @14,TBLXIFGT(,@14)                                  0283 00392000
         MVI   GTPFUNCT(@14),X'01'                                 0283 00393000
*   CALL HMASMGTA(HMASMGTP);        /* ...                           */ 00394000
         ST    @14,@AL00001                                        0284 00395000
         L     @15,@CV01666                                        0284 00396000
         LA    @01,@AL00001                                        0284 00397000
         BALR  @14,@15                                             0284 00398000
*   CSRGTARC=MAX(CSRGTARC,GTPRETRN);/* ERROR CODE                    */ 00399000
*                                                                  0285 00400000
         L     @14,@PC00001                                        0285 00401000
         L     @14,TBLXIFGT(,@14)                                  0285 00402000
         SLR   @10,@10                                             0285 00403000
         IC    @10,GTPRETRN(,@14)                                  0285 00404000
         CR    CSRGTARC,@10                                        0285 00405000
         BNL   *+6                                                      00406000
         LR    CSRGTARC,@10                                        0285 00407000
*   /*****************************************************************/ 00408000
*   /*                                                               */ 00409000
*   /* PROCESS ALL ++IF GTA RECORDS SUPPLIED BY THE CURRENT SYSMOD   */ 00410000
*   /*                                                               */ 00411000
*   /*****************************************************************/ 00412000
*                                                                  0286 00413000
*   DO WHILE CSRGTARC=ZERO&GTPRETRN=ZERO;                          0286 00414000
