         L     @02,ENTRYPTR(,@02)                                  0254 00258000
         CLC   TPIFMID(7,@10),ICTPTFS(@02)                         0254 00259000
         BNE   @RF00254                                            0254 00260000
*       DO;                         /* YES,                          */ 00261000
*                                                                  0255 00262000
*         /***********************************************************/ 00263000
*         /*                                                         */ 00264000
*         /* CHECK CAUSER IN THE ICT FOR APPROPRIATE STATE, I.E.     */ 00265000
*         /* SYSMOD IS NOT NOGO AND IS NOT SUP'ED BY ANOTHER SYSMOD  */ 00266000
*         /* IN THE ICT                                              */ 00267000
*         /*                                                         */ 00268000
*         /***********************************************************/ 00269000
*                                                                  0256 00270000
*         TSLFUNCT=TSLFISRH;        /* SEARCH ICT                    */ 00271000
