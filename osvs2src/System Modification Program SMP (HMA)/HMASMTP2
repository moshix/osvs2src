*         TSLPMLAD=ADDR(TSLICTSR);  /* POINT TO SEARCH PARM LIST     */ 00274000
         LA    @02,ICTSR                                           0257 00275000
         ST    @02,TSLPMLAD(,@14)                                  0257 00276000
*         TSLICTSR=''B;             /* CLEAR SEARCH PARM LIST        */ 00277000
