         L     @14,TBLXIFGT(,@14)                                  0287 00419000
         MVI   GTPFUNCT(@14),X'08'                                 0287 00420000
*     CALL HMASMGTA(HMASMGTP);      /* GET NEXT RECORD IN GTA        */ 00421000
         ST    @14,@AL00001                                        0288 00422000
         L     @15,@CV01666                                        0288 00423000
         LA    @01,@AL00001                                        0288 00424000
         BALR  @14,@15                                             0288 00425000
