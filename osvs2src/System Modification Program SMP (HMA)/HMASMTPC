         TITLE 'HMASMTPC - COMPLETE PTF ENTRY AND SAVE IF DATA'         00001000
HMASMTPC CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMTPC  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
         MVC   @PC00001(20),0(@01)                                 0001 00017000
*                                                                  0191 00018000
*   /*****************************************************************/ 00019000
*   /*                                                               */ 00020000
*   /* HMASMTPC << MAINLINE                                          */ 00021000
*   /*                                                               */ 00022000
*   /*****************************************************************/ 00023000
*                                                                  0191 00024000
*   TPCRC=ZERO;                     /* CLEAR THE RETURN CODE         */ 00025000
         SLR   TPCRC,TPCRC                                         0191 00026000
*   TPCFLAGS=''B;                   /* CLEAR LOCAL CONTROL FLAGS     */ 00027000
         MVI   TPCFLAGS,X'00'                                      0192 00028000
*   RFY                                                            0193 00029000
*     HMASMIOP BASED(ADDR(MCBIOP)); /*                               */ 00030000
*   SAVBUFAD=IOPBUFAD;              /* SAVE BUFFER ADDR TO BE      0194 00031000
*                                      RESTORED AT THE END           */ 00032000
         L     @10,@PC00001+16                                     0194 00033000
         L     SAVBUFAD,IOPBUFAD(,@10)                             0194 00034000
*   RFY                                                            0195 00035000
*     HMASMIOP BASED(ADDR(SMDIOP)); /*                               */ 00036000
*   SYSMOD8=' ';                    /* CLEAR SYSMOD NAME             */ 00037000
         MVI   SYSMOD8+1,C' '                                      0196 00038000
         MVC   SYSMOD8+2(6),SYSMOD8+1                              0196 00039000
         MVI   SYSMOD8,C' '                                        0196 00040000
*   SYSMOD=IOPNAM17;                /* SAVE THE SYSMOD NAME          */ 00041000
         L     @10,@PC00001+4                                      0197 00042000
         MVC   SYSMOD(7),IOPNAM17(@10)                             0197 00043000
*   HMASMCRP=''B;                   /* INITIALIZE THE CRP FOR      0198 00044000
*                                      SUBSEQUENT PARSING OF THE MCS    00045000
*                                      ENTRY                         */ 00046000
         XC    HMASMCRP(106),HMASMCRP                              0198 00047000
*   CRPINDS=IOPPTS;                 /* CRP INPUT IS THE PTS          */ 00048000
*                                                                  0199 00049000
         MVI   CRPINDS,X'06'                                       0199 00050000
*   /*****************************************************************/ 00051000
*   /*                                                               */ 00052000
*   /* LOCATE MCS ENTRY ON THE SMPPTS                                */ 00053000
*   /*                                                               */ 00054000
*   /*****************************************************************/ 00055000
*                                                                  0200 00056000
*   CALL LOCMCS;                    /*                               */ 00057000
         BAL   @14,LOCMCS                                          0200 00058000
*   RFY                                                            0201 00059000
*     RTNCODE RSTD;                                                0201 00060000
*   TPCRC=MAX(RTNCODE,TPCRC);       /* SET RETURN CODE               */ 00061000
         CR    TPCRC,RTNCODE                                       0202 00062000
         BNL   *+6                                                      00063000
         LR    TPCRC,RTNCODE                                       0202 00064000
*   RFY                                                            0203 00065000
*     RTNCODE UNRSTD;                                              0203 00066000
*   IF TPCRC=ZERO THEN              /* NO ERROR SO FAR?              */ 00067000
         LTR   TPCRC,TPCRC                                         0204 00068000
         BNZ   @RF00204                                            0204 00069000
*     DO;                           /* YES,                          */ 00070000
*                                                                  0205 00071000
*       /*************************************************************/ 00072000
*       /*                                                           */ 00073000
*       /* SEARCH THE MCS PTS ENTRY TO FIND THE ++VER CORRESPONDING  */ 00074000
*       /* TO THE ++VER INDEX SUPPLIED BY THE CALLER                 */ 00075000
*       /*                                                           */ 00076000
*       /*************************************************************/ 00077000
*                                                                  0206 00078000
*       CALL VERSRH;                /*                               */ 00079000
         BAL   @14,VERSRH                                          0206 00080000
*       RFY                                                        0207 00081000
*         RTNCODE RSTD;                                            0207 00082000
*       TPCRC=MAX(RTNCODE,TPCRC);   /* SET RETURN CODE               */ 00083000
         CR    TPCRC,RTNCODE                                       0208 00084000
         BNL   *+6                                                      00085000
         LR    TPCRC,RTNCODE                                       0208 00086000
*       RFY                                                        0209 00087000
*         RTNCODE UNRSTD;                                          0209 00088000
*     END;                                                         0210 00089000
*   IF TPCRC<STOPFUNC THEN          /* NO SERIOUS ERROR YET?         */ 00090000
@RF00204 CH    TPCRC,@CH00273                                      0211 00091000
         BNL   @RF00211                                            0211 00092000
*     DO;                           /* YES,                          */ 00093000
*                                                                  0212 00094000
*       /*************************************************************/ 00095000
*       /*                                                           */ 00096000
*       /* BUILD A PTF ENTRY IN THE ICT BASED ON THE DATA IN THE     */ 00097000
*       /* SYSMOD SMPPTS IOP FOR 'SELECTED' ++VER INDEX              */ 00098000
*       /*                                                           */ 00099000
*       /*************************************************************/ 00100000
*                                                                  0213 00101000
*       PTFPTR=ENTRYPTR;            /* ASSUME ENTRY POINTER SUPPLIED */ 00102000
         L     @10,@PC00001+12                                     0213 00103000
         L     @10,ENTRYPTR(,@10)                                  0213 00104000
         ST    @10,PTFPTR                                          0213 00105000
*       IF PTFPTR=ZERO THEN         /* NO ENTRY POINTER PASSED?      */ 00106000
         LTR   @10,@10                                             0214 00107000
         BNZ   @RF00214                                            0214 00108000
*         DO;                       /* YES,                          */ 00109000
*           PTFPTR=TBLXTOP;         /* ENTRY WILL BE AT CURRENT ICT     00110000
*                                      TOP                           */ 00111000
         L     @10,@PC00001                                        0216 00112000
         MVC   PTFPTR(4),TBLXTOP(@10)                              0216 00113000
*         END;                      /*                               */ 00114000
*       ELSE                                                       0218 00115000
*         ;                         /*                               */ 00116000
@RF00214 DS    0H                                                  0219 00117000
*       CALL BLDSMD;                /*                               */ 00118000
         BAL   @14,BLDSMD                                          0219 00119000
*       RFY                                                        0220 00120000
*         RTNCODE RSTD;                                            0220 00121000
*       TPCRC=MAX(RTNCODE,TPCRC);   /* SET RETURN CODE               */ 00122000
         CR    TPCRC,RTNCODE                                       0221 00123000
         BNL   *+6                                                      00124000
         LR    TPCRC,RTNCODE                                       0221 00125000
*       IF RTNCODE<STOPFUNC THEN    /* WAS A SYSMOD ENTRY COMPLETED? */ 00126000
         CH    RTNCODE,@CH00273                                    0222 00127000
         BNL   @RF00222                                            0222 00128000
*         DO;                       /* YES,                          */ 00129000
*           RFY                                                    0224 00130000
*             RTNCODE UNRSTD;                                      0224 00131000
*           ENTBUILT=ON;            /* ENTRY BUILT FLAG              */ 00132000
         OI    ENTBUILT,B'10000000'                                0225 00133000
*         END;                      /*                               */ 00134000
*       ELSE                                                       0227 00135000
*         ;                         /* SEVERE ERROR                  */ 00136000
@RF00222 DS    0H                                                  0228 00137000
*     END;                                                         0228 00138000
*   IF TPCRC=ZERO THEN              /* NO ERROR SO FAR?              */ 00139000
@RF00211 LTR   TPCRC,TPCRC                                         0229 00140000
         BNZ   @RF00229                                            0229 00141000
*     DO;                           /* YES,                          */ 00142000
*                                                                  0230 00143000
*       /*************************************************************/ 00144000
*       /*                                                           */ 00145000
*       /* SAVE THE ++IF DATA FOLLOWING THE 'SELECTED' ++VER IN THE  */ 00146000
*       /* SMPPTS MCS ENTRY AS RECORDS IN THE TBLXIFGT GTA FILE      */ 00147000
*       /*                                                           */ 00148000
*       /*************************************************************/ 00149000
*                                                                  0231 00150000
*       CALL SAVEIF;                /*                               */ 00151000
         BAL   @14,SAVEIF                                          0231 00152000
*       RFY                                                        0232 00153000
*         RTNCODE RSTD;                                            0232 00154000
*       TPCRC=MAX(RTNCODE,TPCRC);   /* SET RETURN CODE               */ 00155000
         CR    TPCRC,RTNCODE                                       0233 00156000
         BNL   *+6                                                      00157000
         LR    TPCRC,RTNCODE                                       0233 00158000
*       RFY                                                        0234 00159000
*         RTNCODE UNRSTD;                                          0234 00160000
*     END;                                                         0235 00161000
*   IF TPCRC=STOPPTF                /* SYSMOD FAILED ...             */ 00162000
*       &ENTBUILT=ON THEN           /* AND ENTRY BUILT ?             */ 00163000
@RF00229 CH    TPCRC,@CH00061                                      0236 00164000
         BNE   @RF00236                                            0236 00165000
         TM    ENTBUILT,B'10000000'                                0236 00166000
         BNO   @RF00236                                            0236 00167000
*     DO;                           /* YES,                          */ 00168000
*       PTFPTR->ICTPNOGO(1)=ON;     /* MARK SYSMOD 'NOGO'            */ 00169000
         L     @10,PTFPTR                                          0238 00170000
         OI    ICTPNOGO(@10),B'01000000'                           0238 00171000
*     END;                          /*                               */ 00172000
*   ELSE                                                           0240 00173000
*     ;                             /* SYSMOD NOT FAILED OR ENTRY NOT   00174000
*                                      BUILT                         */ 00175000
@RF00236 DS    0H                                                  0241 00176000
*   RFY                                                            0241 00177000
*     HMASMIOP BASED(ADDR(MCBIOP)); /*                               */ 00178000
*   IOPBUFAD=SAVBUFAD;              /* RESTORE BUFFER ADDR TO IOP  0242 00179000
*                                      USED AS MCB                   */ 00180000
         L     @10,@PC00001+16                                     0242 00181000
         ST    SAVBUFAD,IOPBUFAD(,@10)                             0242 00182000
*   RFY                                                            0243 00183000
*     HMASMIOP BASED(ADDR(SMDIOP)); /*                               */ 00184000
*   RETURN CODE(TPCRC);             /*                               */ 00185000
         LR    @15,TPCRC                                           0244 00186000
         L     @13,4(,@13)                                         0244 00187000
         L     @14,12(,@13)                                        0244 00188000
         LM    @00,@12,20(@13)                                     0244 00189000
         BR    @14                                                 0244 00190000
*   RFY                                                            0245 00191000
*     HMASMIOP BASED;                                              0245 00192000
*                                                                  0246 00193000
*   /*****************************************************************/ 00194000
*   /*                                                               */ 00195000
*   /* LOCMCS - ROUTINE TO LOCATE MCS ENTRY ON THE SMPPTS            */ 00196000
*   /*                                                               */ 00197000
*   /*****************************************************************/ 00198000
*                                                                  0246 00199000
*LOCMCS:                                                           0246 00200000
*   PROC;                           /*                               */ 00201000
LOCMCS   STM   @14,@12,@SA00002                                    0246 00202000
*   DCL                                                            0247 00203000
*     LOCMCSRC FIXED(15);           /* LOCMCS RETURN CODE            */ 00204000
*   RFY                                                            0248 00205000
*     HMASMIOP BASED(ADDR(TPCIOPF));                               0248 00206000
*   LOCMCSRC=ZERO;                  /* CLEAR RETURN CODE             */ 00207000
         SLR   LOCMCSRC,LOCMCSRC                                   0249 00208000
*   IOPDSID=IOPPTS;                 /* LOCATE PTS MCS ENTRY FOR ...  */ 00209000
         LA    @10,TPCIOPF                                         0250 00210000
         MVI   IOPDSID(@10),X'06'                                  0250 00211000
*   IOPNAME=SYSMOD;                 /* ... SYSMOD BEING PROCESSED    */ 00212000
         MVI   IOPNAME+7(@10),C' '                                 0251 00213000
         MVC   IOPNAME(7,@10),SYSMOD                               0251 00214000
*   IOPFUNCT=IOPBLDL;               /* ...                           */ 00215000
         MVI   IOPFUNCT(@10),X'04'                                 0252 00216000
*   IOPTYPE=IOPMCS;                 /* ...                           */ 00217000
         MVI   IOPTYPE(@10),X'10'                                  0253 00218000
*   CALL HMASMIO(HMASMIOP);         /* ...                           */ 00219000
         ST    @10,@AL00001                                        0254 00220000
         L     @15,@CV01718                                        0254 00221000
         LA    @01,@AL00001                                        0254 00222000
         BALR  @14,@15                                             0254 00223000
*   IF IOPRETRN^=IOPGOOD THEN       /* WAS THERE AN ERROR            */ 00224000
         LA    @10,TPCIOPF                                         0255 00225000
         CLI   IOPRETRN(@10),0                                     0255 00226000
         BE    @RF00255                                            0255 00227000
*     DO;                           /* YES,                          */ 00228000
*       LOCMCSRC=STOPPTF;           /* STOP THE SYSMOD               */ 00229000
*                                                                  0257 00230000
         LA    LOCMCSRC,8                                          0257 00231000
*       /*************************************************************/ 00232000
*       /*                                                           */ 00233000
*       /* ISSUE MESSAGE HMA246I XXXXXXXX XXXXXXXX NOT FOUND ON      */ 00234000
*       /* XXXXXXXX                                                  */ 00235000
*       /*                                                           */ 00236000
*       /*************************************************************/ 00237000
*                                                                  0258 00238000
*       DO;                         /* HMASMPMG M1(HMA246I)VAR(TXTMCS   00239000
*                                      ,SYSMOD,TXTPTS)TYPE(ERR)TARGET   00240000
*                                      (SYSOUT,LOG)                  */ 00241000
*         HMASMMGP=HMASMMGP&&HMASMMGP;/* SET PARMS TO ZEROS          */ 00242000
         XC    HMASMMGP(24),HMASMMGP                               0259 00243000
*         MGPMGNO1=HMA246I;         /* SET UP MSG 1 CODE             */ 00244000
         MVC   MGPMGNO1(2),@CH01727                                0260 00245000
*         MGPTYPE=MGPERR;           /* SET MESSAGE TYPE              */ 00246000
         MVI   MGPTYPE,X'03'                                       0261 00247000
*         MGPPRINT='1'B;            /* SET TARGET FLAG               */ 00248000
*         MGPHLDS='1'B;             /* SET TARGET FLAG               */ 00249000
         OI    MGPPRINT,B'10010000'                                0263 00250000
*         MGPVARPT(1)=ADDR(TXTMCS); /* SET VARIABLE PTR              */ 00251000
         LA    @10,TXTMCS                                          0264 00252000
         ST    @10,MGPVARPT                                        0264 00253000
*         MGPVARPT(2)=ADDR(SYSMOD); /* SET VARIABLE PTR              */ 00254000
         LA    @10,SYSMOD                                          0265 00255000
         ST    @10,MGPVARPT+4                                      0265 00256000
*         MGPVARPT(3)=ADDR(TXTPTS); /* SET VARIABLE PTR              */ 00257000
         LA    @10,TXTPTS                                          0266 00258000
         ST    @10,MGPVARPT+8                                      0266 00259000
*         CALL HMASMMSG(HMASMMGP);  /* HAVE MESSAGE ISSUED           */ 00260000
         L     @15,@CV01719                                        0267 00261000
         LA    @01,@AL00267                                        0267 00262000
         BALR  @14,@15                                             0267 00263000
*       END;                                                       0268 00264000
*     END;                          /*                               */ 00265000
*   ELSE                                                           0270 00266000
*     ;                             /* IO WAS OK                     */ 00267000
@RF00255 DS    0H                                                  0271 00268000
*   RETURN CODE(LOCMCSRC);          /* RETURN WITH RETURN CODE       */ 00269000
         LR    @15,LOCMCSRC                                        0271 00270000
         L     @14,@SA00002                                        0271 00271000
         LM    @00,@12,@SA00002+8                                  0271 00272000
         BR    @14                                                 0271 00273000
*   RFY                                                            0272 00274000
*     HMASMIOP BASED;                                              0272 00275000
*   END LOCMCS;                     /*                               */ 00276000
*                                                                  0274 00277000
*   /*****************************************************************/ 00278000
*   /*                                                               */ 00279000
*   /* VERSRH - SEARCH THRU MCS ENTRY FOR SPECIFIED ++VER            */ 00280000
*   /*                                                               */ 00281000
*   /*****************************************************************/ 00282000
*                                                                  0274 00283000
*VERSRH:                                                           0274 00284000
*   PROC;                           /*                               */ 00285000
VERSRH   STM   @14,@12,@SA00003                                    0274 00286000
*   DCL                                                            0275 00287000
*     VERSRHRC FIXED(15);           /* VERSRH RETURN CODE            */ 00288000
*   DCL                                                            0276 00289000
*     1 VERFLAGS BIT(8),            /* LOCAL CONTROL FLAGS           */ 00290000
*      2 RIGHTVER BIT(1),           /* ++VER CORRESPONDING TO INPUT     00291000
*                                      VER INDEX FOUND               */ 00292000
*      2 * BIT(7);                  /* UNUSED                        */ 00293000
*   DCL                                                            0277 00294000
*     VERCNT FIXED(8);              /* COUNT OF ++VER'S FOUND SO FAR    00295000
*                                      IN MCS ENTRY                  */ 00296000
*   RFY                                                            0278 00297000
*     HMASMIOP BASED(ADDR(TPCIOPF));                               0278 00298000
*   RFY                                                            0279 00299000
*     MCBVERMP BASED(ADDR(MCBIOP)); /* USE INPUT IOP FOR MCB AREA    */ 00300000
*   VERSRHRC=ZERO;                  /* CLEAR THE RETURN CODE         */ 00301000
*                                                                  0280 00302000
         SLR   VERSRHRC,VERSRHRC                                   0280 00303000
*   /*****************************************************************/ 00304000
*   /*                                                               */ 00305000
*   /* POINT TO PTS MCS MEMBER BASED ON LOCATE DATA OBTAINED ABOVE   */ 00306000
*   /*                                                               */ 00307000
*   /*****************************************************************/ 00308000
*                                                                  0281 00309000
*   IOPFUNCT=IOPPOSI;               /* ...                           */ 00310000
         LA    @10,TPCIOPF                                         0281 00311000
         MVI   IOPFUNCT(@10),X'02'                                 0281 00312000
*   CALL HMASMIO(HMASMIOP);         /* ...                           */ 00313000
         ST    @10,@AL00001                                        0282 00314000
         L     @15,@CV01718                                        0282 00315000
         LA    @01,@AL00001                                        0282 00316000
         BALR  @14,@15                                             0282 00317000
*   IF IOPRETRN^=IOPGOOD THEN       /* ERROR ON THE POINT?           */ 00318000
         LA    @10,TPCIOPF                                         0283 00319000
         CLI   IOPRETRN(@10),0                                     0283 00320000
         BE    @RF00283                                            0283 00321000
*     DO;                           /* YES,                          */ 00322000
*       VERSRHRC=STOPFUNC;          /* STOP THE SMP FUNCTION         */ 00323000
         LA    VERSRHRC,12                                         0285 00324000
*     END;                                                         0286 00325000
*   ELSE                            /* NO ERROR ON THE POINT         */ 00326000
*     DO;                                                          0287 00327000
*                                                                  0287 00328000
         B     @RC00283                                            0287 00329000
@RF00283 DS    0H                                                  0288 00330000
*       /*************************************************************/ 00331000
*       /*                                                           */ 00332000
*       /* PARSE MCS RECORDS UNTIL THE 'SELECTED' ++VER IS FOUND     */ 00333000
*       /*                                                           */ 00334000
*       /*************************************************************/ 00335000
*                                                                  0288 00336000
*       RIGHTVER=OFF;               /* INITIALIZE TO VER NOT YET   0288 00337000
*                                      FOUND                         */ 00338000
         NI    RIGHTVER,B'01111111'                                0288 00339000
*       VERCNT=ZERO;                /* CLEAR NUMBER OF ++VER       0289 00340000
*                                      STATEMENTS SEEN               */ 00341000
         SLR   VERCNT,VERCNT                                       0289 00342000
*       DO WHILE(VERSRHRC=ZERO      /* PARSE UNTIL ERROR             */ 00343000
*             &RIGHTVER=OFF);       /* OR CORRECT ++VER IS FOUND     */ 00344000
*                                                                  0290 00345000
         B     @DE00290                                            0290 00346000
@DL00290 DS    0H                                                  0291 00347000
*         /***********************************************************/ 00348000
*         /*                                                         */ 00349000
*         /* PARSE MODIFICATION CONTROL STATEMENTS BY CALLING        */ 00350000
*         /* PARSERTN                                                */ 00351000
*         /*                                                         */ 00352000
*         /***********************************************************/ 00353000
*                                                                  0291 00354000
*         CALL PARSERTN;                                           0291 00355000
         BAL   @14,PARSERTN                                        0291 00356000
*         RFY                                                      0292 00357000
*           RTNCODE RSTD;                                          0292 00358000
*         VERSRHRC=MAX(VERSRHRC,RTNCODE);/* SAVE RETURN CODE         */ 00359000
         CR    VERSRHRC,RTNCODE                                    0293 00360000
         BNL   *+6                                                      00361000
         LR    VERSRHRC,RTNCODE                                    0293 00362000
*         RFY                                                      0294 00363000
*           RTNCODE UNRSTD;                                        0294 00364000
*         IF VERSRHRC=ZERO THEN     /* DID PARSE WORK?               */ 00365000
         LTR   VERSRHRC,VERSRHRC                                   0295 00366000
         BNZ   @RF00295                                            0295 00367000
*           DO;                                                    0296 00368000
*                                                                  0296 00369000
*             /*******************************************************/ 00370000
*             /*                                                     */ 00371000
*             /* CHECK FOR 'SELECTED' ++VER, IF FOUND SAVE THE FMID  */ 00372000
*             /* NOTE THAT THIS ALSO POSITIONS FOR PROCESSING ANY    */ 00373000
*             /* ++IF DATA                                           */ 00374000
*             /*                                                     */ 00375000
*             /*******************************************************/ 00376000
*                                                                  0297 00377000
*             IF ADDR(MCBVERMP)->MCBTYPE=MCBVERTP THEN/* IS IT A   0297 00378000
*                                      ++VER CARD?                   */ 00379000
         L     @10,@PC00001+16                                     0297 00380000
         CLI   MCBTYPE(@10),2                                      0297 00381000
         BNE   @RF00297                                            0297 00382000
*               DO;                 /* YES,                          */ 00383000
*                 VERCNT=VERCNT+1;  /* INCREAMENT ++VER COUNTER      */ 00384000
         LA    VERCNT,1(,VERCNT)                                   0299 00385000
*                 IF VERCNT=VERNDX THEN/* IS THIS THE CORRECT ++VER? */ 00386000
         L     @05,@PC00001+8                                      0300 00387000
         SLR   @02,@02                                             0300 00388000
         IC    @02,VERNDX(,@05)                                    0300 00389000
         CR    VERCNT,@02                                          0300 00390000
         BNE   @RF00300                                            0300 00391000
*                   DO;             /* YES,                          */ 00392000
*                     RIGHTVER=ON;  /* STOP LOOKING                  */ 00393000
         OI    RIGHTVER,B'10000000'                                0302 00394000
*                     FMID=MCBSFMID;/* SAVE THE FMID, IF ANY, FROM 0303 00395000
*                                      ++VER                         */ 00396000
         MVC   FMID(7),MCBSFMID(@10)                               0303 00397000
*                   END;                                           0304 00398000
*                 ELSE                                             0305 00399000
*                   ;               /* A ++VER BUT NOT THE CORRECT 0305 00400000
*                                      ONE                           */ 00401000
@RF00300 DS    0H                                                  0306 00402000
*               END;                                               0306 00403000
*             ELSE                                                 0307 00404000
*               ;                   /* NOT A ++VER, CONTINUE SEARCH  */ 00405000
@RF00297 DS    0H                                                  0308 00406000
*           END;                                                   0308 00407000
*       END;                        /* END OF ++VER PARSE LOOP       */ 00408000
@RF00295 DS    0H                                                  0309 00409000
@DE00290 LTR   VERSRHRC,VERSRHRC                                   0309 00410000
         BNZ   @DC00290                                            0309 00411000
         TM    RIGHTVER,B'10000000'                                0309 00412000
         BZ    @DL00290                                            0309 00413000
@DC00290 DS    0H                                                  0310 00414000
*       IF VERSRHRC=STOPPTF THEN    /* SYSMOD FAILED?                */ 00415000
         CH    VERSRHRC,@CH00061                                   0310 00416000
         BNE   @RF00310                                            0310 00417000
*         DO;                       /* YES,                          */ 00418000
*                                                                  0311 00419000
*           /*********************************************************/ 00420000
*           /*                                                       */ 00421000
*           /* ISSUE A MESSAGE HMA355I 'INVALID XXXXXXXX ENTRY FOR   */ 00422000
*           /* SYSMOD XXXXXXXX ON THE XXXXXXXX'                      */ 00423000
*           /*                                                       */ 00424000
*           /*********************************************************/ 00425000
*                                                                  0312 00426000
*           DO;                     /* HMASMPMG M1(HMA355I)TYPE(ERR)V   00427000
*                                      AR(TXTMCS,SYSMOD,TXTPTS)TARGET   00428000
*                                      (SYSOUT,LOG)                  */ 00429000
*             HMASMMGP=HMASMMGP&&HMASMMGP;/* SET PARMS TO ZEROS      */ 00430000
         XC    HMASMMGP(24),HMASMMGP                               0313 00431000
*             MGPMGNO1=HMA355I;     /* SET UP MSG 1 CODE             */ 00432000
         MVC   MGPMGNO1(2),@CH01729                                0314 00433000
*             MGPTYPE=MGPERR;       /* SET MESSAGE TYPE              */ 00434000
