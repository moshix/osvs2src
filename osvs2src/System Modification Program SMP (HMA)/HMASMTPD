         BNO   @RF00207                                            0207 00420000
@RT00207 DS    0H                                                  0208 00421000
*           CSRCODE=PTFWARN;        /* INDICATE TO CALLER            */ 00422000
         LA    CSRCODE,4                                           0208 00423000
*       END;                        /*                               */ 00424000
@RF00207 DS    0H                                                  0210 00425000
*   END;                            /* ICT SYSMOD LOOP               */ 00426000
@RF00205 AH    C,@CH00061                                          0210 00427000
@DE00204 LR    @10,C                                               0210 00428000
         MH    @10,@CH01568                                        0210 00429000
         L     @01,CCAICPTF(,CCAPTR)                               0210 00430000
         AL    @01,@CF01578                                        0210 00431000
         L     @10,ICTPEND(@10,@01)                                0210 00432000
         CL    @10,ICTENDNT                                        0210 00433000
         BNE   @DL00204                                            0210 00434000
*   RETURN CODE(CSRCODE);           /*                               */ 00435000
         LR    @15,CSRCODE                                         0211 00436000
         L     @14,12(,@13)                                        0211 00437000
         LM    @00,@12,20(@13)                                     0211 00438000
         BR    @14                                                 0211 00439000
*   END CSRCHK;                     /*                               */ 00440000
*                                                                  0213 00441000
*   /*****************************************************************/ 00442000
*   /*                                                               */ 00443000
*   /* IF IN GROUP MODE THE PRE, REQ, MREQ, AND IREQ LISTS OF SYSMODS*/ 00444000
*   /* SPECIFIED IN THE GROUP LIST ARE CHECKED AGAINST THE ICT SYSMOD*/ 00445000
*   /* ENTRIES. IF THE ICT ENTRY FOR THE ASSOCIATED SYSMOD IS FOUND  */ 00446000
*   /* IN THE ICT AND IS ELIGIBLE FOR PROCESSING, IT IS MARKED AS    */ 00447000
*   /* RELAVENT. WHEN ALL SYSMODS IN THE SET HAVE BEEN PROCESSED     */ 00448000
*   /* THERE MAY BE SOME ICT SYSMOD ENTRIES NOT MARKED AS RELAVENT.  */ 00449000
*   /* EACH OF THESE NON-RELAVENT SYSMODS ARE MARKED AS ELIMATED TO  */ 00450000
*   /* PREVENT FURTHER APPLY ACCEPT PROCESSING FOR THEM. THEY ARE    */ 00451000
*   /* SYSMODS WHICH WERE PUT INTO THE ICT BECAUSE OF PRE REQ, MREQ, */ 00452000
*   /* AND IREQ RELATIONSHIPS WITH THE SYSMODS IN THE SET OR SYSMODS */ 00453000
*   /* SIMILARLY RELATED TO THEM.                                    */ 00454000
*   /*                                                               */ 00455000
