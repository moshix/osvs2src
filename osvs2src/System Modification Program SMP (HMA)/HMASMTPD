         BNE   @DL00188                                            0194 00330000
*   END;                            /* LOOP THRU ICT SYSMODS         */ 00331000
@RF00187 AH    A,@CH00061                                          0195 00332000
@DE00186 LR    @10,A                                               0195 00333000
