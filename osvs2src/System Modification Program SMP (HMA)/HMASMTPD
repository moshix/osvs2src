         L     @10,ICTPEND(@10,@03)                                0272 00885000
         CL    @10,ICTENDNT                                        0272 00886000
         BNE   @DL00263                                            0272 00887000
*   END CHKCHN2;                    /*                               */ 00888000
@EL00009 DS    0H                                                  0273 00889000
@EF00009 DS    0H                                                  0273 00890000
@ER00009 LM    @14,@12,12(@13)                                     0273 00891000
         BR    @14                                                 0273 00892000
*                                                                  0274 00893000
*   /*****************************************************************/ 00894000
*   /*                                                               */ 00895000
*   /* EACH ICT SYSMOD IS CHECKED. IF IT SUPERSEDED OTHER SYSMODS    */ 00896000
*   /* THEN THESE SUPERSEDED ENTRIES GET THEIR SUP COUNT FIELD       */ 00897000
*   /* INCREMENTED.                                                  */ 00898000
*   /*                                                               */ 00899000
*   /*****************************************************************/ 00900000
*                                                                  0274 00901000
*SUPCHK:                                                           0274 00902000
*   PROC;                           /*                               */ 00903000
SUPCHK   STM   @14,@12,@SA00010                                    0274 00904000
*   RFY                                                            0275 00905000
*     ICTPTF BASED(CCAICPTF);       /* ICT SYSMOD BASE               */ 00906000
*   RFY                                                            0276 00907000
*     ICTSUPLS BASED(ICTPSCHN(1));  /* SUP LIST BASE                 */ 00908000
*   DCL                                                            0277 00909000
*     M FIXED(31);                  /* LOOP COUNTER                  */ 00910000
*   DCL                                                            0278 00911000
*     P FIXED(31);                  /* LOOP COUNTER                  */ 00912000
*                                                                  0278 00913000
*   /*****************************************************************/ 00914000
*   /*                                                               */ 00915000
*   /* ICT SYSMOD LOOP                                               */ 00916000
*   /*                                                               */ 00917000
*   /*****************************************************************/ 00918000
*                                                                  0279 00919000
*   DO M=1 BY 1 WHILE(ICTPEND(M)^=ICTENDNT);/*                       */ 00920000
         LA    M,1                                                 0279 00921000
         B     @DE00279                                            0279 00922000
@DL00279 DS    0H                                                  0280 00923000
*     IF ICTPNOGO(M)=OFF            /* GOOD SYSMOD AND               */ 00924000
*         &ICTPSUPP(M)=ON           /* THIS SYSMOD SUPES OTHERS      */ 00925000
*         &ICTPDEXP(M)=OFF&ICTPDIMP(M)=OFF/* NOT DELETED             */ 00926000
*       THEN                        /*                               */ 00927000
*                                                                  0280 00928000
         LR    @10,M                                               0280 00929000
         MH    @10,@CH01568                                        0280 00930000
         L     @03,CCAICPTF(,CCAPTR)                               0280 00931000
         ST    @10,@TF00001                                        0280 00932000
         ALR   @10,@03                                             0280 00933000
         AL    @10,@CF01580                                        0280 00934000
         TM    ICTPNOGO-8(@10),B'01000000'                         0280 00935000
         BNZ   @RF00280                                            0280 00936000
         LR    @10,@03                                             0280 00937000
         AL    @10,@TF00001                                        0280 00938000
         AL    @10,@CF01575                                        0280 00939000
         TM    ICTPSUPP-9(@10),B'00000100'                         0280 00940000
         BNO   @RF00280                                            0280 00941000
         AL    @03,@TF00001                                        0280 00942000
         AL    @03,@CF01587                                        0280 00943000
         TM    ICTPDEXP-13(@03),B'00110000'                        0280 00944000
         BNZ   @RF00280                                            0280 00945000
*       /*************************************************************/ 00946000
*       /*                                                           */ 00947000
*       /* LOOK THRU LIST OF SUPERSEDED SYSMODS                      */ 00948000
*       /*                                                           */ 00949000
*       /*************************************************************/ 00950000
*                                                                  0281 00951000
*       DO P=1 BY 1 WHILE ICTSUPND(P)^=ICTENDIX;/*                   */ 00952000
*                                                                  0281 00953000
         LA    P,1                                                 0281 00954000
         B     @DE00281                                            0281 00955000
@DL00281 DS    0H                                                  0282 00956000
*         /***********************************************************/ 00957000
*         /*                                                         */ 00958000
*         /* SEARCH ICT SYSMODS LOOKING FOR THIS SUPED SYSMOD AND    */ 00959000
*         /* INCREMENT ITS SUPBY COUNT.                              */ 00960000
*         /*                                                         */ 00961000
*         /***********************************************************/ 00962000
*                                                                  0282 00963000
*         CALL SUPCNT(ADDR(ICTSUPLS(P)));/* PASS ADDR OF SUPED       */ 00964000
         LR    @10,P                                               0282 00965000
         SLA   @10,3                                               0282 00966000
         L     @01,CCAICPTF(,CCAPTR)                               0282 00967000
         L     @01,ICTPSCHN(,@01)                                  0282 00968000
         AL    @01,@CF01589                                        0282 00969000
         LA    @10,ICTSUPLS(@10,@01)                               0282 00970000
         ST    @10,@AFTEMPS+4                                      0282 00971000
         LA    @01,@AL00282                                        0282 00972000
         BAL   @14,SUPCNT                                          0282 00973000
*       END;                        /* SUPED LIST LOOP               */ 00974000
         AH    P,@CH00061                                          0283 00975000
@DE00281 LR    @10,P                                               0283 00976000
         SLA   @10,3                                               0283 00977000
         L     @01,CCAICPTF(,CCAPTR)                               0283 00978000
         L     @01,ICTPSCHN(,@01)                                  0283 00979000
         AL    @01,@CF01589                                        0283 00980000
         L     @10,ICTSUPND(@10,@01)                               0283 00981000
         CL    @10,ICTENDIX                                        0283 00982000
         BNE   @DL00281                                            0283 00983000
*   END;                            /* ICT SYSMOD LOOP               */ 00984000
@RF00280 AH    M,@CH00061                                          0284 00985000
@DE00279 LR    @10,M                                               0284 00986000
         MH    @10,@CH01568                                        0284 00987000
         L     @03,CCAICPTF(,CCAPTR)                               0284 00988000
         AL    @03,@CF01578                                        0284 00989000
         L     @10,ICTPEND(@10,@03)                                0284 00990000
         CL    @10,ICTENDNT                                        0284 00991000
         BNE   @DL00279                                            0284 00992000
*   END SUPCHK;                     /*                               */ 00993000
@EL00010 DS    0H                                                  0285 00994000
@EF00010 DS    0H                                                  0285 00995000
@ER00010 LM    @14,@12,@SA00010                                    0285 00996000
         BR    @14                                                 0285 00997000
*                                                                  0286 00998000
*   /*****************************************************************/ 00999000
*   /*                                                               */ 01000000
*   /* THE CALLER PASSES ADDRESS OF SUPERCEDED SYSMOD NAME WITHIN A  */ 01001000
*   /* SYSMOD SUP LIST. THE ICT SYSMODS ARE SEARCHED FOR THIS        */ 01002000
*   /* SUPERCEDED SYSMOD AND WHEN FOUND THE SUPERCEDED SYSMODS COUND */ 01003000
*   /* (SUPBY COUNT) IS INCREMENTED .                                */ 01004000
*   /*                                                               */ 01005000
*   /*****************************************************************/ 01006000
*                                                                  0286 01007000
*SUPCNT:                            /*                               */ 01008000
*   PROC(SUPADDR);                  /* ADDR OF LIST ENTRY            */ 01009000
SUPCNT   STM   @14,@12,12(@13)                                     0286 01010000
         MVC   @PC00011(4),0(@01)                                  0286 01011000
*   RFY                                                            0287 01012000
*     ICTSUPLS BASED(SUPADDR);      /* BASE TO PICK UP SUPED NAME    */ 01013000
*   DCL                                                            0288 01014000
*     SUPADDR PTR(31);              /* PTR TO SUP'D SYSMOD           */ 01015000
*   DCL                                                            0289 01016000
*     J FIXED(31);                  /* LOOP COUNTER                  */ 01017000
*   STOP=OFF;                       /* LOOP CONTROL                  */ 01018000
*                                                                  0290 01019000
         NI    STOP,B'10111111'                                    0290 01020000
*   /*****************************************************************/ 01021000
*   /*                                                               */ 01022000
*   /* ICT SYSMOD LOOP                                               */ 01023000
*   /*                                                               */ 01024000
*   /*****************************************************************/ 01025000
*                                                                  0291 01026000
*   DO J=1 BY 1 WHILE ICTPEND(J)^=ICTENDNT/*                         */ 01027000
*         &STOP=OFF;                /*                               */ 01028000
*                                                                  0291 01029000
         LA    J,1                                                 0291 01030000
         B     @DE00291                                            0291 01031000
@DL00291 DS    0H                                                  0292 01032000
*     /***************************************************************/ 01033000
*     /*                                                             */ 01034000
*     /* LOOK FOR SUPERCEDED (PARAMETER) NAME IN ICT SYSMOD SECTION  */ 01035000
*     /*                                                             */ 01036000
*     /***************************************************************/ 01037000
*                                                                  0292 01038000
*     IF ICTSUPNM(1)=ICTPTFS(J)     /* FOUND IN ICT                  */ 01039000
*         &ICTPNOGO(1)=OFF          /* NOT NOGO                      */ 01040000
*         &ICTPDEXP(1)=OFF&ICTPDIMP(1)=OFF/* NOT DELETED             */ 01041000
*       THEN                        /*                               */ 01042000
         L     @10,@PC00011                                        0292 01043000
         L     @10,SUPADDR(,@10)                                   0292 01044000
         LR    @03,J                                               0292 01045000
         MH    @03,@CH01568                                        0292 01046000
         L     @15,CCAICPTF(,CCAPTR)                               0292 01047000
         ST    @03,@TF00001                                        0292 01048000
         ALR   @03,@15                                             0292 01049000
         AL    @03,@CF01578                                        0292 01050000
         CLC   ICTSUPNM(7,@10),ICTPTFS(@03)                        0292 01051000
         BNE   @RF00292                                            0292 01052000
         TM    ICTPNOGO(@15),B'01000000'                           0292 01053000
         BNZ   @RF00292                                            0292 01054000
         TM    ICTPDEXP(@15),B'00110000'                           0292 01055000
         BNZ   @RF00292                                            0292 01056000
*       DO;                         /*                               */ 01057000
*                                                                  0293 01058000
*         /***********************************************************/ 01059000
*         /*                                                         */ 01060000
*         /* BUMP THIS FOUND ICT SYSMOD ENTRIES SUPBY COUNT BECAUSE  */ 01061000
*         /* IT HAS BEEN SUPERCEDED BY ANOTHER SYSMOD                */ 01062000
*         /*                                                         */ 01063000
*         /***********************************************************/ 01064000
*                                                                  0294 01065000
*         ICTPSUPN(J)=ICTPSUPN(J)+1;/*                               */ 01066000
         LA    @10,1                                               0294 01067000
         LR    @03,@15                                             0294 01068000
         AL    @03,@TF00001                                        0294 01069000
         AL    @03,@CF01579                                        0294 01070000
         SLR   @14,@14                                             0294 01071000
         ICM   @14,3,ICTPSUPN-20(@03)                              0294 01072000
         ALR   @10,@14                                             0294 01073000
         L     @03,@TF00001                                        0294 01074000
         AL    @15,@CF01579                                        0294 01075000
         STH   @10,ICTPSUPN-20(@03,@15)                            0294 01076000
*         STOP=ON;                  /* STOP SEARCH                   */ 01077000
         OI    STOP,B'01000000'                                    0295 01078000
*       END;                        /*                               */ 01079000
*   END;                            /* END ICT SYSMOD LOOP           */ 01080000
@RF00292 AH    J,@CH00061                                          0297 01081000
@DE00291 LR    @10,J                                               0297 01082000
         MH    @10,@CH01568                                        0297 01083000
         L     @03,CCAICPTF(,CCAPTR)                               0297 01084000
         AL    @03,@CF01578                                        0297 01085000
         L     @10,ICTPEND(@10,@03)                                0297 01086000
