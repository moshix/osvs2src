*                                                                  0197 00345000
*   /*****************************************************************/ 00346000
*   /*                                                               */ 00347000
*   /* THE IREQ LIST ELEMENT ADDRESS IS PASSED AS A PARM. THE ICT    */ 00348000
*   /* SYSMOD ENTRIES ARE CHECKED LOOKING FOR THE CAUSER OF THIS     */ 00349000
*   /* IREQ. IF THE CAUSER IS SUPED THE RETURN CODE TO CALLER IS SET */ 00350000
*   /* TO FOUR.                                                      */ 00351000
*   /*                                                               */ 00352000
*   /*****************************************************************/ 00353000
*                                                                  0197 00354000
*CSRCHK:                            /*                               */ 00355000
*   PROC(IREQADDR);                 /* A SYSMOD CHAINED IREQ ENTRY   */ 00356000
CSRCHK   STM   @14,@12,12(@13)                                     0197 00357000
         MVC   @PC00006(4),0(@01)                                  0197 00358000
*   DCL                                                            0198 00359000
*     IREQADDR PTR(31);             /* ADDRESS OF IREQ ENTRY         */ 00360000
*   DCL                                                            0199 00361000
*     C FIXED(31);                  /* LOOP COUNTER                  */ 00362000
*   DCL                                                            0200 00363000
*     CSRCODE FIXED(31);            /* SUBROUTINE RETURN CODE        */ 00364000
*   RFY                                                            0201 00365000
*     ICTPTF BASED(CCAICPTF);       /* BASE OF ICT SYSMOD ENTRIES    */ 00366000
*   RFY                                                            0202 00367000
*     ICTIRQLS BASED(IREQADDR);     /* BASE OR IREQ LIST             */ 00368000
*   CSRCODE=ZERO;                   /* INIT CODE                     */ 00369000
*                                                                  0203 00370000
         SLR   CSRCODE,CSRCODE                                     0203 00371000
*   /*****************************************************************/ 00372000
*   /*                                                               */ 00373000
*   /* ICT SYSMOD LOOP                                               */ 00374000
*   /*                                                               */ 00375000
*   /*****************************************************************/ 00376000
*                                                                  0204 00377000
*   DO C=1 BY 1 WHILE ICTPEND(C)^=ICTENDNT;/*                        */ 00378000
*                                                                  0204 00379000
         LA    C,1                                                 0204 00380000
         B     @DE00204                                            0204 00381000
@DL00204 DS    0H                                                  0205 00382000
*     /***************************************************************/ 00383000
*     /*                                                             */ 00384000
*     /* LOOK FOR IREQ CAUSER IN THE ICT SYSMOD ENTRIES              */ 00385000
*     /*                                                             */ 00386000
*     /***************************************************************/ 00387000
*                                                                  0205 00388000
*     IF ICTCSRNM(1)=ICTPTFS(C) THEN/* IF CAUSER FOUND               */ 00389000
*                                                                  0205 00390000
         L     @10,@PC00006                                        0205 00391000
         L     @10,IREQADDR(,@10)                                  0205 00392000
         LR    @15,C                                               0205 00393000
         MH    @15,@CH01568                                        0205 00394000
         L     @14,CCAICPTF(,CCAPTR)                               0205 00395000
         ST    @15,@TF00001                                        0205 00396000
         ALR   @15,@14                                             0205 00397000
         AL    @15,@CF01578                                        0205 00398000
         CLC   ICTCSRNM(7,@10),ICTPTFS(@15)                        0205 00399000
         BNE   @RF00205                                            0205 00400000
*       /*************************************************************/ 00401000
*       /*                                                           */ 00402000
*       /* SEE IF CAUSER IS STILL VALID                              */ 00403000
*       /*                                                           */ 00404000
*       /*************************************************************/ 00405000
*                                                                  0206 00406000
*       DO;                         /*                               */ 00407000
*         IF ICTPSUPN(C)^=ZERO      /* CAUSER SUPED                  */ 00408000
*             |ICTPNOGO(C)=ON THEN  /* OR NOGO                       */ 00409000
         LR    @10,@14                                             0207 00410000
         AL    @10,@TF00001                                        0207 00411000
         AL    @10,@CF01579                                        0207 00412000
