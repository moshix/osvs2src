         TITLE 'HMASMTPO - ICT SYSMOD ORDER ROUTINE'                    00001000
HMASMTPO CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMTPO  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  LA    @10,4095(,@12)                                      0001 00011000
         USING @PSTART,@12                                         0001 00012000
         USING @PSTART+4095,@10                                    0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
         MVC   @PC00001(4),0(@01)                                  0001 00018000
*   TPORTNCD=ZERO;                  /*                               */ 00019000
         SLR   TPORTNCD,TPORTNCD                                   0170 00020000
*   IOPPTR=CCAIOPTR;                /*                               */ 00021000
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0171 00022000
*   HMASMMGP=''B;                   /*                               */ 00023000
         XC    HMASMMGP(24),HMASMMGP                               0172 00024000
*   MGPPRINT=ON;                    /*                               */ 00025000
*   MGPHLDS=ON;                     /*                               */ 00026000
         OI    MGPPRINT,B'10010000'                                0174 00027000
*   LON=1;                          /* INITIAL ORDER NUMBER          */ 00028000
*                                                                  0175 00029000
         LA    LON,1                                               0175 00030000
*   /*****************************************************************/ 00031000
*   /*                                                               */ 00032000
*   /* ORDER ALL SERVICE SYSMODS THAT FMID FUNCTIONS THAT ARE NOT IN */ 00033000
*   /* THE ICT (IE THE FUNCTION IS ALREADY APPLIED OR ACCEPTED) ...  */ 00034000
*   /* EXCEPT SERVICE SYSMODS WHICH ARE BEING RE-INSTALLED. ORDERING */ 00035000
*   /* IS DONE FOR PTFS FIRST, THEN APARS, THEN USERMODS SO THAT THE */ 00036000
*   /* ORDER OF APPLICATION WILL BE IN THAT ORDER                    */ 00037000
*   /*                                                               */ 00038000
*   /*****************************************************************/ 00039000
*                                                                  0176 00040000
*   CALL TPOEFSVC(IOPPPTF);         /* PROCESS PTF TYPE SYSMODS      */ 00041000
         LA    @01,@AL00176                                        0176 00042000
         BAL   @14,TPOEFSVC                                        0176 00043000
*   CALL TPOEFSVC(IOPPAPAR);        /* PROCESS APAR TYPE SYSMODS     */ 00044000
         LA    @01,@AL00177                                        0177 00045000
         BAL   @14,TPOEFSVC                                        0177 00046000
*   CALL TPOEFSVC(IOPPUSER);        /* PROCESS USERMOD TYPE SYSMODS  */ 00047000
*                                                                  0178 00048000
         LA    @01,@AL00178                                        0178 00049000
         BAL   @14,TPOEFSVC                                        0178 00050000
*   /*****************************************************************/ 00051000
*   /*                                                               */ 00052000
*   /* NOW THAT ALL SERVICE TO EXISTING FUNCTIONS HAVE BEEN ORDERED  */ 00053000
*   /* CALL ROUTINE TO ORDER THE FUNCTION TYPE SYSMODS THAT ARE      */ 00054000
*   /* PRESENT IN THE ICT                                            */ 00055000
*   /*                                                               */ 00056000
*   /*****************************************************************/ 00057000
*                                                                  0179 00058000
*   CALL TPONF;                     /* ORDER NEW FUNCTION            */ 00059000
*                                                                  0179 00060000
         BAL   @14,TPONF                                           0179 00061000
*   /*****************************************************************/ 00062000
*   /*                                                               */ 00063000
*   /* ORDER ANY REMAINING SERVICE SYSMODS IN THE ICT ... THOSE WHICH*/ 00064000
*   /* REMAIN AT THIS POINT ARE THOSE APPLY TO NEW FUNCTIONS AND     */ 00065000
*   /* THOSE THAT ARE BEING RE-INSTALLED . ORDERING IS DONE FOR PTFS */ 00066000
*   /* FIRST, THEN APARS, THEN USERMODS SO THAT THE ORDER OF         */ 00067000
*   /* APPLICATION WILL BE IN THAT ORDER                             */ 00068000
*   /*                                                               */ 00069000
*   /*****************************************************************/ 00070000
*                                                                  0180 00071000
*   CALL TPORESVC(IOPPPTF);         /* PROCESS PTF TYPE SYSMODS      */ 00072000
         LA    @01,@AL00180                                        0180 00073000
         BAL   @14,TPORESVC                                        0180 00074000
*   CALL TPORESVC(IOPPAPAR);        /* PROCESS APAR TYPE SYSMODS     */ 00075000
         LA    @01,@AL00181                                        0181 00076000
         BAL   @14,TPORESVC                                        0181 00077000
*   CALL TPORESVC(IOPPUSER);        /* PROCESS USERMOD TYPE SYSMODS  */ 00078000
*                                                                  0182 00079000
         LA    @01,@AL00182                                        0182 00080000
         BAL   @14,TPORESVC                                        0182 00081000
*   /*****************************************************************/ 00082000
*   /*                                                               */ 00083000
*   /* AT THIS TIME EACH SYSMOD IN THE ICT SHOULD HAVE BEEN ASSIGNED */ 00084000
*   /* AN ORDER NUMBER. IF ANY SYSMOD HAS NOT BENN ASSIGNED ONE THEN */ 00085000
*   /* THERE IS INSUFFICIENT DATA PRESENT IN THE SYSMOD TO PROCESS   */ 00086000
*   /* IT. ISSUE ERROR MESSAGES                                      */ 00087000
*   /*                                                               */ 00088000
*   /*****************************************************************/ 00089000
*                                                                  0183 00090000
*   CALL TPOERRCK;                  /*                               */ 00091000
         BAL   @14,TPOERRCK                                        0183 00092000
*   RFY                                                            0184 00093000
*     RTNCODE RSTD;                 /*                               */ 00094000
*   TPORTNCD=MAX(TPORTNCD,RTNCODE); /*                               */ 00095000
         CR    TPORTNCD,RTNCODE                                    0185 00096000
         BNL   *+6                                                      00097000
         LR    TPORTNCD,RTNCODE                                    0185 00098000
*   RFY                                                            0186 00099000
*     RTNCODE UNRSTD;               /*                               */ 00100000
*                                                                  0186 00101000
*   /*****************************************************************/ 00102000
*   /*                                                               */ 00103000
*   /* RETURN TO CALLER                                              */ 00104000
*   /*                                                               */ 00105000
*   /*****************************************************************/ 00106000
*                                                                  0187 00107000
*   RETURN CODE(TPORTNCD);          /*                               */ 00108000
         LR    @15,TPORTNCD                                        0187 00109000
         L     @13,4(,@13)                                         0187 00110000
         L     @14,12(,@13)                                        0187 00111000
         LM    @00,@12,20(@13)                                     0187 00112000
         BR    @14                                                 0187 00113000
         EJECT                                                          00114000
*                                                                  0188 00115000
*   /*****************************************************************/ 00116000
*   /*                                                               */ 00117000
*   /* THIS ROUTINE ORDERS SPECIFIED TYPE SYSMODS (WHICH ARE NOT     */ 00118000
*   /* BEING RE-INSTALLED) THAT FMID FUNCTIONS THAT HAVE ALREADY BEEN*/ 00119000
*   /* PROCESSED (AND ARE NOT BEING RE-PROCESSED)                    */ 00120000
*   /*                                                               */ 00121000
*   /*****************************************************************/ 00122000
*                                                                  0188 00123000
*TPOEFSVC:                          /*                               */ 00124000
*   PROCEDURE(EFSVCTYP);            /*                               */ 00125000
TPOEFSVC STM   @14,@04,@SA00002                                    0188 00126000
         STM   @06,@12,@SA00002+28                                 0188 00127000
         MVC   @PC00002(4),0(@01)                                  0188 00128000
*   DECLARE                         /*                               */ 00129000
*     EFSVCTYP FIXED(8);            /* TYPE SYSMOD                   */ 00130000
*   DECLARE                         /*                               */ 00131000
*     P1 FIXED(31),                 /* PRIMARY INDEX THRU ICT PTFS   */ 00132000
*     P2 FIXED(31),                 /* SECONDARY INDEX THRU ICT PTFS */ 00133000
*     PX1 FIXED(31);                /* INDEX THRU PRE LIST           */ 00134000
*   RFY                                                            0191 00135000
*     ICTPTF BASED(CCAICPTF);       /*                               */ 00136000
*   RFY                                                            0192 00137000
*     ICTPRELS BASED(ICTPPCHN(P1)); /*                               */ 00138000
*   RFY                                                            0193 00139000
*     MAPICT BASED(ADDR(ICTPFLGA(P1)));/* USER FLAGS                 */ 00140000
*                                                                  0193 00141000
*   /*****************************************************************/ 00142000
*   /*                                                               */ 00143000
*   /* MARK APPLICABLE SYSMODS AS SELECTED FOR PROCESS               */ 00144000
*   /*                                                               */ 00145000
*   /*****************************************************************/ 00146000
*                                                                  0194 00147000
*   STOP=OFF;                                                      0194 00148000
         NI    STOP,B'01111111'                                    0194 00149000
*   DO WHILE(STOP=OFF);             /* LOOP TILL NO MORE FOUND       */ 00150000
         B     @DE00195                                            0195 00151000
@DL00195 DS    0H                                                  0196 00152000
*     STOP=ON;                      /* STOP LOOP AT END              */ 00153000
         OI    STOP,B'10000000'                                    0196 00154000
*     DO P1=1 BY 1 WHILE(           /* LOOP THRU ICT SYSMODS         */ 00155000
*           ICTPEND(P1)^=ICTENDNT); /* TILL END                      */ 00156000
         LA    P1,1                                                0197 00157000
         B     @DE00197                                            0197 00158000
@DL00197 DS    0H                                                  0198 00159000
*       IF ICTPTYPE(P1)=EFSVCTYP    /* IF SYSMOD IS TYPE TO BE PROC. */ 00160000
*           &ICTPREDO(P1)=OFF       /* AND IS NOT BEING RE-INSTALLED */ 00161000
*           &ONA=OFF                /* AND HAS NOT BEEN ASSIGNED NUM */ 00162000
*         THEN                      /*                               */ 00163000
         LR    @04,P1                                              0198 00164000
         MH    @04,@CH01831                                        0198 00165000
         L     @03,CCAICPTF(,CCAPTR)                               0198 00166000
         L     @15,@PC00002                                        0198 00167000
         LA    @01,0(@04,@03)                                      0198 00168000
         AL    @01,@CF01842                                        0198 00169000
         SLR   @00,@00                                             0198 00170000
         IC    @00,ICTPTYPE-17(,@01)                               0198 00171000
         SLR   @14,@14                                             0198 00172000
         IC    @14,EFSVCTYP(,@15)                                  0198 00173000
         CR    @00,@14                                             0198 00174000
         BNE   @RF00198                                            0198 00175000
         ST    @04,@TF00001                                        0198 00176000
         ALR   @04,@03                                             0198 00177000
         AL    @04,@CF01843                                        0198 00178000
         TM    ICTPREDO-13(@04),B'01000000'                        0198 00179000
         BNZ   @RF00198                                            0198 00180000
         L     @04,@TF00001                                        0198 00181000
         AL    @03,@CF01844                                        0198 00182000
         LA    @04,ICTPFLGA-16(@04,@03)                            0198 00183000
         TM    ONA(@04),B'00100000'                                0198 00184000
         BNZ   @RF00198                                            0198 00185000
*         DO;                       /*                               */ 00186000
*           DO P2=1 BY 1 WHILE(     /* LOOP THRU ICT SYSMODS LOOKING */ 00187000
*                 ICTPEND(P2)^=ICTENDNT/* FOR FUNCTION EQUAL TO FMID */ 00188000
*                 &ICTPTFS(P2)^=ICTPFMID(P1));/* OF PRIMARY SYSMOD   */ 00189000
         LA    P2,1                                                0200 00190000
         B     @DE00200                                            0200 00191000
@DL00200 DS    0H                                                  0201 00192000
*           END;                    /*                               */ 00193000
         AH    P2,@CH00063                                         0201 00194000
@DE00200 LA    @04,116                                             0201 00195000
         LR    @01,P2                                              0201 00196000
         MR    @00,@04                                             0201 00197000
         L     @15,CCAICPTF(,CCAPTR)                               0201 00198000
         LA    @14,0(@01,@15)                                      0201 00199000
         AL    @14,@CF01845                                        0201 00200000
         L     @00,ICTPEND(,@14)                                   0201 00201000
         CL    @00,ICTENDNT                                        0201 00202000
         BE    @DC00200                                            0201 00203000
         LR    @07,@04                                             0201 00204000
         MR    @06,P1                                              0201 00205000
         ALR   @01,@15                                             0201 00206000
         AL    @01,@CF01845                                        0201 00207000
         ALR   @15,@07                                             0201 00208000
         AL    @15,@CF01846                                        0201 00209000
         CLC   ICTPTFS(7,@01),ICTPFMID-22(@15)                     0201 00210000
         BNE   @DL00200                                            0201 00211000
@DC00200 DS    0H                                                  0202 00212000
*           IF ICTPEND(P2)=ICTENDNT /* IF FMID NOT FOUND OR WAS      */ 00213000
*               |SFP=ON             /* SELECTED DUE TO PRE           */ 00214000
*             THEN                  /* PRIMARY SYSMOD IS CANDIDATE   */ 00215000
         LA    @04,116                                             0202 00216000
         LR    @01,P2                                              0202 00217000
         MR    @00,@04                                             0202 00218000
         L     @15,CCAICPTF(,CCAPTR)                               0202 00219000
         AL    @01,@CF01845                                        0202 00220000
         L     @14,ICTPEND(@01,@15)                                0202 00221000
         CL    @14,ICTENDNT                                        0202 00222000
         BE    @RT00202                                            0202 00223000
         LR    @01,@04                                             0202 00224000
         MR    @00,P1                                              0202 00225000
         AL    @15,@CF01844                                        0202 00226000
         LA    @04,ICTPFLGA-16(@01,@15)                            0202 00227000
         TM    SFP(@04),B'01000000'                                0202 00228000
         BNO   @RF00202                                            0202 00229000
@RT00202 DS    0H                                                  0203 00230000
*             DO;                   /*                               */ 00231000
*               IF SFP=OFF          /* IF NOT ALREADY SELECTED       */ 00232000
*                 THEN              /*                               */ 00233000
         LR    @04,P1                                              0204 00234000
         MH    @04,@CH01831                                        0204 00235000
         L     @03,CCAICPTF(,CCAPTR)                               0204 00236000
         AL    @03,@CF01844                                        0204 00237000
         LA    @04,ICTPFLGA-16(@04,@03)                            0204 00238000
         TM    SFP(@04),B'01000000'                                0204 00239000
         BNZ   @RF00204                                            0204 00240000
*                 DO;               /*                               */ 00241000
*                   SFP=ON;         /* FOR PROCESSING                */ 00242000
         OI    SFP(@04),B'01000000'                                0206 00243000
*                   STOP=OFF;       /* CONTINUE LOOP                 */ 00244000
         NI    STOP,B'01111111'                                    0207 00245000
*                 END;              /*                               */ 00246000
*                                                                  0208 00247000
*               /*****************************************************/ 00248000
*               /*                                                   */ 00249000
*               /* NOW MARK ALL PRES (WHICH ARE NOT BEING            */ 00250000
*               /* RE-INSTALLED) AS SELECTED                         */ 00251000
*               /*                                                   */ 00252000
*               /*****************************************************/ 00253000
*                                                                  0209 00254000
*               RFY                                                0209 00255000
*                 MAPICT BASED(ADDR(ICTPFLGA(P2)));/* USER FLAGS     */ 00256000
@RF00204 DS    0H                                                  0210 00257000
*               DO PX1=1 BY 1 WHILE(/* LOOP THRU PRE LIST            */ 00258000
*                     ICTPREND(PX1)^=ICTENDIX);/*                    */ 00259000
         LA    PX1,1                                               0210 00260000
         B     @DE00210                                            0210 00261000
@DL00210 DS    0H                                                  0211 00262000
*                 DO P2=1 BY 1 WHILE(/* FIND PRE IN ICT              */ 00263000
*                       ICTPEND(P2)^=ICTENDNT/*                      */ 00264000
*                       &ICTPTFS(P2)^=ICTPRENM(PX1));/*              */ 00265000
         LA    P2,1                                                0211 00266000
         B     @DE00211                                            0211 00267000
@DL00211 DS    0H                                                  0212 00268000
*                 END;              /*                               */ 00269000
         AH    P2,@CH00063                                         0212 00270000
@DE00211 LA    @15,116                                             0212 00271000
         LR    @01,P2                                              0212 00272000
         MR    @00,@15                                             0212 00273000
         L     @14,CCAICPTF(,CCAPTR)                               0212 00274000
         LA    @09,0(@01,@14)                                      0212 00275000
         AL    @09,@CF01845                                        0212 00276000
         L     @00,ICTPEND(,@09)                                   0212 00277000
         CL    @00,ICTENDNT                                        0212 00278000
         BE    @DC00211                                            0212 00279000
         LR    @09,PX1                                             0212 00280000
         SLA   @09,3                                               0212 00281000
         ST    @14,@TF00001                                        0212 00282000
         MR    @14,P1                                              0212 00283000
         L     @14,@TF00001                                        0212 00284000
         AL    @14,@CF01848                                        0212 00285000
         L     @08,ICTPPCHN-76(@15,@14)                            0212 00286000
         L     @15,@TF00001                                        0212 00287000
         ALR   @15,@01                                             0212 00288000
         AL    @15,@CF01845                                        0212 00289000
         ALR   @08,@09                                             0212 00290000
         AL    @08,@CF01849                                        0212 00291000
         CLC   ICTPTFS(7,@15),ICTPRENM(@08)                        0212 00292000
         BNE   @DL00211                                            0212 00293000
@DC00211 DS    0H                                                  0213 00294000
*                 IF ICTPEND(P2)^=ICTENDNT/* IF PRE FOUND IN ICT     */ 00295000
*                     &ICTPREDO(P2)=OFF/* AND PRE NOT BEING        0213 00296000
*                                      RE-INSTALL                    */ 00297000
*                     &SFP=OFF      /* AND NOT ALREADY SELECTED      */ 00298000
*                   THEN            /*                               */ 00299000
         LR    @15,P2                                              0213 00300000
         MH    @15,@CH01831                                        0213 00301000
         L     @14,CCAICPTF(,CCAPTR)                               0213 00302000
         LA    @01,0(@15,@14)                                      0213 00303000
         AL    @01,@CF01845                                        0213 00304000
         L     @09,ICTPEND(,@01)                                   0213 00305000
         CL    @09,ICTENDNT                                        0213 00306000
         BE    @RF00213                                            0213 00307000
         ST    @15,@TF00001                                        0213 00308000
         ALR   @15,@14                                             0213 00309000
         AL    @15,@CF01843                                        0213 00310000
         TM    ICTPREDO-13(@15),B'01000000'                        0213 00311000
         BNZ   @RF00213                                            0213 00312000
         L     @15,@TF00001                                        0213 00313000
         AL    @14,@CF01844                                        0213 00314000
         LA    @15,ICTPFLGA-16(@15,@14)                            0213 00315000
         TM    SFP(@15),B'01000000'                                0213 00316000
         BNZ   @RF00213                                            0213 00317000
*                   DO;             /*                               */ 00318000
*                     SFP=ON;       /* MARK AS SELECTED              */ 00319000
         OI    SFP(@15),B'01000000'                                0215 00320000
*                     STOP=OFF;     /* CONTINUE LOOP THRU PTFS       */ 00321000
         NI    STOP,B'01111111'                                    0216 00322000
*                   END;            /*                               */ 00323000
*               END;                /* END LOOP THRU PRE LIST        */ 00324000
@RF00213 AH    PX1,@CH00063                                        0218 00325000
@DE00210 LR    @03,PX1                                             0218 00326000
         SLA   @03,3                                               0218 00327000
         LR    @15,P1                                              0218 00328000
         MH    @15,@CH01831                                        0218 00329000
         L     @01,CCAICPTF(,CCAPTR)                               0218 00330000
         AL    @01,@CF01848                                        0218 00331000
         L     @01,ICTPPCHN-76(@15,@01)                            0218 00332000
         AL    @01,@CF01849                                        0218 00333000
         L     @03,ICTPREND(@03,@01)                               0218 00334000
         CL    @03,ICTENDIX                                        0218 00335000
         BNE   @DL00210                                            0218 00336000
*             END;                  /* END FMID NOT FOUND            */ 00337000
*         END;                      /* END TYPE MATCH                */ 00338000
@RF00202 DS    0H                                                  0221 00339000
*     END;                          /* END ICT SYSMOD LOOP           */ 00340000
@RF00198 AH    P1,@CH00063                                         0221 00341000
@DE00197 LR    @04,P1                                              0221 00342000
         MH    @04,@CH01831                                        0221 00343000
         L     @03,CCAICPTF(,CCAPTR)                               0221 00344000
         AL    @03,@CF01845                                        0221 00345000
         L     @04,ICTPEND(@04,@03)                                0221 00346000
         CL    @04,ICTENDNT                                        0221 00347000
         BNE   @DL00197                                            0221 00348000
*   END;                            /* END DO WHILE STOP LOOP        */ 00349000
*                                                                  0222 00350000
@DE00195 TM    STOP,B'10000000'                                    0222 00351000
         BZ    @DL00195                                            0222 00352000
*   /*****************************************************************/ 00353000
*   /*                                                               */ 00354000
*   /* NOW THAT ALL APPLICABLE SYSMODS HAVE BEEN DETERMINED CALL     */ 00355000
*   /* ROUTINE TO ORDER THOSE SYSMODS ACCORDING TO RELATIONSHIPS     */ 00356000
*   /* SPECIFIED IN SYSMOD VER STATEMENT                             */ 00357000
*   /*                                                               */ 00358000
*   /*****************************************************************/ 00359000
*                                                                  0223 00360000
*   CALL TPOORDER;                  /*                               */ 00361000
         BAL   @14,TPOORDER                                        0223 00362000
*   RETURN CODE(ZERO);              /*                               */ 00363000
         SLR   @15,@15                                             0224 00364000
         L     @14,@SA00002                                        0224 00365000
         LM    @00,@04,@SA00002+8                                  0224 00366000
         LM    @06,@12,@SA00002+28                                 0224 00367000
         BR    @14                                                 0224 00368000
*   END TPOEFSVC;                   /*                               */ 00369000
         EJECT                                                          00370000
*                                                                  0226 00371000
*   /*****************************************************************/ 00372000
*   /*                                                               */ 00373000
*   /* THIS ROUTINE IS CALLED TO ASSIGN AN ORDER NUMBER TO ALL       */ 00374000
*   /* FUNCTION TYPE SYSMODS PRESENT IN THE ICT.                     */ 00375000
*   /*                                                               */ 00376000
*   /*****************************************************************/ 00377000
*                                                                  0226 00378000
*TPONF:                             /*                               */ 00379000
*   PROCEDURE;                      /*                               */ 00380000
TPONF    STM   @14,@04,@SA00003                                    0226 00381000
         STM   @06,@12,@SA00003+28                                 0226 00382000
*   DECLARE                         /*                               */ 00383000
*     P3 FIXED(31),                 /* PRIMARY INDEX THRU ICT        */ 00384000
*     P4 FIXED(31),                 /* SECONDARY INDEX THRU ICT      */ 00385000
*     PX3 FIXED(31);                /* INDEX THRU SYSMOD REQ LIST    */ 00386000
*   RFY                                                            0228 00387000
*     ICTPTF BASED(CCAICPTF);       /*                               */ 00388000
*   RFY                                                            0229 00389000
*     ICTPRELS BASED(ICTPPCHN(P3)); /* PRE LIST BASE                 */ 00390000
*   RFY                                                            0230 00391000
*     MAPICT BASED(ADDR(ICTPFLGA(P3)));/* USER FLAGS                 */ 00392000
*                                                                  0230 00393000
*   /*****************************************************************/ 00394000
*   /*                                                               */ 00395000
*   /* LOOP LOOKING FOR FUNCTION TYPE SYSMODS AND ALL ITS PRES UNTIL */ 00396000
*   /* NO MORE ARE FOUND                                             */ 00397000
*   /*                                                               */ 00398000
*   /*****************************************************************/ 00399000
*                                                                  0231 00400000
*   STOP=OFF;                                                      0231 00401000
         NI    STOP,B'01111111'                                    0231 00402000
*   DO WHILE(STOP=OFF);             /* LOOP TILL NO MORE FOUND       */ 00403000
         B     @DE00232                                            0232 00404000
@DL00232 DS    0H                                                  0233 00405000
*     STOP=ON;                      /* STOP LOOP AT END              */ 00406000
         OI    STOP,B'10000000'                                    0233 00407000
*     DO P3=1 BY 1 WHILE(           /* LOOP THRU ICT SYSMODS         */ 00408000
*           ICTPEND(P3)^=ICTENDNT); /* TILL END                      */ 00409000
         LA    P3,1                                                0234 00410000
         B     @DE00234                                            0234 00411000
@DL00234 DS    0H                                                  0235 00412000
*       IF(ICTPTYPE(P3)=IOPPFUNC    /* IF SYSMOD IS FUNCTION         */ 00413000
*           &ONA=OFF)               /* AND HAS NOT BEEN ASSIGNED NUM */ 00414000
*           |SFP=ON                 /* OR HAS BEEN SELECTED          */ 00415000
*         THEN                      /*                               */ 00416000
         LR    @04,P3                                              0235 00417000
         MH    @04,@CH01831                                        0235 00418000
         L     @03,CCAICPTF(,CCAPTR)                               0235 00419000
         LA    @01,0(@04,@03)                                      0235 00420000
         AL    @01,@CF01842                                        0235 00421000
         SLR   @00,@00                                             0235 00422000
         IC    @00,ICTPTYPE-17(,@01)                               0235 00423000
         CH    @00,@CH00037                                        0235 00424000
