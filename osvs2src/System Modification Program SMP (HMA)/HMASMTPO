*                                                                  0293 00756000
*   /*****************************************************************/ 00757000
*   /*                                                               */ 00758000
*   /* THIS ROUTINE IS CALLED TO ASSIGN AN ORDER NUMBER TO EACH      */ 00759000
*   /* SYSMOD IN THE ICT THAT HAS BEEN SELECTED FOR PROCESSING. ORDER*/ 00760000
*   /* NUMBER IS ASSIGNED BY USING THE PRE, VERSION, AND FMID VALUES */ 00761000
*   /* IN THE SYSMOD ENTRY                                           */ 00762000
*   /*                                                               */ 00763000
*   /*****************************************************************/ 00764000
*                                                                  0293 00765000
*TPOORDER:                          /*                               */ 00766000
*   PROCEDURE;                      /*                               */ 00767000
TPOORDER STM   @14,@04,12(@13)                                     0293 00768000
         STM   @06,@12,44(@13)                                     0293 00769000
*   DECLARE                         /*                               */ 00770000
*     P7 FIXED(31),                 /*                               */ 00771000
*     P8 FIXED(31),                 /*                               */ 00772000
*     P9 FIXED(31),                 /*                               */ 00773000
*     PX8 FIXED(31);                /*                               */ 00774000
*   DECLARE                         /*                               */ 00775000
*     1 ORDERSW BIT(8),             /*                               */ 00776000
*       3 ORDSTOP BIT(1),           /*                               */ 00777000
*       3 ORDSTOP2 BIT(1),          /*                               */ 00778000
*       3 SFPFND BIT(1),            /*                               */ 00779000
*       3 ONAASN BIT(1),            /*                               */ 00780000
*       3 * BIT(4);                 /*                               */ 00781000
*   RFY                                                            0296 00782000
*     ICTPTF BASED(CCAICPTF);       /*                               */ 00783000
*   RFY                                                            0297 00784000
*     ICTPRELS BASED(ICTPPCHN(P8)); /* PRE LIST                      */ 00785000
*   RFY                                                            0298 00786000
*     ICTVRSLS BASED(ICTPVCHN(P8)); /* VERSION LIST                  */ 00787000
*   RFY                                                            0299 00788000
*     MAPICT BASED(ADDR(ICTPFLGA(P8)));/* USER FLAGS                 */ 00789000
*   ORDSTOP=OFF;                    /* INIT LOOP CONTROL SW          */ 00790000
         NI    ORDSTOP,B'01111111'                                 0300 00791000
*   DO WHILE ORDSTOP=OFF;           /* CONTINUE TILL SW RESET        */ 00792000
         B     @DE00301                                            0301 00793000
@DL00301 DS    0H                                                  0302 00794000
*     ORDSTOP=ON;                   /* STOP LOOP UNLESS RESET        */ 00795000
*     SFPFND=OFF;                   /* NO SELECTED SYSMODS FOUND     */ 00796000
*     ONAASN=OFF;                   /* NO SYSMODS ORDERED            */ 00797000
*                                                                  0304 00798000
         OI    ORDSTOP,B'10000000'                                 0304 00799000
         NI    SFPFND,B'11001111'                                  0304 00800000
*     /***************************************************************/ 00801000
*     /*                                                             */ 00802000
*     /* LOOP THRU SYSMODS UNTIL END OR UNTIL ALL SYSMODS SEARCHED   */ 00803000
*     /* WITHOUT FINDING ANOTHER SELECTED SYSMOD THAT IS APPLICABLE. */ 00804000
*     /* THIS IS CONTROLLED BY OUTER LOOP (ORDSTOP) AND SYSMOD FOUND */ 00805000
*     /* SW (SFPFND)                                                 */ 00806000
*     /*                                                             */ 00807000
*     /***************************************************************/ 00808000
*                                                                  0305 00809000
*     DO P7=1 BY 1 WHILE(           /* LOOP THRU ALL SYSMODS         */ 00810000
*           ICTPEND(P7)^=ICTENDNT); /*                               */ 00811000
*                                                                  0305 00812000
         LA    P7,1                                                0305 00813000
         B     @DE00305                                            0305 00814000
@DL00305 DS    0H                                                  0306 00815000
*       /*************************************************************/ 00816000
*       /*                                                           */ 00817000
*       /* FIND NEXT SELECT SYSMOD WITHOUT ORDER NUMBER ASSIGNED     */ 00818000
