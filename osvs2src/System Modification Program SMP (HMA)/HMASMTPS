         TITLE 'HMASMTPS - SYSMOD-TO-MODID VALIDITY CHECK PROCEDURE'    00001000
HMASMTPS CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMTPS  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         L     @00,@SIZDATD                                        0001 00013000
         GETMAIN  R,LV=(0)                                              00014000
         LR    @10,@01                                             0001 00015000
         USING @DATD,@10                                           0001 00016000
         ST    @13,@SA00001+4                                      0001 00017000
         LM    @00,@01,20(@13)                                     0001 00018000
         ST    @10,8(,@13)                                         0001 00019000
         LR    @13,@10                                             0001 00020000
         MVC   @PC00001(12),0(@01)                                 0001 00021000
*                                                                  0148 00022000
*   /*****************************************************************/ 00023000
*   /*                                                               */ 00024000
*   /* MAIN MODULE SEGMENT FOR HMASMTPS - FMID PRE/SUP CHECK         */ 00025000
*   /*                                                               */ 00026000
*   /*****************************************************************/ 00027000
*                                                                  0148 00028000
*   TPSFLGS=''B;                    /* CLEAR LOCAL FLAGS             */ 00029000
         MVI   TPSFLGS,X'00'                                       0148 00030000
*   RELATION=INVALID;               /* INITIALIZE CALLERS PARAMETER  */ 00031000
         L     @15,@PC00001+8                                      0149 00032000
         MVI   RELATION(@15),C'N'                                  0149 00033000
*   RFY                                                            0150 00034000
*     ICTPTF BASED(PKG1@);          /* BASE ON SYSMOD POINTER PASSED */ 00035000
*                                                                  0150 00036000
*   /*****************************************************************/ 00037000
*   /*                                                               */ 00038000
*   /* DETERMINE IF PKG1 IS EQUAL TO THE MODID PASSED OR IF PKG1'S   */ 00039000
*   /* FMID IS EQUAL TO THE MODID PASSED.                            */ 00040000
*   /*                                                               */ 00041000
*   /*****************************************************************/ 00042000
*                                                                  0151 00043000
*   IF ICTPTFS(1)=MODID             /* SYSMOD NUMBER OR FMID EQUAL   */ 00044000
*       |ICTPFMID(1)=MODID THEN     /* TO PASSED MODID?              */ 00045000
         L     @15,@PC00001                                        0151 00046000
         L     @15,PKG1@(,@15)                                     0151 00047000
         L     @14,@PC00001+4                                      0151 00048000
         CLC   ICTPTFS(7,@15),MODID(@14)                           0151 00049000
         BE    @RT00151                                            0151 00050000
         CLC   ICTPFMID(7,@15),MODID(@14)                          0151 00051000
         BNE   @RF00151                                            0151 00052000
@RT00151 DS    0H                                                  0152 00053000
*     DO;                                                          0152 00054000
*       RELATION=PRESUP;            /* INDICATE A VALID RELATIONSHIP */ 00055000
         L     @15,@PC00001+8                                      0153 00056000
         MVI   RELATION(@15),C'Y'                                  0153 00057000
*       RETURN;                     /* RETURN TO CALLER              */ 00058000
@EL00001 L     @13,4(,@13)                                         0154 00059000
@EF00001 L     @00,@SIZDATD                                        0154 00060000
         LR    @01,@10                                             0154 00061000
         FREEMAIN R,LV=(0),A=(1)                                        00062000
@ER00001 LM    @14,@12,12(@13)                                     0154 00063000
         BR    @14                                                 0154 00064000
*     END;                                                         0155 00065000
*   RFY                                                            0156 00066000
*     ICTENTLS BASED(PSPTR(L));     /* BASE GENERAL ENTRY MAPPING    */ 00067000
*                                                                  0156 00068000
@RF00151 DS    0H                                                  0157 00069000
*   /*****************************************************************/ 00070000
*   /*                                                               */ 00071000
*   /* DETERMINE IF MODID IS MENTIONED IN EITHER PKG1'S PREREQ OR    */ 00072000
*   /* SUPERSEDE CHAIN LIST. PREREQ CHAIN IS TESTED WHEN L=1 AND     */ 00073000
*   /* SUPERSEDE CHAIN IS TESTED WHEN L=2.                           */ 00074000
*   /*                                                               */ 00075000
*   /*****************************************************************/ 00076000
*                                                                  0157 00077000
*   PSPTR(1)=ICTPPCHN(1);           /* ADDRESS OF PREREQ LIST        */ 00078000
         L     @15,@PC00001                                        0157 00079000
         L     @15,PKG1@(,@15)                                     0157 00080000
         MVC   PSPTR(4),ICTPPCHN(@15)                              0157 00081000
*   PSPTR(2)=ICTPSCHN(1);           /* ADDRESS OF SUPERSEDE LIST     */ 00082000
         MVC   PSPTR+4(4),ICTPSCHN(@15)                            0158 00083000
*   DO L=1 TO 2;                    /* PROCESS PRE AND SUP LISTS     */ 00084000
         LA    L,1                                                 0159 00085000
@DL00159 DS    0H                                                  0160 00086000
*     DO M=1 BY 1 WHILE             /* PROCESS EACH LIST TILL        */ 00087000
*           ICTENTND(M)^=ICTENDIX;  /* END OF LIST REACHED           */ 00088000
         LA    M,1                                                 0160 00089000
         B     @DE00160                                            0160 00090000
@DL00160 DS    0H                                                  0161 00091000
*       IF ICTENTNM(M)=MODID THEN   /* ENTRY MATCH MODID PASSED?     */ 00092000
         LR    @15,M                                               0161 00093000
         SLA   @15,3                                               0161 00094000
         LR    @14,L                                               0161 00095000
         SLA   @14,2                                               0161 00096000
         L     @01,PSPTR-4(@14)                                    0161 00097000
         L     @14,@PC00001+4                                      0161 00098000
         ALR   @01,@15                                             0161 00099000
         AL    @01,@CF01426                                        0161 00100000
         CLC   ICTENTNM(7,@01),MODID(@14)                          0161 00101000
         BNE   @RF00161                                            0161 00102000
*         DO;                                                      0162 00103000
*           RELATION=PRESUP;        /* INDICATE A VALID RELATIONSHIP */ 00104000
         L     @15,@PC00001+8                                      0163 00105000
         MVI   RELATION(@15),C'Y'                                  0163 00106000
*           RETURN;                 /* RETURN TO CALLER              */ 00107000
         B     @EL00001                                            0164 00108000
*         END;                                                     0165 00109000
*     END;                                                         0166 00110000
@RF00161 AH    M,@CH00040                                          0166 00111000
@DE00160 LR    @15,M                                               0166 00112000
         SLA   @15,3                                               0166 00113000
         LR    @14,L                                               0166 00114000
         SLA   @14,2                                               0166 00115000
         L     @01,PSPTR-4(@14)                                    0166 00116000
         AL    @01,@CF01426                                        0166 00117000
         L     @15,ICTENTND(@15,@01)                               0166 00118000
         CL    @15,ICTENDIX                                        0166 00119000
         BNE   @DL00160                                            0166 00120000
*   END;                                                           0167 00121000
         AH    L,@CH00040                                          0167 00122000
         CH    L,@CH00187                                          0167 00123000
         BNH   @DL00159                                            0167 00124000
*                                                                  0168 00125000
*   /*****************************************************************/ 00126000
*   /*                                                               */ 00127000
*   /* FOR EACH ENTRY IN THE PRE/SUP CHAIN, SET UP TO SEE IF THAT    */ 00128000
*   /* SYSMOD HAS A ICT PTF SECTION ENTRY SO THAT THE NEXT LOWER     */ 00129000
*   /* LEVEL OF PRE/SUP CHAINS CAN BE TESTED.                        */ 00130000
*   /*                                                               */ 00131000
*   /*****************************************************************/ 00132000
*                                                                  0168 00133000
*   IF ICTPTPS(1)=ON                /* RECURSION FLAG SET IN CURRENT    00134000
*                                      ENTRY                         */ 00135000
*     THEN                                                         0168 00136000
         L     @15,@PC00001                                        0168 00137000
         L     @01,PKG1@(,@15)                                     0168 00138000
         TM    ICTPTPS(@01),B'00100000'                            0168 00139000
         BNO   @RF00168                                            0168 00140000
*     LOCALTPS=ON;                  /* SET LOCAL FLAG                */ 00141000
         OI    LOCALTPS,B'10000000'                                0169 00142000
*   ELSE                                                           0170 00143000
*     LOCALTPS=OFF;                 /* CLEAR LOCAL FLAG              */ 00144000
         B     @RC00168                                            0170 00145000
@RF00168 NI    LOCALTPS,B'01111111'                                0170 00146000
*   ICTPTPS(1)=ON;                  /* SET RECURSIVE LOOP FLAG IN  0171 00147000
*                                      CURRENT ENTRY                 */ 00148000
@RC00168 L     @15,@PC00001                                        0171 00149000
         L     @01,PKG1@(,@15)                                     0171 00150000
         OI    ICTPTPS(@01),B'00100000'                            0171 00151000
*   DO L=1 TO 2;                    /* PROCESS EACH ENTRY IN PREREQ  */ 00152000
         LA    L,1                                                 0172 00153000
@DL00172 DS    0H                                                  0173 00154000
*     DO P=1 BY 1 WHILE             /* AND SUPERSEDE LIST TILL END   */ 00155000
*           ICTENTND(P)^=ICTENDIX;  /* OF LIST REACHED               */ 00156000
         LA    P,1                                                 0173 00157000
         B     @DE00173                                            0173 00158000
@DL00173 DS    0H                                                  0174 00159000
*       LISTPKG=ICTENTNM(P);        /* SET UP TO SEARCH ICT          */ 00160000
         LR    @05,P                                               0174 00161000
         SLA   @05,3                                               0174 00162000
         LR    @02,L                                               0174 00163000
         SLA   @02,2                                               0174 00164000
         L     @02,PSPTR-4(@02)                                    0174 00165000
         ALR   @02,@05                                             0174 00166000
         AL    @02,@CF01426                                        0174 00167000
         MVC   LISTPKG(7),ICTENTNM(@02)                            0174 00168000
*       RFY                                                        0175 00169000
*         ICTPTF BASED(CCAICPTF);   /* POINT TO START OF ICT PTFS    */ 00170000
*                                                                  0175 00171000
*       /*************************************************************/ 00172000
*       /*                                                           */ 00173000
*       /* FOR EACH ENTRY IN THE PRE/SUP CHAIN, LOOK FOR A MATCHING  */ 00174000
*       /* ENTRY IN THE ICT PTF SECTION.                             */ 00175000
*       /*                                                           */ 00176000
*       /*************************************************************/ 00177000
*                                                                  0176 00178000
*       DO N=1 BY 1 WHILE           /* PROCESS EACH ENTRY IN ICT PTF */ 00179000
*             ICTPEND(N)^=ICTENDNT; /* SECTION TILL END OF LIST      */ 00180000
         LA    N,1                                                 0176 00181000
         B     @DE00176                                            0176 00182000
@DL00176 DS    0H                                                  0177 00183000
*         IF LISTPKG=ICTPTFS(N)     /* DOES ICT PTF ENTRY EXIST      */ 00184000
*             &ICTPDEXP(N)=OFF      /* ... NOT EXPLICITLY DELETED    */ 00185000
*             &ICTPDIMP(N)=OFF      /* ... NOT IMPLICITLY DELETED    */ 00186000
*           THEN                                                   0177 00187000
         LR    @05,N                                               0177 00188000
         MH    @05,@CH01424                                        0177 00189000
         L     @15,CCAICPTF(,CCAPTR)                               0177 00190000
         ST    @05,@TF00001                                        0177 00191000
         ALR   @05,@15                                             0177 00192000
         AL    @05,@CF01430                                        0177 00193000
         CLC   LISTPKG(7),ICTPTFS(@05)                             0177 00194000
         BNE   @RF00177                                            0177 00195000
         LR    @05,@15                                             0177 00196000
         AL    @05,@TF00001                                        0177 00197000
         AL    @05,@CF01431                                        0177 00198000
         TM    ICTPDEXP-13(@05),B'00110000'                        0177 00199000
         BNZ   @RF00177                                            0177 00200000
*           DO;                     /*                               */ 00201000
*                                                                  0178 00202000
*             /*******************************************************/ 00203000
*             /*                                                     */ 00204000
*             /* CHECK FOR INVALID CHAIN AS DESCRIBED IN DEPENDENCIES*/ 00205000
*             /*                                                     */ 00206000
*             /*******************************************************/ 00207000
*                                                                  0179 00208000
*             IF ICTPTPS(N)=ON THEN /* CHECK FOR RECURSIVE BIT ON    */ 00209000
         AL    @15,@TF00001                                        0179 00210000
         AL    @15,@CF01432                                        0179 00211000
         TM    ICTPTPS-12(@15),B'00100000'                         0179 00212000
         BNO   @RF00179                                            0179 00213000
*               DO;                 /* INVALID PRE/SUP CHAIN         */ 00214000
*                 RELATION=INVALID; /* SET INVALID RELATIONSHIP      */ 00215000
         L     @15,@PC00001+8                                      0181 00216000
         MVI   RELATION(@15),C'N'                                  0181 00217000
*                 IF LOCALTPS=OFF   /* ICTPTPS NOT SET ON ENTRY      */ 00218000
*                   THEN                                           0182 00219000
         TM    LOCALTPS,B'10000000'                                0182 00220000
         BNZ   @RF00182                                            0182 00221000
*                   PKG1@->ICTPTPS(1)=OFF;/* CLEAR RECURSION FLAG    */ 00222000
         L     @15,@PC00001                                        0183 00223000
         L     @01,PKG1@(,@15)                                     0183 00224000
         NI    ICTPTPS(@01),B'11011111'                            0183 00225000
*                 RETURN;                                          0184 00226000
         B     @EL00001                                            0184 00227000
*               END;                /*                               */ 00228000
*                                                                  0185 00229000
*             /*******************************************************/ 00230000
*             /*                                                     */ 00231000
*             /* CALL HMASMTPS PASSING IT A POINTER TO THE LOWER     */ 00232000
*             /* LEVEL SYSMOD'S ICT PTF ENTRY.                       */ 00233000
*             /*                                                     */ 00234000
*             /*******************************************************/ 00235000
*                                                                  0186 00236000
*             CALL HMASMTPS(ADDR(ICTPTFS(N)),MODID,RELATION);/*      */ 00237000
@RF00179 LR    @05,N                                               0186 00238000
         MH    @05,@CH01424                                        0186 00239000
         L     @01,CCAICPTF(,CCAPTR)                               0186 00240000
         AL    @01,@CF01430                                        0186 00241000
         LA    @05,ICTPTFS(@05,@01)                                0186 00242000
         ST    @05,@AFTEMPS                                        0186 00243000
         LA    @05,@AFTEMPS                                        0186 00244000
         ST    @05,@AL00001                                        0186 00245000
         L     @05,@PC00001+4                                      0186 00246000
         ST    @05,@AL00001+4                                      0186 00247000
         L     @05,@PC00001+8                                      0186 00248000
         ST    @05,@AL00001+8                                      0186 00249000
         LA    @01,@AL00001                                        0186 00250000
         L     @15,@CA00028                                        0186 00251000
         BALR  @14,@15                                             0186 00252000
*             IF RELATION=PRESUP THEN/* STOP IF MATCH IS FOUND       */ 00253000
         L     @05,@PC00001+8                                      0187 00254000
         CLI   RELATION(@05),C'Y'                                  0187 00255000
         BNE   @RF00187                                            0187 00256000
*               DO;                 /* SET UP TO RETURN              */ 00257000
*                 IF LOCALTPS=OFF   /* ICTPTPS NOT SET ON ENTRY      */ 00258000
*                   THEN                                           0189 00259000
         TM    LOCALTPS,B'10000000'                                0189 00260000
         BNZ   @RF00189                                            0189 00261000
*                   PKG1@->ICTPTPS(1)=OFF;/* CLEAR RECURSION FLAG    */ 00262000
         L     @15,@PC00001                                        0190 00263000
         L     @01,PKG1@(,@15)                                     0190 00264000
         NI    ICTPTPS(@01),B'11011111'                            0190 00265000
*                 RETURN;           /* RETURN TO CALLER WITH ANS=Y   */ 00266000
         B     @EL00001                                            0191 00267000
*               END;                /*                               */ 00268000
*           END;                    /*                               */ 00269000
@RF00187 DS    0H                                                  0194 00270000
*       END;                        /*                               */ 00271000
@RF00177 AH    N,@CH00040                                          0194 00272000
@DE00176 LR    @05,N                                               0194 00273000
         MH    @05,@CH01424                                        0194 00274000
         L     @01,CCAICPTF(,CCAPTR)                               0194 00275000
         AL    @01,@CF01430                                        0194 00276000
         L     @05,ICTPEND(@05,@01)                                0194 00277000
         CL    @05,ICTENDNT                                        0194 00278000
         BNE   @DL00176                                            0194 00279000
*       RFY                                                        0195 00280000
*         ICTPTF BASED(PKG1@);      /* BASE BACK ON SINGLE ENTRY     */ 00281000
*     END;                          /*                               */ 00282000
         AH    P,@CH00040                                          0196 00283000
@DE00173 LR    @05,P                                               0196 00284000
         SLA   @05,3                                               0196 00285000
         LR    @02,L                                               0196 00286000
         SLA   @02,2                                               0196 00287000
         L     @02,PSPTR-4(@02)                                    0196 00288000
         AL    @02,@CF01426                                        0196 00289000
         L     @05,ICTENTND(@05,@02)                               0196 00290000
         CL    @05,ICTENDIX                                        0196 00291000
         BNE   @DL00173                                            0196 00292000
*   END;                            /*                               */ 00293000
         AH    L,@CH00040                                          0197 00294000
         CH    L,@CH00187                                          0197 00295000
         BNH   @DL00172                                            0197 00296000
*   IF LOCALTPS=OFF                 /* ICTPTPS NOT SET ON ENTRY      */ 00297000
*     THEN                                                         0198 00298000
         TM    LOCALTPS,B'10000000'                                0198 00299000
         BNZ   @RF00198                                            0198 00300000
*     PKG1@->ICTPTPS(1)=OFF;        /* CLEAR RECURSION FLAG          */ 00301000
         L     @15,@PC00001                                        0199 00302000
         L     @01,PKG1@(,@15)                                     0199 00303000
         NI    ICTPTPS(@01),B'11011111'                            0199 00304000
*   RETURN CODE(ZERO);              /*                               */ 00305000
@RF00198 L     @13,4(,@13)                                         0200 00306000
         L     @00,@SIZDATD                                        0200 00307000
         LR    @01,@10                                             0200 00308000
         FREEMAIN R,LV=(0),A=(1)                                        00309000
         SLR   @15,@15                                             0200 00310000
         L     @14,12(,@13)                                        0200 00311000
         LM    @00,@12,20(@13)                                     0200 00312000
         BR    @14                                                 0200 00313000
*   END HMASMTPS                    /*                               */ 00314000
*                                                                  0201 00315000
