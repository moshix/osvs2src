*                                                                  0147 00598000
*CKICTSUP:                                                         0147 00599000
*   PROC(LEPTR2,REQTYPE2);                                         0147 00600000
CKICTSUP STM   @14,@12,@SA00004                                    0147 00601000
         MVC   @PC00004(8),0(@01)                                  0147 00602000
*   DCL                                                            0148 00603000
*     LEPTR2 PTR(31);               /* PTR TO REQ LIST ENTRY FOR   0148 00604000
*                                      REQUISITE TO BE CHECKED       */ 00605000
*   DCL                                                            0149 00606000
*     REQTYPE2 FIXED(8);            /* REQUISITE TYPE (PRE REQ IRQ)  */ 00607000
*   DCL                                                            0150 00608000
*     SPTR PTR(31);                 /* PTR TO ICT PTF SECTION ENTRY     00609000
*                                      FOR SUPERSEDER OF REQ (IF   0150 00610000
*                                      FOUND IN ICT)                 */ 00611000
*   RESPECIFY                                                      0151 00612000
*     ICTENTLS BASED(LEPTR2);       /* ADDRESS REQ LIST ENTRY PASSED */ 00613000
*   RESPECIFY                                                      0152 00614000
*     ICTPTF BASED(PTFPTR);         /* ADDRESS PTF PASSED TO TR1     */ 00615000
*   SPTR=0;                         /* INITIALIZE PTR SO SUPICT PROC    00616000
*                                      STARTS LOOKING FOR SUPS AT THE   00617000
*                                      BEGINNING OF THE ICT.         */ 00618000
         SLR   @15,@15                                             0153 00619000
         ST    @15,SPTR                                            0153 00620000
*   CALL SUPICT(ICTENTNM(1),SPTR);  /* CHECK ICT FOR SUPING PTF      */ 00621000
         L     @15,@PC00004                                        0154 00622000
         L     @01,LEPTR2(,@15)                                    0154 00623000
         ST    @01,@AL00001                                        0154 00624000
         LA    @15,SPTR                                            0154 00625000
         ST    @15,@AL00001+4                                      0154 00626000
         LA    @01,@AL00001                                        0154 00627000
         BAL   @14,SUPICT                                          0154 00628000
*   DO WHILE ICTEREQS(1)=OFF        /* REQ NOT SATISFIED             */ 00629000
*         &SPTR^=0;                 /* SUPERSEDER FOUND IN ICT       */ 00630000
*                                                                  0155 00631000
         B     @DE00155                                            0155 00632000
@DL00155 DS    0H                                                  0156 00633000
*     /***************************************************************/ 00634000
*     /*                                                             */ 00635000
*     /* CHECK A SYSMOD (FOUND IN THE ICT) WHICH SUPERSEDES THE      */ 00636000
*     /* REQUISITE                                                   */ 00637000
*     /*                                                             */ 00638000
*     /***************************************************************/ 00639000
*                                                                  0156 00640000
*     IF REQTYPE2=IRQ               /* IFREQ REQUISITE               */ 00641000
*         &ICTENTNM(2)=ICTPTFS(1)   /* CAUSED BY "SELF"              */ 00642000
*         &SPTR=PTFPTR              /* SUPERSEDING SYSMOD IS "SELF"  */ 00643000
*       THEN                                                       0156 00644000
         L     @15,@PC00004+4                                      0156 00645000
         CLI   REQTYPE2(@15),3                                     0156 00646000
         BNE   @RF00156                                            0156 00647000
