         OI    ICTEREQS(@01),B'00010000'                           0170 00724000
*               END;                                               0171 00725000
@RF00169 DS    0H                                                  0172 00726000
*           END;                                                   0172 00727000
@RC00165 DS    0H                                                  0173 00728000
*       END;                                                       0173 00729000
@RC00159 DS    0H                                                  0174 00730000
*     IF ICTEREQS(1)=OFF THEN                                      0174 00731000
*                                                                  0174 00732000
@RT00156 L     @15,@PC00004                                        0174 00733000
         L     @15,LEPTR2(,@15)                                    0174 00734000
         TM    ICTEREQS(@15),B'00010000'                           0174 00735000
         BNZ   @RF00174                                            0174 00736000
*       /*************************************************************/ 00737000
*       /*                                                           */ 00738000
*       /* THE SUPERSEDER DID NOT CHECK SUCCESSFULLY. LOOK FOR       */ 00739000
*       /* ANOTHER SUPERSEDER IN THE ICT.                            */ 00740000
*       /*                                                           */ 00741000
*       /*************************************************************/ 00742000
*                                                                  0175 00743000
*       CALL SUPICT(ICTENTNM(1),SPTR);/* GET ANOTHER SUP             */ 00744000
         ST    @15,@AL00001                                        0175 00745000
         LA    @15,SPTR                                            0175 00746000
         ST    @15,@AL00001+4                                      0175 00747000
         LA    @01,@AL00001                                        0175 00748000
         BAL   @14,SUPICT                                          0175 00749000
*   END;                            /* END OF LOOP FOR ALL         0176 00750000
*                                      SUPERSEDING SYSMODS IN ICT    */ 00751000
@RF00174 DS    0H                                                  0176 00752000
@DE00155 L     @15,@PC00004                                        0176 00753000
         L     @01,LEPTR2(,@15)                                    0176 00754000
         TM    ICTEREQS(@01),B'00010000'                           0176 00755000
         BNZ   @DC00155                                            0176 00756000
         L     @15,SPTR                                            0176 00757000
         LTR   @15,@15                                             0176 00758000
         BNZ   @DL00155                                            0176 00759000
@DC00155 DS    0H                                                  0177 00760000
*   RETURN;                                                        0177 00761000
@EL00004 DS    0H                                                  0177 00762000
@EF00004 DS    0H                                                  0177 00763000
@ER00004 LM    @14,@12,@SA00004                                    0177 00764000
         BR    @14                                                 0177 00765000
*   END CKICTSUP;                                                  0178 00766000
*                                                                  0179 00767000
*/********************************************************************/ 00768000
*/*                                                                  */ 00769000
*/*    CHKICT  -  CHECK THE ICT FOR A NON-NOGO, NON-DELETED, NON-    */ 00770000
*/*               SUPERSEDED ICT PTF SECTION ENTRY REPRESENTING      */ 00771000
*/*               THE SYSMOD NAME PASSED BY THE CALLER.              */ 00772000
*/*                                                                  */ 00773000
*/*               IF SUCH AN ENTRY IS FOUND, RETURN A POINTER TO IT. */ 00774000
*/*                                                                  */ 00775000
*/*               IF SUCH AN ENTRY IS NOT FOUND, RETURN THE POINTER  */ 00776000
*/*               AS ZERO.                                           */ 00777000
*/*                                                                  */ 00778000
*/********************************************************************/ 00779000
*                                                                  0179 00780000
*CHKICT:                                                           0179 00781000
*   PROC(SYSMNAME,SRCHPTR);                                        0179 00782000
CHKICT   STM   @14,@12,12(@13)                                     0179 00783000
         MVC   @PC00005(8),0(@01)                                  0179 00784000
*   DCL                                                            0180 00785000
*     SYSMNAME CHAR(7);             /* SYSMOD TO BE FOUND IN ICT     */ 00786000
*   DCL                                                            0181 00787000
*     SRCHPTR PTR(31);              /* RETURNS PTR TO SYSMOD FOUND   */ 00788000
*   DCL                                                            0182 00789000
*     PPTR1 PTR(31);                /* ADDR OF ICT PTF SECT ENTRY    */ 00790000
*   RESPECIFY                                                      0183 00791000
*     ICTPTF BASED(PPTR1);          /* SET UP ADDRESSIBILITY FOR PTF    00792000
*                                      SECTION ENTRIES               */ 00793000
*   SRCHPTR=0;                      /* INITIALIZE TO ZERO WHICH    0184 00794000
*                                      IMPLIES "NOT FOUND"           */ 00795000
*                                                                  0184 00796000
         L     @15,@PC00005+4                                      0184 00797000
         SLR   @14,@14                                             0184 00798000
         ST    @14,SRCHPTR(,@15)                                   0184 00799000
*   /*****************************************************************/ 00800000
*   /*                                                               */ 00801000
*   /* GO THRU THE ICT PTF SECTION ENTIRES LOOKING FOR "SYSMNAME"    */ 00802000
*   /*                                                               */ 00803000
*   /*****************************************************************/ 00804000
*                                                                  0185 00805000
*   DO PPTR1=CCAICPTF BY LENGTH(ICTPTF)/* GO THRU ICT PTF SECTION    */ 00806000
*         WHILE ICTPEND(1)^=ICTENDNT/* NOT AT END OF PTF SECTION     */ 00807000
*         &SRCHPTR=0;               /* SYSMOD NOT FOUND              */ 00808000
         L     PPTR1,CCAICPTF(,CCAPTR)                             0185 00809000
         B     @DE00185                                            0185 00810000
@DL00185 DS    0H                                                  0186 00811000
*     IF ICTPTFS(1)=SYSMNAME THEN                                  0186 00812000
*                                                                  0186 00813000
         L     @03,@PC00005                                        0186 00814000
         CLC   ICTPTFS(7,PPTR1),SYSMNAME(@03)                      0186 00815000
         BNE   @RF00186                                            0186 00816000
*       /*************************************************************/ 00817000
*       /*                                                           */ 00818000
*       /* SYSMNAME FOUND IN ICT                                     */ 00819000
*       /*                                                           */ 00820000
*       /*************************************************************/ 00821000
*                                                                  0187 00822000
*       IF ICTPNOGO(1)=OFF          /* NOT NOGO                      */ 00823000
*           &ICTPDEXP(1)=OFF        /* NOT EXPLICITLY DELETED        */ 00824000
*           &ICTPDIMP(1)=OFF        /* NOT IMPLICITLY DELETED        */ 00825000
*           &ICTPSUPN(1)=0          /* NOT SUPERSEDED                */ 00826000
*         THEN                                                     0187 00827000
         TM    ICTPNOGO(PPTR1),B'01000000'                         0187 00828000
         BNZ   @RF00187                                            0187 00829000
         TM    ICTPDEXP(PPTR1),B'00110000'                         0187 00830000
         BNZ   @RF00187                                            0187 00831000
         MVC   @ZT00002+2(2),ICTPSUPN(PPTR1)                       0187 00832000
         L     @03,@ZT00002                                        0187 00833000
         LTR   @03,@03                                             0187 00834000
         BNZ   @RF00187                                            0187 00835000
*         SRCHPTR=ADDR(ICTPTF(1));  /* RETURN POINTER TO CALLER      */ 00836000
         L     @03,@PC00005+4                                      0188 00837000
         ST    PPTR1,SRCHPTR(,@03)                                 0188 00838000
*   END;                                                           0189 00839000
@RF00187 DS    0H                                                  0189 00840000
@RF00186 AH    PPTR1,@CH00922                                      0189 00841000
@DE00185 CLC   ICTPEND(4,PPTR1),ICTENDNT                           0189 00842000
         BE    @DC00185                                            0189 00843000
         L     @03,@PC00005+4                                      0189 00844000
         L     @03,SRCHPTR(,@03)                                   0189 00845000
         LTR   @03,@03                                             0189 00846000
         BZ    @DL00185                                            0189 00847000
@DC00185 DS    0H                                                  0190 00848000
*   RETURN;                                                        0190 00849000
@EL00005 DS    0H                                                  0190 00850000
@EF00005 DS    0H                                                  0190 00851000
@ER00005 LM    @14,@12,12(@13)                                     0190 00852000
         BR    @14                                                 0190 00853000
*   END CHKICT;                                                    0191 00854000
*                                                                  0192 00855000
*/********************************************************************/ 00856000
*/*                                                                  */ 00857000
*/*    SUPICT  -  CHECK THE ICT FOR A NON-NOGO, NON-DELETED, NON-    */ 00858000
*/*               SUPERSEDED SYSMOD WHICH SUPERSEDES THE SYSMOD      */ 00859000
*/*               PASSED BY THE CALLER.                              */ 00860000
*/*                                                                  */ 00861000
*/*            -  THIS PROCEDURE WILL START IST SEARCH THRU THE ICT  */ 00862000
*/*               PTF SECTION FROM THE PTF SECTION ENTRY AFTER THE   */ 00863000
*/*               ENTRY POINTED TO BY THE PARAMETER, "SUPPTR".       */ 00864000
*/*                                                                  */ 00865000
*/*               IF THE PARAMETER, "SUPPTR", IS ZERO UPON ENTRY,    */ 00866000
*/*               THE SEARCH WILL START FROM THE BEGINNING OF THE    */ 00867000
*/*               ICT PTF SECTION.                                   */ 00868000
*/*                                                                  */ 00869000
*/*            -  ON EXIT, "SUPPTR" WILL POINT TO THE NEXT SYSMOD    */ 00870000
*/*               FOUND IN THE ICT WHICH SUPERSEDES THE SYSMOD       */ 00871000
*/*               PASSED.  IF "SUPPTR" IS ZERO ON EXIT, NO OTHER     */ 00872000
*/*               SYSMOD WAS FOUND IN THE ICT WHICH SUPERSEDED THE   */ 00873000
*/*               SYSMOD PASSED.                                     */ 00874000
*/*                                                                  */ 00875000
*/********************************************************************/ 00876000
*                                                                  0192 00877000
*SUPICT:                                                           0192 00878000
*   PROC(SUPNAME,SUPPTR);                                          0192 00879000
SUPICT   STM   @14,@12,12(@13)                                     0192 00880000
         MVC   @PC00006(8),0(@01)                                  0192 00881000
*   DCL                                                            0193 00882000
*     SUPNAME CHAR(7);              /* SYSMOD TO BE FOUND AS       0193 00883000
*                                      SUPERSEDED IN ICT             */ 00884000
*   DCL                                                            0194 00885000
*     SUPPTR PTR(31);               /* UPON ENTRY THIS PARAMETER IS     00886000
*                                      EITHER 0 OR POINTS TO AN ICT     00887000
*                                      PTF SECTION ENTRY. .........     00888000
*                                      UPON EXIT THIS PARAMETER IS 0194 00889000
*                                      SET TO EITHER 0 OR POINTS TO     00890000
*                                      THE ICT PTF SECTION ENTRY FOR    00891000
*                                      A SYSMOD WHICH SUPERSEDES   0194 00892000
*                                      "SUPNAME".                    */ 00893000
*   DCL                                                            0195 00894000
*     PPTR2 PTR(31);                /* ADDR PTF SECTION ENTRY        */ 00895000
*   DCL                                                            0196 00896000
*     L FIXED(31);                  /* INDEX - SUP LIST ENTRIES      */ 00897000
*   RESPECIFY                                                      0197 00898000
*     ICTPTF BASED(SUPPTR);         /* ADDRESS PTF SECT ENTRY PASSED */ 00899000
*   IF SUPPTR=0 THEN                                               0198 00900000
         L     @15,@PC00006+4                                      0198 00901000
         L     @15,SUPPTR(,@15)                                    0198 00902000
         LTR   @15,@15                                             0198 00903000
         BNZ   @RF00198                                            0198 00904000
*     PPTR2=CCAICPTF;               /* START WITH THE FIRST PTF    0199 00905000
*                                      SECTION ENTRY.                */ 00906000
         L     PPTR2,CCAICPTF(,CCAPTR)                             0199 00907000
*   ELSE                                                           0200 00908000
*     PPTR2=ADDR(ICTPTF(2));        /* START WITH THE NEXT PTF     0200 00909000
*                                      SECTION ENTRY.                */ 00910000
         B     @RC00198                                            0200 00911000
@RF00198 L     @15,@PC00006+4                                      0200 00912000
         L     PPTR2,SUPPTR(,@15)                                  0200 00913000
         LA    PPTR2,ICTPTF+116(,PPTR2)                            0200 00914000
*   SUPPTR=0;                       /* CLEAR PTR TO SUPERSEDER       */ 00915000
@RC00198 L     @03,@PC00006+4                                      0201 00916000
         SLR   @15,@15                                             0201 00917000
         ST    @15,SUPPTR(,@03)                                    0201 00918000
*   RESPECIFY                                                      0202 00919000
*     ICTPTF BASED(PPTR2);          /* ADDRESS PTF SECT ENTRY        */ 00920000
*   RESPECIFY                                                      0203 00921000
*     ICTSUPLS BASED(ICTPSCHN(1));  /* ADDRESS PTF ENTRY SUP LIST    */ 00922000
*                                                                  0203 00923000
*   /*****************************************************************/ 00924000
*   /*                                                               */ 00925000
*   /* GO THRU ALL REMAINING PTF SECTION ENTRIES                     */ 00926000
*   /*                                                               */ 00927000
*   /*****************************************************************/ 00928000
*                                                                  0204 00929000
*   DO PPTR2=PPTR2 BY LENGTH(ICTPTF)/* GO THRU PTF SECTION           */ 00930000
*         WHILE ICTPEND(1)^=ICTENDNT/* NOT AT END                    */ 00931000
*         &SUPPTR=0;                /* SUPERSEDING SYSMOD NOT FOUND  */ 00932000
         B     @DE00204                                            0204 00933000
@DL00204 DS    0H                                                  0205 00934000
*     IF ICTPNOGO(1)=OFF            /* NOT NOGO                      */ 00935000
*         &ICTPDEXP(1)=OFF          /* NOT EXPLICITLY DELETED        */ 00936000
*         &ICTPDIMP(1)=OFF          /* NOT IMPLICITLY DELETED        */ 00937000
*         &ICTPSUPN(1)=0            /* NOT SUPERSEDED                */ 00938000
*         &ICTPSCHN(1)^=0           /* THERE IS A LIST OF SUPS       */ 00939000
*       THEN                                                       0205 00940000
         TM    ICTPNOGO(PPTR2),B'01000000'                         0205 00941000
         BNZ   @RF00205                                            0205 00942000
         TM    ICTPDEXP(PPTR2),B'00110000'                         0205 00943000
         BNZ   @RF00205                                            0205 00944000
         MVC   @ZT00002+2(2),ICTPSUPN(PPTR2)                       0205 00945000
         L     @03,@ZT00002                                        0205 00946000
         LTR   @03,@03                                             0205 00947000
         BNZ   @RF00205                                            0205 00948000
         MVC   @TF00001(4),ICTPSCHN(PPTR2)                         0205 00949000
         L     @03,@TF00001                                        0205 00950000
         LTR   @03,@03                                             0205 00951000
         BZ    @RF00205                                            0205 00952000
*       DO;                                                        0206 00953000
*                                                                  0206 00954000
*         /***********************************************************/ 00955000
*         /*                                                         */ 00956000
*         /* GO THRU THE LIST OF SUPERSEDED SYSMODS IN THE PTF       */ 00957000
*         /* SECTION ENTRY                                           */ 00958000
*         /*                                                         */ 00959000
*         /***********************************************************/ 00960000
*                                                                  0207 00961000
*         DO L=1 BY 1 WHILE ICTSUPND(L)^=ICTENDIX/* NOT AT END       */ 00962000
*               &SUPPTR=0;          /* SUPERSEDING SYSMOD NOT FND    */ 00963000
         LA    L,1                                                 0207 00964000
         B     @DE00207                                            0207 00965000
@DL00207 DS    0H                                                  0208 00966000
*           IF ICTSUPNM(L)=SUPNAME  /* SUPS THE SYSMOD PASSED        */ 00967000
*             THEN                                                 0208 00968000
         LR    @15,L                                               0208 00969000
         SLA   @15,3                                               0208 00970000
         MVC   @TF00001(4),ICTPSCHN(PPTR2)                         0208 00971000
         L     @01,@TF00001                                        0208 00972000
         L     @14,@PC00006                                        0208 00973000
         ALR   @01,@15                                             0208 00974000
         AL    @01,@CF00930                                        0208 00975000
         CLC   ICTSUPNM(7,@01),SUPNAME(@14)                        0208 00976000
         BNE   @RF00208                                            0208 00977000
*             SUPPTR=ADDR(ICTPTF(1));/* POINT TO SUPERSEDER          */ 00978000
         L     @15,@PC00006+4                                      0209 00979000
         ST    PPTR2,SUPPTR(,@15)                                  0209 00980000
*         END;                                                     0210 00981000
@RF00208 AH    L,@CH00066                                          0210 00982000
@DE00207 LR    @15,L                                               0210 00983000
         SLA   @15,3                                               0210 00984000
         MVC   @TF00001(4),ICTPSCHN(PPTR2)                         0210 00985000
         L     @01,@TF00001                                        0210 00986000
         AL    @01,@CF00930                                        0210 00987000
         L     @15,ICTSUPND(@15,@01)                               0210 00988000
         CL    @15,ICTENDIX                                        0210 00989000
         BE    @DC00207                                            0210 00990000
         L     @15,@PC00006+4                                      0210 00991000
         L     @15,SUPPTR(,@15)                                    0210 00992000
         LTR   @15,@15                                             0210 00993000
         BZ    @DL00207                                            0210 00994000
@DC00207 DS    0H                                                  0211 00995000
*       END;                                                       0211 00996000
*   END;                                                           0212 00997000
@RF00205 AH    PPTR2,@CH00922                                      0212 00998000
@DE00204 CLC   ICTPEND(4,PPTR2),ICTENDNT                           0212 00999000
         BE    @DC00204                                            0212 01000000
         L     @03,@PC00006+4                                      0212 01001000
         L     @03,SUPPTR(,@03)                                    0212 01002000
         LTR   @03,@03                                             0212 01003000
         BZ    @DL00204                                            0212 01004000
@DC00204 DS    0H                                                  0213 01005000
*   RETURN;                                                        0213 01006000
@EL00006 DS    0H                                                  0213 01007000
@EF00006 DS    0H                                                  0213 01008000
@ER00006 LM    @14,@12,12(@13)                                     0213 01009000
         BR    @14                                                 0213 01010000
*   END SUPICT;                                                    0214 01011000
*   END HMASMTR1                                                   0215 01012000
