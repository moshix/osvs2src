         L     @15,@PC00001+4                                      0156 00650000
         L     @15,PTFPTR(,@15)                                    0156 00651000
         CLC   ICTENTNM+8(7,@01),ICTPTFS(@15)                      0156 00652000
         BNE   @RF00156                                            0156 00653000
         C     @15,SPTR                                            0156 00654000
         BE    @RT00156                                            0156 00655000
*       ;                           /* DO NOT USE "SELF"             */ 00656000
*     ELSE                                                         0158 00657000
*       DO;                                                        0158 00658000
@RF00156 DS    0H                                                  0159 00659000
*         IF SPTR->ICTPRQCK(1)=ON   /* SUPERSEDING SYSMOD ALREADY  0159 00660000
*                                      CHECKED                       */ 00661000
*           THEN                                                   0159 00662000
         L     @15,SPTR                                            0159 00663000
         TM    ICTPRQCK(@15),B'00001000'                           0159 00664000
         BNO   @RF00159                                            0159 00665000
*           DO;                                                    0160 00666000
*             IF SPTR->ICTPRQOK(1)=ON/* CHECKED "OK"                 */ 00667000
*               THEN                                               0161 00668000
         TM    ICTPRQOK(@15),B'00000100'                           0161 00669000
