         TITLE 'HMASMTR1 CHECK SYSMOD REQUISITES'                       00001000
HMASMTR1 CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMTR1  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         L     @00,@SIZDATD                                        0001 00013000
         GETMAIN  R,LV=(0)                                              00014000
         LR    @10,@01                                             0001 00015000
         USING @DATD,@10                                           0001 00016000
         ST    @13,@SA00001+4                                      0001 00017000
         LM    @00,@01,20(@13)                                     0001 00018000
         ST    @10,8(,@13)                                         0001 00019000
         LR    @13,@10                                             0001 00020000
         XC    @ZTEMPS(@ZLEN),@ZTEMPS                                   00021000
         MVC   @PC00001(12),0(@01)                                 0001 00022000
*                                      ICT PTF SECTION ENTRY PASSED     00023000
*                                      BY THE CALLER                 */ 00024000
*   TR1RETRN=ZERO;                  /* INITIALIZE CALLER''S PARM     */ 00025000
         L     @15,@PC00001+8                                      0063 00026000
         MVI   TR1RETRN(@15),X'00'                                 0063 00027000
*   TR1FLGS=''B;                    /* CLEAR LOCAL FLAGS             */ 00028000
         MVI   TR1FLGS,X'00'                                       0064 00029000
*   IF ICTPTR1(1)=ON THEN           /* CHECK TR1 FLAG IN ENTRY PASSED*/ 00030000
         L     @15,@PC00001+4                                      0065 00031000
         L     @01,PTFPTR(,@15)                                    0065 00032000
         TM    ICTPTR1(@01),B'01000000'                            0065 00033000
         BNO   @RF00065                                            0065 00034000
*     LOCALTR1=ON;                  /* REMEMBER THAT TR1 PROCESSING     00035000
*                                      FLAG WAS SET UPON ENTRY       */ 00036000
         OI    LOCALTR1,B'10000000'                                0066 00037000
*   ELSE                                                           0067 00038000
*     ICTPTR1(1)=ON;                /* OTHERWISE SET TR1 PROCESSING     00039000
*                                      FLAG IN CURRENT PTF ENTRY     */ 00040000
*                                                                  0067 00041000
         B     @RC00065                                            0067 00042000
@RF00065 L     @15,@PC00001+4                                      0067 00043000
         L     @01,PTFPTR(,@15)                                    0067 00044000
         OI    ICTPTR1(@01),B'01000000'                            0067 00045000
*   /*****************************************************************/ 00046000
*   /*                                                               */ 00047000
*   /* CHECK THE REQUISITES OF THIS SYSMOD                           */ 00048000
*   /*                                                               */ 00049000
*   /*****************************************************************/ 00050000
*                                                                  0068 00051000
*   IF ICTPPCHN(1)^=0 THEN                                         0068 00052000
@RC00065 L     @15,@PC00001+4                                      0068 00053000
         L     @15,PTFPTR(,@15)                                    0068 00054000
         MVC   @TF00001(4),ICTPPCHN(@15)                           0068 00055000
         L     @14,@TF00001                                        0068 00056000
         LTR   @14,@14                                             0068 00057000
         BZ    @RF00068                                            0068 00058000
*     DO;                                                          0069 00059000
*                                                                  0069 00060000
*       /*************************************************************/ 00061000
*       /*                                                           */ 00062000
*       /* CHECK THAT PREREQS ARE SATISFIED                          */ 00063000
*       /*                                                           */ 00064000
*       /*************************************************************/ 00065000
*                                                                  0070 00066000
*       CALL REQCHK(ICTPPCHN(1),PRE);                              0070 00067000
         LA    @15,ICTPPCHN(,@15)                                  0070 00068000
         ST    @15,@AL00001                                        0070 00069000
         LA    @15,@CH00066+1                                      0070 00070000
         ST    @15,@AL00001+4                                      0070 00071000
         LA    @01,@AL00001                                        0070 00072000
         BAL   @14,REQCHK                                          0070 00073000
*     END;                                                         0071 00074000
*   IF ICTPRCHN(1)^=0 THEN                                         0072 00075000
@RF00068 L     @15,@PC00001+4                                      0072 00076000
         L     @15,PTFPTR(,@15)                                    0072 00077000
         MVC   @TF00001(4),ICTPRCHN(@15)                           0072 00078000
         L     @14,@TF00001                                        0072 00079000
         LTR   @14,@14                                             0072 00080000
         BZ    @RF00072                                            0072 00081000
*     DO;                                                          0073 00082000
*                                                                  0073 00083000
*       /*************************************************************/ 00084000
*       /*                                                           */ 00085000
*       /* CHECK THAT REQS ARE SATISFIED                             */ 00086000
*       /*                                                           */ 00087000
*       /*************************************************************/ 00088000
*                                                                  0074 00089000
*       CALL REQCHK(ICTPRCHN(1),REQ);                              0074 00090000
         LA    @15,ICTPRCHN(,@15)                                  0074 00091000
         ST    @15,@AL00001                                        0074 00092000
         LA    @15,@CH00276+1                                      0074 00093000
         ST    @15,@AL00001+4                                      0074 00094000
         LA    @01,@AL00001                                        0074 00095000
         BAL   @14,REQCHK                                          0074 00096000
*     END;                                                         0075 00097000
*   IF ICTPICHN(1)^=0 THEN                                         0076 00098000
@RF00072 L     @15,@PC00001+4                                      0076 00099000
         L     @15,PTFPTR(,@15)                                    0076 00100000
         MVC   @TF00001(4),ICTPICHN(@15)                           0076 00101000
         L     @14,@TF00001                                        0076 00102000
         LTR   @14,@14                                             0076 00103000
         BZ    @RF00076                                            0076 00104000
*     DO;                                                          0077 00105000
*                                                                  0077 00106000
*       /*************************************************************/ 00107000
*       /*                                                           */ 00108000
*       /* CHECK THAT IREQS ARE SATISFIED                            */ 00109000
*       /*                                                           */ 00110000
*       /*************************************************************/ 00111000
*                                                                  0078 00112000
*       CALL REQCHK(ICTPICHN(1),IRQ);                              0078 00113000
         LA    @15,ICTPICHN(,@15)                                  0078 00114000
         ST    @15,@AL00001                                        0078 00115000
         LA    @15,@CH00089+1                                      0078 00116000
         ST    @15,@AL00001+4                                      0078 00117000
         LA    @01,@AL00001                                        0078 00118000
         BAL   @14,REQCHK                                          0078 00119000
*     END;                                                         0079 00120000
*                                                                  0079 00121000
*   /*****************************************************************/ 00122000
*   /*                                                               */ 00123000
*   /* REQUISITE CHECKING COMPLETED FOR THIS SYSMOD ... RETURN TO THE*/ 00124000
*   /* CALLER                                                        */ 00125000
*   /*                                                               */ 00126000
*   /*****************************************************************/ 00127000
*                                                                  0080 00128000
*   IF LOCALTR1=OFF                 /* RECURSION FLAG NOT SET ON   0080 00129000
*                                      ENTRY                         */ 00130000
*     THEN                                                         0080 00131000
@RF00076 TM    LOCALTR1,B'10000000'                                0080 00132000
         BNZ   @RF00080                                            0080 00133000
*     ICTPTR1(1)=OFF;               /* TURN OFF TR1 RECURSION FLAG   */ 00134000
         L     @15,@PC00001+4                                      0081 00135000
         L     @01,PTFPTR(,@15)                                    0081 00136000
         NI    ICTPTR1(@01),B'10111111'                            0081 00137000
*   RETURN CODE(TR1RETRN);                                         0082 00138000
@RF00080 L     @09,@PC00001+8                                      0082 00139000
         SLR   @08,@08                                             0082 00140000
         IC    @08,TR1RETRN(,@09)                                  0082 00141000
         L     @13,4(,@13)                                         0082 00142000
         L     @00,@SIZDATD                                        0082 00143000
         LR    @01,@10                                             0082 00144000
         FREEMAIN R,LV=(0),A=(1)                                        00145000
         LR    @15,@08                                             0082 00146000
         L     @14,12(,@13)                                        0082 00147000
         LM    @00,@12,20(@13)                                     0082 00148000
         BR    @14                                                 0082 00149000
*                                                                  0083 00150000
*/********************************************************************/ 00151000
*/*                                                                  */ 00152000
*/*  REQCHK  - CHECK THE REQUISITES OF A SYSMOD TO DETERMINE WHETHER */ 00153000
*/*            THEY ARE SATISFIED BY BEING PRESENT ON EITHER THE     */ 00154000
*/*            TARGET SYSTEM OR IN THE ICT.                          */ 00155000
*/*                                                                  */ 00156000
*/*          - SET FLAGS, ICTEREQS AND ICTEREQB, IN THE REQUISITE    */ 00157000
*/*            LIST ENTRIES FOR REQUISITES WHICH ARE SATISFIED AND   */ 00158000
*/*            BYPASSED RESPECTIVELY.                                */ 00159000
*/*                                                                  */ 00160000
*/*          - SET TR1RETRN FOR REQUISITE FAILURES AND BYPASSES.     */ 00161000
*/*                                                                  */ 00162000
*/*                                                                  */ 00163000
*/*       THE FOLLOWING ILLUSTRATES THE FLAGS SET AND TESTED IN      */ 00164000
*/*       THE PTF SECTION ENTRY POINTED TO BY RLISTPTR:              */ 00165000
*/*                                                                  */ 00166000
*/*       ICT SECTION ENTRY - ICTPTF(*)                              */ 00167000
*/*       -----------------                                          */ 00168000
*/*       |               |                                          */ 00169000
*/*       |         PRQCK | ALL REQS "CHECKED" FLAG                  */ 00170000
*/*       |         PRQOK | ALL REQS "OK" FLAG                       */ 00171000
*/*       |         PRQBY | SOME REQS "BYPASSED" FLAG                */ 00172000
*/*       |         PTR1  | HMASMTR1 PROCESSING THIS ENTRY           */ 00173000
*/*       |               |                                          */ 00174000
*/*       |               |         ICTENTLS - REQUISITE LIST        */ 00175000
*/*       | ICTREQLS      | ------> --------------                   */ 00176000
*/*       | ICTPRELS      |         | REQ PTF #  |                   */ 00177000
*/*       | ICTIRQLS      |         |   ICTEREQS | REQ "SATISFIED"   */ 00178000
*/*       |               |         |   ICTEREQB | REQ "BYPASSED"    */ 00179000
*/*       |               |         |   ICTEREQI | "MUST BE IN ICT"  */ 00180000
*/*       |               |         |   ICTEREQT | REQ "ON (A)CDS"   */ 00181000
*/*       |               |         |------------|                   */ 00182000
*/*       |               |         | REQ PTF #  |                   */ 00183000
*/*       |               |         |            |                   */ 00184000
*/*       |_______________|         |            |                   */ 00185000
*/*                                 |------------|                   */ 00186000
*/*                                       .                          */ 00187000
*/*                                       .                          */ 00188000
*/*                                       .                          */ 00189000
*/*                                                                  */ 00190000
*/********************************************************************/ 00191000
*                                                                  0083 00192000
*REQCHK:                                                           0083 00193000
*   PROC(RLISTPTR,REQTYPE);                                        0083 00194000
REQCHK   STM   @14,@12,@SA00002                                    0083 00195000
         MVC   @PC00002(8),0(@01)                                  0083 00196000
*   DCL                                                            0084 00197000
*     RLISTPTR PTR(31);             /* POINTER TO REQUISITE LIST     */ 00198000
*   DCL                                                            0085 00199000
*     REQTYPE FIXED(8);             /* REQ TYPE (PRE,REQ,IRQ)        */ 00200000
*                                                                  0086 00201000
*   /*****************************************************************/ 00202000
*   /*                                                               */ 00203000
*   /* LOCAL VARIABLES                                               */ 00204000
*   /*                                                               */ 00205000
*   /*****************************************************************/ 00206000
*                                                                  0086 00207000
*   DCL                                                            0086 00208000
*     I FIXED(31);                  /* REQ LIST ENTRY INDEX          */ 00209000
*   DCL                                                            0087 00210000
*     LISTENT@ PTR(31);             /* REQ LIST ENTRY ADDRESS        */ 00211000
*   DCL                                                            0088 00212000
*     INDXSTEP FIXED(31);           /* INDEX STEP INCREMENT          */ 00213000
*                                                                  0088 00214000
*   /*****************************************************************/ 00215000
*   /*                                                               */ 00216000
*   /* SET UP LOOP INDEX INCREMENT BASED UPON THE TYPE OF REQUISITE  */ 00217000
*   /* LIST ENTRY BEING DEALT WITH                                   */ 00218000
*   /*                                                               */ 00219000
*   /*****************************************************************/ 00220000
*                                                                  0089 00221000
*   IF REQTYPE=IRQ THEN                                            0089 00222000
         L     @15,@PC00002+4                                      0089 00223000
         CLI   REQTYPE(@15),3                                      0089 00224000
         BNE   @RF00089                                            0089 00225000
*     INDXSTEP=2;                   /* INDEX STEP FOR IREQ ENTRIES IS   00226000
*                                      2 ... IREQ ENTRIES ARE MADE UP   00227000
*                                      OF TWO "GENERAL" ENTRIES      */ 00228000
         LA    INDXSTEP,2                                          0090 00229000
*   ELSE                                                           0091 00230000
*     INDXSTEP=1;                   /* INDEX STEP FOR ALL OTHERS IS 1*/ 00231000
         B     @RC00089                                            0091 00232000
@RF00089 LA    INDXSTEP,1                                          0091 00233000
*   RESPECIFY                                                      0092 00234000
*     ICTENTLS BASED(RLISTPTR);     /* ADDRESS REQUISITE LIST PASSED */ 00235000
*                                                                  0092 00236000
@RC00089 DS    0H                                                  0093 00237000
*   /*****************************************************************/ 00238000
*   /*                                                               */ 00239000
*   /* GO THRU ALL REQUISITE LIST ENTRIES                            */ 00240000
*   /*                                                               */ 00241000
*   /*****************************************************************/ 00242000
*                                                                  0093 00243000
*   DO I=1 BY INDXSTEP WHILE ICTENTND(I)^=ICTENDIX;                0093 00244000
         LA    I,1                                                 0093 00245000
         B     @DE00093                                            0093 00246000
@DL00093 DS    0H                                                  0094 00247000
*     LISTENT@=ADDR(ICTENTLS(I));   /* POINT TO REQ LIST ENTRY       */ 00248000
         LR    @15,I                                               0094 00249000
         SLA   @15,3                                               0094 00250000
         L     @14,@PC00002                                        0094 00251000
         L     @01,RLISTPTR(,@14)                                  0094 00252000
         AL    @01,@CF00930                                        0094 00253000
         LA    @15,ICTENTLS(@15,@01)                               0094 00254000
         ST    @15,LISTENT@                                        0094 00255000
*     RESPECIFY                                                    0095 00256000
*       ICTENTLS BASED(LISTENT@);   /* ADDRESS "CURRENT" REQ LIST  0095 00257000
*                                      ENTRY                         */ 00258000
*     ICTEREQS(1)=OFF;              /* CLEAR REQ SATISFIED INDICATOR */ 00259000
*     ICTEREQB(1)=OFF;              /* CLEAR REQ BYPASSED INDICATOR  */ 00260000
*                                                                  0097 00261000
         NI    ICTEREQS(@15),B'11100111'                           0097 00262000
*     /***************************************************************/ 00263000
*     /*                                                             */ 00264000
*     /* CHECK THE TARGET SYSTEM FOR THIS REQ                        */ 00265000
*     /*                                                             */ 00266000
*     /***************************************************************/ 00267000
*                                                                  0098 00268000
*     IF ICTEREQT(1)=ON             /* REQ IS ON TARGET SYSTEM       */ 00269000
*         &ICTEREQI(1)=OFF          /* ... NOT REQUIRED TO BE IN ICT */ 00270000
*       THEN                                                       0098 00271000
         TM    ICTEREQT(@15),B'00000010'                           0098 00272000
         BNO   @RF00098                                            0098 00273000
         TM    ICTEREQI(@15),B'00000100'                           0098 00274000
         BNZ   @RF00098                                            0098 00275000
*       ICTEREQS(1)=ON;             /* SET THE SATISFIED INDICATOR   */ 00276000
         OI    ICTEREQS(@15),B'00010000'                           0099 00277000
*     IF ICTEREQS(1)=OFF            /* REQUISITE NOT SATISFIED       */ 00278000
*       THEN                                                       0100 00279000
*                                                                  0100 00280000
@RF00098 L     @01,LISTENT@                                        0100 00281000
         TM    ICTEREQS(@01),B'00010000'                           0100 00282000
         BNZ   @RF00100                                            0100 00283000
*       /*************************************************************/ 00284000
*       /*                                                           */ 00285000
*       /* CHECK THE ICT FOR THIS REQ                                */ 00286000
*       /*                                                           */ 00287000
*       /*************************************************************/ 00288000
*                                                                  0101 00289000
*       CALL CKICTREQ(LISTENT@,     /* PTR TO REQ LIST ENTRY         */ 00290000
*           REQTYPE);               /* REQUISITE TYPE                */ 00291000
         LA    @15,LISTENT@                                        0101 00292000
         ST    @15,@AL00001                                        0101 00293000
         L     @15,@PC00002+4                                      0101 00294000
         ST    @15,@AL00001+4                                      0101 00295000
         LA    @01,@AL00001                                        0101 00296000
         BAL   @14,CKICTREQ                                        0101 00297000
*     IF ICTEREQS(1)=OFF            /* REQUISITE NOT SATISFIED       */ 00298000
*       THEN                                                       0102 00299000
*                                                                  0102 00300000
@RF00100 L     @01,LISTENT@                                        0102 00301000
         TM    ICTEREQS(@01),B'00010000'                           0102 00302000
         BNZ   @RF00102                                            0102 00303000
*       /*************************************************************/ 00304000
*       /*                                                           */ 00305000
*       /* CHECK THE ICT FOR SYSMODS WHICH SUPERSEDE THIS REQ        */ 00306000
*       /*                                                           */ 00307000
*       /*************************************************************/ 00308000
*                                                                  0103 00309000
*       CALL CKICTSUP(LISTENT@,     /* PTR TO REQ LIST ENTRY         */ 00310000
*           REQTYPE);               /* REQUISITE TYPE                */ 00311000
         LA    @15,LISTENT@                                        0103 00312000
         ST    @15,@AL00001                                        0103 00313000
         L     @15,@PC00002+4                                      0103 00314000
         ST    @15,@AL00001+4                                      0103 00315000
         LA    @01,@AL00001                                        0103 00316000
         BAL   @14,CKICTSUP                                        0103 00317000
*     IF ICTEREQS(1)=OFF            /* REQUISITE NOT SATISFIED       */ 00318000
*       THEN                                                       0104 00319000
@RF00102 L     @01,LISTENT@                                        0104 00320000
         TM    ICTEREQS(@01),B'00010000'                           0104 00321000
         BNZ   @RF00104                                            0104 00322000
*       DO;                                                        0105 00323000
*                                                                  0105 00324000
*         /***********************************************************/ 00325000
*         /*                                                         */ 00326000
*         /* THIS REQ HAS FAILED ... TRY THE BYPASS OPTIONS          */ 00327000
*         /*                                                         */ 00328000
*         /***********************************************************/ 00329000
*                                                                  0106 00330000
*         IF(REQTYPE=PRE&TBLBPPRE=ON)/* PRE AND BYPASS(PRE)          */ 00331000
*             |(REQTYPE=REQ&TBLBPREQ=ON)/* REQ AND BYPASS(REQ)       */ 00332000
*             |(REQTYPE=IRQ&TBLBPIRQ=ON)/* IREQ AND BYPASS(IREQ)     */ 00333000
*           THEN                                                   0106 00334000
         L     @15,@PC00002+4                                      0106 00335000
         CLI   REQTYPE(@15),1                                      0106 00336000
         BNE   @GL00002                                            0106 00337000
         L     @15,@PC00001                                        0106 00338000
         L     @01,TBLXTBL(,@15)                                   0106 00339000
         TM    TBLBPPRE(@01),B'01000000'                           0106 00340000
         BO    @RT00106                                            0106 00341000
@GL00002 L     @15,@PC00002+4                                      0106 00342000
         CLI   REQTYPE(@15),2                                      0106 00343000
         BNE   @GL00001                                            0106 00344000
         L     @15,@PC00001                                        0106 00345000
         L     @01,TBLXTBL(,@15)                                   0106 00346000
         TM    TBLBPREQ(@01),B'00100000'                           0106 00347000
         BO    @RT00106                                            0106 00348000
@GL00001 L     @15,@PC00002+4                                      0106 00349000
         CLI   REQTYPE(@15),3                                      0106 00350000
         BNE   @RF00106                                            0106 00351000
         L     @15,@PC00001                                        0106 00352000
         L     @01,TBLXTBL(,@15)                                   0106 00353000
         TM    TBLBPIRQ(@01),B'00010000'                           0106 00354000
         BNO   @RF00106                                            0106 00355000
@RT00106 DS    0H                                                  0107 00356000
*           DO;                                                    0107 00357000
*                                                                  0107 00358000
*             /*******************************************************/ 00359000
*             /*                                                     */ 00360000
*             /* THE FAILURE IS BYPASSED                             */ 00361000
*             /*                                                     */ 00362000
*             /*******************************************************/ 00363000
*                                                                  0108 00364000
*             ICTEREQB(1)=ON;       /* SET "BYPASSED" FLAG IN LIST   */ 00365000
*             ICTEREQS(1)=ON;       /* SET "SATISFIED" FLAG IN LIST  */ 00366000
         L     @01,LISTENT@                                        0109 00367000
         OI    ICTEREQB(@01),B'00011000'                           0109 00368000
*             TR1RETRN=MAX(TR1RETRN,BYPASSED);/* SET RETURN PARM     */ 00369000
         L     @15,@PC00001+8                                      0110 00370000
         SLR   @14,@14                                             0110 00371000
         IC    @14,TR1RETRN(,@15)                                  0110 00372000
         LA    @09,4                                               0110 00373000
         CR    @14,@09                                             0110 00374000
         BNL   *+6                                                      00375000
         LR    @14,@09                                             0110 00376000
         STC   @14,TR1RETRN(,@15)                                  0110 00377000
*           END;                                                   0111 00378000
*         ELSE                                                     0112 00379000
*                                                                  0112 00380000
*           /*********************************************************/ 00381000
*           /*                                                       */ 00382000
*           /* THE FAILURE IS NOT BYPASSED                           */ 00383000
*           /*                                                       */ 00384000
*           /*********************************************************/ 00385000
*                                                                  0112 00386000
*           TR1RETRN=MAX(TR1RETRN,FAILED);/* SET RETURN PARM         */ 00387000
         B     @RC00106                                            0112 00388000
@RF00106 L     @15,@PC00001+8                                      0112 00389000
         SLR   @14,@14                                             0112 00390000
         IC    @14,TR1RETRN(,@15)                                  0112 00391000
         LA    @09,8                                               0112 00392000
         CR    @14,@09                                             0112 00393000
         BNL   *+6                                                      00394000
         LR    @14,@09                                             0112 00395000
         STC   @14,TR1RETRN(,@15)                                  0112 00396000
*       END;                        /* END OF BYPASS OPTION CHECK    */ 00397000
@RC00106 DS    0H                                                  0114 00398000
*   END;                            /* END OF LOOP THRU ALL        0114 00399000
*                                      REQUISITES IN CURRENT REQ LIST*/ 00400000
*                                                                  0114 00401000
@RF00104 ALR   I,INDXSTEP                                          0114 00402000
@DE00093 LR    @15,I                                               0114 00403000
         SLA   @15,3                                               0114 00404000
         L     @14,@PC00002                                        0114 00405000
         L     @01,RLISTPTR(,@14)                                  0114 00406000
         AL    @01,@CF00930                                        0114 00407000
         L     @15,ICTENTND(@15,@01)                               0114 00408000
         CL    @15,ICTENDIX                                        0114 00409000
         BNE   @DL00093                                            0114 00410000
*   /*****************************************************************/ 00411000
*   /*                                                               */ 00412000
*   /* ALL REQS IN THE LIST HAVE BEEN CHECKED                        */ 00413000
*   /*                                                               */ 00414000
*   /*****************************************************************/ 00415000
*                                                                  0115 00416000
*   RETURN;                                                        0115 00417000
@EL00002 DS    0H                                                  0115 00418000
@EF00002 DS    0H                                                  0115 00419000
@ER00002 LM    @14,@12,@SA00002                                    0115 00420000
         BR    @14                                                 0115 00421000
*   END REQCHK;                                                    0116 00422000
*                                                                  0117 00423000
*/********************************************************************/ 00424000
*/*                                                                  */ 00425000
*/*      CKICTREQ - CHECK ICT TO DETERMINE WHETHER THE CURRENT       */ 00426000
*/*                 REQUISITE IS SATISFIED.                          */ 00427000
*/*                                                                  */ 00428000
*/*                                                                  */ 00429000
*/*        NOTE - THE REQUISITE SATISFIED FLAG, ICTEREQS, IS "OFF"   */ 00430000
*/*               ON ENTRY TO THIS PROCEDURE.                        */ 00431000
*/*                                                                  */ 00432000
*/*             - THE REQUISITE SATISFIED FLAG, ICTEREQS, WILL BE    */ 00433000
*/*               SET ON IF THE REQUISITE CAN BE FOUND (AND CHECKED) */ 00434000
*/*               IN THE ICT.                                        */ 00435000
*/*                                                                  */ 00436000
*/********************************************************************/ 00437000
*                                                                  0117 00438000
*CKICTREQ:                                                         0117 00439000
*   PROC(LEPTR1,REQTYPE1);                                         0117 00440000
CKICTREQ STM   @14,@12,@SA00003                                    0117 00441000
         MVC   @PC00003(8),0(@01)                                  0117 00442000
*   DCL                                                            0118 00443000
*     LEPTR1 PTR(31);               /* PTR TO REQ LIST ENTRY FOR   0118 00444000
*                                      REQUISITE TO BE CHECKED       */ 00445000
*   DCL                                                            0119 00446000
*     REQTYPE1 FIXED(8);            /* REQUISITE TYPE (PRE, REQ,IRQ) */ 00447000
