         OI    ICTEREQS(@01),B'00010000'                           0162 00674000
*           END;                                                   0163 00675000
*         ELSE                                                     0164 00676000
*           DO;                                                    0164 00677000
*                                                                  0164 00678000
         B     @RC00159                                            0164 00679000
@RF00159 DS    0H                                                  0165 00680000
*             /*******************************************************/ 00681000
*             /*                                                     */ 00682000
*             /* CHECK THE SUPERSEDING SYSMOD FOUND IN ICT           */ 00683000
*             /*                                                     */ 00684000
*             /*******************************************************/ 00685000
*                                                                  0165 00686000
*             IF SPTR->ICTPTR1(1)=ON/* CURRENT REQ CHAIN             */ 00687000
*                 |SPTR=PTFPTR      /* SUPERSEDING SYSMOD IS "SELF"  */ 00688000
