         TITLE 'HMASMTSB - HMASMP TABLE SUBROUTINES'                    00001000
HMASMTSB CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'HMASMTSB  78.334'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  LA    @10,4095(,@12)                                      0001 00011000
         USING @PSTART,@12                                         0001 00012000
         USING @PSTART+4095,@10                                    0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @09,@01                                             0001 00016000
         USING @DATD,@09                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @09,8(,@13)                                         0001 00020000
         LR    @13,@09                                             0001 00021000
         MVC   @PC00001(4),0(@01)                                  0001 00022000
*   TSBRTNCD=TSLGOOD;               /* SET GOOD RETURN CODE          */ 00023000
         SLR   TSBRTNCD,TSBRTNCD                                   0204 00024000
*   CCAPTR=ADDR(HMASMCCA);          /*                               */ 00025000
         L     CCAPTR,@CA00147                                     0205 00026000
*   LOCALTSL=''B;                   /* CLEAR LOCAL TSL PARM LIST     */ 00027000
         XC    LOCALTSL(10),LOCALTSL                               0206 00028000
*   LOCALTSS=''B;                   /* ...                           */ 00029000
         XC    LOCALTSS(256),LOCALTSS                              0207 00030000
*   ADDR(LOCALTSL)->TSLPMLAD=ADDR(LOCALTSS);/* PARM POINTER IN TSL   */ 00031000
         LA    @08,LOCALTSL                                        0208 00032000
         LA    @03,LOCALTSS                                        0208 00033000
         ST    @03,TSLPMLAD(,@08)                                  0208 00034000
*   ADDR(LOCALTSL)->TSLTBLXP=TSLTBLXP;/* COPY TBLXPARM PTR TO LOCAL  */ 00035000
*                                                                  0209 00036000
         L     @03,@PC00001                                        0209 00037000
         MVC   TSLTBLXP(4,@08),TSLTBLXP(@03)                       0209 00038000
*   /*****************************************************************/ 00039000
*   /*                                                               */ 00040000
*   /* CALL THE REQUESTED FUNCTION PROCESSING ROUTINE                */ 00041000
*   /*                                                               */ 00042000
*   /*****************************************************************/ 00043000
*                                                                  0210 00044000
*   CALL TSBRTN;                    /* CALL REQUESTED SUBROUTINE     */ 00045000
         SLR   @08,@08                                             0210 00046000
         IC    @08,TSLFUNCT(,@03)                                  0210 00047000
         SLA   @08,2                                               0210 00048000
         L     @15,SUBRTNS-4(@08)                                  0210 00049000
         BALR  @14,@15                                             0210 00050000
*   RFY                                                            0211 00051000
*     RTNCODE RSTD;                 /*                               */ 00052000
*   TSBRTNCD=MAX(TSBRTNCD,RTNCODE); /*                               */ 00053000
         CR    TSBRTNCD,RTNCODE                                    0212 00054000
         BNL   *+6                                                      00055000
         LR    TSBRTNCD,RTNCODE                                    0212 00056000
*   RFY                                                            0213 00057000
*     RTNCODE UNRSTD;               /*                               */ 00058000
*   TSLRETRN=TSBRTNCD;              /* SET PARMLIST RETURN CODE SAME    00059000
*                                      AS REG 15                     */ 00060000
         L     @08,@PC00001                                        0214 00061000
         STC   TSBRTNCD,TSLRETRN(,@08)                             0214 00062000
*   RETURN CODE(TSBRTNCD);          /* RETURN WITH CODE IN PARM AREA */ 00063000
         L     @13,4(,@13)                                         0215 00064000
         L     @00,@SIZDATD                                        0215 00065000
         LR    @01,@09                                             0215 00066000
         FREEMAIN R,LV=(0),A=(1)                                        00067000
         LR    @15,@02                                             0215 00068000
         L     @14,12(,@13)                                        0215 00069000
         LM    @00,@12,20(@13)                                     0215 00070000
         BR    @14                                                 0215 00071000
*   RFY                                                            0216 00072000
*     TSLPARMS BASED(ADDR(PARM1));                                 0216 00073000
*   RFY                                                            0217 00074000
*     TBLXPARM BASED(TSLTBLXP);                                    0217 00075000
         EJECT                                                          00076000
*                                                                  0218 00077000
*/****************************************************************** */ 00078000
*/*                                                                  */ 00079000
*/* CHKICT  - CHECK THE ICT PTF SECTION TO DETERMINE WHETHER THE     */ 00080000
*/*           SYSMOD PASSED IS IN THE ICT.                           */ 00081000
*/*                                                                  */ 00082000
*/****************************************************************** */ 00083000
*                                                                  0218 00084000
*CHKICT:                                                           0218 00085000
*   PROC;                                                          0218 00086000
CHKICT   STM   @14,@12,12(@13)                                     0218 00087000
*   DCL                                                            0219 00088000
*     CKIPTFX FIXED(15);            /* LOCAL INDEX - PTF SECT ENTRIES*/ 00089000
*   DCL                                                            0220 00090000
*     CKIRC FIXED(15);              /* RETURN CODE                   */ 00091000
*   DCL                                                            0221 00092000
*     CKISTRTI FIXED(15);           /* START OF SEARCH INDEX         */ 00093000
*   DCL                                                            0222 00094000
*     CKICTPTR PTR(31);             /* BASE FOR ICT PTF SECTION      */ 00095000
*   RFY                                                            0223 00096000
*     TSLPARMS BASED(ADDR(PARM1));                                 0223 00097000
*   RFY                                                            0224 00098000
*     TBLXPARM BASED(TSLTBLXP);                                    0224 00099000
*   RESPECIFY                                                      0225 00100000
*     ICTPTF BASED(CKICTPTR);       /* ADDRESS ENTIRE PTF SECTION    */ 00101000
*   CKIRC=ZERO;                     /* CLEAR RETURN CODE             */ 00102000
         SLR   CKIRC,CKIRC                                         0226 00103000
*   TSLICTF=''B;                    /* CLEAR RETURN INFORMATION FLAGS*/ 00104000
         L     @08,@PC00001                                        0227 00105000
         L     @08,TSLPMLAD(,@08)                                  0227 00106000
         MVI   TSLICTF(@08),X'00'                                  0227 00107000
*   IF TSLIONXT=ON THEN             /* SEARCH FOR NEXT ENTRY?        */ 00108000
         TM    TSLIONXT(@08),B'10000000'                           0228 00109000
         BNO   @RF00228                                            0228 00110000
*     DO;                           /* YES,                          */ 00111000
*       CKISTRTI=2;                 /* START AT NEXT ENTRY           */ 00112000
         LA    CKISTRTI,2                                          0230 00113000
*       CKICTPTR=TSLICTP;           /* ICT BASE IS PREVIOUS FOUND  0231 00114000
*                                      ENTRY                         */ 00115000
         L     CKICTPTR,TSLICTP(,@08)                              0231 00116000
*       IF ICTPEND(1)=ICTENDNT THEN /* PREVIOUS IS END?              */ 00117000
         CLC   ICTPEND(4,CKICTPTR),ICTENDNT                        0232 00118000
         BNE   @RF00232                                            0232 00119000
*         DO;                                                      0233 00120000
*           CKIRC=TSLNTFND;         /* NOT FOUND RETURN CODE         */ 00121000
         LA    CKIRC,4                                             0234 00122000
*         END;                                                     0235 00123000
*       ELSE                                                       0236 00124000
*         ;                                                        0236 00125000
@RF00232 DS    0H                                                  0237 00126000
*     END;                                                         0237 00127000
*   ELSE                            /* SEARCH FOR 1ST ENTRY          */ 00128000
*     DO;                                                          0238 00129000
         B     @RC00228                                            0238 00130000
@RF00228 DS    0H                                                  0239 00131000
*       CKISTRTI=1;                 /* START AT NEXT ENTRY           */ 00132000
         LA    CKISTRTI,1                                          0239 00133000
*       CKICTPTR=CCAICPTF;          /* ICT BASE BEGINNING OF THE ICT */ 00134000
         L     @08,@CA00147                                        0240 00135000
         L     CKICTPTR,CCAICPTF-HMASMCCA(,@08)                    0240 00136000
*     END;                                                         0241 00137000
*                                                                  0242 00138000
*   /*****************************************************************/ 00139000
*   /*                                                               */ 00140000
*   /* SEARCH THE ICT PTF SECTION FOR THE SYSMOD                     */ 00141000
*   /*                                                               */ 00142000
*   /*****************************************************************/ 00143000
*                                                                  0242 00144000
*   DO CKIPTFX=CKISTRTI BY 1        /*                               */ 00145000
*         WHILE(ICTPEND(CKIPTFX)^=ICTENDNT/*                         */ 00146000
*         &CKIRC=TSLGOOD);                                         0242 00147000
@RC00228 LR    CKIPTFX,CKISTRTI                                    0242 00148000
         B     @DE00242                                            0242 00149000
@DL00242 DS    0H                                                  0243 00150000
*     IF ICTPTFS(CKIPTFX)=TSLISMD   /* MATCH CALLER''S SYSMOD ?      */ 00151000
*       THEN                                                       0243 00152000
         LR    @08,CKIPTFX                                         0243 00153000
         MH    @08,@CH02096                                        0243 00154000
         L     @15,@PC00001                                        0243 00155000
         L     @15,TSLPMLAD(,@15)                                  0243 00156000
         ST    @08,@TF00001                                        0243 00157000
         ALR   @08,CKICTPTR                                        0243 00158000
         AL    @08,@CF02115                                        0243 00159000
         CLC   ICTPTFS(7,@08),TSLISMD(@15)                         0243 00160000
         BNE   @RF00243                                            0243 00161000
*       DO;                         /* YES,                          */ 00162000
*                                                                  0244 00163000
*         /***********************************************************/ 00164000
*         /*                                                         */ 00165000
*         /* SYSMOD FOUND IN ICT PTF SECTION SET TSL BASED ON ITS    */ 00166000
*         /* STATUS AND PREVIOUSLY FOUND ENTRIES                     */ 00167000
*         /*                                                         */ 00168000
*         /***********************************************************/ 00169000
*                                                                  0245 00170000
*         TSLIFND=ON;               /* SYSMOD AT LEAST FOUND         */ 00171000
         OI    TSLIFND(@15),B'10000000'                            0245 00172000
*         IF ICTPNOGO(CKIPTFX)=ON THEN/* NOGO?                       */ 00173000
         LR    @08,CKICTPTR                                        0246 00174000
         AL    @08,@TF00001                                        0246 00175000
         AL    @08,@CF02117                                        0246 00176000
         TM    ICTPNOGO-8(@08),B'01000000'                         0246 00177000
         BNO   @RF00246                                            0246 00178000
*           DO;                     /*                               */ 00179000
*             IF TSLIOK=OFF         /* RETURN PTR NOT PREVIOUSLY SET?*/ 00180000
*                 &TSLISUP=OFF      /* ...                           */ 00181000
*                 &TSLIDEL=OFF      /* ...                           */ 00182000
*                 &TSLINOG=OFF THEN /* ... AND NOT ALREADY FOUND     */ 00183000
         TM    TSLIOK(@15),B'01110010'                             0248 00184000
         BNZ   @RF00248                                            0248 00185000
*               DO;                 /* YES,                          */ 00186000
*                 TSLICTP=ADDR(ICTPTF(CKIPTFX));/* SET RETURN PTR    */ 00187000
         L     @08,@TF00001                                        0250 00188000
         AL    @08,@CF02115                                        0250 00189000
         LA    @08,ICTPTF(@08,CKICTPTR)                            0250 00190000
         ST    @08,TSLICTP(,@15)                                   0250 00191000
*               END;                                               0251 00192000
*             ELSE                                                 0252 00193000
*               ;                   /* RETURN PTR SET BY HIGHER    0252 00194000
*                                      PRIORITY ENTRY                */ 00195000
@RF00248 DS    0H                                                  0253 00196000
*             TSLINOG=ON;           /* FOUND AS NOGO                 */ 00197000
         L     @08,@PC00001                                        0253 00198000
         L     @08,TSLPMLAD(,@08)                                  0253 00199000
         OI    TSLINOG(@08),B'00000010'                            0253 00200000
*           END;                    /*                               */ 00201000
*         ELSE                      /* NON NOGO ICT ENTRY            */ 00202000
*           DO;                     /*                               */ 00203000
         B     @RC00246                                            0255 00204000
@RF00246 DS    0H                                                  0256 00205000
*             IF ICTPDEXP(CKIPTFX)=ON/* DELETE?                      */ 00206000
*                 |ICTPDIMP(CKIPTFX)=ON THEN/*                       */ 00207000
         LR    @08,CKIPTFX                                         0256 00208000
         MH    @08,@CH02096                                        0256 00209000
         ST    @08,@TF00001                                        0256 00210000
         ALR   @08,CKICTPTR                                        0256 00211000
         AL    @08,@CF02119                                        0256 00212000
         TM    ICTPDEXP-13(@08),B'00110000'                        0256 00213000
         BZ    @RF00256                                            0256 00214000
*               DO;                 /* YES                           */ 00215000
*                 IF TSLIOK=OFF     /* RETURN PTR NOT PREVIOUSLY SET?*/ 00216000
*                     &TSLIDEL=OFF THEN/* AND NOT PREVIOSLY FOUND?   */ 00217000
         L     @08,@PC00001                                        0258 00218000
         L     @08,TSLPMLAD(,@08)                                  0258 00219000
         TM    TSLIOK(@08),B'01010000'                             0258 00220000
         BNZ   @RF00258                                            0258 00221000
*                   DO;             /* YES,                          */ 00222000
*                     TSLICTP=ADDR(ICTPTF(CKIPTFX));/* SET RETURN PTR*/ 00223000
         L     @01,@TF00001                                        0260 00224000
         AL    @01,@CF02115                                        0260 00225000
         LA    @15,ICTPTF(@01,CKICTPTR)                            0260 00226000
         ST    @15,TSLICTP(,@08)                                   0260 00227000
*                   END;                                           0261 00228000
*                 ELSE                                             0262 00229000
*                   ;               /* RETURN PTR SET BY HIGHER    0262 00230000
*                                      PRIORITY ENTRY                */ 00231000
@RF00258 DS    0H                                                  0263 00232000
*                 TSLIDEL=ON;       /* FOUND AS DELETED SYSMOD       */ 00233000
         L     @08,@PC00001                                        0263 00234000
         L     @08,TSLPMLAD(,@08)                                  0263 00235000
         OI    TSLIDEL(@08),B'00010000'                            0263 00236000
*                 IF ICTPDEXP(CKIPTFX)=ON/* EXPLICITLY DELETED ?     */ 00237000
*                   THEN                                           0264 00238000
         LR    @15,CKIPTFX                                         0264 00239000
         MH    @15,@CH02096                                        0264 00240000
         ALR   @15,CKICTPTR                                        0264 00241000
         AL    @15,@CF02119                                        0264 00242000
         TM    ICTPDEXP-13(@15),B'00100000'                        0264 00243000
         BNO   @RF00264                                            0264 00244000
*                   DO;             /*                               */ 00245000
*                     TSLIDLE=ON;   /* EXPLICITLY DELETED SYSMOD     */ 00246000
         OI    TSLIDLE(@08),B'00001000'                            0266 00247000
*                   END;            /*                               */ 00248000
*                 IF ICTPDIMP(CKIPTFX)=ON/* IMPLICITLY DELETED ?     */ 00249000
*                   THEN                                           0268 00250000
@RF00264 LR    @08,CKIPTFX                                         0268 00251000
         MH    @08,@CH02096                                        0268 00252000
         ALR   @08,CKICTPTR                                        0268 00253000
         AL    @08,@CF02119                                        0268 00254000
         TM    ICTPDIMP-13(@08),B'00010000'                        0268 00255000
         BNO   @RF00268                                            0268 00256000
*                   DO;             /*                               */ 00257000
*                     TSLIDLI=ON;   /* IMPLICITLY DELETED SYSMOD     */ 00258000
         L     @08,@PC00001                                        0270 00259000
         L     @08,TSLPMLAD(,@08)                                  0270 00260000
         OI    TSLIDLI(@08),B'00000100'                            0270 00261000
*                   END;            /*                               */ 00262000
*               END;                /*                               */ 00263000
*             ELSE                  /* NOT DELETED                   */ 00264000
*               DO;                 /*                               */ 00265000
         B     @RC00256                                            0273 00266000
@RF00256 DS    0H                                                  0274 00267000
*                 IF ICTPSUPN(CKIPTFX)^=ZERO THEN/*                  */ 00268000
         LR    @08,CKIPTFX                                         0274 00269000
         MH    @08,@CH02096                                        0274 00270000
         ST    @08,@TF00001                                        0274 00271000
         ALR   @08,CKICTPTR                                        0274 00272000
         AL    @08,@CF02123                                        0274 00273000
         SLR   @15,@15                                             0274 00274000
         ICM   @15,3,ICTPSUPN-20(@08)                              0274 00275000
         LTR   @15,@15                                             0274 00276000
         BZ    @RF00274                                            0274 00277000
*                   DO;             /*                               */ 00278000
*                     IF TSLIOK=OFF /* RETURN PTR NOT PREVIOUSLY SET?*/ 00279000
*                         &TSLIDEL=OFF/* ...                         */ 00280000
*                         &TSLISUP=OFF THEN/* ... AND NOT ALREADY  0276 00281000
*                                      SUPED                         */ 00282000
         L     @08,@PC00001                                        0276 00283000
         L     @08,TSLPMLAD(,@08)                                  0276 00284000
         TM    TSLIOK(@08),B'01110000'                             0276 00285000
         BNZ   @RF00276                                            0276 00286000
*                       DO;         /* YES,                          */ 00287000
*                         TSLICTP=ADDR(ICTPTF(CKIPTFX));/* SET RETURN   00288000
*                                      PTR                           */ 00289000
         L     @01,@TF00001                                        0278 00290000
         AL    @01,@CF02115                                        0278 00291000
         LA    @15,ICTPTF(@01,CKICTPTR)                            0278 00292000
         ST    @15,TSLICTP(,@08)                                   0278 00293000
*                       END;                                       0279 00294000
*                     ELSE                                         0280 00295000
*                       ;           /* RETURN PTR SET BY HIGHER    0280 00296000
*                                      PRIORITY ENTRY                */ 00297000
@RF00276 DS    0H                                                  0281 00298000
*                     TSLISUP=ON;   /* IMPLICITLY DELETED SYSMOD     */ 00299000
         L     @08,@PC00001                                        0281 00300000
         L     @08,TSLPMLAD(,@08)                                  0281 00301000
         OI    TSLISUP(@08),B'00100000'                            0281 00302000
*                   END;            /*                               */ 00303000
*                 ELSE              /* WONDER OF WONDERS THE SYSMOD     00304000
*                                      IS OK!                        */ 00305000
*                   DO;             /*                               */ 00306000
         B     @RC00274                                            0283 00307000
@RF00274 DS    0H                                                  0284 00308000
*                     IF TSLIOK=OFF THEN/* NOT ALREADY FOUND         */ 00309000
         L     @08,@PC00001                                        0284 00310000
         L     @08,TSLPMLAD(,@08)                                  0284 00311000
         TM    TSLIOK(@08),B'01000000'                             0284 00312000
         BNZ   @RF00284                                            0284 00313000
*                       DO;         /* YES,                          */ 00314000
*                         TSLICTP=ADDR(ICTPTF(CKIPTFX));/* SET RETURN   00315000
*                                      PTR                           */ 00316000
         LR    @15,CKIPTFX                                         0286 00317000
         MH    @15,@CH02096                                        0286 00318000
         AL    @15,@CF02115                                        0286 00319000
         LA    @15,ICTPTF(@15,CKICTPTR)                            0286 00320000
         ST    @15,TSLICTP(,@08)                                   0286 00321000
*                         TSLIOK=ON;/* FOUND AND IS GO               */ 00322000
         OI    TSLIOK(@08),B'01000000'                             0287 00323000
*                       END;                                       0288 00324000
*                   END;            /*                               */ 00325000
*               END;                /*                               */ 00326000
*           END;                    /*                               */ 00327000
*       END;                                                       0292 00328000
*     ELSE                          /* NOT A MATCHING ENTRY          */ 00329000
*       DO;                         /*                               */ 00330000
         B     @RC00243                                            0293 00331000
@RF00243 DS    0H                                                  0294 00332000
*       END;                        /*                               */ 00333000
*   END;                            /* END OF ICT PTF SEARCH LOOP    */ 00334000
@RC00243 AH    CKIPTFX,@CH00048                                    0295 00335000
@DE00242 LR    @08,CKIPTFX                                         0295 00336000
         MH    @08,@CH02096                                        0295 00337000
         AL    @08,@CF02115                                        0295 00338000
         L     @08,ICTPEND(@08,CKICTPTR)                           0295 00339000
         CL    @08,ICTENDNT                                        0295 00340000
         BE    @DC00242                                            0295 00341000
         LTR   CKIRC,CKIRC                                         0295 00342000
         BZ    @DL00242                                            0295 00343000
@DC00242 DS    0H                                                  0296 00344000
*   IF TSLIFND=OFF THEN             /* ENTRY NOT FOUND?              */ 00345000
         L     @08,@PC00001                                        0296 00346000
         L     @08,TSLPMLAD(,@08)                                  0296 00347000
         TM    TSLIFND(@08),B'10000000'                            0296 00348000
         BNZ   @RF00296                                            0296 00349000
*     DO;                                                          0297 00350000
*       CKIRC=TSLNTFND;             /* NOT FOUND RC                  */ 00351000
         LA    CKIRC,4                                             0298 00352000
*     END;                                                         0299 00353000
*   RETURN CODE(CKIRC);                                            0300 00354000
@RF00296 LR    @15,CKIRC                                           0300 00355000
         L     @14,12(,@13)                                        0300 00356000
         LM    @00,@12,20(@13)                                     0300 00357000
         BR    @14                                                 0300 00358000
*   RFY                                                            0301 00359000
*     TSLPARMS BASED;                                              0301 00360000
*   RFY                                                            0302 00361000
*     TBLXPARM BASED;                                              0302 00362000
*   RFY                                                            0303 00363000
*     ICTPTF BASED;                 /*                               */ 00364000
*   END CHKICT;                                                    0304 00365000
*                                                                  0305 00366000
*   /*****************************************************************/ 00367000
*   /*                                                               */ 00368000
*   /* LSTICTCK - FIND ICT PTF ENTRY THAT CONTAINS THE NAMED SYSMOD  */ 00369000
*   /* IN THE SPECIFIED INDEX LIST                                   */ 00370000
*   /*                                                               */ 00371000
*   /*****************************************************************/ 00372000
*                                                                  0305 00373000
*LSTICTCK:                                                         0305 00374000
*   PROC;                                                          0305 00375000
LSTICTCK STM   @14,@12,12(@13)                                     0305 00376000
*   DCL                                                            0306 00377000
*     1 LICFLAGS BIT(8),            /* LOCAL CONTROL FLAGS           */ 00378000
*      2 LICFOUND BIT(1),           /* NAME FOUND IN INDEX LIST OF AN   00379000
*                                      ICT PTF ENT                   */ 00380000
*      2 * BIT(7);                  /* UNUSED                        */ 00381000
*   DCL                                                            0307 00382000
*     LICPTFX FIXED(15);            /* INDEX FOR ICT PTF SECTION LOOP*/ 00383000
*   DCL                                                            0308 00384000
*     LICNDXI FIXED(15);            /* INDEX FOR INDEX LIST SEARCH   */ 00385000
*   DCL                                                            0309 00386000
*     LICRC FIXED(15);              /* RETURN CODE                   */ 00387000
*   DCL                                                            0310 00388000
*     LICICTP PTR(31);              /* BASE FOR ICT PTF SECTION      */ 00389000
*   DCL                                                            0311 00390000
*     LICSTRTI FIXED(15);           /* START OF SEARCH INDEX         */ 00391000
*   DCL                                                            0312 00392000
*     LICPTR31 PTR(31) BASED;       /* BASED POINTER                 */ 00393000
*   DCL                                                            0313 00394000
*     LICIRQOF FIXED(31);           /* OFFSET OF ++IF REQ CHAIN      */ 00395000
*   DCL                                                            0314 00396000
*     LICLSTI FIXED(31);            /* INCRAEMENT FOR SEARCH (1 OR 2)*/ 00397000
*   RFY                                                            0315 00398000
*     TSLPARMS BASED(ADDR(PARM1));                                 0315 00399000
*   RFY                                                            0316 00400000
*     TBLXPARM BASED(TSLTBLXP);                                    0316 00401000
*   RFY                                                            0317 00402000
*     ICTPTF BASED(LICICTP);                                       0317 00403000
*   RFY                                                            0318 00404000
*     ICTENTLS BASED((ADDR(ICTPTF(LICPTFX))+TSLLOFST)->LICPTR31);  0318 00405000
*   LICRC=ZERO;                     /* CLEAR RETURN CODE             */ 00406000
         SLR   LICRC,LICRC                                         0319 00407000
*   LICFLAGS=''B;                   /* CLEAR LOCAL CONTROL FLAGS     */ 00408000
         MVI   LICFLAGS,X'00'                                      0320 00409000
*   LICIRQOF=ADDR(ICTPICHN(1))      /* COMPUTE OFFSET OF ++IF REQ  0321 00410000
*                                      CHAIN PTR                     */ 00411000
*       -ADDR(ICTPTF(1));                                          0321 00412000
         L     @08,LICICTP                                         0321 00413000
         LA    LICIRQOF,ICTPICHN(,@08)                             0321 00414000
         SLR   LICIRQOF,@08                                        0321 00415000
*   IF TSLLOFST=LICIRQOF THEN       /* ++IF REQ INDEX BEING        0322 00416000
*                                      PROCESSED?                    */ 00417000
