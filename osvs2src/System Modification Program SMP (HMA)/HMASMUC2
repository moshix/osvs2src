         BCTR  @07,0                                               0755 02124000
         L     @06,SCPCHAR(,@15)                                   0755 02125000
         EX    @07,@SM02037                                        0755 02126000
*   /*****************************************************************/ 02127000
*   /*                                                               */ 02128000
*   /* CONVERT PASSED VALUE INTO NEWRC                               */ 02129000
*   /*                                                               */ 02130000
*   /*****************************************************************/ 02131000
*                                                                  0756 02132000
*   CHAR8=EBCZEROS;                 /*                               */ 02133000
         MVC   CHAR8(8),@CC01461                                   0756 02134000
*   CHAR8(9-SCPPMLN:8)=SCPSTRNG(1:SCPPMLN);/* MOVE DATA              */ 02135000
         LCR   @14,@14                                             0757 02136000
         AH    @14,@CH00538                                        0757 02137000
         LA    @01,CHAR8-1(@14)                                    0757 02138000
         LCR   @14,@14                                             0757 02139000
         AH    @14,@CH00036                                        0757 02140000
         EX    @14,@SM02039                                        0757 02141000
*   PACK(PACK8,CHAR8);              /* PACK DATA                     */ 02142000
         PACK  PACK8(8),CHAR8(8)                                   0758 02143000
*   CVB(CVB15,PACK8);               /*                               */ 02144000
         CVB   @14,PACK8                                           0759 02145000
         STH   @14,CVB15                                           0759 02146000
*   NEWRC=CVB15;                    /* MOVE RC TO WORK AREA          */ 02147000
*                                                                  0760 02148000
         STC   @14,NEWRC                                           0760 02149000
*   /*****************************************************************/ 02150000
*   /*                                                               */ 02151000
*   /* CONVERT DATA IN RC FOR POSSIBLE MESSAGES                      */ 02152000
*   /*                                                               */ 02153000
*   /*****************************************************************/ 02154000
*                                                                  0761 02155000
*   CVD(PGMRC,PACK8);               /*                               */ 02156000
         L     @01,RCPTR                                           0761 02157000
         SLR   @00,@00                                             0761 02158000
         IC    @00,PGMRC(,@01)                                     0761 02159000
         CVD   @00,PACK8                                           0761 02160000
*   UNPK(CHAR12,PACK8);             /*                               */ 02161000
         UNPK  CHAR12(12),PACK8(8)                                 0762 02162000
*   CHAR12(12)=CHAR12(12)|HEX0;     /*                               */ 02163000
         OI    CHAR12+11,X'F0'                                     0763 02164000
*   MSGPART3(1:2)=CHAR12(11:12);    /*                               */ 02165000
*                                                                  0764 02166000
         MVC   MSGPART3(2),CHAR12+10                               0764 02167000
*   /*****************************************************************/ 02168000
*   /*                                                               */ 02169000
*   /* CHECK DATA ENTEREED, FUNCTION REQUESTED, ANF CURRENT DATA IN  */ 02170000
*   /* ENTRY. ALSO CHECK TO INSURE THAT RC MAX NOT EXCEEDED.         */ 02171000
*   /*                                                               */ 02172000
*   /*****************************************************************/ 02173000
*                                                                  0765 02174000
*   IF NEWRC>STOPSMP                /*                               */ 02175000
*     THEN                          /*                               */ 02176000
         CLI   NEWRC,16                                            0765 02177000
         BNH   @RF00765                                            0765 02178000
*     DO;                           /*                               */ 02179000
*       SCPRETRN=SCPSTOP;           /*                               */ 02180000
         MVI   SCPRETRN(@15),X'08'                                 0767 02181000
*       RETURN;                     /*                               */ 02182000
         B     @EL00004                                            0768 02183000
*     END;                          /*                               */ 02184000
*   IF REP=ON                       /* IF REP BUT NOT TOTAL ADD      */ 02185000
*     THEN                          /*                               */ 02186000
@RF00765 TM    REP,B'10000000'                                     0770 02187000
         BNO   @RF00770                                            0770 02188000
*     DO;                           /*                               */ 02189000
*       IF PGMRC=IOPEOLST&TOTAL=OFF /* AND RC NOT PRESENT            */ 02190000
*         THEN                      /*                               */ 02191000
         L     @01,RCPTR                                           0772 02192000
         CLI   PGMRC(@01),255                                      0772 02193000
         BNE   @RF00772                                            0772 02194000
         TM    TOTAL,B'00010000'                                   0772 02195000
         BNZ   @RF00772                                            0772 02196000
*         CALL UCL358I;             /* ISSUE WARNING AND             */ 02197000
         BAL   @14,UCL358I                                         0773 02198000
*       PGMRC=NEWRC;                /*                               */ 02199000
@RF00772 L     @01,RCPTR                                           0774 02200000
         MVC   PGMRC(1,@01),NEWRC                                  0774 02201000
*     END;                          /*                               */ 02202000
*   ELSE                            /*                               */ 02203000
*     IF ADD=ON                     /* IF ADD                        */ 02204000
*       THEN                        /*                               */ 02205000
         B     @RC00770                                            0776 02206000
@RF00770 TM    ADD,B'00100000'                                     0776 02207000
         BNO   @RF00776                                            0776 02208000
*       DO;                         /*                               */ 02209000
*         IF PGMRC=IOPEOLST         /* AND NOT PRESENT               */ 02210000
*           THEN                    /*                               */ 02211000
