*                                                                  0369 00723000
         B     @EL00003                                            0369 00724000
*   /*****************************************************************/ 00725000
*   /*                                                               */ 00726000
*   /* THIS ROUTINE IS ENTERED WHEN A PERIOD IS FOUND IMMEDIATELY    */ 00727000
*   /* AFTER THE DEL AND NAME OF THE UCL STATEMENT. SET A SWITCH TO  */ 00728000
*   /* INDICATE DELETE ONLY OPERATION AFTER RETURN FROM SCAN         */ 00729000
*   /*                                                               */ 00730000
*   /*****************************************************************/ 00731000
*                                                                  0370 00732000
*RSYSTD:                                                           0370 00733000
*RSMDTD:                                                           0370 00734000
*RMACTD:                                                           0370 00735000
*RSRCTD:                                                           0370 00736000
*   ENTRY;                          /*                               */ 00737000
RSYSTD   DS    0H                                                  0370 00738000
RSMDTD   DS    0H                                                  0370 00739000
RMACTD   DS    0H                                                  0370 00740000
RSRCTD   STM   @14,@12,@SA00003                                    0370 00741000
*   IF DEL=ON&UCLFLUSH=OFF          /* DEL VERB AND NOT TERMINATE?   */ 00742000
*     THEN                                                         0371 00743000
         TM    DEL,B'01000000'                                     0371 00744000
         BNO   @RF00371                                            0371 00745000
         L     @15,@PC00001                                        0371 00746000
         TM    UCLFLUSH(@15),B'00000010'                           0371 00747000
         BNZ   @RF00371                                            0371 00748000
*     TOTALDEL=ON;                  /* INDICATE TOTAL DELETE         */ 00749000
         OI    TOTALDEL,B'00001000'                                0372 00750000
*   RETURN;                         /* RETURN TO HMSMSCN             */ 00751000
         B     @EL00003                                            0373 00752000
*                                                                  0374 00753000
*   /*****************************************************************/ 00754000
*   /*                                                               */ 00755000
*   /* PERFORM A LOCATE ON THE CDS ENTRY NOW THAT WE KNOW THE FULL   */ 00756000
*   /* NAME. IF THE OPERATION IS ADD, THE LOCATE MUST FAIL OR THERE  */ 00757000
*   /* IS AN ERROR. FOR ALL OTHER OPERATIONS THE LOCATE MUST FIND THE*/ 00758000
*   /* ENTRY OR THERE IS AN ERROR. THE LOCATE OPERATION ALSO READS IN*/ 00759000
*   /* THE EXTENDED DIRECTORY WHICH WILL BE USED FOR THE UPDATE      */ 00760000
*   /*                                                               */ 00761000
*   /*****************************************************************/ 00762000
*                                                                  0374 00763000
*RSYSNAM:                                                          0374 00764000
*RSMDNAM:                                                          0374 00765000
*RMACNAM:                                                          0374 00766000
*RSRCNAM:                                                          0374 00767000
*   ENTRY;                          /*                               */ 00768000
RSYSNAM  DS    0H                                                  0374 00769000
RSMDNAM  DS    0H                                                  0374 00770000
RMACNAM  DS    0H                                                  0374 00771000
RSRCNAM  STM   @14,@12,@SA00003                                    0374 00772000
*   IOPNAME=SCPSTRNG(1:SCPPMLN);    /*                               */ 00773000
         MVI   IOPNAME+1(IOPPTR),C' '                              0375 00774000
         MVC   IOPNAME+2(6,IOPPTR),IOPNAME+1(IOPPTR)               0375 00775000
         L     @15,SCPPTR                                          0375 00776000
         LH    @14,SCPPMLN(,@15)                                   0375 00777000
         BCTR  @14,0                                               0375 00778000
         L     @01,SCPCHAR(,@15)                                   0375 00779000
         EX    @14,@SM02023                                        0375 00780000
*RXXXLOC:                                                          0376 00781000
*   IOPDSID=UC2DSID;                /* SET DSID                      */ 00782000
RXXXLOC  MVC   IOPDSID(1,IOPPTR),UC2DSID                           0376 00783000
*   IOPFUNCT=IOPLOC;                /* PREPARE FOR LOCATE OPERATION  */ 00784000
         MVI   IOPFUNCT(IOPPTR),X'03'                              0377 00785000
*   CALL HMASMIO(HMASMIOP);         /* PERFORM THE LOCATE            */ 00786000
*                                                                  0378 00787000
         ST    IOPPTR,@AL00001                                     0378 00788000
         L     @15,@CV01409                                        0378 00789000
         LA    @01,@AL00001                                        0378 00790000
         BALR  @14,@15                                             0378 00791000
*   /*****************************************************************/ 00792000
*   /*                                                               */ 00793000
*   /* CHECK FOR ADD OR REP OF ENTRY NOT FOUND. SET INDICATOR THAT   */ 00794000
*   /* ENTRY IS TOTAL ADD                                            */ 00795000
*   /*                                                               */ 00796000
*   /*****************************************************************/ 00797000
*                                                                  0379 00798000
*   IF(ADD=ON|REP=ON)               /* ADD/REP NEW ENTRY             */ 00799000
*       &IOPRETRN=IOPNTFND          /* AND NTRY NOT FOUND            */ 00800000
*     THEN                                                         0379 00801000
         TM    ADD,B'10100000'                                     0379 00802000
         BZ    @RF00379                                            0379 00803000
         CLI   IOPRETRN(IOPPTR),4                                  0379 00804000
         BNE   @RF00379                                            0379 00805000
*     DO;                           /*                               */ 00806000
*       IF PTF=ON                   /* IF ADD OF NEW PTF             */ 00807000
*         THEN                      /*                               */ 00808000
         TM    PTF,B'10000000'                                     0381 00809000
         BNO   @RF00381                                            0381 00810000
*         DO;                       /*                               */ 00811000
*           INVLOP=ON;              /* INVALID OPERATION             */ 00812000
         OI    INVLOP,B'00001000'                                  0383 00813000
*           SCPRETRN=SCPNDSCN;      /* STOP SCAN WITH RC=0           */ 00814000
         L     @01,SCPPTR                                          0384 00815000
         MVI   SCPRETRN(@01),X'04'                                 0384 00816000
*         END;                      /*                               */ 00817000
*       ELSE                        /*                               */ 00818000
*         TOTAL=ON;                 /* YES INDICATE THIS IN TABLE    */ 00819000
         B     @RC00381                                            0386 00820000
@RF00381 OI    TOTAL,B'00010000'                                   0386 00821000
*     END;                          /*                               */ 00822000
*                                                                  0387 00823000
@RC00381 DS    0H                                                  0388 00824000
*   /*****************************************************************/ 00825000
*   /*                                                               */ 00826000
