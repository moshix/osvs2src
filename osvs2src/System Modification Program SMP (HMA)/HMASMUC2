         STC   @15,UCLRTNCD                                        0289 00429000
*       END;                        /* END RC RCD AND SCANND         */ 00430000
*   END;                            /* END READ/SCAN LOOP            */ 00431000
@RF00228 DS    0H                                                  0291 00432000
@DE00213 CLI   UCLRTNCD,16                                         0291 00433000
         BNL   @DC00213                                            0291 00434000
         TM    UCLTERM,B'00010000'                                 0291 00435000
         BNZ   @DC00213                                            0291 00436000
         L     @15,@PC00001+4                                      0291 00437000
         TM    CRPEOFEN(@15),B'10000000'                           0291 00438000
         BZ    @DL00213                                            0291 00439000
@DC00213 DS    0H                                                  0292 00440000
*                                                                  0292 00441000
*   /*****************************************************************/ 00442000
*   /*                                                               */ 00443000
*   /* IF LOOP ENDED DUE TO I/O ERROR FROM READ ROUTINE THEN ISSUE   */ 00444000
*   /* MESSAGE NOW                                                   */ 00445000
*   /*                                                               */ 00446000
*   /*****************************************************************/ 00447000
*                                                                  0292 00448000
*   IF CRPRETRN=CRPEOF              /* IF END OF FILE                */ 00449000
*       &UCLTERM=OFF                /* BEFORE ENDUCL                 */ 00450000
*     THEN                          /*                               */ 00451000
         L     @15,@PC00001+4                                      0292 00452000
         CLI   CRPRETRN(@15),4                                     0292 00453000
         BNE   @RF00292                                            0292 00454000
         TM    UCLTERM,B'00010000'                                 0292 00455000
         BNZ   @RF00292                                            0292 00456000
*     CALL UCL258I;                 /*                               */ 00457000
         BAL   @14,UCL258I                                         0293 00458000
*   ELSE                            /*                               */ 00459000
*     IF CRPRETRN>CRPEOF            /* IF GREATER THEN EOF           */ 00460000
*       THEN                        /*                               */ 00461000
         B     @RC00292                                            0294 00462000
@RF00292 L     @15,@PC00001+4                                      0294 00463000
         CLI   CRPRETRN(@15),4                                     0294 00464000
         BNH   @RF00294                                            0294 00465000
*       CALL UCL256I;               /*                               */ 00466000
*                                                                  0295 00467000
         BAL   @14,UCL256I                                         0295 00468000
*   /*****************************************************************/ 00469000
*   /*                                                               */ 00470000
*   /* UCL UPDATE COMPLETED SUCCESSFULLY RETURN TO THE CALLER        */ 00471000
*   /*                                                               */ 00472000
*   /*****************************************************************/ 00473000
*                                                                  0296 00474000
*   RETURN CODE(UCLRTNCD);          /* RETURN TO CALLER WITH HIGHEST    00475000
*                                      RETURN CODE ENCOUNTERED       */ 00476000
@RF00294 DS    0H                                                  0296 00477000
@RC00292 SLR   @15,@15                                             0296 00478000
         IC    @15,UCLRTNCD                                        0296 00479000
         L     @13,4(,@13)                                         0296 00480000
         L     @14,12(,@13)                                        0296 00481000
         LM    @00,@12,20(@13)                                     0296 00482000
         BR    @14                                                 0296 00483000
         EJECT                                                          00484000
*                                                                  0297 00485000
*   /*****************************************************************/ 00486000
*   /*                                                               */ 00487000
*   /* THESE ROUTINE ARE CALLED WHEN THE FUNCTION KEYWORD IS         */ 00488000
*   /* ENCOUNTERED. THEY SAVE AN INDICATOR AS TO THE TYPE FUNCTION   */ 00489000
*   /* REQUESTED                                                     */ 00490000
*   /*                                                               */ 00491000
*   /*****************************************************************/ 00492000
*                                                                  0297 00493000
*RXXXFNC:                           /*                               */ 00494000
*   PROCEDURE;                      /*                               */ 00495000
RXXXFNC  STM   @14,@12,12(@13)                                     0297 00496000
*RENDUCL:                           /*                               */ 00497000
