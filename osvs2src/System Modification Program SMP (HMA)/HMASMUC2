*         UCLFLUSH=ON;              /* SET FLUSH BIT                 */ 00107000
         L     @15,@PC00001                                        0217 00108000
         OI    UCLFLUSH(@15),B'00000010'                           0217 00109000
*       END;                        /*                               */ 00110000
*     ELSE                          /*                               */ 00111000
*       SCPNORT=OFF;                /* CALL ROUTS                    */ 00112000
         B     @RC00214                                            0219 00113000
@RF00214 L     @01,SCPPTR                                          0219 00114000
         NI    SCPNORT(@01),B'11111011'                            0219 00115000
*     UCLSWTBL=''B;                 /* CLEAR THE CONTROL SWITCHES    */ 00116000
@RC00214 XC    UCLSWTBL(3),UCLSWTBL                                0220 00117000
*     DO LOOPINDX=1 TO CCAPESIZ;    /* CLEAR IOP TO ZERO             */ 00118000
         LA    @15,1                                               0221 00119000
