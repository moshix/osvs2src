         CLI   PGMRC(@15),255                                      0778 02213000
         BNE   @RF00778                                            0778 02214000
*           PGMRC=NEWRC;            /* ADD RC                        */ 02215000
         MVC   PGMRC(1,@15),NEWRC                                  0779 02216000
*         ELSE                      /*                               */ 02217000
*           CALL UCL356I;           /* IF PRESENT ISSUE ERR MSG      */ 02218000
         B     @RC00778                                            0780 02219000
@RF00778 BAL   @14,UCL356I                                         0780 02220000
*       END;                        /*                               */ 02221000
*     ELSE                          /*                               */ 02222000
*       DO;                         /* IF DELETE                     */ 02223000
         B     @RC00776                                            0782 02224000
@RF00776 DS    0H                                                  0783 02225000
*         IF PGMRC=NEWRC            /*                               */ 02226000
*           THEN                    /*                               */ 02227000
         L     @01,RCPTR                                           0783 02228000
         CLC   PGMRC(1,@01),NEWRC                                  0783 02229000
         BNE   @RF00783                                            0783 02230000
*           DO;                     /*                               */ 02231000
*RRCD:                              /*                               */ 02232000
*             ENTRY;                /*                               */ 02233000
