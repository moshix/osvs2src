RXXXNAM  STM   @14,@12,@SA00002                                    0245 00371000
@EC00245 DS    0H                                                  0246 00372000
*   IOPTYPE=IOPSMD;                 /* INDICATE SYSMOD ENTRY         */ 00373000
         MVI   IOPTYPE(IOPPTR),X'0B'                               0246 00374000
*   IOPNAME=SCPSTRNG(1:SCPPMLN);    /*                               */ 00375000
         MVI   IOPNAME+1(IOPPTR),C' '                              0247 00376000
         MVC   IOPNAME+2(6,IOPPTR),IOPNAME+1(IOPPTR)               0247 00377000
         L     @08,SCPPTR                                          0247 00378000
         LH    @15,SCPPMLN(,@08)                                   0247 00379000
         BCTR  @15,0                                               0247 00380000
         L     @08,SCPCHAR(,@08)                                   0247 00381000
         EX    @15,@SM01504                                        0247 00382000
*   PTFNO=IOPNAME;                  /* 7 CHAR ALPHA NUMERIC          */ 00383000
         MVC   PTFNO(7),IOPNAME(IOPPTR)                            0248 00384000
*   IOPDSID=IOPSCDS;                /* SET DSID FOR SCDS             */ 00385000
         MVI   IOPDSID(IOPPTR),X'21'                               0249 00386000
*   IOPFUNCT=IOPLOC;                /* PREPARE FOR LOCATE OPERATION  */ 00387000
         MVI   IOPFUNCT(IOPPTR),X'03'                              0250 00388000
*   CALL HMASMIO(HMASMIOP);         /* PERFORM THE LOCATE            */ 00389000
*                                                                  0251 00390000
         ST    IOPPTR,@AL00001                                     0251 00391000
         L     @15,@CV01390                                        0251 00392000
         LA    @01,@AL00001                                        0251 00393000
         BALR  @14,@15                                             0251 00394000
*   /*****************************************************************/ 00395000
*   /*                                                               */ 00396000
*   /* CHECK AT THIS POINT FOR DELETE ERRORS.                        */ 00397000
*   /*                                                               */ 00398000
*   /*****************************************************************/ 00399000
*                                                                  0252 00400000
*   IF IOPRETRN=IOPNTFND            /* IF DEL AND NOT FOUND          */ 00401000
*     THEN                          /* THE CHECK IS MADE FOR THE   0252 00402000
*                                      ERROR CONDITION               */ 00403000
         CLI   IOPRETRN(IOPPTR),4                                  0252 00404000
         BNE   @RF00252                                            0252 00405000
*     DO;                           /* WRONG COMBINATION WAS FOUND   */ 00406000
*       DELERR=ON;                  /* SET SW                        */ 00407000
         OI    DELERR,B'00000100'                                  0254 00408000
*       SCPNORT=ON;                 /* DONT CALL ROUTS               */ 00409000
         L     @08,SCPPTR                                          0255 00410000
         OI    SCPNORT(@08),B'00000100'                            0255 00411000
*     END;                                                         0256 00412000
*   IF IOPRETRN>IOPNTFND            /* IF SERIOUS ERROR IN LOC       */ 00413000
*     THEN                          /* YES - ERR                     */ 00414000
@RF00252 CLI   IOPRETRN(IOPPTR),4                                  0257 00415000
         BNH   @RF00257                                            0257 00416000
*     DO;                           /*                               */ 00417000
*       SCPRET=MAX(SCPRET,IOPRETRN);/*                               */ 00418000
         L     @08,SCPPTR                                          0259 00419000
         SLR   @15,@15                                             0259 00420000
         IC    @15,SCPRET(,@08)                                    0259 00421000
         SLR   @14,@14                                             0259 00422000
         IC    @14,IOPRETRN(,IOPPTR)                               0259 00423000
         CR    @15,@14                                             0259 00424000
         BNL   *+6                                                      00425000
         LR    @15,@14                                             0259 00426000
         STC   @15,SCPRET(,@08)                                    0259 00427000
*       IF IOPRETRN=IOPIOERR        /* CHECK FOR AN I/O ERROR ON THE    00428000
*                                      LOCATE OPERATION              */ 00429000
*         THEN                      /* YES - THERE WAS AN ERROR      */ 00430000
         CLI   IOPRETRN(IOPPTR),16                                 0260 00431000
         BNE   @RF00260                                            0260 00432000
*         CALL UCL256I;             /* ISSUE THE MESSAGE             */ 00433000
         BAL   @14,UCL256I                                         0261 00434000
