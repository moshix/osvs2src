         LR    @13,@14                                             0001 00017000
         MVC   @PC00001(8),0(@01)                                  0001 00018000
*                                                                  0176 00019000
*   /*****************************************************************/ 00020000
*   /*                                                               */ 00021000
*   /* HMASMUC4 - SMP UPDATE CONTROL LANGUAGE HANDLER.               */ 00022000
*   /*                                                               */ 00023000
*   /*****************************************************************/ 00024000
*                                                                  0176 00025000
*   IOPPTR=CCAIOPTR;                /* SET ADDRESSABILITY FOR I/O  0176 00026000
