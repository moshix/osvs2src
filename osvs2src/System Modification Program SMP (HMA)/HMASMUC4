         L     @08,@PC00001                                        0215 00213000
         TM    UCLFLUSH(@08),B'00000010'                           0215 00214000
         BNZ   @RF00215                                            0215 00215000
*           DO;                     /*                               */ 00216000
*             IF ERR1SW=ON          /*                               */ 00217000
