*   CALLUXD=ON;                     /* SET CALL FOR UXD RTN CK       */ 00364000
         OI    CALLUXD,B'10000000'                                 0115 00365000
*   CALL UXCRTNCK(DETERRC);         /* CHECK ITS RETURN CODE         */ 00366000
         LA    @01,@AL00116                                        0116 00367000
         BAL   @14,UXCRTNCK                                        0116 00368000
*   DETERRC=RTNCODE;                /*                               */ 00369000
         STC   RTNCODE,DETERRC                                     0117 00370000
*   CALLUXD=OFF;                    /* NOT CALL FOR UXD CK           */ 00371000
         NI    CALLUXD,B'01111111'                                 0118 00372000
*   RFY                                                            0119 00373000
*     RTNCODE UNRSTD;               /*                               */ 00374000
*   IF DETERRC=ZERO                 /* IF RTNCD OK THEN              */ 00375000
