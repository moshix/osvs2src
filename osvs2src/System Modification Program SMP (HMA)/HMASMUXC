*UXCINTF:                           /*                               */ 00405000
*   PROCEDURE;                      /*                               */ 00406000
UXCINTF  STM   @14,@12,@SA00004                                    0127 00407000
*   DECLARE                         /*                               */ 00408000
*     INTFRC FIXED(8);              /* RETURN CODE                   */ 00409000
*   DECLARE                         /*                               */ 00410000
*     SAVEREGS CHAR(60) BDY(WORD);  /*                               */ 00411000
*   DECLARE                         /*                               */ 00412000
*     SMPEXIT ENTRY BASED(EXITADDR(UXCCODE));/* USER EXIT            */ 00413000
*   UXPUXNUM=UXCCODE;               /* EXIT CODE                     */ 00414000
