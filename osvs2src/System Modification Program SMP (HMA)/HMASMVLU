         L     @10,@PC00001                                        0052 00103000
         TM    VLPFMSG(@10),B'10000000'                            0052 00104000
         BNO   @RF00052                                            0052 00105000
*             DO;                   /*                               */ 00106000
*               MGPVARPT(1)=ADDR(VLPSTYPE);/*                        */ 00107000
