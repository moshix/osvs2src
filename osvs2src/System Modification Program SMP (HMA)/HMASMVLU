*   MGPHLDS=ON;                     /* SET FOR MSG TO LOG            */ 00032000
*   MGPPRINT=ON;                    /* AND PRINTER                   */ 00033000
         OI    MGPHLDS,B'10010000'                                 0038 00034000
         EJECT                                                          00035000
*                                                                  0039 00036000
*   /*****************************************************************/ 00037000
*   /*                                                               */ 00038000
*   /* FOR A REPLACE OR CLEAR FUNCTION DELETE ALL ENTRIES MATCHING   */ 00039000
*   /* THE INPUT TYPE                                                */ 00040000
*   /*                                                               */ 00041000
*   /*****************************************************************/ 00042000
*                                                                  0039 00043000
*   IF VLPFUNCT=VLPREP              /* IF REP OR CLEAR THEN FIRST    */ 00044000
*       |VLPFUNCT=VLPCLR            /* ANY EXISTING ENTRIES          */ 00045000
*     THEN                          /* OF THE SPECIFIED TYPE         */ 00046000
         CLI   VLPFUNCT(@10),3                                     0039 00047000
         BE    @RT00039                                            0039 00048000
         CLI   VLPFUNCT(@10),4                                     0039 00049000
         BNE   @RF00039                                            0039 00050000
@RT00039 DS    0H                                                  0040 00051000
*     DO;                           /*                               */ 00052000
*       FFOUND=OFF;                 /* NOTHING FOUND TO DELETE YET   */ 00053000
