         MVI   VLPRETRN(@10),X'00'                                 0034 00027000
*   VPTR=VLPLPTR;                   /* PTR TO VARIABLE LIST          */ 00028000
         L     VPTR,VLPLPTR(,@10)                                  0035 00029000
*   HMASMMGP=''B;                   /* CLEAR MSG PARM                */ 00030000
