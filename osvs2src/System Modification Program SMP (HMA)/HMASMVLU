*   /* COMPRESS THE LIST                                             */ 00427000
*   /*                                                               */ 00428000
*   /*****************************************************************/ 00429000
*                                                                  0133 00430000
*VLUCMPRS:                                                         0133 00431000
*   PROCEDURE;                      /* COMPRESS THE LIST             */ 00432000
         B     @EL00001                                            0133 00433000
VLUCMPRS STM   @14,@12,12(@13)                                     0133 00434000
*   DECLARE                         /*                               */ 00435000
*     P1 FIXED(31),                 /* FROM INDEX THRU LIST          */ 00436000
*     P2 FIXED(31);                 /* TO INDEX THRU LIST            */ 00437000
*   P2=1;                           /* INIT TO INDEX                 */ 00438000
         LA    @10,1                                               0135 00439000
         LR    P2,@10                                              0135 00440000
*   DO P1=1 BY 1 WHILE(             /* LOOP THRU ACTIVE LIST ENTRIES */ 00441000
*         VENTMRK(P1)^=LISTEND);    /* TILL END                      */ 00442000
         LR    P1,@10                                              0136 00443000
         B     @DE00136                                            0136 00444000
@DL00136 DS    0H                                                  0137 00445000
*     IF VENT(P1)^=NULLITM          /* IF DELETED ENTRY              */ 00446000
*       THEN                        /* THEN COMPRESS IT              */ 00447000
         LA    @10,9                                               0137 00448000
         LR    @01,P1                                              0137 00449000
         MR    @00,@10                                             0137 00450000
         ST    @01,@TF00001                                        0137 00451000
         ALR   @01,VPTR                                            0137 00452000
         AL    @01,@CF00473                                        0137 00453000
         CLC   VENT(9,@01),@CB00415                                0137 00454000
         BE    @RF00137                                            0137 00455000
*       DO;                         /*                               */ 00456000
*         VENT(P2)=VENT(P1);        /* MOVE FROM ENTRY TO TO ENTRY   */ 00457000
         LR    @01,@10                                             0139 00458000
         MR    @00,P2                                              0139 00459000
         ALR   @01,VPTR                                            0139 00460000
         AL    @01,@CF00473                                        0139 00461000
         LR    @10,VPTR                                            0139 00462000
         AL    @10,@TF00001                                        0139 00463000
         AL    @10,@CF00473                                        0139 00464000
         MVC   @TS00001(9),VENT(@10)                               0139 00465000
         MVC   VENT(9,@01),@TS00001                                0139 00466000
*         P2=P2+1;                  /* UPDATE TO INDEX               */ 00467000
         AH    P2,@CH00039                                         0140 00468000
*       END;                        /* END COMPRESS IT               */ 00469000
*   END;                            /* END COMPRESS LOOP             */ 00470000
@RF00137 AH    P1,@CH00039                                         0142 00471000
@DE00136 LA    @10,9                                               0142 00472000
         LR    @01,P1                                              0142 00473000
         MR    @00,@10                                             0142 00474000
         ALR   @01,VPTR                                            0142 00475000
         AL    @01,@CF00473                                        0142 00476000
         CLI   VENTMRK(@01),X'FF'                                  0142 00477000
         BNE   @DL00136                                            0142 00478000
*   VENTMRK(P2)=LISTEND;            /* RESET END OF LIST             */ 00479000
         LR    @01,@10                                             0143 00480000
         MR    @00,P2                                              0143 00481000
         ALR   @01,VPTR                                            0143 00482000
         AL    @01,@CF00473                                        0143 00483000
         MVI   VENTMRK(@01),X'FF'                                  0143 00484000
*   END VLUCMPRS;                   /* END COMPRESS PROC             */ 00485000
@EL00002 DS    0H                                                  0144 00486000
@EF00002 DS    0H                                                  0144 00487000
@ER00002 LM    @14,@12,12(@13)                                     0144 00488000
         BR    @14                                                 0144 00489000
         EJECT                                                          00490000
*                                                                  0145 00491000
*   /*****************************************************************/ 00492000
*   /*                                                               */ 00493000
*   /* CHECK THE DATA TO INSURE NO NON-PRINTABLE CHARACTERS          */ 00494000
*   /*                                                               */ 00495000
*   /*****************************************************************/ 00496000
*                                                                  0145 00497000
*VLUSTMSG:                                                         0145 00498000
*   PROCEDURE;                      /* SET UP DATA AREA FOR MSG      */ 00499000
VLUSTMSG STM   @14,@12,12(@13)                                     0145 00500000
*   DECLARE                         /*                               */ 00501000
*     SP1 CHAR(1) CONSTANT('?'),    /* SPECIAL CHAR FOR MSG          */ 00502000
*     SP2 CHAR(1) CONSTANT('#'),    /*                               */ 00503000
*     SP3 CHAR(1) CONSTANT('$'),    /*                               */ 00504000
*     SP4 CHAR(1) CONSTANT('@');    /*                               */ 00505000
*   DECLARE                         /*                               */ 00506000
*     CHARA CHAR(1) CONSTANT('A'),  /*                               */ 00507000
*     CHARI CHAR(1) CONSTANT('I'),  /*                               */ 00508000
*     CHARJ CHAR(1) CONSTANT('J'),  /*                               */ 00509000
*     CHARR CHAR(1) CONSTANT('R'),  /*                               */ 00510000
*     CHARS CHAR(1) CONSTANT('S'),  /*                               */ 00511000
*     CHARZ CHAR(1) CONSTANT('Z'),  /*                               */ 00512000
*     CHAR0 CHAR(1) CONSTANT('0'),  /*                               */ 00513000
*     CHAR9 CHAR(1) CONSTANT('9');  /*                               */ 00514000
*   DECLARE                         /*                               */ 00515000
*     J FIXED(31);                  /*                               */ 00516000
*                                                                  0148 00517000
*   /*****************************************************************/ 00518000
*   /*                                                               */ 00519000
*   /* LOOP THRU VALUE AND BLANK OUT ALL NON-PRINTABLES              */ 00520000
*   /*                                                               */ 00521000
*   /*****************************************************************/ 00522000
*                                                                  0149 00523000
*   DATA16=VLPDATA;                 /* MOVE DATA TO WORK AREA        */ 00524000
         MVI   DATA16+8,C' '                                       0149 00525000
         MVC   DATA16+9(7),DATA16+8                                0149 00526000
         L     @10,@PC00001                                        0149 00527000
         MVC   DATA16(8),VLPDATA(@10)                              0149 00528000
*   DO J=1 TO LENGTH(DATA16);       /* LOOP THRU ALL DATA            */ 00529000
         LA    J,1                                                 0150 00530000
@DL00150 DS    0H                                                  0151 00531000
*     IF(DATA16(J)>=CHARA&DATA16(J)<=CHARI)/* IF ALPHA               */ 00532000
*         |(DATA16(J)>=CHARJ&DATA16(J)<=CHARR)/*                     */ 00533000
*         |(DATA16(J)>=CHARS&DATA16(J)<=CHARZ)/*                     */ 00534000
*         |(DATA16(J)>=CHAR0&DATA16(J)<=CHAR9)/* OR NUMERIC          */ 00535000
*         |DATA16(J)=SP1            /* OR SPECIAL CHAR               */ 00536000
*         |DATA16(J)=SP2            /*                               */ 00537000
*         |DATA16(J)=SP3            /*                               */ 00538000
*         |DATA16(J)=SP4            /*                               */ 00539000
*       THEN                        /*                               */ 00540000
         LA    @10,DATA16-1(J)                                     0151 00541000
         CLI   0(@10),C'A'                                         0151 00542000
         BL    @GL00013                                            0151 00543000
         LA    @10,DATA16-1(J)                                     0151 00544000
         CLI   0(@10),C'I'                                         0151 00545000
         BNH   @RT00151                                            0151 00546000
@GL00013 LA    @10,DATA16-1(J)                                     0151 00547000
         CLI   0(@10),C'J'                                         0151 00548000
         BL    @GL00012                                            0151 00549000
         LA    @10,DATA16-1(J)                                     0151 00550000
         CLI   0(@10),C'R'                                         0151 00551000
         BNH   @RT00151                                            0151 00552000
@GL00012 LA    @10,DATA16-1(J)                                     0151 00553000
         CLI   0(@10),C'S'                                         0151 00554000
         BL    @GL00011                                            0151 00555000
         LA    @10,DATA16-1(J)                                     0151 00556000
         CLI   0(@10),C'Z'                                         0151 00557000
         BNH   @RT00151                                            0151 00558000
@GL00011 LA    @10,DATA16-1(J)                                     0151 00559000
         CLI   0(@10),C'0'                                         0151 00560000
         BL    @GL00010                                            0151 00561000
         LA    @10,DATA16-1(J)                                     0151 00562000
         CLI   0(@10),C'9'                                         0151 00563000
         BNH   @RT00151                                            0151 00564000
@GL00010 LA    @10,DATA16-1(J)                                     0151 00565000
         CLI   0(@10),C'?'                                         0151 00566000
         BE    @RT00151                                            0151 00567000
         LA    @10,DATA16-1(J)                                     0151 00568000
         CLI   0(@10),C'#'                                         0151 00569000
         BE    @RT00151                                            0151 00570000
         LA    @10,DATA16-1(J)                                     0151 00571000
         CLI   0(@10),C'$'                                         0151 00572000
         BE    @RT00151                                            0151 00573000
         LA    @10,DATA16-1(J)                                     0151 00574000
         CLI   0(@10),C'@'                                         0151 00575000
         BE    @RT00151                                            0151 00576000
*       ;                           /*                               */ 00577000
*     ELSE                          /*                               */ 00578000
*       DATA16(J)=BLANK;            /* IF NOT ONE OF ABOVE THEN BLNK */ 00579000
         LA    @10,DATA16-1(J)                                     0153 00580000
         MVI   0(@10),C' '                                         0153 00581000
*   END;                            /* END LOOP THRU DATA            */ 00582000
@RT00151 AH    J,@CH00039                                          0154 00583000
         CH    J,@CH00053                                          0154 00584000
         BNH   @DL00150                                            0154 00585000
*   RETURN CODE(0);                 /*                               */ 00586000
