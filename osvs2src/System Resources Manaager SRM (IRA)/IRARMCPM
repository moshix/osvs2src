*             DO;                   /*                       @YM05632*/ 01261000
*               IF APDEL>APCPR THEN /* SAVE POINTER TO APG USER      */ 01262000
         L     @15,APDEL                                           0302 01263000
         CH    @15,APCPR                                           0302 01264000
         BNH   @RF00302                                            0302 01265000
*                 DO;               /* WITH THE LARGEST              */ 01266000
*                   APOUCB=OUCBPTR; /* MEASURED MEAN TIME            */ 01267000
         ST    OUCBPTR,APOUCB                                      0304 01268000
*                   APCPR=APDEL;    /* TO WAIT                       */ 01269000
         STH   @15,APCPR                                           0305 01270000
*                 END;              /*                               */ 01271000
*               APDEL=APDEL         /* NEXT COMPUTE DELTA FROM MTW TO*/ 01272000
*                   /CCCAPDIV;      /* SUBTRACT FROM APG BASE      0307 01273000
*                                      PRIORITY                      */ 01274000
@RF00302 L     @08,APDEL                                           0307 01275000
         SRDA  @08,32                                              0307 01276000
         LH    @15,CCCAPDIV(,RMCTPTR)                              0307 01277000
         DR    @08,@15                                             0307 01278000
         ST    @09,APDEL                                           0307 01279000
*               IF APDEL>CCCAPGDP-CCCAPMDP THEN/* INSURE THAT DELTA     01280000
*                                      DOES NOT              @Z40BPEB*/ 01281000
