*/*                                                                  */ 01235000
*/*  SUBROUTINE NAME : RMRR2GST                                      */ 01236000
*/*                                                                  */ 01237000
*/*  FUNCTION :                                                      */ 01238000
*/*  GET STORAGE FROM SQA (SUBPOOL 245) FOR A CONTROL BLOCK.         */ 01239000
*/*  WHEN THIS ROUTINE IS ENTERED, THE CALLER IS HOLDING THE DISP    */ 01240000
*/*  LOCK.  THIS ROUTINE WILL RELEASE THE DISP LOCK BEFORE IT INVOKES*/ 01241000
*/*  IRARMI04 ROUTINE TO GET STORAGE FOR THE CONTROL BLOCK.  IT THEN */ 01242000
*/*  OBTAINS THE DISP LOCK AGAIN BEFORE CONTROL IS RETURNED TO THE   */ 01243000
*/*  CALLER.                                                         */ 01244000
*/*  INPUT  - REG 0 - SIZE OF CONTROL BLOCK                          */ 01245000
*/*  OUTPUT - REG 15 - RETURN CODE = 0 IF STORAGE IS OBTAINED        */ 01246000
*/*                                ^=0 IF STORAGE IS NOT OBTAINED    */ 01247000
*/*           REG 1 - ADDR OF STORAGE IF RETURN CODE=0               */ 01248000
*/*                                                                  */ 01249000
*/********************************************************************/ 01250000
*                                                                  0393 01251000
*RMRR2GST:                                                         0393 01252000
*   ;                               /* GET SQA STORAGE SUBRTN        */ 01253000
RMRR2GST DS    0H                                                  0394 01254000
*   GEN REFS(GPR00P,GPR15P,SVEREG,RMR2WKPT,RMR2SA01);              0394 01255000
         LA    SVEREG,RMR2SA01(,RMR2WKPT) SAVE AREA 1                   01256000
         STM   GPR00P,GPR15P,0(SVEREG)  SAVE REGS                       01257000
*   GEN REFS(PSALITA,FLC);                                         0395 01258000
RMRR2RD1 SETLOCK RELEASE,TYPE=DISP,                                   XX01259000
               RELATED=(DISP,IRARMERR(RMRR2GD1))                        01260000
*   GPR01P=GETCODE;                 /* GETMAIN REQUEST CODE          */ 01261000
*                                                                  0396 01262000
         SLR   GPR01P,GPR01P                                       0396 01263000
*   /*****************************************************************/ 01264000
*   /*                                                               */ 01265000
*   /* ? IRACTLCL EPN(I04)                                           */ 01266000
*   /*                                                               */ 01267000
*   /*****************************************************************/ 01268000
*                                                                  0397 01269000
*   CALL RMSBI04->IRARMRTN;         /* INVOKE I04 SUBRTN             */ 01270000
*                                                                  0397 01271000
         L     @15,RMSBI04(,RMCTPTR)                               0397 01272000
         BALR  @14,@15                                             0397 01273000
*   /*****************************************************************/ 01274000
*   /*                                                               */ 01275000
*   /* GET STORAGE, R0 - STORAGE SIZE                                */ 01276000
*   /*                                                               */ 01277000
*   /*****************************************************************/ 01278000
*                                                                  0398 01279000
*   WKREG3=RTNCOD;                  /* SAVE RET CODE FORM IRARMI04   */ 01280000
         LR    WKREG3,RTNCOD                                       0398 01281000
*   GEN REFS(PSALITA,FLC);                                         0399 01282000
RMRR2GD2 SETLOCK OBTAIN,TYPE=DISP,MODE=UNCOND,                        XX01283000
               RELATED=(DISP,IRARMERR(RMRR2RD2))                        01284000
*   RTNCOD=WKREG3;                  /* RESTORE RET CODE FROM IRARMI04*/ 01285000
         LR    RTNCOD,WKREG3                                       0400 01286000
*   RESPECIFY                                                      0401 01287000
*     RTNREG RSTD;                                                 0401 01288000
*   GEN REFS(GPR02P,RTNREG,SVEREG,RMR2WKPT,RMR2SA01);              0402 01289000
REG2SAVE EQU   8                      OFFSET OF REG 2 SAVE AREA         01290000
         LA    SVEREG,RMR2SA01(,RMR2WKPT) SAVE AREA 1                   01291000
         LM    GPR02P,RTNREG,REG2SAVE(SVEREG) RESTORE REGS              01292000
*   RESPECIFY                                                      0403 01293000
*     RTNREG UNRSTD;                                               0403 01294000
*   RETURN;                         /* RETURN TO CALLER              */ 01295000
         B     @EL00001                                            0404 01296000
*                                                                  0405 01297000
*/********************************************************************/ 01298000
*/*                                                                  */ 01299000
*/*  SUBROUTINE NAME : RMRR2CKQ                                      */ 01300000
*/*                                                                  */ 01301000
*/*  FUNCTION :                                                      */ 01302000
*/*  SEARCH ALL OUCB QUEUES TO MAKE SURE THAT THE OUCB IS ON AN OUCB */ 01303000
*/*  QUEUE.  IF IT IS NOT ON ANY ONE OF THE QUEUES, CALL USER STATE  */ 01304000
*/*  CHANGE ROUTINE TO REPOSITION THE OUCB.                          */ 01305000
*/*                                                                  */ 01306000
*/********************************************************************/ 01307000
*                                                                  0405 01308000
*RMRR2CKQ:                                                         0405 01309000
*   ;                               /* VALIDATE OUCB QUEUES SUBRTN   */ 01310000
RMRR2CKQ DS    0H                                                  0406 01311000
*   GEN REFS(GPR00P,GPR15P,SVEREG,RMR2WKPT,RMR2SA01);              0406 01312000
         LA    SVEREG,RMR2SA01(,RMR2WKPT)  SAVE AREA 1                  01313000
         STM   GPR00P,GPR15P,0(SVEREG)  SAVE REGS                       01314000
*   QHDPPTR=ADDR(RMCTWTQE);         /* ADDR OF 1ST QUEUE HEADER PTR  */ 01315000
         LA    QHDPPTR,RMCTWTQE(,RMCTPTR)                          0407 01316000
*   DO WKREG1=ONE TO OUCBQNO;       /* SEARCH ALL OUCB QUEUES        */ 01317000
         LA    WKREG1,1                                            0408 01318000
@DL00408 DS    0H                                                  0409 01319000
*     QHEADPTR=QHDP;                /* ADDR OF QUEUE HEADER          */ 01320000
         L     QHEADPTR,QHDP(,QHDPPTR)                             0409 01321000
*     OUCBPTR=QHEADPTR->OUCBFWD;    /* ADDR OF 1ST OUCB ON QUEUE     */ 01322000
         L     OUCBPTR,OUCBFWD(,QHEADPTR)                          0410 01323000
*     DO WHILE(OUCBPTR^=QHEADPTR);  /* SEARCH OUCB QUEUE UNTIL END OF   01324000
*                                      QUEUE IS REACHED              */ 01325000
         B     @DE00411                                            0411 01326000
@DL00411 DS    0H                                                  0412 01327000
*       IF OUCBPTR=ASCBOUCB THEN    /* FOUND OUCB IN QUEUE           */ 01328000
         C     OUCBPTR,ASCBOUCB(,ASCBPTR)                          0412 01329000
         BE    @RT00412                                            0412 01330000
*         GO TO RMRR2FNQ;           /* EXIT                          */ 01331000
*       ELSE                        /* OUCB NOT FOUND                */ 01332000
*         OUCBPTR=OUCBFWD;          /* POINT TO NEXT OUCB IN QUEUE   */ 01333000
         L     OUCBPTR,OUCBFWD(,OUCBPTR)                           0414 01334000
*     END;                          /* COMPLETED SEARCHING A QUEUE   */ 01335000
