         TM    SDWAPCHK(SDWAPTR),B'01000000'                       0105 00307000
         BNO   @RF00105                                            0105 00308000
         CLC   SDWAICD1(1,SDWAPTR),KEYCHK                          0105 00309000
         BNE   @RF00105                                            0105 00310000
@RT00105 DS    0H                                                  0106 00311000
*     DO;                           /* PROVIDE REASON CODE           */ 00312000
*       IF SDWATEXC='1'B THEN       /* PAGE EXCEPTION ERROR ?        */ 00313000
         TM    SDWATEXC(SDWAPTR),B'00000010'                       0107 00314000
         BNO   @RF00107                                            0107 00315000
*         SDWAGR15=PAGEERR;         /* REASON CODE FOR PAGE EXCEPTION*/ 00316000
         MVC   SDWAGR15(4,SDWAPTR),@CF02289                        0108 00317000
*       ELSE                        /* PROTECTION EXCEPTION ERROR    */ 00318000
*         SDWAGR15=SKEYERR;         /* REASON CODE FOR PROTECTION  0109 00319000
*                                      EXCEPTION                     */ 00320000
         B     @RC00107                                            0109 00321000
