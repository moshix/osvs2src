         C     WKREG1,@CF00292                                     0417 01343000
         BNH   @DL00408                                            0417 01344000
*/*  OUCB IS NOT FOUND IN ANY OUCB QUEUE, ENQUEUE IT TO AN OUCB QUEUE*/ 01345000
*                                                                  0418 01346000
*   OUCBPTR=ASCBOUCB;               /* ADDR OF OUCB                  */ 01347000
         L     OUCBPTR,ASCBOUCB(,ASCBPTR)                          0418 01348000
*   CALL RMRR2REQ;                  /* ENQUEUE OUCB BLOCK            */ 01349000
         BAL   @14,RMRR2REQ                                        0419 01350000
*/*  OUCB IS ON A QUEUE                                              */ 01351000
*                                                                  0420 01352000
*RMRR2FNQ:                                                         0420 01353000
*   GEN REFS(GPR00P,GPR15P,SVEREG,RMR2WKPT,RMR2SA01);              0420 01354000
RMRR2FNQ DS    0H                                                  0420 01355000
*                                                 /* EXIT CODE      */  01356000
         LA    SVEREG,RMR2SA01(,RMR2WKPT)  SAVE AREA 1                  01357000
         LM    GPR00P,GPR15P,0(SVEREG) RESTORE REGS                     01358000
*   RETURN;                         /* RETURN TO CALLER              */ 01359000
         B     @EL00001                                            0421 01360000
