         BZ    @RT00123                                            0123 00368000
*     GO TO RMRR2PER;               /* PERCOLATE THE ERROR           */ 00369000
*                                                                  0125 00370000
*/*  DETERMINE THE LEVEL OF THE SYSTEM RESOURCES MGR STRUCTURE IN    */ 00371000
*/*  WHICH THE ERROR OCCURRED, SO THAT THE APPROPRIATE SAVEAREA IS   */ 00372000
*/*  USED TO RESTORE THE REGISTERS BEFORE CONTROL IS GIVEN TO THE    */ 00373000
*/*  RETRY ROUTINE                                                   */ 00374000
*                                                                  0125 00375000
*   RMR2REGP=ADDR(RMSAVE01);        /* ADDR OF SAVE AREA 1           */ 00376000
         L     RMR2REGP,RMCTRMSA(,RMCTPTR)                         0125 00377000
*RMRR2RTT:                                                         0126 00378000
*   ;                               /* SET UP FOR RETRY              */ 00379000
RMRR2RTT DS    0H                                                  0127 00380000
*   RMEPPTR=RRPAEPA;                /* ADDR OF RMEP BLOCK            */ 00381000
         L     RMEPPTR,RRPAEPA(,RRPAPTR)                           0127 00382000
*   IF RMEPACN='1'B THEN            /* RMEP BLOCK IS IN EPDT         */ 00383000
         TM    RMEPACN(RMEPPTR),B'00000001'                        0128 00384000
         BNO   @RF00128                                            0128 00385000
*     DO;                           /* ERROR IS IN ACTION ROUTINE    */ 00386000
*       RMR2REGP=ADDR(RMSAVE02);    /* RESTORE REGS FROM SAVE AREA 2 */ 00387000
