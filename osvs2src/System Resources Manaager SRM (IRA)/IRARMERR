         TITLE 'IRARMERR - SYSTEM RESOURCES MANAGER FUNCTIONAL RECOVERY*00001000
               Y        '                                               00002000
IRARMERR CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
IRARMRR1 DS    0H                                                  0001 00008000
         USING *,@15                                               0001 00009000
         B     @PROLOG                                             0001 00010000
         DC    AL2(@EP00029-IRARMRR1)                                   00011000
         ENTRY IRARMRR1                                                 00012000
IRARMRR2 DS    0H                                                  0001 00013000
         USING *,@15                                               0001 00014000
         B     @PROLOG                                             0001 00015000
         DC    AL2(@EP00041-IRARMRR2)                                   00016000
         ENTRY IRARMRR2                                                 00017000
         DROP  @15                                                      00018000
@PROLOG  BALR  @10,0                                               0001 00019000
@PSTART  DS    0H                                                  0001 00020000
         USING @PSTART,@10                                         0001 00021000
         AH    @15,4(,@15)                                         0001 00022000
         BR    @15                                                 0001 00023000
@EP00001 DS    0H                                                  0002 00024000
*                                                                  0029 00025000
*/********************************************************************/ 00026000
*/*                                                                  */ 00027000
*/*  ENTRY POINT NAME : IRARMRR1                                     */ 00028000
*/*                                                                  */ 00029000
*/*  FUNCTION :                                                      */ 00030000
*/*  FUNCTIONAL RECOVERY FOR SRM ENTERED FOR GLOBALLY LOCKED ENTRIES */ 00031000
*/*                                                                  */ 00032000
*/********************************************************************/ 00033000
*                                                                  0029 00034000
*IRARMRR1:                                                         0029 00035000
*   ENTRY;                                                         0029 00036000
*                                                                  0029 00037000
@EP00029 DS    0H                                                  0030 00038000
*/*  SWITCH TO SRM PROTECT KEY                                       */ 00039000
*                                                                  0030 00040000
*   GEN( MODESET EXTKEY=SRM );                                     0030 00041000
*                                                                  0030 00042000
          MODESET EXTKEY=SRM                                            00043000
*/*  SAVE ADDRESS OF WORK AREA PASSED FROM RTM VIA REG 0             */ 00044000
*/*  SAVE RTM'S RETURN ADDRESS                                       */ 00045000
*                                                                  0031 00046000
*   RMR2WKPT=R0;                    /* ADDR OF WORK AREA             */ 00047000
         LR    RMR2WKPT,R0                                         0031 00048000
*   RMR2SRTN=RTNREG;                /* RTM'S RETURN ADDRESS          */ 00049000
         ST    RTNREG,RMR2SRTN(,RMR2WKPT)                          0032 00050000
*   RESPECIFY                                                      0033 00051000
*     RTNREG UNRSTD;                                               0033 00052000
*   CALL RMRR2INT;                  /* INITIALIZATION                */ 00053000
         BAL   @14,RMRR2INT                                        0034 00054000
*/*  IF RRPA HAS NOT BEEN INITIALIZED OR IF THE ERROR IS RECURSIVE,  */ 00055000
*/*  PERCOLATE THE ERROR                                             */ 00056000
*/*  OTHERWISE, RETRY THE ROUTINE THAT WAS IN CONTROL AT THE TIME    */ 00057000
*/*  OF ERROR                                                        */ 00058000
*                                                                  0035 00059000
*   IF RRPAEPA=ZERO|RRPARCI='1'B THEN/* RRPA IS NOT INITIALIZED OR 0035 00060000
*                                      RETRY HAS BEEN ATTEMPTED      */ 00061000
         L     @14,RRPAEPA(,RRPAPTR)                               0035 00062000
         LTR   @14,@14                                             0035 00063000
         BZ    @RT00035                                            0035 00064000
         TM    RRPARCI(RRPAPTR),B'00000001'                        0035 00065000
         BO    @RT00035                                            0035 00066000
*     GO TO RMRR2PER;               /* PERCOLATE ERROR               */ 00067000
*   ELSE                            /* RETRY HAS NOT BEEN ATTEMPTED  */ 00068000
*     DO;                                                          0037 00069000
*       RMEPPTR=RRPAEPA;            /* ADDR OF RMEP                  */ 00070000
         LR    RMEPPTR,@14                                         0038 00071000
*       GO TO RMRR2RTR;             /* RETRY ERROR                   */ 00072000
         B     RMRR2RTR                                            0039 00073000
*     END;                                                         0040 00074000
*                                                                  0041 00075000
*/********************************************************************/ 00076000
*/*                                                                  */ 00077000
*/*  ENTRY POINT NAME : IRARMRR2                                     */ 00078000
*/*                                                                  */ 00079000
*/*  FUNCTION :                                                      */ 00080000
*/*  FUNCTIONAL RECOVERY FOR SRM ENTERED FOR NON-GLOBALLY LOCKED     */ 00081000
*/*  ENTRIES                                                         */ 00082000
*/*                                                                  */ 00083000
*/********************************************************************/ 00084000
*                                                                  0041 00085000
*IRARMRR2:                                                         0041 00086000
*   ENTRY;                                                         0041 00087000
*                                                                  0041 00088000
@EP00041 DS    0H                                                  0042 00089000
*/*  SWITCH TO SRM PROTECT KEY                                       */ 00090000
*                                                                  0042 00091000
*   GEN( MODESET EXTKEY=SRM );                                     0042 00092000
*                                                                  0042 00093000
          MODESET EXTKEY=SRM                                            00094000
*/*  SAVE ADDRESS OF WORK AREA PASSED FROM RTM VIA REG 0             */ 00095000
*/*  SAVE RTM'S RETURN ADDRESS                                       */ 00096000
*                                                                  0043 00097000
*   RESPECIFY                                                      0043 00098000
*     RTNREG RSTD;                                                 0043 00099000
*   RMR2WKPT=R0;                    /* ADDR OF WORK AREA             */ 00100000
         LR    RMR2WKPT,R0                                         0044 00101000
*   RMR2SRTN=RTNREG;                /* RTM'S RETURN ADDRESS          */ 00102000
         ST    RTNREG,RMR2SRTN(,RMR2WKPT)                          0045 00103000
*   RESPECIFY                                                      0046 00104000
*     RTNREG UNRSTD;                                               0046 00105000
*   RESPECIFY                                                      0047 00106000
*     R0 UNRSTD;                                                   0047 00107000
*   CALL RMRR2INT;                  /* INITIALIZATION                */ 00108000
         BAL   @14,RMRR2INT                                        0048 00109000
*/*  IF MF1 IS ACTIVE BUT THE ADDRESS OF THE WORKLOAD ACTIVITY       */ 00110000
*/*  MEASUREMENT TABLE (WAMT) OR THE ADDRESS OF THE WORKLOAD         */ 00111000
*/*  ACTIVITY REPORT ROUTINE (IRARMWAR) IS INVALID, TERMINATE MF1.   */ 00112000
*                                                                  0049 00113000
*   IF RMCTMFA='1'B                 /* MF1 IS ACTIVE                 */ 00114000
*       &RRPAOPT='1'B               /* HOLDING SRM LOCK              */ 00115000
*     THEN                          /* TEST IF NEED TO TERMINATE MF1 */ 00116000
         TM    RMCTMFA(RMCTPTR),B'10000000'                        0049 00117000
         BNO   @RF00049                                            0049 00118000
         TM    RRPAOPT(RRPAPTR),B'00000010'                        0049 00119000
         BNO   @RF00049                                            0049 00120000
*     DO;                                                          0050 00121000
*       BLKPTR=RMCTWAMT;            /* ADDR OF WORKLOAD ACTIVITY   0051 00122000
*                                      MEASUREMENT TABLE             */ 00123000
         L     BLKPTR,RMCTWAMT(,RMCTPTR)                           0051 00124000
*       CALL RMRR2VFB;              /* VERIFY ADDRESS                */ 00125000
         BAL   @14,RMRR2VFB                                        0052 00126000
*       IF RTNCOD^=ZERO THEN        /* ADDRESS IS INVALID            */ 00127000
         LTR   RTNCOD,RTNCOD                                       0053 00128000
         BNZ   @RT00053                                            0053 00129000
*         GO TO RMRR2WAT;           /* TERMINATE MF1                 */ 00130000
*       ELSE                        /* WAMT ADDR IS VALID            */ 00131000
*         DO;                                                      0055 00132000
*           WAMTPTR=RMCTWAMT;       /* ADDR OF WORKLOAD ACTIVITY   0056 00133000
*                                      MEASUREMENT TABLE             */ 00134000
         L     WAMTPTR,RMCTWAMT(,RMCTPTR)                          0056 00135000
*           BLKPTR=WAMTWAR;         /* ADDR OF IRARMWAR              */ 00136000
         L     BLKPTR,WAMTWAR(,WAMTPTR)                            0057 00137000
*           CALL RMRR2VFB;          /* VERIFY ADDRESS                */ 00138000
         BAL   @14,RMRR2VFB                                        0058 00139000
*           IF RTNCOD=ZERO THEN     /* ADDR IS VALID                 */ 00140000
         LTR   RTNCOD,RTNCOD                                       0059 00141000
         BZ    @RT00059                                            0059 00142000
*             GO TO RMRR2CKA;       /* CONTINUE PROCESSING           */ 00143000
*         END;                                                     0061 00144000
*RMRR2WAT:                                                         0062 00145000
*       ;                           /* TERMINATE MF1                 */ 00146000
RMRR2WAT DS    0H                                                  0063 00147000
*       RMCTMFA='0'B;               /* DEACTIVATE MF1                */ 00148000
*       RMCTSTW='0'B;               /* STOPPED WAR COLLECTION        */ 00149000
         NI    RMCTMFA(RMCTPTR),B'01111011'                        0064 00150000
*       RMCTMFS='0'B;               /* STOPPED WAR COLLECTION      0065 00151000
*                                                            @Z40BPCH*/ 00152000
         NI    RMCTMFS(RMCTPTR),B'10111111'                        0065 00153000
*     END;                                                         0066 00154000
*                                                                  0066 00155000
*/*  IF ABEND HAS BEEN ISSUED BY SYSTEM RESOURCES MANAGER, OR        */ 00156000
*/*  IF ERROR RECOVERY IS ENTERED RECURSIVELY DUE TO THE DEPRESSED   */ 00157000
*/*  CONSOLE RESTART KEY, RETURN CONTROL TO RTM TO PERCOLATE THE     */ 00158000
*/*  ERROR                                                           */ 00159000
*                                                                  0067 00160000
*RMRR2CKA:                                                         0067 00161000
*   ;                               /* CHECK ABEND CODE              */ 00162000
@RF00049 DS    0H                                                  0067 00163000
RMRR2CKA DS    0H                                                  0068 00164000
*   IF RMR2SC2(2:2)=RMABENDC        /* ABEND ISSUED BY SRM           */ 00165000
*       |(SDWARKEY='1'B&RRPARFI='1'B)/* CONSOLE RESTART KEY IS     0068 00166000
*                                      DEPRESSED RECURSIVELY         */ 00167000
*     THEN                          /* PERCOLATE ERROR               */ 00168000
         CLC   RMR2SC2+1(1,RMR2WKPT),RMABENDC                      0068 00169000
         BE    @RT00068                                            0068 00170000
         TM    SDWARKEY(SDWAPTR),B'00100000'                       0068 00171000
         BNO   @RF00068                                            0068 00172000
         TM    RRPARFI(RRPAPTR),B'00000010'                        0068 00173000
         BNO   @RF00068                                            0068 00174000
@RT00068 DS    0H                                                  0069 00175000
*     DO;                                                          0069 00176000
*       IF(RMR2SC2=RMABC15F&SDWAGR15=OUCBERR)/* ABEND CODE=15F &   0070 00177000
*                                      REASON CODE=OUCBERR           */ 00178000
*         THEN                      /* OUCB IS INVALID               */ 00179000
         CLC   RMR2SC2(2,RMR2WKPT),RMABC15F                        0070 00180000
         BNE   @RF00070                                            0070 00181000
         CLC   SDWAGR15(4,SDWAPTR),@CF00070                        0070 00182000
         BNE   @RF00070                                            0070 00183000
*         DO;                       /* TEST IF NECESSARY TO SET    0071 00184000
*                                      OUCBINV BIT OFF       @Z40BPCJ*/ 00185000
*           OUCBPTR=SDWAGR04;       /* REG 4 AT TIME OF 15F ABEND    */ 00186000
         L     OUCBPTR,SDWAGR04(,SDWAPTR)                          0072 00187000
*           BLKPTR=OUCBPTR;         /* ADDR OF OUCB TO BE VERIFIED   */ 00188000
         LR    BLKPTR,OUCBPTR                                      0073 00189000
*           CALL RMRR2VFB;          /* TEST IF ADDR WILL CAUSE PAGE     00190000
*                                      EXCEPTION                     */ 00191000
         BAL   @14,RMRR2VFB                                        0074 00192000
*           IF RTNCOD=ZERO          /* ADDR OF OUCB IS VALID         */ 00193000
*               &OUCBPTR^=RMCTOUCB  /* OUCB IS NOT THE PREASSEMBLED     00194000
*                                      OUCB                          */ 00195000
*               &OUCBNAME='OUCB'    /* OUCB IS VALID OUCB            */ 00196000
*             THEN                  /* USER WAS ABENDED BECAUSE ITS     00197000
*                                      OUCBINV BIT WAS ON    @Z40BPCJ*/ 00198000
         LTR   RTNCOD,RTNCOD                                       0075 00199000
         BNZ   @RF00075                                            0075 00200000
         C     OUCBPTR,RMCTOUCB(,RMCTPTR)                          0075 00201000
         BE    @RF00075                                            0075 00202000
         CLC   OUCBNAME(4,OUCBPTR),@CC02382                        0075 00203000
         BNE   @RF00075                                            0075 00204000
*             OUCBINV='0'B;         /* SET OUCBINV BIT OFF   @Z40BPCH*/ 00205000
         NI    OUCBINV(OUCBPTR),B'11101111'                        0076 00206000
*           RMR2SC2=RMABC25F;       /* CHANGE ABEND CODE TO 25F      */ 00207000
@RF00075 MVC   RMR2SC2(2,RMR2WKPT),RMABC25F                        0077 00208000
*         END;                                                     0078 00209000
*       GO TO RMRR2PER;             /* PERCOLATE THE ERROR           */ 00210000
         B     RMRR2PER                                            0079 00211000
*     END;                                                         0080 00212000
*                                                                  0080 00213000
*/*  IF THE ERROR OCCURRED IN THE MF1 TASK AND THE WAMT POINTER IS   */ 00214000
*/*  VALID, ABTERM THE MF1 TASK.  IF SRM AND MF1 ARE RUNNING IN THE  */ 00215000
*/*  SAME ADDRESS SPACE, CHANGE THE COMPLETION CODE TO 25F AND       */ 00216000
*/*  PERCOLATE THE ERROR.                                            */ 00217000
*                                                                  0081 00218000
*   IF RRPAWAR='1'B THEN            /* WORKLOAD ACTIVITY WAS RUNNING */ 00219000
@RF00068 TM    RRPAWAR(RRPAPTR),B'01000000'                        0081 00220000
         BNO   @RF00081                                            0081 00221000
*     DO;                                                          0082 00222000
*       WAMTPTR=RMCTWAMT;           /* ADDR OF WORKLOAD ACTIVITY   0083 00223000
*                                      MEASUREMENT TABLE             */ 00224000
         L     WAMTPTR,RMCTWAMT(,RMCTPTR)                          0083 00225000
*       BLKPTR=WAMTPTR;             /* ADDR OF WAMT                  */ 00226000
         LR    BLKPTR,WAMTPTR                                      0084 00227000
*       CALL RMRR2VFB;              /* VERIFY ADDRESS                */ 00228000
         BAL   @14,RMRR2VFB                                        0085 00229000
*       IF RTNCOD=ZERO THEN         /* WAMT ADDRESS IS VALID         */ 00230000
         LTR   RTNCOD,RTNCOD                                       0086 00231000
         BNZ   @RF00086                                            0086 00232000
*         DO;                                                      0087 00233000
*           TCBPTR=WAMTTCB;         /* ADDR OF MF1'S TCB             */ 00234000
         L     TCBPTR,WAMTTCB(,WAMTPTR)                            0088 00235000
*           ASIDNUM=WAMTASD;        /* ASID OF MEMORY IN WHICH MF1 0089 00236000
*                                      WAS STARTED                   */ 00237000
         L     ASIDNUM,WAMTASD(,WAMTPTR)                           0089 00238000
*           RESPECIFY                                              0090 00239000
*            (GPR00P,                                              0090 00240000
*             GPR01P,                                              0090 00241000
*             GPR14P,                                              0090 00242000
*             GPR15P) RSTD;                                        0090 00243000
*           WKREGC=RMR2SCC;         /* COMPLETION CODE               */ 00244000
         L     WKREGC,RMR2SCC(,RMR2WKPT)                           0091 00245000
*           GEN REFS(TCBPTR,ASIDNUM,CVTBTERM);                     0092 00246000
         CALLRTM TYPE=ABTERM,COMPCOD=(WKREGC),TCB=(TCBPTR),           XX00247000
               ASID=(ASIDNUM)                                           00248000
*           RESPECIFY                                              0093 00249000
*            (GPR00P,                                              0093 00250000
*             GPR01P,                                              0093 00251000
*             GPR14P,                                              0093 00252000
*             GPR15P) UNRSTD;                                      0093 00253000
*           RMCTPTR=CVTOPCTP;       /* RESTORE RMCT PTR              */ 00254000
         L     @14,CVTPTR                                          0094 00255000
         L     RMCTPTR,CVTOPCTP(,@14)                              0094 00256000
*           IF RRPAASD=ASIDNUM THEN /* ERROR OCCURRED IN SYSEVENT  0095 00257000
*                                      ISSUER'S ADDRESS SPACE        */ 00258000
         LH    @14,RRPAASD(,RRPAPTR)                               0095 00259000
         N     @14,@CF02451                                        0095 00260000
         CR    ASIDNUM,@14                                         0095 00261000
         BNE   @RF00095                                            0095 00262000
*             DO;                                                  0096 00263000
*               IF RMR2SC2^=RMABC15F THEN/* IF COMPLETION CODE IS NOT   00264000
*                                      15F                           */ 00265000
         CLC   RMR2SC2(2,RMR2WKPT),RMABC15F                        0097 00266000
         BE    @RF00097                                            0097 00267000
*                 RMR2SC2=RMABC25F; /* CHANGE COMPLETION CODE TO 25F */ 00268000
         MVC   RMR2SC2(2,RMR2WKPT),RMABC25F                        0098 00269000
*               GO TO RMRR2PER;     /* PERCOLATE ERROR               */ 00270000
         B     RMRR2PER                                            0099 00271000
*             END;                                                 0100 00272000
*         END;                                                     0101 00273000
@RF00095 DS    0H                                                  0102 00274000
*     END;                                                         0102 00275000
*                                                                  0102 00276000
@RF00086 DS    0H                                                  0103 00277000
*/*  IF SET IPS IS ACTIVE & THE SYSEVENT ISSUER IS IN THE MASTER     */ 00278000
*/*  SCHEDULER'S ADDRESS SPACE (SET IPS TASK IS STARTED IN THIS      */ 00279000
*/*  ADDRESS SPACE) AND IF THE ABEND CODE IS NOT '15F', CHANGE IT TO */ 00280000
*/*  '25F'.                                                          */ 00281000
*                                                                  0103 00282000
*   IF RRPAIPS='1'B                 /* SET IPS IS ACTIVE             */ 00283000
*       &RRPAASD=MSASID             /* SYSEVENT ISSUER IS IN MASTER     00284000
*                                      SCHEDULER'S ADDR SPACE        */ 00285000
*       &RMR2SC2^=RMABC15F THEN     /* COMPLETION CODE ^=15F         */ 00286000
@RF00081 TM    RRPAIPS(RRPAPTR),B'00100000'                        0103 00287000
         BNO   @RF00103                                            0103 00288000
         CLC   RRPAASD(2,RRPAPTR),@CH00061                         0103 00289000
         BNE   @RF00103                                            0103 00290000
         CLC   RMR2SC2(2,RMR2WKPT),RMABC15F                        0103 00291000
         BE    @RF00103                                            0103 00292000
*     RMR2SC2=RMABC25F;             /* CHANGE COMPLETION CODE TO 25F */ 00293000
         MVC   RMR2SC2(2,RMR2WKPT),RMABC25F                        0104 00294000
*/*  IF TRANSLATION EXCEPTION OR PROTECTION EXCEPTION HAS OCCURRED,  */ 00295000
*/*  CHANGE THE COMPLETION CODE TO X'15F', PROVIDE REASON CODE,      */ 00296000
*/*  VALIDATE SRM CONTROL BLOCKS AND PERCOLATE THE ERROR             */ 00297000
*                                                                  0105 00298000
*   IF RRPACTL='0'B                 /* ERROR OCCURS IN SYSEVENT PROC */ 00299000
*       &(SDWATEXC='1'B             /* TRANSLATION EXCEPTION         */ 00300000
*       |(SDWAPCHK='1'B&SDWAICD1=KEYCHK))/* PROTECTION EXCEPTION     */ 00301000
*     THEN                          /* PERCOLATE ERROR               */ 00302000
@RF00103 TM    RRPACTL(RRPAPTR),B'00001000'                        0105 00303000
         BNZ   @RF00105                                            0105 00304000
         TM    SDWATEXC(SDWAPTR),B'00000010'                       0105 00305000
