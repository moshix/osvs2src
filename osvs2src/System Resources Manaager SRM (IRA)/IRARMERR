*/*  RESOURCES MANAGER                                               */ 00764000
*                                                                  0234 00765000
*   QHDPPTR=ADDR(RMCTWTQE);         /* ADDR OF 1ST OUCB QUEUE HEADER    00766000
*                                      POINTER IN RMCT               */ 00767000
         LA    QHDPPTR,RMCTWTQE(,RMCTPTR)                          0234 00768000
*   DO WKREG1=1 TO OUCBQNO;         /* RE-HOOK ALL OUCB QUEUES       */ 00769000
         LA    WKREG1,1                                            0235 00770000
@DL00235 DS    0H                                                  0236 00771000
*     QHEADPTR=QHDP;                /* ADDR OF QUEUE HEADER BLOCK    */ 00772000
         L     QHEADPTR,QHDP(,QHDPPTR)                             0236 00773000
*/*  RE-QUEUE THE OUCBS ACCORDING TO THE BACKWARD POINTERS UNTIL     */ 00774000
*/*  THE QUEUE ADDRESS BLOCK IS REACHED                              */ 00775000
*/*  AS LONG AS THE BACKWARD CHAIN IS VALID, IT WILL HAVE PRECEDENCE */ 00776000
*/*  OVER THE FORWARD CHAIN.  IF EITHER THE FORWARD OR THE BACKWARD  */ 00777000
*/*  CHAIN IS BAD, ONE WILL BE RECONSTRUCTED FROM THE OTHER.         */ 00778000
*/*  IF BOTH ARE BAD, THEY WILL BE CONNECTED AT THEIR LAST VALID     */ 00779000
*/*  POINTS.                                                         */ 00780000
*                                                                  0237 00781000
*     SOUCBPTR=QHEADPTR;            /* ADDR OF NEXT OUCB ON QUEUE    */ 00782000
         LR    SOUCBPTR,QHEADPTR                                   0237 00783000
*     OUCBPTR=QHEADPTR->OUCBBCK;    /* ADDR OF 1ST OUCB ON BACKWARD     00784000
*                                      CHAIN                         */ 00785000
