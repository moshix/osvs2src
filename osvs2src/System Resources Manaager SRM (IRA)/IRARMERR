*       GO TO RMRR2RTY;             /* RETURN TO RTM FOR RETRY       */ 00353000
         B     RMRR2RTY                                            0120 00354000
*     END;                                                         0121 00355000
*                                                                  0121 00356000
*/*  VALIDATE THE OUCB QUEUE                                 @Z40BPCJ*/ 00357000
*                                                                  0122 00358000
*   CALL RMRR2VLD;                  /* VALIDATE QUEUES & CONTROL BLKS*/ 00359000
@RF00114 BAL   @14,RMRR2VLD                                        0122 00360000
*/*  IF RETRY IS NOT POSSIBLE, PERCOLATE THE ERROR                   */ 00361000
*                                                                  0123 00362000
*   IF RRPARTI='1'B|RRPAEPA=ZERO THEN/* RETRY IS NOT POSSIBLE        */ 00363000
         TM    RRPARTI(RRPAPTR),B'00000100'                        0123 00364000
