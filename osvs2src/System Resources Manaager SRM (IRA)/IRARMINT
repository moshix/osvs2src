*               CALL RMI10001;      /* ABEND INVOKER                 */ 00620000
         BAL   @14,RMI10001                                        0200 00621000
*             END;                                                 0201 00622000
*           FRRADDR=CVTOPCTP->      /* RECOVERY RTN TO HANDLE ERRORS */ 00623000
*               RMEXRR2;            /* THAT OCCUR WITHIN THE SRM   0202 00624000
