*       RRPASVC='1'B;               /* INDICATE SVC ENTRY            */ 00091000
*       RRPAENB='1'B;               /* SET ENABLED ENTRY FLAG ON     */ 00092000
         OI    RRPASVC(RRPAPTR),B'01100000'                        0089 00093000
*     END;                                                         0090 00094000
*   ELSE                            /* SVC ISSUER NOT AUTHORIZED     */ 00095000
*     DO;                           /* ABEND INVOKER                 */ 00096000
         B     @RC00083                                            0091 00097000
@RF00083 DS    0H                                                  0092 00098000
*       RESNCODE=CON8;              /* ABEND REASON CODE             */ 00099000
