*         GENERATE CODE REFS(PSALITA,FLC);                         0161 00465000
RMINT010 DS    0H                                                  0161 00466000
         SETLOCK RELEASE,TYPE=SRM,RELATED=(SRM,IGC095(RMINT005))        00467000
*       ELSE                        /* SRM ENTERED DISABLED,         */ 00468000
*RMINT015:                          /* FREE SRM LOCK AND STAY DISABLD*/ 00469000
*         GENERATE CODE REFS(PSALITA,FLC);                         0162 00470000
         B     @RC00160                                            0162 00471000
@RF00160 DS    0H                                                  0162 00472000
