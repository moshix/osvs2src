*   ICVOLCBT=I0;                    /* CLEAR OVERUTILIZED BIT PATTERN*/ 01021000
         ST    @01,ICVOLCBT(,RMCTPTR)                              0261 01022000
*   ICVULCBT=I0;                    /* CLEAR UNDERUTILIZED BIT     0262 01023000
*                                      PATTERN                       */ 01024000
         ST    @01,ICVULCBT(,RMCTPTR)                              0262 01025000
*   DO RLCTPTR=RMCTRLCT             /* LOOK AT EACH LOGICAL CHANNEL  */ 01026000
