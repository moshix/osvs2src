*   IMCBSLCB=I0;                    /* CLEAR SIGNIFICANT LCH USAGE 0123 00264000
*                                      BIT PATTERN                   */ 00265000
         SLR   @11,@11                                             0123 00266000
         ST    @11,IMCBSLCB(,IMCBPTR)                              0123 00267000
*   DO RLCTPTR=ICCLCLST             /* LOOK AT EACH LOGICAL CHANNEL  */ 00268000
*         TO RMCTRLCT BY-RLCHLEN;   /* STARTING FROM END OF TABLE    */ 00269000
         L     RLCTPTR,ICCLCLST(,RMCTPTR)                          0124 00270000
         B     @DE00124                                            0124 00271000
@DL00124 DS    0H                                                  0125 00272000
*     RLCTX=RLCTPTR-RMCTRLCT;       /* COMPUTE OFFSET OF RLCT ENTRY  */ 00273000
         LR    RLCTX,RLCTPTR                                       0125 00274000
         SL    RLCTX,RMCTRLCT(,RMCTPTR)                            0125 00275000
*/*                                                                  */ 00276000
*/*        IF LOGICAL CHANNEL ALREADY HAS AN ENTRY IN THE USER       */ 00277000
*/*        IMCB,  COMPUTE USER EXCP RATES ON THE LCH AND --          */ 00278000
*/*                                                                  */ 00279000
*                                                                  0126 00280000
*     IF RLCTX=IMCBLCTX THEN        /* IF LCH IS REPRESENTED IN THE  */ 00281000
         CH    RLCTX,IMCBLCTX(,IMCBLPTR)                           0126 00282000
         BNE   @RF00126                                            0126 00283000
*       DO;                         /* IMCB THEN                     */ 00284000
*         ILUR=                     /* LCH EXCP RATE EQUALS          */ 00285000
*             (RLCTUMWA             /* RECENT EXCP                   */ 00286000
*             -IMCBBLCC)            /* COUNT                         */ 00287000
*             *I1000                /* TIMES 1000 FOR EXCPS / SEC    */ 00288000
*             /ILTIME;              /* DIVIDED BY ELAPSED TIME       */ 00289000
         L     @11,RLCTUMWA(,RLCTPTR)                              0128 00290000
         LR    @08,@11                                             0128 00291000
         SL    @08,IMCBBLCC(,IMCBLPTR)                             0128 00292000
         LR    @11,@08                                             0128 00293000
         M     @10,@CF02921                                        0128 00294000
         D     @10,ILTIME                                          0128 00295000
         LR    ILUR,@11                                            0128 00296000
*         IMCBBLCC=RLCTUMWA;        /* SAVE NEW BASE EXCP COUNT      */ 00297000
         L     @08,RLCTUMWA(,RLCTPTR)                              0129 00298000
