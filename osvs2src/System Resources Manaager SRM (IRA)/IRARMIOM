*         IF ILREQ<=I0 THEN         /* IF NO REQS WERE MADE  @ZM43081*/ 01052000
         CLR   ILREQ,@01                                           0267 01053000
         BH    @RF00267                                            0267 01054000
*           DO;                     /* OR WRAP OF LCHTOTCT           */ 01055000
*             RLCTUTIL=I0;          /* UTILIZATION = ZERO            */ 01056000
         STH   @01,RLCTUTIL(,RLCTPTR)                              0269 01057000
*             RLCTRQRT=I0;          /* REQUEST RATE = ZERO           */ 01058000
         STH   @01,RLCTRQRT(,RLCTPTR)                              0270 01059000
*           END;                    /*                               */ 01060000
*         ELSE                      /* OTHERWISE IF THERE            */ 01061000
