*   DEVSAVE='7FFFFFFF'X;            /* GET INIT UTILIZATION > MAX    */ 01937000
         L     DEVSAVE,@CF03053                                    0442 01938000
*   DO IDX=IDXST                    /* LOOK AT EACH CANDIDATE ON     */ 01939000
*         TO IDXEND BY IDXLEN;      /* PRIMARY LIST                  */ 01940000
         LR    IDX,IDXST                                           0443 01941000
         B     @DE00443                                            0443 01942000
@DL00443 DS    0H                                                  0444 01943000
*     IF RLCTUMWA<=                 /* IF LCH UTIL FOR THIS  @Z30WIAW*/ 01944000
*         DEVSAVE THEN              /* CANDIDATE IS <= LOWEST SO FAR */ 01945000
         LH    @15,IDXPTR(,IDX)                                    0444 01946000
         AL    @15,RLCTST                                          0444 01947000
         L     @15,RLCTPTR2(,@15)                                  0444 01948000
         L     @15,RLCTUMWA(,@15)                                  0444 01949000
         CR    @15,DEVSAVE                                         0444 01950000
         BH    @RF00444                                            0444 01951000
*       DO;                         /* THEN                          */ 01952000
*         IF RLCTUMWA<              /* LCH UTIL < OLD LOW    @Z30WIAW*/ 01953000
*             DEVSAVE THEN          /* THEN                          */ 01954000
         CR    @15,DEVSAVE                                         0446 01955000
         BNL   @RF00446                                            0446 01956000
*           DO;                                                    0447 01957000
*             JDX=JDXST;            /* START NEW SECONDARY LIST      */ 01958000
         LR    JDX,JDXST                                           0448 01959000
*             DEVSAVE=RLCTUMWA;     /* SAVE NEW LOW UTIL.    @Z30WIAW*/ 01960000
         LR    DEVSAVE,@15                                         0449 01961000
*           END;                                                   0450 01962000
*         JDXPTR=IDXPTR;            /* MOVE CANDIDATE TO             */ 01963000
@RF00446 LH    @15,IDXPTR(,IDX)                                    0451 01964000
         STH   @15,JDXPTR(,JDX)                                    0451 01965000
*         JDX=JDX+IDXLEN;           /* SECONDARY LIST                */ 01966000
         ALR   JDX,IDXLEN                                          0452 01967000
*       END;                                                       0453 01968000
*   END;                                                           0454 01969000
@RF00444 ALR   IDX,IDXLEN                                          0454 01970000
@DE00443 CR    IDX,IDXEND                                          0454 01971000
         BNH   @DL00443                                            0454 01972000
*   END DEVRULE2;                   /*                       @Z30WIAW*/ 01973000
@EL00005 DS    0H                                                  0455 01974000
@EF00005 DS    0H                                                  0455 01975000
@ER00005 BR    @14                                                 0455 01976000
*                                                                  0456 01977000
*/********************************************************************/ 01978000
*/*                                                                  */ 01979000
*/*      DEVRULE3 - DEVALLOC INTERNAL SUBROUTINE             @Z30WIAW*/ 01980000
*/*                                                                  */ 01981000
*/*      FUNCTION -                                                  */ 01982000
*/*        CHOOSE DIRECT ACCESS UCBS WITH LOWEST ALLOCATED           */ 01983000
*/*        USER COUNT                                                */ 01984000
*/*        FOR TAPE CANDIDATES, PICK THE DEVICE WITH UCB     @ZM43099*/ 01985000
*/*        ADDRESS THAT IS HIGHER THAN, AND NEAREST TO,      @ZM43099*/ 01986000
*/*        THE LAST PREVIOUSLY ASSIGNED TAPE.                @ZM43099*/ 01987000
*/*        IF NO HIGHER UCB ADDRESS EXISTS, PICK THE UCB     @ZM43099*/ 01988000
*/*        WITH THE LOWEST ADDRESS.                          @ZM43099*/ 01989000
*/********************************************************************/ 01990000
*                                                                  0456 01991000
*DEVRULE3:                                                         0456 01992000
*   PROC OPTIONS(NOSAVE);           /*                       @Z30WIAW*/ 01993000
DEVRULE3 DS    0H                                                  0457 01994000
*   DEVSAVE=CON256;                 /* INIT USER COUNT > MAX POSSIBLE*/ 01995000
         LA    DEVSAVE,256                                         0457 01996000
*   IDX=IDXST;                      /* SET UP ADDRESSING TO FIRST UCB*/ 01997000
         LR    IDX,IDXST                                           0458 01998000
*   IF UCB3DACC='1'B THEN           /* IF THE CANDIDATES ARE DA,     */ 01999000
         LH    @15,IDXPTR(,IDX)                                    0459 02000000
         AL    @15,DEVST1                                          0459 02001000
         L     @15,UCBPTR2(,@15)                                   0459 02002000
         TM    UCB3DACC(@15),B'00100000'                           0459 02003000
         BNO   @RF00459                                            0459 02004000
*     DO IDX=IDX                    /* LOOK AT EACH CANDIDATE ON     */ 02005000
*           TO IDXEND BY IDXLEN;    /* PRIMARY LIST                  */ 02006000
         B     @DE00460                                            0460 02007000
@DL00460 DS    0H                                                  0461 02008000
*       IF UCBUSER<=DEVSAVE THEN    /* IF UCB USER COUNT IS <=       */ 02009000
         LH    @15,IDXPTR(,IDX)                                    0461 02010000
         AL    @15,DEVST1                                          0461 02011000
         L     @15,UCBPTR2(,@15)                                   0461 02012000
         SLR   @08,@08                                             0461 02013000
         IC    @08,UCBUSER(,@15)                                   0461 02014000
         CR    @08,DEVSAVE                                         0461 02015000
         BH    @RF00461                                            0461 02016000
*         DO;                       /* LOWEST SO FAR THEN            */ 02017000
*           IF UCBUSER<DEVSAVE THEN /* IF UCB USER COUNT LESS THAN   */ 02018000
         CR    @08,DEVSAVE                                         0463 02019000
         BNL   @RF00463                                            0463 02020000
*             DO;                   /* OLD LOW THEN                  */ 02021000
*               JDX=JDXST;          /* START NEW SECONDARY LIST      */ 02022000
         LR    JDX,JDXST                                           0465 02023000
*               DEVSAVE=UCBUSER;    /* SAVE NEW LOW USER COUNT       */ 02024000
         LR    DEVSAVE,@08                                         0466 02025000
*             END;                                                 0467 02026000
*           JDXPTR=IDXPTR;          /* MOVE CANDIDATE TO             */ 02027000
@RF00463 LH    @15,IDXPTR(,IDX)                                    0468 02028000
         STH   @15,JDXPTR(,JDX)                                    0468 02029000
*           JDX=JDX+IDXLEN;         /* TO SECONDARY LIST             */ 02030000
         ALR   JDX,IDXLEN                                          0469 02031000
*         END;                                                     0470 02032000
*     END;                                                         0471 02033000
@RF00461 ALR   IDX,IDXLEN                                          0471 02034000
@DE00460 CR    IDX,IDXEND                                          0471 02035000
         BNH   @DL00460                                            0471 02036000
*   ELSE                            /* CANDIDATES ARE NOT DA,      0472 02037000
*                                                            @ZM43099*/ 02038000
*     IF UCB3TAPE='1'B THEN         /* BUT ARE TAPE          @ZM43099*/ 02039000
         B     @RC00459                                            0472 02040000
@RF00459 LH    @15,IDXPTR(,IDX)                                    0472 02041000
         AL    @15,DEVST1                                          0472 02042000
         L     @15,UCBPTR2(,@15)                                   0472 02043000
         TM    UCB3TAPE(@15),B'10000000'                           0472 02044000
         BNO   @RF00472                                            0472 02045000
*       DO;                         /* PICK ONE :            @ZM43099*/ 02046000
*         DEVSAVE=MAXCUA;           /* INIT TO > MAX PTR.    @ZM43099*/ 02047000
         L     DEVSAVE,@CF02998                                    0474 02048000
*         DO IDX=IDX TO IDXEND      /* LOOK THROUGH PRIMARY  @ZM43099*/ 02049000
*               BY IDXLEN;          /* LIST                  @ZM43099*/ 02050000
         B     @DE00475                                            0475 02051000
@DL00475 DS    0H                                                  0476 02052000
*           IF UCBPTR2>RCVTAPAD&    /* HIGHER PTR THAN LAST, @ZA20975*/ 02053000
*               UCBPTR2<DEVSAVE     /* BUT LOWER THAN PREVIOUS     0476 02054000
*                                                            @ZA20975*/ 02055000
*             THEN                                                 0476 02056000
         LH    @15,IDXPTR(,IDX)                                    0476 02057000
         L     @08,DEVST1                                          0476 02058000
         ALR   @08,@15                                             0476 02059000
         L     @08,UCBPTR2(,@08)                                   0476 02060000
         C     @08,RCVTAPAD(,RMCTPTR)                              0476 02061000
         BNH   @RF00476                                            0476 02062000
         CR    @08,DEVSAVE                                         0476 02063000
         BNL   @RF00476                                            0476 02064000
*             DO;                   /* IN THE LIST,          @ZM43099*/ 02065000
*               DEVSAVE=UCBPTR2;    /* SAVE FOR LOOP.        @ZA20975*/ 02066000
         LR    DEVSAVE,@08                                         0478 02067000
*               JDX=JDXST;          /* RESTART SECONDARY LIST      0479 02068000
*                                                            @ZM43099*/ 02069000
         LR    JDX,JDXST                                           0479 02070000
*               JDXPTR=IDXPTR;      /* MOVE CANDIDATE        @ZM43099*/ 02071000
         STH   @15,JDXPTR(,JDX)                                    0480 02072000
*               JDX=JDX+IDXLEN;     /* TO SECONDARY LIST.    @ZM43099*/ 02073000
         ALR   JDX,IDXLEN                                          0481 02074000
*             END;                  /* END SEARCH FOR NEXT   @ZM43099*/ 02075000
*         END;                      /* HIGHEST PTR.          @ZM43099*/ 02076000
@RF00476 ALR   IDX,IDXLEN                                          0483 02077000
@DE00475 CR    IDX,IDXEND                                          0483 02078000
         BNH   @DL00475                                            0483 02079000
*         IF DEVSAVE=MAXCUA THEN    /* IF NONE WAS FOUND,    @ZM43099*/ 02080000
         C     DEVSAVE,@CF02998                                    0484 02081000
         BNE   @RF00484                                            0484 02082000
*           DO IDX=IDXST TO IDXEND  /* SEARCH PRIMARY LIST   @ZM43099*/ 02083000
*                 BY IDXLEN;        /* FOR THE LOWEST UCBPTR @ZM43099*/ 02084000
         LR    IDX,IDXST                                           0485 02085000
         B     @DE00485                                            0485 02086000
@DL00485 DS    0H                                                  0486 02087000
*             IF UCBPTR2<DEVSAVE    /* IF THIS IS LOWER THAN @ZA20975*/ 02088000
*               THEN                                               0486 02089000
         LH    @15,IDXPTR(,IDX)                                    0486 02090000
         L     @08,DEVST1                                          0486 02091000
         ALR   @08,@15                                             0486 02092000
         L     @08,UCBPTR2(,@08)                                   0486 02093000
         CR    @08,DEVSAVE                                         0486 02094000
         BNL   @RF00486                                            0486 02095000
*               DO;                 /* OTHERS                @ZM43099*/ 02096000
*                 DEVSAVE=UCBPTR2;  /* SAVE FOR LOOP         @ZA20975*/ 02097000
         LR    DEVSAVE,@08                                         0488 02098000
*                 JDX=JDXST;        /* RESTART SECONDARY LIST      0489 02099000
*                                                            @ZM43099*/ 02100000
         LR    JDX,JDXST                                           0489 02101000
*                 JDXPTR=IDXPTR;    /* WITH THIS CANDIDATE.  @ZM43099*/ 02102000
         STH   @15,JDXPTR(,JDX)                                    0490 02103000
*                 JDX=JDX+IDXLEN;   /* UPDATE                @ZM43099*/ 02104000
         ALR   JDX,IDXLEN                                          0491 02105000
*               END;                /* AND CONTINUE SEARCH.  @ZM43099*/ 02106000
*           END;                    /* LOWEST UCBPTR IS FOUND      0493 02107000
*                                                            @ZM43099*/ 02108000
@RF00486 ALR   IDX,IDXLEN                                          0493 02109000
@DE00485 CR    IDX,IDXEND                                          0493 02110000
         BNH   @DL00485                                            0493 02111000
*       END;                                                       0494 02112000
*   END DEVRULE3;                   /*                       @Z30WIAW*/ 02113000
@EL00006 DS    0H                                                  0495 02114000
@EF00006 DS    0H                                                  0495 02115000
@ER00006 BR    @14                                                 0495 02116000
*                                                                  0496 02117000
*/********************************************************************/ 02118000
*/*                                                                  */ 02119000
*/*      DEVRULE4 - DEVALLOC INTERNAL SUBROUTINE             @Z30WIAW*/ 02120000
*/*                                                                  */ 02121000
*/*      FUNCTION -                                                  */ 02122000
*/*        FROM LIST OF EQUAL CANDIDATES CHOOSE ONE AT RANDOM        */ 02123000
*/*                                                                  */ 02124000
*/********************************************************************/ 02125000
*                                                                  0496 02126000
*DEVRULE4:                                                         0496 02127000
*   PROC OPTIONS(NOSAVE);           /*                       @Z30WIAW*/ 02128000
DEVRULE4 DS    0H                                                  0497 02129000
*   IDX=IDXST                       /* CHOOSE CANDIDATE FROM PRIMARY */ 02130000
*       +ABS((DEVRV/CON4096*TWO)    /* LIST AT POSITION DETERMINED   */ 02131000
*       //(IDXEND-IDXST+TWO));      /* FROM CURRENT TOD CLOCK        */ 02132000
         L     IDX,DEVRV(,RMCTPTR)                                 0497 02133000
         SRL   IDX,12                                              0497 02134000
         ALR   IDX,IDX                                             0497 02135000
         LR    @15,IDXEND                                          0497 02136000
         SLR   @15,IDXST                                           0497 02137000
         AL    @15,@CF00188                                        0497 02138000
         LR    @11,IDX                                             0497 02139000
         SLR   @10,@10                                             0497 02140000
         DR    @10,@15                                             0497 02141000
         ALR   @10,IDXST                                           0497 02142000
         LR    IDX,@10                                             0497 02143000
*   JDXPTR=IDXPTR;                  /* MOVE CANDIDATE TO SECONDARY   */ 02144000
         LH    @15,IDXPTR(,IDX)                                    0498 02145000
         STH   @15,JDXPTR(,JDX)                                    0498 02146000
*   JDX=JDX+IDXLEN;                 /* LIST                          */ 02147000
         ALR   JDX,IDXLEN                                          0499 02148000
*   END DEVRULE4;                   /*                       @Z30WIAW*/ 02149000
@EL00007 DS    0H                                                  0500 02150000
@EF00007 DS    0H                                                  0500 02151000
@ER00007 BR    @14                                                 0500 02152000
*                                                                  0501 02153000
*/********************************************************************/ 02154000
*/*                                                                  */ 02155000
*/*      DEVRULE5 - DEVALLOC INTERNAL SUBROUTINE             @Z30WIAW*/ 02156000
*/*                                                                  */ 02157000
*/*      FUNCTION - INSURE THAT IF POSSIBLE SELECTED CANDIDATE       */ 02158000
*/*        HAS NOT BEEN PREVIOUSLY ALLOCATED TO SAME USER.           */ 02159000
*/*        IF SO, FIND A CANDIDATE THAT HAS NOT BEEN         @ZM43099*/ 02160000
*/*        ALLOCATED TO SAME USER.  IF NONE EXIST, RECREATE  @ZM43099*/ 02161000
*/*        THE ORIGINAL LIST AND PICK ONE CANDIDATE AT       @ZM43099*/ 02162000
*/*        RANDOM TO AVOID CLUMPING TO ONE CHANNEL & DEVICE. @ZM43099*/ 02163000
*/*                                                                  */ 02164000
*/********************************************************************/ 02165000
*                                                                  0501 02166000
*DEVRULE5:                                                         0501 02167000
*   PROC OPTIONS(NOSAVE);           /*                       @Z30WIAW*/ 02168000
DEVRULE5 DS    0H                                                  0502 02169000
*   IDX=IDXST;                      /* POINT TO SELECTED CANDIDATE   */ 02170000
         LR    IDX,IDXST                                           0502 02171000
*   IF UCB3DACC='1'B                /* IF CANDIDATE IS DA DEVICE     */ 02172000
*       &DEVLCB^=ZERO               /* AND USER HAS OTHER DEVICES  0503 02173000
*                                      ALLOCATED             @Z40BPEB*/ 02174000
*       &DEVDUP='0'B THEN           /* AND SUCH DEVICES HAVE NOT BEEN*/ 02175000
         LH    @15,IDXPTR(,IDX)                                    0503 02176000
         AL    @15,DEVST1                                          0503 02177000
         L     @15,UCBPTR2(,@15)                                   0503 02178000
         TM    UCB3DACC(@15),B'00100000'                           0503 02179000
         BNO   @RF00503                                            0503 02180000
         L     @08,DEVLCB                                          0503 02181000
         LTR   @08,@08                                             0503 02182000
         BZ    @RF00503                                            0503 02183000
         TM    DEVDUP,B'10000000'                                  0503 02184000
         BNZ   @RF00503                                            0503 02185000
*     DO;                           /* REMOVED FROM THE INPUT LIST,  */ 02186000
*       DEVSAVE=ADDR(UCBOB);        /* GET ADDRESS OF CANDIDATE      */ 02187000
         LR    DEVSAVE,@15                                         0505 02188000
*       DO DEVX2=DEVST2 TO          /* COMPARE CANDIDATES ADDRESS TO */ 02189000
*             DEVEND2 BY DEVXLEN    /* EACH ADDRESS IN ALLOCATED     */ 02190000
*             WHILE DEVSAVE^=       /* UCB LIST                      */ 02191000
*             DEVUCB2;              /*                               */ 02192000
         L     DEVX2,DEVST2                                        0506 02193000
         B     @DE00506                                            0506 02194000
@DL00506 C     DEVSAVE,DEVUCB2(,DEVX2)                             0506 02195000
         BE    @DC00506                                            0506 02196000
*       END;                                                       0507 02197000
         AL    DEVX2,DEVXLEN                                       0507 02198000
@DE00506 C     DEVX2,DEVEND2                                       0507 02199000
         BNH   @DL00506                                            0507 02200000
@DC00506 DS    0H                                                  0508 02201000
*       IF DEVX2<=DEVEND2 THEN      /* IF THERE IS A MATCH,          */ 02202000
         C     DEVX2,DEVEND2                                       0508 02203000
         BH    @RF00508                                            0508 02204000
*         DO;                       /* REBUILD PRIMARY LIST TO     0509 02205000
*                                      CONTAIN                       */ 02206000
*           DO DEVX1=DEVST1 TO      /* NO PREVIOUSLY ALLOCATED UCBS. */ 02207000
*                 DEVEND1 BY DEVXLEN;/* LOOK AT EACH CANDIDATE       */ 02208000
         L     @15,DEVST1                                          0510 02209000
         B     @DE00510                                            0510 02210000
@DL00510 DS    0H                                                  0511 02211000
*             DEVSAVE=DEVUCB1;      /* SAVE ADDRESS OF CANDIDATE     */ 02212000
         L     DEVSAVE,DEVUCB1(,@15)                               0511 02213000
*             DO DEVX2=DEVST2 TO    /* COMPARE CANDIDATES ADDRESS    */ 02214000
*                   DEVEND2 BY DEVXLEN/* TO EACH ADDRESS IN THE      */ 02215000
*                   WHILE DEVSAVE^= /* ALLOCATED UCBLIST             */ 02216000
*                   DEVUCB2;        /*                               */ 02217000
         L     DEVX2,DEVST2                                        0512 02218000
         B     @DE00512                                            0512 02219000
@DL00512 C     DEVSAVE,DEVUCB2(,DEVX2)                             0512 02220000
         BE    @DC00512                                            0512 02221000
*             END;                  /*                               */ 02222000
         AL    DEVX2,DEVXLEN                                       0513 02223000
@DE00512 C     DEVX2,DEVEND2                                       0513 02224000
         BNH   @DL00512                                            0513 02225000
@DC00512 DS    0H                                                  0514 02226000
*             IF DEVX2>DEVEND2 THEN /* IF THERE IS NOT A MATCH       */ 02227000
         C     DEVX2,DEVEND2                                       0514 02228000
         BNH   @RF00514                                            0514 02229000
*               DO;                 /*                               */ 02230000
*                 JDXPTR=           /* COMPUTE INDEX INTO DEV LIST   */ 02231000
*                     DEVX1-DEVST1;                                0516 02232000
         L     @15,DEVX1                                           0516 02233000
         SL    @15,DEVST1                                          0516 02234000
         STH   @15,JDXPTR(,JDX)                                    0516 02235000
*                 JDX=JDX+IDXLEN;   /* POINT TO NEXT ENTRY IN SEC LST*/ 02236000
         ALR   JDX,IDXLEN                                          0517 02237000
*               END;                                               0518 02238000
*           END;                    /*                               */ 02239000
@RF00514 L     @15,DEVX1                                           0519 02240000
         AL    @15,DEVXLEN                                         0519 02241000
@DE00510 ST    @15,DEVX1                                           0519 02242000
         C     @15,DEVEND1                                         0519 02243000
         BNH   @DL00510                                            0519 02244000
*           DEVDUP='1'B;            /* ALLOCATED DEVICES HAVE BEEN 0520 02245000
*                                      REMOVED FROM LIST.... @ZM43099*/ 02246000
         OI    DEVDUP,B'10000000'                                  0520 02247000
*           IF JDX^=JDXST THEN      /* SOME UNALLOCATED CANDIDATES...*/ 02248000
         CR    JDX,JDXST                                           0521 02249000
         BE    @RF00521                                            0521 02250000
*             DEVEPX=DEVEPRST;      /* APPLY ALL RULES TO SHORT LIST */ 02251000
         L     @15,DEVEPRST                                        0522 02252000
         ST    @15,DEVEPX                                          0522 02253000
*           ELSE                    /* ALL CANDIDATES HAVE   @ZM43099*/ 02254000
*             DO;                   /* ALREADY BEEN ALLOC.   @ZM43099*/ 02255000
         B     @RC00521                                            0523 02256000
@RF00521 DS    0H                                                  0524 02257000
*               DEVEPX=ADDR(DEVEPTAB(3));/* PREPARE RULE 4   @ZM43099*/ 02258000
         LA    @15,DEVEPTAB+8                                      0524 02259000
         ST    @15,DEVEPX                                          0524 02260000
*               DO DEVX1=DEVST1 TO DEVEND1/* USING ENTIRE    @ZM43099*/ 02261000
*                     BY DEVXLEN;   /* ORIGINAL CANDIDATE    @ZM43099*/ 02262000
         L     @15,DEVST1                                          0525 02263000
         B     @DE00525                                            0525 02264000
@DL00525 DS    0H                                                  0526 02265000
*                 JDXPTR=DEVX1-DEVST1;/* LIST TO FORM THE    @ZM43099*/ 02266000
         LR    @08,@15                                             0526 02267000
         SL    @08,DEVST1                                          0526 02268000
         STH   @08,JDXPTR(,JDX)                                    0526 02269000
*                 JDX=JDX+IDXLEN;   /* NEW PRIMARY LIST.     @ZM43099*/ 02270000
         ALR   JDX,IDXLEN                                          0527 02271000
*               END;                /* LIST IS DONE.         @ZM43099*/ 02272000
         AL    @15,DEVXLEN                                         0528 02273000
@DE00525 ST    @15,DEVX1                                           0528 02274000
         C     @15,DEVEND1                                         0528 02275000
         BNH   @DL00525                                            0528 02276000
*             END;                  /* READY FOR RULE 4      @ZM43099*/ 02277000
*         END;                                                     0530 02278000
*     END;                                                         0531 02279000
*   END DEVRULE5;                   /*                       @Z30WIAW*/ 02280000
@EL00008 DS    0H                                                  0532 02281000
@EF00008 DS    0H                                                  0532 02282000
@ER00008 BR    @14                                                 0532 02283000
*   RESPECIFY                                                      0533 02284000
*     R14 UNRESTRICTED;                                            0533 02285000
*   RESPECIFY                                                      0534 02286000
*    (R1,                                                          0534 02287000
*     RTNREG,                                                      0534 02288000
*     NTRREG,                                                      0534 02289000
*     RLCTPTR,                                                     0534 02290000
*     IMCBPTR)                      /* PROTECT REGS AGAIN    @ZA20975*/ 02291000
*         RESTRICTED;                                              0534 02292000
*   RESPECIFY                                                      0535 02293000
*     UCBOB BASED(UCBPTR);          /* NORMAL BASING         @ZA20975*/ 02294000
*   RESPECIFY                                                      0536 02295000
*     RLCT BASED(RLCTPTR);          /* NORMAL BASING         @ZA20975*/ 02296000
*   END IRARME28;                                                  0537 02297000
*   END IRARMIOM                                                   0538 02298000
*                                                                  0538 02299000
