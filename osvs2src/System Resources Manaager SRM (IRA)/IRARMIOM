*                 END;                                             0295 01160000
*               ELSE                /* OTHERWIZE                     */ 01161000
*                 ILDIF=RLCTUTIL;   /* INSURE ZERO LCH UTILIZATION 0296 01162000
*                                      FACTOR                        */ 01163000
         B     @RC00289                                            0296 01164000
@RF00289 LH    ILDIF,RLCTUTIL(,RLCTPTR)                            0296 01165000
*             ILDIF=ILDIF           /* COMPUTE DIFFERENCE BETWEEN    */ 01166000
*                 -RLCTUTIL;        /* UTILIZATION AND THRESHOLD     */ 01167000
@RC00289 DS    0H                                                  0297 01168000
@RC00282 SH    ILDIF,RLCTUTIL(,RLCTPTR)                            0297 01169000
*             RLCTRVUF=             /* UTILIZATION FACTOR IS SQUARE     01170000
*                                      OF                            */ 01171000
*                 ABS(ILDIF)        /* THE DIFFERENCE WITH SAME SIGN */ 01172000
*                 *ILDIF;           /* AS DIFFERENCE                 */ 01173000
         LPR   @11,ILDIF                                           0298 01174000
         MR    @10,ILDIF                                           0298 01175000
         STH   @11,RLCTRVUF(,RLCTPTR)                              0298 01176000
*           END;                    /*                               */ 01177000
