@RF00064 DS    0H                                                  0069 00054000
*       IF ILTIME>ICCMNUIN THEN     /* IF ELAPSED TIME IS LONG ENOUGH*/ 00055000
         L     @07,ILTIME                                          0069 00056000
         C     @07,ICCMNUIN(,RMCTPTR)                              0069 00057000
         BNH   @RF00069                                            0069 00058000
*         DO;                       /*                               */ 00059000
*           OUCBIOR                 /* COMPUTE USER I/O REQUEST RATE */ 00060000
*               =(OUCBIOSM+DAIOSM   /* IN TERMS OF REQUESTS PER    0071 00061000
*                                                            @ZA10808*/ 00062000
*               -OUXBILS)           /* SECOND OF ELAPSED TIME        */ 00063000
*               *I1000              /*                               */ 00064000
*               /ILTIME;            /*                               */ 00065000
         LH    @11,DAIOSM(,ASCBPTR)                                0071 00066000
         N     @11,@CF03075                                        0071 00067000
         AL    @11,OUCBIOSM(,OUCBPTR)                              0071 00068000
         LR    @08,@11                                             0071 00069000
         SL    @08,OUXBILS(,OUXBPTR)                               0071 00070000
         ST    @11,@TF00001                                        0071 00071000
         LR    @11,@08                                             0071 00072000
