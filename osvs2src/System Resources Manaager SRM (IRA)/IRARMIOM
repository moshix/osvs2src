         BNL   @RF00282                                            0282 01126000
*               DO;                                                0283 01127000
*                 ILDIF=ICCHIUTH;   /* SAVE HIGH THRESHOLD VALUE     */ 01128000
         LR    ILDIF,@01                                           0284 01129000
*                 ILLCBPT=ILLCBPT   /* REMEMBER LCH IS OUT OF BALANCE*/ 01130000
*                     |RLCTBIT;     /*                               */ 01131000
         OC    ILLCBPT(4),RLCTBIT(RLCTPTR)                         0285 01132000
*                 ICVOLCBT=         /* REMEMBER LCH IS OVERUTILIZED  */ 01133000
*                     ICVOLCBT      /*                               */ 01134000
*                     |RLCTBIT;     /*                               */ 01135000
         OC    ICVOLCBT(4,RMCTPTR),RLCTBIT(RLCTPTR)                0286 01136000
*                 RLCTOVER='1'B;    /* INDICATE LCH IS OVER UTILIZED */ 01137000
         OI    RLCTOVER(RLCTPTR),B'10000000'                       0287 01138000
*               END;                                               0288 01139000
*             ELSE                  /* OTHERWIZE IF LCH UTILIZATION  */ 01140000
*               IF RLCTUTIL<        /* LESS THAN LOW IMBALANCE       */ 01141000
*                   ICCLOUTH THEN   /* THRESHOLD,                    */ 01142000
         B     @RC00282                                            0289 01143000
