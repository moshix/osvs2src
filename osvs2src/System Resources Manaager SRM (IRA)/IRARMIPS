         N     @14,OPMASKTB-4(@15)                                 0846 04148000
         LTR   @14,@14                                             0846 04149000
         BZ    @RF00846                                            0846 04150000
*                 OPWRMPPT->OPWARRAY(J)=OPWARYPT->OPWARRAY(J);/*   0847 04151000
*                                      STORE PARAMETER VALUE IN RMPT    04152000
*                                      ENTRY                         */ 04153000
         L     @01,OPWARYPT                                        0847 04154000
         AL    @01,@CF02036                                        0847 04155000
         L     @14,OPWARRAY(@15,@01)                               0847 04156000
         L     @01,OPWRMPPT                                        0847 04157000
         AL    @01,@CF02036                                        0847 04158000
         ST    @14,OPWARRAY(@15,@01)                               0847 04159000
*             END;                                                 0848 04160000
@RF00846 AL    J,@CF00077                                          0848 04161000
@DE00845 C     J,OPCPARMN                                          0848 04162000
         BNH   @DL00845                                            0848 04163000
*           END;                    /* COMPLETED PROCESSING        0849 04164000
*                                      CATEGORY(I)                   */ 04165000
*       END;                        /* COMPLETED PROCESSING ALL    0850 04166000
*                                      CATEGORIES                    */ 04167000
@RF00839 AL    I,@CF00077                                          0850 04168000
         C     I,@CF00082                                          0850 04169000
         BNH   @DL00838                                            0850 04170000
*     END;                                                         0851 04171000
*   RETURN;                         /* RETURN TO CALLER              */ 04172000
         B     @EL00001                                            0852 04173000
*                                                                  0853 04174000
*/********************************************************************/ 04175000
*/*                                                                  */ 04176000
*/*  OPT DATA PROCESSING ROUTINES                                    */ 04177000
*/*                                                                  */ 04178000
*/*    PURPOSE - TO PROCESS DATA FROM THE OPT LIST                   */ 04179000
*/*    LINKAGE - VIA CALL                                            */ 04180000
*/*    INPUT - ADDR OF A 1-BYTE FIELD CONTAINING THE PROCESSING      */ 04181000
*/*            SUBROUTINE NUMBER                                     */ 04182000
*/*    OUTPUT - NONE                                                 */ 04183000
*/*                                                                  */ 04184000
*/********************************************************************/ 04185000
*                                                                  0853 04186000
*RMOPINIT:                                                         0853 04187000
*   PROC(OPCODEPT);                 /* OPT PARM PROCESSING    SUBRTNS*/ 04188000
RMOPINIT STM   @14,@12,@SA00012                                    0853 04189000
         MVC   @PC00012(4),0(@01)                                  0853 04190000
*   DECLARE                                                        0854 04191000
*     OPCODEPT PTR(31),             /* PTR TO INPUT CODE             */ 04192000
*     OPBRINDX FIXED(8) BASED(OPCODEPT),/* INPUT CODE                */ 04193000
*/*                                                                  */ 04194000
*/* OPBRTAB IS A TABLE CONTAINING THE ADDRESSES OF THE SUBROUTINES   */ 04195000
*/* WHICH PROCESS OPT DATA.  THE DIMENSION OF OPBRTAB IS THE NUMBER  */ 04196000
*/* OF SUBROUTINES.  THE INPUT PARAMETER TO THE RMOPINIT PROCEDURE   */ 04197000
*/* IS USED TO ACCESS THE SUBSCRIPT FOR OPBRTAB                      */ 04198000
*/*                                                                  */ 04199000
*                                                                  0854 04200000
*     OPBRTAB(07) PTR INIT(ADDR(RMOP001),ADDR(RMOP002),ADDR(RMOP003),   04201000
*         ADDR(RMOP004),ADDR(RMOP005),ADDR(RMOP006),ADDR(RMOP007)),     04202000
*     RMOPLBL LABEL BASED(OPBRTAB(OPBRINDX))/* LABEL FOR OPT PARM  0854 04203000
*                                      PROC SUBROUTINES              */ 04204000
*         ;                                                        0854 04205000
*   DCL                                                            0855 04206000
*     DIGIT1 CHAR(1),               /* RFC VALUES AS THEY    @Z40BPEM*/ 04207000
*     DIGIT2 CHAR(1),               /* APPEAR IN OPT         @Z40BPEM*/ 04208000
*     OPTERV CHAR(6);               /* ERV VALUE AS IT APPEARS IN OPT   04209000
*                                                            @Z40BPEM*/ 04210000
*                                                                  0856 04211000
*/*  BRANCH TO THE SUBROUTINE BASED ON THE INPUT PARAMETER           */ 04212000
*                                                                  0856 04213000
*   GO TO RMOPLBL;                  /* BRANCH TO OPT PROCESSING    0856 04214000
*                                      SUBROUTINES                   */ 04215000
         L     @15,@PC00012                                        0856 04216000
         L     @01,OPCODEPT(,@15)                                  0856 04217000
         SLR   @15,@15                                             0856 04218000
         IC    @15,OPBRINDX(,@01)                                  0856 04219000
         SLA   @15,2                                               0856 04220000
         L     @01,OPBRTAB-4(@15)                                  0856 04221000
         BR    @01                                                 0856 04222000
*RMOP001:                                                          0857 04223000
*                                                                  0857 04224000
*/*  FOUND AN ERROR , MARK CATEGORY BEING PROCESSED AS ERRONEOUS     */ 04225000
*                                                                  0857 04226000
*   OPCATGER=OPCATGER|OPCATMK;      /* SET ERROR INDICATOR           */ 04227000
RMOP001  L     @15,@PC00001+16                                     0857 04228000
         OC    OPCATGER(4,@15),OPCATMK                             0857 04229000
*   RETURN;                         /* RETURN TO CALLER              */ 04230000
@EL00012 DS    0H                                                  0858 04231000
@EF00012 DS    0H                                                  0858 04232000
@ER00012 LM    @14,@12,@SA00012                                    0858 04233000
         BR    @14                                                 0858 04234000
*RMOP002:                                                          0859 04235000
