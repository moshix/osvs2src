         BNZ   @RF00659                                            0659 03198000
*     DO;                                                          0660 03199000
*                                                                  0660 03200000
*/*  FOR EACH PERF GRP PERIOD, UPDATE WPGPDURP AND WPGPISVP          */ 03201000
*/*  STORE THE ISV FOR THE FIRST PERIOD IN WPGDISV.  IF ISV IS NOT   */ 03202000
*/*  SPECIFIED FOR A PERIOD, USE THE BASE ISV VALUE                  */ 03203000
*/*  FOR THE LAST PERIOD, SET THE LAST PERIOD INDICATOR AND SET      */ 03204000
*/*  DURATION TO ZERO TO INDICATE THAT DURATION IS TO BE IGNORED.    */ 03205000
*/*  BUILD WPGL ENTRIES FOLLOWING THE PERFORMANCE GROUP PERIODS.     */ 03206000
*                                                                  0661 03207000
*       WPGLPTR=WPGPPTR;            /* PTR TO 1ST WPGL ENTRY         */ 03208000
         L     WPGLPTR,WPGPPTR                                     0661 03209000
*       WPGPPTR=WPGDPTR+LENGTH(WPGD)-LENGTH(WPGP);/* PTR TO 1ST    0662 03210000
*                                      PERIOD                        */ 03211000
         L     @02,WPGDPTR                                         0662 03212000
         SL    @02,@CF02029                                        0662 03213000
         ST    @02,WPGPPTR                                         0662 03214000
*       DO I=ONE TO IPWPGPC;        /* NO OF PERF GRP PERIODS FOR  0663 03215000
*                                      CURRENT PERF GRP              */ 03216000
         LA    I,1                                                 0663 03217000
         B     @DE00663                                            0663 03218000
@DL00663 DS    0H                                                  0664 03219000
*         IF I=ONE THEN             /* 1ST PERF GRP PERIOD           */ 03220000
         C     I,@CF00077                                          0664 03221000
         BNE   @RF00664                                            0664 03222000
*           DO;                                                    0665 03223000
*             WPGDISV=IPWPGL(WPGPISVP);/* STORE 1ST PERIOD ISV       */ 03224000
         L     @15,WPGPPTR                                         0666 03225000
         SLR   @14,@14                                             0666 03226000
         IC    @14,WPGPISVP(,@15)                                  0666 03227000
         LR    @07,@14                                             0666 03228000
         SLA   @07,2                                               0666 03229000
         L     @00,IPWPGL-4(@07)                                   0666 03230000
         L     @07,WPGDPTR                                         0666 03231000
         ST    @00,WPGDISV(,@07)                                   0666 03232000
*             IPWPGLP(WPGPISVP)=BASEISVP;/* SAVE OFFSET OF ISV       */ 03233000
         SLR   @07,@07                                             0667 03234000
         IC    @07,BASEISVP                                        0667 03235000
         STC   @07,IPWPGLP-1(@14)                                  0667 03236000
*             WPGPISVP=BASEISVP;    /* OFFSET OF WPGDISV             */ 03237000
         STC   @07,WPGPISVP(,@15)                                  0668 03238000
*           END;                                                   0669 03239000
*         ELSE                      /* NOT THE 1ST PERF GRP PERIOD   */ 03240000
*           CALL RMIPMPGL(WPGPISVP);/* UPDATE OFFSET OF ISV ENTRY &     03241000
*                                      BUILD WPGL                    */ 03242000
