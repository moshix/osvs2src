         TITLE 'IRARMIPS - SYSTEM RESOURCES MANAGER LIST PROCESSOR     *00001000
                        '                                               00002000
         MACRO                                                          00003000
&MFSNAME MFSYSTR &INITIAL,&ALTLST,&RCALTLS                              00004000
         AGO   .BEGIN                                                   00005000
* MODULE NAME = MFSYSTR                                            *    00006000
*                                                                  *    00007000
* DESCRIPTIVE NAME = MF/1 SYNTAX TABLE STRUCTURE ENTRY             *    00008000
*                                                                  *    00009000
* COPYRIGHT = NONE                                                 *    00010000
*                                                                  *    00011000
* STATUS = RELEASE 2, VERSION 0                                    *    00012000
*                                                                  *    00013000
* FUNCTION = EXECUTABLE MACRO FOR THE SYNTAX TABLE (MFSYNTAB).     *    00014000
*            IT DESCRIBES A STRUCTURE ENTRY. (SEE METHOD OF INVOCA-*    00015000
*            TION.)                                                *    00016000
*                                                                  *    00017000
* NOTES = NONE                                                     *    00018000
*                                                                  *    00019000
* MODULE TYPE = BAL EXECUTABLE MACRO                               *    00020000
*                                                                  *    00021000
* CHANGE ACTIVITY = A000000-999999                           Y02150*    00022000
*                                                                  *    00023000
* MACCOMP Y-2 SC1CQ/GAM20                                          *    00024000
*                                                                  *    00025000
* MACMEAN MFSYSTR MEANS MF/1 SYNTAX TABLE STRUCTURE ENTRY          *    00026000
*                                                                  *    00027000
* METHOD OF INVOCATION                                             *    00028000
*    BAL - MFSYSTR A,B,C                                           *    00029000
*        WHERE                                                     *    00030000
*             A -'1' INDICATES INITIAL STRUCTURE                   *    00031000
*                '0' OTHERWISE                                     *    00032000
*             B -ADDRESS OF A LIST OF ALTERNATIVE ENTRIES (REGULAR)*    00033000
*             C -ADDRESS OF A LIST OF RECURSIVE ALTERNATIVES       *    00034000
*                                                                  *    00035000
*    PLS - NONE                                                    *    00036000
.BEGIN   ANOP                                                           00037000
* MACDATE Y-2 73026                                                *    00038000
         AIF (NOT ('&INITIAL' EQ '1' AND '&RCALTLS' NE '0')).A          00039000
&MFSNAME DC    X'C0' .            INITIAL STRUCTURE WITH RECURSIVE      00040000
*                                 ALTERNATIVES                          00041000
         AGO   .D                                                       00042000
.A       AIF   (NOT ('&INITIAL' EQ '1' AND '&RCALTLS' EQ '0')).B        00043000
&MFSNAME DC    X'80' .            INITIAL STRUCTURE - NOT RECURSIVE     00044000
         AGO   .D                                                       00045000
.B       AIF   (NOT ('&INITIAL' NE '1' AND '&RCALTLS' NE '0')).C        00046000
&MFSNAME DC    X'40' .            RECURSIVE ALTERNATIVES EXIST          00047000
         AGO   .D                                                       00048000
.C       AIF   (NOT ('&INITIAL' NE '1' AND '&RCALTLS' EQ '0')).D        00049000
&MFSNAME DC    X'00' .            NOT INITIAL STRUCUTRE - NO RECURSIVE  00050000
*                                 ALTERNATIVES                          00051000
.D       ANOP                                                           00052000
         DC    AL3(0) .           RESERVED                              00053000
         DC    A(&ALTLST) .       ALTERNATIVE LIST ADDRESS              00054000
         DC    A(&RCALTLS) .      RECURSIVE ALTERNATIVE                 00055000
         MEND                                                           00056000
         MACRO                                                          00057000
&MFANAME MFSYALT &ENDLST,&ITMLST,&INITRTN,&INITPRM                      00058000
         AGO   .BEGIN                                                   00059000
* MODULE NAME = MFSYALT                                            *    00060000
*                                                                  *    00061000
* DESCRIPTIVE NAME = MF/1 SYNTAX TABLE ALTERNATIVE ENTRY           *    00062000
*                                                                  *    00063000
* COPYRIGHT = NONE                                                 *    00064000
*                                                                  *    00065000
* STATUS = RELEASE 2, LEVEL 0                                      *    00066000
*                                                                  *    00067000
* FUNCTION = EXECUTABLE MACRO FOR THE SYNTAX TABLE (MFSYNTAB).     *    00068000
*            IT DESCRIBES AN ALTERNATIVE ENTRY.  (SEE METHOD OF    *    00069000
*            INVOCATION                                            *    00070000
*                                                                  *    00071000
* NOTES = NONE                                                     *    00072000
*                                                                  *    00073000
* MODULE TYPE = EXECUTABLE MACRO (BAL)                             *    00074000
*                                                                  *    00075000
* CHANGE ACTIVITY = A000000-999999                           Y02150*    00076000
*                                                                  *    00077000
* MACCOMP Y-2 SC1CQ/GAM20                                          *    00078000
*                                                                  *    00079000
* MACMEAN MFSYALT MEANS MF/1 SYNTAX TABLE ALTERNATIVE ENTRY        *    00080000
*                                                                  *    00081000
* METHOD OF INVOCATION                                             *    00082000
*    BAL - MFSYALT A,B,C,D                                         *    00083000
*        WHERE                                                     *    00084000
*             A - '1' INDICATES END OF ALTERNATIVES LIST           *    00085000
*                 '0' OTHERWISE                                    *    00086000
*             B - ADDRESS OF AN ITEM ENTRY                         *    00087000
*             C - ADDRESS OF AN INITIALIZATION ROUTINE OR ZERO     *    00088000
*             D - ADDRESS OF A PARAMETER LIST TO BE PASSED TO THE  *    00089000
*                 INITIALIZATION ROUTINE                           *    00090000
*                                                                  *    00091000
*    PLS - NONE                                                    *    00092000
.BEGIN   ANOP                                                           00093000
* MACDATE Y-2 73026                                                *    00094000
         AIF   (NOT ('&ENDLST' EQ '1' AND '&INITRTN' NE '0')).A         00095000
&MFANAME DC    X'C0' .            END OF LIST AND INITIALIZATION        00096000
*                                 ROUTINE EXISTS                        00097000
         AGO   .D                                                       00098000
.A       AIF   (NOT ('&ENDLST' EQ '1' AND '&INITRTN' EQ '0')).B         00099000
&MFANAME DC    X'80' .            END OF LIST                           00100000
         AGO   .D                                                       00101000
.B       AIF   (NOT ('&ENDLST' NE '1' AND '&INITRTN' NE '0')).C         00102000
&MFANAME DC    X'40' .            INITIALIZATION ROUTINE EXISTS         00103000
         AGO   .D                                                       00104000
.C       AIF (NOT ('&ENDLST' NE '1' AND '&INITRTN' EQ '0')).D           00105000
&MFANAME DC    X'00' .            NOT END OF LIST - NO INITIALIZATION   00106000
*                                 ROUTINE EXISTS                        00107000
.D       ANOP                                                           00108000
         DC    AL3(0) .           RESERVED                              00109000
         DC    A(&ITMLST) .       ADDRESS OF AN ITEM LIST               00110000
         DC    A(&INITRTN) .      INITIALIZATION RTN ADDRESS            00111000
         DC    A(&INITPRM) .      ADDRESS OF A PARAMETER LIST           00112000
*                                 FOR INITIALIZATION ROUTINE            00113000
         MEND                                                           00114000
         MACRO                                                          00115000
&MFINAME MFSYITM &ENDLST,&RECGNZR,&TRMDEF,&RCGPARM                      00116000
         AGO   .BEGIN                                                   00117000
* MODULE NAME = MFSYITM                                            *    00118000
*                                                                  *    00119000
* DESCRIPTIVE NAME = MF/1 SYNTAX TABLE ITEM ENTRY                  *    00120000
*                                                                  *    00121000
* COPYRIGHT = NONE                                                 *    00122000
*                                                                  *    00123000
* STATUS = RELEASE 2, LEVEL 0                                      *    00124000
*                                                                  *    00125000
* FUNCTION = EXECUTABLE MACRO FOR MF/1 SYNTAX TABLE (MFSYNTAB).    *    00126000
*                                                                  *    00127000
* NOTES = NONE                                                     *    00128000
*                                                                  *    00129000
* MODULE TYPE = BAL EXECUTABLE MACRO                               *    00130000
*                                                                  *    00131000
* CHANGE ACTIVITY = A000000-999999                           Y02150*    00132000
*                                                                  *    00133000
* MACCOMP Y-2 SC1SQ/GAM20                                          *    00134000
*                                                                  *    00135000
* MACMEAN MFSYITM MEANS MF/1 SYNTAX TABLE ITEM ENTRY               *    00136000
*                                                                  *    00137000
* METHOD OF INVOCATION                                             *    00138000
*    BAL - MFSYITM A,B,C,D                                         *    00139000
*        WHERE                                                     *    00140000
*             A - '1' INDICATES END OF ITEM LIST                   *    00141000
*                 '0' OTHERWISE                                    *    00142000
*             B - ADDRESS OF A RECOGNIZER ROUTINE OR A STRUCTURE   *    00143000
*                 ENTRY                                            *    00144000
*             C - '1' INDICATES ITEM ENTRY IS A TERMINAL TYPE      *    00145000
*                 '0' INDICATES ITEM ENTRY IS A STRUCTURE TYPE     *    00146000
*             D - ADDRESS OF A PARAMETER LIST TO BE PASSED TO THE  *    00147000
*                 RECOGNIZE ROUTINE OR 0.                          *    00148000
*                                                                  *    00149000
*    PLS - NONE                                                    *    00150000
.BEGIN   ANOP                                                           00151000
* MACDATE Y-2 73026                                                *    00152000
         AIF   (NOT ('&ENDLST' EQ '1' AND '&TRMDEF' EQ '1')).A          00153000
&MFINAME DC    X'C0' .            END OF LIST AND ITEM IS               00154000
*                                 TERMINAL TYPE                         00155000
         AGO   .D                                                       00156000
.A       AIF   (NOT ('&ENDLST' EQ '1' AND '&TRMDEF' NE '1')).B          00157000
&MFINAME DC    X'80'              END OF LIST                           00158000
         AGO   .D                                                       00159000
.B       AIF   (NOT ('&ENDLST' NE '1' AND '&TRMDEF' EQ '1')).C          00160000
&MFINAME DC    X'40'              ITEM IS TERMINAL TYPE                 00161000
         AGO   .D                                                       00162000
.C       AIF   (NOT ('&ENDLST' NE '1' AND '&TRMDEF' NE '1')).D          00163000
&MFINAME DC    X'00'              NOT END OF LIST AND ITEM IS           00164000
*                                 DEFINED TYPE                          00165000
.D       ANOP                                                           00166000
         DC    AL3(0) .           RESERVED                              00167000
         DC    A(&RECGNZR) .      ADDRESS OF A RECOGNIZER               00168000
*                                 OR STRUCTURE ENTRY                    00169000
         DC    A(&RCGPARM) .      ADDRESS OF A PARAMETER LIST           00170000
*                                 PASSED TO RECOGNIZER ROUTINE          00171000
         MEND                                                           00172000
IRARMIPS CSECT ,                                                   0002 00173000
@MAINENT DS    0H                                                  0002 00174000
         USING *,@15                                               0002 00175000
         B     @PROLOG                                             0002 00176000
         DC    AL2(@EP00002-@MAINENT)                                   00177000
IRARMFRE DS    0H                                                  0002 00178000
         USING *,@15                                               0002 00179000
         B     @PROLOG                                             0002 00180000
         DC    AL2(@EP00768-IRARMFRE)                                   00181000
         ENTRY IRARMFRE                                                 00182000
IRARMOPT DS    0H                                                  0002 00183000
         USING *,@15                                               0002 00184000
         B     @PROLOG                                             0002 00185000
         DC    AL2(@EP00796-IRARMOPT)                                   00186000
         ENTRY IRARMOPT                                                 00187000
         DROP  @15                                                      00188000
@PROLOG  STM   @14,@12,12(@13)                                     0002 00189000
         BALR  @09,0                                               0002 00190000
@PSTART  LA    @10,4095(,@09)                                      0002 00191000
         LA    @11,4095(,@10)                                      0002 00192000
         LA    @12,4095(,@11)                                      0002 00193000
         USING @PSTART,@09                                         0002 00194000
         USING @PSTART+4095,@10                                    0002 00195000
         USING @PSTART+8190,@11                                    0002 00196000
         USING @PSTART+12285,@12                                   0002 00197000
         L     @00,@SIZDATD                                        0002 00198000
         GETMAIN  R,LV=(0)                                              00199000
         LR    @08,@01                                             0002 00200000
         USING @DATD,@08                                           0002 00201000
         ST    @13,@SA00001+4                                      0002 00202000
         LM    @15,@01,16(@13)                                     0002 00203000
         ST    @08,8(,@13)                                         0002 00204000
         LR    @13,@08                                             0002 00205000
         AH    @15,4(,@15)                                         0002 00206000
         BR    @15                                                 0002 00207000
@EP00002 MVC   @PC00001(12),0(@01)                                 0002 00208000
*   IPWMSTAD=ZERO;                  /* ZERO OUTPUT PARM              */ 00209000
         L     @03,@PC00001+4                                      0029 00210000
         SLR   @15,@15                                             0029 00211000
         ST    @15,IPWMSTAD(,@03)                                  0029 00212000
*   RMCTPTR=CVTOPCTP;               /* ADDR OF SYSTEM RESOURCES    0030 00213000
*                                      MANAGER CONTROL TABLE         */ 00214000
         L     @01,CVTPTR                                          0030 00215000
         L     RMCTPTR,CVTOPCTP(,@01)                              0030 00216000
*   RMPTPTR=RMCTRMPT;               /* ADDR OF RESOURCE MANAGER    0031 00217000
*                                      PARAMETER TABLE               */ 00218000
         L     @15,RMCTRMPT(,RMCTPTR)                              0031 00219000
         ST    @15,RMPTPTR                                         0031 00220000
*/*  SET  MFRTPTR TO THE ADDRESS OF THE ROUTINE WHICH PROCESSES      */ 00221000
*/*  IPS LIST DATA                                                   */ 00222000
*                                                                  0032 00223000
*   MFRTPTR=ADDR(RMIPINIT);         /* ADDR OF IPS LIST PROCESSING 0032 00224000
*                                      ROUTINE                       */ 00225000
         LA    MFRTPTR,RMIPINIT                                    0032 00226000
*/* INITIALIZE  ERROR MESSAGE BUFFERS                                */ 00227000
*                                                                  0033 00228000
*   IPMSGCOM=IPMSGCTX;              /* COMMON MESSAGE TEXT           */ 00229000
         MVC   IPMSGCOM(20),IPMSGCTX                               0033 00230000
*   IPMSGIPS=IPIPSID;               /* IPS MEMBER SUFFIX             */ 00231000
         L     @02,@PC00001                                        0034 00232000
         MVC   IPMSGIPS(2),IPIPSID(@02)                            0034 00233000
*   IPMSGIPA=IPMSGID(1);            /* MESSAGE ID IN INPUT STRING  0035 00234000
*                                      MESSAGE                       */ 00235000
         MVI   IPMSGIPA+8,C' '                                     0035 00236000
         MVC   IPMSGIPA+9(79),IPMSGIPA+8                           0035 00237000
         MVC   IPMSGIPA(8),IPMSGID                                 0035 00238000
*/*  INVOKE RMIPGM ROUTINE TO CONSTRUCT THE FOLLOWING TABLES-        */ 00239000
*/*    WORKLOAD MANAGER SPECIFICATION TABLE   - WMST                 */ 00240000
*                                                                  0036 00241000
*   IPWMSTLN=LENGTH(WMST);          /* LENGTH OF WORKLOAD MGR TBL    */ 00242000
         MVC   IPWMSTLN(4),@CF02023                                0036 00243000
*   CALL RMIPGM(IPWMSTAD,IPWMSTLN,WMSTPTR);/* GETMAIN FOR WMST       */ 00244000
         ST    @03,@AL00001                                        0037 00245000
         LA    @15,IPWMSTLN                                        0037 00246000
         ST    @15,@AL00001+4                                      0037 00247000
         LA    @15,WMSTPTR                                         0037 00248000
         ST    @15,@AL00001+8                                      0037 00249000
         LA    @01,@AL00001                                        0037 00250000
         BAL   @14,RMIPGM                                          0037 00251000
*/*    PERFORMANCE GROUP VECTOR TABLE         - WPGVT                */ 00252000
*                                                                  0038 00253000
*   WMSTPGVS=MAXPGE                 /* MAX SIZE OF PERFORMANCE GROUP    00254000
*                                      VECTOR TABLE                  */ 00255000
*       *LENGTH(WPGVTENT);                                         0038 00256000
         L     @15,WMSTPTR                                         0038 00257000
         MVC   WMSTPGVS(4,@15),@CF02024                            0038 00258000
*   CALL RMIPGM(WMSTPGVT,WMSTPGVS,WPGVPTR);/* GETMAIN FOR WPGV       */ 00259000
         LA    @14,WMSTPGVT(,@15)                                  0039 00260000
         ST    @14,@AL00001                                        0039 00261000
         LA    @15,WMSTPGVS(,@15)                                  0039 00262000
         ST    @15,@AL00001+4                                      0039 00263000
         LA    @15,WPGVPTR                                         0039 00264000
         ST    @15,@AL00001+8                                      0039 00265000
         LA    @01,@AL00001                                        0039 00266000
         BAL   @14,RMIPGM                                          0039 00267000
*/*    PERFORMANCE OBJECTIVE VECTOR TABLE     - WPOVT                */ 00268000
*                                                                  0040 00269000
*   WMSTPOVS=MAXOBJE                /* MAX SIZE OF PERFORMANCE     0040 00270000
*                                      OBJECTIVE VECTOR TABLE        */ 00271000
*       *LENGTH(WPOVTENT);                                         0040 00272000
         L     @15,WMSTPTR                                         0040 00273000
         MVC   WMSTPOVS(4,@15),@CF02025                            0040 00274000
*   CALL RMIPGM(WMSTPOVT,WMSTPOVS,WPOVPTR);/* GETMAIN FOR WPOV       */ 00275000
         LA    @14,WMSTPOVT(,@15)                                  0041 00276000
         ST    @14,@AL00001                                        0041 00277000
         LA    @15,WMSTPOVS(,@15)                                  0041 00278000
         ST    @15,@AL00001+4                                      0041 00279000
         LA    @15,WPOVPTR                                         0041 00280000
         ST    @15,@AL00001+8                                      0041 00281000
         LA    @01,@AL00001                                        0041 00282000
         BAL   @14,RMIPGM                                          0041 00283000
*/*     DOMAIN VECTOR TABLE      - DMVT                      @Z40BPEM*/ 00284000
*                                                                  0042 00285000
*   WMSTDMVS=MAXDMNE*LENGTH(DMVTENT);/* MAX SIZE OF VECTOR TABLE   0042 00286000
*                                                            @Z40BPEM*/ 00287000
         L     @15,WMSTPTR                                         0042 00288000
         MVC   WMSTDMVS(4,@15),@CF02026                            0042 00289000
*   CALL RMIPGM(WMSTDMVT,WMSTDMVS,DMVTPTR);/* GETMAIN FOR DMVT     0043 00290000
*                                                            @Z40BPEM*/ 00291000
         LA    @14,WMSTDMVT(,@15)                                  0043 00292000
         ST    @14,@AL00001                                        0043 00293000
         LA    @15,WMSTDMVS(,@15)                                  0043 00294000
         ST    @15,@AL00001+4                                      0043 00295000
         LA    @15,DMVTPTR                                         0043 00296000
         ST    @15,@AL00001+8                                      0043 00297000
         LA    @01,@AL00001                                        0043 00298000
         BAL   @14,RMIPGM                                          0043 00299000
*/*    PERFORMANCE GROUP DESCRIPTOR TABLE     - WPGDT                */ 00300000
*                                                                  0044 00301000
*   WMSTPGDS=TBLSIZE;               /* INITIAL SIZE OF PERF GRP    0044 00302000
*                                      DESCRIPTOR TABLE              */ 00303000
         L     @15,WMSTPTR                                         0044 00304000
         MVC   WMSTPGDS(4,@15),@CF01245                            0044 00305000
*   CALL RMIPGM(WMSTPGDT,WMSTPGDS,WPGDPTR);/* GETMAIN FOR WPGD       */ 00306000
         LA    @14,WMSTPGDT(,@15)                                  0045 00307000
         ST    @14,@AL00001                                        0045 00308000
         LA    @15,WMSTPGDS(,@15)                                  0045 00309000
         ST    @15,@AL00001+4                                      0045 00310000
         LA    @15,WPGDPTR                                         0045 00311000
         ST    @15,@AL00001+8                                      0045 00312000
         LA    @01,@AL00001                                        0045 00313000
         BAL   @14,RMIPGM                                          0045 00314000
*/*    PERFORMANCE OBJECTIVE DESCRIPTOR TABLE - WPODT                */ 00315000
*                                                                  0046 00316000
*   WMSTPODS=TBLSIZE;               /* INITIAL SIZE OF PERF OBJ    0046 00317000
*                                      DESCRIPTOR TABLE              */ 00318000
         L     @15,WMSTPTR                                         0046 00319000
         MVC   WMSTPODS(4,@15),@CF01245                            0046 00320000
*   CALL RMIPGM(WMSTPODT,WMSTPODS,WPODPTR);/* GETMAIN FOR WPOD       */ 00321000
         LA    @14,WMSTPODT(,@15)                                  0047 00322000
         ST    @14,@AL00001                                        0047 00323000
         LA    @15,WMSTPODS(,@15)                                  0047 00324000
         ST    @15,@AL00001+4                                      0047 00325000
         LA    @15,WPODPTR                                         0047 00326000
         ST    @15,@AL00001+8                                      0047 00327000
         LA    @01,@AL00001                                        0047 00328000
         BAL   @14,RMIPGM                                          0047 00329000
*/*  DOMAIN DESCRIPTION TABLE - DMDT                         @Z40BPEM*/ 00330000
*                                                                  0048 00331000
*   WMSTDMDS=TBLSIZE;               /* INIT SIZE OF DMDT     @Z40BPEM*/ 00332000
         L     @15,WMSTPTR                                         0048 00333000
         MVC   WMSTDMDS(4,@15),@CF01245                            0048 00334000
*   CALL RMIPGM(WMSTDMDT,WMSTDMDS,DMDTPTR);/* GETMAIN FOR DMDT     0049 00335000
*                                                            @Z40BPEM*/ 00336000
         LA    @14,WMSTDMDT(,@15)                                  0049 00337000
         ST    @14,@AL00001                                        0049 00338000
         LA    @15,WMSTDMDS(,@15)                                  0049 00339000
         ST    @15,@AL00001+4                                      0049 00340000
         LA    @15,DMDTPTR                                         0049 00341000
         ST    @15,@AL00001+8                                      0049 00342000
         LA    @01,@AL00001                                        0049 00343000
         BAL   @14,RMIPGM                                          0049 00344000
*/*  INITIALIZE WORKLOAD MANAGER SPECIFICATION TABLE (WMST)          */ 00345000
*                                                                  0050 00346000
*   WMSTNAME=WMSTCHAR;              /* TABLE NAME                    */ 00347000
         L     @15,WMSTPTR                                         0050 00348000
         MVC   WMSTNAME(4,@15),@CC01275                            0050 00349000
*   WMSTID=IPIPSID;                 /* IPS LIST ID                   */ 00350000
         L     @14,@PC00001                                        0051 00351000
         MVC   WMSTID(2,@15),IPIPSID(@14)                          0051 00352000
*   WMSTCPU=DEFCPUSC;               /* DEFAULT CPU SERVICE COEF      */ 00353000
         LA    @14,10                                              0052 00354000
         ST    @14,WMSTCPU(,@15)                                   0052 00355000
*   WMSTIOC=DEFIOCSC;               /* DEFAULT IOC SERVICE COEF      */ 00356000
         ST    @14,WMSTIOC(,@15)                                   0053 00357000
*   WMSTMSO=DEFMSOSC;               /* DEFAULT MSO SERVICE COEF      */ 00358000
         ST    @14,WMSTMSO(,@15)                                   0054 00359000
*/*    SET DEFAULTS IN PRINTABLE FORM FOR COEFFICIENTS       @Z40BPEM*/ 00360000
*                                                                  0055 00361000
*   DO LOOPC=ONE TO NSRVCOEF;       /* ALL COEFF HAVE A      @Z40BPEM*/ 00362000
         LA    LOOPC,1                                             0055 00363000
@DL00055 DS    0H                                                  0056 00364000
*     REALIPSV(LOOPC)=DEFSCOEF;     /* DEFAULT OF '1.0'      @Z40BPEM*/ 00365000
         LR    @03,LOOPC                                           0056 00366000
         SLA   @03,2                                               0056 00367000
         L     @01,WMSTPTR                                         0056 00368000
         ALR   @01,@03                                             0056 00369000
         MVC   REALIPSV-4(4,@01),DEFSCOEF                          0056 00370000
*   END;                            /*                       @Z40BPEM*/ 00371000
         AL    LOOPC,@CF00077                                      0057 00372000
         C     LOOPC,@CF00082                                      0057 00373000
         BNH   @DL00055                                            0057 00374000
*/*  INITIALIZE COUNTERS AND POINTERS                                */ 00375000
*                                                                  0058 00376000
*   WPGVPTR=WPGVPTR+LENGTH(WPGVTENT);/* RESERVE 1 ENTRY              */ 00377000
         LA    @15,2                                               0058 00378000
         L     @14,WPGVPTR                                         0058 00379000
         ALR   @14,@15                                             0058 00380000
         ST    @14,WPGVPTR                                         0058 00381000
*   WPOVPTR=WPOVPTR+LENGTH(WPOVTENT);/* RESERVE 1 ENTRY              */ 00382000
         L     @14,WPOVPTR                                         0059 00383000
         ALR   @14,@15                                             0059 00384000
         ST    @14,WPOVPTR                                         0059 00385000
*   DMVTPTR=DMVTPTR+LENGTH(DMVTENT);/* RESERVE 1 ENTRY       @Z40BPEM*/ 00386000
         AL    @15,DMVTPTR                                         0060 00387000
         ST    @15,DMVTPTR                                         0060 00388000
*/*  INITIALIZE DESCRIPTION FOR DOMAIN 0                     @Z40BPEM*/ 00389000
*                                                                  0061 00390000
*   DMDT=DMD0;                      /* SET DEF DMN 0         @Z40BPEM*/ 00391000
         L     @15,DMDTPTR                                         0061 00392000
         MVC   DMDT(20,@15),DMD0                                   0061 00393000
*/* INITIALIZE DESCRIPTION FOR DOMAIN 1                      @Z40BPEM*/ 00394000
*                                                                  0062 00395000
*   DMDTPTR=DMDTPTR+LENGTH(DMDT);   /* PTR TO DMN1 ENTRY     @Z40BPEM*/ 00396000
         AL    @15,@CF01251                                        0062 00397000
         ST    @15,DMDTPTR                                         0062 00398000
*   DMN1PTR=DMDTPTR;                /* SAVE PTR DMN 1        @Z40BPEM*/ 00399000
         ST    @15,DMN1PTR                                         0063 00400000
*   DMDT=DMD1;                      /* SET DEF DMN 1         @Z40BPEM*/ 00401000
         MVC   DMDT(20,@15),DMD1                                   0064 00402000
*/*  INITIALIZE DESCRIPTION FOR OBJECTIVE 0                  @Z40BPEM*/ 00403000
*                                                                  0065 00404000
