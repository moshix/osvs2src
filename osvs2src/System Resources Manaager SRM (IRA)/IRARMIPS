         L     @14,@SA00015                                        1009 04902000
         LM    @00,@12,@SA00015+8                                  1009 04903000
         BR    @14                                                 1009 04904000
*   END MFRBLANK;                                                  1010 04905000
*                                                                  1011 04906000
*/*  PROCEDURE = MFRRERR                                             */ 04907000
*/*                                                                  */ 04908000
*/*  DESCRIPTIVE NAME = ERROR RECOGNIZER                             */ 04909000
*/*                                                                  */ 04910000
*/*  FUNCTION = SKIPS TEXT UP TO BUT NOT INCLUDING A COMMA, COMMENT, */ 04911000
*/*             BLANK OR RIGHT PARENTHESIS.                          */ 04912000
*/*                                                                  */ 04913000
*                                                                  1011 04914000
*MFRRERR:                                                          1011 04915000
*   PROCEDURE(RRPARM,RRSCB);                                       1011 04916000
MFRRERR  STM   @14,@12,@SA00016                                    1011 04917000
         MVC   @PC00016(8),0(@01)                                  1011 04918000
*   DECLARE                                                        1012 04919000
*     RRPARM                        /* PTR TO A PARAMETER LIST       */ 04920000
*         PTR(31),                                                 1012 04921000
*                                                                  1012 04922000
*/*  PARAMETERS FOR ERROR RECOGNIZER                                 */ 04923000
*                                                                  1012 04924000
*     1 RRPARMLS                    /* PARAMETER LIST                */ 04925000
*           BASED(RRPARM),                                         1012 04926000
*      2 RRTYPE                     /* TYPE OF ERROR RECOGNIZER      */ 04927000
*            BIT(1),                                               1012 04928000
*      2 *                          /* NOT USED                      */ 04929000
*            BIT(7),                                               1012 04930000
*      2 RRLENGTH                   /* LENGTH OF ERROR TEXT TO BE  1012 04931000
*                                      SAVED FOR FUTURE REFERENCE    */ 04932000
*            FIXED(8),                                             1012 04933000
*      2 RRSAVETX                   /* CODE FOR WHERE TO SAVE        */ 04934000
*            FIXED(8),                                             1012 04935000
*                                                                  1012 04936000
*/*  SCAN CONTROL BLOCK                                              */ 04937000
*                                                                  1012 04938000
*     1 RRSCB,                      /* SCB PASSED FROM ANALYZER      */ 04939000
*      2 RRCURBCB                   /* CURRENT BCB PTR               */ 04940000
*            PTR(31),                                              1012 04941000
*      2 RRTXTCUR                   /* CURRENT TEXT PTR              */ 04942000
*            PTR(31),                                              1012 04943000
*      2 RRBYTLFT                   /* NO. BYTES LEFT IN BUFFER      */ 04944000
*            FIXED(31),                                            1012 04945000
*     RRTXTPTR                      /* PTR TO TEXT IN BUFFER         */ 04946000
*         PTR(31),                                                 1012 04947000
*     RRCHAR                        /* CHARS. IN BUFFER              */ 04948000
*         CHAR(2) BASED(RRTXTPTR),                                 1012 04949000
*     RRERRTXT                      /* BEGINNING OF ERROR TEXT       */ 04950000
*         CHAR(*) BASED(INERRPTR(RRSAVETX)),                       1012 04951000
*     RRFINISH                      /* FINISHED LOOPING INDICATOR    */ 04952000
*         BIT(1),                                                  1012 04953000
*     RRFOUND                       /* ERROR TEXT FOUND              */ 04954000
*         BIT(1),                                                  1012 04955000
*     RRERRSCN                      /* # CHARACTERS SCANNED          */ 04956000
*         FIXED(31),                                               1012 04957000
*     RRCOMBEG                      /* COMMENT DELIMITER             */ 04958000
*         CHAR(2) STATIC LOCAL GEND;                               1012 04959000
*   GENERATE DATA DEFS(RRCOMBEG);                                  1013 04960000
*   RRFOUND='0'B;                   /* INITIALIZE FOUND BIT          */ 04961000
         NI    RRFOUND,B'01111111'                                 1014 04962000
*   RRFINISH='0'B;                  /* INITIALIZE FINISH BIT         */ 04963000
         NI    RRFINISH,B'01111111'                                1015 04964000
*   RRERRSCN=ONE;                   /* INITIALIZE TEXT SCAN          */ 04965000
         LA    RRERRSCN,1                                          1016 04966000
*/*  CHECK IF IT IS NECESSARY TO READ IN NEXT RECORD                 */ 04967000
*                                                                  1017 04968000
*   IF RRBYTLFT=ZERO THEN           /* AT END OF BUFFER?             */ 04969000
