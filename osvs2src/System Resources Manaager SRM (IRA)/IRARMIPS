         AL    @15,RBBYTLFT(,@03)                                  1003 04877000
         ST    @15,RBBYTLFT(,@03)                                  1003 04878000
*     RBCURTXT=RBTXTPTR;            /* UPDATE CURRENT TEXT PTR       */ 04879000
         ST    RBTXTPTR,RBCURTXT(,@03)                             1004 04880000
*RBENDLOP:                                                         1005 04881000
*     ;                             /* CONTINUE SCAN FOR MORE BLANKS    04882000
*                                      OR COMMENTS                   */ 04883000
RBENDLOP DS    0H                                                  1006 04884000
*   END;                                                           1006 04885000
@DE00954 TM    RBFINISH,B'10000000'                                1006 04886000
         BZ    @DL00954                                            1006 04887000
*   IF RBFOUND='0'B&RBTYPBIT='1'B THEN/* NO BLANKS/COMMENTS FOUND?   */ 04888000
