*     DO;                                                          1018 04974000
*       CALL MFNXTREC(RRSCB);       /* GET NEXT RECORD               */ 04975000
         ST    @15,@AL00001                                        1019 04976000
         LA    @01,@AL00001                                        1019 04977000
         BAL   @14,MFNXTREC                                        1019 04978000
*       RFY                                                        1020 04979000
*        (GPR15F) RSTD;                                            1020 04980000
*       IF GPR15F=FAIL THEN         /* NO MORE RECORDS               */ 04981000
         C     GPR15F,@CF00054                                     1021 04982000
         BNE   @RF01021                                            1021 04983000
*         DO;                                                      1022 04984000
*           RRFINISH='1'B;          /* INDICATE FINISH               */ 04985000
         OI    RRFINISH,B'10000000'                                1023 04986000
*           GO TO RRBEGLOP;         /* BEGIN SCAN FOR ERROR          */ 04987000
         B     RRBEGLOP                                            1024 04988000
*         END;                                                     1025 04989000
*       RFY                                                        1026 04990000
*        (GPR15F) UNRSTD;                                          1026 04991000
@RF01021 DS    0H                                                  1027 04992000
*     END;                                                         1027 04993000
*                                                                  1027 04994000
*/*  SAVE COLUMN NO. AT WHICH SCAN BEGINS                            */ 04995000
*/*  SAVE INPUT STRING                                               */ 04996000
*                                                                  1028 04997000
*   IPERRCOL=IPDATALN-RRBYTLFT;     /* COLUMN NO.                    */ 04998000
@RF01017 L     @15,@PC00016+4                                      1028 04999000
         LA    @14,72                                              1028 05000000
         SL    @14,RRBYTLFT(,@15)                                  1028 05001000
         ST    @14,IPERRCOL                                        1028 05002000
*   IPMSGINP=RRCURBCB->INBCBTXT->IPINPUTS(2:81);                   1029 05003000
         L     @01,RRCURBCB(,@15)                                  1029 05004000
         L     @01,INBCBTXT(,@01)                                  1029 05005000
         MVC   IPMSGINP(80),IPINPUTS+1(@01)                        1029 05006000
*RRBEGLOP:                                                         1030 05007000
*   ;                               /* BEGIN SCAN FOR ERROR INPUT    */ 05008000
RRBEGLOP DS    0H                                                  1031 05009000
*   DO WHILE RRFINISH='0'B;                                        1031 05010000
