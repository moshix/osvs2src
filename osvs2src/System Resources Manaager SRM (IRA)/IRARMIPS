*                                                                  0704 03393000
*/********************************************************************/ 03394000
*/*                                                                  */ 03395000
*/*  PROCEDURE NAME: RMIPBPGL                                        */ 03396000
*/*                                                                  */ 03397000
*/*  FUNCTION:                                                       */ 03398000
*/*  THIS ROUTINE CONSTRUCTS IN WORK AREA A 1-WORD ENTRY WHICH       */ 03399000
*/*  CONTAINS THE INTERVAL SERVICE VALUE OR THE PERIOD DURATION ,    */ 03400000
*/*  AND RETURNS THE INDEX OF THIS ENTRY IN THE RETURN PARAMETER.    */ 03401000
*/*                                                                  */ 03402000
*/********************************************************************/ 03403000
*                                                                  0704 03404000
*RMIPBPGL:                                                         0704 03405000
*   PROC(IPBPGLP);                                                 0704 03406000
RMIPBPGL STM   @14,@12,12(@13)                                     0704 03407000
         MVC   @PC00007(4),0(@01)                                  0704 03408000
*   DECLARE                                                        0705 03409000
*     IPBPGLP PTR(08)               /* INDEX TO A LIST OF PERF GRP 0705 03410000
*                                      DURATION ENTRIES              */ 03411000
*         ;                                                        0705 03412000
*   IF IPWPGPC<=MAXPGPC THEN        /* NOT EXCEEDING THE MAX NO OF 0706 03413000
*                                      PERF GRP PERIODS ALLOWED      */ 03414000
         LH    @00,IPWPGPC                                         0706 03415000
         C     @00,@CF00046                                        0706 03416000
         BH    @RF00706                                            0706 03417000
*     DO;                                                          0707 03418000
*       IPWPGLCT=IPWPGLCT+ONE;      /* UPDATE INDEX TO WPGL          */ 03419000
         LA    @00,1                                               0708 03420000
         AH    @00,IPWPGLCT                                        0708 03421000
         STH   @00,IPWPGLCT                                        0708 03422000
*       IPWPGL(IPWPGLCT)=IPWINT1;   /* SAVE ENTRY IN WORK AREA       */ 03423000
         LR    @15,@00                                             0709 03424000
         SLA   @15,2                                               0709 03425000
         L     @14,IPWINT1                                         0709 03426000
         ST    @14,IPWPGL-4(@15)                                   0709 03427000
*       IPBPGLP=IPWPGLCT;           /* INDEX TO WPGL                 */ 03428000
         L     @15,@PC00007                                        0710 03429000
         STC   @00,IPBPGLP(,@15)                                   0710 03430000
*     END;                                                         0711 03431000
*   END RMIPBPGL;                                                  0712 03432000
*                                                                  0712 03433000
@EL00007 DS    0H                                                  0712 03434000
@EF00007 DS    0H                                                  0712 03435000
@ER00007 LM    @14,@12,12(@13)                                     0712 03436000
         BR    @14                                                 0712 03437000
*/********************************************************************/ 03438000
*/*                                                                  */ 03439000
*/*  PROCEDURE NAME: RMIPMPGL                                        */ 03440000
*/*                                                                  */ 03441000
*/*  FUNCTION:                                                       */ 03442000
*/*  THIS ROUTINE CONSTRUCTS A WPGL ENTRY IN THE PERF GRP DESCRIPTOR */ 03443000
*/*  IF AN INDENTICAL ENTRY HAS NOT BEEN CONSTRUCTED.  IT UPDATES    */ 03444000
*/*  THE INPUT PARAMETER FROM THE INDEX TO IPWPGL TO THE OFFSET OF   */ 03445000
*/*  THE WPGL ENTRY FROM THE BEGINNING OF THE WPGD.                  */ 03446000
*/*                                                                  */ 03447000
*/********************************************************************/ 03448000
*                                                                  0713 03449000
*RMIPMPGL:                                                         0713 03450000
*   PROC(IPMPGLP);                                                 0713 03451000
RMIPMPGL STM   @14,@02,12(@13)                                     0713 03452000
         STM   @04,@12,36(@13)                                     0713 03453000
         MVC   @PC00008(4),0(@01)                                  0713 03454000
*   DECLARE                                                        0714 03455000
*     IPMPGEND FIXED,               /* NO OF ENTRIES TO BE MATCHED   */ 03456000
*     IPMPGLP PTR(08)               /* OFFSET OF PERF GROUP DURATION    03457000
*                                      ENTRY                         */ 03458000
*         ;                                                        0714 03459000
*   IPMPGEND=IPMPGLP;               /* NO OF ENTRIES TO BE MATCHED   */ 03460000
         L     @02,@PC00008                                        0715 03461000
         SLR   @00,@00                                             0715 03462000
         IC    @00,IPMPGLP(,@02)                                   0715 03463000
         ST    @00,IPMPGEND                                        0715 03464000
*   DO IPWPGLI=ONE TO IPMPGEND;     /* FIND AN IDENTICAL ENTRY IN  0716 03465000
*                                      IPWPGL                        */ 03466000
         LA    IPWPGLI,1                                           0716 03467000
         B     @DE00716                                            0716 03468000
@DL00716 DS    0H                                                  0717 03469000
*     IF IPWPGL(IPWPGLI)=IPWPGL(IPMPGLP) THEN/* SAME DUR/ISV VALUE 0717 03470000
*                                      FOUND                         */ 03471000
         LR    @15,IPWPGLI                                         0717 03472000
         SLA   @15,2                                               0717 03473000
         L     @14,@PC00008                                        0717 03474000
         SLR   @07,@07                                             0717 03475000
         IC    @07,IPMPGLP(,@14)                                   0717 03476000
         SLA   @07,2                                               0717 03477000
         L     @14,IPWPGL-4(@07)                                   0717 03478000
         C     @14,IPWPGL-4(@15)                                   0717 03479000
         BNE   @RF00717                                            0717 03480000
*       DO;                                                        0718 03481000
*         IF IPWPGLP(IPWPGLI)=ZERO  /* IDENTICAL ENTRY HAS NOT BEEN     03482000
*                                      CONSTRUCTED IN THE WPGL       */ 03483000
*           THEN                    /* STORE DUR/ISV VALUE IN WPGL   */ 03484000
         SLR   @15,@15                                             0719 03485000
         IC    @15,IPWPGLP-1(IPWPGLI)                              0719 03486000
         C     @15,@CF00044                                        0719 03487000
         BNE   @RF00719                                            0719 03488000
*           DO;                                                    0720 03489000
*             IPWPGLP(IPWPGLI)=WPGLPTR-WPGDPTR;/* OFFSET OF WPGL     */ 03490000
         LR    @15,WPGLPTR                                         0721 03491000
         SL    @15,WPGDPTR                                         0721 03492000
         STC   @15,IPWPGLP-1(IPWPGLI)                              0721 03493000
*             WPGLISV=IPWPGL(IPMPGLP);/* BUILD WPGL ENTRY            */ 03494000
         ST    @14,WPGLISV(,WPGLPTR)                               0722 03495000
*             WPGLPTR=WPGLPTR+LENGTH(WPGL);/* POINT TO NEXT WPGL   0723 03496000
*                                      ENTRY                         */ 03497000
         AL    WPGLPTR,@CF00054                                    0723 03498000
*           END;                                                   0724 03499000
*         IPMPGLP=IPWPGLP(IPWPGLI); /* OFFSET OF WPGLL ENTRY         */ 03500000
@RF00719 L     @15,@PC00008                                        0725 03501000
         SLR   @14,@14                                             0725 03502000
         IC    @14,IPWPGLP-1(IPWPGLI)                              0725 03503000
         STC   @14,IPMPGLP(,@15)                                   0725 03504000
*         RETURN;                   /* RETURN TO CALLER              */ 03505000
@EL00008 DS    0H                                                  0726 03506000
@EF00008 DS    0H                                                  0726 03507000
@ER00008 LM    @14,@02,12(@13)                                     0726 03508000
         LM    @04,@12,36(@13)                                     0726 03509000
         BR    @14                                                 0726 03510000
*       END;                                                       0727 03511000
*   END;                                                           0728 03512000
@RF00717 AL    IPWPGLI,@CF00077                                    0728 03513000
@DE00716 C     IPWPGLI,IPMPGEND                                    0728 03514000
         BNH   @DL00716                                            0728 03515000
*   END RMIPMPGL;                                                  0729 03516000
         B     @EL00008                                            0729 03517000
*   END RMIPINIT;                                                  0730 03518000
         B     @EL00006                                            0730 03519000
*                                                                  0731 03520000
*/********************************************************************/ 03521000
*/*                                                                  */ 03522000
*/*  PROCEDURE NAME: RMIPMSGR                                        */ 03523000
*/*                                                                  */ 03524000
*/*  FUNCTION:                                                       */ 03525000
*/*                                                                  */ 03526000
*/*  THIS ROUTINE DETERMINES WHICH MESSAGE IS TO BE INSERTED INTO    */ 03527000
*/*  THE WTL PARAMETER LIST.  IT THEN INVOKES THE MESSAGE OUTPUT     */ 03528000
*/*  ROUTINE TO OUTPUT THE MESSAGE.                                  */ 03529000
*/*  INPUT - A 1-BYTE CODE INDICATING WHICH MESSAGE IS TO BE OUTPUTED*/ 03530000
*/*                                                                  */ 03531000
*/********************************************************************/ 03532000
*                                                                  0731 03533000
*RMIPMSGR:                                                         0731 03534000
*   PROC(IPMSGNUM);                                                0731 03535000
RMIPMSGR STM   @14,@12,@SA00009                                    0731 03536000
         MVC   @PC00009(4),0(@01)                                  0731 03537000
*   DCL                                                            0732 03538000
*     IPMSGNUM FIXED(8);            /* INPUT MESSAGE NUMBER          */ 03539000
*   IPMSGTXA=IPMSGTXT(IPMSGNUM);    /* INSERT MSG TEXT               */ 03540000
         MVI   IPMSGTXA+42,C' '                                    0733 03541000
         MVC   IPMSGTXA+43(17),IPMSGTXA+42                         0733 03542000
         L     @15,@PC00009                                        0733 03543000
         SLR   @14,@14                                             0733 03544000
         IC    @14,IPMSGNUM(,@15)                                  0733 03545000
         MH    @14,@CH01345                                        0733 03546000
         LA    @07,IPMSGTXT-42(@14)                                0733 03547000
         MVC   IPMSGTXA(42),0(@07)                                 0733 03548000
*   IF IPMSGNUM=ERRCINP THEN        /* INPUT PARM ERROR              */ 03549000
         CLC   IPMSGNUM(1,@15),ERRCINP                             0734 03550000
         BNE   @RF00734                                            0734 03551000
*     DO;                                                          0735 03552000
*       IPMSGLTA(1:8)=IPMSGID(1);   /* INSERT MSG ID                 */ 03553000
         MVC   IPMSGLTA(8),IPMSGID                                 0736 03554000
*       CALL RMIPTRT(IPERRCOL,TWO,ADDR(IPMSGCOL));/* CONVERT COL NO. */ 03555000
         LA    @00,IPERRCOL                                        0737 03556000
         ST    @00,@AL00001                                        0737 03557000
         LA    @00,@CF00118                                        0737 03558000
         ST    @00,@AL00001+4                                      0737 03559000
         LA    @00,IPMSGCOL                                        0737 03560000
