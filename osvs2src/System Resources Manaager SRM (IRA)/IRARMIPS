         LA    @00,@AFTEMPS+4                                      0748 03637000
         ST    @00,@AL00001+8                                      0748 03638000
         LA    @01,@AL00001                                        0748 03639000
         BAL   @14,RMIPTRT                                         0748 03640000
*       IF IPMSGNUM=ERRCSTS THEN    /* DOMAIN CNSTRS MSG     @Z40BPEM*/ 03641000
@RF00747 L     @15,@PC00009                                        0749 03642000
         CLC   IPMSGNUM(1,@15),ERRCSTS                             0749 03643000
         BNE   @RF00749                                            0749 03644000
*         CALL RMIPTRT(IPWDMN,THREE,ADDR(IPMSGCST));/* CONVERT AND 0750 03645000
