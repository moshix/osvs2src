*   RESPECIFY                                                      0831 04071000
*     R1 RSTD;                                                     0831 04072000
*   R0=INWKARSZ;                    /* SIZE OF IRBMFANL WORK AREA    */ 04073000
         LR    R0,INWKARSZ                                         0832 04074000
*   R1=INWKARPT;                    /* PTR TO WORK AREA              */ 04075000
         L     R1,INWKARPT                                         0833 04076000
*   GEN( FREEMAIN R,LV=(0),A=(1) ); /* FREE WORK AREA                */ 04077000
          FREEMAIN R,LV=(0),A=(1)                                       04078000
*   RESPECIFY                                                      0835 04079000
*    (R0,                                                          0835 04080000
*     R1,                                                          0835 04081000
*     R14,                                                         0835 04082000
*     R15) UNRSTD;                                                 0835 04083000
*                                                                  0835 04084000
*/*  IF I/O ERROR HAS OCCURRED DURING IEAOPTXX LIST PROCESSING,      */ 04085000
*/*  RETURN TO CALLER                                                */ 04086000
*                                                                  0836 04087000
*   IF OPIOERR='0'B THEN            /* I/O ERROR NOT FOUND           */ 04088000
         L     @15,@PC00001+20                                     0836 04089000
         TM    OPIOERR(@15),B'10000000'                            0836 04090000
         BNZ   @RF00836                                            0836 04091000
*     DO;                                                          0837 04092000
*                                                                  0837 04093000
*/*  FOR EACH CATEGORY OF DATA THAT HAS NOT BEEN FLAGGED AS ERROR.   */ 04094000
*/*  AND FOR EACH PARAMETER THAT HAS BEEN CORRECTLY SPECIFIED IN A   */ 04095000
*/*  CATEGORY, STORE THE PARAMETER VALUE IN RESOURCE MANAGER         */ 04096000
*/*  PARAMETER TABLE(RMPT)                                           */ 04097000
*                                                                  0838 04098000
*       DO I=ONE TO DIM(OPWCTGTB);  /* MAX NO OF CATEGORIES          */ 04099000
         LA    I,1                                                 0838 04100000
@DL00838 DS    0H                                                  0839 04101000
*         IF(OPCATGER&OPMASKTB(I))=ZERO&OPWCMASK(I)^=ZERO THEN/*   0839 04102000
*                                      CATEGORY(I) IS ERROR FREE AND    04103000
*                                      AT LEAST ONE PARAMETER HAS  0839 04104000
*                                      BEEN SPECIFIED FOR THIS     0839 04105000
*                                      CATEGORY                      */ 04106000
         SLR   @03,@03                                             0839 04107000
         L     @15,@PC00001+16                                     0839 04108000
         LR    @14,I                                               0839 04109000
         SLA   @14,2                                               0839 04110000
         L     @15,OPCATGER(,@15)                                  0839 04111000
         N     @15,OPMASKTB-4(@14)                                 0839 04112000
         CLR   @15,@03                                             0839 04113000
         BNE   @RF00839                                            0839 04114000
         LR    @15,I                                               0839 04115000
         MH    @15,@CH02027                                        0839 04116000
         L     @14,OPWCMASK-12(@15)                                0839 04117000
         CLR   @14,@03                                             0839 04118000
         BE    @RF00839                                            0839 04119000
*           DO;                                                    0840 04120000
*             OPWRMPPT=OPWCRMPP(I); /* ADDR OF ARRAY IN RMPT IN WHICH   04121000
*                                      PARAMETER VALUES ARE TO BE  0841 04122000
*                                      STORED                        */ 04123000
         L     @03,OPWCRMPP-12(@15)                                0841 04124000
         ST    @03,OPWRMPPT                                        0841 04125000
*             OPWARYPT=OPWCARYP(I); /* ADDR OF ARRAY IN WHICH THE  0842 04126000
*                                      PARM VALUES ARE STORED DURING    04127000
*                                      OPT LIST PROCESSING           */ 04128000
         L     @03,OPWCARYP-12(@15)                                0842 04129000
         ST    @03,OPWARYPT                                        0842 04130000
*             OPCPMASK=OPWCMASK(I); /* PARM INDICATOR                */ 04131000
         ST    @14,OPCPMASK                                        0843 04132000
*             OPCPARMN=OPCPARMT(I); /* NO OF PARMS IN CATG           */ 04133000
         SLR   @03,@03                                             0844 04134000
         IC    @03,OPCPARMT-1(I)                                   0844 04135000
         ST    @03,OPCPARMN                                        0844 04136000
*             DO J=ONE TO OPCPARMN; /* MAX NO OF PARAMETERS FOR    0845 04137000
*                                      CATEGORY(I)                   */ 04138000
         LA    J,1                                                 0845 04139000
         B     @DE00845                                            0845 04140000
@DL00845 DS    0H                                                  0846 04141000
*               IF(OPCPMASK&OPMASKTB(J))^=ZERO/* PARM HAS BEEN     0846 04142000
*                                      SPECIFIED                     */ 04143000
*                 THEN              /* SAVE PARM VALUE               */ 04144000
         LR    @15,J                                               0846 04145000
         SLA   @15,2                                               0846 04146000
