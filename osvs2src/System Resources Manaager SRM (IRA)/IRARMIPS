         TM    IPWPMAPG,B'00000010'                                0621 03065000
         BNZ   @RF00621                                            0621 03066000
*         DO;                                                      0622 03067000
*           IPWPMAPG='1'B;          /* INDICATE APG HAS BEEN       0623 03068000
*                                      SPECIFIED             @Z40BPEM*/ 03069000
         OI    IPWPMAPG,B'00000010'                                0623 03070000
*           WPGPAPG='1'B;           /* IND APG VALUE IS IN WPGP    0624 03071000
*                                                            @Z40BPEM*/ 03072000
         L     @14,WPGPPTR                                         0624 03073000
         OI    WPGPAPG(@14),B'00100000'                            0624 03074000
*           WPGPAPGV=IPWINT1;       /* SAVE VALUE IN WPGP    @Z40BPEM*/ 03075000
         STC   @15,WPGPAPGV(,@14)                                  0625 03076000
*         END;                      /*                       @Z40BPEM*/ 03077000
*       RETURN;                                                    0627 03078000
         B     @EL00006                                            0627 03079000
*     END;                          /*                       @Z40BPEM*/ 03080000
*   ELSE                                                           0629 03081000
*     GO TO RMIPIPE;                /* IPS IS INVALID        @Z40BPEM*/ 03082000
*RMIP027:                           /*                       @Z40BPEM*/ 03083000
*/*  COMPLETED PROCESSING A PERFORMANCE GROUP PERIOD SPECIFICATION   */ 03084000
*/*  UPDATE THE COUNT OF PERIODS FOR THIS GROUP,                     */ 03085000
*/*  UPDATE THE PTR TO THE NEXT AVAILABLE PERIOD ENTRY IF NOT MORE   */ 03086000
*/*  THAN THE ALLOWABLE NO. OF PERIODS HAVE BEEN SPECIFIED           */ 03087000
*                                                                  0630 03088000
*   IF IPWPGPC<=MAXPGPC             /* PERIOD NO. IS VALID           */ 03089000
*     THEN                          /* PERIOD SPECIFICATION IS OK    */ 03090000
RMIP027  LH    @15,IPWPGPC                                         0630 03091000
         C     @15,@CF00046                                        0630 03092000
         BH    @RF00630                                            0630 03093000
*     DO;                                                          0631 03094000
*       IPWPGPC=IPWPGPC+ONE;        /* INC PG PERIOD CNT             */ 03095000
         LA    @15,1(,@15)                                         0632 03096000
         STH   @15,IPWPGPC                                         0632 03097000
*       IF WPGPUNT=RUNIT            /* DURATION IS IN REAL TIME      */ 03098000
*         THEN                      /* CONVERT DUR FROM SECS TO    0633 03099000
*                                      MILLISECS                     */ 03100000
         L     @15,WPGPPTR                                         0633 03101000
         TM    WPGPUNT(@15),B'01000000'                            0633 03102000
         BNO   @RF00633                                            0633 03103000
*         DO;                                                      0634 03104000
*           IF IPWPGL(WPGPDURP)>MAXRDURC/* TEST IF DURATION>MAX NO 0635 03105000
*                                      ALLOWED FOR DURATION IN RELA     03106000
*                                      TIME                          */ 03107000
*             THEN                  /* DUR NO. IN REAL TIME IS TOO 0635 03108000
*                                      LARGE                         */ 03109000
         L     @14,@CF01655                                        0635 03110000
         SLR   @07,@07                                             0635 03111000
         IC    @07,WPGPDURP(,@15)                                  0635 03112000
         SLA   @07,2                                               0635 03113000
         C     @14,IPWPGL-4(@07)                                   0635 03114000
         BNL   @RF00635                                            0635 03115000
*             IPWPGL(WPGPDURP)=MAXRDURC;/* USE MAX DURATION TIME     */ 03116000
         ST    @14,IPWPGL-4(@07)                                   0636 03117000
*           IPWPGL(WPGPDURP)=IPWPGL(WPGPDURP)*MILLISEC;/* CONVERT TO    03118000
*                                      MILLISECONDS                  */ 03119000
@RF00635 L     @15,WPGPPTR                                         0637 03120000
         SLR   @14,@14                                             0637 03121000
         IC    @14,WPGPDURP(,@15)                                  0637 03122000
         SLA   @14,2                                               0637 03123000
         L     @15,IPWPGL-4(@14)                                   0637 03124000
         SLA   @15,10                                              0637 03125000
         ST    @15,IPWPGL-4(@14)                                   0637 03126000
*         END;                                                     0638 03127000
*       IF IPWPMOBJ='0'B            /* OBJ NOT SPECIFIED             */ 03128000
*         THEN                                                     0639 03129000
@RF00633 TM    IPWPMOBJ,B'10000000'                                0639 03130000
         BNZ   @RF00639                                            0639 03131000
*         DO;                       /* ERROR PROCESSING      @Z40BPEM*/ 03132000
*           CALL RMIPMSGR(ERRCOBJ); /* WRITE MISSING OBJ MSG @Z40BPEM*/ 03133000
         LA    @01,@AL00641                                        0641 03134000
         BAL   @14,RMIPMSGR                                        0641 03135000
*           IPWFIPER='1'B;          /* MARK IPS INVALID      @Z40BPEM*/ 03136000
         OI    IPWFIPER,B'10000000'                                0642 03137000
*         END;                      /*                       @Z40BPEM*/ 03138000
*       WPGPPTR=WPGPPTR+LENGTH(WPGP);/* POINT TO NEXT WPGP           */ 03139000
@RF00639 LA    @00,8                                               0644 03140000
         AL    @00,WPGPPTR                                         0644 03141000
         ST    @00,WPGPPTR                                         0644 03142000
*     END;                                                         0645 03143000
*   IPWPGPM=ZERO;                   /* RESET PERF GRP PER IND      0646 03144000
*                                                            @Z40BPEM*/ 03145000
@RF00630 MVI   IPWPGPM,X'00'                                       0646 03146000
*   WPGP=DEFWPGP;                   /* SET DEFAULT VALUES    @Z40BPEM*/ 03147000
         L     @15,WPGPPTR                                         0647 03148000
         MVC   WPGP(8,@15),DEFWPGP                                 0647 03149000
*   RETURN;                         /* RETURN TO CALLER              */ 03150000
         B     @EL00006                                            0648 03151000
*RMIP028:                           /*                       @Z40BPEM*/ 03152000
*/*  COMPLETED PROCESSING A PERFORMANCE GROUP SPECIFICATION          */ 03153000
*                                                                  0649 03154000
*   IPWPGPC=IPWPGPC-ONE;            /* ADJUST PG PERIOD CNT          */ 03155000
RMIP028  LH    @02,IPWPGPC                                         0649 03156000
         BCTR  @02,0                                               0649 03157000
         STH   @02,IPWPGPC                                         0649 03158000
*/*  IF PERIOD DURATION FOR A PERIOD, OTHER THAN THE LAST ONE, HAS   */ 03159000
*/*  NOT BEEN SPECIFIED CORRECTLY, THEN THE IPS IS INVALID           */ 03160000
*                                                                  0650 03161000
*   IPWLOOPC=IPWPGPC-ONE;           /* DUR FOR LAST PERIOD IS      0650 03162000
*                                      IMMATERIAL                    */ 03163000
         BCTR  @02,0                                               0650 03164000
         LR    IPWLOOPC,@02                                        0650 03165000
*   DO I=ONE TO IPWLOOPC;                                          0651 03166000
         LA    I,1                                                 0651 03167000
