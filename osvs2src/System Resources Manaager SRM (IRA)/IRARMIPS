*   INWKARPT=R1;                    /* PTR TO IRBMFANL WORK AREA     */ 00574000
         ST    R1,INWKARPT                                         0107 00575000
*   RESPECIFY                                                      0108 00576000
*     R1 UNRSTD;                                                   0108 00577000
*                                                                  0108 00578000
*/*  INVOKE SYNTAX ANALYZER TO ANALYZE THE IPS LIST                  */ 00579000
*                                                                  0109 00580000
*   CALL IRBMFANL(INDUMITM,         /* ITEM ENTRY                    */ 00581000
