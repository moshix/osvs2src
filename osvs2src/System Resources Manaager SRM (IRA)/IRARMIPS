         SLR   @14,@15                                             0676 03280000
         ST    @14,WPGPPTR                                         0676 03281000
*                 PREVPDMN=WPGPDMN; /* GET PREV. DMN #       @Z40BPEM*/ 03282000
         IC    @00,WPGPDMN(,@14)                                   0677 03283000
         STC   @00,PREVPDMN                                        0677 03284000
*                 WPGPPTR=WPGPPTR+LENGTH(WPGP);/* RESET PTR TO     0678 03285000
*                                      CURRENT PERIOD        @Z40BPEM*/ 03286000
         ALR   @14,@15                                             0678 03287000
