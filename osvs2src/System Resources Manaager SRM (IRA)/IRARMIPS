*   END MFRINTGR;                                                  0948 04671000
*                                                                  0949 04672000
*/*  PROCEDURE NAME = MFRBLANK                                       */ 04673000
*/*                                                                  */ 04674000
*/*  DESCRIPTIVE NAME = BLANK RECOGNIZER                             */ 04675000
*/*                                                                  */ 04676000
*/*  FUNCTION = RECOGNIZES BLANKS DEPENDING ON THE INPUT PARAMTER.   */ 04677000
*/*             THIS RECOGNIZER WILL ALWAYS RETURN SUCCESSFULLY (EVEN*/ 04678000
*/*             IF NO BLANKS OR COMMENTS WERE FOUND) OR IT WILL      */ 04679000
*/*             RETURN A SUCCESSFUL INDICATION ONLY IF AT LEAST ONE  */ 04680000
*/*             BLANK OR COMMENT IS FOUND. (DELIMITER TYPE).         */ 04681000
*/*                                                                  */ 04682000
*                                                                  0949 04683000
*MFRBLANK:                                                         0949 04684000
*   PROCEDURE(RBTYPE,RBSCB);                                       0949 04685000
MFRBLANK STM   @14,@12,@SA00015                                    0949 04686000
         MVC   @PC00015(8),0(@01)                                  0949 04687000
*   DECLARE                                                        0950 04688000
*     RBTYPE                        /* PTR TO TYPE OF BLANK        0950 04689000
*                                      RECOGNIZER TO BE EXECUTED     */ 04690000
*         PTR(31),                                                 0950 04691000
*     RBTYPBIT                      /* TYPE OF BLANK RECOGNIZER 0 -     04692000
*                                      GENERAL BLANK RECOGNIZER 1 -     04693000
*                                      DELIMITER TYPE BLANK        0950 04694000
*                                      RECOGNIZER                    */ 04695000
*         BASED(RBTYPE) BIT(1),                                    0950 04696000
*                                                                  0950 04697000
*/*  SCAN CONTROL BLOCK                                              */ 04698000
*                                                                  0950 04699000
*     1 RBSCB,                      /* SCAN CONTROL BLOCK            */ 04700000
*      2 RBCURBCB                   /* CURRENT BCB PTR               */ 04701000
*            PTR(31),                                              0950 04702000
*      2 RBCURTXT                   /* CURRENT TXT PTR               */ 04703000
*            PTR(31),                                              0950 04704000
*      2 RBBYTLFT                   /* # BYTES LEFT                  */ 04705000
*            FIXED(31),                                            0950 04706000
*     RBTXTPTR                      /* PTR TO TEXT IN BUFFER         */ 04707000
*         PTR(31),                                                 0950 04708000
*     RBCHAR                        /* CHARACTER POINTED TO BY     0950 04709000
*                                      RBTXTPTR                      */ 04710000
*         CHAR(*) BASED(RBTXTPTR),                                 0950 04711000
*     RBFINISH                      /* FINISH SCANNING FOR BLANKS OR    04712000
*                                      COMMENTS                      */ 04713000
*         BIT(1),                                                  0950 04714000
*     RBFOUND                       /* FOUND BLANKS OR COMMENTS      */ 04715000
*         BIT(1),                                                  0950 04716000
