         ST    @15,@AL00001+4                                      0739 03582000
         L     @15,@CV01277                                        0739 03583000
         LA    @01,@AL00001                                        0739 03584000
         BALR  @14,@15                                             0739 03585000
*     END;                                                         0740 03586000
*   ELSE                            /* NOT PARM ERROR                */ 03587000
*     DO;                                                          0741 03588000
         B     @RC00734                                            0741 03589000
@RF00734 DS    0H                                                  0742 03590000
*       IPMSGLTA(1:8)=IPMSGID(2);   /* INSERT MSG ID                 */ 03591000
         MVC   IPMSGLTA(8),IPMSGID+8                               0742 03592000
*       IF IPMSGNUM=ERRCRPG THEN    /* REQ PERF GRP MSG              */ 03593000
         L     @15,@PC00009                                        0743 03594000
         CLC   IPMSGNUM(1,@15),ERRCRPG                             0743 03595000
         BNE   @RF00743                                            0743 03596000
*         CALL RMIPTRT(IPWPGN,THREE,ADDR(IPMSGRPG));/* CONVERT &   0744 03597000
*                                      STORE REQ PERF GRP NO         */ 03598000
         LA    @00,IPWPGN                                          0744 03599000
         ST    @00,@AL00001                                        0744 03600000
         LA    @00,@CF00082                                        0744 03601000
         ST    @00,@AL00001+4                                      0744 03602000
         LA    @00,IPMSGRPG                                        0744 03603000
         ST    @00,@AFTEMPS+4                                      0744 03604000
         LA    @00,@AFTEMPS+4                                      0744 03605000
         ST    @00,@AL00001+8                                      0744 03606000
         LA    @01,@AL00001                                        0744 03607000
         BAL   @14,RMIPTRT                                         0744 03608000
*       IF IPMSGNUM=ERRCDUR THEN    /* PERF GRP DURATION MSG         */ 03609000
@RF00743 L     @15,@PC00009                                        0745 03610000
         CLC   IPMSGNUM(1,@15),ERRCDUR                             0745 03611000
         BNE   @RF00745                                            0745 03612000
*         CALL RMIPTRT(IPWPGN,THREE,ADDR(IPMSGDPG));/* CONVERT &   0746 03613000
*                                      STORE PERF GRP NO             */ 03614000
         LA    @00,IPWPGN                                          0746 03615000
         ST    @00,@AL00001                                        0746 03616000
         LA    @00,@CF00082                                        0746 03617000
         ST    @00,@AL00001+4                                      0746 03618000
         LA    @00,IPMSGDPG                                        0746 03619000
         ST    @00,@AFTEMPS+4                                      0746 03620000
         LA    @00,@AFTEMPS+4                                      0746 03621000
         ST    @00,@AL00001+8                                      0746 03622000
         LA    @01,@AL00001                                        0746 03623000
         BAL   @14,RMIPTRT                                         0746 03624000
*       IF IPMSGNUM=ERRCOBJ THEN    /* PERF GRP OBJ MSG              */ 03625000
@RF00745 L     @15,@PC00009                                        0747 03626000
         CLC   IPMSGNUM(1,@15),ERRCOBJ                             0747 03627000
         BNE   @RF00747                                            0747 03628000
*         CALL RMIPTRT(IPWPGN,THREE,ADDR(IPMSGOPG));/* CONVERT AND 0748 03629000
*                                      STORE PERF GRP NO     @Z40BPEM*/ 03630000
         LA    @00,IPWPGN                                          0748 03631000
         ST    @00,@AL00001                                        0748 03632000
         LA    @00,@CF00082                                        0748 03633000
