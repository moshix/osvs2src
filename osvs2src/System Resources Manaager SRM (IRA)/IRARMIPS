         ST    @15,@AL00001+4                                      0751 03662000
         L     @15,@CV01277                                        0751 03663000
         LA    @01,@AL00001                                        0751 03664000
         BALR  @14,@15                                             0751 03665000
*     END;                                                         0752 03666000
*   END RMIPMSGR;                                                  0753 03667000
@EL00009 DS    0H                                                  0753 03668000
@EF00009 DS    0H                                                  0753 03669000
@ER00009 LM    @14,@12,@SA00009                                    0753 03670000
         BR    @14                                                 0753 03671000
*                                                                  0754 03672000
*/********************************************************************/ 03673000
*/*                                                                  */ 03674000
*/*  PROCEDURE NAME: RMIPTRT                                         */ 03675000
*/*                                                                  */ 03676000
*/*  FUNCTION:                                                       */ 03677000
*/*  THIS ROUTINE CONVERTS A BINARY NO. TO DECIMAL IN EBCDIC FORM    */ 03678000
*/*  AND STORES THE RESULT IN THE OUTPUT AREA                        */ 03679000
*/*  INPUT - (1) NO. TO BE CONVERTED                                 */ 03680000
*/*          (2) NO. OF DIGITS TO BE STORED                          */ 03681000
*/*          (3) ADDR OF OUTPUT AREA                                 */ 03682000
*/*                                                                  */ 03683000
*/********************************************************************/ 03684000
*                                                                  0754 03685000
*RMIPTRT:                                                          0754 03686000
*   PROC(IPTRTNUM,IPTRTDGT,IPTRTADR);                              0754 03687000
RMIPTRT  STM   @14,@12,@SA00010                                    0754 03688000
         MVC   @PC00010(12),0(@01)                                 0754 03689000
*   DECLARE                                                        0755 03690000
*     SHIFTHW FIXED CONSTANT(16),   /* NO. OF BITS IN HALF WORD      */ 03691000
*     IPTRTNUM FIXED(31),           /* NO. TO BE CONVERTED           */ 03692000
*     IPTRTDGT FIXED(31),           /* NO. OF DIGITS TO BE STORED    */ 03693000
*     IPTRTADR PTR(31),             /* ADDR OF OUTPUT AREA           */ 03694000
*     IPTRTRST CHAR(8) BDY(DWORD),  /* TEMP LOC FOR STORING RESULT   */ 03695000
*     IPTRTIND FIXED,               /* WORK AREA                     */ 03696000
*     IPTRTUNP CHAR(6) BDY(HWORD) STATIC LOCAL GENERATED,/* UNPACK 0755 03697000
*                                      AREA                          */ 03698000
*     IPTRTBLK CHAR(1) BASED(EVAL(IPTRTADR+IPTRTIND)),/* LOC FOR   0755 03699000
*                                      BLANK                         */ 03700000
*     IPFZONE CHAR(1) INIT('0F'X),  /* ZONE FOR NUMERICS             */ 03701000
*     IPZERO CHAR(1) INIT('F0'X),   /* ZERO IN EBCDIC                */ 03702000
*     IPBLANK CHAR(1) INIT('40'X)   /* BLANK                         */ 03703000
*         ;                                                        0755 03704000
*   GEN DATA DEFS(IPTRTUNP);                                       0756 03705000
*   RESPECIFY                                                      0757 03706000
*    (R1,                                                          0757 03707000
*     R14,                                                         0757 03708000
*     R15) RSTD;                                                   0757 03709000
*   R1=IPTRTNUM;                    /* NO. TO BE CONVERTED           */ 03710000
         L     @07,@PC00010                                        0758 03711000
         L     R1,IPTRTNUM(,@07)                                   0758 03712000
*   R14=IPTRTDGT;                   /* NO. OF DIGITS                 */ 03713000
         L     @07,@PC00010+4                                      0759 03714000
         L     R14,IPTRTDGT(,@07)                                  0759 03715000
*   R14=(R14-ONE)*SHIFTHW;          /* SHIFT OUT LOW ORDER 16 BITS   */ 03716000
         LR    @00,R14                                             0760 03717000
         BCTR  @00,0                                               0760 03718000
         SLA   @00,4                                               0760 03719000
         LR    R14,@00                                             0760 03720000
*   R15=IPTRTADR;                   /* ADDR OF OUTPUT AREA           */ 03721000
         L     @07,@PC00010+8                                      0761 03722000
         L     R15,IPTRTADR(,@07)                                  0761 03723000
*   GEN REFS(R1,R14,IPTRTRST,IPTRTUNP,IPFZONE);                    0762 03724000
IPTRTSGN EQU   7                         SIGN POSITION                  03725000
         CVD   R1,IPTRTRST               CONVERT TO DECIMAL             03726000
         OC    IPTRTRST+IPTRTSGN(R1),IPFZONE MASK OUT SIGN              03727000
         EX    R14,IPTRTUNP              UNPACK                         03728000
*   RESPECIFY                                                      0763 03729000
*    (R1,                                                          0763 03730000
*     R14,                                                         0763 03731000
*     R15) UNRSTD;                                                 0763 03732000
*                                                                  0763 03733000
*/*  CONVERT LEADING ZEROES TO BLANKS                                */ 03734000
*                                                                  0764 03735000
*   DO IPTRTIND=0 TO(IPTRTDGT-TWO) WHILE(IPTRTBLK=IPZERO);         0764 03736000
         SLR   IPTRTIND,IPTRTIND                                   0764 03737000
         B     @DE00764                                            0764 03738000
@DL00764 L     @03,@PC00010+8                                      0764 03739000
         L     @03,IPTRTADR(,@03)                                  0764 03740000
         LR    @15,IPTRTIND                                        0764 03741000
         ALR   @15,@03                                             0764 03742000
         CLC   IPTRTBLK(1,@15),IPZERO                              0764 03743000
         BNE   @DC00764                                            0764 03744000
*     IPTRTBLK=IPBLANK;                                            0765 03745000
         ALR   @03,IPTRTIND                                        0765 03746000
         MVC   IPTRTBLK(1,@03),IPBLANK                             0765 03747000
*   END;                                                           0766 03748000
         AL    IPTRTIND,@CF00077                                   0766 03749000
@DE00764 L     @03,@PC00010+4                                      0766 03750000
         L     @03,IPTRTDGT(,@03)                                  0766 03751000
         BCTR  @03,0                                               0766 03752000
         BCTR  @03,0                                               0766 03753000
         CR    IPTRTIND,@03                                        0766 03754000
         BNH   @DL00764                                            0766 03755000
@DC00764 DS    0H                                                  0767 03756000
*   END RMIPTRT;                                                   0767 03757000
@EL00010 DS    0H                                                  0767 03758000
@EF00010 DS    0H                                                  0767 03759000
@ER00010 LM    @14,@12,@SA00010                                    0767 03760000
         BR    @14                                                 0767 03761000
*                                                                  0768 03762000
*/********************************************************************/ 03763000
*/*                                                                  */ 03764000
*/*  ENTRY POINT - IRARMFRE                                          */ 03765000
*/*                                                                  */ 03766000
*/*  FUNCTION - IT FREES IPS TABLES IN SQA                           */ 03767000
*/*                                                                  */ 03768000
*/********************************************************************/ 03769000
*                                                                  0768 03770000
*IRARMFRE:                                                         0768 03771000
*   ENTRY(FRIPSPTR);                                               0768 03772000
@EP00768 MVC   @PC00001+12(4),0(@01)                               0768 03773000
*   CALL RMFREIPS(FRIPSPTR);        /* FREE IPS TABLES               */ 03774000
         L     @15,@PC00001+12                                     0769 03775000
         ST    @15,@AL00001                                        0769 03776000
         LA    @01,@AL00001                                        0769 03777000
         BAL   @14,RMFREIPS                                        0769 03778000
*   RETURN;                         /* RETURN TO CALLER              */ 03779000
@EL00001 L     @13,4(,@13)                                         0770 03780000
@EF00001 L     @00,@SIZDATD                                        0770 03781000
         LR    @01,@08                                             0770 03782000
         FREEMAIN R,LV=(0),A=(1)                                        03783000
@ER00001 LM    @14,@12,12(@13)                                     0770 03784000
         BR    @14                                                 0770 03785000
*                                                                  0771 03786000
*/********************************************************************/ 03787000
*/*                                                                  */ 03788000
*/*  PROCEDURE NAME: RMFREIPS                                        */ 03789000
*/*                                                                  */ 03790000
*/*  FUNCTION:                                                       */ 03791000
*/*  THE CONTROL BLOCKS WHICH CONTAIN IPS DATA ARE FREED AND STORAGE */ 03792000
*/*  IS RETURNED TO SUBPOOL 245                                      */ 03793000
*/*  INPUT - ADDR OF IPS TO BE FREED                                 */ 03794000
*/*                                                                  */ 03795000
*/********************************************************************/ 03796000
*                                                                  0771 03797000
*RMFREIPS:                                                         0771 03798000
*   PROC(FRWMSTPT);                 /* FREE IPS SUBROUTINE           */ 03799000
RMFREIPS STM   @14,@12,@SA00011                                    0771 03800000
         MVC   @PC00011(4),0(@01)                                  0771 03801000
*   DECLARE                                                        0772 03802000
*     MAXIPSBK FIXED CONSTANT(6),   /* NO. OF IPS CONTROL BLOCKS   0772 03803000
*                                                            @Z40BPEM*/ 03804000
*     FRWMSTPT PTR(31),             /* PTR TO WMST TABLE             */ 03805000
*     FRTBLCNT FIXED,               /* CNT OF IPS TABLES             */ 03806000
*     SUBPN245 FIXED(32) INIT('F5000000'X),/* SUBPOOL NO=245         */ 03807000
*     BLKHEADP PTR,                 /* ADDR OF CONTROL BLK HEADER    */ 03808000
*/*  IPS CONTROL BLOCK HEADER                                        */ 03809000
*                                                                  0772 03810000
*     1 BLKHEAD BASED(BLKHEADP),    /* CONTROL BLOCK HEADER          */ 03811000
*      2 BLKPTR PTR(31),            /* ADDR OF CONTROL BLOCK         */ 03812000
*      2 BLKSZE FIXED(31)           /* SIZE OF CONTROL BLOCK         */ 03813000
*            ;                                                     0772 03814000
*   RESPECIFY                                                      0773 03815000
*     WMST BASED(WMSTPTR);                                         0773 03816000
*   RESPECIFY                                                      0774 03817000
*    (GPR00P,                                                      0774 03818000
*     GPR01P,                                                      0774 03819000
*     GPR14P,                                                      0774 03820000
*     GPR15P) RSTD;                                                0774 03821000
*                                                                  0774 03822000
*/* VALIDITY CHECK THE INPUT PARAMETER                               */ 03823000
*                                                                  0775 03824000
*   IF FRWMSTPT=ZERO THEN           /* NO NEED TO RELEASE STORAGE    */ 03825000
         L     @07,@PC00011                                        0775 03826000
         L     @07,FRWMSTPT(,@07)                                  0775 03827000
         LTR   @07,@07                                             0775 03828000
         BZ    @RT00775                                            0775 03829000
*     RETURN;                       /* RETURN TO CALLER              */ 03830000
*/*  FREE THE CONTROL BLOCKS WHICH CONTAIN INSTALLATION PERFORMANCE  */ 03831000
*/*  SPECIFICATION (IPS)                                             */ 03832000
*                                                                  0777 03833000
*   WMSTPTR=FRWMSTPT;                                              0777 03834000
         ST    @07,WMSTPTR                                         0777 03835000
*   BLKHEADP=ADDR(WMSTPGVT);        /* ADDR OF 1ST CONTROL BLOCK   0778 03836000
*                                      HEADER                        */ 03837000
         LA    BLKHEADP,WMSTPGVT(,@07)                             0778 03838000
*   DO FRTBLCNT=ONE TO MAXIPSBK;    /* NO OF CONTROL BLOCKS          */ 03839000
         LA    FRTBLCNT,1                                          0779 03840000
