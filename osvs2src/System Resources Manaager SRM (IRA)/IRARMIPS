         LTR   @14,@14                                             0671 03259000
         BNZ   @RF00671                                            0671 03260000
*           DO;                     /* SET DMN VALUES        @Z40BPEM*/ 03261000
*             IF I=ONE              /* DMN MISSING IN FIRST PERIOD 0673 03262000
*                                                            @Z40BPEM*/ 03263000
*               THEN                /* INC # 1ST PERIODS             */ 03264000
         LA    @15,1                                               0673 03265000
         CR    I,@15                                               0673 03266000
         BNE   @RF00673                                            0673 03267000
*               PGP1DMN=PGP1DMN+1;  /* WITH DMN MISSING      @Z40BPEM*/ 03268000
         AH    @15,PGP1DMN                                         0674 03269000
         STH   @15,PGP1DMN                                         0674 03270000
*             ELSE                  /* NOT FIRST PER.        @Z40BPEM*/ 03271000
