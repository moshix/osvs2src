*                                                            @Z40BPEM*/ 03379000
RMIP029  DS    0H                                                  0700 03380000
*RMIPIPE:                                                          0700 03381000
*   ;                               /* IPS ERROR PROCESSING    SUBRTN*/ 03382000
RMIPIPE  DS    0H                                                  0701 03383000
*/*  ERROR IN IPS SPECIFICATION, MARK IPS INVALID                    */ 03384000
*                                                                  0701 03385000
*   IPWFIPER='1'B;                                                 0701 03386000
         OI    IPWFIPER,B'10000000'                                0701 03387000
*   CALL RMIPMSGR(ERRCINP);         /* WRITE INPUT ERROR MSG         */ 03388000
         LA    @01,@AL00702                                        0702 03389000
         BAL   @14,RMIPMSGR                                        0702 03390000
*   RETURN;                         /* RETURN TO CALLER              */ 03391000
