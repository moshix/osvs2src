*     IF RRBYTLFT=ZERO THEN         /* AT END OF BUFFER?             */ 05013000
         L     @15,@PC00016+4                                      1032 05014000
         L     @14,RRBYTLFT(,@15)                                  1032 05015000
         LTR   @14,@14                                             1032 05016000
         BNZ   @RF01032                                            1032 05017000
*       DO;                                                        1033 05018000
*         CALL MFNXTREC(RRSCB);     /* GET NEXT RECORD               */ 05019000
         ST    @15,@AL00001                                        1034 05020000
         LA    @01,@AL00001                                        1034 05021000
         BAL   @14,MFNXTREC                                        1034 05022000
*         RFY                                                      1035 05023000
*          (GPR15F) RSTD;                                          1035 05024000
*         IF GPR15F=FAIL THEN       /* NO MORE RECORDS?              */ 05025000
         C     GPR15F,@CF00054                                     1036 05026000
         BNE   @RF01036                                            1036 05027000
*           DO;                                                    1037 05028000
*             RRFINISH='1'B;        /* INDICATE LOOP FINISHED        */ 05029000
         OI    RRFINISH,B'10000000'                                1038 05030000
*             GOTO RRENDLOP;        /* END THIS LOOP                 */ 05031000
         B     RRENDLOP                                            1039 05032000
*           END;                                                   1040 05033000
*         RFY                                                      1041 05034000
*          (GPR15F) UNRSTD;                                        1041 05035000
@RF01036 DS    0H                                                  1042 05036000
*       END;                                                       1042 05037000
*     IF RRTYPE='0'B THEN           /* SKIP UP TO COMMA, BLANK OR  1043 05038000
*                                      COMMENT                       */ 05039000
@RF01032 L     @15,@PC00016                                        1043 05040000
         L     @01,RRPARM(,@15)                                    1043 05041000
         TM    RRTYPE(@01),B'10000000'                             1043 05042000
         BNZ   @RF01043                                            1043 05043000
*       DO RRTXTPTR=RRTXTCUR TO RRTXTCUR+RRBYTLFT-ONE;             1044 05044000
         L     @15,@PC00016+4                                      1044 05045000
         L     RRTXTPTR,RRTXTCUR(,@15)                             1044 05046000
         B     @DE01044                                            1044 05047000
@DL01044 DS    0H                                                  1045 05048000
*         IF RRCHAR(1:1)=','|RRCHAR(1:1)=' ' THEN/* COMMA OR BLANK?  */ 05049000
         CLI   RRCHAR(RRTXTPTR),C','                               1045 05050000
         BE    @RT01045                                            1045 05051000
         CLI   RRCHAR(RRTXTPTR),C' '                               1045 05052000
         BE    @RT01045                                            1045 05053000
*           GOTO RRSCNEND;          /* END THIS LOOP                 */ 05054000
*         IF RRTXTPTR=RRTXTCUR+RRBYTLFT-ONE THEN/* REACH END OF    1047 05055000
*                                      BUFFER ?                      */ 05056000
         L     @03,@PC00016+4                                      1047 05057000
         L     @15,RRTXTCUR(,@03)                                  1047 05058000
         AL    @15,RRBYTLFT(,@03)                                  1047 05059000
         BCTR  @15,0                                               1047 05060000
         CR    RRTXTPTR,@15                                        1047 05061000
         BE    @RT01047                                            1047 05062000
*           GOTO RRUPDATE;          /* MOVE IN ERROR TEXT            */ 05063000
*         IF RRCHAR(1:2)=RRCOMBEG THEN/* BEGINNING OF A COMMENT?     */ 05064000
         CLC   RRCHAR(2,RRTXTPTR),RRCOMBEG                         1049 05065000
         BE    @RT01049                                            1049 05066000
*           GOTO RRSCNEND;          /* UPDATE BCB & SCB              */ 05067000
*RRUPDATE:                                                         1051 05068000
*         RRFOUND='1'B;             /* INDICATE END OF BUFFER        */ 05069000
RRUPDATE OI    RRFOUND,B'10000000'                                 1051 05070000
*       END;                                                       1052 05071000
         AL    RRTXTPTR,@CF00077                                   1052 05072000
@DE01044 L     @03,@PC00016+4                                      1052 05073000
         L     @15,@PC00016+4                                      1052 05074000
         L     @03,RRTXTCUR(,@03)                                  1052 05075000
         AL    @03,RRBYTLFT(,@15)                                  1052 05076000
         BCTR  @03,0                                               1052 05077000
         CR    RRTXTPTR,@03                                        1052 05078000
         BNH   @DL01044                                            1052 05079000
*     ELSE                          /* SKIP UP TO COMMA,BLANK,COMMENT   05080000
*                                      OR RIGHT PAREN                */ 05081000
*       DO RRTXTPTR=RRTXTCUR TO RRTXTCUR+RRBYTLFT-ONE;             1053 05082000
         B     @RC01043                                            1053 05083000
@RF01043 L     @15,@PC00016+4                                      1053 05084000
         L     RRTXTPTR,RRTXTCUR(,@15)                             1053 05085000
         B     @DE01053                                            1053 05086000
@DL01053 DS    0H                                                  1054 05087000
*         IF RRCHAR(1:1)=','|RRCHAR(1:1)=' '|RRCHAR(1:1)=')'/*     1054 05088000
*                                      COMMA,BLANK OR RIGHT PAREN?   */ 05089000
*           THEN                    /* STOP SCAN                     */ 05090000
         CLI   RRCHAR(RRTXTPTR),C','                               1054 05091000
         BE    @RT01054                                            1054 05092000
         CLI   RRCHAR(RRTXTPTR),C' '                               1054 05093000
         BE    @RT01054                                            1054 05094000
         CLI   RRCHAR(RRTXTPTR),C')'                               1054 05095000
         BE    @RT01054                                            1054 05096000
*           GOTO RRSCNEND;          /* UPDATE BCB & SCB              */ 05097000
*         IF RRTXTPTR=RRTXTCUR+RRBYTLFT-ONE THEN/* REACH END OF    1056 05098000
*                                      BUFFER?                       */ 05099000
         L     @03,@PC00016+4                                      1056 05100000
         L     @15,RRTXTCUR(,@03)                                  1056 05101000
         AL    @15,RRBYTLFT(,@03)                                  1056 05102000
         BCTR  @15,0                                               1056 05103000
         CR    RRTXTPTR,@15                                        1056 05104000
         BE    @RT01056                                            1056 05105000
*           GOTO RRUPDAT2;          /* MOVE IN ERROR TEXT            */ 05106000
*         IF RRCHAR(1:2)=RRCOMBEG THEN/* BEGINNING OF A COMMENT?     */ 05107000
         CLC   RRCHAR(2,RRTXTPTR),RRCOMBEG                         1058 05108000
         BE    @RT01058                                            1058 05109000
*           GOTO RRSCNEND;          /* UPDATE BCB & SCB              */ 05110000
*RRUPDAT2:                                                         1060 05111000
*         RRFOUND='1'B;             /* INDICATE END OF BUFFER        */ 05112000
RRUPDAT2 OI    RRFOUND,B'10000000'                                 1060 05113000
*       END;                                                       1061 05114000
         AL    RRTXTPTR,@CF00077                                   1061 05115000
@DE01053 L     @03,@PC00016+4                                      1061 05116000
         L     @15,@PC00016+4                                      1061 05117000
         L     @03,RRTXTCUR(,@03)                                  1061 05118000
         AL    @03,RRBYTLFT(,@15)                                  1061 05119000
         BCTR  @03,0                                               1061 05120000
         CR    RRTXTPTR,@03                                        1061 05121000
         BNH   @DL01053                                            1061 05122000
*RRSCNEND:                                                         1062 05123000
*     ;                             /* UPDATE POINTER TO INPUT       */ 05124000
@RC01043 DS    0H                                                  1062 05125000
RRSCNEND DS    0H                                                  1063 05126000
*     RRBYTLFT=RRBYTLFT-(RRTXTPTR-RRTXTCUR);/* NUMBER OF BYTES LEFT  */ 05127000
         L     @03,@PC00016+4                                      1063 05128000
         LR    @15,RRTXTPTR                                        1063 05129000
         SL    @15,RRTXTCUR(,@03)                                  1063 05130000
         LCR   @15,@15                                             1063 05131000
         AL    @15,RRBYTLFT(,@03)                                  1063 05132000
         ST    @15,RRBYTLFT(,@03)                                  1063 05133000
*     RRTXTCUR=RRTXTPTR;            /* UPDATE CURRENT TEXT PTR       */ 05134000
         ST    RRTXTPTR,RRTXTCUR(,@03)                             1064 05135000
*     IF RRBYTLFT^=ZERO THEN        /* AT END OF BUFFER?             */ 05136000
         LTR   @15,@15                                             1065 05137000
         BZ    @RF01065                                            1065 05138000
*       RRFINISH='1'B;              /* INDICATE SO                   */ 05139000
         OI    RRFINISH,B'10000000'                                1066 05140000
*RRENDLOP:                                                         1067 05141000
*     ;                             /* COMPLETED SCAN FOR ERROR      */ 05142000
@RF01065 DS    0H                                                  1067 05143000
RRENDLOP DS    0H                                                  1068 05144000
*   END;                                                           1068 05145000
@DE01031 TM    RRFINISH,B'10000000'                                1068 05146000
         BZ    @DL01031                                            1068 05147000
*   IF RRFOUND='0'B THEN            /* NO TEXT SKIPPED?              */ 05148000
         TM    RRFOUND,B'10000000'                                 1069 05149000
         BNZ   @RF01069                                            1069 05150000
*     RETURN CODE(4);               /* RETURN UNSUCCESSFUL           */ 05151000
         LA    @15,4                                               1070 05152000
         L     @14,@SA00016                                        1070 05153000
         LM    @00,@12,@SA00016+8                                  1070 05154000
         BR    @14                                                 1070 05155000
*   ELSE                            /* MOVE IN MORE TEXT FOR CLARITY */ 05156000
*     RETURN CODE(0);               /* RETURN SUCCESSFUL             */ 05157000
@RF01069 SLR   @15,@15                                             1071 05158000
         L     @14,@SA00016                                        1071 05159000
         LM    @00,@12,@SA00016+8                                  1071 05160000
         BR    @14                                                 1071 05161000
*   END MFRRERR;                                                   1072 05162000
*                                                                  1073 05163000
*/*  PROCEDURE NAME = MFFREBUF                                       */ 05164000
*/*                                                                  */ 05165000
*/*  DESCRIPTIVE NAME = FREE UP BUFFERS AND BCBS                     */ 05166000
*/*                                                                  */ 05167000
*/*  FUNCTION = THIS ROUTINE FREES UP BUFFERS AND BCBS NO LONGER     */ 05168000
*/*             NEEDED IN SCANNING AN INPUT SOURCE. A CODE IS PASSED */ 05169000
*/*              TO THIS ROUTINE TO INDICATE WHETHER ALL BUFFERS AND */ 05170000
*/*              BCBS ARE TO BE FREED OR FREE ONLY UP TO CURRENT ONE */ 05171000
*/*                                                                  */ 05172000
*                                                                  1073 05173000
*MFFREBUF:                                                         1073 05174000
*   PROCEDURE(FRFRECOD,FRSCB);                                     1073 05175000
MFFREBUF STM   @14,@12,@SA00017                                    1073 05176000
         MVC   @PC00017(8),0(@01)                                  1073 05177000
*   DECLARE                                                        1074 05178000
*     FRFRECOD                      /* FREE CODE - 0=FREE ALL BCB'S     05179000
*                                      AND BUFFERS. 4=FREE UP TO   1074 05180000
*                                      CURRENT BCB AND BUFFER        */ 05181000
*         FIXED(31),                                               1074 05182000
*                                                                  1074 05183000
*/*  SCAN CONTROL BLOCK                                              */ 05184000
*                                                                  1074 05185000
*     1 FRSCB,                      /* SCAN CONTROL BLOCK            */ 05186000
*      2 FRCURBCB                   /* CURRENT BCB PTR               */ 05187000
*            PTR(31),                                              1074 05188000
*      2 FRCURTXT                   /* CURRENT TEXT PTR              */ 05189000
*            PTR(31),                                              1074 05190000
*      2 FRBYTLFT                   /* # BYTES LEFT                  */ 05191000
*            FIXED(31),                                            1074 05192000
*     FRFREBCB                      /* BCB TO FREE                   */ 05193000
*         PTR(31),                                                 1074 05194000
*                                                                  1074 05195000
*/*  SUBPOOL INFORMATION                                             */ 05196000
*                                                                  1074 05197000
*     1 FRBCBCOR                    /* FREEMAIN PARAMETERS           */ 05198000
*           BDY(WORD),                                             1074 05199000
*      2 FRSUBPOL                   /* SUBPOOL                       */ 05200000
*            FIXED(8),                                             1074 05201000
*      2 FRBCBLEN                   /* LENGTH                        */ 05202000
*            FIXED(24),                                            1074 05203000
*     FRCONTRL                      /* CONTROL FOR DO GROUP          */ 05204000
*         PTR(31);                                                 1074 05205000
*   FRSUBPOL=ZERO;                  /* SUPOOL 0                      */ 05206000
         MVI   FRSUBPOL,X'00'                                      1075 05207000
*   IF FRFRECOD=ZERO|INBCBFRS=ZERO THEN/* FREE ALL BUFFERS AND BCBS     05208000
*                                      OR NONE TO FREE?              */ 05209000
         SLR   @15,@15                                             1076 05210000
         L     @14,@PC00017                                        1076 05211000
         C     @15,FRFRECOD(,@14)                                  1076 05212000
         BE    @RT01076                                            1076 05213000
         C     @15,INBCBFRS                                        1076 05214000
         BNE   @RF01076                                            1076 05215000
@RT01076 DS    0H                                                  1077 05216000
*     FRCONTRL=ZERO;                                               1077 05217000
         SLR   FRCONTRL,FRCONTRL                                   1077 05218000
*   ELSE                            /* FREE UP TO CURRENT            */ 05219000
*     FRCONTRL=FRCURBCB;                                           1078 05220000
         B     @RC01076                                            1078 05221000
@RF01076 L     @15,@PC00017+4                                      1078 05222000
         L     FRCONTRL,FRCURBCB(,@15)                             1078 05223000
*   DO WHILE INBCBFRS^=FRCONTRL;                                   1079 05224000
@RC01076 B     @DE01079                                            1079 05225000
@DL01079 DS    0H                                                  1080 05226000
*     FRBCBLEN=INBCBFRS->INBCBLEN;  /* BUFFER LENGTH                 */ 05227000
         L     @15,INBCBFRS                                        1080 05228000
         LH    @14,INBCBLEN(,@15)                                  1080 05229000
         STCM  @14,7,FRBCBLEN                                      1080 05230000
*     RFY                                                          1081 05231000
*      (GPR00F,                                                    1081 05232000
*       GPR01P,                                                    1081 05233000
*       GPR14F,                                                    1081 05234000
*       GPR15F) RSTD;                                              1081 05235000
*     GPR00F=FRBCBCOR;              /* SUBPOOL & LENGTH              */ 05236000
         L     GPR00F,FRBCBCOR                                     1082 05237000
*     GPR01P=INBCBFRS->INBCBTXT;    /* ADDRESS OF BUFFER TO FREE     */ 05238000
         L     @07,INBCBFRS                                        1083 05239000
         L     GPR01P,INBCBTXT(,@07)                               1083 05240000
*     GEN(FREEMAIN R,LV=(0),A=(1)); /* FREE BUFFER                   */ 05241000
         FREEMAIN R,LV=(0),A=(1)                                        05242000
*     FRFREBCB=INBCBFRS;            /* SAVE BCB TO BE FREED          */ 05243000
         L     @07,INBCBFRS                                        1085 05244000
         LR    FRFREBCB,@07                                        1085 05245000
*     INBCBFRS=INBCBFRS->INBCBNXT;  /* FIRST = NEXT BCB              */ 05246000
         L     @07,INBCBNXT(,@07)                                  1086 05247000
         ST    @07,INBCBFRS                                        1086 05248000
*     FRBCBLEN=LENGTH(INBCB);       /* BCB LENGTH                    */ 05249000
         MVC   FRBCBLEN(3),@CF02027+1                              1087 05250000
*     GPR00F=FRBCBCOR;              /* SUBPOOL & LENGTH              */ 05251000
         L     GPR00F,FRBCBCOR                                     1088 05252000
*     GPR01P=FRFREBCB;              /* ADDRESS OF BCB TO FREE        */ 05253000
         LR    GPR01P,FRFREBCB                                     1089 05254000
*     GEN(FREEMAIN R,LV=(0),A=(1)); /* FREE BCB                      */ 05255000
         FREEMAIN R,LV=(0),A=(1)                                        05256000
*     RFY                                                          1091 05257000
*      (GPR00F,                                                    1091 05258000
*       GPR01P,                                                    1091 05259000
*       GPR14F,                                                    1091 05260000
*       GPR15F) UNRSTD;                                            1091 05261000
*   END;                                                           1092 05262000
@DE01079 C     FRCONTRL,INBCBFRS                                   1092 05263000
         BNE   @DL01079                                            1092 05264000
*   IF FRFRECOD=ZERO THEN           /* FREE ALL?                     */ 05265000
         SLR   @15,@15                                             1093 05266000
         L     @14,@PC00017                                        1093 05267000
         C     @15,FRFRECOD(,@14)                                  1093 05268000
         BNE   @RF01093                                            1093 05269000
*     DO;                                                          1094 05270000
*       INBCBLST=ADDR(INBCBFRS);    /* LAST BCB = ADDR(FIRST)        */ 05271000
         LA    @14,INBCBFRS                                        1095 05272000
         ST    @14,INBCBLST                                        1095 05273000
*       FRCURBCB=ADDR(INBCBFRS);    /* SCB INITIAL CONDITION         */ 05274000
         L     @07,@PC00017+4                                      1096 05275000
         ST    @14,FRCURBCB(,@07)                                  1096 05276000
*       FRCURTXT=ZERO;              /* SCB INITIAL CONDITION         */ 05277000
         ST    @15,FRCURTXT(,@07)                                  1097 05278000
*       FRBYTLFT=ZERO;              /* SCB INITIAL CONDITION         */ 05279000
         ST    @15,FRBYTLFT(,@07)                                  1098 05280000
*     END;                                                         1099 05281000
*   RETURN;                         /* RETURN TO CALLER              */ 05282000
@EL00017 DS    0H                                                  1100 05283000
@EF00017 DS    0H                                                  1100 05284000
@ER00017 LM    @14,@12,@SA00017                                    1100 05285000
         BR    @14                                                 1100 05286000
*   END MFFREBUF;                                                  1101 05287000
         B     @EL00017                                            1101 05288000
*                                                                  1102 05289000
*/*  PROCEDURE NAME = MFBUFENQ                                       */ 05290000
*/*                                                                  */ 05291000
*/*  DESCRIPTIVE NAME = BUFFER ENQUEING ROUTINE                      */ 05292000
*/*                                                                  */ 05293000
*/*  FUNCTION = THIS ROUTINE QUEUES UP THE BCBS. EACH BCB POINTS TO  */ 05294000
*/*             A BUFFER CONTAINING TEXT PASSED FROM CALLING ROUTINE.*/ 05295000
*/*             AS A RESULT OF QUEUEING BUFFERS AND BCBS, THE SCB AND*/ 05296000
*/*             BQCB ARE UPDATED ACCORDINGLY.                        */ 05297000
*/*                                                                  */ 05298000
*/*                                                                  */ 05299000
*                                                                  1102 05300000
*MFBUFENQ:                                                         1102 05301000
*   PROCEDURE(BUBUFTXT,BUBUFSZE,BUSCB);                            1102 05302000
MFBUFENQ STM   @14,@12,@SA00018                                    1102 05303000
         MVC   @PC00018(12),0(@01)                                 1102 05304000
*   DECLARE                                                        1103 05305000
*     BUBUFTXT                      /* TEXT TO BE PLACED IN BUFFER   */ 05306000
*         CHAR(*),                                                 1103 05307000
*     BUBUFSZE                      /* LENGTH OF TEXT                */ 05308000
*         FIXED(31),                                               1103 05309000
*                                                                  1103 05310000
*/*  SCAN CONTROL BLOCK                                              */ 05311000
*                                                                  1103 05312000
*     1 BUSCB,                      /* SCAN CONTROL BLOCK            */ 05313000
*      2 BUCURBCB                   /* CURRENT BCB PTR               */ 05314000
*            PTR(31),                                              1103 05315000
*      2 BUCURTXT                   /* CURRENT TEXT PTR              */ 05316000
*            PTR(31),                                              1103 05317000
*      2 BUBYTLFT                   /* # BYTES LEFT                  */ 05318000
*            FIXED(31),                                            1103 05319000
*                                                                  1103 05320000
*/*  SUBPOOL INFORMATION                                             */ 05321000
*                                                                  1103 05322000
*     1 BUBCBCOR                    /* GETMAIN PARAMETERS            */ 05323000
*           BDY(WORD),                                             1103 05324000
*      2 BUSUBPOL                   /* SUBPOOL                       */ 05325000
*            FIXED(8),                                             1103 05326000
*      2 BUBUFLEN                   /* LENGTH                        */ 05327000
*            FIXED(24),                                            1103 05328000
*     BUBCBARA                      /* PTR TO GOTTEN AREA FOR BCB    */ 05329000
*         PTR(31);                                                 1103 05330000
*                                                                  1103 05331000
*   /*****************************************************************/ 05332000
*   /*                                                               */ 05333000
*   /* GET CORE FOR A BCB                                            */ 05334000
*   /*                                                               */ 05335000
*   /*****************************************************************/ 05336000
*                                                                  1104 05337000
*   BUSUBPOL=ZERO;                                                 1104 05338000
         MVI   BUSUBPOL,X'00'                                      1104 05339000
*   BUBUFLEN=LENGTH(INBCB);                                        1105 05340000
         MVC   BUBUFLEN(3),@CF02027+1                              1105 05341000
*   RFY                                                            1106 05342000
*    (GPR00F,                                                      1106 05343000
*     GPR01P,                                                      1106 05344000
*     GPR14P,                                                      1106 05345000
*     GPR15F) RSTD;                                                1106 05346000
*   GPR00F=BUBCBCOR;                /* SET UP SUBPOOL ID AND LENGTH     05347000
*                                      IN REG0                       */ 05348000
         L     GPR00F,BUBCBCOR                                     1107 05349000
*   GEN(GETMAIN R,LV=(0));          /* GET CORE FOR BCB              */ 05350000
         GETMAIN R,LV=(0)                                               05351000
*   BUBCBARA=GPR01P;                /* GET ADDRESS OF THE GOTTEN CORE*/ 05352000
         LR    BUBCBARA,GPR01P                                     1109 05353000
*   RFY                                                            1110 05354000
*    (GPR00F,                                                      1110 05355000
*     GPR01P,                                                      1110 05356000
*     GPR14P,                                                      1110 05357000
*     GPR15F) UNRSTD;                                              1110 05358000
*   IF INBCBFRS=ZERO THEN           /* IF THIS BCB IS 1ST ON THE   1111 05359000
*                                      CHAIN                         */ 05360000
         L     @03,INBCBFRS                                        1111 05361000
         LTR   @03,@03                                             1111 05362000
         BNZ   @RF01111                                            1111 05363000
*     DO;                           /* INDICATE THIS IN THE BQCB     */ 05364000
*       INBCBFRS=BUBCBARA;          /* FIRST = GOTTEN AREA           */ 05365000
         ST    BUBCBARA,INBCBFRS                                   1113 05366000
*       INBCBLST=INBCBFRS;          /* FIRST BCB IS LAST BCB ON CHAIN*/ 05367000
         L     @03,INBCBFRS                                        1114 05368000
         ST    @03,INBCBLST                                        1114 05369000
*     END;                                                         1115 05370000
*   ELSE                            /* IF A BCB ALREADY EXISTS       */ 05371000
*     DO;                                                          1116 05372000
         B     @RC01111                                            1116 05373000
@RF01111 DS    0H                                                  1117 05374000
*       INBCBLST->INBCBNXT=BUBCBARA;/* NEXT BCB IS THIS NEW BCB      */ 05375000
         L     @03,INBCBLST                                        1117 05376000
         ST    BUBCBARA,INBCBNXT(,@03)                             1117 05377000
*       INBCBLST=BUBCBARA;          /* THIS BCB IS LAST ON CHAIN     */ 05378000
         ST    BUBCBARA,INBCBLST                                   1118 05379000
*     END;                                                         1119 05380000
*   BUBCBARA->INBCBNXT=ZERO;        /* INDICATES LAST BCB            */ 05381000
@RC01111 SLR   @03,@03                                             1120 05382000
         ST    @03,INBCBNXT(,BUBCBARA)                             1120 05383000
*   BUBCBARA->INBCBLEN=BUBUFSZE;    /* INITIALIZE BCB BUFFER LENGTH  */ 05384000
         L     @03,@PC00018+4                                      1121 05385000
         L     @03,BUBUFSZE(,@03)                                  1121 05386000
         STH   @03,INBCBLEN(,BUBCBARA)                             1121 05387000
*   BUBCBARA->INBCBTXT=ADDR(BUBUFTXT);/* POINT BCB TO BUFFER         */ 05388000
         L     @03,@PC00018                                        1122 05389000
         ST    @03,INBCBTXT(,BUBCBARA)                             1122 05390000
*   BUCURBCB=BUBCBARA;              /* CURRENT BCB IS THIS BCB       */ 05391000
         L     @15,@PC00018+8                                      1123 05392000
         ST    BUBCBARA,BUCURBCB(,@15)                             1123 05393000
*   BUCURTXT=ADDR(BUBUFTXT);        /* CURRENT INPUT STRING NOT    1124 05394000
*                                      SCANNED IS ENTIRE INPUT STRING*/ 05395000
         ST    @03,BUCURTXT(,@15)                                  1124 05396000
*   BUBYTLFT=IPDATALN;              /* LENGTH OF DATA FIELD          */ 05397000
         MVC   BUBYTLFT(4,@15),@CF01232                            1125 05398000
*   RETURN;                         /* RETURN TO CALLER              */ 05399000
@EL00018 DS    0H                                                  1126 05400000
@EF00018 DS    0H                                                  1126 05401000
@ER00018 LM    @14,@12,@SA00018                                    1126 05402000
         BR    @14                                                 1126 05403000
*   END MFBUFENQ;                                                  1127 05404000
*                                                                  1128 05405000
*/*  PROCEDURE NAME = MFNXTREC                                       */ 05406000
*/*                                                                  */ 05407000
*/*  DESCRIPTIVE NAME = GET THE NEXT SEQUENTIAL RECORD OF AN INPUT   */ 05408000
*/*                     SOURCE                                       */ 05409000
*/*                                                                  */ 05410000
*/*  FUNCTION = THIS ROUTINE RETRIEVES THE NEXT SEQUENTIAL RECORD OF */ 05411000
*/*             AN INPUT SOURCE. IF NONE EXIST, IT RETURNS UNSUCCESS-*/ 05412000
*/*             FUL TO CALLER.                                       */ 05413000
*/*                                                                  */ 05414000
*                                                                  1128 05415000
*MFNXTREC:                                                         1128 05416000
*   PROCEDURE(NXSCB);                                              1128 05417000
MFNXTREC STM   @14,@12,@SA00019                                    1128 05418000
         MVC   @PC00019(4),0(@01)                                  1128 05419000
*   DECLARE                                                        1129 05420000
*                                                                  1129 05421000
*/*  SCAN CONTROL BLOCK                                              */ 05422000
*                                                                  1129 05423000
*     1 NXSCB,                      /* SCAN CONTROL BLOCK            */ 05424000
*      2 NXCURBCB                   /* CURRENT BCB PTR               */ 05425000
*            PTR(31),                                              1129 05426000
*      2 NXCURTXT                   /* CURRENT TEXT PTR              */ 05427000
*            PTR(31),                                              1129 05428000
*      2 NXBYTLFT                   /* # BYTES LEFT                  */ 05429000
*            FIXED(31),                                            1129 05430000
*                                                                  1129 05431000
*/*  SUBPOOL INFORMATION                                             */ 05432000
*                                                                  1129 05433000
*     1 NXFRECOR BDY(WORD),         /* GETMAIN PARAMETERS            */ 05434000
*      2 NXSUBPOL                   /* SUBPOOL                       */ 05435000
*            FIXED(8),                                             1129 05436000
*      2 NXLEN                      /* LENGTH                        */ 05437000
*            FIXED(24),                                            1129 05438000
*     NXFOUND BIT(1),               /* RECORD FOUND BIT              */ 05439000
*     NXLENGTH                      /* LENGTH OF TEXT IN A RECORD    */ 05440000
*         FIXED(31),                                               1129 05441000
*     NXTEXT                        /* TEXT IN RECORD                */ 05442000
*         CHAR(*) BASED(NXTEXPTR),                                 1129 05443000
*     NXTEXPTR                      /* PTR TO RECORD                 */ 05444000
*         PTR(31),                                                 1129 05445000
*     NXSUCCES                      /* SUCCESSFUL RETURN             */ 05446000
*         FIXED(31) CONSTANT(0),                                   1129 05447000
*     NXUNSUCC                      /* UNSUCCESSFUL RETURN           */ 05448000
*         FIXED(31) CONSTANT(4);                                   1129 05449000
*   IF NXCURBCB=INBCBLST THEN       /* LAST BCB ON CHAIN?            */ 05450000
         L     @15,@PC00019                                        1130 05451000
         CLC   NXCURBCB(4,@15),INBCBLST                            1130 05452000
         BNE   @RF01130                                            1130 05453000
*     DO;                           /* YES                           */ 05454000
*       NXFOUND='0'B;                                              1132 05455000
         NI    NXFOUND,B'01111111'                                 1132 05456000
*       CALL IRARMRDR(NXTEXPTR,NXLENGTH,NXFRECOR,DAPTR);/* READ IN A    05457000
*                                      RECORD                @Z40BPEM*/ 05458000
         LA    @15,NXTEXPTR                                        1133 05459000
         ST    @15,@AL00001                                        1133 05460000
         LA    @15,NXLENGTH                                        1133 05461000
         ST    @15,@AL00001+4                                      1133 05462000
         LA    @15,NXFRECOR                                        1133 05463000
         ST    @15,@AL00001+8                                      1133 05464000
         L     @15,@PC00001+8                                      1133 05465000
         ST    @15,@AL00001+12                                     1133 05466000
         L     @15,@CV01276                                        1133 05467000
         LA    @01,@AL00001                                        1133 05468000
         BALR  @14,@15                                             1133 05469000
*       RFY                                                        1134 05470000
*        (GPR15F) RSTD;                                            1134 05471000
*       IF GPR15F=NXSUCCES THEN     /* WAS READ OK?                  */ 05472000
         LTR   GPR15F,GPR15F                                       1135 05473000
         BNZ   @RF01135                                            1135 05474000
*         DO;                       /* YES                           */ 05475000
*           RFY                                                    1137 05476000
*            (GPR15F) UNRSTD;                                      1137 05477000
*           NXFOUND='1'B;           /* A RECORD WAS RETRIEVED        */ 05478000
         OI    NXFOUND,B'10000000'                                 1138 05479000
*           CALL MFBUFENQ(NXTEXT,NXLENGTH,NXSCB);/* SET UP BUFFER &     05480000
*                                      BCB                           */ 05481000
         L     @01,NXTEXPTR                                        1139 05482000
         ST    @01,@AL00001                                        1139 05483000
         LA    @15,NXLENGTH                                        1139 05484000
         ST    @15,@AL00001+4                                      1139 05485000
         L     @15,@PC00019                                        1139 05486000
         ST    @15,@AL00001+8                                      1139 05487000
         LA    @01,@AL00001                                        1139 05488000
         BAL   @14,MFBUFENQ                                        1139 05489000
*         END;                                                     1140 05490000
*       IF NXFOUND='1'B THEN        /* RECORD RETRIEVED?             */ 05491000
@RF01135 TM    NXFOUND,B'10000000'                                 1141 05492000
         BNO   @RF01141                                            1141 05493000
*         RETURN CODE(NXSUCCES);    /* RETURN SUCCESSFUL             */ 05494000
         SLR   @15,@15                                             1142 05495000
         L     @14,@SA00019                                        1142 05496000
         LM    @00,@12,@SA00019+8                                  1142 05497000
         BR    @14                                                 1142 05498000
*       ELSE                        /* END OF FILE                   */ 05499000
*         RETURN CODE(NXUNSUCC);    /* RETURN UNSUCCESSFUL           */ 05500000
@RF01141 LA    @15,4                                               1143 05501000
         L     @14,@SA00019                                        1143 05502000
         LM    @00,@12,@SA00019+8                                  1143 05503000
         BR    @14                                                 1143 05504000
*     END;                                                         1144 05505000
*   NXCURBCB=NXCURBCB->INBCBNXT;    /* POINT CURRENT TO NEXT BCB     */ 05506000
