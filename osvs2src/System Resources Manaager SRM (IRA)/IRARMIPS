         M     @00,@CF01259                                        0156 00787000
         D     @00,@CF01216                                        0156 00788000
         AL    @01,@CF00077                                        0156 00789000
         ST    @01,WMSTIOC(,@15)                                   0156 00790000
*       IF WMSTMSO^=ZERO THEN       /* MSO SERV COEF ^=0             */ 00791000
@RF00155 L     @15,WMSTPTR                                         0157 00792000
         L     @14,WMSTMSO(,@15)                                   0157 00793000
         LTR   @14,@14                                             0157 00794000
         BZ    @RF00157                                            0157 00795000
*         WMSTMSO=WMSTMSO*MSOSCM/MSOSF+ONE;/* SCALE MSO SER          */ 00796000
         LR    @01,@14                                             0158 00797000
         M     @00,@CF01259                                        0158 00798000
         D     @00,@CF01263                                        0158 00799000
         AL    @01,@CF00077                                        0158 00800000
         ST    @01,WMSTMSO(,@15)                                   0158 00801000
*     END;                                                         0159 00802000
*                                                                  0159 00803000
@RF00157 DS    0H                                                  0160 00804000
*/*  FOR THE FOLLOWING TABLES -                                      */ 00805000
*/*  RELEASE THE UNUSED STORAGE AND ADJUST THE RESPECTIVE ENTRIES    */ 00806000
*/*  IN WORKLOAD MANAGER SPECIFICATION TABLE TO REFLECT THE ACTUAL   */ 00807000
*/*  SIZES OF THE TABLES                                             */ 00808000
*/*  THE ACTUAL SIZE OF THE TABLE IS COMPUTED AND PASSED TO RMIPFM   */ 00809000
*/*  ROUTINE VIA IPTBASZE                                            */ 00810000
*/*    PERFORMANCE GROUP VECTOR TABLE         - WPGVT              0160 00811000
*      LEAVE THIS TABLE AT MAXIMUM SIZE, SO THAT ANY SRM MODULE    0160 00812000
*      CAN STUDY A PGD MAPPING WITHOUT WORRY OF STEPPING BEYOND    0160 00813000
*      THE END OF THE PGVT.                                  @ZA27109*/ 00814000
*/*    PERFORMANCE OBJECTIVE VECTOR TABLE     - WPOVT                */ 00815000
*                                                                  0160 00816000
*   IPTBASZE=(IPWOBJHI+ONE)*LENGTH(WPOVTENT);/* ACTUAL SIZE OF WPOVT */ 00817000
@RC00150 LA    @15,1                                               0160 00818000
         AH    @15,IPWOBJHI                                        0160 00819000
         ALR   @15,@15                                             0160 00820000
         ST    @15,IPTBASZE                                        0160 00821000
*   CALL RMIPFM(WMSTPOVT,WMSTPOVS,IPTBASZE);/* FREE UNUSED SPACE IN     00822000
*                                      WPOVT                         */ 00823000
         L     @15,WMSTPTR                                         0161 00824000
         LA    @14,WMSTPOVT(,@15)                                  0161 00825000
         ST    @14,@AL00001                                        0161 00826000
         LA    @15,WMSTPOVS(,@15)                                  0161 00827000
         ST    @15,@AL00001+4                                      0161 00828000
         LA    @15,IPTBASZE                                        0161 00829000
         ST    @15,@AL00001+8                                      0161 00830000
         LA    @01,@AL00001                                        0161 00831000
         BAL   @14,RMIPFM                                          0161 00832000
*/* DOMAIN VECTOR TABLE       - DMVT                         @Z40BPEM*/ 00833000
*                                                                  0162 00834000
*   IPTBASZE=(DMNHI+ONE)*LENGTH(DMVTENT);/* ACTUAL SIZE OF DMVT    0162 00835000
*                                                            @Z40BPEM*/ 00836000
         LA    @15,1                                               0162 00837000
         AH    @15,DMNHI                                           0162 00838000
         ALR   @15,@15                                             0162 00839000
         ST    @15,IPTBASZE                                        0162 00840000
*   CALL RMIPFM(WMSTDMVT,WMSTDMVS,IPTBASZE);/* FREE UNUSED SPACE IN     00841000
*                                      DMVT                  @Z40BPEM*/ 00842000
         L     @15,WMSTPTR                                         0163 00843000
         LA    @14,WMSTDMVT(,@15)                                  0163 00844000
         ST    @14,@AL00001                                        0163 00845000
         LA    @15,WMSTDMVS(,@15)                                  0163 00846000
         ST    @15,@AL00001+4                                      0163 00847000
         LA    @15,IPTBASZE                                        0163 00848000
         ST    @15,@AL00001+8                                      0163 00849000
         LA    @01,@AL00001                                        0163 00850000
         BAL   @14,RMIPFM                                          0163 00851000
*/*    PERFORMANCE GROUP DESCRIPTOR TABLE     - WPGDT                */ 00852000
*                                                                  0164 00853000
*   IPTBASZE=WPGDPTR-WMSTPGDT;                                     0164 00854000
         L     @15,WMSTPTR                                         0164 00855000
         L     @14,WPGDPTR                                         0164 00856000
         SL    @14,WMSTPGDT(,@15)                                  0164 00857000
         ST    @14,IPTBASZE                                        0164 00858000
*   CALL RMIPFM(WMSTPGDT,WMSTPGDS,IPTBASZE);/* FREE UNUSED SPACE IN     00859000
*                                      WPGDT                         */ 00860000
         LA    @14,WMSTPGDT(,@15)                                  0165 00861000
         ST    @14,@AL00001                                        0165 00862000
         LA    @15,WMSTPGDS(,@15)                                  0165 00863000
         ST    @15,@AL00001+4                                      0165 00864000
         LA    @15,IPTBASZE                                        0165 00865000
         ST    @15,@AL00001+8                                      0165 00866000
         LA    @01,@AL00001                                        0165 00867000
         BAL   @14,RMIPFM                                          0165 00868000
*/*    PERFORMANCE OBJECTIVE DESCRIPTOR TABLE - WPODT                */ 00869000
*                                                                  0166 00870000
*   IPTBASZE=WPODPTR-WMSTPODT;                                     0166 00871000
         L     @15,WMSTPTR                                         0166 00872000
         L     @14,WPODPTR                                         0166 00873000
         SL    @14,WMSTPODT(,@15)                                  0166 00874000
         ST    @14,IPTBASZE                                        0166 00875000
*   CALL RMIPFM(WMSTPODT,WMSTPODS,IPTBASZE);/* FREE UNUSED SPACE IN     00876000
*                                      WPODT                         */ 00877000
         LA    @14,WMSTPODT(,@15)                                  0167 00878000
         ST    @14,@AL00001                                        0167 00879000
         LA    @15,WMSTPODS(,@15)                                  0167 00880000
         ST    @15,@AL00001+4                                      0167 00881000
         LA    @15,IPTBASZE                                        0167 00882000
         ST    @15,@AL00001+8                                      0167 00883000
         LA    @01,@AL00001                                        0167 00884000
         BAL   @14,RMIPFM                                          0167 00885000
*/*    DOMAIN DESCRIPTION TABLE    - DMDT                    @Z40BPEM*/ 00886000
*                                                                  0168 00887000
*   IPTBASZE=DMDTPTR-WMSTDMDT;      /* SIZE OF DMDT          @Z40BPEM*/ 00888000
         L     @15,WMSTPTR                                         0168 00889000
         L     @14,DMDTPTR                                         0168 00890000
         SL    @14,WMSTDMDT(,@15)                                  0168 00891000
         ST    @14,IPTBASZE                                        0168 00892000
*   CALL RMIPFM(WMSTDMDT,WMSTDMDS,IPTBASZE);/* FREE UNUSED SPACE IN     00893000
*                                      DMDT                  @Z40BPEM*/ 00894000
         LA    @14,WMSTDMDT(,@15)                                  0169 00895000
         ST    @14,@AL00001                                        0169 00896000
         LA    @15,WMSTDMDS(,@15)                                  0169 00897000
         ST    @15,@AL00001+4                                      0169 00898000
         LA    @15,IPTBASZE                                        0169 00899000
         ST    @15,@AL00001+8                                      0169 00900000
         LA    @01,@AL00001                                        0169 00901000
         BAL   @14,RMIPFM                                          0169 00902000
*/* GET CONTIGUOUS STORAGE FOR ALL THE IPS TABLES            @Z40BPEM*/ 00903000
*                                                                  0170 00904000
*   TOTSIZE=WMSTPGVS+WMSTPOVS+WMSTDMVS+WMSTPGDS+WMSTPODS+WMSTDMDS+ 0170 00905000
*       LENGTH(WMST);               /* SIZE REQUIRED FOR ALL TABLES     00906000
*                                                            @Z40BPEM*/ 00907000
         L     @15,WMSTPTR                                         0170 00908000
         L     @14,WMSTPGVS(,@15)                                  0170 00909000
         AL    @14,WMSTPOVS(,@15)                                  0170 00910000
         AL    @14,WMSTDMVS(,@15)                                  0170 00911000
         AL    @14,WMSTPGDS(,@15)                                  0170 00912000
         AL    @14,WMSTPODS(,@15)                                  0170 00913000
         AL    @14,WMSTDMDS(,@15)                                  0170 00914000
         AL    @14,@CF02023                                        0170 00915000
         ST    @14,TOTSIZE                                         0170 00916000
*   CALL RMIPGMCS(NEWMST,TOTSIZE);  /* GET NEW STORAGE,FREE OLD    0171 00917000
*                                      STORAGE               @Z40BPEM*/ 00918000
         LA    @15,NEWMST                                          0171 00919000
         ST    @15,@AL00001                                        0171 00920000
         LA    @15,TOTSIZE                                         0171 00921000
         ST    @15,@AL00001+4                                      0171 00922000
         LA    @01,@AL00001                                        0171 00923000
         BAL   @14,RMIPGMCS                                        0171 00924000
*/* SETUP NEW ADDRESSES TO IPS TABLES IN NEW WMST            @Z40BPEM*/ 00925000
*                                                                  0172 00926000
*   IPWMSTAD=NEWMST;                /* SET PTR TO WMST IN OUTPUT   0172 00927000
*                                      PARAMETER             @Z40BPEM*/ 00928000
         L     @15,@PC00001+4                                      0172 00929000
         L     @14,NEWMST                                          0172 00930000
         ST    @14,IPWMSTAD(,@15)                                  0172 00931000
*   WMSTPGVT=WMSTPTR+LENGTH(WMST);  /* ADDR OF NEW PGVT      @Z40BPEM*/ 00932000
         L     @15,WMSTPTR                                         0173 00933000
         LA    @14,112                                             0173 00934000
         ALR   @14,@15                                             0173 00935000
         ST    @14,WMSTPGVT(,@15)                                  0173 00936000
*   WMSTPGDT=WMSTPGVT+TABSIZE(2);   /* ADDR OF NEW PGDT      @Z40BPEM*/ 00937000
         AL    @14,TABSIZE+8(,@15)                                 0174 00938000
         ST    @14,WMSTPGDT(,@15)                                  0174 00939000
*   WMSTPOVT=WMSTPGDT+TABSIZE(3);   /* ADDR OF NEW POVT      @Z40BPEM*/ 00940000
         AL    @14,TABSIZE+16(,@15)                                0175 00941000
         ST    @14,WMSTPOVT(,@15)                                  0175 00942000
*   WMSTPODT=WMSTPOVT+TABSIZE(4);   /* ADDR OF NEW PODT      @Z40BPEM*/ 00943000
         AL    @14,TABSIZE+24(,@15)                                0176 00944000
         ST    @14,WMSTPODT(,@15)                                  0176 00945000
*   WMSTDMDT=WMSTPODT+TABSIZE(5);   /* ADDR OF NEW DMDT      @Z40BPEM*/ 00946000
         AL    @14,TABSIZE+32(,@15)                                0177 00947000
         ST    @14,WMSTDMDT(,@15)                                  0177 00948000
*   WMSTDMVT=WMSTDMDT+TABSIZE(6);   /* ADDR OF NEW DMVT      @Z40BPEM*/ 00949000
         AL    @14,TABSIZE+40(,@15)                                0178 00950000
         ST    @14,WMSTDMVT(,@15)                                  0178 00951000
*/*  RETURN TO CALLER WITH A RETURN CODE INDICATING SUCCESSFUL RETURN*/ 00952000
*                                                                  0179 00953000
*   RETURN CODE(SUCCESS);           /* RETURN SUCCESSFUL             */ 00954000
         L     @13,4(,@13)                                         0179 00955000
         L     @00,@SIZDATD                                        0179 00956000
         LR    @01,@08                                             0179 00957000
         FREEMAIN R,LV=(0),A=(1)                                        00958000
         SLR   @15,@15                                             0179 00959000
         L     @14,12(,@13)                                        0179 00960000
         LM    @00,@12,20(@13)                                     0179 00961000
         BR    @14                                                 0179 00962000
*/*  RETURN TO CALLER WITH A RETURN CODE INDICATING FAILURE          */ 00963000
*                                                                  0180 00964000
*RMIPERR:                                                          0180 00965000
*   ;                               /* IPS IS INVALID                */ 00966000
RMIPERR  DS    0H                                                  0181 00967000
*   CALL IRARMFRE(IPWMSTAD);        /* FREE IPS STORAGE              */ 00968000
         L     @15,@PC00001+4                                      0181 00969000
         ST    @15,@AL00001                                        0181 00970000
         LA    @01,@AL00001                                        0181 00971000
         L     @15,@CA01542                                        0181 00972000
         BALR  @14,@15                                             0181 00973000
*   RETURN CODE(FAIL);              /* RETURN UNSUCCESSFUL           */ 00974000
         L     @13,4(,@13)                                         0182 00975000
         L     @00,@SIZDATD                                        0182 00976000
         LR    @01,@08                                             0182 00977000
         FREEMAIN R,LV=(0),A=(1)                                        00978000
         LA    @15,4                                               0182 00979000
         L     @14,12(,@13)                                        0182 00980000
         LM    @00,@12,20(@13)                                     0182 00981000
         BR    @14                                                 0182 00982000
*                                                                  0183 00983000
*/********************************************************************/ 00984000
*/*    PROCEDURE NAME = RMIPGMCS                                     */ 00985000
*/*                                                                  */ 00986000
*/*    FUNCTION:                                                     */ 00987000
*/*    CONTIGUOUS STORAGE IS OBTAINED FROM SUBPOOL 245 FOR THE       */ 00988000
*/*    WMST AND CONTROL BLOCKS WHICH CONTAIN IPS DATA.               */ 00989000
*/*    PREVIOUS STORAGE FOR CONTROL BLOCKS IS RETURNED               */ 00990000
*/*    TO SUBPOOL 245.                                               */ 00991000
*/*                                                                  */ 00992000
*/*    PARAMETERS:                                                   */ 00993000
*/*             1. 1 WORD-ADDR OF AREA UPON EXIT                     */ 00994000
*/*             2. 1 WORD-SIZE OF AREA                               */ 00995000
*/*                                                          @Z40BPEM*/ 00996000
*/********************************************************************/ 00997000
*                                                                  0183 00998000
*RMIPGMCS:                                                         0183 00999000
*   PROC(NEWAREA,SIZE);                                            0183 01000000
RMIPGMCS STM   @14,@12,@SA00002                                    0183 01001000
         MVC   @PC00002(8),0(@01)                                  0183 01002000
*   DECLARE                                                        0184 01003000
*     NEWAREA PTR,                  /* PTR TO AREA                   */ 01004000
*     SIZE FIXED,                   /* SIZE OF AREA                  */ 01005000
*     NOTABLES FIXED(15) CONSTANT(7),/* # OF IPS CONTROL BLOCKS      */ 01006000
*     TABLEPTR PTR,                 /* NEW PTR TO TABLES             */ 01007000
*     LENTH FIXED,                  /* LENGTH OF TABLE               */ 01008000
*     DUMMYPTR PTR;                 /* DUMMY PARAMETER FOR RMIPGM    */ 01009000
*/*  GET STORAGE FOR THE TABLES                                      */ 01010000
*                                                                  0185 01011000
*   CALL RMIPGM(NEWAREA,SIZE,DUMMYPTR);                            0185 01012000
         L     @15,@PC00002                                        0185 01013000
         ST    @15,@AL00001                                        0185 01014000
         L     @15,@PC00002+4                                      0185 01015000
         ST    @15,@AL00001+4                                      0185 01016000
         LA    @15,DUMMYPTR                                        0185 01017000
         ST    @15,@AL00001+8                                      0185 01018000
         LA    @01,@AL00001                                        0185 01019000
         BAL   @14,RMIPGM                                          0185 01020000
*   LENTH=ZERO;                     /* INITIAL LENGTH                */ 01021000
         SLR   @02,@02                                             0186 01022000
         ST    @02,LENTH                                           0186 01023000
*   TABLEPTR=NEWAREA;               /* PTR TO 1ST BYTE OF AREA     0187 01024000
*                                      OBTAINED                      */ 01025000
         L     @15,@PC00002                                        0187 01026000
         L     TABLEPTR,NEWAREA(,@15)                              0187 01027000
*   RESPECIFY                                                      0188 01028000
*    (R0,                                                          0188 01029000
*     R1,                                                          0188 01030000
*     R14,                                                         0188 01031000
*     R15) RSTD;                                                   0188 01032000
*                                                                  0188 01033000
*/* MOVE DATA TO NEW AREA AND FREE OLD STORAGE                       */ 01034000
*                                                                  0189 01035000
*   DO I=ONE TO NOTABLES;           /* MOVE DATA FOR EACH TABLE      */ 01036000
         LA    I,1                                                 0189 01037000
@DL00189 DS    0H                                                  0190 01038000
*     TABLEPTR=TABLEPTR+LENTH;      /* PTR TO NEW AREA FOR TABLE     */ 01039000
         AL    TABLEPTR,LENTH                                      0190 01040000
*     R0=TABLEPTR;                  /* ADDR OF NEW TABLE             */ 01041000
         LR    R0,TABLEPTR                                         0191 01042000
*     IF I=ONE THEN                                                0192 01043000
         C     I,@CF00077                                          0192 01044000
         BNE   @RF00192                                            0192 01045000
*       DO;                         /* FIRST TABLE IN AREA IS WMST   */ 01046000
*         R14=WMSTPTR;              /* ADDR OF WMST                  */ 01047000
         L     R14,WMSTPTR                                         0194 01048000
*         R1=LENGTH(WMST);          /* SIZE OF WMST                  */ 01049000
         LA    R1,112                                              0195 01050000
*       END;                                                       0196 01051000
*     ELSE                                                         0197 01052000
*       DO;                         /* IPS CONTROL BLOCKS FOLLOW     */ 01053000
         B     @RC00192                                            0197 01054000
@RF00192 DS    0H                                                  0198 01055000
*         R14=TABADDR(I);           /* ADDR OF OLD TABLE             */ 01056000
         L     @07,WMSTPTR                                         0198 01057000
         LR    @06,I                                               0198 01058000
         SLA   @06,3                                               0198 01059000
         LA    @05,0(@06,@07)                                      0198 01060000
         AL    @05,@CF02035                                        0198 01061000
         L     R14,TABADDR(,@05)                                   0198 01062000
*         R1=TABSIZE(I);            /* LENGTH OF OLD TABLE           */ 01063000
         AL    @07,@CF02036                                        0199 01064000
         L     R1,TABSIZE-4(@06,@07)                               0199 01065000
*       END;                                                       0200 01066000
*     R15=R1;                                                      0201 01067000
*                                                                  0201 01068000
@RC00192 LR    R15,R1                                              0201 01069000
*/* MOVE DATA TO NEW AREA                                            */ 01070000
*                                                                  0202 01071000
*     GEN REFS(R0,R1,R14,R15)(MVCL R0,R14);                        0202 01072000
*                                                                  0202 01073000
         MVCL R0,R14                                                    01074000
*/* FREE OLD STORAGE                                                 */ 01075000
*                                                                  0203 01076000
*     IF I=ONE THEN                                                0203 01077000
         C     I,@CF00077                                          0203 01078000
         BNE   @RF00203                                            0203 01079000
*       DO;                         /* FREE WMST                     */ 01080000
*         R0=LENGTH(WMST)|SUBPL245; /* SUBPOOL # AND STOR SIZE       */ 01081000
         LA    @07,112                                             0205 01082000
         L     R0,SUBPL245                                         0205 01083000
         OR    R0,@07                                              0205 01084000
*         R1=WMSTPTR;               /* ADDR OF OLD TABLE             */ 01085000
         L     R1,WMSTPTR                                          0206 01086000
*         WMSTPTR=TABLEPTR;         /* RESET WMSTPTR         @Z40BPEM*/ 01087000
         ST    TABLEPTR,WMSTPTR                                    0207 01088000
*         LENTH=LENGTH(WMST);       /* DISP INTO AREA FOR NEXT TABLE */ 01089000
         ST    @07,LENTH                                           0208 01090000
*       END;                                                       0209 01091000
*     ELSE                                                         0210 01092000
*       DO;                         /* FREE THE OTHER TABLES         */ 01093000
         B     @RC00203                                            0210 01094000
@RF00203 DS    0H                                                  0211 01095000
*         R0=TABSIZE(I)|SUBPL245;   /* SUBPOOL # AND STORAGE SZE     */ 01096000
         L     @07,WMSTPTR                                         0211 01097000
         LR    @06,I                                               0211 01098000
         SLA   @06,3                                               0211 01099000
         LA    @05,0(@06,@07)                                      0211 01100000
         AL    @05,@CF02036                                        0211 01101000
         L     @05,TABSIZE-4(,@05)                                 0211 01102000
         LR    R0,@05                                              0211 01103000
         O     R0,SUBPL245                                         0211 01104000
*         R1=TABADDR(I);            /* ADDR OF OLD TABLE             */ 01105000
         AL    @07,@CF02035                                        0212 01106000
         L     R1,TABADDR(@06,@07)                                 0212 01107000
*         LENTH=TABSIZE(I);         /* DISP INTO AREA FOR NEXT TABLE */ 01108000
         ST    @05,LENTH                                           0213 01109000
*       END;                                                       0214 01110000
*     GEN(FREEMAIN R,LV=(0),A=(1));                                0215 01111000
@RC00203 DS    0H                                                  0215 01112000
         FREEMAIN R,LV=(0),A=(1)                                        01113000
*   END;                                                           0216 01114000
         AL    I,@CF00077                                          0216 01115000
         C     I,@CF01119                                          0216 01116000
         BNH   @DL00189                                            0216 01117000
*   RETURN;                                                        0217 01118000
@EL00002 DS    0H                                                  0217 01119000
@EF00002 DS    0H                                                  0217 01120000
@ER00002 LM    @14,@12,@SA00002                                    0217 01121000
         BR    @14                                                 0217 01122000
*   RESPECIFY                                                      0218 01123000
*    (R0,                                                          0218 01124000
*     R1,                                                          0218 01125000
*     R14,                                                         0218 01126000
*     R15) UNRSTD;                                                 0218 01127000
*   END RMIPGMCS;                                                  0219 01128000
*                                                                  0220 01129000
*/********************************************************************/ 01130000
*/*                                                                  */ 01131000
*/*  PROCEDURE NAME: RMIPGM                                          */ 01132000
*/*                                                                  */ 01133000
*/*  FUNCTION:                                                       */ 01134000
*/*  GETMAIN FROM SUBPOOL 245 FOR A CONTROL BLOCK                    */ 01135000
*/*  PARAMTERS FOR RMIPGM:                                           */ 01136000
*/*    1. 1 WORD-ADDR OF CONTROL BLOCK UPON EXIT                     */ 01137000
*/*    2. 1 WORD-SIZE OF CONTROL BLOCK                               */ 01138000
*/*    3. 1 WORD-ADDR OF CONTROL BLOCK UPON EXIT                     */ 01139000
*/*                                                                  */ 01140000
*/********************************************************************/ 01141000
*                                                                  0220 01142000
*RMIPGM:                                                           0220 01143000
*   PROC(IPGMPPTR,IPGMPSZE,IPGFTPTR);                              0220 01144000
RMIPGM   STM   @14,@12,@SA00003                                    0220 01145000
         MVC   @PC00003(12),0(@01)                                 0220 01146000
*   DECLARE                                                        0221 01147000
*     IPGMPPTR PTR,                 /* PTR TO TABLE                  */ 01148000
*     IPGMPSZE FIXED,               /* SIZE OF TABLE                 */ 01149000
*     IPGFTPTR PTR                  /* PTR TO TABLE                  */ 01150000
*         ;                                                        0221 01151000
*   RESPECIFY                                                      0222 01152000
*    (R0,                                                          0222 01153000
*     R1,                                                          0222 01154000
*     R14,                                                         0222 01155000
*     R15) RESTRICTED;                                             0222 01156000
*   R0=IPGMPSZE|SUBPL245;           /* SUBPOOL NO & STORAGE SIZE     */ 01157000
         L     @07,@PC00003+4                                      0223 01158000
         L     R0,IPGMPSZE(,@07)                                   0223 01159000
         O     R0,SUBPL245                                         0223 01160000
*   GEN(GETMAIN R,LV=(0));                                         0224 01161000
         GETMAIN R,LV=(0)                                               01162000
*   IPGMPPTR=R1;                    /* ADDR OF AREA GOTTEN           */ 01163000
         L     @07,@PC00003                                        0225 01164000
         ST    R1,IPGMPPTR(,@07)                                   0225 01165000
*   IPGFTPTR=R1;                    /* ADDR OF AREA GOTTEN           */ 01166000
         L     @07,@PC00003+8                                      0226 01167000
         ST    R1,IPGFTPTR(,@07)                                   0226 01168000
*/*  CLEAR AREA GOTTEN                                               */ 01169000
*                                                                  0227 01170000
*   R0=R1;                          /* ADDR OF AREA TO BE CLEARED    */ 01171000
         LR    R0,R1                                               0227 01172000
*   R1=IPGMPSZE;                    /* LENGTH OF AREA                */ 01173000
         L     @07,@PC00003+4                                      0228 01174000
         L     R1,IPGMPSZE(,@07)                                   0228 01175000
*   R14=ADDR(CLEAR);                /* ADDR OF LOCATION WITH 0       */ 01176000
         LA    R14,CLEAR                                           0229 01177000
*   R15=ZERO;                       /* PADDING CHAR IS 0             */ 01178000
         SLR   R15,R15                                             0230 01179000
*   GEN REFS(R0,R1,R14,R15)( MVCL  R0,R14 );                       0231 01180000
          MVCL  R0,R14                                                  01181000
*   RETURN;                         /* RETURN TO CALLER              */ 01182000
@EL00003 DS    0H                                                  0232 01183000
@EF00003 DS    0H                                                  0232 01184000
@ER00003 LM    @14,@12,@SA00003                                    0232 01185000
         BR    @14                                                 0232 01186000
*   END RMIPGM;                                                    0233 01187000
*                                                                  0234 01188000
*/********************************************************************/ 01189000
*/*                                                                  */ 01190000
*/*  PROCEDURE NAME: RMIPFM                                          */ 01191000
*/*                                                                  */ 01192000
*/*  FUNCTION:                                                       */ 01193000
*/*  FREEMAIN FROM SUBPOOL 245 FOR UNUSED SPACE IN A CONTROL BLOCK   */ 01194000
*/*  PARAMETERS FOR RMIPFM:                                          */ 01195000
*/*    1. 1 WORD-ADDR OF CONTROL BLOCK                               */ 01196000
*/*    2. 1 WORD-SIZE OF CONTROL BLOCK                               */ 01197000
*/*    3. 1 WORD-ACTUAL SIZE OF STORAGE USED IN CONTROL BLOCK        */ 01198000
*/*  IF THE ACTUAL SIZE OF THE CONTROL BLOCK IS NOT A MULTIPLE OF A  */ 01199000
*/*  DOUBLE WORD, ROUND IT UP TO THE NEXT DOUBLEWORD BOUNDARY        */ 01200000
*/*                                                                  */ 01201000
*/********************************************************************/ 01202000
*                                                                  0234 01203000
*RMIPFM:                                                           0234 01204000
*   PROC(IPFMPPTR,IPFMPSZE,IPFMASZE);                              0234 01205000
RMIPFM   STM   @14,@12,@SA00004                                    0234 01206000
         MVC   @PC00004(12),0(@01)                                 0234 01207000
*   DECLARE                                                        0235 01208000
*     IPFMPPTR PTR,                 /* PTR TO TABLE                  */ 01209000
*     IPFMPSZE FIXED,               /* SIZE OF TABLE                 */ 01210000
*     IPFMASZE FIXED                /* SIZE OF STORAGE USED          */ 01211000
*         ;                                                        0235 01212000
*   IF(IPFMASZE//DBLWORD)=ZERO THEN /* SIZE OF STORAGE USED IS     0236 01213000
*                                      MULTIPLE OF DOUBLE WORD       */ 01214000
         L     @07,@PC00004+8                                      0236 01215000
         L     @07,IPFMASZE(,@07)                                  0236 01216000
         LR    @02,@07                                             0236 01217000
         SRDA  @02,32                                              0236 01218000
         D     @02,@CF00046                                        0236 01219000
         LTR   @02,@02                                             0236 01220000
         BNZ   @RF00236                                            0236 01221000
*     IPFMRSZE=IPFMASZE;                                           0237 01222000
         LR    IPFMRSZE,@07                                        0237 01223000
*   ELSE                            /* NOT A MULTIPLE OF DWORD       */ 01224000
*     IPFMRSZE=(IPFMASZE/DBLWORD+ONE)*DBLWORD;/* ROUND TO NEXT DWORD    01225000
*                                      BOUNDARY                      */ 01226000
         B     @RC00236                                            0238 01227000
@RF00236 L     @07,@PC00004+8                                      0238 01228000
         L     IPFMRSZE,IPFMASZE(,@07)                             0238 01229000
         SRDA  IPFMRSZE,32                                         0238 01230000
         D     IPFMRSZE,@CF00046                                   0238 01231000
         AL    @03,@CF00077                                        0238 01232000
         SLA   @03,3                                               0238 01233000
         LR    IPFMRSZE,@03                                        0238 01234000
*/*  ADDR OF AREA TO BE FREED = ADDR OF BEG OF TABLE+SIZE OF STORAGE */ 01235000
*/*  USED (ROUNDED TO A DWORD)                                       */ 01236000
*/*  SIZE OF AREA TO BE FREED = INITIAL SIZE OF TABLE-SIZE OF STORAGE*/ 01237000
*/*  USED (ROUNDED TO A DWORD)                                       */ 01238000
*                                                                  0239 01239000
*   IPGFQSZE=IPFMPSZE-IPFMRSZE;     /* SIZE OF STORAGE TO BE FREED =    01240000
*                                      ALLOCATED SIZE - ACTUAL SIZE  */ 01241000
@RC00236 L     @07,@PC00004+4                                      0239 01242000
         L     IPGFQSZE,IPFMPSZE(,@07)                             0239 01243000
         SLR   IPGFQSZE,IPFMRSZE                                   0239 01244000
*   IF IPGFQSZE>ZERO THEN           /* UNUSED STORAGE SIZE>0         */ 01245000
         LTR   IPGFQSZE,IPGFQSZE                                   0240 01246000
         BNP   @RF00240                                            0240 01247000
*     DO;                           /* FREE UNUSED STORAGE           */ 01248000
*       R0=IPGFQSZE|SUBPL245;       /* SUBPOOL NO & STORAGE SIZE     */ 01249000
         LR    R0,IPGFQSZE                                         0242 01250000
         O     R0,SUBPL245                                         0242 01251000
*       R1=IPFMPPTR+IPFMRSZE;       /* ADDR OF STORAGE TO BE FREED   */ 01252000
         L     @03,@PC00004                                        0243 01253000
         LR    R1,IPFMRSZE                                         0243 01254000
         AL    R1,IPFMPPTR(,@03)                                   0243 01255000
*       GEN(FREEMAIN R,LV=(0),A=(1));                              0244 01256000
         FREEMAIN R,LV=(0),A=(1)                                        01257000
*     END;                                                         0245 01258000
*   IPFMPSZE=IPFMRSZE;              /* ACTUAL SIZE OF TABLE ROUNDED     01259000
*                                      UP TO NEXT DWORD              */ 01260000
@RF00240 L     @03,@PC00004+4                                      0246 01261000
         ST    IPFMRSZE,IPFMPSZE(,@03)                             0246 01262000
*   RETURN;                         /* RETURN TO CALLER              */ 01263000
@EL00004 DS    0H                                                  0247 01264000
@EF00004 DS    0H                                                  0247 01265000
@ER00004 LM    @14,@12,@SA00004                                    0247 01266000
         BR    @14                                                 0247 01267000
*   END RMIPFM;                                                    0248 01268000
*   RESPECIFY                                                      0249 01269000
*    (R0,                                                          0249 01270000
*     R1,                                                          0249 01271000
*     R14,                                                         0249 01272000
*     R15) UNRESTRICTED;                                           0249 01273000
*                                                                  0250 01274000
*/********************************************************************/ 01275000
*/*                                                                  */ 01276000
*/*  PROCEDURE NAME: RMIPCKTS                                        */ 01277000
*/*                                                                  */ 01278000
*/*  FUNCTION:                                                       */ 01279000
*/*  THIS ROUTINE CHECKS IF THERE IS SUFFICIENT SPACE IN THE CURRENT */ 01280000
*/*  TABLE TO ACCOMMODATE THE NEXT ENTRY.  IF SPACE IS INSUFFICIENT, */ 01281000
*/*  IT WILL GET STORAGE FOR THE MAX SIZE OF THE TABLE, COPY THE DATA*/ 01282000
*/*  FROM THE OLD TABLE TO THE NEW TABLE, UPDATE TABLE POINTERS TO   */ 01283000
*/*  POINT TO THE NEW TABLE AND FREE THE OLD TABLE.                  */ 01284000
*/*                                                                  */ 01285000
*/*  PARAMETERS FOR RMIPCKTS:                                        */ 01286000
*/*    1. 1 WORD-POINTER TO 1ST BYTE OF CURRENT TABLE                */ 01287000
*/*    2. 1 WORD-SIZE OF CURRENT TABLE                               */ 01288000
*/*    3. 1 WORD-POINTER TO NEXT TABLE ENTRY                         */ 01289000
*/*    4. 1 WORD-SIZE OF NEXT ENTRY                                  */ 01290000
*/*    5. 1 WORD - MAX SIZE OF TABLE                                 */ 01291000
*/*                                                                  */ 01292000
*/********************************************************************/ 01293000
*                                                                  0250 01294000
*RMIPCKTS:                                                         0250 01295000
*   PROC(IPCKCTPT,IPCKCTSZ,IPCKCEPT,IPCKNXES,IPCKMAXS);            0250 01296000
RMIPCKTS STM   @14,@12,@SA00005                                    0250 01297000
         MVC   @PC00005(20),0(@01)                                 0250 01298000
*   DECLARE                                                        0251 01299000
*     IPCKCTPT FIXED,               /* PTR TO 1ST BYTE OF CURRENT  0251 01300000
*                                      TABLE                         */ 01301000
*     IPCKCTSZ FIXED,               /* SIZE OF CURRENT TABLE         */ 01302000
*     IPCKCEPT FIXED,               /* PTR TO NEXT ENTRY             */ 01303000
*     IPCKNXES FIXED,               /* SIXE OF NEXT ENTRY            */ 01304000
*     IPCKMAXS FIXED,               /* MAX SIZE OF TABLE             */ 01305000
*     IPCKOTPT FIXED,               /* PTR TO 1ST BYTE OF OLD TBL    */ 01306000
*     IPCKOTSZ FIXED,               /* SIZE OF OLD TABLE             */ 01307000
*     IPCKOEPT FIXED                /* PTR TO NEXT ENTRY IN OLD TABLE*/ 01308000
*         ;                                                        0251 01309000
*   IF(IPCKCEPT+IPCKNXES)>(IPCKCTPT+IPCKCTSZ) THEN/* SIZE OF       0252 01310000
*                                      ENTRIE+SIZE OF NEXT ENTRY>SIZE   01311000
*                                      OF CURRENT TABLE              */ 01312000
         L     @15,@PC00005+8                                      0252 01313000
         L     @14,IPCKCEPT(,@15)                                  0252 01314000
         L     @07,@PC00005+12                                     0252 01315000
         LR    @06,@14                                             0252 01316000
         AL    @06,IPCKNXES(,@07)                                  0252 01317000
         L     @07,@PC00005                                        0252 01318000
         L     @05,IPCKCTPT(,@07)                                  0252 01319000
         L     @04,@PC00005+4                                      0252 01320000
         L     @03,IPCKCTSZ(,@04)                                  0252 01321000
         LR    @02,@05                                             0252 01322000
         ALR   @02,@03                                             0252 01323000
         CR    @06,@02                                             0252 01324000
         BNH   @RF00252                                            0252 01325000
*     DO;                           /* CONSTRUCT TABLE OF MAX SIZE   */ 01326000
*       IPCKOTPT=IPCKCTPT;          /* SAVE PTR TO CURRENT TABLE     */ 01327000
         LR    IPCKOTPT,@05                                        0254 01328000
*       IPCKOTSZ=IPCKCTSZ;          /* SAVE SIZE OF CURRENT TABLE    */ 01329000
         L     @06,@PC00005+4                                      0255 01330000
         L     @06,IPCKCTSZ(,@06)                                  0255 01331000
         LR    IPCKOTSZ,@06                                        0255 01332000
*       IPCKOEPT=IPCKCEPT;          /* SAVE PTR TO NEXT ENTRY IN   0256 01333000
*                                      CURRENT TABLE                 */ 01334000
         ST    @14,IPCKOEPT                                        0256 01335000
*       IPCKCTSZ=IPCKMAXS;          /* MAX SIZE OF TABLE             */ 01336000
         L     @05,@PC00005+16                                     0257 01337000
         L     @05,IPCKMAXS(,@05)                                  0257 01338000
         ST    @05,IPCKCTSZ(,@04)                                  0257 01339000
*       CALL RMIPGM(IPCKCTPT,IPCKCTSZ,IPCKCEPT);/* GET STORAGE FOR 0258 01340000
*                                      TABLE OF MAX SIZE             */ 01341000
         ST    @07,@AL00001                                        0258 01342000
         ST    @04,@AL00001+4                                      0258 01343000
         ST    @15,@AL00001+8                                      0258 01344000
         LA    @01,@AL00001                                        0258 01345000
         BAL   @14,RMIPGM                                          0258 01346000
*/*  MOVE DATA TO NEW TABLE                                          */ 01347000
*                                                                  0259 01348000
*       RESPECIFY                                                  0259 01349000
*        (R0,                                                      0259 01350000
*         R1,                                                      0259 01351000
*         R14,                                                     0259 01352000
*         R15) RSTD;                                               0259 01353000
*       R0=IPCKCTPT;                /* ADDR OF NEW TABLE             */ 01354000
         L     @07,@PC00005                                        0260 01355000
         L     R0,IPCKCTPT(,@07)                                   0260 01356000
*       R14=IPCKOTPT;               /* ADDR OF 1ST BYTE OF OLD TBL   */ 01357000
         LR    R14,IPCKOTPT                                        0261 01358000
*       R1=IPCKOEPT-IPCKOTPT;       /* SIZE OF DATA FIELD TO BE    0262 01359000
*                                      COPIED                        */ 01360000
         L     R1,IPCKOEPT                                         0262 01361000
         SLR   R1,IPCKOTPT                                         0262 01362000
*       R15=R1;                     /* SIZE OF DATA FIELD            */ 01363000
         LR    R15,R1                                              0263 01364000
*       GEN REFS(R0,R1,R14,R15)( MVCL R0,R14 );                    0264 01365000
          MVCL R0,R14                                                   01366000
*       IPCKCEPT=R0;                /* PTR TO NEXT ENTRY IN NEW TABLE*/ 01367000
         L     @07,@PC00005+8                                      0265 01368000
         ST    R0,IPCKCEPT(,@07)                                   0265 01369000
*/*  FREE OLD TABLE                                                  */ 01370000
*                                                                  0266 01371000
*       R0=IPCKOTSZ|SUBPL245;       /* SUBPOOL NO. & STORAGE SIZE    */ 01372000
         LR    R0,IPCKOTSZ                                         0266 01373000
         O     R0,SUBPL245                                         0266 01374000
*       R1=IPCKOTPT;                /* ADDR OF OLD TABLE             */ 01375000
         LR    R1,IPCKOTPT                                         0267 01376000
*       GEN( FREEMAIN R,LV=(0),A=(1) );                            0268 01377000
          FREEMAIN R,LV=(0),A=(1)                                       01378000
*     END;                                                         0269 01379000
*   RETURN;                         /* RETURN TO CALLER              */ 01380000
@EL00005 DS    0H                                                  0270 01381000
@EF00005 DS    0H                                                  0270 01382000
@ER00005 LM    @14,@12,@SA00005                                    0270 01383000
         BR    @14                                                 0270 01384000
*   END RMIPCKTS;                                                  0271 01385000
         B     @EL00005                                            0271 01386000
*   RESPECIFY                                                      0272 01387000
*    (R0,                                                          0272 01388000
*     R1,                                                          0272 01389000
*     R14,                                                         0272 01390000
*     R15) UNRSTD;                                                 0272 01391000
*                                                                  0273 01392000
*/********************************************************************/ 01393000
*/*                                                                  */ 01394000
*/*  PROCEDURE NAME: RMIPINIT                                        */ 01395000
*/*                                                                  */ 01396000
*/*   PURPOSE     = TO PROCESS DATA FROM THE IEAIPSXX LIST           */ 01397000
*/*   LINKAGE     = VIA CALL                                         */ 01398000
*/*   INPUT       = ADDR OF A 1-BYTE FIELD CONTAINING THE PROCESSING */ 01399000
*/*                 SUBROUTINE NUMBER                                */ 01400000
*/*   INVOKER     = IRBMFANL                                         */ 01401000
*/*   EXIT-NORMAL = RETURN TO CALLER                                 */ 01402000
*/*    OUTPUT DATA= NONE                                             */ 01403000
*/*                                                                  */ 01404000
*/********************************************************************/ 01405000
*                                                                  0273 01406000
*RMIPINIT:                                                         0273 01407000
*   PROC(IPCODEPT);                                                0273 01408000
RMIPINIT STM   @14,@12,@SA00006                                    0273 01409000
         MVC   @PC00006(4),0(@01)                                  0273 01410000
*   DECLARE                                                        0274 01411000
*     IPCODEPT PTR(31),             /* POINTER TO CODE               */ 01412000
*     IPBRINDX FIXED(8) BASED(IPCODEPT),/* INDEX TO BRANCH TABLE     */ 01413000
*/*  IPBRTAB IS A TABLE CONTAINING THE ADDRESSES OF THE SUBROUTINES  */ 01414000
*/*  WHICH PROCESS IPS DATA.  THE DIMENSION OF IPBRTAB IS THE NUMBER */ 01415000
*/*  OF SUBROUTINES.  THE INPUT PARAMETER TO THE RMIPINIT PROCEDURE  */ 01416000
*/*  IS USED TO ACCESS THE SUBSCRIPT FOR IPBRTAB                     */ 01417000
*                                                                  0274 01418000
*     IPBRTAB(31) PTR INIT(ADDR(RMIP001),ADDR(RMIP002),ADDR(RMIP003),   01419000
*         ADDR(RMIP004),ADDR(RMIP005),ADDR(RMIP006),ADDR(RMIP007),ADDR( 01420000
*         RMIP008),ADDR(RMIP009),ADDR(RMIP010),ADDR(RMIP011),ADDR( 0274 01421000
*         RMIP012),ADDR(RMIP013),ADDR(RMIP014),ADDR(RMIP015),ADDR( 0274 01422000
*         RMIP016),ADDR(RMIP017),ADDR(RMIP018),ADDR(RMIP019),ADDR( 0274 01423000
*         RMIP020),ADDR(RMIP021),ADDR(RMIP022),ADDR(RMIP023),ADDR( 0274 01424000
*         RMIP024),                 /*                       @Z40BPEM*/ 01425000
*         ADDR(RMIP025),            /*                       @Z40BPEM*/ 01426000
*         ADDR(RMIP026),            /*                       @Z40BPEM*/ 01427000
*         ADDR(RMIP027),            /*                       @Z40BPEM*/ 01428000
*         ADDR(RMIP028),            /*                       @Z40BPEM*/ 01429000
*         ADDR(RMIP029),            /*                       @Z40BPEM*/ 01430000
*         ADDR(RMIP030),            /*                       @Z40BPEM*/ 01431000
*         ADDR(RMIP031)),           /*                       @Z40BPEM*/ 01432000
*     RMIPLBL LABEL BASED(IPBRTAB(IPBRINDX))/* LABEL FOR PROCESSING     01433000
*                                      SUBROUTINE                    */ 01434000
*         ;                                                        0274 01435000
*   DECLARE                                                        0275 01436000
*     K FIXED(31) CONSTANT(1000),   /* 1K                            */ 01437000
*     MILLISEC FIXED(31) CONSTANT(1024),/* CONSTANT USED TO CONVERT A   01438000
*                                      SECOND TO A MILLISECOND       */ 01439000
*     CPU FIXED CONSTANT(1),        /* CPU SERVICE COEFF INDEX NO.   */ 01440000
*     IOC FIXED CONSTANT(2),        /* IOC SERVICE COEFF INDEX NO.   */ 01441000
*     MSO FIXED CONSTANT(3),        /* MSO SERVICE COEFF INDEX NO.   */ 01442000
*     CSTMIN FIXED CONSTANT(1),     /* MIN INDEX #           @Z40BPEM*/ 01443000
*     CSTMAX FIXED CONSTANT(2),     /* MAX INDEX #           @Z40BPEM*/ 01444000
*     CSTWT FIXED CONSTANT(3),      /* WT INDEX #            @Z40BPEM*/ 01445000
*     MINWKLC FIXED(31) CONSTANT(2),/* MIN WORKLOAD LEVEL COUNT      */ 01446000
*     MINWKLN FIXED(31) CONSTANT(1),/* MIN WORKLOAD LEVEL NO         */ 01447000
*     MAXWKLN FIXED(31) CONSTANT(128),/* MAX WORKLOAD LEVEL NUMBER   */ 01448000
*     MAXSRV FIXED(31) CONSTANT(9999),/* MAX SERVICE RATE NO.        */ 01449000
*     MINPGN FIXED(31) CONSTANT(1), /* MIN PERFORMANCE GROUP NO.     */ 01450000
*     MINOBJ FIXED(31) CONSTANT(1), /* MIN OBJECTIVE NO.             */ 01451000
*     MINDMN FIXED(31) CONSTANT(1), /* MIN DMN #             @Z40BPEM*/ 01452000
*     MAXDUR FIXED(31) CONSTANT(999999999),/* MAX DURATION NO.       */ 01453000
*     MAXDURK FIXED(31) CONSTANT(999999),/* MAX DURATION NO. IN K  0275 01454000
*                                      UNITS                         */ 01455000
*     MAXISV FIXED(31) CONSTANT(999999),/* MAX INTERVAL SERVICE VALUE*/ 01456000
*     MAXISVK FIXED(31) CONSTANT(999),/* MAX INTERVAL SERVICE VALUE     01457000
*                                      IN K UNITS                    */ 01458000
*     MINRTB FIXED(31) CONSTANT(0), /* MIN RTB VALUE                 */ 01459000
*     MAXRTB FIXED(31) CONSTANT(1), /* MAX RTB VALUE                 */ 01460000
*     MAXRDURC FIXED(31) CONSTANT(1000000),/* MAX NO. IN SECS FOR  0275 01461000
*                                      PERF GRP PERIOD DURATION      */ 01462000
*     MAXCSTC FIXED(31) CONSTANT(3),/* MAX # OF CNSTRS       @Z40BPEM*/ 01463000
*     MAXAPG FIXED CONSTANT(15),    /* UPPER LIMIT ON APG    @Z40BPEM*/ 01464000
*     CNSTRLB FIXED,                /* LOWER BOUND ON CNSTRS @Z40BPEM*/ 01465000
*     CNSTRUB FIXED CONSTANT(255),  /* UPPER LIMIT ON CNSTRS @Z40BPEM*/ 01466000
*     INDX FIXED,                   /* WORK AREA             @Z40BPEM*/ 01467000
*     RUNIT BIT(1) CONSTANT('1'B)   /* REAL TIME UNIT CODE           */ 01468000
*         ;                                                        0275 01469000
*   DECLARE                                                        0276 01470000
*     NEGSRV FIXED(15) INIT(-32768),/* NEGATIVE SERVICE RATE         */ 01471000
*/*  PERF OBJ ENTRY FOR THE POINT FOR MAX SERV RATE & WKL=0          */ 01472000
*                                                                  0276 01473000
*     1 MAXSRVPT STATIC LOCAL,      /* WPOD ENTRY FOR MAX SERVICE  0276 01474000
*                                      RATE AND WKL=0                */ 01475000
*      2 * FIXED(15) INIT(32767),   /* MAX SERVICE RATE              */ 01476000
*      2 * FIXED(16) UNSIGNED INIT(0),/* WKL=0                       */ 01477000
*/*  PERIOD DURATION MASK FOR 8 PERIODS                              */ 01478000
*                                                                  0276 01479000
*     PDURMASK(8) FIXED(32) STATIC LOCAL INIT(1,/* PERIOD 1          */ 01480000
*         2,                        /* PERIOD 2                      */ 01481000
*         4,                        /* PERIOD 3                      */ 01482000
*         8,                        /* PERIOD 4                      */ 01483000
*         16,                       /* PERIOD 5                      */ 01484000
*         32,                       /* PERIOD 6                      */ 01485000
*         64,                       /* PERIOD 7                      */ 01486000
*         128),                     /* PERIOD 8                      */ 01487000
*/*  PERIOD DOMAIN MASK FOR 8 PERIODS                        @Z40BPEM*/ 01488000
*                                                                  0276 01489000
*     PDMNMASK(8) FIXED(32) STATIC LOCAL INIT(1,/* PERIOD 1  @Z40BPEM*/ 01490000
*         2,                        /* PERIOD 2              @Z40BPEM*/ 01491000
*         4,                        /* PERIOD 3              @Z40BPEM*/ 01492000
*         8,                        /* PERIOD 4              @Z40BPEM*/ 01493000
*         16,                       /* PERIOD 5              @Z40BPEM*/ 01494000
*         32,                       /* PERIOD 6              @Z40BPEM*/ 01495000
*         64,                       /* PERIOD 7              @Z40BPEM*/ 01496000
*         128),                     /* PERIOD 8              @Z40BPEM*/ 01497000
*/*  DOMAIN CONSTRAINTS MASK                                 @Z40BPEM*/ 01498000
*                                                                  0276 01499000
*     DCSTMASK(3) FIXED(32) STATIC LOCAL INIT(1,/* MIN IS SPECIFIED     01500000
*                                                            @Z40BPEM*/ 01501000
*         2,                        /* MAX IS SPECIFIED      @Z40BPEM*/ 01502000
*         4),                       /* WT IS SPECIFIED       @Z40BPEM*/ 01503000
*/*  DEFAULT PERF GROUP PERIOD ENTRY                                 */ 01504000
*                                                                  0276 01505000
*     1 DEFWPGP CHAR(8) BDY(WORD) STATIC LOCAL,/*            @Z40BPEM*/ 01506000
*      2 DPGPOBJN PTR(08) INIT(0),  /* OBJ NO.=0                     */ 01507000
*      2 DPGPDMN PTR(8) INIT(1),    /* DMN NO.=1             @Z40BPEM*/ 01508000
*      2 * CHAR(2) INIT(''),        /* RESERVED              @Z40BPEM*/ 01509000
*      2 DPGPAPGV PTR(8) INIT(0),   /* APG DISP. PRTY.       @Z40BPEM*/ 01510000
*      2 DPGPINDS BIT(8) INIT('00000000'B),/* RTB = 0        @ZM43078*/ 01511000
*      2 DPGPISVP PTR(08) INIT('01'X),/* INDEX TO IPWPGL ENTRY WHICH    01512000
*                                      CONTAINS THE BASE ISV         */ 01513000
*      2 DPGPDURP PTR(08) INIT(0),  /* PERIOD DUR=0                  */ 01514000
*/*  DEFAULT DOMAIN TABLE ENTRY                              @Z40BPEM*/ 01515000
*                                                                  0276 01516000
*     1 DEFDMDE CHAR(20) BDY(WORD) STATIC LOCAL,/*           @Z40BPEM*/ 01517000
*      2 DMDNO FIXED(8) INIT(1),    /* DOMAIN NO.            @Z40BPEM*/ 01518000
*      2 DMDLO FIXED(8) INIT(1),    /* MIN MPL NO.           @Z40BPEM*/ 01519000
*      2 DMDHI FIXED(8) INIT(255),  /* MAX MPL NO.           @Z40BPEM*/ 01520000
*      2 DMDWT FIXED(8) INIT(1),    /* WEIGHTING FACTOR      @Z40BPEM*/ 01521000
*      2 DMDMPLT FIXED(15) INIT(1), /* MPL TARGET            @Z40BPEM*/ 01522000
*      2 DMDGOOU FIXED(15) INIT(0), /* CURRENT # OF USERS GOING OUT     01523000
*                                                            @Z40BPEM*/ 01524000
*      2 DMDCMPL FIXED(15) INIT(0), /* CURRENT MPL           @Z40BPEM*/ 01525000
*      2 DMDOUTU FIXED(15) INIT(0), /* CURRENT # USERS ON OUT QUEUE     01526000
*                                                            @Z40BPEM*/ 01527000
*      2 DMDINCU FIXED(15) INIT(0), /* CURRENT # OF SWAPPABLE IN CORE   01528000
*                                      USERS                 @Z40BPEM*/ 01529000
*      2 DMDRUA FIXED(15) INIT(0),  /* AVE # OF READY USERS  @Z40BPEM*/ 01530000
*      2 DMDRUC FIXED(31) INIT(0);  /* ACCUMULATOR FOR READY USER  0276 01531000
*                                      AVERAGE               @Z40BPEM*/ 01532000
*   DECLARE                                                        0277 01533000
*     WPGPPTR PTR(31),              /* PTR TO PERF GRP PERIOD        */ 01534000
*     WPGLPTR PTR(31),              /* PTR TO WPGL                   */ 01535000
*     IPWDURVL FIXED(31),           /* MAX VALUE FOR DURATION        */ 01536000
*     IPWISVL FIXED(31),            /* MAX VALUE FOR INTERVAL SERVICE   01537000
*                                      VALUE                         */ 01538000
