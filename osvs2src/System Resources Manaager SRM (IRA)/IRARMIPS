*    PARAMETERS FOR RMIPINIT PROCEDURE                               *  06472000
*    THE PARAMETER IS USED TO INDEX INTO A BRANCH TABLE, SO THAT     *  06473000
*    CONTROL IS GIVEN TO THE APPROPRIATE ROUTINE TO PROCESS THE DATA *  06474000
IPGRC001 DC   FL1'1'                                                    06475000
IPGRC002 DC   FL1'2'                                                    06476000
IPGRC003 DC   FL1'3'                                                    06477000
IPGRC004 DC   FL1'4'                                                    06478000
IPGRC005 DC   FL1'5'                                                    06479000
IPGRC006 DC   FL1'6'                                                    06480000
IPGRC007 DC   FL1'7'                                                    06481000
IPGRC008 DC   FL1'8'                                                    06482000
IPGRC009 DC   FL1'9'                                                    06483000
IPGRC010 DC   FL1'10'                                                   06484000
IPGRC011 DC   FL1'11'                                                   06485000
IPGRC012 DC   FL1'12'                                                   06486000
IPGRC013 DC   FL1'13'                                                   06487000
IPGRC014 DC   FL1'14'                                                   06488000
IPGRC015 DC   FL1'15'                                                   06489000
IPGRC016 DC   FL1'16'                                                   06490000
IPGRC017 DC   FL1'17'                                                   06491000
IPGRC018 DC   FL1'18'                                                   06492000
IPGRC019 DC   FL1'19'                                                   06493000
IPGRC020 DC   FL1'20'                                                   06494000
IPGRC021 DC   FL1'21'                                                   06495000
IPGRC022 DC   FL1'22'                                                   06496000
IPGRC023 DC   FL1'23'                                                   06497000
IPGRC024 DC   FL1'24'                                        @Z40BPEM*  06498000
IPGRC025 DC   FL1'25'                                        @Z40BPEM*  06499000
IPGRC026 DC   FL1'26'                                        @Z40BPEM*  06500000
IPGRC027 DC   FL1'27'                                        @Z40BPEM*  06501000
IPGRC028 DC   FL1'28'                                        @Z40BPEM*  06502000
IPGRC029 DC   FL1'29'                                        @Z40BPEM*  06503000
IPGRC030 DC   FL1'30'                                        @Z40BPEM*  06504000
IPGRC031 DC   FL1'31'                                        @Z40BPEM*  06505000
*    PARAMETERS FOR MFRINTGR PROCEDURE                               *  06506000
*    THE PARAMETER IS USED AS AN INDEX TO A TABLE WHICH CONTAINS     *  06507000
*    THE ADDRESS OF THE LOCATION WHERE THE INTEGER VALUE IS TO BE    *  06508000
*    STORED                                                          *  06509000
IPINT001 EQU   IPGRC001                                                 06510000
IPINT002 EQU   IPGRC002                                                 06511000
IPINT003 EQU   IPGRC003                                                 06512000
IPINT004 EQU   IPGRC004                                                 06513000
IPINT005 EQU   IPGRC005                                      @Z40BPEM*  06514000
*    PARAMETER LISTS FOR MFRTEXT PROCEDURE                           *  06515000
*    PARAMETERS FOR MFRTEXT -                                        *  06516000
*      (1) A 1-BYTE FIELD CONTAINING THE LENGTH OF THE TEXT TO BE    *  06517000
*          MATCHED                                                   *  06518000
*      (2) THE TEXT IN EBCDIC                                        *  06519000
IPSYMCPU DC   FL1'3'                                                    06520000
         DC    CL3'CPU'                  KEYWORD=CPU                    06521000
IPSYMIOC DC   FL1'3'                                                    06522000
         DC    CL3'IOC'                  KEYWORD=IOC                    06523000
IPSYMMSO DC   FL1'3'                                                    06524000
         DC    CL3'MSO'                  KEYWORD=MSO                    06525000
IPSYMWKL DC   FL1'3'                                                    06526000
         DC    CL3'WKL'                  KEYWORD=WKL                    06527000
IPSYMSRV DC   FL1'3'                                                    06528000
         DC    CL3'SRV'                  KEYWORD=SRV                    06529000
IPSYMOBJ DC   FL1'3'                                                    06530000
         DC    CL3'OBJ'                  KEYWORD=OBJ                    06531000
IPSYMDMN DC   FL1'3'                                         @Z40BPEM*  06532000
         DC    CL3'DMN'                  KEYWORD=DMN         @Z40BPEM*  06533000
IPSYMCST DC   FL1'5'                                         @Z40BPEM*  06534000
         DC    CL5'CNSTR'                KEYWORD=CNSTR       @Z40BPEM*  06535000
IPSYMAPG DC   FL1'3'                                         @Z40BPEM*  06536000
         DC    CL3'APG'                  KEYWORD=APG         @Z40BPEM*  06537000
IPSYMPGN DC   FL1'3'                                                    06538000
         DC    CL3'PGN'                  KEYWORD=PGN                    06539000
IPSYMDUR DC   FL1'3'                                                    06540000
         DC    CL3'DUR'                  KEYWORD=DUR                    06541000
IPSYMISV DC   FL1'3'                                                    06542000
         DC    CL3'ISV'                  KEYWORD=ISV                    06543000
IPSYMRTB DC   FL1'3'                                                    06544000
         DC    CL3'RTB'                  KEYWORD=RTB                    06545000
IPSYMUNT DC   FL1'3'                                                    06546000
         DC    CL3'UNT'                  KEYWORD=UNT                    06547000
IPSYMK   DC   FL1'1'                                                    06548000
         DC    CL1'K'                    KEYWORD=K                      06549000
IPSYMS   DC   FL1'1'                                                    06550000
         DC    CL1'S'                    KEYWORD=S                      06551000
IPSYMR   DC   FL1'1'                                                    06552000
         DC    CL1'R'                    KEYWORD=R                      06553000
IPSYMLPR DC   FL1'1'                                                    06554000
         DC    CL1'('                    KEYWORD=(                      06555000
IPSYMRPR DC   FL1'1'                                                    06556000
         DC    CL1')'                    KEYWORD=)                      06557000
IPSYMCOM DC   FL1'1'                                                    06558000
         DC    CL1','                    KEYWORD=,                      06559000
IPSYMPER DC   FL1'1'                                                    06560000
         DC    CL1'.'                    KEYWORD=.                      06561000
IPSYMAST DC   FL1'1'                                                    06562000
         DC    CL1'*'                    KEYWORD=*                      06563000
IPSYMEQ  DC   FL1'1'                                                    06564000
         DC    CL1'='                    KEYWORD==                      06565000
*    PARAMETER LISTS FOR MFRBLANK PROCEDURE                          *  06566000
IPBLNK   DC    AL.1(0)                   INDICATOR FOR SKIPPING ZERO    06567000
*                                        OR MORE BLANKS                 06568000
IPBLNK1  DC    AL.1(1)                   INDICATOR FOR SKIPPING AT      06569000
*                                        LEAST 1 BLANK                  06570000
*    PARAMETER LISTS FOR MFRRERR PROCEDURE                           *  06571000
IPERR    DC    AL.1(0)                   INDICATOR FOR SKIPPING TO      06572000
*                                        TO A BLANK, A COMMA OR EOF     06573000
         DC    FL1'0'                    RESERVED                       06574000
         DC    FL1'0'                    RESERVED                       06575000
IPERR1   DC    AL.1(1)                   INDICATOR FOR SKIPPING TO      06576000
*                                        A BLANK, A COMMA, A ) OR EOF   06577000
         DC    FL1'0'                    RESERVED                       06578000
         DC    FL1'0'                    RESERVED                       06579000
IPSYNTE  DS    0F                                                       06580000
@DATD    DSECT                                                          06581000
         ORG   *+1-(*-@DATD)/(*-@DATD) INSURE DSECT DATA                06582000
@ENDDATD EQU   *                                                        06583000
IRARMIPS CSECT                                                          06584000
@00      EQU   00                      EQUATES FOR REGISTERS 0-15       06585000
@01      EQU   01                                                       06586000
@02      EQU   02                                                       06587000
@03      EQU   03                                                       06588000
@04      EQU   04                                                       06589000
@05      EQU   05                                                       06590000
@06      EQU   06                                                       06591000
@07      EQU   07                                                       06592000
@08      EQU   08                                                       06593000
@09      EQU   09                                                       06594000
@10      EQU   10                                                       06595000
@11      EQU   11                                                       06596000
@12      EQU   12                                                       06597000
@13      EQU   13                                                       06598000
@14      EQU   14                                                       06599000
@15      EQU   15                                                       06600000
BUBCBARA EQU   @02                                                      06601000
FRCONTRL EQU   @02                                                      06602000
FRFREBCB EQU   @03                                                      06603000
RRERRSCN EQU   @02                                                      06604000
RRTXTPTR EQU   @02                                                      06605000
