@RF00065 IC    @11,RMCABCPG(,RMCTPTR)                              0067 00138000
         STC   @11,OUCBNPG(,OUCBPTR)                               0067 00139000
*       /*************************************************************/ 00140000
*       /*                                                           */ 00141000
*       /* START A NEW TRANSACTION FOR EVERY USER.           @ZA27109*/ 00142000
*       /* ? IRACTLCL EPN(WMN)                                       */ 00143000
*       /*                                                           */ 00144000
*       /*************************************************************/ 00145000
*                                                                  0068 00146000
*       CALL RMSBWMN->IRARMRTN;     /* INVOKE WMN SUBRTN             */ 00147000
@RC00065 DS    0H                                                  0068 00148000
@RF00064 L     @15,RMSBWMN(,RMCTPTR)                               0068 00149000
