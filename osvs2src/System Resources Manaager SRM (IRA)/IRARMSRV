*                                                                  0092 00124000
*           GENERATE CODE REFS(CVTPTR,CVTMAP,CVTGETCL)             0092 00125000
*               (GETCELL CPID=(0),BRANCH=YES,SAVE=NO);             0092 00126000
         GETCELL CPID=(0),BRANCH=YES,SAVE=NO                            00127000
*           IF RTNCOD^=CON0 THEN    /* NO FREE CELLS IN THE POOL (4)    00128000
*                                      POOL CHAIN POINTERS DESTROYED    00129000
*                                      (8) OR FIRST POOL NOT BUILT 0093 00130000
*                                      (12). (CODE 16 SHOULDN'T    0093 00131000
*                                      OCCUR)                @Z40BPJC*/ 00132000
*                                                                  0093 00133000
         LTR   RTNCOD,RTNCOD                                       0093 00134000
         BZ    @RF00093                                            0093 00135000
*             /*******************************************************/ 00136000
*             /*                                                     */ 00137000
*             /* EXPAND THE POOL TO MEET THE INCREASED DEMAND FOR    */ 00138000
*             /* STORAGE BY ADDING A 2K BYTE EXTENSION TO THE        */ 00139000
*             /* EXISTING POOL.                                      */ 00140000
*             /*                                                     */ 00141000
*             /*******************************************************/ 00142000
*                                                                  0094 00143000
*             DO;                                                  0094 00144000
*               R0=BLOCKLEN;        /* LENGTH OF BLOCK TO GETMAIN    */ 00145000
         LA    R0,2048                                             0095 00146000
*               CALL GETSTOR;       /* GET STORAGE                   */ 00147000
         BAL   @14,GETSTOR                                         0096 00148000
*               IF RTNCOD=CON0 THEN /* STORAGE FOR EXTENSION OBTAINED*/ 00149000
         LTR   RTNCOD,RTNCOD                                       0097 00150000
         BNZ   @RF00097                                            0097 00151000
*                 DO;               /* ADD STORAGE TO EXISTING POOL  */ 00152000
*                   RMCTPTR=CVTOPCTP;/* RESTORE RMCT POINTER         */ 00153000
         L     @12,CVTPTR                                          0099 00154000
         L     RMCTPTR,CVTOPCTP(,@12)                              0099 00155000
*                   R0=RMCTCPID;    /* CELL POOL ID OF SRM STORAGE   */ 00156000
         L     R0,RMCTCPID(,RMCTPTR)                               0100 00157000
