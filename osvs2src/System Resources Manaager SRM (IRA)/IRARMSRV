*     ERRETPT,                                                     0144 00384000
*     ASCBADD) RSTD;                                               0144 00385000
*   COMPCODE=R0;                    /* COMPLETION CODE       @ZA16887*/ 00386000
         LR    COMPCODE,R0                                         0145 00387000
*   ECBADDR=R1|                     /* ADDRESS OF ECB TO XMPOST      */ 00388000
*       HIORDER;                    /* XMPOST TO BE PERFORMED        */ 00389000
         L     @07,@CF02352                                        0146 00390000
         LR    ECBADDR,R1                                          0146 00391000
         OR    ECBADDR,@07                                         0146 00392000
*   ERRETPT=RMEXXPE|                /* ERROR ENTRY POINT ADDRESS     */ 00393000
*       HIORDER;                    /* DO NOT SCHEDULE ERROR ROUTINE    00394000
*                                      IN THE WAIT TASK MEMORY       */ 00395000
         O     @07,RMEXXPE(,RMCTPTR)                               0147 00396000
         LR    ERRETPT,@07                                         0147 00397000
*   ASCBADD=ASCBPTR;                /* ASCB ADDRESS OF MEM TO XMPOST */ 00398000
         LR    ASCBADD,ASCBPTR                                     0148 00399000
*   CALL XMPOST;                    /* XMPOST MEMORY                 */ 00400000
         L     @07,CVTPTR                                          0149 00401000
         L     @15,CVT0PT01(,@07)                                  0149 00402000
         BALR  @14,@15                                             0149 00403000
*   RTNCOD=CON0;                    /* INDICATE SUCCESSFUL COMPLETION*/ 00404000
         SLR   RTNCOD,RTNCOD                                       0150 00405000
*   RESPECIFY                                                      0151 00406000
*    (COMPCODE,                                                    0151 00407000
*     ECBADDR,                                                     0151 00408000
