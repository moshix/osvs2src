        BNE RTMRETN                                                     00794000
*           IF ASCBASCB='ASCB' THEN /* POINTS TO AN ASCB?    @ZA16887*/ 00795000
         CLC   ASCBASCB(4,ASCBPTR),@CC02438                        0240 00796000
         BNE   @RF00240                                            0240 00797000
*             DO;                   /* YES, SO SET UP DUMP BUFFER  0241 00798000
*                                                            @ZA16887*/ 00799000
*               WRKREG1=CVTSDBF;    /* ADDRESS OF BUFFER     @ZA16887*/ 00800000
         L     @12,CVTPTR                                          0242 00801000
         L     WRKREG1,CVTSDBF(,@12)                               0242 00802000
*               RFY                                                0243 00803000
*                 WRKREG2 RSTD;     /* HOLD CS REGISTER      @ZA16887*/ 00804000
*SDBFLOOP:                                                         0244 00805000
*               IF CVTSDBF>CON0 THEN/* BUFFER FREE TO USE?   @ZA16887*/ 00806000
SDBFLOOP L     @12,CVTPTR                                          0244 00807000
         L     @12,CVTSDBF(,@12)                                   0244 00808000
         LTR   @12,@12                                             0244 00809000
         BNP   @RF00244                                            0244 00810000
*                 DO;               /* YES, ATTEMPT TO RESERVE IT  0245 00811000
*                                                            @ZA16887*/ 00812000
*                   WRKREG2=CVTSDBF|SDMPMASK;/* SET HI-ORDER BIT   0246 00813000
*                                                            @ZA16887*/ 00814000
         O     @12,@CF02352                                        0246 00815000
         LR    WRKREG2,@12                                         0246 00816000
*                   CS(WRKREG1,WRKREG2,CVTSDBF);/* SERIALIZE FIELD 0247 00817000
*                                                            @ZA16887*/ 00818000
         L     @12,CVTPTR                                          0247 00819000
         CS    WRKREG1,@07,CVTSDBF(@12)                            0247 00820000
*                   BC(7,SDBFLOOP); /* TRY AGAIN IF UPDATE FAILED  0248 00821000
*                                                            @ZA16887*/ 00822000
         BC    7,SDBFLOOP                                          0248 00823000
*                   RFY                                            0249 00824000
*                     WRKREG2 UNRSTD;/* DONE WITH REGISTER   @ZA16887*/ 00825000
*                                                                  0249 00826000
*                   /*************************************************/ 00827000
*                   /*                                               */ 00828000
*                   /* INITIALIZE SDUMP BUFFER AND COPY THE ASCB AND */ 00829000
*                   /* OUCB INTO IT PRIOR TO TERMINATING THE ADDRESS */ 00830000
*                   /* SPACE                                 @ZA16887*/ 00831000
*                   /*                                               */ 00832000
*                   /*************************************************/ 00833000
*                                                                  0250 00834000
*                   SDMPDPTR=ADDR(SDMPDATA);/* POINTER TO DATA     0250 00835000
*                                                            @ZA16887*/ 00836000
         L     @12,CVTPTR                                          0250 00837000
         L     @12,CVTSDBF(,@12)                                   0250 00838000
         LA    @07,SDMPDATA(,@12)                                  0250 00839000
         ST    @07,SDMPDPTR(,@12)                                  0250 00840000
*                   SDMPLENG=LENGTH(SDMPDATA);/* LENGTH OF DATA    0251 00841000
*                                                            @ZA16887*/ 00842000
         MVC   SDMPLENG(2,@12),@CH02447                            0251 00843000
*                   SDMPFLGS=CON0;  /* INITIALIZE FLAG FIELD @ZA16887*/ 00844000
         SLR   @07,@07                                             0252 00845000
         STH   @07,SDMPFLGS(,@12)                                  0252 00846000
*                   SDMPEND=''B;    /* INDICATE END OF DATA  @ZA16887*/ 00847000
         XC    SDMPEND(6,@12),SDMPEND(@12)                         0253 00848000
*                   SDMPAPTR=ASCBPTR;/* VIRTUAL ADDRESS OF ASCB    0254 00849000
*                                                            @ZA16887*/ 00850000
         ST    ASCBPTR,SDMPAPTR(,@12)                              0254 00851000
*                   SDMPASCB=ASCB;  /* COPY ASCB             @ZA16887*/ 00852000
         MVC   SDMPASCB(208,@12),ASCB(ASCBPTR)                     0255 00853000
*                   SDMPABIT='1'B;  /* INDICATE ASCB WAS COPIED    0256 00854000
*                                                            @ZA16887*/ 00855000
         OI    SDMPABIT(@12),B'10000000'                           0256 00856000
*                   OUCBPTR=ASCBOUCB;/* GET OUCB ADDRESS     @ZA16887*/ 00857000
         L     OUCBPTR,ASCBOUCB(,ASCBPTR)                          0257 00858000
*                   IF OUCBPTR^=CON0 THEN/* IS ADDRESS VALID?      0258 00859000
*                                                            @ZA16887*/ 00860000
         CR    OUCBPTR,@07                                         0258 00861000
         BE    @RF00258                                            0258 00862000
*                     DO;           /* CHECK BEGINNING AND END ADDR     00863000
*                                                            @ZA16887*/ 00864000
*                       GEN CODE REFS(WRKREG2,OUCBPTR,OUCB,OUCBEND,     00865000
*                           DUMPBUFF);                             0260 00866000
              LRA WRKREG2,OUCB(,OUCBPTR)                                00867000
              BNE DUMPBUFF                                              00868000
              LRA WRKREG2,OUCBEND-1(,OUCBPTR)                           00869000
              BNE DUMPBUFF                                              00870000
*                       SDMPOPTR=OUCBPTR;/* VIRTUAL ADDRESS OF OUCB     00871000
*                                                            @ZA16887*/ 00872000
         L     @12,CVTPTR                                          0261 00873000
         L     @12,CVTSDBF(,@12)                                   0261 00874000
         ST    OUCBPTR,SDMPOPTR(,@12)                              0261 00875000
*                       SDMPOUCB=OUCB;/* COPY OUCB           @ZA16887*/ 00876000
         MVC   SDMPOUCB(136,@12),OUCB(OUCBPTR)                     0262 00877000
*                       SDMPOBIT='1'B;/* INDICATE OUCB WAS COPIED  0263 00878000
*                                                            @ZA16887*/ 00879000
         OI    SDMPOBIT(@12),B'01000000'                           0263 00880000
*                     END;          /* DONE WITH OUCB        @ZA16887*/ 00881000
*DUMPBUFF:                                                         0265 00882000
*                   ASIDREG=CON1;   /* SDUMP TO RUN IN MASTER A.S. 0265 00883000
*                                                            @ZA16887*/ 00884000
@RF00258 DS    0H                                                  0265 00885000
DUMPBUFF LA    ASIDREG,1                                           0265 00886000
*                   GEN CODE REFS(ASIDREG);                        0266 00887000
*                                   /* SCHEDULE SDUMP       @ZA16887*/  00888000
  SDUMP HDR='SRM - IRARMSRV 55F ABEND DURING XMPOST',ASID=(ASIDREG),  ++00889000
               SDATA=(SQA,PSA,TRT),BUFFER=YES,QUIESCE=NO,BRANCH=YES     00890000
*                 END;              /* DONE WITH SDUMP       @ZA16887*/ 00891000
*               ASIDREG=ASCBASID;   /* TERMINATE WAITING ADDR SPACE     00892000
*                                                            @ZA16887*/ 00893000
@RF00244 LH    ASIDREG,ASCBASID(,ASCBPTR)                          0268 00894000
         N     ASIDREG,@CF02450                                    0268 00895000
*               GEN CODE REFS(ASIDREG);                            0269 00896000
*                                 /* TELL RTM TO TERMINATE  @ZA16887*/  00897000
          CALLRTM TYPE=MEMTERM,COMPCOD=X'55F',ASID=(ASIDREG)            00898000
*             END;                  /* END VALID ASCB ADDR CASE    0270 00899000
*                                                            @ZA16887*/ 00900000
*         END;                      /* END NON=ZERO ASCB ADDR CASE 0271 00901000
*                                                            @ZA16887*/ 00902000
@RF00240 DS    0H                                                  0272 00903000
*       RFY                                                        0272 00904000
*         ASIDREG UNRSTD;           /* FREE UP ASID REGISTER @ZA16887*/ 00905000
@RF00237 DS    0H                                                  0273 00906000
*     END;                          /* END 55F ABEND CASE    @ZA16887*/ 00907000
*                                                                  0273 00908000
*   /*****************************************************************/ 00909000
*   /*                                                               */ 00910000
*   /* FOR CASES OTHER THAN 55F ABEND, RETRY                 @ZA16887*/ 00911000
*   /*                                                               */ 00912000
*   /*****************************************************************/ 00913000
*                                                                  0274 00914000
*   ELSE                                                           0274 00915000
*     DO;                           /* SET UP FOR RETRY      @ZA16887*/ 00916000
         B     @RC00233                                            0274 00917000
@RF00233 DS    0H                                                  0275 00918000
*       SDWASR08=BASEPARM;          /* RESTORE BASE REG ON RETRY   0275 00919000
*                                                            @ZA07127*/ 00920000
         L     @12,SDWAPARM(,SDWAPTR)                              0275 00921000
         L     @11,BASEPARM(,@12)                                  0275 00922000
         ST    @11,SDWASR08(,SDWAPTR)                              0275 00923000
*       SDWASR10=RETADDR;           /* RESTORE RETURN ADDRESS      0276 00924000
*                                                            @ZA07127*/ 00925000
         L     @12,RETADDR(,@12)                                   0276 00926000
         ST    @12,SDWASR10(,SDWAPTR)                              0276 00927000
*       DO;                         /* SETRP RC(SDWARETY)RETADDR(RETR   00928000
*                                      Y1)RETREGS(YES)               */ 00929000
*         RESPECIFY                                                0278 00930000
*          (GPR00P,                                                0278 00931000
*           GPR01P,                                                0278 00932000
*           GPR14P,                                                0278 00933000
*           GPR15P) RSTD;                                          0278 00934000
*         GPR01P->SDWARCDE=SDWARETY;/* STORE RC INTO SDWA            */ 00935000
         MVI   SDWARCDE(GPR01P),X'04'                              0279 00936000
*         GPR01P->SDWARTYA=ADDR(RETRY1);/* SAVE RETRY ADDRESS        */ 00937000
         LA    @12,RETRY1                                          0280 00938000
         ST    @12,SDWARTYA(,GPR01P)                               0280 00939000
*         GPR01P->SDWAUPRG='1'B;    /* TURN ON RETREGS INDICATOR     */ 00940000
         OI    SDWAUPRG(GPR01P),B'00001000'                        0281 00941000
*         RESPECIFY                                                0282 00942000
*          (GPR00P,                                                0282 00943000
*           GPR01P,                                                0282 00944000
*           GPR14P,                                                0282 00945000
*           GPR15P) UNRSTD;                                        0282 00946000
*       END;                                                       0283 00947000
*     END;                          /* END RETRY CASE        @ZA16887*/ 00948000
*RTMRETN:                                                          0285 00949000
*   RTNREG=RTNSAVE;                 /* RESTORE RETURN ADDRESS      0285 00950000
*                                                            @ZA16887*/ 00951000
@RC00233 DS    0H                                                  0285 00952000
RTMRETN  LR    RTNREG,RTNSAVE                                      0285 00953000
*   RETURN;                         /* RETURN TO RTM         @ZA16887*/ 00954000
@EL00003 DS    0H                                                  0286 00955000
@EF00003 DS    0H                                                  0286 00956000
@ER00003 BR    @14                                                 0286 00957000
*   RFY                                                            0287 00958000
*     RTNSAVE UNRSTD;               /* FREE UP REGISTER      @ZA16887*/ 00959000
*   END IRARMRR3;                   /* END OF FRR ROUTINE    @ZA07127*/ 00960000
@PB00003 DS    0H                                                  0288 00961000
*   GENERATE CODE REFS(RTNCOD,SRBPTR,SRBPARM) DEFS(IRARMSRE);      0289 00962000
         ENTRY IRARMSRE                SRB ERROR CLEAN-UP               00963000
IRARMSRE DS    0H                                                       00964000
         SR    RTNCOD,RTNCOD           CLEAR SRBPARM TO ALLOW THE       00965000
         ST    RTNCOD,SRBPARM(,SRBPTR) SRM SRB TO BE RESCHEDULED        00966000
         BR    RTNREG                  RETURN               @ZA07127    00967000
*   END IRARMSRV                                                   0290 00968000
