*                       LEVEL5;                                    0102 00162000
         AL    SVEREG,@CF01410                                     0102 00163000
*                   GENERATE CODE REFS(CVTPTR,CVTMAP,CVTBLDCP,OUXBEND); 00164000
*                                                         /*@ZA10808*/  00165000
         BLDCPOOL CPID=(0),BRANCH=YES,POOLSIZ=2,CSIZE=OUXBEND,        XX00166000
               SP=245,CPADDR=(1),AUTODEL=YES,BNDRY=DWORD      @ZA10808  00167000
*                   SVEREG=WRKREG1; /* RESTORE SAVEAREA ADDRESS      */ 00168000
         LR    SVEREG,WRKREG1                                      0104 00169000
*                   IF RTNCOD=CON0  /* EXTENSION ADDED TO SRM POOL   */ 00170000
*                     THEN          /* TRY GETCELL AGAIN             */ 00171000
         LTR   RTNCOD,RTNCOD                                       0105 00172000
         BNZ   @RF00105                                            0105 00173000
*                     DO;                                          0106 00174000
*                       R1=CON0;    /* CLEAR FOR GETCELL REQUEST     */ 00175000
         SLR   R1,R1                                               0107 00176000
*                       GOTO GETREQ;/* RE-ISSUE GETCELL REQUEST      */ 00177000
         B     GETREQ                                              0108 00178000
*                     END;                                         0109 00179000
*                   ELSE            /* POOL EXTENSION NOT BUILT      */ 00180000
*                     DO;           /* FREE STORAGE OBTAINED BY    0110 00181000
*                                      GETMAIN FOR POOL EXTENSION    */ 00182000
@RF00105 DS    0H                                                  0111 00183000
*                       R0=BLOCKLEN;/* BLOCK LENGTH FOR FREEMAIN     */ 00184000
         LA    R0,2048                                             0111 00185000
*                       CALL FREESTOR;/* FREE BLOCK                  */ 00186000
         BAL   @14,FREESTOR                                        0112 00187000
*                       RTNCOD=CON4;/* INDICATE GETCELL FAILED       */ 00188000
         LA    RTNCOD,4                                            0113 00189000
*                     END;                                         0114 00190000
*                 END;                                             0115 00191000
*             END;                                                 0116 00192000
*         END;                                                     0117 00193000
*       ELSE                        /* GETMAIN REQUEST               */ 00194000
*         CALL GETSTOR;             /* GETMAIN STORAGE               */ 00195000
         B     @RC00089                                            0118 00196000
@RF00089 BAL   @14,GETSTOR                                         0118 00197000
*     END;                                                         0119 00198000
*   ELSE                            /* FREE STORAGE REQUEST          */ 00199000
*     DO;                           /* FREECELL OR FREEMAIN REQUEST  */ 00200000
         B     @RC00087                                            0120 00201000
@RF00087 DS    0H                                                  0121 00202000
*       IF R0=CELLSIZE THEN         /* FREECELL REQUEST              */ 00203000
         C     R0,CELLSIZE                                         0121 00204000
         BNE   @RF00121                                            0121 00205000
*         DO;                                                      0122 00206000
*           R0=RMCTCPID;            /* CELL POOL ID OF SRM STORAGE   */ 00207000
*                                                                  0123 00208000
         L     R0,RMCTCPID(,RMCTPTR)                               0123 00209000
*           /*********************************************************/ 00210000
*           /*                                                       */ 00211000
*           /* ISSUE FREECELL REQUEST.                               */ 00212000
*           /*                                                       */ 00213000
*           /*********************************************************/ 00214000
*                                                                  0124 00215000
*           GENERATE CODE REFS(CVTPTR,CVTMAP,CVTFRECL)             0124 00216000
