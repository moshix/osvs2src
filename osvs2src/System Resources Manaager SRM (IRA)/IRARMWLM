*        RTNREG = WKRO ; /* RESTORE RETURN REG CONTENTS */      02S0375 01673000
         LR    RTNREG,WKRO                                      02S0375 01674000
*        END;                                                   02S0376 01675000
*      ELSE                         /* SAME XACN COULD RESUME LATER  */ 01676000
*        /* INSURE TRANSACTION STATISTICS ARE UP TO DATE             */ 01677000
*        DO;                                                    02S0377 01678000
         B     @RC00337                                         02S0377 01679000
@RF00337 DS    0H                                               02S0378 01680000
*        IF RRPACOD = QSCECMP THEN  /* QUIESCE COMPLETE FOR SWAPOUT  */ 01681000
