*            OUCBNTR = '1'B |       /* OLD XACTN TO BE STOPPED       */ 01557000
*            (OUCBLOG = '1'B &      /* TSO USER TRANSACTION          */ 01558000
*            (RRPACOD = INITDET |   /* STEP DETACH SYSEVENT          */ 01559000
*            (OUCBTRM = '1'B & OUCBCIM = '0'B))) THEN /* TERMNL WAIT */ 01560000
         TM    OUCBSTR(OUCBPTR),B'01100000'                     02S0337 01561000
         BNZ   @RT00337                                         02S0337 01562000
         TM    OUCBLOG(OUCBPTR),B'00100000'                     02S0337 01563000
         BNO   @RF00337                                         02S0337 01564000
         CLI   RRPACOD(RRPAPTR),11                              02S0337 01565000
         BE    @RT00337                                         02S0337 01566000
         TM    OUCBTRM(OUCBPTR),B'01000000'                     02S0337 01567000
         BNO   @RF00337                                         02S0337 01568000
         TM    OUCBCIM(OUCBPTR),B'00010000'                     02S0337 01569000
         BNZ   @RF00337                                         02S0337 01570000
@RT00337 DS    0H                                               02S0338 01571000
*        /* ACCUMULATE TRANSACTION STATISTICS TO USER SESSION/JOB    */ 01572000
*        DO;                                                    02S0338 01573000
*        IF OUCBRTR = '0'B &        /* TRANSACTION WAS NOT SUSPENDED */ 01574000
*              OUCBNTR = '0'B THEN  /* XACTN PERFORM GROUP NOT RESET */ 01575000
         TM    OUCBRTR(OUCBPTR),B'00110000'                     02S0339 01576000
         BNZ   @RF00339                                         02S0339 01577000
*          CALL IRARMWMI;           /* UPDATE TRANSACTION STATISTICS */ 01578000
         BAL   @14,IRARMWMI                                     02S0340 01579000
*        ELSE                       /* XACTN ENDED WHILE SWAPPED OUT */ 01580000
*          IF OUCBSTR = '0'B THEN   /* NEW TRANSACTION NOT YET START */ 01581000
         B     @RC00339                                         02S0341 01582000
@RF00339 TM    OUCBSTR(OUCBPTR),B'01000000'                     02S0341 01583000
         BNZ   @RF00341                                         02S0341 01584000
*            CALL IRARMWMH;         /* RESET TRANSACTN NTRVL MEASURM */ 01585000
         BAL   @14,IRARMWMH                                     02S0342 01586000
*        IF OUCBNTR = '0'B &        /* TRANSACTN STOP NOT COLLECTED  */ 01587000
*              RMCTMFA = '1'B THEN  /* WKLD ACTIVITY BEING MEASURED  */ 01588000
@RF00341 DS    0H                                               02S0343 01589000
@RC00339 TM    OUCBNTR(OUCBPTR),B'00100000'                     02S0343 01590000
         BNZ   @RF00343                                         02S0343 01591000
         TM    RMCTMFA(RMCTPTR),B'10000000'                     02S0343 01592000
         BNO   @RF00343                                         02S0343 01593000
*          /* UPDATE WORKLOAD ACTIVITY TRANSACTION MEASUREMENTS      */ 01594000
*          DO;                                                  02S0344 01595000
*          RRPAWAR = '1'B;          /* INSERT WKLD ACTIVTY RTNE FLAG */ 01596000
         OI    RRPAWAR(RRPAPTR),B'01000000'                     02S0345 01597000
*          /* ? IRACTLCL EPN(WR6) */                            02S0346 01598000
*          CALL RMSBWR6 -> IRARMRTN ;           /* INVOKE WR6 SUBRTN */ 01599000
         L     @15,RMSBWR6(,RMCTPTR)                            02S0346 01600000
         BALR  @14,@15                                          02S0346 01601000
*          RRPAWAR = '0'B;          /* DELETE WKLD ACTIVTY RTNE FLAG */ 01602000
         NI    RRPAWAR(RRPAPTR),B'10111111'                     02S0347 01603000
*          END;                                                 02S0348 01604000
*        OUXBJBS = OUXBJBS + OUXBTRS; /* ADD TO SESSION XACTN SERV   */ 01605000
@RF00343 L     @09,OUXBJBS(,OUXBPTR)                            02S0349 01606000
         AL    @09,OUXBTRS(,OUXBPTR)                            02S0349 01607000
         ST    @09,OUXBJBS(,OUXBPTR)                            02S0349 01608000
*        OUXBJBT = OUXBJBT + OUXBTRT; /* ADD TO SESSION XACTN TIME   */ 01609000
         L     @09,OUXBJBT(,OUXBPTR)                            02S0350 01610000
         AL    @09,OUXBTRT(,OUXBPTR)                            02S0350 01611000
         ST    @09,OUXBJBT(,OUXBPTR)                            02S0350 01612000
*        OUXBJBR = OUXBJBR + OUXBTRR; /* ADD TO SESSION RESID TIME   */ 01613000
         L     @09,OUXBJBR(,OUXBPTR)                            02S0351 01614000
         AL    @09,OUXBTRR(,OUXBPTR)                            02S0351 01615000
         ST    @09,OUXBJBR(,OUXBPTR)                            02S0351 01616000
*        OUCBATR = '0'B;            /* NO LONGER HAVE ACTIVE XACTN   */ 01617000
*        OUCBSTR = '0'B;            /* SET OFF XACTN START REQUEST   */ 01618000
*        OUCBNTR = '0'B;            /* SET OFF XACTN END INDICATOR   */ 01619000
*        OUCBRTR = '0'B;            /* SET OFF XACTN SUSPENSE FLAG   */ 01620000
*        OUCBPCH = '0'B;            /* SET OFF PER CHG OUTSTD FLAG   */ 01621000
         NI    OUCBATR(OUCBPTR),B'00000111'                     02S0356 01622000
