         LR    R1,@09                                           02S0369 01658000
*        R0 = ZERO ; /* INDICATED ZERO SERVICE */               02S0370 01659000
         SLR   R0,R0                                            02S0370 01660000
*        WKRO = RTNREG ; /* SAVE RETURN REG */                  02S0371 01661000
         LR    WKRO,RTNREG                                      02S0371 01662000
*        /* CALC USER NORMALIZED WORK LEVEL */                  02S0372 01663000
*        /* ? IRACTLCL EPN(WM4) */                              02S0372 01664000
