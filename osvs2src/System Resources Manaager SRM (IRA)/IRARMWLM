         TM    OUCBINP(OUCBPTR),B'00000010'                     02S0262 01293000
         BNO   @RF00262                                         02S0262 01294000
*      /* WORKLOAD MANAGER TRANSACTION PROCESSING APPLICABLE TO USER */ 01295000
*      DO;                                                      02S0263 01296000
*      OUCBSTR = '1'B;              /* FLAG XACTN START REQST PENDNG */ 01297000
         OI    OUCBSTR(OUCBPTR),B'01000000'                     02S0264 01298000
*      IF OUCBINC = '1'B &          /* USER IS SUBJECT TO WLM CONTRL */ 01299000
*            OUCBOUT = '0'B THEN    /* USER FIELDS ARE AVAILABLE NOW */ 01300000
         TM    OUCBINC(OUCBPTR),B'00000001'                     02S0265 01301000
         BNO   @RF00265                                         02S0265 01302000
         TM    OUCBOUT(OUCBPTR),B'00000100'                     02S0265 01303000
         BNZ   @RF00265                                         02S0265 01304000
*        /* INITIALIZE PROPR USER FIELDS TO REFLECT NEW STARTD XACTN */ 01305000
*        DO;                                                    02S0266 01306000
*        IF OUCBATR = '1'B THEN     /* PREVIOUS XACTN IN EXISTENCE   */ 01307000
         TM    OUCBATR(OUCBPTR),B'10000000'                     02S0267 01308000
         BNO   @RF00267                                         02S0267 01309000
*          /* INVOKE ROUTINE TO CLEAN UP THE PREVIOUS TRANSACTION    */ 01310000
*          /* ? IRACTLCL EPN(WMO) */                            02S0268 01311000
*          CALL RMSBWMO -> IRARMRTN ;           /* INVOKE WMO SUBRTN */ 01312000
         L     @15,RMSBWMO(,RMCTPTR)                            02S0268 01313000
         BALR  @14,@15                                          02S0268 01314000
*        OUCBWMG = WPGDPTR - WMSTPGDT; /* PERF GRP DESCRIPTOR OFFSET */ 01315000
@RF00267 LR    @10,WPGDPTR                                      02S0269 01316000
         SL    @10,WMSTPGDT(,WMSTPTR)                           02S0269 01317000
         STH   @10,OUCBWMG(,OUCBPTR)                            02S0269 01318000
*        WPGPPTR = WPGDPTR + OUCBPGP ; /* ADDR OF PERF GROUP PERIOD  */ 01319000
         LR    WPGPPTR,WPGDPTR                                  02S0270 01320000
         SLR   @10,@10                                          02S0270 01321000
         IC    @10,OUCBPGP(,OUCBPTR)                            02S0270 01322000
         ALR   WPGPPTR,@10                                      02S0270 01323000
*        OUCBATR = '1'B;            /* INDICATE HAVE ACTIVE TRANSACN */ 01324000
*        OUCBSTR = '0'B;            /* RESET XACTN START REQUEST FLG */ 01325000
*        OUCBNTR = '0'B;            /* RESET XACTN TERMINATE REQ FLG */ 01326000
*        OUCBRTR = '0'B;            /* RESET XACTN RESUM REQUEST FLG */ 01327000
*        OUCBPCH = '0'B;            /* RESET PERIOD CHNG OUTSTND FLG */ 01328000
         OI    OUCBATR(OUCBPTR),B'10000000'                     02S0275 01329000
         NI    OUCBSTR(OUCBPTR),B'10000111'                     02S0275 01330000
*        OUXBTRC = OUXBTRC + ONE;   /* INDICATE HAV ONE MOR TRANSACN */ 01331000
         L     @10,OUXBTRC(,OUXBPTR)                            02S0276 01332000
         AL    @10,ONE                                          02S0276 01333000
         ST    @10,OUXBTRC(,OUXBPTR)                            02S0276 01334000
*        OUCBSWC = ZERO;            /* RESET USER SWAP COUNT        */  01335000
         SLR   @10,@10                                          02S0277 01336000
         STH   @10,OUCBSWC(,OUCBPTR)                            02S0277 01337000
*        OUXBTRS = ZERO;            /* RESET XACTN SERVIC UNIT ACCUM */ 01338000
         ST    @10,OUXBTRS(,OUXBPTR)                            02S0278 01339000
*        OUXBTRT = ZERO;            /* RESET XACTN ACTIVE TIME ACCUM */ 01340000
         ST    @10,OUXBTRT(,OUXBPTR)                            02S0279 01341000
*        OUXBTRR = ZERO;            /* RESET XACTN RESDNT TIME ACCUM */ 01342000
         ST    @10,OUXBTRR(,OUXBPTR)                            02S0280 01343000
