*  END ;                                                        02S0487 02188000
*/* SET APPLICABLE ALGORITHM INDICATORS                              */ 02189000
*CALL IRARMWMJ ;                                                02S0488 02190000
@RF00482 BAL   @14,IRARMWMJ                                     02S0488 02191000
*  /* ? IRACTLEP ENN(WMK) */                                    02S0489 02192000
*  GEN CODE EXIT NOSEQFLOW;                                     02S0489 02193000
*                                  /*                                   02194000
         LM    @00,@14,128+000(@13)    RELOAD INVOKING RTNE REGS        02195000
