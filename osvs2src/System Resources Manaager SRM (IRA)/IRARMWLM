         ST    @09,OUCBWMR(,OUCBPTR)                            02S0362 01635000
*        OUCBSWC = ZERO;            /* RESET TRANSACTN SWAPOUT CNT   */ 01636000
         STH   @09,OUCBSWC(,OUCBPTR)                            02S0363 01637000
*        OUXBTRS = ZERO;            /* RESET XACTN SERV ACCUMULATR   */ 01638000
         ST    @09,OUXBTRS(,OUXBPTR)                            02S0364 01639000
*        OUXBTRT = ZERO;            /* RESET XACTN TIME ACCUMULATR   */ 01640000
         ST    @09,OUXBTRT(,OUXBPTR)                            02S0365 01641000
*        OUXBTRR = ZERO;            /* RESET RESID TIME ACCUMULATR   */ 01642000
         ST    @09,OUXBTRR(,OUXBPTR)                            02S0366 01643000
*        OUCBPGP = WPGPOFST;        /* PERF GRP OFST TO 1ST PERIOD   */ 01644000
         LA    @09,12                                           02S0367 01645000
         STC   @09,OUCBPGP(,OUCBPTR)                            02S0367 01646000
*        WPGPPTR = WMSTPGDT + OUCBWMG+OUCBPGP ;/*PERF GROUP DESC */     01647000
         LH    WPGPPTR,OUCBWMG(,OUCBPTR)                        02S0368 01648000
         AL    WPGPPTR,WMSTPGDT(,WMSTPTR)                       02S0368 01649000
         ALR   WPGPPTR,@09                                      02S0368 01650000
*        R1=WMSTPODT+WPOVTENT; /* USER PERF OBJ */              02S0369 01651000
         SLR   @09,@09                                          02S0369 01652000
         IC    @09,WPGPOBJN(,WPGPPTR)                           02S0369 01653000
         ALR   @09,@09                                          02S0369 01654000
         AL    @09,WMSTPOVT(,WMSTPTR)                           02S0369 01655000
