         TM    OUCBATR(OUCBPTR),B'10000000'                     02S0392 01765000
         BZ    @RT00392                                         02S0392 01766000
*      /* 'XCTL' TO WMO SBRTNE WHICH USES SAME REG SAVE AREA AS WMQ  */ 01767000
*      GOTO IRARMWME;               /* DISPOSE OF XACN USER GAVE UP  */ 01768000
*    ELSE                           /* SWAPOUT CAUSED BY SYSTM ACTN  */ 01769000
