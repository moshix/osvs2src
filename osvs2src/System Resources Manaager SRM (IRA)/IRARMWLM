@EF00003 DS    0H                                               02S0456 02036000
@ER00003 BR    @14                                              02S0456 02037000
*                                                               01S0457 02038000
*/* ALGORITHM APPLICABILITY DETERMINATION SUBROUTINE                 */ 02039000
*/* **************************************************************** */ 02040000
*/*                                                                  */ 02041000
*/*    ROUTINE NAME - IRARMWMJ                                       */ 02042000
*/*                                                                  */ 02043000
*/*    DESCRIPTIVE NAME - ROUTINE TO DETERMINE THE SCOPE OF          */ 02044000
*/*          APPLICABILITY OF ANALYSIS PROCESSING TO A USER          */ 02045000
*/*                                                                  */ 02046000
*/*    FUNCTION - THE IRARMWMJ ROUTINE EXAMINES THE CURRENT          */ 02047000
*/*          SWAPPABILITY STATUS AND THE PERFORMANCE SPECI-          */ 02048000
*/*          FICATION FOR AN USER.  IT INDICATES IF THE RESOURCE     */ 02049000
*/*          MANAGER ALGORITHMS ARE APPLICABLE TO THIS USER.         */ 02050000
*/*                                                                  */ 02051000
*/*    CALLED BY - IRARMWMK - WLM DONTSWAP/OKSWAP SERVICE UPDT RTNE  */ 02052000
*/*                IRARMWMN - USER TRANSACTION STARTUP ROUTINE       */ 02053000
*/*                IRARMWMO - USER TRANSACTION SUSPEND ROUTINE       */ 02054000
*/*                IRARMWMR - USER TRANSACTION RESUMPT ROUTINE       */ 02055000
*/*                IRARMWM5 - PERFORMANCE PERIOD CHECK SBRTINE       */ 02056000
*/*                                                                  */ 02057000
*/*    INPUT - REG 4 CONTAINS THE ADDRESS OF THE OUCB FOR THE        */ 02058000
*/*            USER WHOSE ANALYSIS APPLIC IS BEING DETERMINED.       */ 02059000
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */ 02060000
*/*          - REG 15 CONTAINS THE IRARMWMK RTNE ADDRESS.            */ 02061000
*/*          - OUCBNSW INDICATES IF THE USER IS NON-SWAPPABLE.       */ 02062000
*/*          - OUCBNPG INDICATES THE PERF GROUP FOR THE USER.        */ 02063000
*/*          - OUCBPGP INDICATES THE PERF PERIOD FOR THE USER.       */ 02064000
*/*          - WPGDRTB INDICATES THE RESPONSE/THROUGHPUT BIAS.       */ 02065000
*/*                                                                  */ 02066000
*/*    OUTPT - OUCBRMA IS SET ON IF RESOURCE ALGORITHM RECOM-        */ 02067000
*/*            MENDATIONS ARE TO BE SOLICITED FOR THIS USER.         */ 02068000
*/*                                                                  */ 02069000
*/*    EXIT  - BRANCH ON REGISTER 14.                                */ 02070000
*/*          - THERE IS NO ERROR EXIT OR INDICATION.                 */ 02071000
*/*                                                                  */ 02072000
*/* **************************************************************** */ 02073000
*IRARMWMJ: PROC OPTIONS(NOSAVE,NOSAVEAREA,NOENTREG,RETREG,NOPARMREG);   02074000
IRARMWMJ DS    0H                                               02S0458 02075000
*    RFY (WKRE) UNRSTD;                                         02S0458 02076000
*    STPR = RTNREG;                 /* SAVE RETURN REG CONTENTS      */ 02077000
         LR    STPR,RTNREG                                      02S0459 02078000
*    /* NON-SWAPPABLE USERS ARE EXCLUDED FROM SWAP ANALYSIS          */ 02079000
*    OUCBRMA = '0'B;                /* RESET RESOURCE ANALYSIS FLG   */ 02080000
         NI    OUCBRMA(OUCBPTR),B'11011111'                     02S0460 02081000
*    IF OUCBNSW = '0'B THEN /* USER  WKLD CONTROLLED */         02S0461 02082000
         TM    OUCBNSW(OUCBPTR),B'10000000'                     02S0461 02083000
         BNZ   @RF00461                                         02S0461 02084000
*      DO;                                                      02S0462 02085000
*      WMSTPTR = RMCTWMST;          /* PICK WLM SPECIFICATION TABLE  */ 02086000
         L     WMSTPTR,RMCTWMST(,RMCTPTR)                       02S0463 02087000
*      WPGDPTR = WMSTPGDT + WPGVTENT; /* PICK PERF GROUP DESCRIPTOR  */ 02088000
         SLR   @10,@10                                          02S0464 02089000
         IC    @10,OUCBNPG(,OUCBPTR)                            02S0464 02090000
         ALR   @10,@10                                          02S0464 02091000
         AL    @10,WMSTPGVT(,WMSTPTR)                           02S0464 02092000
         LH    WPGDPTR,WPGVTENT(,@10)                           02S0464 02093000
         AL    WPGDPTR,WMSTPGDT(,WMSTPTR)                       02S0464 02094000
*      WPGPPTR = WPGDPTR + OUCBPGP; /* PICK CURR PERF GRP PERIOD   */   02095000
         LR    WPGPPTR,WPGDPTR                                  02S0465 02096000
         SLR   @10,@10                                          02S0465 02097000
         IC    @10,OUCBPGP(,OUCBPTR)                            02S0465 02098000
         ALR   WPGPPTR,@10                                      02S0465 02099000
*      IF WPGPRTB = '1'B THEN     /* THROUGHPUT BIAS INDICATED     */   02100000
         TM    WPGPRTB(WPGPPTR),B'00000001'                     02S0466 02101000
         BNO   @RF00466                                         02S0466 02102000
*        OUCBRMA = '1'B;          /* RESOURCE ALGORITHMS APPLIC    */   02103000
         OI    OUCBRMA(OUCBPTR),B'00100000'                     02S0467 02104000
*      ELSE                       /* IF RTB=0 BE SURE THAT  @ZA26908*/  02105000
*        DO;                      /* LOAD BALANCER RECOMMEN @ZA26908*/  02106000
         B     @RC00466                                         02S0468 02107000
@RF00466 DS    0H                                               02S0469 02108000
*        OUCBCRV=ZERO;            /* DATION VALUES ARE      @ZA26908*/  02109000
         SLR   @10,@10                                          02S0469 02110000
         STH   @10,OUCBCRV(,OUCBPTR)                            02S0469 02111000
*        OUCBIRV=ZERO;            /* ZERO                   @ZA26908*/  02112000
         STH   @10,OUCBIRV(,OUCBPTR)                            02S0470 02113000
*        END;                     /*                        @ZA26908*/  02114000
*      END;                                                     02S0472 02115000
*    ELSE                         /* IF NONSWAPPABLE MEMORY @ZA26908*/  02116000
*      DO;                        /* LOAD BALANCER RECOMEN  @ZA26908*/  02117000
         B     @RC00461                                         02S0473 02118000
@RF00461 DS    0H                                               02S0474 02119000
*      OUCBCRV=ZERO;              /* DATION VALUES SHOULD   @ZA26908*/  02120000
         SLR   @10,@10                                          02S0474 02121000
         STH   @10,OUCBCRV(,OUCBPTR)                            02S0474 02122000
*      OUCBIRV=ZERO;              /* BE ZERO                @ZA26908*/  02123000
         STH   @10,OUCBIRV(,OUCBPTR)                            02S0475 02124000
*      END;                       /*                        @ZA26908*/  02125000
*    RTNREG = STPR;                 /* RESTORE RETURN REG CONTENTS   */ 02126000
@RC00461 LR    RTNREG,STPR                                      02S0477 02127000
*    RFY (WKRE) RSTD;                                           02S0478 02128000
*    END IRARMWMJ;                                              02S0479 02129000
@EL00004 DS    0H                                               02S0479 02130000
@EF00004 DS    0H                                               02S0479 02131000
@ER00004 BR    @14                                              02S0479 02132000
*                                                               01S0480 02133000
*/* WLM DONTSWAP/OKSWAP USER ANALYSIS ROUTINE                        */ 02134000
*/* ******************************************************************/ 02135000
*/*                                                                  */ 02136000
*/*    ROUTINE NAME - IRARMWMK                                       */ 02137000
*/*                                                                  */ 02138000
*/*    DESCRIPTIVE NAME - WLM DONTSWAP/OKSWAP USER ANALYSY ROUTINE   */ 02139000
*/*                                                                  */ 02140000
*/*    FUNCTION - THE IRARMWMK  ROUTINE CALCULATES THE CURRENT       */ 02141000
*/*               SERVICE AND INSURES THAT THE USER IS IN THE        */ 02142000
*/*               CORRECT PERFORMANCE GROUP PERIOD. APPLICABLE       */ 02143000
*/*               ALGORITHM INDICATORS ARE SET BASED ON THE NEW      */ 02144000
*/*               SWAPPABILITY STATUS OF THE USER.                   */ 02145000
*/*    CALLED BY - IRARME41 - USER DON'T SWAP SYSEVENT ROUTINE       */ 02146000
*/*                IRARME42 - USER OK TO SWAP SYSEVENT ROUTINE       */ 02147000
*/*                                                                  */ 02148000
*/*    INPUT - REG 4 CONTAINS THA ADDRESS OF THE OUCB FOR THE USER   */ 02149000
*/*            TO BE EVALUATED.                                      */ 02150000
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */ 02151000
*/*          - REG 15 CONTAINS THE IRARMWMK ROUTINE ADDRESS          */ 02152000
*/*                                                                  */ 02153000
*/*    OUTPUT - SEE FUNCTION                                         */ 02154000
*/*                                                                  */ 02155000
*/*    EXIT - BRANCH ON REGISTER 14.                                 */ 02156000
*/*         - THERE IS NO ERROR EXIT OR INDICATION.                  */ 02157000
*/*                                                                  */ 02158000
*/* ******************************************************************/ 02159000
*  /* ? IRACTLEP EPN(WMK) */                                    02S0480 02160000
*  DCL IRARMWMK ENTRY INT;                                      02S0480 02161000
*  GEN CODE DEFS(IRARMWMK) NOEXIT SEQFLOW;                      02S0481 02162000
*                                  /*                                   02163000
         ENTRY IRARMWMK                                                 02164000
IRARMWMK DS    0H                                                       02165000
         STM   @00,@14,128+000(@13)    SAVE INVOKING RTNE REGS          02166000
         LA    @12,IRARMWMK-@PSTART    DISPLACEMENT TO MODULE BASE      02167000
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT            02168000
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY      02169000
*IF OUCBTMW ^= ZERO THEN            /* TRANSACTION INTERVAL EXISTS   */ 02170000
         L     @09,OUCBTMW(,OUCBPTR)                            02S0482 02171000
         LTR   @09,@09                                          02S0482 02172000
         BZ    @RF00482                                         02S0482 02173000
*  DO ;                                                         02S0483 02174000
*  IF OUCBOUT = '0'B & OUCBGOB = '0'B & /* USER RECIEVING SERVICE    */ 02175000
*     OUCBGOO = '0'B & OUCBGOI = '0'B THEN /* USER NOT TRANSITIONING */ 02176000
         TM    OUCBOUT(OUCBPTR),B'11100100'                     02S0484 02177000
         BNZ   @RF00484                                         02S0484 02178000
*    /* OBTAIN USER SERVICE                                          */ 02179000
*      /* ? IRACTLCL EPN(WM1) */                                02S0485 02180000
*      CALL RMSBWM1 -> IRARMRTN ;               /* INVOKE WM1 SUBRTN */ 02181000
