         SLR   @09,@09                                          02S0357 01624000
         ST    @09,OUCBTMW(,OUCBPTR)                            02S0357 01625000
*        OUCBWMS = ZERO;            /* RESET WLM INTERVL CURR SRVC   */ 01626000
         ST    @09,OUCBWMS(,OUCBPTR)                            02S0358 01627000
*        OUCBCPU = ZERO ;           /* RESET CPU SERVICE             */ 01628000
         ST    @09,OUCBCPU(,OUCBPTR)                            02S0359 01629000
*        OUCBIOC = ZERO ;           /* RESET IO SERVICE              */ 01630000
         ST    @09,OUCBIOC(,OUCBPTR)                            02S0360 01631000
*        OUCBMSO = ZERO ;           /* RESET MSO SERVICE             */ 01632000
         ST    @09,OUCBMSO(,OUCBPTR)                            02S0361 01633000
