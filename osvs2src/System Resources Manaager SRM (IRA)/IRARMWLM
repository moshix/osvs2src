*              CALL RMSBWR4 -> IRARMRTN ;       /* INVOKE WR4 SUBRTN */ 01429000
         L     @15,RMSBWR4(,RMCTPTR)                            02S0311 01430000
         BALR  @14,@15                                          02S0311 01431000
*              OUCBWMG = WPGDPTR-WMSTPGDT;/* OFFSET TO NEW PERF 02S0312 01432000
*                                       GRP DESCRIPTOR       @ZA20350*/ 01433000
         LR    @10,WPGDPTR                                      02S0312 01434000
         SL    @10,WMSTPGDT(,WMSTPTR)                           02S0312 01435000
         STH   @10,OUCBWMG(,OUCBPTR)                            02S0312 01436000
*              OUCBPGP = WPGPOFST;  /* PERF GRP OFFSET TO 1ST PERIOD */ 01437000
         MVI   OUCBPGP(OUCBPTR),X'0C'                           02S0313 01438000
*              OUCBMAR = '1'B;      /* SPECFY NEGATIV PERIOD ACCUMUL */ 01439000
         OI    OUCBMAR(OUCBPTR),B'00000100'                     02S0314 01440000
*              /* ? IRACTLCL EPN(WR4) */                        02S0315 01441000
*              CALL RMSBWR4 -> IRARMRTN ;       /* INVOKE WR4 SUBRTN */ 01442000
         L     @15,RMSBWR4(,RMCTPTR)                            02S0315 01443000
         BALR  @14,@15                                          02S0315 01444000
*              OUCBMAR = '0'B;      /* RESET NEGATIVE PERIOD ACCUMUL */ 01445000
         NI    OUCBMAR(OUCBPTR),B'11111011'                     02S0316 01446000
*              END;                                             02S0317 01447000
*            RRPAWAR = '0'B;        /* DELETE WKLD ACTIVTY RTNE FLAG */ 01448000
@RF00309 NI    RRPAWAR(RRPAPTR),B'10111111'                     02S0318 01449000
*            END;                                               02S0319 01450000
*          OUCBNTR = '1'B;          /* INDICATE XACTN STOP COLLECTED */ 01451000
@RF00305 OI    OUCBNTR(OUCBPTR),B'00100000'                     02S0320 01452000
*          END;                     /* END COLLECTING XACN      02S0321 01453000
*                                      MEASUREMENTS          @ZA20350*/ 01454000
*          OUCBWMG = WPGDPTR-WMSTPGDT;/* OFFST TO NEW PERF GRP  02S0322 01455000
