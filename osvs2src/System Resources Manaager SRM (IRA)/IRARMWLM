*/* IN-CORE INTERVAL CHANGE SUBROUTINE                               */ 01920000
*/* **************************************************************** */ 01921000
*/*                                                                  */ 01922000
*/*    ROUTINE NAME - IRARMWMI                                       */ 01923000
*/*                                                                  */ 01924000
*/*    DESCRIPTIVE NAME - WORKLOAD MANAGER IN-CORE INTERVAL          */ 01925000
*/*                       CHANGE SUBROUTINE                          */ 01926000
*/*                                                                  */ 01927000
*/*    FUNCTION - THE IRARMWMI SUBROUTINE UPDATES THE TRANSACTION    */ 01928000
*/*          ACCUMULATORS WITH THE SERVICE AND THE TIME RECEIVED     */ 01929000
*/*          BY THE USER DURING THE PRECEDING IN-CORE INTERVAL.      */ 01930000
*/*                                                                  */ 01931000
*/*    CALLED BY - IRARMWMO - USER TRANSACTION COMPLET ROUTINE       */ 01932000
*/*                IRARMWMQ - USER TRANSACTION QUIESCE ROUTINE       */ 01933000
*/*                                                                  */ 01934000
*/*                                                                  */ 01935000
*/*    ROUTINE NAME - IRARMWMH                                       */ 01936000
*/*                                                                  */ 01937000
*/*    DESCRIPTIVE NAME - WORKLOAD MANAGER MEASUREMENT INTERVAL      */ 01938000
*/*                       RESET SUBROUTINE                           */ 01939000
*/*                                                                  */ 01940000
*/*    FUNCTION - THE IRARMWMH SUBROUTINE RESETS THE WORKLOAD        */ 01941000
*/*          MANAGER MEASUREMENT INTERVAL START VALUE FIELDS TO      */ 01942000
*/*          BEGIN A NEW WORKLOAD MANAGER MEASUREMENT INTERVAL.      */ 01943000
*/*                                                                  */ 01944000
*/*    CALLED BY - IRARMWMN - USER TRANSACTION STARTUP ROUTINE       */ 01945000
*/*                IRARMWMR - USER TRANSACTION RESUMPT ROUTINE       */ 01946000
*/*                                                                  */ 01947000
*/*                                                                  */ 01948000
*/*    INPUT - REG 4 CONTAINS THE ADDRESS OF THE OUCB FOR THE USER   */ 01949000
*/*            WHOSE MEASUREMENT INTERVAL IS TO BE CHANGED/RESET.    */ 01950000
*/*          - REG 5 CONTAINS THE USER ASCB BLOCK ADDRESS.           */ 01951000
*/*          - REG 6 CONTAINS THE USER OUXB BLOCK ADDRESS.           */ 01952000
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */ 01953000
*/*          - OUCBTMS CONTAINS THE BEG TIME OF THE CURR NTRVL.      */ 01954000
*/*                                                                  */ 01955000
*/*    OUTPT - SEE FUNCTION.                                         */ 01956000
*/*                                                                  */ 01957000
*/*    EXIT  - BRANCH ON REGISTER 14.                                */ 01958000
*/*          - THERE IS NO ERROR EXIT OR INDICATION.                 */ 01959000
*/*                                                                  */ 01960000
*/* **************************************************************** */ 01961000
*IRARMWMI: PROC OPTIONS(NOSAVE,NOSAVEAREA,NOENTREG,RETREG,NOPARMREG);   01962000
IRARMWMI DS    0H                                               02S0434 01963000
*    PMRO = RTNREG;                 /* SAVE RETURN REG CONTENTS      */ 01964000
         LR    PMRO,RTNREG                                      02S0434 01965000
*    /* ? IRACTLCL EPN(WM1) */                                  02S0435 01966000
*    CALL RMSBWM1 -> IRARMRTN ;                 /* INVOKE WM1 SUBRTN */ 01967000
         L     @15,RMSBWM1(,RMCTPTR)                            02S0435 01968000
         BALR  @14,@15                                          02S0435 01969000
*    OUXBTRS = OUXBTRS + OUCBWMS;   /* ADD SERVICE TO TRANSACTION    */ 01970000
         L     @09,OUXBTRS(,OUXBPTR)                            02S0436 01971000
         AL    @09,OUCBWMS(,OUCBPTR)                            02S0436 01972000
         ST    @09,OUXBTRS(,OUXBPTR)                            02S0436 01973000
*    OUXBTRT = OUXBTRT + (RRPATOD - OUCBTMS); /* ADD TIME TO XACN    */ 01974000
         L     @09,RRPATOD(,RRPAPTR)                            02S0437 01975000
         SL    @09,OUCBTMS(,OUCBPTR)                            02S0437 01976000
         L     @08,OUXBTRT(,OUXBPTR)                            02S0437 01977000
         ALR   @08,@09                                          02S0437 01978000
         ST    @08,OUXBTRT(,OUXBPTR)                            02S0437 01979000
*    OUXBTRR = OUXBTRR + (RRPATOD - OUCBTMS); /* ADD RESIDNT TIME    */ 01980000
         AL    @09,OUXBTRR(,OUXBPTR)                            02S0438 01981000
         ST    @09,OUXBTRR(,OUXBPTR)                            02S0438 01982000
*    IF RMCTMFA = '1'B THEN         /* WORKLOAD ACTIVITY MEASURED    */ 01983000
         TM    RMCTMFA(RMCTPTR),B'10000000'                     02S0439 01984000
         BNO   @RF00439                                         02S0439 01985000
*      /* UPDATE WORKLOAD ACTIVITY MEASUREMENT REPORT STATISTICS     */ 01986000
*      DO;                                                      02S0440 01987000
*      RRPAWAR = '1'B;              /* INSERT WKLD ACTIVTY RTNE FLAG */ 01988000
         OI    RRPAWAR(RRPAPTR),B'01000000'                     02S0441 01989000
*      /* ? IRACTLCL EPN(WR4) */                                02S0442 01990000
*      CALL RMSBWR4 -> IRARMRTN ;               /* INVOKE WR4 SUBRTN */ 01991000
         L     @15,RMSBWR4(,RMCTPTR)                            02S0442 01992000
         BALR  @14,@15                                          02S0442 01993000
*      RRPAWAR = '0'B;              /* DELETE WKLD ACTIVTY RTNE FLAG */ 01994000
         NI    RRPAWAR(RRPAPTR),B'10111111'                     02S0443 01995000
*      END;                                                     02S0444 01996000
*    RTNREG = PMRO;                 /* RESTORE RETURN REG CONTENTS   */ 01997000
@RF00439 LR    RTNREG,PMRO                                      02S0445 01998000
*/*    ENTRY WHEN DONT NEED TO ACCUM DATA, JUST START INTERVAL ANEW  */ 01999000
*IRARMWMH: /* SUBROUTINE TO RESET WORKLOAD MANAGER INTERVAL          */ 02000000
*      /* SAVE EXTRA CALL BY CODING AS ENTRY INSTEAD OF SEPARAT PROC */ 02001000
*      ENTRY;                                                   02S0446 02002000
         B     @EC00446                                         02S0446 02003000
IRARMWMH DS    0H                                               02S0446 02004000
@EC00446 DS    0H                                               02S0447 02005000
*    WMSTPTR = RMCTWMST;            /* PICK WLM SPECIFICATION TABLE  */ 02006000
         L     WMSTPTR,RMCTWMST(,RMCTPTR)                       02S0447 02007000
*    WPGDPTR = WMSTPGDT + OUCBWMG;  /* PICK CURR PERF GRP DESCRIPTOR */ 02008000
         LH    WPGDPTR,OUCBWMG(,OUCBPTR)                        02S0448 02009000
         AL    WPGDPTR,WMSTPGDT(,WMSTPTR)                       02S0448 02010000
*    IF OUCBPVL = '0'B THEN         /* USER SUBJECT TO WLM ANALYSIS  */ 02011000
