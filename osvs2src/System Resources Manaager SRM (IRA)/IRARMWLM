         AL    @09,ONE                                          02S0396 01776000
         STH   @09,OUCBSWC(,OUCBPTR)                            02S0396 01777000
*      CALL IRARMWMI;               /* UPDAT TRANSACTION STATISTICS  */ 01778000
         BAL   @14,IRARMWMI                                     02S0397 01779000
*      CALL IRARMWM5;               /* CHECK TRANSACTION PERIOD END  */ 01780000
         LA    @15,IRARMWM5                                     02S0398 01781000
         BALR  @14,@15                                          02S0398 01782000
*      CALL IRARMWM7;               /* OBTAIN USER NORMALIZED LEVEL  */ 01783000
         BAL   @14,IRARMWM7                                     02S0399 01784000
