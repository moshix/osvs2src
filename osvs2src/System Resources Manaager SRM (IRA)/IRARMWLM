*            OUCBTMP = RRPATOD;     /* SAVE NEW PERIOD STARTING TIME */ 00695000
         B     @RC00126                                         02S0128 00696000
@RF00126 L     @09,RRPATOD(,RRPAPTR)                            02S0128 00697000
         ST    @09,OUCBTMP(,OUCBPTR)                            02S0128 00698000
*          IF OUCBOUT = '0'B THEN   /* USER FIELDS ARE AVAILABLE NOW */ 00699000
@RC00126 TM    OUCBOUT(OUCBPTR),B'00000100'                     02S0129 00700000
         BNZ   @RF00129                                         02S0129 00701000
*            /* UPDATE BEGINNING SERVICE VALUE FOR NEW PERIOD        */ 00702000
*            DO;                                                02S0130 00703000
*            OUCBPCH = '0'B;        /* RESET PERIOD CHNG OUTSTND FLG */ 00704000
         NI    OUCBPCH(OUCBPTR),B'11110111'                     02S0131 00705000
*            OUXBPRS = OUXBTRS;     /* SAVE NEW PERIOD START SERVICE */ 00706000
         L     @09,OUXBTRS(,OUXBPTR)                            02S0132 00707000
         ST    @09,OUXBPRS(,OUXBPTR)                            02S0132 00708000
*            IF OUCBRTR ^= '1'B &   /* TRANSACT NOT IN SUSPEND STATE */ 00709000
*                  OUCBTMS ^= RRPATOD THEN /* XACTION NOT YET UPDATD */ 00710000
         TM    OUCBRTR(OUCBPTR),B'00010000'                     02S0133 00711000
         BO    @RF00133                                         02S0133 00712000
         CLC   OUCBTMS(4,OUCBPTR),RRPATOD(RRPAPTR)              02S0133 00713000
         BE    @RF00133                                         02S0133 00714000
*              OUXBPRS = OUXBPRS + OUCBWMS; /* ADD CURR NTRVL SERVIC */ 00715000
         AL    @09,OUCBWMS(,OUCBPTR)                            02S0134 00716000
         ST    @09,OUXBPRS(,OUXBPTR)                            02S0134 00717000
*            END;                                               02S0135 00718000
*          ELSE                     /* UPDATE WHEN USER SWAPPED IN   */ 00719000
*            OUCBPCH = '1'B;        /* INDICAT PERIOD CHNG INCOMPLET */ 00720000
         B     @RC00129                                         02S0136 00721000
@RF00129 OI    OUCBPCH(OUCBPTR),B'00001000'                     02S0136 00722000
*          IF RMCTMFA = '1'B &      /* WRKLD ACTIVITY BEING MEASURED */ 00723000
*                OUCBATR = '1'B &   /* HAVE TRANSACTION FOR THIS USR */ 00724000
*                (OUCBRDY = '1'B | OUCBOUT = '0'B) & /* XACTN ACTIVE */ 00725000
*                OUCBTMS ^= RRPATOD THEN /* SERVICE ALREADY RECORDED */ 00726000
@RC00129 TM    RMCTMFA(RMCTPTR),B'10000000'                     02S0137 00727000
         BNO   @RF00137                                         02S0137 00728000
         TM    OUCBATR(OUCBPTR),B'10000000'                     02S0137 00729000
         BNO   @RF00137                                         02S0137 00730000
         TM    OUCBRDY(OUCBPTR),B'10000000'                     02S0137 00731000
         BO    @GL00014                                         02S0137 00732000
         TM    OUCBOUT(OUCBPTR),B'00000100'                     02S0137 00733000
         BNZ   @RF00137                                         02S0137 00734000
@GL00014 CLC   OUCBTMS(4,OUCBPTR),RRPATOD(RRPAPTR)              02S0137 00735000
         BE    @RF00137                                         02S0137 00736000
*            /* INVOKE MEASUREMENT ROUTINE TO COLLECT PERIOD SERVICE */ 00737000
*            DO;                                                02S0138 00738000
*            RRPAWAR = '1'B;        /* INSERT WKLD ACTIVTY RTNE FLAG */ 00739000
         OI    RRPAWAR(RRPAPTR),B'01000000'                     02S0139 00740000
*            /* ? IRACTLCL EPN(WR4) */                          02S0140 00741000
*            CALL RMSBWR4 -> IRARMRTN ;         /* INVOKE WR4 SUBRTN */ 00742000
         L     @15,RMSBWR4(,RMCTPTR)                            02S0140 00743000
         BALR  @14,@15                                          02S0140 00744000
*            OUCBPGP = OUCBPGP + LENGTH(WPGP); /* STEPP PERFM PERIOD */ 00745000
         LA    @09,8                                            02S0141 00746000
         SLR   @08,@08                                          02S0141 00747000
         IC    @08,OUCBPGP(,OUCBPTR)                            02S0141 00748000
         ALR   @09,@08                                          02S0141 00749000
         STC   @09,OUCBPGP(,OUCBPTR)                            02S0141 00750000
*            OUCBMAR = '1'B;        /* SPECFY NEGATIV PERIOD ACCUMUL */ 00751000
         OI    OUCBMAR(OUCBPTR),B'00000100'                     02S0142 00752000
*            /* ? IRACTLCL EPN(WR4) */                          02S0143 00753000
*            CALL RMSBWR4 -> IRARMRTN ;         /* INVOKE WR4 SUBRTN */ 00754000
         L     @15,RMSBWR4(,RMCTPTR)                            02S0143 00755000
         BALR  @14,@15                                          02S0143 00756000
*            OUCBMAR = '0'B;        /* RESET NEGATIVE PERIOD ACCUMUL */ 00757000
         NI    OUCBMAR(OUCBPTR),B'11111011'                     02S0144 00758000
*            RRPAWAR = '0'B;        /* DELETE WKLD ACTIVTY RTNE FLAG */ 00759000
         NI    RRPAWAR(RRPAPTR),B'10111111'                     02S0145 00760000
*            END;                                               02S0146 00761000
*          ELSE                     /* NO NEED TO REPORT PERIOD CHNG */ 00762000
*            OUCBPGP = OUCBPGP + LENGTH(WPGP); /* STEPP PERFM PERIOD */ 00763000
         B     @RC00137                                         02S0147 00764000
@RF00137 LA    @09,8                                            02S0147 00765000
         SLR   @08,@08                                          02S0147 00766000
         IC    @08,OUCBPGP(,OUCBPTR)                            02S0147 00767000
         ALR   @09,@08                                          02S0147 00768000
         STC   @09,OUCBPGP(,OUCBPTR)                            02S0147 00769000
*          WPGPPTR = WPGDPTR + OUCBPGP; /* PICK CURR PERF GRP PERIOD */ 00770000
@RC00137 LR    WPGPPTR,WPGDPTR                                  02S0148 00771000
         SLR   @09,@09                                          02S0148 00772000
         IC    @09,OUCBPGP(,OUCBPTR)                            02S0148 00773000
         ALR   WPGPPTR,@09                                      02S0148 00774000
*          IF OUCBDMN ^= WPGPDMN THEN   /* SWITCHING DOMAINS ??      */ 00775000
         CLC   OUCBDMN(1,OUCBPTR),WPGPDMN(WPGPPTR)              02S0149 00776000
         BE    @RF00149                                         02S0149 00777000
*            DO;                        /* YES                       */ 00778000
*            OUCBDMN = WPGPDMN ;    /* SAVE THE NEW DOMAIN NUMBER    */ 00779000
         SLR   @09,@09                                          02S0151 00780000
         IC    @09,WPGPDMN(,WPGPPTR)                            02S0151 00781000
         STC   @09,OUCBDMN(,OUCBPTR)                            02S0151 00782000
*            OUCBDMO = WDMVTENT;    /* OFFSET INTO THE DOMAIN TABLE  */ 00783000
         ALR   @09,@09                                          02S0152 00784000
         AL    @09,WMSTDMVT(,WMSTPTR)                           02S0152 00785000
         LH    @09,WDMVTENT(,@09)                               02S0152 00786000
         STH   @09,OUCBDMO(,OUCBPTR)                            02S0152 00787000
*            /* RE-POSITION USER AT END OF QUEUE                     */ 00788000
*            OUCBGOB = '1'B;        /* QUEUE POSITION BEING CHANGED  */ 00789000
         OI    OUCBGOB(OUCBPTR),B'00100000'                     02S0153 00790000
*              /* ? IRACTLCL EPN(RPS) */                        02S0154 00791000
*              DO;                                              02S0154 00792000
*              R1 = ADDR(RMEPBRPS);          /* RPS REQUEST IS PARAM */ 00793000
         LA    R1,RMEPBRPS(,RMCTPTR)                            02S0155 00794000
*              CALL RMSBCRN -> IRARMRTN ;       /* ACTION INVK FUNCT */ 00795000
         L     @15,RMSBCRN(,RMCTPTR)                            02S0156 00796000
         BALR  @14,@15                                          02S0156 00797000
*              END;                                             02S0157 00798000
*            END;                                               02S0158 00799000
*          RFY (NTRREG,WPGLPTR,R15) UNRSTD;   /*             @Z40BPEB*/ 00800000
@RF00149 DS    0H                                               02S0160 00801000
*          IF OUCBAPG='1'B          /* IF USER IN APG        @Z40BPEB*/ 00802000
*           THEN IF WPGPAPG = '1'B &   /* PERIOD APG         @Z40BPEB*/ 00803000
         TM    OUCBAPG(OUCBPTR),B'01000000'                     02S0160 00804000
         BNO   @RF00160                                         02S0160 00805000
*             (WPGPAPGV > CCCAPGDP-CCCAPMDP)  /*SPECIFIES DP @Z40BPEB*/ 00806000
*            THEN                  /* ABOVE MEAN TIME WAIT   @Z40BPEB*/ 00807000
         TM    WPGPAPG(WPGPPTR),B'00100000'                     02S0161 00808000
         BNO   @RF00161                                         02S0161 00809000
         SLR   @15,@15                                          02S0161 00810000
         IC    @15,WPGPAPGV(,WPGPPTR)                           02S0161 00811000
         LH    @09,CCCAPMDP(,RMCTPTR)                           02S0161 00812000
         LH    @08,CCCAPGDP(,RMCTPTR)                           02S0161 00813000
         SLR   @08,@09                                          02S0161 00814000
         CR    @15,@08                                          02S0161 00815000
         BNH   @RF00161                                         02S0161 00816000
*              OUCBNDP=CCCAPMDP+WPGPAPGV; /*REQUESTED DP     @Z40BPEB*/ 00817000
         ALR   @09,@15                                          02S0162 00818000
         STC   @09,OUCBNDP(,OUCBPTR)                            02S0162 00819000
*            ELSE                   /* IN MEAN TIME TO WAIT  @Z40BPEB*/ 00820000
*              IF OUCBNDP > CCCAPGDP  /* LAST PERIOD WAS     @Z40BPEB*/ 00821000
*                THEN                 /* ABOVE MTW           @Z40BPEB*/ 00822000
         B     @RC00161                                         02S0163 00823000
@RF00161 LH    @15,CCCAPGDP(,RMCTPTR)                           02S0163 00824000
         SLR   @09,@09                                          02S0163 00825000
         IC    @09,OUCBNDP(,OUCBPTR)                            02S0163 00826000
         CR    @15,@09                                          02S0163 00827000
         BNL   @RF00163                                         02S0163 00828000
*                  OUCBNDP=CCCAPGDP;  /*BEGIN MTW DP.        @Z40BPEB*/ 00829000
         STC   @15,OUCBNDP(,OUCBPTR)                            02S0164 00830000
*          RFY (NTRREG,WPGLPTR,R15) RSTD;     /*HOLD R15    @Z40BPEB*/  00831000
@RF00163 DS    0H                                               02S0165 00832000
@RC00161 DS    0H                                               02S0165 00833000
@RF00160 DS    0H                                               02S0166 00834000
*          IF WPGPUNT = '1'B &      /* PERIOD DURATION IN REAL TIME  */ 00835000
*                OUCBOFF = '1'B THEN /* MULTI-CHECK ONLY WAIT USERS  */ 00836000
         TM    WPGPUNT(WPGPPTR),B'01000000'                     02S0166 00837000
         BNO   @RF00166                                         02S0166 00838000
         TM    OUCBOFF(OUCBPTR),B'00001000'                     02S0166 00839000
         BNO   @RF00166                                         02S0166 00840000
*            SVRE = RMSBWM5;        /* WILL RECURSE THROUGH WM5 RTN  */ 00841000
         L     SVRE,RMSBWM5(,RMCTPTR)                           02S0167 00842000
*          ELSE                     /* PERIOD DURATN IN SERVC UNITS  */ 00843000
*            SVRE = WM5RTRN;        /* WILL RETURN TO REQSTR OF WM5  */ 00844000
         B     @RC00166                                         02S0168 00845000
@RF00166 L     SVRE,WM5RTRN(,SVEREG)                            02S0168 00846000
*          CALL IRARMWMJ;           /* SET APPLICABLE ALGORITHM INDS */ 00847000
@RC00166 BAL   @14,IRARMWMJ                                     02S0169 00848000
*          NTRREG = SVRE;           /* RESTORE WM5 EXIT REGISTER     */ 00849000
         LR    NTRREG,SVRE                                      02S0170 00850000
*          END;                                                 02S0171 00851000
*      END;                                                     02S0172 00852000
@RF00124 DS    0H                                               02S0173 00853000
*    END;                                                       02S0173 00854000
@RF00112 DS    0H                                               02S0174 00855000
*  /* ? IRACTLEP ENN(WM5) */                                    02S0174 00856000
*  GEN CODE EXIT NOSEQFLOW;                                     02S0174 00857000
@RF00102 DS    0H                                               02S0174 00858000
*                                  /*                                   00859000
         LM    @00,@14,128+000(@13)    RELOAD INVOKING RTNE REGS        00860000
         BR    NTRREG                  EXIT TO REQUESTOR/RECURSE        00861000
*                                                               01S0175 00862000
*/* USER RECOMMENDATION VALUE CALCULATION ROUTINE                    */ 00863000
*/* ******************************************************************/ 00864000
*/*                                                                  */ 00865000
*/*    ROUTINE NAME - IRARMWM7                                       */ 00866000
*/*                                                                  */ 00867000
*/*    DESCRIPTIVE NAME - WLM RECOMMENDATION CALCULATION ROUTINE     */ 00868000
*/*                                                                  */ 00869000
*/*    FUNCTION - THE IRARMWM7 ROUTINE CALCULATES A WORKLOAD         */ 00870000
*/*               MANAGER RECOMMENDATION VALUE FOR A USER BASED ON   */ 00871000
*/*               THE SERVICE WHICH WAS RECEIVED AND ON THE          */ 00872000
*/*               PERFORMANCE OBJECTIVE CURRENTLY ASSOCIATED WITH    */ 00873000
*/*               THE USER. USERS WHICH HAVE NOT YET RECEIVIED AN    */ 00874000
*/*               AMOUNT OF SERVICE EQUAL TO THEIR INTERVAL SERVICE  */ 00875000
*/*               VALUE (ISV) SPECIFICATION WHILE IN CORE ARE GIVEN  */ 00876000
*/*               A RECOMMENDATION VALUE BOOST. THE BOOST GIVES      */ 00877000
*/*               PREFERENTIAL TREATMENT TO USERS IN THEIR ISV AS    */ 00878000
*/*               COMPARED TO USERS NOT IN THEIR ISV AND USERS       */ 00879000
*/*               BETWEEN JOB STEPS.                                 */ 00880000
*/*                                                                  */ 00881000
*/*    CALLED BY - IRARMWM3 - INDIVIDUAL USER EVALUATION SUBROUTINE  */ 00882000
*/*              - IRARMWM2 - WLM SWAPPABLE USER EVALUATION ROUTINE  */ 00883000
*/*              - IRARMWMO - WLM TRANSACTION STOP ROUTINE           */ 00884000
*/*              - IRARMWMQ - WLM QUIESCE COMPLETE PROCESSING ROUTIN */ 00885000
*/*                                                                  */ 00886000
*/*    INPUT - REG 4 CONTAINS THE ADDRESS OF THE OUCB FOR THE USER   */ 00887000
*/*            WHOSE RECOMMENDATION VALUE IS TO BE DETERMINED.       */ 00888000
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */ 00889000
*/*          - REG 15 CONTAINS THE IRARMWM7 ROUTINE ADDRESS.         */ 00890000
*/*                                                                  */ 00891000
*/*    OUTPT - OUCBWMR CONTAINS THE USER RECOMMENDATION VALUE.       */ 00892000
*/*                                                                  */ 00893000
*/*    EXIT - BRANCH ON REGISTER 14.                                 */ 00894000
*/*         - THERE IS NO ERROR EXIT OR INDICATION.                  */ 00895000
*/*                                                                  */ 00896000
*/* ******************************************************************/ 00897000
*IRARMWM7: PROC OPTIONS(NOSAVE,NOSAVEAREA,NOENTREG,RETREG,NOPARMREG);   00898000
IRARMWM7 DS    0H                                               02S0176 00899000
*    IF OUCBNSW = '0'B &           /* USER IS SWAPPABLE     */  02S0176 00900000
*       OUCBGOO = '0'B & OUCBGOI = '0'B & /* NOT TRANSITIONING       */ 00901000
*       OUCBGOB = '0'B THEN                                     02S0176 00902000
         TM    OUCBNSW(OUCBPTR),B'10000000'                     02S0176 00903000
         BNZ   @RF00176                                         02S0176 00904000
         TM    OUCBGOO(OUCBPTR),B'11100000'                     02S0176 00905000
         BNZ   @RF00176                                         02S0176 00906000
*      DO;                                                      02S0177 00907000
*      /* PICK UP PERFORMANCE GROUP PERIOD                           */ 00908000
*      WMSTPTR = RMCTWMST ;         /* PICK WLM SPECIFICATION TABLE  */ 00909000
         L     WMSTPTR,RMCTWMST(,RMCTPTR)                       02S0178 00910000
*      WPGDPTR = WMSTPGDT + OUCBWMG ;  /* PICK PERF GROUP DESCRIPTOR */ 00911000
         LH    WPGDPTR,OUCBWMG(,OUCBPTR)                        02S0179 00912000
         AL    WPGDPTR,WMSTPGDT(,WMSTPTR)                       02S0179 00913000
*      WPGPPTR = WPGDPTR + OUCBPGP; /* PICK PERF GROUP PERIOD        */ 00914000
         LR    WPGPPTR,WPGDPTR                                  02S0180 00915000
         SLR   @09,@09                                          02S0180 00916000
         IC    @09,OUCBPGP(,OUCBPTR)                            02S0180 00917000
         ALR   WPGPPTR,@09                                      02S0180 00918000
*      /* SET PERFORMANCE OBJECTIVE APPLICABLE TO USER               */ 00919000
*      R1 = WMSTPODT + WPOVTENT;    /* PTR TO PERF PERIOD OBJECTIVE  */ 00920000
         SLR   @09,@09                                          02S0181 00921000
         IC    @09,WPGPOBJN(,WPGPPTR)                           02S0181 00922000
         ALR   @09,@09                                          02S0181 00923000
         AL    @09,WMSTPOVT(,WMSTPTR)                           02S0181 00924000
         LH    @09,WPOVTENT(,@09)                               02S0181 00925000
         AL    @09,WMSTPODT(,WMSTPTR)                           02S0181 00926000
         LR    R1,@09                                           02S0181 00927000
*      WPGLPTR = WPGDPTR + WPGPISVP; /* PTR TO ISV VALUE             */ 00928000
         LR    WPGLPTR,WPGDPTR                                  02S0182 00929000
         SLR   @09,@09                                          02S0182 00930000
         IC    @09,WPGPISVP(,WPGPPTR)                           02S0182 00931000
         ALR   WPGLPTR,@09                                      02S0182 00932000
*      SVRO = ZERO;                 /* INITIALIZE ISV BOOST          */ 00933000
         SLR   SVRO,SVRO                                        02S0183 00934000
*      /* TEST IF SERVICE RECIEVED LESS THAN ISV AMOUNT OR IF USER   */ 00935000
*      /* IS BETWEEN JOB STEPS. IF SO, ASSIGN RECOMMENDATION VALUE   */ 00936000
*      /* WHERE OBJECTIVE GOES TO ZERO POINT.                        */ 00937000
*      IF OUCBWMS < WPGLISV |       /* SERVICE LESS THAN ISV AMOUNT  */ 00938000
*        OUCBINC = '0'B THEN        /* OR JOB IS BETWEEN STEPS       */ 00939000
         L     @09,OUCBWMS(,OUCBPTR)                            02S0184 00940000
         C     @09,WPGLISV(,WPGLPTR)                            02S0184 00941000
         BL    @RT00184                                         02S0184 00942000
         TM    OUCBINC(OUCBPTR),B'00000001'                     02S0184 00943000
         BNZ   @RF00184                                         02S0184 00944000
@RT00184 DS    0H                                               02S0185 00945000
*        DO;                                                    02S0185 00946000
*        R0 = ZERO;                 /* PEG NORMLZ LEVEL AT ZERO RATE */ 00947000
         SLR   R0,R0                                            02S0186 00948000
*        IF OUCBPVL = '0'B &        /* USER NOT PRIVILEDGED          */ 00949000
*           OUCBOUT = '0'B &        /* AND IN CORE                   */ 00950000
*           OUCBTMW ^= ZERO THEN    /* AND HAS TRANSACTION INTERVAL  */ 00951000
         TM    OUCBPVL(OUCBPTR),B'00000100'                     02S0187 00952000
         BNZ   @RF00187                                         02S0187 00953000
         TM    OUCBOUT(OUCBPTR),B'00000100'                     02S0187 00954000
         BNZ   @RF00187                                         02S0187 00955000
         L     @09,OUCBTMW(,OUCBPTR)                            02S0187 00956000
         LTR   @09,@09                                          02S0187 00957000
         BZ    @RF00187                                         02S0187 00958000
*          SVRO = RMCAISV;          /* GIVE USER ISV BOOST           */ 00959000
         LH    SVRO,RMCAISV(,RMCTPTR)                           02S0188 00960000
         N     SVRO,@CF02217                                    02S0188 00961000
*        END;                                                   02S0189 00962000
*      ELSE                                                     02S0190 00963000
*        /* CALCULATE RATE - DIVIDE AMOUNT OF SERVICE BY TIME NTRVL  */ 00964000
*        DO;                                                    02S0190 00965000
         B     @RC00184                                         02S0190 00966000
@RF00184 DS    0H                                               02S0191 00967000
*        WKRO = RRPATOD - OUCBTMW;  /* SET WORKLOAD INTERVAL DURATN  */ 00968000
         L     WKRO,RRPATOD(,RRPAPTR)                           02S0191 00969000
         SL    WKRO,OUCBTMW(,OUCBPTR)                           02S0191 00970000
*        VLRO = MAX(VLRO,ONE);      /* PROTECT FRM DIVIDE EXCEPTION  */ 00971000
         L     @09,ONE                                          02S0192 00972000
         CR    VLRO,@09                                         02S0192 00973000
         BNL   *+6                                                      00974000
         LR    VLRO,@09                                         02S0192 00975000
*        R0 = OUCBWMS * RMTMFCT / WKRO; /* SERVICE RATE FOR INTERVL  */ 00976000
         LH    @09,RMTMFCT                                      02S0193 00977000
         M     @08,OUCBWMS(,OUCBPTR)                            02S0193 00978000
         DR    @08,WKRO                                         02S0193 00979000
         LR    R0,@09                                           02S0193 00980000
*        END;                                                   02S0194 00981000
*      WKRO = RTNREG;               /* SAVE RETURN REG CONTENTS      */ 00982000
@RC00184 LR    WKRO,RTNREG                                      02S0195 00983000
*       /* ? IRACTLCL EPN(WM4) */                               02S0196 00984000
*       CALL RMSBWM4 -> IRARMRTN ;              /* INVOKE WM4 SUBRTN */ 00985000
         L     @15,RMSBWM4(,RMCTPTR)                            02S0196 00986000
         BALR  @14,@15                                          02S0196 00987000
*        /* CALC USER NORM WKL LEVEL      */                    02S0197 00988000
*      OUCBWMR = R0+SVRO ;          /* GIVE USER NORMALIZED LEVEL    */ 00989000
         LR    @09,R0                                           02S0197 00990000
         ALR   @09,SVRO                                         02S0197 00991000
         ST    @09,OUCBWMR(,OUCBPTR)                            02S0197 00992000
*      OUCBTMA=RRPATOD ;            /* INDICATE USER RECENTLY ANALZD */ 00993000
         L     @09,RRPATOD(,RRPAPTR)                            02S0198 00994000
         ST    @09,OUCBTMA(,OUCBPTR)                            02S0198 00995000
*      RTNREG = WKRO;               /* RESTORE RETURN REG CONTENTS   */ 00996000
         LR    RTNREG,WKRO                                      02S0199 00997000
*      END;                                                     02S0200 00998000
*    END IRARMWM7;                                              02S0201 00999000
@EL00002 DS    0H                                               02S0201 01000000
@EF00002 DS    0H                                               02S0201 01001000
@ER00002 BR    @14                                              02S0201 01002000
*                                                               01S0202 01003000
*/* USERRDY SYSEVENT PROCESSOR                                       */ 01004000
*/* **************************************************************** */ 01005000
*/*                                                                  */ 01006000
*/*    ROUTINE NAME - IRARMHIT                                       */ 01007000
*/*                                                                  */ 01008000
*/*    DESCRIPTIVE NAME - WORKLOAD MANAGER USER READY SYSEVENT       */ 01009000
*/*                       SWAP-IN SCHEDULING ROUTINE                 */ 01010000
*/*                                                                  */ 01011000
*/*    FUNCTION - THE IRARMHIT ROUTINE RECEIVES CONTROL AS THE       */ 01012000
*/*          RESULT OF A DECISION TO APPLY SWAPIN PROCESSING         */ 01013000
*/*          TO A NEWLY AWAKENED USER.                               */ 01014000
*/*                                                                  */ 01015000
*/*    CALLED BY - IRARME04 - USER READY SYSEVENT PROCESS RTNE       */ 01016000
*/*                IRARME15 - SWAP OUT COMPLETE SYSEVENT RTNE        */ 01017000
*/*                IRARME44 - BRINGIN SYSEVENT RTNE                  */ 01018000
*/*                                                                  */ 01019000
*/*    INPUT - REG4 CONTAINS THE ADDRESS OF THE OUCB FOR THE USER    */ 01020000
*/*                                                                  */ 01021000
*/*    OUTPT - SEE FUNCTION.                                         */ 01022000
*/*                                                                  */ 01023000
*/*    EXIT  - BRANCH ON REGISTER 14.                                */ 01024000
*/*          - THERE IS NO ERROR EXIT OR INDICATION.                 */ 01025000
*/*                                                                  */ 01026000
*/*    CHANGE ACTIVITY - XXXXXXX                                    */  01027000
*/*                                                                  */ 01028000
*/* **************************************************************** */ 01029000
*  /* ? IRACTLEP EPN(HIT) */                                    02S0202 01030000
*  DCL IRARMHIT ENTRY INT;                                      02S0202 01031000
*  GEN CODE REFS(RMCTRMSA,RRPAEPA)                              02S0203 01032000
