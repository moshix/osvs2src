         BNE   @RF00575                                         03S0575 00381000
@RT00575 DS    0H                                               03S0576 00382000
*                       RETCODE=RCDSPDSP;/* SET RETURN CODE FOR 03S0576 00383000
*                                      DSPDISP               @YM04669*/ 00384000
         MVI   RETCODE,X'04'                                    03S0576 00385000
*                     ELSE                                      03S0577 00386000
*                       CALL LCBPROC;/* PROCESS THE LCB      @YM05631*/ 00387000
         B     @RC00575                                         03S0577 00388000
@RF00575 BAL   @14,LCBPROC                                      03S0577 00389000
*                 END;                                          03S0578 00390000
*               ELSE                                            03S0579 00391000
*                 CALL LCBPROC;     /* PROCESS LCB POST TO SEND 03S0579 00392000
*                                      SCHEDULER                     */ 00393000
         B     @RC00566                                         03S0579 00394000
@RF00566 BAL   @14,LCBPROC                                      03S0579 00395000
*             END;                  /*                       @Y17XAYP*/ 00396000
*         END;                      /*                       @YM08526*/ 00397000
