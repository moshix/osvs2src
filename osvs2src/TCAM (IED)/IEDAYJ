*         RETCODE=TCAMSEND;         /* INDICATE TCAM SEND STATE      */ 03031000
         MVI   RETCODE,X'08'                                    02S1310 03032000
*       ELSE                                                    02S1311 03033000
*         RETCODE=REMOVESS;         /* SET RETURN TO REMOVE SEND        03034000
*                                      SCHEDULER                     */ 03035000
         B     @RC01309                                         02S1311 03036000
