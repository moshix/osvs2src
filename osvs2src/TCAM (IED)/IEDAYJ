*         CALL DSPUNAVR;            /* MOVE SEND SCHED TO QCB        */ 03355000
         LA    @15,DSPUNAVR(,RDISP)                             02S1390 03356000
         BALR  @14,@15                                          02S1390 03357000
*         RQCB=TRMDESTQ;            /* RESTORE QCB ADDRESS           */ 03358000
         SLR   RQCB,RQCB                                        02S1391 03359000
