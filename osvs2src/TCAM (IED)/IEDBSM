@EF00002 DS    0H                                                  0331 00463000
@ER00002 BR    @14                                                 0331 00464000
*END DOACTION;                                                     0332 00465000
         B     @EL00002                                            0332 00466000
*/********************************************************************/ 00467000
*/*                                                                  */ 00468000
*/* THIS ROUTINE CALCULATES THE INPUT COORDINATE. IT IS DONE IN      */ 00469000
*/* TWO STEPS.                                                       */ 00470000
*/*          1) COMPUTE THE MASK VALUE. THE MASK IS COMPOSED OF BITS */ 00471000
*/*             WHICH ARE SET ACCORDING TO THE TYPE OF INPUT.        */ 00472000
*/*          2) USE THE COMPUTED MASK VALUE AS AN INDEX INTO A TABLE */ 00473000
*/*             (THE TABLE USED DEPENDS UPON THE TYPE OF INPUT). THE */ 00474000
*/*             VALUE OBTAINED FROM THE TABLE IS THE INPUT           */ 00475000
*/*             COORDINATE.                                          */ 00476000
*/*                                                                  */ 00477000
*/********************************************************************/ 00478000
*DOICOORD:PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0333 00479000
DOICOORD DS    0H                                                  0334 00480000
*  IF TRHTYPE=OFF THEN             /* REQUEST                        */ 00481000
         LA    @10,4                                               0334 00482000
         ALR   @10,PLBMPTR                                         0334 00483000
         TM    TRHTYPE(@10),B'10000000'                            0334 00484000
         BNZ   @RF00334                                            0334 00485000
*    DO;                           /* YES                            */ 00486000
*      IF PLBMSTAT=PLBMNORM THEN   /* 'NORMAL' STATUS REQUEST        */ 00487000
         TM    PLBMSTAT(PLBMPTR),B'01100000'                       0336 00488000
         BNZ   @RF00336                                            0336 00489000
*        DO;                       /* YES                            */ 00490000
*          NRQMASK=''B;            /* INIT MASK TO ZERO              */ 00491000
         XC    NRQMASK(2),NRQMASK                                  0338 00492000
*          IF TRHRUCAT^=TRHFMD THEN /* NOT AN FM DATA REQUEST        */ 00493000
         TM    TRHRUCAT(@10),B'01100000'                           0339 00494000
         BZ    @RF00339                                            0339 00495000
*            DO;                   /* YES                            */ 00496000
*              IF PLBMCMD=CD1CANCL THEN /* CANCEL                    */ 00497000
         CLI   PLBMCMD(PLBMPTR),X'83'                              0341 00498000
         BNE   @RF00341                                            0341 00499000
*                NRQTYPE='001'B;   /* YES-SET CANCEL TYPE BITS       */ 00500000
         NC    NRQTYPE(2),@CB00910                                 0342 00501000
         OI    NRQTYPE+1,B'01000000'                               0342 00502000
*              ELSE                                                0343 00503000
*                IF PLBMCMD=CD1BID THEN /* BID                       */ 00504000
         B     @RC00341                                            0343 00505000
@RF00341 CLI   PLBMCMD(PLBMPTR),X'C8'                              0343 00506000
         BNE   @RF00343                                            0343 00507000
*                  NRQTYPE='010'B; /* YES-SET BID TPYE BITS          */ 00508000
         NC    NRQTYPE(2),@CB00913                                 0344 00509000
         OI    NRQTYPE+1,B'10000000'                               0344 00510000
*                ELSE                                              0345 00511000
*                  IF PLBMCMD=CD1RTR THEN /* RTR                     */ 00512000
