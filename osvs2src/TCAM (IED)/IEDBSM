*/*                                                                  */ 00708000
*/* THIS ROUTINE CALCULATES THE STATE COORDINATE.  IT SIMPLY USES    */ 00709000
*/* THE VALUE IN THE SIBBSTAT (BRACKET STATE) FIELD.                 */ 00710000
*/*                                                                  */ 00711000
*/********************************************************************/ 00712000
*DOSCOORD:PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0402 00713000
DOSCOORD DS    0H                                                  0403 00714000
*  R2=SIBSTAT1;                    /* GET 1 BYTE FIELD CONTAINING  0403 00715000
*                                     SIBBSTAT                       */ 00716000
         SLR   R2,R2                                               0403 00717000
         IC    R2,SIBSTAT1(,RSIB)                                  0403 00718000
*  R2=R2&ANDMASK;                  /* USE A MASK TO AND OFF ALL BUT     00719000
*                                     SIBBSTAT BITS                  */ 00720000
         N     R2,@CF00746                                         0404 00721000
*  SCOORD=R2+1;                    /* SET STATE COORDINATE VALUE     */ 00722000
         LA    SCOORD,1                                            0405 00723000
         ALR   SCOORD,R2                                           0405 00724000
*  RETURN;                         /* RETURN TO CALLER               */ 00725000
