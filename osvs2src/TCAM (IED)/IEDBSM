         TITLE 'IEDBSM, BRACKET MANAGER                                *00001000
                        '                                               00002000
IEDBSM   CSECT ,                                                   0001 00003000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00004000
         BALR  @11,0                                               0001 00005000
@PSTART  DS    0H                                                  0001 00006000
         USING @PSTART,@11                                         0001 00007000
IEDBSM   IEDHJN START,HJN                                               00008000
*/********************************************************************/ 00009000
*/*                                                                  */ 00010000
*/*     M A I N     R O U T I N E                                    */ 00011000
*/*                                                                  */ 00012000
*/********************************************************************/ 00013000
*  RC=0;                           /* INIT RETURN CODE TO 0          */ 00014000
         SLR   RC,RC                                               0197 00015000
*  RLCB=PLBMLCB;                   /* LCB ADDRESSABILITY             */ 00016000
         L     RLCB,PLBMLCB(,PLBMPTR)                              0198 00017000
*  RSIB=PLBMSIB;                   /* SIB ADDRESSABILITY             */ 00018000
         L     RSIB,PLBMSIB(,PLBMPTR)                              0199 00019000
*/*                                                                  */ 00020000
*  CALL DOICOORD;                  /* CALCULATE INPUT COORDINATE     */ 00021000
         BAL   @14,DOICOORD                                        0200 00022000
*/*                                                                  */ 00023000
*  IF ICOORD^=0 THEN               /* VALID INPUT COORDINATE         */ 00024000
         LTR   ICOORD,ICOORD                                       0201 00025000
         BZ    @RF00201                                            0201 00026000
*    DO;                           /* YES                            */ 00027000
*      CALL DOSCOORD;              /* CALCULATE STATE COORDINATE     */ 00028000
         BAL   @14,DOSCOORD                                        0203 00029000
*      CALL DOACTION;              /* PERFORM ACTION                 */ 00030000
         BAL   @14,DOACTION                                        0204 00031000
*    END;                                                          0205 00032000
*/*                                                                  */ 00033000
*  RETURN CODE(RC);                /* RETURN (PASS RETURN CODE IN R15*/ 00034000
@RF00201 LR    @15,RC                                              0206 00035000
         L     @14,12(,@13)                                        0206 00036000
         LM    @00,@12,20(@13)                                     0206 00037000
         BR    @14                                                 0206 00038000
*/********************************************************************/ 00039000
*/*  E N D   O F   M A I N   R O U T I N E                           */ 00040000
*/********************************************************************/ 00041000
*/********************************************************************/ 00042000
*/*                                                                  */ 00043000
*/* THIS ROUTINE USES THE INPUT AND STATE COORDINATES PREVIOUSLY     */ 00044000
*/* CALCULATED TO OBTAIN THE ACTION CODE FROM THE BRACKET STATE      */ 00045000
*/* ACTION TABLE.  IT USES THE ACTION CODE FROM THIS TABLE AS AN     */ 00046000
*/* INDEX TO A CASE STATEMENT WHICH CAUSES CONTROL TO BE PASSED TO   */ 00047000
*/* THE PROPER SUBCASE ROUTINE. THE SUBCASE ROUTINE THEN PERFORMS    */ 00048000
*/* THE DESIRED ACTION.                                              */ 00049000
*/*                                                                  */ 00050000
*/********************************************************************/ 00051000
*DOACTION: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0207 00052000
DOACTION DS    0H                                                  0208 00053000
*  ACTION=BSATBL(ICOORD,SCOORD);   /* GET ACTION CODE FROM BRACKET STAT 00054000
*                                     ACTION TABLE                   */ 00055000
*                                                                  0208 00056000
