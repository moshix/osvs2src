@RF00343 CLI   PLBMCMD(PLBMPTR),X'05'                              0345 00514000
         BNE   @RF00345                                            0345 00515000
*                    NRQTYPE='011'B; /* YES-SET RTR TYPE BITS        */ 00516000
         NI    NRQTYPE,B'11111110'                                 0346 00517000
         OI    NRQTYPE+1,B'11000000'                               0346 00518000
*                  ELSE                                            0347 00519000
*                    NRQTYPE='100'B; /* 'OTHER' CATAGORY             */ 00520000
         B     @RC00345                                            0347 00521000
@RF00345 NI    NRQTYPE+1,B'00111111'                               0347 00522000
         OI    NRQTYPE,B'00000001'                                 0347 00523000
*            END;                                                  0348 00524000
@RC00345 DS    0H                                                  0348 00525000
@RC00343 DS    0H                                                  0348 00526000
@RC00341 DS    0H                                                  0349 00527000
*          IF PLBMSNDI=ON THEN     /* THIS REQUEST GOING OUTBOUND    */ 00528000
@RF00339 TM    PLBMSNDI(PLBMPTR),B'10000000'                       0349 00529000
         BNO   @RF00349                                            0349 00530000
*            NRQSNDI=ON;           /* YES-SET SEND INDICATOR         */ 00531000
         OI    NRQSNDI,B'00100000'                                 0350 00532000
*          IF TRHBB=ON THEN        /* BEGIN BRACKET (BB)             */ 00533000
@RF00349 TM    TRHBB+4(PLBMPTR),B'10000000'                        0351 00534000
         BNO   @RF00351                                            0351 00535000
*            NRQBB=ON;             /* YES-SET BB INDICATOR           */ 00536000
         OI    NRQBB,B'00010000'                                   0352 00537000
*          IF TRHEB=ON THEN        /* END BRACKET (EB)               */ 00538000
@RF00351 TM    TRHEB+4(PLBMPTR),B'01000000'                        0353 00539000
         BNO   @RF00353                                            0353 00540000
*            NRQEB=ON;             /* YES-SET EB INDICATOR           */ 00541000
         OI    NRQEB,B'00001000'                                   0354 00542000
*          IF (TRHDR1=ON | TRHDR2=ON) &                            0355 00543000
*             TRHER=OFF THEN       /* RQ ASKING FOR DEFINITE RESPONSE*/ 00544000
