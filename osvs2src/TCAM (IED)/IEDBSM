*       GOTO CE0001@; /*BRANCH TO END OF CASE*/                    0319 00429000
         B     CE0001@                                             0319 00430000
*     END BSA22; /*END OF SUBCASE*/                                0320 00431000
*/*                                                                  */ 00432000
*/*                                                                  */ 00433000
*/*                                                                  */ 00434000
*                                                                  0321 00435000
*BSA23:                                                            0321 00436000
*     DO; /*SUBCASE*/                                              0321 00437000
BSA23    DS    0H                                                  0322 00438000
*  SIBRTRP=ON;                     /* GO TO 'RTR PENDING' STATE      */ 00439000
         OI    SIBRTRP(RSIB),B'00010000'                           0322 00440000
*       GOTO CE0001@; /*BRANCH TO END OF CASE*/                    0323 00441000
         B     CE0001@                                             0323 00442000
*     END BSA23; /*END OF SUBCASE*/                                0324 00443000
*/*                                                                  */ 00444000
*/*                                                                  */ 00445000
*/*                                                                  */ 00446000
*                                                                  0325 00447000
*BSA24:                                                            0325 00448000
*     DO; /*SUBCASE*/                                              0325 00449000
BSA24    DS    0H                                                  0326 00450000
*  SIBBSTAT=SIBBETB;               /* GO TO 'BETWEEN BRACKET' STATE  */ 00451000
         NI    SIBBSTAT(RSIB),B'11110000'                          0326 00452000
