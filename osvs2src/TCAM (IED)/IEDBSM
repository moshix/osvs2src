         BNO   @RF00359                                            0359 00560000
*            NRQEC=ON;             /* YES-SET EC INDICATOR           */ 00561000
         OI    NRQEC,B'00000001'                                   0360 00562000
*/*                                                                  */ 00563000
*          ICOORD=NRQTBL(NRQMASK+1); /* USE MASK VALUE PLUS ONE(CANNNOT 00564000
*                                     HAVE 0 INDEX) TO INDEX 'NORMAL'   00565000
*                                     REQUEST TABLE TO GET INPUT   0361 00566000
