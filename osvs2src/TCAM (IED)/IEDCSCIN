         TITLE 'IEDCSCIN, COMMON SESSION CONTROL INBOUND               *00001000
                        '                                               00002000
IEDCSCIN CSECT ,                                                   0001 00003000
@PROLOG  DS    0H                                                  0002 00004000
         DC    A(0)                START OF QCB                         00005000
         DC    A(0)                                                     00006000
         DC    X'06'               START OF STCB                        00007000
         DC    AL3(*-1)                                                 00008000
         USING *,RBASE                                                  00009000
         LR    RBASE,R15           ESTABLISH ADDRESSABILTY              00010000
IEDCSCIN IEDHJN START,HJN                                               00011000
*                                                                  0397 00012000
*/********************************************************************/ 00013000
*/*                                                                  */ 00014000
*/*     M A I N     R O U T I N E                                    */ 00015000
*/*                                                                  */ 00016000
*/********************************************************************/ 00017000
*                                                                  0397 00018000
*   PIUBFRP=RPARM;                  /* PIU BUFFER ADDRESS            */ 00019000
         LR    PIUBFRP,RPARM                                       0397 00020000
*   PIUSTART=ADDR(PRFPIUO)+PRFPIUO; /* ADDRESS OF PIU DATA(TH-RH-RU) */ 00021000
         LA    PIUSTART,PRFPIUO(,IEDQPPTR)                         0398 00022000
         SLR   @09,@09                                             0398 00023000
         IC    @09,PRFPIUO(,IEDQPPTR)                              0398 00024000
         ALR   PIUSTART,@09                                        0398 00025000
*/*                                                                  */ 00026000
*                                                                  0399 00027000
*   CALL TTEFIND;                   /* GET OAF & DAF TERMINAL ENTRIES*/ 00028000
         BAL   @14,TTEFIND                                         0399 00029000
*   CALL SETCMD;                    /* SET CMDBYTE WITH COMMAND CODE */ 00030000
         BAL   @14,SETCMD                                          0400 00031000
*   RTRM=OAFTEPTR;                  /* OAF TERMINAL ENTRY ADDRESS    */ 00032000
         L     RTRM,OAFTEPTR                                       0401 00033000
*   IF TRMSIBPT^=0 THEN             /* IS A SIB PRESENT              */ 00034000
         LR    @09,IEDQTPTR                                        0402 00035000
         SL    @09,@CF01607                                        0402 00036000
         SLR   @02,@02                                             0402 00037000
         ICM   @02,7,TRMSIBPT(@09)                                 0402 00038000
         LTR   @02,@02                                             0402 00039000
         BZ    @RF00402                                            0402 00040000
*     DO;                           /* YES                           */ 00041000
*       SIBPTR=TRMSIBPT;            /* GET 1ST SIB ADDRESS           */ 00042000
         ST    @02,SIBPTR                                          0404 00043000
*       IF TTHDAF=SAVTSCPT THEN     /* IS DAF THE SSCP               */ 00044000
         L     @02,AVTSAVTP(,IEDQAPTR)                             0405 00045000
         CLC   TTHDAF(2,PIUSTART),SAVTSCPT(@02)                    0405 00046000
         BNE   @RF00405                                            0405 00047000
*         DO;                       /* YES                           */ 00048000
*           IF TRMTYPE=TRMLUNT THEN /* IS OAF AN LU                  */ 00049000
         CLI   TRMTYPE(@09),X'30'                                  0407 00050000
         BNE   @RF00407                                            0407 00051000
*             CALL SSCPLU;          /* SSCP-LU SESSION               */ 00052000
         BAL   @14,SSCPLU                                          0408 00053000
*           ELSE                    /* OAF NOT LU                    */ 00054000
*             DO;                                                  0409 00055000
         B     @RC00407                                            0409 00056000
@RF00407 DS    0H                                                  0410 00057000
*               IF TRMTYPE=TRMPUNT THEN/* IS OAF A PU(CLUSTER)       */ 00058000
         LR    @09,IEDQTPTR                                        0410 00059000
         AL    @09,@CF01611                                        0410 00060000
         CLI   TRMTYPE-11(@09),X'18'                               0410 00061000
         BNE   @RF00410                                            0410 00062000
*                 CALL SSCPPUC;     /* SSCP-PU(CLUSTER) SESS.        */ 00063000
         BAL   @14,SSCPPUC                                         0411 00064000
*               ELSE                /* OAF NOT A PU(CLUSTER)         */ 00065000
*                 DO;                                              0412 00066000
         B     @RC00410                                            0412 00067000
@RF00410 DS    0H                                                  0413 00068000
*                   IF TRMTYPE=TRMLNCP THEN/* IS OAF A PU(NCP)       */ 00069000
         LR    @09,IEDQTPTR                                        0413 00070000
         AL    @09,@CF01611                                        0413 00071000
         CLI   TRMTYPE-11(@09),X'0F'                               0413 00072000
         BNE   @RF00413                                            0413 00073000
*                     CALL SSCPPUN; /* SSCP-PU(NCP) SESS.            */ 00074000
         BAL   @14,SSCPPUN                                         0414 00075000
*                   ELSE            /* INVALID OAF TYPE              */ 00076000
*                     DO;                                          0415 00077000
         B     @RC00413                                            0415 00078000
@RF00413 DS    0H                                                  0416 00079000
*                       SNC=SNSNOSES;/* SENSE=SESSION NOT ACTIVE     */ 00080000
         MVC   SNC(4),@CB01525                                     0416 00081000
*                       CALL SNDERRSP;/* SEND ERROR RESPONSE         */ 00082000
         BAL   @14,SNDERRSP                                        0417 00083000
*                     END;                                         0418 00084000
*                 END;                                             0419 00085000
*             END;                                                 0420 00086000
*         END;                                                     0421 00087000
*       ELSE                        /* DAF IS AN LU                  */ 00088000
*         DO;                                                      0422 00089000
         B     @RC00405                                            0422 00090000
@RF00405 DS    0H                                                  0423 00091000
*           IF TRMTYPE=TRMLUNT THEN /* IS OAF AN LU                  */ 00092000
