*/********************************************************************/ 00426000
*/*                                                                  */ 00427000
*/* THIS ROUTINE POST THE PIU TO THE SSCP RESPONSE HANDLER           */ 00428000
*/* (IEDSCPRP) QCB.                                                  */ 00429000
*/*                                                                  */ 00430000
*/********************************************************************/ 00431000
*                                                                  0516 00432000
*POSTSSRP:                                                         0516 00433000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0516 00434000
POSTSSRP DS    0H                                                  0517 00435000
*   PRFQCBA=SAVTSSRP;               /* ADDRESS OF IEDSCPRP'S QCB     */ 00436000
         L     @10,AVTSAVTP(,IEDQAPTR)                             0517 00437000
         MVC   PRFQCBA(3,IEDQPPTR),SAVTSSRP+1(@10)                 0517 00438000
*   PRFPRI=PRISCPIN;                /* POST PRIORITY                 */ 00439000
         MVI   PRFPRI(IEDQPPTR),X'E5'                              0518 00440000
*   RPARM=PIUBFRP;                  /* PIU BUFFER ADDRESS            */ 00441000
         LR    RPARM,PIUBFRP                                       0519 00442000
*   CALL DSPPOST;                   /* POST THE PIU BUFFER TO      0520 00443000
*                                      IEDSCPRP                      */ 00444000
         LA    @15,DSPPOST(,RDISP)                                 0520 00445000
         BALR  @14,@15                                             0520 00446000
*   END POSTSSRP;                                                  0521 00447000
@EL00008 DS    0H                                                  0521 00448000
@EF00008 DS    0H                                                  0521 00449000
@ER00008 BR    @14                                                 0521 00450000
*                                                                  0522 00451000
*/********************************************************************/ 00452000
*/*                                                                  */ 00453000
*/* THIS ROUTINE POST THE PIU TO THE SSCP REQUEST HANDLER(IEDSCPRQ)  */ 00454000
*/* QCB.                                                             */ 00455000
*/*                                                                  */ 00456000
*/********************************************************************/ 00457000
*                                                                  0522 00458000
*POSTSSRQ:                                                         0522 00459000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0522 00460000
POSTSSRQ DS    0H                                                  0523 00461000
*   PRFQCBA=SAVTSSRQ;               /* ADDRESS OF IEDSCPRQ'S QCB     */ 00462000
         L     @10,AVTSAVTP(,IEDQAPTR)                             0523 00463000
         MVC   PRFQCBA(3,IEDQPPTR),SAVTSSRQ+1(@10)                 0523 00464000
*   PRFPRI=PRISCPIN;                /* POST PRIORITY                 */ 00465000
         MVI   PRFPRI(IEDQPPTR),X'E5'                              0524 00466000
*   RPARM=PIUBFRP;                  /* PIU BUFFER ADDRESS            */ 00467000
         LR    RPARM,PIUBFRP                                       0525 00468000
*   CALL DSPPOST;                   /* POST THE PIU BUFFER TO      0526 00469000
*                                      IEDSCPRQ                      */ 00470000
         LA    @15,DSPPOST(,RDISP)                                 0526 00471000
         BALR  @14,@15                                             0526 00472000
*   END POSTSSRQ;                                                  0527 00473000
@EL00009 DS    0H                                                  0527 00474000
@EF00009 DS    0H                                                  0527 00475000
@ER00009 BR    @14                                                 0527 00476000
*                                                                  0528 00477000
*/********************************************************************/ 00478000
*/*                                                                  */ 00479000
*/* THIS ROUTINE BUILDS AN ERROR RESPONSE. IT USES THE SENSE IN THE  */ 00480000
*/* FIELD NAMED 'SNC'(SENSE CODE).                                   */ 00481000
*/*                                                                  */ 00482000
*/********************************************************************/ 00483000
*                                                                  0528 00484000
*RSPBUILD:                                                         0528 00485000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0528 00486000
RSPBUILD DS    0H                                                  0529 00487000
*   R14SV03=R14;                    /* SAVE RETURN REG               */ 00488000
         ST    R14,R14SV03                                         0529 00489000
*/*                                                                  */ 00490000
*                                                                  0530 00491000
*   IF PRFTIC^=INVALTIC THEN        /* LAST UNIT IN THE PIU          */ 00492000
         CLC   PRFTIC(4,IEDQPPTR),@CB00932                         0530 00493000
         BE    @RF00530                                            0530 00494000
*     DO;                           /* NO-NOT LAST UNIT              */ 00495000
*       RPARM=PRFTIC;               /* SET TO DICARD FROM 2ND UNIT ON*/ 00496000
         L     RPARM,PRFTIC(,IEDQPPTR)                             0532 00497000
*       CALL DISCARD;               /* DISCARD ALL BUT 1ST UNIT OF 0533 00498000
*                                      PIU                           */ 00499000
         BAL   @14,DISCARD                                         0533 00500000
*       PRFTIC=INVALTIC;            /* MARK 1ST UNIT AS 'LAST'       */ 00501000
         MVC   PRFTIC(4,IEDQPPTR),@CB00932                         0534 00502000
*     END;                                                         0535 00503000
*                                                                  0535 00504000
*/*                                                                  */ 00505000
*                                                                  0536 00506000
*   IF TRHRUCAT=TRHFMD THEN         /* FM DATA                       */ 00507000
@RF00530 LA    @09,10                                              0536 00508000
         LR    @02,PIUSTART                                        0536 00509000
         ALR   @02,@09                                             0536 00510000
         TM    TRHRUCAT(@02),B'01100000'                           0536 00511000
         BNZ   @RF00536                                            0536 00512000
*     DO;                           /* YES                           */ 00513000
*       IF TTHDAF=SAVTSCPT THEN     /* IS THE DAF THE SSCP           */ 00514000
         L     @05,AVTSAVTP(,IEDQAPTR)                             0538 00515000
         CLC   TTHDAF(2,PIUSTART),SAVTSCPT(@05)                    0538 00516000
         BNE   @RF00538                                            0538 00517000
*         DO;                       /* YES                           */ 00518000
*           TTHDCF=10;              /* SET DATA COUNT TO 10          */ 00519000
         STCM  @09,3,TTHDCF(PIUSTART)                              0540 00520000
*           IF TRHSDI=OFF THEN      /* SENSE DATA INCLUDED           */ 00521000
         TM    TRHSDI(@02),B'00000100'                             0541 00522000
         BNZ   @RF00541                                            0541 00523000
*             RU(5:7)=RU(1:3);      /* NO-MOVE 1ST 3 BYTES(COMMAND 0542 00524000
*                                      CODE) OVER SO SENSE BYTES MAY    00525000
*                                      BE INSERTED                   */ 00526000
         LA    @09,13                                              0542 00527000
         ALR   @09,PIUSTART                                        0542 00528000
         MVC   RU+4(3,@09),RU(@09)                                 0542 00529000
*         END;                                                     0543 00530000
*       ELSE                        /* DAF NOT SSCP                  */ 00531000
*         TTHDCF=7;                 /* SET DATA COUNT = 7            */ 00532000
         B     @RC00538                                            0544 00533000
@RF00538 MVC   TTHDCF(2,PIUSTART),@CH00572                         0544 00534000
*     END;                                                         0545 00535000
*   ELSE                            /* NOT FM DATA                   */ 00536000
*     DO;                                                          0546 00537000
         B     @RC00536                                            0546 00538000
@RF00536 DS    0H                                                  0547 00539000
*       RU(5)=CMDBYTE;              /* MOVE IN COMMAND BYTE(LEAVE  0547 00540000
*                                      ROOM FOR SENSE BYTES)         */ 00541000
         MVC   RU+17(1,PIUSTART),CMDBYTE                           0547 00542000
*       TTHDCF=8;                   /* SET DATA COUNT = 8            */ 00543000
         MVC   TTHDCF(2,PIUSTART),@CH00044                         0548 00544000
*     END;                                                         0549 00545000
*                                                                  0549 00546000
*/* SWITCH THE OAF AND DAF                                           */ 00547000
*                                                                  0550 00548000
*   TNTOAF=TTHOAF;                  /* SAVE OAF                      */ 00549000
@RC00536 MVC   TNTOAF(2),TTHOAF(PIUSTART)                          0550 00550000
*   TTHOAF=TTHDAF;                  /* MOVE DAF INTO OAF             */ 00551000
         MVC   @TS00001(2),TTHDAF(PIUSTART)                        0551 00552000
         MVC   TTHOAF(2,PIUSTART),@TS00001                         0551 00553000
*   TTHDAF=TNTOAF;                  /* MOVE OAF INTO DAF             */ 00554000
         MVC   TTHDAF(2,PIUSTART),TNTOAF                           0552 00555000
*/* SET UP RH FOR RESPONSE                                           */ 00556000
*                                                                  0553 00557000
*   TRHTYPE=ON;                     /* SET RESPONSE INDICATOR        */ 00558000
*   TRHBC=ON;                       /* SET BEGIN CHAIN               */ 00559000
*   TRHEC=ON;                       /* SET END CHAIN                 */ 00560000
         LA    @09,10                                              0555 00561000
         ALR   @09,PIUSTART                                        0555 00562000
         OI    TRHTYPE(@09),B'10000011'                            0555 00563000
*   TRHPI=OFF;                      /* SET PACING OFF                */ 00564000
         NI    TRHPI(@09),B'11111110'                              0556 00565000
*   TRHSDI=ON;                      /* SET SENSE INCLUDED INDICATOR  */ 00566000
         OI    TRHSDI(@09),B'00000100'                             0557 00567000
*   TRHER=ON;                       /* SET ERROR INDICATOR           */ 00568000
         OI    TRHER(@09),B'00010000'                              0558 00569000
*/* MOVE IN SENSE CODE BYTES                                         */ 00570000
*                                                                  0559 00571000
*   RU(1:4)=SNC;                    /* SENSE BYTES                   */ 00572000
         MVC   RU+13(4,PIUSTART),SNC                               0559 00573000
*/*                                                                  */ 00574000
*                                                                  0560 00575000
*   R14=R14SV03;                    /* RESTORE RETURN REG            */ 00576000
         L     R14,R14SV03                                         0560 00577000
*   RETURN;                         /* RETURN TO CALLER              */ 00578000
@EL00010 DS    0H                                                  0561 00579000
@EF00010 DS    0H                                                  0561 00580000
@ER00010 BR    @14                                                 0561 00581000
*   END RSPBUILD;                                                  0562 00582000
*                                                                  0563 00583000
*/********************************************************************/ 00584000
*/*                                                                  */ 00585000
*/* THIS ROUTINE SETS UP A FIELD WHICH CONTAINS THE COMMAND BYTE.    */ 00586000
*/* THE NAME OF THE FIELD IS 'CMDBYTE'.                              */ 00587000
*/*                                                                  */ 00588000
*/********************************************************************/ 00589000
*                                                                  0563 00590000
*SETCMD:                                                           0563 00591000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0563 00592000
SETCMD   DS    0H                                                  0564 00593000
*   IF TRHRUCAT=TRHFMD THEN         /* FM DATA                       */ 00594000
         TM    TRHRUCAT+10(PIUSTART),B'01100000'                   0564 00595000
         BNZ   @RF00564                                            0564 00596000
*     CMDBYTE=0;                    /* YES-ZERO COMMAND BYTE FIELD   */ 00597000
         MVI   CMDBYTE,X'00'                                       0565 00598000
*   ELSE                            /* NOT FM DATA                   */ 00599000
*     DO;                                                          0566 00600000
         B     @RC00564                                            0566 00601000
@RF00564 DS    0H                                                  0567 00602000
*       IF TRHSDI=ON THEN           /* SENSE DATA INCLUDED           */ 00603000
         TM    TRHSDI+10(PIUSTART),B'00000100'                     0567 00604000
         BNO   @RF00567                                            0567 00605000
*         CMDBYTE=RU(5);            /* SET COMMAND BYTE              */ 00606000
         MVC   CMDBYTE(1),RU+17(PIUSTART)                          0568 00607000
*       ELSE                        /* NO SENSE INCLUDED             */ 00608000
*         CMDBYTE=RU(1);            /* SET COMMAND BYTE              */ 00609000
         B     @RC00567                                            0569 00610000
@RF00567 MVC   CMDBYTE(1),RU+13(PIUSTART)                          0569 00611000
*     END;                                                         0570 00612000
*   RETURN;                         /* RETURN TO CALLER              */ 00613000
@EL00011 DS    0H                                                  0571 00614000
@EF00011 DS    0H                                                  0571 00615000
@ER00011 BR    @14                                                 0571 00616000
*   END SETCMD;                                                    0572 00617000
         B     @EL00011                                            0572 00618000
*                                                                  0573 00619000
*/********************************************************************/ 00620000
*/*                                                                  */ 00621000
*/* THIS ROUTINE SENDS AN ERROR RESPONSE TO THE OAF.                 */ 00622000
*/*                                                                  */ 00623000
*/********************************************************************/ 00624000
*                                                                  0573 00625000
*SNDERRSP:                                                         0573 00626000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0573 00627000
SNDERRSP DS    0H                                                  0574 00628000
*   IF TRHTYPE=OFF&                 /* REQUEST AND...                */ 00629000
*       (TRHDR1=ON|                 /* ASKING FOR AN EXCEPTION OR    */ 00630000
*       TRHDR2=ON) THEN             /* DEFINITE RESPONSE             */ 00631000
         LA    @09,10                                              0574 00632000
         ALR   @09,PIUSTART                                        0574 00633000
         TM    TRHTYPE(@09),B'10000000'                            0574 00634000
         BNZ   @RF00574                                            0574 00635000
         TM    TRHDR1(@09),B'10100000'                             0574 00636000
         BZ    @RF00574                                            0574 00637000
*     DO;                           /* YES                           */ 00638000
*       CALL RSPBUILD;              /* BUILD AN ERROR RESPONSE       */ 00639000
         BAL   @14,RSPBUILD                                        0576 00640000
*       CALL BYPPCOUT;              /* BYPASS TO PATH CONTROL      0577 00641000
*                                      OUTBOUND (IEDPCOUT)           */ 00642000
         BAL   @14,BYPPCOUT                                        0577 00643000
*     END;                                                         0578 00644000
*   ELSE                                                           0579 00645000
*     DO;                                                          0579 00646000
         B     @RC00574                                            0579 00647000
@RF00574 DS    0H                                                  0580 00648000
*       RPARM=PIUBFRP;              /* PIU BUFFER ADDRESS            */ 00649000
         LR    RPARM,PIUBFRP                                       0580 00650000
*       CALL DISCARD;               /* DISCARD PIU                   */ 00651000
         BAL   @14,DISCARD                                         0581 00652000
*       CALL DSPDISP;               /* EXIT TO DISPATCHER            */ 00653000
         LA    @15,DSPDISP(,RDISP)                                 0582 00654000
         BALR  @14,@15                                             0582 00655000
*     END;                                                         0583 00656000
*   END SNDERRSP;                                                  0584 00657000
@EL00012 DS    0H                                                  0584 00658000
@EF00012 DS    0H                                                  0584 00659000
@ER00012 BR    @14                                                 0584 00660000
*                                                                  0585 00661000
*/********************************************************************/ 00662000
*/*                                                                  */ 00663000
*/* THIS ROUTINE CHECKS SESSION STATES FOR SSCP-LU FLOW.             */ 00664000
*/*                                                                  */ 00665000
*/********************************************************************/ 00666000
*                                                                  0585 00667000
*SSCPLU:                                                           0585 00668000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0585 00669000
SSCPLU   DS    0H                                                  0586 00670000
*   IF SIBINDEX^=SAVTSCPT THEN      /* IS THIS SSCP'S SIB            */ 00671000
         L     @09,SIBPTR                                          0586 00672000
         L     @02,AVTSAVTP(,IEDQAPTR)                             0586 00673000
         CLC   SIBINDEX(2,@09),SAVTSCPT(@02)                       0586 00674000
         BE    @RF00586                                            0586 00675000
*     SIBPTR=SIBCHAIN;              /* NO-GET NEXT SIB(MUST BE     0587 00676000
*                                                             SSCP'S)*/ 00677000
         L     @09,SIBCHAIN(,@09)                                  0587 00678000
         ST    @09,SIBPTR                                          0587 00679000
*/*                                                                  */ 00680000
*                                                                  0588 00681000
*   IF SIBSSTAT=SIBSACTV THEN       /* SESSION STATE ACTIVE          */ 00682000
@RF00586 L     @09,SIBPTR                                          0588 00683000
         TM    SIBSSTAT(@09),B'00011100'                           0588 00684000
         BNO   @RF00588                                            0588 00685000
*     CALL SSCPROUT;                /* ROUTE PIU TO SSCP             */ 00686000
         BAL   @14,SSCPROUT                                        0589 00687000
*   ELSE                            /* SESSION NOT ACTIVE            */ 00688000
*     DO;                                                          0590 00689000
         B     @RC00588                                            0590 00690000
@RF00588 DS    0H                                                  0591 00691000
*       IF TRHTYPE=ON&              /* RESPONSE AND...               */ 00692000
*           TRHRUCAT=TRHSC&         /* SESSION CONTROL AND...        */ 00693000
*           (CMDBYTE=CD1ACTLU|      /* ACTLU COMMAND OR...           */ 00694000
*           CMDBYTE=CD1DATLU) THEN  /* DACTLU COMMAND                */ 00695000
         LA    @09,10                                              0591 00696000
         ALR   @09,PIUSTART                                        0591 00697000
         TM    TRHTYPE(@09),B'10000000'                            0591 00698000
         BNO   @RF00591                                            0591 00699000
         TM    TRHRUCAT(@09),B'01100000'                           0591 00700000
         BNO   @RF00591                                            0591 00701000
         CLI   CMDBYTE,X'0D'                                       0591 00702000
         BE    @RT00591                                            0591 00703000
         CLI   CMDBYTE,X'0E'                                       0591 00704000
         BNE   @RF00591                                            0591 00705000
@RT00591 DS    0H                                                  0592 00706000
*         CALL POSTSSRP;            /* POST PIU TO SSCP RESPONSE   0592 00707000
*                                      HANDLER (IEDSCPRP) QCB        */ 00708000
         BAL   @14,POSTSSRP                                        0592 00709000
*       ELSE                                                       0593 00710000
*         DO;                                                      0593 00711000
         B     @RC00591                                            0593 00712000
@RF00591 DS    0H                                                  0594 00713000
*           SNC=SNSNOSES;           /* SENSE=SESSION NOT ACTIVE      */ 00714000
         MVC   SNC(4),@CB01525                                     0594 00715000
*           CALL SNDERRSP;          /* SEND ERROR RESPONSE           */ 00716000
         BAL   @14,SNDERRSP                                        0595 00717000
*         END;                                                     0596 00718000
*     END;                                                         0597 00719000
*   END SSCPLU;                                                    0598 00720000
@EL00013 DS    0H                                                  0598 00721000
@EF00013 DS    0H                                                  0598 00722000
@ER00013 BR    @14                                                 0598 00723000
*                                                                  0599 00724000
*/********************************************************************/ 00725000
*/*                                                                  */ 00726000
*/* THIS ROUTINE CHECKS SESSION STATES FOR SSCP-PU(CLUSTER,NON-NCP)  */ 00727000
*/* FLOWS.                                                           */ 00728000
*/*                                                                  */ 00729000
*/********************************************************************/ 00730000
*                                                                  0599 00731000
*SSCPPUC:                                                          0599 00732000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0599 00733000
SSCPPUC  DS    0H                                                  0600 00734000
*   IF SIBSSTAT=SIBSACTV THEN       /* SESSION STATE ACTIVE          */ 00735000
         L     @09,SIBPTR                                          0600 00736000
         TM    SIBSSTAT(@09),B'00011100'                           0600 00737000
         BNO   @RF00600                                            0600 00738000
*     CALL SSCPROUT;                /* YES-ROUTE TO SSCP             */ 00739000
         BAL   @14,SSCPROUT                                        0601 00740000
*   ELSE                            /* SESSION STATE NOT ACTIVE      */ 00741000
*     DO;                                                          0602 00742000
         B     @RC00600                                            0602 00743000
@RF00600 DS    0H                                                  0603 00744000
*       IF TRHTYPE=ON&              /* RESPONSE AND...               */ 00745000
*           TRHRUCAT=TRHSC&         /* SESSION CONTROL AND...        */ 00746000
*           (CMDBYTE=CD1ACTPU|      /* ACTPU COMMAND OR...           */ 00747000
*           CMDBYTE=CD1DATPU) THEN  /* DACTPU COMMAND                */ 00748000
         LA    @09,10                                              0603 00749000
         ALR   @09,PIUSTART                                        0603 00750000
         TM    TRHTYPE(@09),B'10000000'                            0603 00751000
         BNO   @RF00603                                            0603 00752000
         TM    TRHRUCAT(@09),B'01100000'                           0603 00753000
         BNO   @RF00603                                            0603 00754000
         CLI   CMDBYTE,X'11'                                       0603 00755000
         BE    @RT00603                                            0603 00756000
         CLI   CMDBYTE,X'12'                                       0603 00757000
         BNE   @RF00603                                            0603 00758000
@RT00603 DS    0H                                                  0604 00759000
*         CALL POSTSSRP;            /* POST PIU TO SSCP RESPONSE   0604 00760000
*                                      HANDLER (IEDSCPRP) QCB        */ 00761000
         BAL   @14,POSTSSRP                                        0604 00762000
*       ELSE                                                       0605 00763000
*         DO;                                                      0605 00764000
         B     @RC00603                                            0605 00765000
@RF00603 DS    0H                                                  0606 00766000
*           SNC=SNSNOSES;           /* SENSE=SESSION NOT ACTIVE      */ 00767000
         MVC   SNC(4),@CB01525                                     0606 00768000
*           CALL SNDERRSP;          /* SEND AN ERROR RESPONSE        */ 00769000
         BAL   @14,SNDERRSP                                        0607 00770000
*         END;                                                     0608 00771000
*     END;                                                         0609 00772000
*   END SSCPPUC;                                                   0610 00773000
@EL00014 DS    0H                                                  0610 00774000
@EF00014 DS    0H                                                  0610 00775000
@ER00014 BR    @14                                                 0610 00776000
*                                                                  0611 00777000
*/********************************************************************/ 00778000
*/*                                                                  */ 00779000
*/* THIS ROUTINE CHECK THE SESSION STATES FOR SSCP-PU(NCP) FLOWS.    */ 00780000
*/*                                                                  */ 00781000
*/********************************************************************/ 00782000
*                                                                  0611 00783000
*SSCPPUN:                                                          0611 00784000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0611 00785000
SSCPPUN  DS    0H                                                  0612 00786000
*   IF SIBSSTAT=SIBSACTV THEN       /* SESSION STATE ACTIVE          */ 00787000
         L     @09,SIBPTR                                          0612 00788000
         TM    SIBSSTAT(@09),B'00011100'                           0612 00789000
         BNO   @RF00612                                            0612 00790000
*     DO;                           /* YES                           */ 00791000
*       IF SIBDSTAT=SIBDACTV THEN   /* DATA TRAFFIC STATE ACTIVE     */ 00792000
         TM    SIBDSTAT(@09),B'00000011'                           0614 00793000
         BNO   @RF00614                                            0614 00794000
*         CALL SSCPROUT;            /* ROUTE PIU TO SSCP             */ 00795000
         BAL   @14,SSCPROUT                                        0615 00796000
*       ELSE                        /* DATA TRAFFIC STATE NOT ACTIVE */ 00797000
*         DO;                                                      0616 00798000
         B     @RC00614                                            0616 00799000
@RF00614 DS    0H                                                  0617 00800000
*           IF TRHTYPE=ON&          /* RESPONSE AND...               */ 00801000
*               TRHRUCAT=TRHSC&     /* SESSION CONTROL AND...        */ 00802000
*               CMDBYTE=CD1SDT THEN /* START DATA TRAFFIC COMMAND    */ 00803000
         LA    @09,10                                              0617 00804000
         ALR   @09,PIUSTART                                        0617 00805000
         TM    TRHTYPE(@09),B'10000000'                            0617 00806000
         BNO   @RF00617                                            0617 00807000
         TM    TRHRUCAT(@09),B'01100000'                           0617 00808000
         BNO   @RF00617                                            0617 00809000
         CLI   CMDBYTE,X'A0'                                       0617 00810000
         BNE   @RF00617                                            0617 00811000
*             CALL POSTSSRP;        /* YES-POST PIU TO SSCP RESPONSE    00812000
*                                      HANDLER (IEDSCPRP) QCB        */ 00813000
         BAL   @14,POSTSSRP                                        0618 00814000
*           ELSE                                                   0619 00815000
*             DO;                                                  0619 00816000
         B     @RC00617                                            0619 00817000
@RF00617 DS    0H                                                  0620 00818000
*               SNC=SNSNODT;        /* SENSE=DATA TRAFFIC NOT ACTIVE */ 00819000
         MVC   SNC(4),@CB01527                                     0620 00820000
*               CALL SNDERRSP;      /* SEND AN ERROR RSP             */ 00821000
         BAL   @14,SNDERRSP                                        0621 00822000
*             END;                                                 0622 00823000
*         END;                                                     0623 00824000
*     END;                                                         0624 00825000
*   ELSE                            /* SESSION STATE NOT ACTIVE      */ 00826000
*     DO;                                                          0625 00827000
         B     @RC00612                                            0625 00828000
@RF00612 DS    0H                                                  0626 00829000
*       IF TRHTYPE=ON&              /* RESPONSE AND...               */ 00830000
*           TRHRUCAT=TRHSC&         /* SESSION CONTROL AND...        */ 00831000
*           (CMDBYTE=CD1ACTPU|      /* ACTPU COMMAND OR...           */ 00832000
*           CMDBYTE=CD1DATPU) THEN  /* DACTPU COMMAND                */ 00833000
         LA    @09,10                                              0626 00834000
         ALR   @09,PIUSTART                                        0626 00835000
         TM    TRHTYPE(@09),B'10000000'                            0626 00836000
         BNO   @RF00626                                            0626 00837000
         TM    TRHRUCAT(@09),B'01100000'                           0626 00838000
         BNO   @RF00626                                            0626 00839000
         CLI   CMDBYTE,X'11'                                       0626 00840000
