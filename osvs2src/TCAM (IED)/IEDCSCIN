         BE    @RT00497                                            0497 00370000
         CLI   CMDBYTE,X'32'                                       0497 00371000
         BNE   @RF00497                                            0497 00372000
@RT00497 DS    0H                                                  0498 00373000
*             CALL POSTSCLU;        /* POST PIU TO LU-LU SESSION   0498 00374000
*                                      CONTROL (IEDSCLU) QCB         */ 00375000
         BAL   @14,POSTSCLU                                        0498 00376000
*           ELSE                                                   0499 00377000
*             DO;                                                  0499 00378000
         B     @RC00497                                            0499 00379000
@RF00497 DS    0H                                                  0500 00380000
*               SNC=SNSNOSES;       /* SENSE=SESSION NOT ACTIVE      */ 00381000
         MVC   SNC(4),@CB01525                                     0500 00382000
*               CALL SNDERRSP;      /* SEND ERROR RESPONSE           */ 00383000
         BAL   @14,SNDERRSP                                        0501 00384000
*             END;                                                 0502 00385000
*         END;                                                     0503 00386000
*     END;                                                         0504 00387000
*   ELSE                            /* NO SIB FOR THIS LU            */ 00388000
*     DO;                                                          0505 00389000
         B     @RC00492                                            0505 00390000
@RF00492 DS    0H                                                  0506 00391000
*       SNC=SNSNOSES;               /* SENSE=SESSION NOT ACTIVE      */ 00392000
         MVC   SNC(4),@CB01525                                     0506 00393000
