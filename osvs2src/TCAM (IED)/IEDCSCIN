*/*                                                                  */ 00176000
*/* THIS ROUTINE DISCARDS ALL THE UNITS IN A PIU BUFFER.  IT POSTS   */ 00177000
*/* THEM TO BUF  RETURN(IEDQGB).                                     */ 00178000
*/*       INPUT= REG1 POINTS TO PIU BUFFER TO BE DISCARDED           */ 00179000
*/*                                                                  */ 00180000
*/********************************************************************/ 00181000
*                                                                  0447 00182000
*DISCARD:                                                          0447 00183000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0447 00184000
DISCARD  DS    0H                                                  0448 00185000
*   R14SV01=R14;                    /* SAVE RETURN REG               */ 00186000
         ST    R14,R14SV01                                         0448 00187000
*/*                                                                  */ 00188000
*                                                                  0449 00189000
*   RPARM->PRFNBUNT=0;              /* SET NUMBER OF UNITS = 0 (THIS    00190000
*                                      MEANS IEDQGB WILL CALCULATE 0449 00191000
*                                      THE NUMBER OF UNITS TO BE   0449 00192000
*                                      FREED)                        */ 00193000
         MVI   PRFNBUNT(RPARM),X'00'                               0449 00194000
*   RPARM->PRFQCBA=ADDR(AVTBFRTB);  /* ADDR OF BUFFER RETURN QCB     */ 00195000
         LA    @09,AVTBFRTB(,IEDQAPTR)                             0450 00196000
         STCM  @09,7,PRFQCBA(RPARM)                                0450 00197000
*   RPARM->PRFPRI=PRIBFRTB;         /* POST PRIORITY                 */ 00198000
         MVI   PRFPRI(RPARM),X'E0'                                 0451 00199000
*   CALL DSPPOSTR;                  /* FREE UNITS                    */ 00200000
         LA    @15,DSPPOSTR(,RDISP)                                0452 00201000
         BALR  @14,@15                                             0452 00202000
*/*                                                                  */ 00203000
*                                                                  0453 00204000
*   R14=R14SV01;                    /* RESTORE RETURN REG            */ 00205000
         L     R14,R14SV01                                         0453 00206000
*   RETURN;                         /* RETURN TO CALLER              */ 00207000
@EL00004 DS    0H                                                  0454 00208000
@EF00004 DS    0H                                                  0454 00209000
@ER00004 BR    @14                                                 0454 00210000
*   END DISCARD;                                                   0455 00211000
*                                                                  0456 00212000
*/********************************************************************/ 00213000
*/*                                                                  */ 00214000
*/* THIS ROUTINE CHECKS DATA TRAFFIC STATES                          */ 00215000
*/*                                                                  */ 00216000
*/********************************************************************/ 00217000
*                                                                  0456 00218000
*DTSTATCK:                                                         0456 00219000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0456 00220000
DTSTATCK DS    0H                                                  0457 00221000
*   IF SIBDSTAT=SIBDACTV THEN       /* DATA TRAFFIC STATE ACTIVE     */ 00222000
         L     @09,SIBPTR                                          0457 00223000
         TM    SIBDSTAT(@09),B'00000011'                           0457 00224000
         BNO   @RF00457                                            0457 00225000
*     DO;                           /* YES                           */ 00226000
*       IF TRHRUCAT=TRHFMD|         /* FM DATA OR...                 */ 00227000
*           TRHRUCAT=TRHDFC THEN    /* DATA FLOW CONTROL             */ 00228000
         LA    @09,10                                              0459 00229000
         ALR   @09,PIUSTART                                        0459 00230000
         TM    TRHRUCAT(@09),B'01100000'                           0459 00231000
         BZ    @RT00459                                            0459 00232000
         TM    TRHRUCAT(@09),B'01000000'                           0459 00233000
         BNO   @RF00459                                            0459 00234000
         TM    TRHRUCAT(@09),B'00100000'                           0459 00235000
         BNZ   @RF00459                                            0459 00236000
@RT00459 DS    0H                                                  0460 00237000
*         CALL BYPCMIN;             /* BYPASS TO IEDCMIN(CONNECTION     00238000
*                                      POINT MANAGER INBOUND)        */ 00239000
         BAL   @14,BYPCMIN                                         0460 00240000
*       ELSE                        /* NOT FM DATA OR DATA FLOW    0461 00241000
*                                      CONTROL                       */ 00242000
*         CALL POSTSCLU;            /* POST THE PIU TO LU-LU SESSION    00243000
*                                      CONTROL(IEDSCLU)              */ 00244000
         B     @RC00459                                            0461 00245000
@RF00459 BAL   @14,POSTSCLU                                        0461 00246000
*     END;                                                         0462 00247000
*   ELSE                            /* DATA TRAFFIC STATE NOT ACTIVE */ 00248000
*     DO;                                                          0463 00249000
         B     @RC00457                                            0463 00250000
@RF00457 DS    0H                                                  0464 00251000
*       IF SIBDSTAT=SIBDRSET THEN   /* DATA TRAFFIC STATE RESET      */ 00252000
         L     @09,SIBPTR                                          0464 00253000
         TM    SIBDSTAT(@09),B'00000011'                           0464 00254000
         BNZ   @RF00464                                            0464 00255000
*         DO;                       /* YES                           */ 00256000
*           SNC=SNSNODT;            /* SENSE=DATA TRAFFIC NOT ACTIVE */ 00257000
         MVC   SNC(4),@CB01527                                     0466 00258000
*           CALL SNDERRSP;          /* SEND AN ERROR RESPONSE        */ 00259000
         BAL   @14,SNDERRSP                                        0467 00260000
*         END;                                                     0468 00261000
*       ELSE                        /* DATA TRAFFIC STATE NOT RESET  */ 00262000
*         DO;                                                      0469 00263000
         B     @RC00464                                            0469 00264000
@RF00464 DS    0H                                                  0470 00265000
*           IF SIBDSTAT=SIBDAPND THEN/* DATA TRAFFIC ACTIVE PENDING  */ 00266000
         L     @09,SIBPTR                                          0470 00267000
         TM    SIBDSTAT(@09),B'00000001'                           0470 00268000
         BNO   @RF00470                                            0470 00269000
         TM    SIBDSTAT(@09),B'00000010'                           0470 00270000
         BNZ   @RF00470                                            0470 00271000
*             DO;                   /* YES                           */ 00272000
*               IF TRHTYPE=ON&      /* RESPONSE AND...               */ 00273000
*                   TRHRUCAT=TRHSC& /* SESSION CONTROL...            */ 00274000
*                   CMDBYTE=CD1SDT THEN/* START DATA TRAFFIC COMMAND */ 00275000
         LA    @09,10                                              0472 00276000
         ALR   @09,PIUSTART                                        0472 00277000
         TM    TRHTYPE(@09),B'10000000'                            0472 00278000
         BNO   @RF00472                                            0472 00279000
         TM    TRHRUCAT(@09),B'01100000'                           0472 00280000
         BNO   @RF00472                                            0472 00281000
         CLI   CMDBYTE,X'A0'                                       0472 00282000
         BNE   @RF00472                                            0472 00283000
*                 CALL POSTSCLU;    /* POST PIU TO LU-LU SESSION   0473 00284000
*                                      CONTROL (IEDSCLU) QCB         */ 00285000
         BAL   @14,POSTSCLU                                        0473 00286000
*               ELSE                                               0474 00287000
*                 DO;                                              0474 00288000
         B     @RC00472                                            0474 00289000
@RF00472 DS    0H                                                  0475 00290000
*                   SNC=SNSNODT;    /* SENSE=DATA TRAFFIC NOT ACTIVE */ 00291000
         MVC   SNC(4),@CB01527                                     0475 00292000
*                   CALL SNDERRSP;  /* SEND AN ERROR RESPONSE        */ 00293000
         BAL   @14,SNDERRSP                                        0476 00294000
*                 END;                                             0477 00295000
*             END;                                                 0478 00296000
*           ELSE                    /* DATA TRAFFIC STATE IS RESET 0479 00297000
*                                      PENDING                       */ 00298000
*             DO;                                                  0479 00299000
         B     @RC00470                                            0479 00300000
@RF00470 DS    0H                                                  0480 00301000
*               IF TRHTYPE=ON&      /* RESPONSE AND...               */ 00302000
*                   TRHRUCAT=TRHSC& /* SESSION CONTROL AND...        */ 00303000
*                   CMDBYTE=CD1CLEAR THEN/* CLEAR COMMAND            */ 00304000
         LA    @09,10                                              0480 00305000
         ALR   @09,PIUSTART                                        0480 00306000
         TM    TRHTYPE(@09),B'10000000'                            0480 00307000
         BNO   @RF00480                                            0480 00308000
         TM    TRHRUCAT(@09),B'01100000'                           0480 00309000
         BNO   @RF00480                                            0480 00310000
         CLI   CMDBYTE,X'A1'                                       0480 00311000
         BNE   @RF00480                                            0480 00312000
*                 CALL POSTSCLU;    /* POST PIU TO LU-LU SESSION   0481 00313000
*                                      CONTROL (IEDSCLU) QCB         */ 00314000
         BAL   @14,POSTSCLU                                        0481 00315000
*               ELSE                                               0482 00316000
*                 DO;                                              0482 00317000
         B     @RC00480                                            0482 00318000
@RF00480 DS    0H                                                  0483 00319000
*                   RPARM=PIUBFRP;  /* PIU TO BE FREED               */ 00320000
         LR    RPARM,PIUBFRP                                       0483 00321000
*                   CALL DISCARD;   /* DISCARD THE PIU               */ 00322000
         BAL   @14,DISCARD                                         0484 00323000
*                   CALL DSPDISP;   /* EXIT TO DISPATCHER            */ 00324000
         LA    @15,DSPDISP(,RDISP)                                 0485 00325000
         BALR  @14,@15                                             0485 00326000
*                 END;                                             0486 00327000
*             END;                                                 0487 00328000
*         END;                                                     0488 00329000
*     END;                                                         0489 00330000
