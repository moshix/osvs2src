         L     @09,SIBPTR                                          0448 00093000
         TM    SIBBRACK(@09),B'00000100'                           0448 00094000
         BNO   @RF00448                                            0448 00095000
*                 CALL BRACKMGR;    /* DO BRACKET MANAGEMENT         */ 00096000
         BAL   @14,BRACKMGR                                        0449 00097000
*               ELSE                                               0450 00098000
*                 DO;                                              0450 00099000
         B     @RC00448                                            0450 00100000
@RF00448 DS    0H                                                  0451 00101000
*                   RQCB=ADDR(IEDCMOUT);/* ADDR OF IEDCMOUT'S QCB    */ 00102000
         L     RQCB,@CV02133                                       0451 00103000
*                   RSTCB=QCBSTCHN; /* ADDR OF IEDCMOUT'S STCB       */ 00104000
         L     RSTCB,QCBSTCHN-1(,IEDQQPTR)                         0452 00105000
         LA    RSTCB,0(,RSTCB)                                     0452 00106000
*                   RPARM=RPRF;     /* SET PIU BUFFER AS ELEMENT     */ 00107000
