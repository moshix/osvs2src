*       IF SIBFTERM=ON THEN         /* FORCED TERMINATE?     @YM08991*/ 00050000
         TM    SIBFTERM(@05),B'00100000'                           0439 00051000
         BNO   @RF00439                                            0439 00052000
*         CALL CHKTERM;             /* YES, SEE IF PIU SHOULD BE   0440 00053000
*                                      ALLOWED TO GO OUT     @YM08991*/ 00054000
         BAL   @14,CHKTERM                                         0440 00055000
*       IF TTHEFI=OFF&              /* NORMAL FLOW AND...            */ 00056000
*           (TRHRUCAT=TRHFMD|       /* FM DATA OR ...                */ 00057000
*           TRHRUCAT=TRHDFC) THEN   /* DATA FLOW CONTROL             */ 00058000
@RF00439 TM    TTHEFI(PIUPTR),B'00000001'                          0441 00059000
         BNZ   @RF00441                                            0441 00060000
         LA    @09,10                                              0441 00061000
         ALR   @09,PIUPTR                                          0441 00062000
         TM    TRHRUCAT(@09),B'01100000'                           0441 00063000
         BZ    @RT00441                                            0441 00064000
