*   END CHAINMGR;                                                  0677 00843000
*                                                                  0678 00844000
*/***********************************************************@YM08991*/ 00845000
*/*                                                          @YM08991*/ 00846000
*/* THIS ROUTINE CHECKS PIUS BEING SENT OUT ON THE LU-LU     @YM08991*/ 00847000
*/* FLOW WHILE A FORCED TERMINATE OF THE SESSION IS IN       @YM08991*/ 00848000
*/* PROGRESS.  ALL RESPONSES PLUS 'CLEAR' AND 'UNBIND'       @YM08991*/ 00849000
*/* REQUESTS ARE ALLOWED TO FLOW NORMALLY.  OTHER TYPES OF   @YM08991*/ 00850000
*/* REQUESTS ARE FLAGGED SO THEY WILL BE STOPPED LATER BY    @YM08991*/ 00851000
*/* IEDPCOUT AND PASSED TO CLEAN-UP.                         @YM08991*/ 00852000
*/*                                                          @YM08991*/ 00853000
*/***********************************************************@YM08991*/ 00854000
*                                                                  0678 00855000
*CHKTERM:                                                          0678 00856000
*   PROC OPTIONS(SAVE(R14));        /*                       @YM08991*/ 00857000
CHKTERM  ST    @14,12(,@13)                                        0678 00858000
*   IF TRHTYPE=OFF THEN             /* REQUEST PIU           @YM08991*/ 00859000
         LA    @09,10                                              0679 00860000
         ALR   @09,PIUPTR                                          0679 00861000
         TM    TRHTYPE(@09),B'10000000'                            0679 00862000
         BNZ   @RF00679                                            0679 00863000
*     DO;                           /* YES                   @YM08991*/ 00864000
*       IF TRHRUCAT=TRHSC&          /* SESSION CONTROL &     @YM08991*/ 00865000
*           (CMDBYTE=CD1CLEAR|      /* ... CLEAR OR          @YM08991*/ 00866000
*           CMDBYTE=CD1UBIND) THEN                                 0681 00867000
         TM    TRHRUCAT(@09),B'01100000'                           0681 00868000
         BNO   @RF00681                                            0681 00869000
         LA    @09,13                                              0681 00870000
         ALR   @09,PIUPTR                                          0681 00871000
         CLI   CMDBYTE(@09),X'A1'                                  0681 00872000
         BE    @RT00681                                            0681 00873000
         CLI   CMDBYTE(@09),X'32'                                  0681 00874000
         BE    @RT00681                                            0681 00875000
*         ;                         /* ... UNBIND?           @YM08991*/ 00876000
*                                                                  0682 00877000
*       /*************************************************************/ 00878000
*       /*                                                           */ 00879000
*       /* YES, LET PIU GO OUT                               @YM08991*/ 00880000
*       /*                                                           */ 00881000
*       /*************************************************************/ 00882000
*                                                                  0683 00883000
*       ELSE                        /* OTHER REQUEST TYPE    @YM08991*/ 00884000
*         PRF2TERM=ON;              /* FLAG FOR IEDPCOUT     @YM08991*/ 00885000
@RF00681 LR    @09,IEDQPPTR                                        0683 00886000
         AL    @09,@CF02224                                        0683 00887000
         OI    PRF2TERM(@09),B'00010000'                           0683 00888000
*     END;                          /*                       @YM08991*/ 00889000
*   END CHKTERM;                    /* RETURN TO CALLER      @YM08991*/ 00890000
@EL00006 DS    0H                                                  0685 00891000
@EF00006 DS    0H                                                  0685 00892000
@ER00006 L     @14,12(,@13)                                        0685 00893000
         BR    @14                                                 0685 00894000
*                                                                  0686 00895000
*/********************************************************************/ 00896000
*/*                                                                  */ 00897000
*/* THIS ROUTINE DISCARDS ALL THE UNITS IN A PIU BUFFER.  IT POSTS   */ 00898000
*/* THEM TO BUF  RETURN(IEDQGB).                                     */ 00899000
*/*       INPUT= REG1 POINTS TO PIU BUFFER TO BE DISCARDED           */ 00900000
*/*                                                                  */ 00901000
*/********************************************************************/ 00902000
*                                                                  0686 00903000
*DISCARD:                                                          0686 00904000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0686 00905000
DISCARD  DS    0H                                                  0687 00906000
*   R14SV05=R14;                    /* SAVE RETURN REG               */ 00907000
         ST    R14,R14SV05                                         0687 00908000
*/*                                                                  */ 00909000
*                                                                  0688 00910000
*   RPARM->PRFNBUNT=0;              /* SET NUMBER OF UNITS = 0 (THIS    00911000
*                                      MEANS IEDQGB WILL CALCULATE 0688 00912000
*                                      THE NUMBER OF UNITS TO BE   0688 00913000
*                                      FREED)                        */ 00914000
         MVI   PRFNBUNT(RPARM),X'00'                               0688 00915000
*   RPARM->PRFQCBA=ADDR(AVTBFRTB);  /* ADDR OF BUFFER RETURN QCB     */ 00916000
         LA    @09,AVTBFRTB(,IEDQAPTR)                             0689 00917000
         STCM  @09,7,PRFQCBA(RPARM)                                0689 00918000
*   RPARM->PRFPRI=PRIBFRTB;         /* POST PRIORITY                 */ 00919000
         MVI   PRFPRI(RPARM),X'E0'                                 0690 00920000
*   CALL DSPPOSTR;                  /* POST BUFFER TO BUFFER RETURN  */ 00921000
         LA    @15,DSPPOSTR(,RDISP)                                0691 00922000
         BALR  @14,@15                                             0691 00923000
*/*                                                                  */ 00924000
*                                                                  0692 00925000
*   R14=R14SV05;                    /* RESTORE RETURN REG            */ 00926000
         L     R14,R14SV05                                         0692 00927000
*   RETURN;                         /* RETURN TO CALLER              */ 00928000
@EL00007 DS    0H                                                  0693 00929000
@EF00007 DS    0H                                                  0693 00930000
@ER00007 BR    @14                                                 0693 00931000
*   END DISCARD;                                                   0694 00932000
*                                                                  0695 00933000
*   /*****************************************************************/ 00934000
*   /*                                                               */ 00935000
*   /* THIS ROUTINE CHECKS THE SIB CHAIN FOR THE SSCP SIB. A SWITCH  */ 00936000
*   /* (FOUNDSW) IS SET WHEN A MATCH IS FOUND.                       */ 00937000
*   /*                                                               */ 00938000
*   /*****************************************************************/ 00939000
*                                                                  0695 00940000
*FINDSIB:                                                          0695 00941000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0695 00942000
FINDSIB  DS    0H                                                  0696 00943000
*   FOUNDSW=OFF;                    /* INITIALIZE LOOP SWITCH        */ 00944000
         NI    FOUNDSW,B'01111111'                                 0696 00945000
*/*                                                                  */ 00946000
*                                                                  0697 00947000
*   DO WHILE SIBPTR>0&FOUNDSW=OFF;  /* CONTINUE UNTIL END OF SIB   0697 00948000
*                                      CHAIN OR MATCH FOUND          */ 00949000
         B     @DE00697                                            0697 00950000
@DL00697 DS    0H                                                  0698 00951000
*     IF SIBINDEX=SAVTSCPT THEN     /* SIB FOR SSCP FLOW -YES        */ 00952000
         L     @09,AVTSAVTP(,IEDQAPTR)                             0698 00953000
         L     @05,SIBPTR                                          0698 00954000
         CLC   SIBINDEX(2,@05),SAVTSCPT(@09)                       0698 00955000
         BNE   @RF00698                                            0698 00956000
*       FOUNDSW=ON;                 /* INDICATE MATCH FOUND          */ 00957000
         OI    FOUNDSW,B'10000000'                                 0699 00958000
*     ELSE                                                         0700 00959000
*       SIBPTR=SIBCHAIN;            /* GET NEXT SIB IN CHAIN         */ 00960000
         B     @RC00698                                            0700 00961000
@RF00698 L     @09,SIBPTR                                          0700 00962000
         L     @09,SIBCHAIN(,@09)                                  0700 00963000
         ST    @09,SIBPTR                                          0700 00964000
*/*                                                                  */ 00965000
*                                                                  0701 00966000
*   END;                                                           0701 00967000
*                                                                  0701 00968000
@RC00698 DS    0H                                                  0701 00969000
@DE00697 L     @09,SIBPTR                                          0701 00970000
         LTR   @09,@09                                             0701 00971000
         BNP   @DC00697                                            0701 00972000
         TM    FOUNDSW,B'10000000'                                 0701 00973000
         BZ    @DL00697                                            0701 00974000
@DC00697 DS    0H                                                  0702 00975000
*/*                                                                  */ 00976000
*                                                                  0702 00977000
*   END FINDSIB;                    /* RETURN TO CALLER              */ 00978000
@EL00008 DS    0H                                                  0702 00979000
@EF00008 DS    0H                                                  0702 00980000
@ER00008 BR    @14                                                 0702 00981000
*                                                                  0703 00982000
*/********************************************************************/ 00983000
*/*                                                                  */ 00984000
*/* THIS ROUTINE PROCESSES PIUS ON THE SSCP FLOW                     */ 00985000
*/*                                                                  */ 00986000
*/********************************************************************/ 00987000
*                                                                  0703 00988000
*SSCPRTN:                                                          0703 00989000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0703 00990000
SSCPRTN  DS    0H                                                  0704 00991000
*   R14SV06=R14;                    /* SAVE RETURN ADDRESS           */ 00992000
         ST    R14,R14SV06                                         0704 00993000
*/*                                                                  */ 00994000
*                                                                  0705 00995000
*                                   /* INITIALIZATION                */ 00996000
*   RPARM=TTHDAF;                   /* TNT OFFSET OF DAF             */ 00997000
         SLR   RPARM,RPARM                                         0705 00998000
         ICM   RPARM,3,TTHDAF(PIUPTR)                              0705 00999000
*   CALL IEDQTNT;                   /* GET TERMINAL ADDRESS          */ 01000000
         L     @15,AVTRNMPT(,IEDQAPTR)                             0706 01001000
         BALR  @14,@15                                             0706 01002000
*   RTRM=RPARM;                     /* TERMINAL ENTRY ADDRESS        */ 01003000
         LR    RTRM,RPARM                                          0707 01004000
*/*                                                                  */ 01005000
*                                                                  0708 01006000
*   SIBPTR=TRMSIBPT;                /* GET FIRST SIB ADDRESS         */ 01007000
         LR    @09,IEDQTPTR                                        0708 01008000
         AL    @09,@CF02212                                        0708 01009000
         SLR   @05,@05                                             0708 01010000
         ICM   @05,7,TRMSIBPT-7(@09)                               0708 01011000
         ST    @05,SIBPTR                                          0708 01012000
*   CALL FINDSIB;                   /* LOOK FOR APPROPRIATE SIB      */ 01013000
         BAL   @14,FINDSIB                                         0709 01014000
*/*                                                                  */ 01015000
*                                                                  0710 01016000
*   IF FOUNDSW=ON THEN              /* CORRECT SIB FOUND- YES        */ 01017000
         TM    FOUNDSW,B'10000000'                                 0710 01018000
         BNO   @RF00710                                            0710 01019000
*     DO;                                                          0711 01020000
*                                                                  0711 01021000
*/*                                                                  */ 01022000
*                                                                  0712 01023000
*       IF TRHRUCAT=TRHSC THEN                                     0712 01024000
         TM    TRHRUCAT+10(PIUPTR),B'01100000'                     0712 01025000
         BNO   @RF00712                                            0712 01026000
*         DO;                       /* IF THIS IS SESSION CONTROL  0713 01027000
*                                                            @XM05714*/ 01028000
*           IF CMDBYTE=CD1ACTPU|CMDBYTE=CD1ACTLU THEN/* ACTPU OR   0714 01029000
*                                      ACTLU ?                       */ 01030000
         LA    @09,13                                              0714 01031000
         ALR   @09,PIUPTR                                          0714 01032000
         CLI   CMDBYTE(@09),X'11'                                  0714 01033000
         BE    @RT00714                                            0714 01034000
         CLI   CMDBYTE(@09),X'0D'                                  0714 01035000
         BNE   @RF00714                                            0714 01036000
@RT00714 DS    0H                                                  0715 01037000
*             SIBSSTAT=SIBSAPND;    /* SET SESSION ACT PENDING       */ 01038000
         L     @10,SIBPTR                                          0715 01039000
         NI    SIBSSTAT(@10),B'11100111'                           0715 01040000
         OI    SIBSSTAT(@10),B'00000100'                           0715 01041000
*           ELSE                                                   0716 01042000
*             DO;                                                  0716 01043000
         B     @RC00714                                            0716 01044000
@RF00714 DS    0H                                                  0717 01045000
*               IF CMDBYTE=CD1DATPU|CMDBYTE=CD1DATLU THEN          0717 01046000
         LA    @09,13                                              0717 01047000
         ALR   @09,PIUPTR                                          0717 01048000
         CLI   CMDBYTE(@09),X'12'                                  0717 01049000
         BE    @RT00717                                            0717 01050000
         CLI   CMDBYTE(@09),X'0E'                                  0717 01051000
         BNE   @RF00717                                            0717 01052000
@RT00717 DS    0H                                                  0718 01053000
*                 DO;               /* DACTPU OR DACTLU ?            */ 01054000
*                   SIBSSTAT=SIBSRPND;/* SET SESSION RSET PENDING    */ 01055000
         L     @10,SIBPTR                                          0719 01056000
         NI    SIBSSTAT(@10),B'11101011'                           0719 01057000
         OI    SIBSSTAT(@10),B'00001000'                           0719 01058000
*                   SIBDSTAT=SIBDRSET;/* SET DATA TRAFFIC RESET      */ 01059000
         NI    SIBDSTAT(@10),B'11111100'                           0720 01060000
*                 END;                                             0721 01061000
*               ELSE                                               0722 01062000
*                 IF CMDBYTE=CD1SDT THEN/* START DATA TRAFFIC        */ 01063000
         B     @RC00717                                            0722 01064000
@RF00717 CLI   CMDBYTE+13(PIUPTR),X'A0'                            0722 01065000
         BNE   @RF00722                                            0722 01066000
*                   SIBDSTAT=SIBDAPND;/* SET DATA TRAFFIC ACTIVE     */ 01067000
         L     @10,SIBPTR                                          0723 01068000
         NI    SIBDSTAT(@10),B'11111101'                           0723 01069000
         OI    SIBDSTAT(@10),B'00000001'                           0723 01070000
*             END;                  /* PENDING                       */ 01071000
@RF00722 DS    0H                                                  0724 01072000
@RC00717 DS    0H                                                  0725 01073000
*         END;                      /*                       @XM05714*/ 01074000
@RC00714 DS    0H                                                  0726 01075000
*/*                                                                  */ 01076000
*                                                                  0726 01077000
*                                   /* BYPASS TO IEDCMOUT            */ 01078000
*       RPARM=RPRF;                 /* ADDRESS OF PIU                */ 01079000
@RF00712 LR    RPARM,RPRF                                          0726 01080000
*       RQCB=ADDR(IEDCMOUT);        /* QCB ADDRESS                   */ 01081000
         L     RQCB,@CV02133                                       0727 01082000
*       RSTCB=QCBSTCHN;             /* STCB ADDRESS                  */ 01083000
         L     RSTCB,QCBSTCHN-1(,IEDQQPTR)                         0728 01084000
         LA    RSTCB,0(,RSTCB)                                     0728 01085000
*       R15=ADDR(DSPBYPAS);         /* BYPASS TO IEDCMOUT            */ 01086000
         LA    R15,DSPBYPAS(,RDISP)                                0729 01087000
*     END;                                                         0730 01088000
*                                                                  0730 01089000
*/*                                                                  */ 01090000
*                                                                  0731 01091000
*   ELSE                            /* CORRECT SIB NOT FOUND         */ 01092000
*     DO;                                                          0731 01093000
         B     @RC00710                                            0731 01094000
@RF00710 DS    0H                                                  0732 01095000
*       RPARM=RPRF;                 /* ADDRESS OF PIU BUFFER         */ 01096000
         LR    RPARM,RPRF                                          0732 01097000
*       CALL DISCARD;               /* DISCARD THIS PIU BUFFER       */ 01098000
         BAL   @14,DISCARD                                         0733 01099000
