*                                                                  0430 00018000
*   RPRF=RPARM;                     /* PIU BUFFER ADDRESS            */ 00019000
         LR    RPRF,RPARM                                          0430 00020000
*   PIUPTR=ADDR(PRFPIUO)+PRFPIUO;   /* PIU(TH-RH-RU) ADDRESSABILITY  */ 00021000
         LA    PIUPTR,PRFPIUO(,IEDQPPTR)                           0431 00022000
         SLR   @09,@09                                             0431 00023000
         IC    @09,PRFPIUO(,IEDQPPTR)                              0431 00024000
