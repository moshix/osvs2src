*/*                                                                  */ 00079000
*                                                                  0445 00080000
*           CALL CHAINMGR;          /* DO CHAIN MANAGEMENT           */ 00081000
         BAL   @14,CHAINMGR                                        0445 00082000
*           IF RC=GO THEN           /* EVERYTHING O.K.               */ 00083000
         CLI   RC,0                                                0446 00084000
         BNE   @RF00446                                            0446 00085000
*             DO;                   /* YES                           */ 00086000
*               IF SAVTBSM^=0&      /* BRACKET MANAGER LOADED AND    */ 00087000
*                   SIBBRACK=ON THEN/* RUNNING BRACKETS THIS SESSION */ 00088000
         L     @09,AVTSAVTP(,IEDQAPTR)                             0448 00089000
         L     @09,SAVTBSM(,@09)                                   0448 00090000
