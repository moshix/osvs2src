*/* THIS ROUTINE BUILDS A CANCEL COMMAND IN THE PIU BUFFER.          */ 00147000
*/*                                                                  */ 00148000
*/********************************************************************/ 00149000
*                                                                  0467 00150000
*BLDCANCL:                                                         0467 00151000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0467 00152000
@EL00001 DS    0H                                                  0467 00153000
@EF00001 DS    0H                                                  0467 00154000
@ER00001 BR    @14                                                 0467 00155000
BLDCANCL DS    0H                                                  0468 00156000
*   R14SV01=R14;                    /* SAVE RETURN ADDRESS           */ 00157000
         ST    R14,R14SV01                                         0468 00158000
*/*                                                                  */ 00159000
*                                                                  0469 00160000
*   TTHDCF=LENGTH(IEDRH)+1;         /* LENGTH OF CANCEL RH/RU        */ 00161000
         MVC   TTHDCF(2,PIUPTR),@CH00330                           0469 00162000
*   IEDRH=CANCELRH;                 /* SET UP CANCEL RH              */ 00163000
         MVC   IEDRH+10(3,PIUPTR),@CB02152                         0470 00164000
*   RU(1)=CD1CANCL;                 /* CANCEL COMMAND CODE           */ 00165000
         MVI   RU+13(PIUPTR),X'83'                                 0471 00166000
*   IF PRFTIC^=INVALTIC THEN        /* NOT LAST UNIT IN BUFFER       */ 00167000
         CLC   PRFTIC(4,IEDQPPTR),@CB01337                         0472 00168000
         BE    @RF00472                                            0472 00169000
*     DO;                           /* YES                           */ 00170000
*       RPARM=PRFTIC;               /* ADDRESS OF 2ND UNIT IN BUFFER */ 00171000
         L     RPARM,PRFTIC(,IEDQPPTR)                             0474 00172000
*       CALL DISCARD;               /* DISCARD 2ND THRU LAST UNITS   */ 00173000
         BAL   @14,DISCARD                                         0475 00174000
*       PRFTIC=INVALTIC;            /* SET INVALID TIC IN 1ST UNIT   */ 00175000
         MVC   PRFTIC(4,IEDQPPTR),@CB01337                         0476 00176000
*     END;                                                         0477 00177000
*                                                                  0477 00178000
*/*                                                                  */ 00179000
*                                                                  0478 00180000
*   R14=R14SV01;                    /* RESTORE RETURN ADDRESS        */ 00181000
@RF00472 L     R14,R14SV01                                         0478 00182000
*   RETURN;                         /* RETURN TO CALLER              */ 00183000
@EL00002 DS    0H                                                  0479 00184000
@EF00002 DS    0H                                                  0479 00185000
@ER00002 BR    @14                                                 0479 00186000
*   END BLDCANCL;                                                  0480 00187000
*                                                                  0481 00188000
*/********************************************************************/ 00189000
*/*                                                                  */ 00190000
*/* THIS ROUTINE BUILDS AN ERROR BUFFER IN THE PIU BUFFER WHOSE      */ 00191000
*/* ADDRESS IS PASSED IN REGISTER 1.  AFTER BUILDING THE ERROR       */ 00192000
*/* BUFFER IT IS POSTED TO THE MH.                                   */ 00193000
*/*                                                                  */ 00194000
*/********************************************************************/ 00195000
*                                                                  0481 00196000
*   RFY                                                            0481 00197000
*     IEDQPRF BASED(BUFPTR);                                       0481 00198000
*   RFY                                                            0482 00199000
*     IEDPIU BASED(PIUBEG);                                        0482 00200000
*BLDERBUF:                                                         0483 00201000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0483 00202000
BLDERBUF DS    0H                                                  0484 00203000
*   R14SV02=R14;                    /* SAVE RETURN ADDRESS           */ 00204000
         ST    R14,R14SV02                                         0484 00205000
*/*                                                                  */ 00206000
*                                                                  0485 00207000
*   BUFPTR=RPARM;                   /* ADDRESS OF PIU BUFFER IN WHICH   00208000
*                                      ERROR BUFFER WILL BE BUILT    */ 00209000
         ST    RPARM,BUFPTR                                        0485 00210000
*   PIUBEG=ADDR(PRFPIUO)+PRFPIUO;   /* PIU(TH-RH-RU) ADDRESSABILITY  */ 00211000
         L     @09,BUFPTR                                          0486 00212000
         LA    @05,PRFPIUO(,@09)                                   0486 00213000
         SLR   @02,@02                                             0486 00214000
         IC    @02,PRFPIUO(,@09)                                   0486 00215000
         ALR   @05,@02                                             0486 00216000
         ST    @05,PIUBEG                                          0486 00217000
*/* SET UP THE MH QCB ADDRESS IN THE BUFFER                          */ 00218000
*                                                                  0487 00219000
*   PRFQCBA=LCBMHA;                 /* GET PROPER MH ADDRESS @YM06085*/ 00220000
         MVC   PRFQCBA(3,@09),LCBMHA(IEDQLPTR)                     0487 00221000
*/*                                                                  */ 00222000
*                                                                  0488 00223000
*   TRHBC=ON;                       /* FORCE BEGIN CHAIN             */ 00224000
*   TRHEC=ON;                       /* FORCE END OF CHAIN            */ 00225000
         OI    TRHBC+10(@05),B'00000011'                           0489 00226000
*/*                                                                  */ 00227000
*                                                                  0490 00228000
*   HWSAVE=TTHDCF;                  /* SAVE DATA COUNT               */ 00229000
         MVC   HWSAVE(2),TTHDCF(@05)                               0490 00230000
*   RPARM=BUFPTR;                   /* ADDRESS OF RESPONSE PIU BUFFER*/ 00231000
         LR    RPARM,@09                                           0491 00232000
*   CALL IEDXFMI;                   /* TRANSFORM INTO 'TCAM' BUFFER     00233000
*                                      FORMAT                        */ 00234000
         L     @09,AVTSAVTP(,IEDQAPTR)                             0492 00235000
         L     @15,SAVTXFMI(,@09)                                  0492 00236000
         BALR  @14,@15                                             0492 00237000
*   PRF1RULN=HWSAVE-LENGTH(IEDRH);  /* SET ACTUAL RU(WHICH CONTAINS     00238000
*                                      THE RESPONSE INFORMATION)   0493 00239000
*                                      LENGTH                        */ 00240000
         L     @09,BUFPTR                                          0493 00241000
         LR    @05,@09                                             0493 00242000
         SL    @05,@CF00044                                        0493 00243000
         LH    @02,HWSAVE                                          0493 00244000
         N     @02,@CF02214                                        0493 00245000
         SL    @02,@CF00065                                        0493 00246000
         STH   @02,PRF1RULN(,@05)                                  0493 00247000
*   PRF1FORM=PRF1SQLI;              /* INDICATE BOTH SEQ # AND RU  0494 00248000
*                                      LENGTH ARE PRESENT            */ 00249000
         OI    PRF1FORM(@05),B'00110000'                           0494 00250000
*/*                                                                  */ 00251000
*                                                                  0495 00252000
*   PRFPRI=PRIERBFR;                /* SET ERROR PRIORITY            */ 00253000
         MVI   PRFPRI(@09),X'E3'                                   0495 00254000
*   PRFLCB=RLCB;                    /* SET LCB ADDRESS               */ 00255000
         STCM  RLCB,7,PRFLCB(@09)                                  0496 00256000
*   PRFSIZE=0;                      /* SET 0 LENGTH                  */ 00257000
         SLR   @05,@05                                             0497 00258000
         STH   @05,PRFSIZE(,@09)                                   0497 00259000
*/* SET QUEUEING POINTERS                                            */ 00260000
*                                                                  0498 00261000
*   PRFCORE=SCBSCHDR;               /* SET CORE ADDR TO HDR  @YM08017*/ 00262000
         L     @05,IEDQSPTR-1(,IEDQLPTR)                           0498 00263000
         LA    @05,0(,@05)                                         0498 00264000
         MVC   PRFCORE(3,@09),SCBSCHDR(@05)                        0498 00265000
*   PRFCRCD=SCBSCHDR;               /* SET CURRENT REC TO HDR      0499 00266000
*                                                            @YM08017*/ 00267000
         MVC   PRFCRCD(3,@09),SCBSCHDR(@05)                        0499 00268000
*   PRFCHDR=SCBSCHDR;               /* SET CURRENT HDR TO HDR      0500 00269000
*                                                            @YM07811*/ 00270000
         MVC   PRFCHDR(3,@09),SCBSCHDR(@05)                        0500 00271000
*/*                                                                  */ 00272000
*                                                                  0501 00273000
*   IF QCBTSSES=ON THEN             /* IN TSO SESSION                */ 00274000
         TM    QCBTSSES(IEDQQPTR),B'10000000'                      0501 00275000
         BNO   @RF00501                                            0501 00276000
*     PRFTSMSG=ON;                  /* YES, SET TSO INDICATOR        */ 00277000
         OI    PRFTSMSG(@09),B'00010000'                           0502 00278000
*/* SET UP ERROR INFORMATION IN LCB                                  */ 00279000
*                                                                  0503 00280000
*   LCBRCQCB=AVTMSGS->BDQCB;        /* SET RETURN QCB TO IEDQBD'S QCB*/ 00281000
@RF00501 L     @09,AVTMSGS-1(,IEDQAPTR)                            0503 00282000
         L     @09,BDQCB(,@09)                                     0503 00283000
         ST    @09,LCBRCQCB(,IEDQLPTR)                             0503 00284000
*   LCBERROR=ON;                    /* SET ERROR TO STOP DISK        */ 00285000
         OI    LCBERROR(IEDQLPTR),B'00000100'                      0504 00286000
*   LCBABRTN=ON;                    /* SET ABORT FLAG                */ 00287000
         OI    LCBABRTN(IEDQLPTR),B'00000001'                      0505 00288000
*   LCBUSUE=ON;                     /* SET UNIT EXCEPTION            */ 00289000
         OI    LCBUSUE(IEDQLPTR),B'00000001'                       0506 00290000
*/*                                                                  */ 00291000
*                                                                  0507 00292000
*   RPARM=BUFPTR;                   /* ADDRESS OF ERROR BUFFER       */ 00293000
         L     RPARM,BUFPTR                                        0507 00294000
*   CALL DSPPOSTR;                  /* POST ERROR BUFFER TO MH       */ 00295000
         LA    @15,DSPPOSTR(,RDISP)                                0508 00296000
         BALR  @14,@15                                             0508 00297000
*/*                                                                  */ 00298000
*                                                                  0509 00299000
*   R14=R14SV02;                    /* RESTORE RETURN ADDRESS        */ 00300000
         L     R14,R14SV02                                         0509 00301000
*   RETURN;                         /* RETURN TO CALLER              */ 00302000
@EL00003 DS    0H                                                  0510 00303000
@EF00003 DS    0H                                                  0510 00304000
@ER00003 BR    @14                                                 0510 00305000
*   END BLDERBUF;                                                  0511 00306000
*   RFY                                                            0512 00307000
*     IEDQPRF BASED(IEDQPPTR);                                     0512 00308000
*   RFY                                                            0513 00309000
*     IEDPIU BASED(PIUPTR);                                        0513 00310000
*                                                                  0514 00311000
*/********************************************************************/ 00312000
*/*                                                                  */ 00313000
*/* THIS ROUTINE INTERFACES TO THE BRACKET MANAGER TO DO BRACKET     */ 00314000
*/* STATE MANAGEMENT.  IT ALSO HANDLES BRACKET ERROR CONDITIONS.     */ 00315000
*/*                                                                  */ 00316000
*/********************************************************************/ 00317000
*                                                                  0514 00318000
*BRACKMGR:                                                         0514 00319000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0514 00320000
BRACKMGR DS    0H                                                  0515 00321000
*   R14SV03=R14;                    /* SAVE RETURN ADDRESS           */ 00322000
         ST    R14,R14SV03                                         0515 00323000
*/*                                                                  */ 00324000
*                                                                  0516 00325000
*   IF TRHTYPE=OFF THEN             /* REQUEST                       */ 00326000
         LA    @09,10                                              0516 00327000
         ALR   @09,PIUPTR                                          0516 00328000
         TM    TRHTYPE(@09),B'10000000'                            0516 00329000
         BNZ   @RF00516                                            0516 00330000
*     DO;                           /* YES                           */ 00331000
*       IF TRHBC=ON THEN            /* BEGIN CHAIN                   */ 00332000
         TM    TRHBC(@09),B'00000010'                              0518 00333000
         BNO   @RF00518                                            0518 00334000
*         DO;                       /* YES                           */ 00335000
*           IF SIBPEBI=OFF THEN     /* PRIMARY MAY NOT SEND EB       */ 00336000
         L     @05,SIBPTR                                          0520 00337000
         TM    SIBPEBI(@05),B'00000100'                            0520 00338000
         BNZ   @RF00520                                            0520 00339000
*             TRHEB=OFF;            /* YES, TURN OFF EB              */ 00340000
         NI    TRHEB(@09),B'10111111'                              0521 00341000
*/*        INSURE CORRECT BRACKET BIT SETTINGS FOR MSGGEN            */ 00342000
*                                                                  0522 00343000
*           IF LCBMSGNN=ON THEN     /* MSGGEN REQUEST        @YM06886*/ 00344000
@RF00520 TM    LCBMSGNN(IEDQLPTR),B'01000000'                      0522 00345000
         BNO   @RF00522                                            0522 00346000
*             DO;                   /* YES, THIS MEANS WE ARE IN THE    00347000
*                                      PROCESS OF SENDING A MSGGEN   */ 00348000
*               IF SIBBSTAT=SIBBETB THEN/* 'BETWEEN BRACKET' STATE   */ 00349000
         L     @09,SIBPTR                                          0524 00350000
         TM    SIBBSTAT(@09),B'00001111'                           0524 00351000
         BNZ   @RF00524                                            0524 00352000
*                 TRHBB=ON;         /* FORCE BEGIN BRACKET           */ 00353000
         OI    TRHBB+10(PIUPTR),B'10000000'                        0525 00354000
*               ELSE                /* 'IN BRACKET' STATE            */ 00355000
*                 TRHBB=OFF;        /* FORCE NOT BEGIN BRACKET       */ 00356000
         B     @RC00524                                            0526 00357000
@RF00524 NI    TRHBB+10(PIUPTR),B'01111111'                        0526 00358000
*             END;                                                 0527 00359000
*         END;                                                     0528 00360000
*       ELSE                        /* NOT BEGIN CHAIN               */ 00361000
*         DO;                                                      0529 00362000
         B     @RC00518                                            0529 00363000
@RF00518 DS    0H                                                  0530 00364000
*           TRHBB=OFF;              /* TURN OFF BB                   */ 00365000
*           TRHEB=OFF;              /* TURN OFF EB                   */ 00366000
         NI    TRHBB+10(PIUPTR),B'00111111'                        0531 00367000
*         END;                                                     0532 00368000
*     END;                                                         0533 00369000
*                                                                  0533 00370000
@RC00518 DS    0H                                                  0534 00371000
*/*                                                                  */ 00372000
*                                                                  0534 00373000
*   PLBM=''B;                       /* INIT PARM LIST TO 0           */ 00374000
@RF00516 XC    PLBM(16),PLBM                                       0534 00375000
*   PLBMSNDI=ON;                    /* INDICATE TCAM IS SENDING    0535 00376000
*                                      (OUTBOUND FLOW)               */ 00377000
         LA    @09,PLBM                                            0535 00378000
         OI    PLBMSNDI(@09),B'10000000'                           0535 00379000
*   PLBMRH=IEDRH;                   /* MOVE RH TO PARM LIST          */ 00380000
         LA    @05,10                                              0536 00381000
         ALR   @05,PIUPTR                                          0536 00382000
         MVC   PLBMRH(3,@09),IEDRH(@05)                            0536 00383000
*   PLBMSIB=SIBPTR;                 /* SIB POINTER TO PARM LIST      */ 00384000
         L     @02,SIBPTR                                          0537 00385000
         ST    @02,PLBMSIB(,@09)                                   0537 00386000
*   PLBMLCB=RLCB;                   /* LCB POINTER TO PARM LIST      */ 00387000
         ST    RLCB,PLBMLCB(,@09)                                  0538 00388000
*/*                                                                  */ 00389000
*                                                                  0539 00390000
*   IF TRHRUCAT^=TRHFMD THEN        /* NOT FM DATA                   */ 00391000
         TM    TRHRUCAT(@05),B'01100000'                           0539 00392000
         BZ    @RF00539                                            0539 00393000
*     PLBMCMD=RU(1);                /* YES, MOVE IN COMMAND BYTE     */ 00394000
         MVC   PLBMCMD(1,@09),RU+13(PIUPTR)                        0540 00395000
*/*                                                                  */ 00396000
*                                                                  0541 00397000
*   RPARM=ADDR(PLBM);               /* BRACKET PARM LIST ADDRESS     */ 00398000
@RF00539 LA    RPARM,PLBM                                          0541 00399000
*   CALL IEDBSM;                    /* DO BRACKET STATE MANAGEMENT   */ 00400000
         L     @09,AVTSAVTP(,IEDQAPTR)                             0542 00401000
         L     @15,SAVTBSM(,@09)                                   0542 00402000
         BALR  @14,@15                                             0542 00403000
*   IF R15=0 THEN                   /* NO ERRORS FOUND BY BRACKET  0543 00404000
*                                      MANAGER                       */ 00405000
