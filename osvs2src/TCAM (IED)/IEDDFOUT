         TM    TRHRUCAT(@09),B'01100000'                           0667 00802000
         BNZ   @RF00667                                            0667 00803000
         TM    LCBCHN(IEDQLPTR),B'00010000'                        0667 00804000
         BNO   @RF00667                                            0667 00805000
         TM    LCBCHN(IEDQLPTR),B'11100000'                        0667 00806000
         BNZ   @RF00667                                            0667 00807000
*             LCBCHN=LCBPURG;       /* GO TO 'PURGING CHAIN' STATE 0668 00808000
*                                                            @YM06990*/ 00809000
         NI    LCBCHN(IEDQLPTR),B'00101111'                        0668 00810000
         OI    LCBCHN(IEDQLPTR),B'00100000'                        0668 00811000
*           IF SIBFMODE=SIBHDXFF THEN/* BOUND HALF-DUPLEX FLIP-FLOP     00812000
*                                                            @YM06990*/ 00813000
@RF00667 L     @09,SIBPTR                                          0669 00814000
         TM    SIBFMODE(@09),B'10000000'                           0669 00815000
         BNO   @RF00669                                            0669 00816000
         TM    SIBFMODE(@09),B'01000000'                           0669 00817000
         BNZ   @RF00669                                            0669 00818000
*             IF SIBRECOV=ON THEN   /* TCAM LU NOT RESPONSIBLE FOR 0670 00819000
*                                      RECOVERY              @YM06990*/ 00820000
         TM    SIBRECOV(@09),B'00100000'                           0670 00821000
         BNO   @RF00670                                            0670 00822000
*               SIBCD=OFF;          /* INDICATE RECEIVE STATE      0671 00823000
*                                                            @YM06990*/ 00824000
         NI    SIBCD(@09),B'01111111'                              0671 00825000
*             ELSE                  /* TCAM LU RESPONSIBLE FOR     0672 00826000
*                                      RECOVERY              @YM06990*/ 00827000
*               SIBCD=ON;           /* INDICATE SEND STATE   @YM06990*/ 00828000
         B     @RC00670                                            0672 00829000
@RF00670 L     @09,SIBPTR                                          0672 00830000
         OI    SIBCD(@09),B'10000000'                              0672 00831000
*         END;                      /*                       @YM06990*/ 00832000
@RC00670 DS    0H                                                  0673 00833000
@RF00669 DS    0H                                                  0674 00834000
*     END;                                                         0674 00835000
@RF00665 DS    0H                                                  0675 00836000
*   R14=R14SV04;                    /* RESTORE RETURN ADDRESS        */ 00837000
@RC00594 L     R14,R14SV04                                         0675 00838000
