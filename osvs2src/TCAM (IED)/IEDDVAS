         BNP   @RF00336                                            0336 00311000
         L     @09,CTRM                                            0336 00312000
         TM    TRMOCNI(@09),B'00100000'                            0336 00313000
         BNZ   @RF00336                                            0336 00314000
         TM    TRMCLOSN(@09),B'00100000'                           0336 00315000
         BNZ   @RF00336                                            0336 00316000
*         DO;                       /* AND CINTVAL>0 AND NOT STOPLINE   00317000
*                                      OR CLOSEDOWN          @XM05693*/ 00318000
*           RTIME=CQCB->QCBINTVL;   /* PICK UP CINTVL        @XM05693*/ 00319000
         LR    RTIME,@14                                           0338 00320000
*           IF RTIME>SEC12HR THEN   /* IS IT GREATER THAN 12 HOURS 0339 00321000
*                                                            @XM05693*/ 00322000
         L     @14,@CF02155                                        0339 00323000
         CR    RTIME,@14                                           0339 00324000
         BNH   @RF00339                                            0339 00325000
*             DO;                   /* YES                   @XM05693*/ 00326000
*               RTIME=RTIME-SEC12HR;/* SUBTRACT 12 HOURS     @XM05693*/ 00327000
         SLR   RTIME,@14                                           0341 00328000
*               CQCB->QCBTIME=ON;   /* INDICATE 12 HOURS     @XM05693*/ 00329000
         OI    QCBTIME(CQCB),B'00000001'                           0342 00330000
*             END;                  /*                       @XM05693*/ 00331000
*           ELSE                                                   0344 00332000
*             ;                     /* CINTVL LESS THAN 12 HOURS   0344 00333000
