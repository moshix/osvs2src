@RF00355 DS    0H                                                  0369 00395000
*                                                                  0369 00396000
*   /*****************************************************************/ 00397000
*   /*                                                               */ 00398000
*   /* WHEN SENDING AND RECEIVING, TCAM'S TTCIN'S ARE TRANSLATED TO  */ 00399000
*   /* AND FROM NETWORK ADDRESSES VIA TWO TABLES: THE RESOURCE VECTOR*/ 00400000
*   /* TABLE (RVT) ON RECEIVE AND THE NETWORK ADDRESS TABLE (NAT) ON */ 00401000
*   /* SEND. SINCE THE NCP COMMUNICATES ONLY WITH THE CTERM, IF TCAM */ 00402000
*   /* IS SENDING TO A LOGICAL CONNECTION INDEX, THE LCI'S TTCIN MUST*/ 00403000
*   /* BE TRANSLATED TO THE CTERM'S NETWORK ADDRESS. ON RECEIVE THE  */ 00404000
*   /* CTERM'S NETWORK ADDRESS MUST BE TRANSLATED TO THE LCI'S TTCIN.*/ 00405000
*   /* THIS IS ACCOMPLISHED BY SWAPPING THE CTERM AND LCI ENTRIES IN */ 00406000
*   /* THE RVT AND NAT TABLES. THE ELEMENT PORTION OF A NETWORK      */ 00407000
*   /* ADDRESS IS AN INDEX INTO THE RVT. A TTCIN IS AN INDEX INTO THE*/ 00408000
*   /* NAT. FOR EACH NCP IN THE SYSTEM, THERE IS A UNIQUE RVT. THE   */ 00409000
*   /* SUBAREA TABLE (SAT) IS A TABLE OF POINTERS TO THESE RVT'S. THE*/ 00410000
*   /* SUBAREA TABLE IS INDEXED BY THE SUBAREA PORTION OF THE NETWORK*/ 00411000
*   /* ADDRESS.                                                      */ 00412000
*   /*                                                               */ 00413000
*   /*****************************************************************/ 00414000
*                                                                  0369 00415000
*/*                                                                  */ 00416000
*/*CALL IEDIAP04 TO CONVERT THE LCI'S TTCIN TO A NETWORK ADDRESS     */ 00417000
*/*                                                                  */ 00418000
*                                                                  0369 00419000
*   RPARM=LCICIN;                   /* PARM PASSED IS TTCIN OF LCI   */ 00420000
         LH    RPARM,LCICIN                                        0369 00421000
         N     RPARM,@CF02175                                      0369 00422000
*   RFY                                                            0370 00423000
*     RLINK RSTD;                                                  0370 00424000
*   CALL IEDIAP04;                  /* ON RETURN, RLINK WILL CONTAIN    00425000
*                                      THE NETWORK ADDRESS           */ 00426000
         L     @09,IEDQAPTR                                        0371 00427000
         L     @09,AVTSAVTP(,@09)                                  0371 00428000
         L     @15,SAVTTNTX(,@09)                                  0371 00429000
         BALR  @14,@15                                             0371 00430000
*   LCISUBA=RLINK;                  /* SAVE THE NETWORK ADDRESS IN 0372 00431000
*                                      TWO LOCATIONS                 */ 00432000
         STCM  RLINK,3,LCISUBA                                     0372 00433000
*   LCIELEM=RLINK;                                                 0373 00434000
*                                                                  0373 00435000
         STCM  RLINK,3,LCIELEM                                     0373 00436000
*/*                                                                  */ 00437000
*/*SEPARATE THE LCI'S NETWORK ADDRESS INTO SUBAREA AND ELEMENT       */ 00438000
*/*                                                                  */ 00439000
*                                                                  0374 00440000
*   SLL(LCIELEM,SAVTSUBL);          /* TO GET ELEMENT ADDRESS, SHIFT    00441000
*                                      SUBAREA'S ADDRESS OUT OF    0374 00442000
*                                      NETWORK ADDRESS               */ 00443000
         SLR   @14,@14                                             0374 00444000
         ICM   @14,3,LCIELEM                                       0374 00445000
         L     @09,IEDQAPTR                                        0374 00446000
         L     @09,AVTSAVTP(,@09)                                  0374 00447000
         SLR   @06,@06                                             0374 00448000
         IC    @06,SAVTSUBL(,@09)                                  0374 00449000
         SLL   @14,0(@06)                                          0374 00450000
         STCM  @14,3,LCIELEM                                       0374 00451000
*   SRL(LCIELEM,SAVTSUBL);          /* RIGHT ADJUST ELEMENT ADDRESS  */ 00452000
         SLR   @14,@14                                             0375 00453000
         ICM   @14,3,LCIELEM                                       0375 00454000
         SRL   @14,0(@06)                                          0375 00455000
         STCM  @14,3,LCIELEM                                       0375 00456000
*/*                                                                  */ 00457000
*                                                                  0376 00458000
*   ELEMLEN=NABITLEN-SAVTSUBL;      /* CALC LENGTH OF ELEMENT'S    0376 00459000
*                                      ADDRESS                       */ 00460000
         LCR   @06,@06                                             0376 00461000
         AL    @06,@CF00464                                        0376 00462000
         LR    ELEMLEN,@06                                         0376 00463000
*   SRL(LCISUBA,ELEMLEN);           /* TO GET SUBAREA ADDRESS, SHIFT    00464000
*                                      ELEMENT'S ADDRESS OUT OF    0377 00465000
*                                      NETWORK ADDRESS               */ 00466000
         SLR   @14,@14                                             0377 00467000
         ICM   @14,3,LCISUBA                                       0377 00468000
         SRL   @14,0(ELEMLEN)                                      0377 00469000
         STCM  @14,3,LCISUBA                                       0377 00470000
*/*                                                                  */ 00471000
*/*CALL IEDIAP04 TO CONVERT THE CTERM'S TTCIN TO A NETWORK ADDRESS   */ 00472000
*/*                                                                  */ 00473000
*                                                                  0378 00474000
*   RPARM=CTERMCIN;                 /* PARM PASSED IS THE CTERM TTCIN*/ 00475000
         LH    RPARM,CTERMCIN                                      0378 00476000
         N     RPARM,@CF02175                                      0378 00477000
*   CALL IEDIAP04;                  /* ON RETURN RLINK CONTAINS THE     00478000
*                                      NETWORK ADDRESS               */ 00479000
         L     @15,SAVTTNTX(,@09)                                  0379 00480000
         BALR  @14,@15                                             0379 00481000
*   CTRMELEM=RLINK;                 /* SAVE THE CTERM'S NETWORK    0380 00482000
*                                      ADDRESS....                   */ 00483000
