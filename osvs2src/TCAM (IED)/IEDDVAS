*       RFROMQCB=RLCB;              /* SET FROM QCB TO PLCB  @YM05686*/ 00223000
         LR    RFROMQCB,RLCB                                       0305 00224000
*       RZERO=RAVT;                 /* SAVE SAVEAREA PTR     @YM08132*/ 00225000
