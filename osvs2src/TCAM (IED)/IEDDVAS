         BO    @RT00297                                            0297 00184000
         TM    TRMCLOSN(RTRM),B'00100000'                          0297 00185000
         BNO   @RF00297                                            0297 00186000
@RT00297 DS    0H                                                  0298 00187000
*     LCBLNENT=LINECIN;             /* SET LCBLNENT FOR OPERATOR CNTL   00188000
*                                                            @YM07428*/ 00189000
         LH    @09,LINECIN                                         0298 00190000
         N     @09,@CF02175                                        0298 00191000
         STH   @09,LCBLNENT(,RLCB)                                 0298 00192000
*   ELSE                            /* NOT STOPLINE OR CLOSEDOWN   0299 00193000
*                                                            @YM07428*/ 00194000
*     LCBLNENT=LCBTTCIN;            /* SET LCBLNENT FOR OLD CODE   0299 00195000
*                                                            @YM07750*/ 00196000
         B     @RC00297                                            0299 00197000
@RF00297 LH    @09,LCBTTCIN(,RLCB)                                 0299 00198000
         N     @09,@CF02175                                        0299 00199000
         STH   @09,LCBLNENT(,RLCB)                                 0299 00200000
*                                                                  0300 00201000
*   /*****************************************************************/ 00202000
*   /*                                                               */ 00203000
*   /* PRIME SENDING IF NECESSARY                            @YM05686*/ 00204000
*   /*                                                               */ 00205000
*   /*****************************************************************/ 00206000
*                                                                  0300 00207000
*   RFY                                                            0300 00208000
*     RRETCODE RSTD;                /* RESTRICT RRETCODE     @YM05686*/ 00209000
@RC00297 DS    0H                                                  0301 00210000
*   IF LCBSTCBA->STCBVTO=DSPSEND THEN/* IF SS STILL IN PLCB  @YM05686*/ 00211000
