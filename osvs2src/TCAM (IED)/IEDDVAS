         CLI   STCBVTO(@14),X'22'                                  0301 00213000
         BNE   @RF00301                                            0301 00214000
*     DO;                           /*                       @YM05686*/ 00215000
*       SAVEQCB=RQCB;               /* SAVE RQCB             @YM05686*/ 00216000
         LR    SAVEQCB,RQCB                                        0303 00217000
