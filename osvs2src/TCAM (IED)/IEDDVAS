*       DO;                         /*                       @YM05686*/ 00368000
*         QCBSNDRQ=OFF;             /* RESET SS REQUESTED    @YM05686*/ 00369000
         NI    QCBSNDRQ(@14),B'11011111'                           0358 00370000
*         SAVEQCB=RQCB;             /* SAVE RQCB             @YM05686*/ 00371000
         LR    SAVEQCB,RQCB                                        0359 00372000
