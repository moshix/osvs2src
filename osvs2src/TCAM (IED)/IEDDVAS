         SLR   @09,@09                                             0314 00252000
         ICM   @09,7,TRMDESTQ(@14)                                 0314 00253000
         CLI   QCBSTPRI(@09),X'50'                                 0314 00254000
         BE    @RF00314                                            0314 00255000
*             DO;                   /*                       @YM05686*/ 00256000
*               CQCB->QCBRCVRQ=ON;  /* SET PLCB REQUESTED    @YM05686*/ 00257000
         OI    QCBRCVRQ(CQCB),B'00010000'                          0316 00258000
*               RPARM=CTRM->TRMDESTQ;/* PASS FROM-TRM'S QCB  @YM05686*/ 00259000
         LR    RPARM,@09                                           0317 00260000
*               CALL IEDIAP01;      /* REQUEST A PLCB        @YM05686*/ 00261000
         L     @10,IEDQAPTR                                        0318 00262000
         L     @10,AVTSAVTP(,@10)                                  0318 00263000
         L     @15,SAVTPREQ(,@10)                                  0318 00264000
         BALR  @14,@15                                             0318 00265000
*               IF RRETCODE^=0 THEN /* IF PLCB ASSIGNED THEN @YM05686*/ 00266000
         LTR   RRETCODE,RRETCODE                                   0319 00267000
         BZ    @RF00319                                            0319 00268000
*                 DO;               /*                       @YM05686*/ 00269000
*                   RPARM=RRETCODE; /* SET ELEMENT REG       @YM05686*/ 00270000
         LR    RPARM,RRETCODE                                      0321 00271000
*                   RPARM->LCBQCBA=RPARM;/* TPOST TO ITSELF  @YM05686*/ 00272000
         STCM  RPARM,7,LCBQCBA(RPARM)                              0322 00273000
*                   RPARM->LCBPRI=PRILNFRE;/* SET PRIORITY   @YM05686*/ 00274000
         MVI   LCBPRI(RPARM),X'E0'                                 0323 00275000
*                   SAVEQCB=RQCB;   /* SAVE RQCB             @YM05686*/ 00276000
         LR    SAVEQCB,RQCB                                        0324 00277000
*                   RZERO=RAVT;     /* SAVE SAVEAREA PTR     @YM08132*/ 00278000
         LR    RZERO,RAVT                                          0325 00279000
*                   RAVT=ADDR(AVTSAVE2);/* GET AVT ADDRESS   @YM08132*/ 00280000
         L     RAVT,IEDQAPTR                                       0326 00281000
         LA    RAVT,AVTSAVE2(,RAVT)                                0326 00282000
*                   CALL DSPPOSTR;  /* TPOST PLCB TO ITSELF  @YM05686*/ 00283000
         LA    @15,DSPPOSTR(,RDISP)                                0327 00284000
         BALR  @14,@15                                             0327 00285000
*                   RAVT=RZERO;     /* RESTORE SAVEAREA PTR  @YM08132*/ 00286000
         LR    RAVT,RZERO                                          0328 00287000
