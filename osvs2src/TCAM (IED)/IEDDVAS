         SLR   IEDQSATP,IEDQSATP                                   0388 00532000
         ICM   IEDQSATP,3,CTRMSUBA                                 0388 00533000
         BCTR  IEDQSATP,0                                          0388 00534000
         SLA   IEDQSATP,3                                          0388 00535000
         ALR   @09,IEDQSATP                                        0388 00536000
         LR    IEDQSATP,@09                                        0388 00537000
*   IEDQRVTP=SATRNRVT;              /* PTR TO CTERM RVT              */ 00538000
         L     @09,SATRNRVT(,IEDQSATP)                             0389 00539000
         ST    @09,IEDQRVTP                                        0389 00540000
*/*                                                                  */ 00541000
*/*SWAP THE LCI AND CTERM ENTRIES IN THE RVT                         */ 00542000
