         TITLE 'IEDDVAS, CONTROL BLOCK ASSIGNMENT SUBROUTINE           *00001000
                        '                                               00002000
IEDDVAS  CSECT ,                                                   0001 00003000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00004000
         BALR  @12,0                                               0001 00005000
@PSTART  DS    0H                                                  0001 00006000
         USING @PSTART,@12                                         0001 00007000
         ST    @13,@SA00001+4                                      0001 00008000
         LA    @14,@SA00001                                        0001 00009000
         ST    @14,8(,@13)                                         0001 00010000
         LR    @13,@14                                             0001 00011000
*                                   /*                                  00012000
IEDDVAS IEDHJN START,HJN                IDENTIFICATION MACRO            00013000
*                                                                  0258 00014000
*   /*****************************************************************/ 00015000
*   /*                                                               */ 00016000
*   /* CONTROL BLOCK POINTERS AT ENTRY ARE:                          */ 00017000
*   /* LINETRM-->CTERMTRM-->CTERMQCB-->PLCB. CONTROL BLOCK POINTERS  */ 00018000
*   /* AFTER DEVICE ASSOCIATION WILL BE:                             */ 00019000
*   /* LINETRM-->LCITRM-->LCIQCB-->PLCB. BEFORE ANY POINTERS ARE     */ 00020000
*   /* MOVED, ADDRESSABILITY WILL BE ESTABLISHED TO NEEDED CONTROL   */ 00021000
*   /* BLOCKS.                                                       */ 00022000
*   /*                                                               */ 00023000
*   /*****************************************************************/ 00024000
*                                                                  0258 00025000
*   IEDQAPTR=TCXAVT;                /* ADDRESSABILITY TO AVT         */ 00026000
         L     @09,CVTPTR                                          0258 00027000
         L     @09,TCXPTR(,@09)                                    0258 00028000
         L     @09,TCXAVT(,@09)                                    0258 00029000
         ST    @09,IEDQAPTR                                        0258 00030000
*   RDISP=AVTEA;                    /* SET DISPATCHER BASE   @YM05686*/ 00031000
         L     RDISP,AVTEA(,@09)                                   0259 00032000
*   CTERMCIN=RZERO;                 /* SAVE TTCIN OF CTERM           */ 00033000
         STH   RZERO,CTERMCIN                                      0260 00034000
*   LCICIN=RPARM;                   /* SAVE TTCIN OF LCI TERMINAL    */ 00035000
         STH   RPARM,LCICIN                                        0261 00036000
*   CALL IEDQTNT;                   /* CONVERT LCI TTCIN TO TRM ADDR */ 00037000
         L     @15,AVTRNMPT(,@09)                                  0262 00038000
         BALR  @14,@15                                             0262 00039000
*   RPARM=RPARM-LENGTH(IEDNTRM);    /* SET POINTER TO START OF     0263 00040000
*                                      CONTROL BLOCK PREFIX          */ 00041000
         SL    RPARM,@CF01964                                      0263 00042000
*   RTRM=RPARM;                     /* ADDRESSABILITY TO LCI TRM     */ 00043000
         LR    RTRM,RPARM                                          0264 00044000
*   RPARM=CTERMCIN;                 /* SET PARM FOR IEDQTNT          */ 00045000
         LH    RPARM,CTERMCIN                                      0265 00046000
         N     RPARM,@CF02175                                      0265 00047000
*   CALL IEDQTNT;                   /* CONVERT CTERM TTCIN TO TRM ADR*/ 00048000
         L     @09,IEDQAPTR                                        0266 00049000
         L     @15,AVTRNMPT(,@09)                                  0266 00050000
         BALR  @14,@15                                             0266 00051000
*   RPARM=RPARM-LENGTH(IEDNTRM);    /* SET POINTER TO START OF CNTRL    00052000
*                                      BLOCK PREFIX                  */ 00053000
         SL    RPARM,@CF01964                                      0267 00054000
*   CTRM=RPARM;                     /* ADDRESSABILITY TO CTERM TRM   */ 00055000
         ST    RPARM,CTRM                                          0268 00056000
*   CQCB=CTRM->TRMDESTQ;            /* ADDRESSABILITY TO CTERM QCB   */ 00057000
         L     @09,CTRM                                            0269 00058000
         SLR   CQCB,CQCB                                           0269 00059000
         ICM   CQCB,7,TRMDESTQ(@09)                                0269 00060000
*   CQCB=CQCB-LENGTH(IEDNQCB);      /* POINT TO NEG PREFIX   @XM05693*/ 00061000
         SL    CQCB,@CF00046                                       0270 00062000
*   RQCB=TRMDESTQ;                  /* ADDRESSABILITY TO LCI QCB     */ 00063000
         SLR   RQCB,RQCB                                           0271 00064000
         ICM   RQCB,7,TRMDESTQ(RTRM)                               0271 00065000
*   IF QCBSTPRI=QCBCNTVL&QCBDELAY=ON THEN/* IS TO QCB CINTVL         */ 00066000
         CLI   QCBSTPRI(IEDQQPTR),X'70'                            0272 00067000
         BNE   @RF00272                                            0272 00068000
         TM    QCBDELAY(IEDQQPTR),B'00000010'                      0272 00069000
         BNO   @RF00272                                            0272 00070000
*     DO;                           /* AND ON TIME DELAY     @XM05693*/ 00071000
*       RPARM=RQCB;                 /* YES SO TAKE OFF TIME DELAY  0274 00072000
*                                                            @XM05693*/ 00073000
         LR    RPARM,RQCB                                          0274 00074000
*       CALL IEDQHG02;              /* INVOKE TIME DELAY DEQUEUE   0275 00075000
*                                                            @XM05693*/ 00076000
         L     @09,IEDQAPTR                                        0275 00077000
         L     @15,AVTHG02(,@09)                                   0275 00078000
         BALR  @14,@15                                             0275 00079000
*     END;                          /*                       @XM05693*/ 00080000
*   ELSE                                                           0277 00081000
*     ;                             /* NOT CINTVL AND ON TDQ @XM05693*/ 00082000
@RF00272 DS    0H                                                  0278 00083000
*   LINECIN=CTRM->TRMCOHRT;         /* SAVE LINE TNT IND     @YM07428*/ 00084000
         L     @09,CTRM                                            0278 00085000
         LH    @09,TRMCOHRT(,@09)                                  0278 00086000
         N     @09,@CF02175                                        0278 00087000
         STH   @09,LINECIN                                         0278 00088000
*   RPARM=CTRM->TRMCOHRT;           /* SET PARM FOR IEDQTNT          */ 00089000
         LR    RPARM,@09                                           0279 00090000
*   CALL IEDQTNT;                   /* CONVERT LINE TTCIN TO TRM ADDR*/ 00091000
         L     @09,IEDQAPTR                                        0280 00092000
         L     @15,AVTRNMPT(,@09)                                  0280 00093000
         BALR  @14,@15                                             0280 00094000
*   RPARM=RPARM-LENGTH(IEDNTRM);    /* SET POINTER TO START OF CNTRL    00095000
*                                      BLK PREFIX                    */ 00096000
         SL    RPARM,@CF01964                                      0281 00097000
*   LINETRM=RPARM;                  /* ADDRESSABILITY TO LINE TRM    */ 00098000
         LR    LINETRM,RPARM                                       0282 00099000
*   RLCB=CQCB->QCBPLCBA;            /* PLCB ADDRESSABILITY   @YM05686*/ 00100000
         L     @09,QCBPLCBA-1(,CQCB)                               0283 00101000
         LA    @09,0(,@09)                                         0283 00102000
         LR    RLCB,@09                                            0283 00103000
*                                                                  0284 00104000
*   /*****************************************************************/ 00105000
*   /*                                                               */ 00106000
*   /* IN THE HEIRARCHIAL CONTROL BLOCK STRUCTURE, REPLACE THE       */ 00107000
*   /* CTERM'S TRM AND QCB WITH THE LCI'S TRM AND QCB. ASSIGN THE    */ 00108000
*   /* CTERM'S PLCB TO THE LCI'S QCB.                                */ 00109000
*   /*                                                               */ 00110000
*   /*****************************************************************/ 00111000
*                                                                  0284 00112000
*   LINETRM->TRMSUBST=LCICIN;       /* LINE TRM MUST POINT TO TRM OF    00113000
*                                      LCI                           */ 00114000
         LH    @06,LCICIN                                          0284 00115000
         N     @06,@CF02175                                        0284 00116000
         STH   @06,TRMSUBST(,LINETRM)                              0284 00117000
*   TRMCOHRT=CTRM->TRMCOHRT;        /* LCI TRM MUST POINT TO TRM OF     00118000
*                                      LINE                          */ 00119000
         L     @06,CTRM                                            0285 00120000
         LH    @05,TRMCOHRT(,@06)                                  0285 00121000
         N     @05,@CF02175                                        0285 00122000
         STH   @05,TRMCOHRT(,RTRM)                                 0285 00123000
*   QCBPLCBA=CQCB->QCBPLCBA;        /* MOVE PLCB FROM CTERM TO LCI   */ 00124000
         LR    @05,IEDQQPTR                                        0286 00125000
         SL    @05,@CF00046                                        0286 00126000
         STCM  @09,7,QCBPLCBA(@05)                                 0286 00127000
*   CQCB->QCBPLCBN=OFF;             /* INDICATE CTERM HAS NO PLCB    */ 00128000
         NI    QCBPLCBN(CQCB),B'01111111'                          0287 00129000
*   QCBPLCBN=ON;                    /* INDICATE LCI HAS A PLCB       */ 00130000
         OI    QCBPLCBN(@05),B'10000000'                           0288 00131000
*   CQCB->QCBTTCIN=CTERMCIN;        /* ASSIGN CTERM'S TTCIN TO QCB   */ 00132000
         LH    @09,CTERMCIN                                        0289 00133000
         N     @09,@CF02175                                        0289 00134000
         STH   @09,QCBTTCIN(,CQCB)                                 0289 00135000
*   LCBTTCIN=LCICIN;                /* ASSIGN LCI TTCIN TO PLCB    0290 00136000
*                                                            @YM04678*/ 00137000
         LH    @09,LCICIN                                          0290 00138000
         N     @09,@CF02175                                        0290 00139000
         STH   @09,LCBTTCIN(,RLCB)                                 0290 00140000
*   IF CTRM->TRMCLOSN=ON THEN       /* COPY TRMCLOSN BIT IF ON     0291 00141000
*                                                            @YM09106*/ 00142000
         TM    TRMCLOSN(@06),B'00100000'                           0291 00143000
         BNO   @RF00291                                            0291 00144000
*     TRMCLOSN=ON;                  /* TO LCI ENTRY          @YM09106*/ 00145000
         LA    @05,TRMCLOSN(,RTRM)                                 0292 00146000
         LR    @15,@05                                             0292 00147000
         LA    @09,3                                               0292 00148000
         NR    @15,@09                                             0292 00149000
         LCR   @15,@15                                             0292 00150000
         ALR   @05,@15                                             0292 00151000
         L     @09,0(,@05)                                         0292 00152000
@GL00002 LR    @06,@09                                             0292 00153000
         O     @06,@CB02178+3(@15)                                 0292 00154000
         CS    @09,@06,0(@05)                                      0292 00155000
         BNE   @GL00002                                            0292 00156000
*   IF CTRM->TRMOCNI=ON THEN        /* COPY TRMOCNI BIT IF ON      0293 00157000
*                                                            @YM09106*/ 00158000
@RF00291 L     @09,CTRM                                            0293 00159000
         TM    TRMOCNI(@09),B'00100000'                            0293 00160000
         BNO   @RF00293                                            0293 00161000
*     TRMOCNI=ON;                   /* TO LCI ENTRY          @YM09106*/ 00162000
         OI    TRMOCNI(RTRM),B'00100000'                           0294 00163000
*   CTRM->TRMCLOSN=OFF;             /* RESET TRMCLOSN IN CTRM ENTRY     00164000
*                                                            @YM09106*/ 00165000
@RF00293 L     @14,CTRM                                            0295 00166000
         LA    @05,TRMCLOSN(,@14)                                  0295 00167000
         LR    @15,@05                                             0295 00168000
         LA    @09,3                                               0295 00169000
         NR    @15,@09                                             0295 00170000
         LCR   @15,@15                                             0295 00171000
         ALR   @05,@15                                             0295 00172000
         L     @09,0(,@05)                                         0295 00173000
@GL00003 LR    @06,@09                                             0295 00174000
         N     @06,@CB02183+3(@15)                                 0295 00175000
         CS    @09,@06,0(@05)                                      0295 00176000
         BNE   @GL00003                                            0295 00177000
*   CTRM->TRMOCNI=OFF;              /* RESET TRMOCNI IN CTRM ENTRY 0296 00178000
*                                                            @YM09106*/ 00179000
         L     @09,CTRM                                            0296 00180000
         NI    TRMOCNI(@09),B'11011111'                            0296 00181000
*   IF TRMOCNI=ON|TRMCLOSN=ON THEN                                 0297 00182000
