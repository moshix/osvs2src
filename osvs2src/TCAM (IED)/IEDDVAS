         TM    TRMOCNI(@14),B'00100000'                            0311 00240000
         BNZ   @RF00311                                            0311 00241000
         TM    TRMCLOSN(@14),B'00100000'                           0311 00242000
         BNZ   @RF00311                                            0311 00243000
*         DO;                       /*                       @YM08570*/ 00244000
*           CQCB->QCBSNDRQ=ON;      /* REQ. SS FOR FROM-TRM  @YM05686*/ 00245000
         OI    QCBSNDRQ(CQCB),B'00100000'                          0313 00246000
*           IF CTRM->TRMTYPE^=TRMCTERM&/* IF FROM-TRM NOT CTERM AND     00247000
*                                      NOT CALL-IN ONLY THEN @YM05686*/ 00248000
*               CTRM->TRMDESTQ->QCBSTPRI^=QCBCALLN THEN            0314 00249000
         CLI   TRMTYPE(@14),X'48'                                  0314 00250000
