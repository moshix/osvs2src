         TITLE 'IEDIAB - PLCB,SIB,SAT,RVT,NAT INITIATION ROUTINE       *00001000
                        '                                               00002000
IEDIAB   CSECT ,                                                   0001 00003000
@PROLOG  DS    0H                                                  0002 00004000
*                                   /*                                  00005000
         USING IEDIAB,RBASE                                             00006000
         STM   14,12,AVTSAVE3+12(13)   SAVE REGS                        00007000
         LR    RBASE,R15               SET BASE                         00008000
IEDIAB   IEDHJN AFTERHJN,HJN                                            00009000
*                                                                  0160 00010000
*   /*****************************************************************/ 00011000
*   /*                                                               */ 00012000
*   /* LEFT ADJUST HOST SUBAREA, DEVELOP BIT MASK TO AND OFF ELEMENT */ 00013000
*   /* PORTION OF NETWORK ADDRESS, DEVELOP SHIFT COUNT TO SHIFT OFF  */ 00014000
*   /* SUBAREA PORTION NETWORK ADDRESS.                              */ 00015000
*   /*                                                               */ 00016000
*   /*****************************************************************/ 00017000
*                                                                  0160 00018000
*   R2TO8TH='80'X                   /* COMPARAND TO DETERMINE      0160 00019000
*                                      HI-ORDER BIT OF MAX SUBAREA.  */ 00020000
*       ;                                                          0160 00021000
         LA    R2TO8TH,128                                         0160 00022000
*   RNUMBITS=8;                     /* # BITS REQUIRED FOR MAX     0161 00023000
*                                      SUBAREA. START AT 8 AND WORK     00024000
*                                      DOWN.                         */ 00025000
         LA    RNUMBITS,8                                          0161 00026000
*   RMASK='FF'X;                    /* MASK TO AND OFF ELEMENT. START   00027000
*                                      AT X'FF00'                    */ 00028000
         LA    RMASK,255                                           0162 00029000
*   RHOST=SAVTSUBV;                 /* WILL CONTAIN HOST SUBAREA LEFT   00030000
*                                      ADJUSTED.                     */ 00031000
         L     @09,AVTSAVTP(,IEDQAPTR)                             0163 00032000
         SLR   @15,@15                                             0163 00033000
         IC    @15,SAVTSUBV(,@09)                                  0163 00034000
         LR    RHOST,@15                                           0163 00035000
*   SUBVSAVE=SAVTSUBV;              /* SAVE RIGHT ADJUSTED HOST    0164 00036000
*                                      SUBAREA FOR LATER             */ 00037000
         STC   @15,SUBVSAVE                                        0164 00038000
*   DO WHILE(RNUMBITS>=2)           /* DO WHILE # BITS >= 2(MINIMUM     00039000
*                                      ALLOWED)                      */ 00040000
*         ;                                                        0165 00041000
         B     @DE00165                                            0165 00042000
@DL00165 DS    0H                                                  0166 00043000
*     IF SAVTSUBA<R2TO8TH           /* IF MAX SUBAREA ^ >= CURRENT 0166 00044000
*                                      HI-BIT                        */ 00045000
*       THEN                                                       0166 00046000
         L     @09,AVTSAVTP(,IEDQAPTR)                             0166 00047000
         SLR   @15,@15                                             0166 00048000
         IC    @15,SAVTSUBA(,@09)                                  0166 00049000
         CLR   R2TO8TH,@15                                         0166 00050000
         BNH   @RF00166                                            0166 00051000
*       DO;                         /* THEN                          */ 00052000
*         R2TO8TH=R2TO8TH/2;        /* SHIFT HI-BIT RIGHT ONE PLACE. */ 00053000
         SRL   R2TO8TH,1                                           0168 00054000
*         RNUMBITS=RNUMBITS-1;      /* # SUBAREA BITS - 1.           */ 00055000
         BCTR  RNUMBITS,0                                          0169 00056000
*         RMASK=RMASK*2;            /* SHIFT MASK TO LEFT 1.         */ 00057000
         ALR   RMASK,RMASK                                         0170 00058000
*         RHOST=RHOST*2;            /* SHIFT HOST SUBAREA LEFT ONE 0171 00059000
*                                      BIT                           */ 00060000
         ALR   RHOST,RHOST                                         0171 00061000
*       END;                                                       0172 00062000
*     ELSE                                                         0173 00063000
*       DO;                         /* ELSE                          */ 00064000
         B     @RC00166                                            0173 00065000
@RF00166 DS    0H                                                  0174 00066000
*         SAVTSUBL=RNUMBITS;        /* SAVE SUBAREA SHIFT COUNT      */ 00067000
         L     @09,AVTSAVTP(,IEDQAPTR)                             0174 00068000
         STC   RNUMBITS,SAVTSUBL(,@09)                             0174 00069000
*         SAVTSUBM=RMASK;           /* SAVE ELEMENT AND MASK.        */ 00070000
         STC   RMASK,SAVTSUBM(,@09)                                0175 00071000
*         SAVTSUBV=RHOST;           /* HOST SUBAREA LEFT ADJUSTED    */ 00072000
         STC   RHOST,SAVTSUBV(,@09)                                0176 00073000
*         RNUMBITS=1;               /* BREAK DO LOOP                 */ 00074000
         LA    RNUMBITS,1                                          0177 00075000
*       END;                                                       0178 00076000
*   END;                                                           0179 00077000
*                                                                  0179 00078000
@RC00166 DS    0H                                                  0179 00079000
@DE00165 CL    RNUMBITS,@CF00067                                   0179 00080000
         BNL   @DL00165                                            0179 00081000
*   /*****************************************************************/ 00082000
*   /*                                                               */ 00083000
*   /* SET INDEX OF FIRST MH TABLE ENTRY INTO SAVT                   */ 00084000
*   /*                                                               */ 00085000
*   /*****************************************************************/ 00086000
*                                                                  0180 00087000
*   RTNT=AVTRNMPT;                  /* TNT BASE                      */ 00088000
