*   /*****************************************************************/ 00143000
*   /*                                                               */ 00144000
*   /* TNT INDEX LAST NAT ENTRY FROM TNT = # TNT ENTRIES + # TOTE TNT*/ 00145000
*   /* ENTRIES.                                                      */ 00146000
*   /*                                                               */ 00147000
*   /*****************************************************************/ 00148000
*                                                                  0188 00149000
*   LASTTNT=TNTLEN+TCXTOTE;         /* TNT INDEX LAST IN NAT         */ 00150000
*                                                                  0188 00151000
         STH   @11,LASTTNT                                         0188 00152000
*   /*****************************************************************/ 00153000
*   /*                                                               */ 00154000
*   /* #RVT ENTRIES FROM TNT = ACCUMULATION FROM SCAN + # TOTE TNT   */ 00155000
*   /* ENTRIES                                                       */ 00156000
*   /*                                                               */ 00157000
*   /*****************************************************************/ 00158000
*                                                                  0189 00159000
*   NRVT=NRVT+TCXTOTE;              /* ACCUMULATION+TOTE=#RVT      0189 00160000
*                                                            @YM07388*/ 00161000
         LH    @11,NRVT                                            0189 00162000
         N     @11,@CF02253                                        0189 00163000
         ALR   @09,@11                                             0189 00164000
         STH   @09,NRVT                                            0189 00165000
*   RFY                                                            0190 00166000
*     TCXPTR UNRSTD;                                               0190 00167000
*                                                                  0190 00168000
*   /*****************************************************************/ 00169000
*   /*                                                               */ 00170000
*   /* SET UP MH TABLE ENTRY COUNT.                                  */ 00171000
*   /*                                                               */ 00172000
*   /*****************************************************************/ 00173000
*                                                                  0191 00174000
*   RNMH=0;                         /* INITIATE CTR TO 0             */ 00175000
         SLR   RNMH,RNMH                                           0191 00176000
*   RMH=SAVTMHTB;                   /* 1ST MH ENTRY ADDRESS          */ 00177000
*                                                                  0192 00178000
         L     RMH,SAVTMHTB(,@15)                                  0192 00179000
*   /*****************************************************************/ 00180000
*   /*                                                               */ 00181000
*   /* IF THERE IS AN MH TABLE THEN                                  */ 00182000
*   /*                                                               */ 00183000
*   /*****************************************************************/ 00184000
*                                                                  0193 00185000
*   IF RMH^=0                       /* IS THERE AN MH TABLE?         */ 00186000
*     THEN                                                         0193 00187000
         LTR   RMH,RMH                                             0193 00188000
         BZ    @RF00193                                            0193 00189000
*     DO;                           /* YES                           */ 00190000
*                                                                  0194 00191000
*       /*************************************************************/ 00192000
*       /*                                                           */ 00193000
*       /* COUNT # MH TABLE ENTRIES AND ADD TO # RVT ENTRIES.        */ 00194000
*       /*                                                           */ 00195000
*       /*************************************************************/ 00196000
*                                                                  0195 00197000
*       CALL MHCOUNT;                                              0195 00198000
         BAL   @14,MHCOUNT                                         0195 00199000
*     END;                                                         0196 00200000
*                                                                  0196 00201000
*   /*****************************************************************/ 00202000
*   /*                                                               */ 00203000
*   /* IF # MH TABLE ENTRIES = 0 THEN                                */ 00204000
*   /*                                                               */ 00205000
*   /*****************************************************************/ 00206000
*                                                                  0197 00207000
*   IF RNMH=0                       /* # MH = 0 ?                    */ 00208000
*     THEN                                                         0197 00209000
@RF00193 LTR   RNMH,RNMH                                           0197 00210000
         BNZ   @RF00197                                            0197 00211000
*     DO;                           /* YES                           */ 00212000
*                                                                  0198 00213000
*       /*************************************************************/ 00214000
*       /*                                                           */ 00215000
*       /* INDEX LAST NAT ENTRY = INDEX LAST TNT NAT ENTRY.          */ 00216000
*       /*                                                           */ 00217000
