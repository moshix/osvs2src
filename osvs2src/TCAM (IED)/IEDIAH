*           ELSE                                                   0560 00775000
*             ;                     /* PROBLEM FOUND,GET OUT @ZA03102*/ 00776000
@RF00550 DS    0H                                                  0561 00777000
*         END;                      /* END DO GROUP          @ZA03102*/ 00778000
*       ELSE                                                       0562 00779000
*         ;                         /* NOT 3270 STATION      @ZA03102*/ 00780000
@RF00543 DS    0H                                                  0563 00781000
*     END;                          /* END DO GROUP          @ZA03102*/ 00782000
*   ELSE                                                           0564 00783000
*     ;                             /* PROBLEM FOUND,GET OUT @ZA03102*/ 00784000
@RF00541 DS    0H                                                  0565 00785000
*   REG14=R14SAVE2;                 /* RESTORE RETURN ADDRESS@ZA03102*/ 00786000
@RC00541 L     REG14,R14SAVE2                                      0565 00787000
*   END FINDATA;                    /* RETURN TO CALLER      @ZA03102*/ 00788000
@EL00003 DS    0H                                                  0566 00789000
@EF00003 DS    0H                                                  0566 00790000
@ER00003 BR    @14                                                 0566 00791000
*                                                                  0567 00792000
*   /*****************************************************************/ 00793000
*   /*                                                               */ 00794000
*   /* THIS SUBROUTINE SETS SCAN POINTER TO SUBSEQUENT UNIT AND      */ 00795000
*   /* ADJUSTS UNIT COUNT WHEN END OF BUFFER UNIT IS REACHED. IF     */ 00796000
*   /* BUFFER TIC FIELD DOES NOT CONTAIN A UNIT ADDRESS FOR LINKAGE, */ 00797000
*   /* A RETURN CODE OF 16 IS SET.                           @ZA03102*/ 00798000
*   /*                                                               */ 00799000
*   /*****************************************************************/ 00800000
*                                                                  0567 00801000
*LINKUNIT:                                                         0567 00802000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/*                       @ZA03102*/ 00803000
LINKUNIT DS    0H                                                  0568 00804000
*   IF PRFTI0=TIC2                  /* TIC FIELD SET TO 2?   @ZA03102*/ 00805000
*     THEN                                                         0568 00806000
         L     @10,PRFPTR                                          0568 00807000
         CLC   PRFTI0(3,@10),@CF00273+1                            0568 00808000
         BNE   @RF00568                                            0568 00809000
*     REG15=AVTHA16;                /* YES,SET BAD RETURN CODE     0569 00810000
*                                                            @ZA03102*/ 00811000
         LH    REG15,AVTHA16(,RAVT)                                0569 00812000
*   ELSE                                                           0570 00813000
*     DO;                           /* NO,LINK UNITS         @ZA03102*/ 00814000
         B     @RC00568                                            0570 00815000
@RF00568 DS    0H                                                  0571 00816000
*       PRFPTR=PRFTI0;              /* GET NEXT UNIT ADDRESS @ZA03102*/ 00817000
         L     @10,PRFPTR                                          0571 00818000
         L     @10,PRFTI0-1(,@10)                                  0571 00819000
         LA    @10,0(,@10)                                         0571 00820000
         ST    @10,PRFPTR                                          0571 00821000
*       UNITCNT=UNITCNT-AVTKEYLE;   /* ADJUST UNIT COUNT FOR NEW UNIT   00822000
*                                                            @ZA03102*/ 00823000
         SH    UNITCNT,AVTKEYLE(,RAVT)                             0572 00824000
*       SCANPTR=ADDR(PRFSUNIT)+UNITCNT;/* POINT TO FIRST BYTE TO BE     00825000
*                                      SCANNED IN NEW UNIT   @ZA03102*/ 00826000
         LA    SCANPTR,PRFSUNIT(,@10)                              0573 00827000
         ALR   SCANPTR,UNITCNT                                     0573 00828000
*     END;                          /* END DO GROUP          @ZA03102*/ 00829000
*   END LINKUNIT;                   /* RETURN TO CALLER      @ZA03102*/ 00830000
@EL00004 DS    0H                                                  0575 00831000
@EF00004 DS    0H                                                  0575 00832000
@ER00004 BR    @14                                                 0575 00833000
