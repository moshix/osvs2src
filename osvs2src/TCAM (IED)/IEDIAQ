*       CALL IEDQGA02;              /* CALL BUFFER STEAL MODULE      */ 01948000
         L     @15,AVTSTEAL(,RAVT)                              02S0977 01949000
         BALR  @14,@15                                          02S0977 01950000
*       IF RBRANCH^=0 THEN          /* TEST FOR BUFFER AVAILABLE     */ 01951000
         LTR   RBRANCH,RBRANCH                                  02S0978 01952000
         BZ    @RF00978                                         02S0978 01953000
*         DO;                       /* BUFFER WAS AVAILABLE          */ 01954000
*           RPRF=RBRANCH;           /* SAVE BUFFER ADDRESS           */ 01955000
         LR    RPRF,RBRANCH                                     02S0980 01956000
*           CALL BLDRESET;          /* YES, ISSUE RESET              */ 01957000
         BAL   @14,BLDRESET                                     02S0981 01958000
*           TRMASNXI=ON;            /* INDICATE RESET IN PROGRESS    */ 01959000
         OI    TRMASNXI(RTRM),B'00010000'                       02S0982 01960000
*         END;                                                  02S0983 01961000
*       ELSE                        /* BUFFER NOT AVAILABLE          */ 01962000
*         CALL CALLIAP;             /* REQUEST A PLCB                */ 01963000
         B     @RC00978                                         02S0984 01964000
@RF00978 BAL   @14,CALLIAP                                      02S0984 01965000
*     END;                                                      02S0985 01966000
*   RFY                                                         02S0986 01967000
