*   /*****************************************************************/ 01923000
*   /*                                                               */ 01924000
*   /* RESETRTN: IF A RESET IS NOT ALREADY IN PROGRESS THEN GET A    */ 01925000
*   /* BUFFER FROM BUFFER STEAL AND REQUEST THAT A RESET PIU BE SENT.*/ 01926000
*   /* IF A BUFFER IS NOT AVAILABLE, THEN CALL SUBROUTINE CALLIAP.   */ 01927000
*   /*                                                               */ 01928000
*   /*****************************************************************/ 01929000
*                                                               02S0971 01930000
*RESETRTN:                                                      02S0971 01931000
*   PROC OPTIONS(RETREG,SAVE(14));                              02S0971 01932000
RESETRTN ST    @14,@SA00007                                     02S0971 01933000
*   RFY                                                         02S0972 01934000
*    (RUNIT,                                                    02S0972 01935000
*     RBRANCH) RSTD;                                            02S0972 01936000
*   IF TRMASNXI=OFF THEN            /* IF RESET NOT ALREADY IN  02S0973 01937000
*                                      PROGRESS ...                  */ 01938000
         TM    TRMASNXI(RTRM),B'00010000'                       02S0973 01939000
         BNZ   @RF00973                                         02S0973 01940000
*     DO;                           /* ... BUILD AND SEND RESET      */ 01941000
*       RUNIT=BFRSUNIT;             /* SET NUMBER OF UNITS PER BUFFER*/ 01942000
         LA    @10,1                                            02S0975 01943000
         LR    RUNIT,@10                                        02S0975 01944000
*       RPARM=BFRSBFRS;             /* SET NUMBER OF BUFFERS    02S0976 01945000
*                                      REQUESTED                     */ 01946000
