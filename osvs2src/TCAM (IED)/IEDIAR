*         CPYCSI='0'B;              /* TURN OFF COPY SENSE           */ 00308000
         NI    CPYCSI(IEDQAPTR),B'11110111'                        0197 00309000
*         MSKCSI='0'B;              /* TURN OFF MASK                 */ 00310000
         NI    MSKCSI,B'11110111'                                  0198 00311000
