*/********************************************************************/ 00396000
*                                                                  0221 00397000
*   IF PLCHGDIR='10'B THEN          /* CHNGDIR SPECIFIED PLUS        */ 00398000
@RF00216 DS    0H                                                  0221 00399000
@RC00214 TM    PLCHGDIR(RPARM),B'00000010'                         0221 00400000
         BNO   @RF00221                                            0221 00401000
         TM    PLCHGDIR(RPARM),B'00000001'                         0221 00402000
         BNZ   @RF00221                                            0221 00403000
*     DUMCDIR='1'B;                 /* SET DUMRH CHGDIR ON           */ 00404000
         OI    DUMCDIR(IEDQAPTR),B'00100000'                       0222 00405000
*   ELSE                                                           0223 00406000
*     IF PLCHGDIR='00'B THEN        /* CHNGDIR NOT SPECIFIED         */ 00407000
         B     @RC00221                                            0223 00408000
@RF00221 TM    PLCHGDIR(RPARM),B'00000011'                         0223 00409000
         BNZ   @RF00223                                            0223 00410000
*       DO;                                                        0224 00411000
*         CPYCDIR='0'B;             /* SET COPY FORMAT OFF           */ 00412000
         NI    CPYCDIR(IEDQAPTR),B'11011111'                       0225 00413000
