         EJECT                                                          01180000
*DEQUEQCB:                                                         0865 01181000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0865 01182000
DEQUEQCB DS    0H                                                  0866 01183000
*   RFY                                                            0866 01184000
*     R14 RSTD;                                                    0866 01185000
*                                                                  0866 01186000
*/********************************************************************/ 01187000
*/*                                                                  */ 01188000
*/*      D E Q U E Q C B   SUBROUTINE                                */ 01189000
*/*                                                                  */ 01190000
*/********************************************************************/ 01191000
*/*                                                                  */ 01192000
*/*      THIS SUBROUTINE SEARCHES THE WAITING-FOR-BUFFER CHAIN       */ 01193000
*/*      (SAVTLUSB) TO SEE IF THE DESTINATION QCB FOR THE CURRENT    */ 01194000
*/*      SLU IS THERE.  IF THE QCB IS FOUND IT IS DEQUEUED.          */ 01195000
*/*                                                                  */ 01196000
*/*      THIS FUNCTION PREVENTS TIMING PROBLEMS WHEN A CINIT FOR     */ 01197000
*/*      AN EXTERNALLY-REQUESTED SESSION COMES IN WHILE TCAM IS      */ 01198000
*/*      PREPARING TO REQUEST A SESSION WITH THE SAME SLU.           */ 01199000
*/*                                                                  */ 01200000
*/********************************************************************/ 01201000
*                                                                  0867 01202000
*   PREVIOUS=ADDR(SAVTLUSB)-LINKOFFS;/* INITIALIZE FOR               */ 01203000
         L     @15,AVTSAVTP(,RAVT)                                 0867 01204000
         LA    @09,SAVTLUSB(,@15)                                  0867 01205000
         SLR   @07,@07                                             0867 01206000
         IC    @07,LINKOFFS                                        0867 01207000
         SLR   @09,@07                                             0867 01208000
         ST    @09,PREVIOUS                                        0867 01209000
*   CURRENT=SAVTLUSB;               /* ... SEARCH                    */ 01210000
         L     @15,SAVTLUSB(,@15)                                  0868 01211000
         ST    @15,CURRENT                                         0868 01212000
*   FOUNDSW=OFF;                    /* ... LOOP                      */ 01213000
         NI    FOUNDSW,B'10111111'                                 0869 01214000
*   DO WHILE(FOUNDSW=OFF&CURRENT>0);                               0870 01215000
         B     @DE00870                                            0870 01216000
@DL00870 DS    0H                                                  0871 01217000
*     IF CURRENT=QCBPTR THEN        /* IS THIS THE QCB?              */ 01218000
         L     @15,CURRENT                                         0871 01219000
         C     @15,QCBPTR                                          0871 01220000
         BNE   @RF00871                                            0871 01221000
*       DO;                         /* YES, DELINK IT & EXIT LOOP    */ 01222000
*         PREVIOUS->QCBLINK=CURRENT->QCBLINK;                      0873 01223000
         L     @09,PREVIOUS                                        0873 01224000
         MVC   QCBLINK(3,@09),QCBLINK(@15)                         0873 01225000
*         FOUNDSW=ON;                                              0874 01226000
         OI    FOUNDSW,B'01000000'                                 0874 01227000
*       END;                                                       0875 01228000
*     ELSE                                                         0876 01229000
*       DO;                         /* NO, BUMP TO NEXT ELEMENT      */ 01230000
         B     @RC00871                                            0876 01231000
@RF00871 DS    0H                                                  0877 01232000
*         PREVIOUS=CURRENT;                                        0877 01233000
         L     @15,CURRENT                                         0877 01234000
         ST    @15,PREVIOUS                                        0877 01235000
*         CURRENT=PREVIOUS->QCBLINK;                               0878 01236000
         L     @15,QCBLINK-1(,@15)                                 0878 01237000
         LA    @15,0(,@15)                                         0878 01238000
         ST    @15,CURRENT                                         0878 01239000
*       END;                                                       0879 01240000
*   END;                                                           0880 01241000
@RC00871 DS    0H                                                  0880 01242000
@DE00870 TM    FOUNDSW,B'01000000'                                 0880 01243000
         BNZ   @DC00870                                            0880 01244000
         L     @15,CURRENT                                         0880 01245000
         LTR   @15,@15                                             0880 01246000
         BP    @DL00870                                            0880 01247000
@DC00870 DS    0H                                                  0881 01248000
*   END DEQUEQCB;                   /* RETURN TO CALLER              */ 01249000
@EL00015 DS    0H                                                  0881 01250000
@EF00015 DS    0H                                                  0881 01251000
@ER00015 BR    @14                                                 0881 01252000
*   RFY                                                            0882 01253000
*     R14 UNRSTD;                                                  0882 01254000
         EJECT                                                          01255000
*DFRDTERM:                                                         0883 01256000
*   PROC OPTIONS(SAVE(R14));                                       0883 01257000
*                                                                  0883 01258000
DFRDTERM ST    @14,@SA00016                                        0883 01259000
*/********************************************************************/ 01260000
*/*                                                                  */ 01261000
*/*      D F R D T E R M   SUBROUTINE                                */ 01262000
*/*                                                                  */ 01263000
*/********************************************************************/ 01264000
*/*                                                                  */ 01265000
*/*      THIS SUBROUTINE DOES DEFERRED PROCESSING OF SESSION-        */ 01266000
*/*      TERMINATION REQUESTS THAT CAME IN WHILE SESSION-            */ 01267000
*/*      INITIATION WAS STILL IN PROGRESS.                           */ 01268000
*/*                                                                  */ 01269000
*/********************************************************************/ 01270000
*                                                                  0884 01271000
*   IF SIBRQEND=ON|                 /* SHOULD SESSION BE ENDED?      */ 01272000
*       SIBTERM=ON THEN                                            0884 01273000
         L     @09,SIBPTR                                          0884 01274000
         TM    SIBRQEND(@09),B'01000000'                           0884 01275000
         BO    @RT00884                                            0884 01276000
         TM    SIBTERM(@09),B'01000000'                            0884 01277000
         BNO   @RF00884                                            0884 01278000
@RT00884 DS    0H                                                  0885 01279000
*     SIBLGOFF=ON;                  /* YES, SET LOGOFF BIT           */ 01280000
         L     @09,SIBPTR                                          0885 01281000
         OI    SIBLGOFF(@09),B'10000000'                           0885 01282000
*   ELSE                                                           0886 01283000
*     IF SIBRQSHT=ON THEN           /* SHOULD SESSION BE SHUT DOWN?  */ 01284000
         B     @RC00884                                            0886 01285000
@RF00884 L     @09,SIBPTR                                          0886 01286000
         TM    SIBRQSHT(@09),B'10000000'                           0886 01287000
         BNO   @RF00886                                            0886 01288000
*       DO;                         /* YES, FIND & RE-PROCESS THE  0887 01289000
*                                      SHUTDOWN REQUEST FOR THIS SLU */ 01290000
*         PREVIOUS=ADDR(SAVTLUSR)-LINKOFFS;/* INITIALIZE FOR         */ 01291000
         L     @09,AVTSAVTP(,RAVT)                                 0888 01292000
         LA    @07,SAVTLUSR(,@09)                                  0888 01293000
         SLR   @05,@05                                             0888 01294000
         IC    @05,LINKOFFS                                        0888 01295000
         SLR   @07,@05                                             0888 01296000
         ST    @07,PREVIOUS                                        0888 01297000
*         CURRENT=SAVTLUSR;         /* ... SEARCH                    */ 01298000
         L     @09,SAVTLUSR(,@09)                                  0889 01299000
         ST    @09,CURRENT                                         0889 01300000
*         FOUNDSW=OFF;              /* ... LOOP                      */ 01301000
         NI    FOUNDSW,B'10111111'                                 0890 01302000
*         DO UNTIL(FOUNDSW=ON);     /* FIND THE ENQUEUED ELEMENT     */ 01303000
@DL00891 DS    0H                                                  0892 01304000
*           IF CURRENT->REQLTNT=TNTSLU&CURRENT->REQTYPE=REQLSHTD THEN   01305000
         L     @09,CURRENT                                         0892 01306000
         CLC   REQLTNT(2,@09),TNTSLU                               0892 01307000
         BNE   @RF00892                                            0892 01308000
         CLI   REQTYPE(@09),X'02'                                  0892 01309000
         BNE   @RF00892                                            0892 01310000
*             FOUNDSW=ON;           /* ELEMENT FOUND                 */ 01311000
         OI    FOUNDSW,B'01000000'                                 0893 01312000
*           ELSE                                                   0894 01313000
*             DO;                   /* KEEP LOOKING                  */ 01314000
         B     @RC00892                                            0894 01315000
@RF00892 DS    0H                                                  0895 01316000
*               PREVIOUS=CURRENT;                                  0895 01317000
         L     @09,CURRENT                                         0895 01318000
         ST    @09,PREVIOUS                                        0895 01319000
*               CURRENT=PREVIOUS->REQLINK;                         0896 01320000
         L     @09,REQLINK-1(,@09)                                 0896 01321000
         LA    @09,0(,@09)                                         0896 01322000
         ST    @09,CURRENT                                         0896 01323000
*             END;                                                 0897 01324000
*         END;                                                     0898 01325000
@RC00892 DS    0H                                                  0898 01326000
@DE00891 TM    FOUNDSW,B'01000000'                                 0898 01327000
         BNO   @DL00891                                            0898 01328000
*                                   /* DELINK THE REQUEST ELEMENT    */ 01329000
*         PREVIOUS->REQLINK=CURRENT->REQLINK;                      0899 01330000
         L     @09,CURRENT                                         0899 01331000
         L     @07,PREVIOUS                                        0899 01332000
         MVC   REQLINK(3,@07),REQLINK(@09)                         0899 01333000
*         RPARM=CURRENT;            /* GET ELEMENT ADDRESS           */ 01334000
         LR    RPARM,@09                                           0900 01335000
*         SIBRQSHT=OFF;             /* RESET ELEMENT-ENQUEUED BIT    */ 01336000
         L     @09,SIBPTR                                          0901 01337000
         NI    SIBRQSHT(@09),B'01111111'                           0901 01338000
*         CALL DACTRTN;             /* RE-PROCESS THE REQUEST ELEMENT*/ 01339000
         BAL   @14,DACTRTN                                         0902 01340000
*       END;                                                       0903 01341000
*   END DFRDTERM;                   /* RETURN TO CALLER              */ 01342000
@EL00016 DS    0H                                                  0904 01343000
@EF00016 DS    0H                                                  0904 01344000
@ER00016 L     @14,@SA00016                                        0904 01345000
         BR    @14                                                 0904 01346000
         EJECT                                                          01347000
*ENDLGOFF:                                                         0905 01348000
*   PROC OPTIONS(SAVE(R14));                                       0905 01349000
*                                                                  0905 01350000
ENDLGOFF ST    @14,@SA00017                                        0905 01351000
*/********************************************************************/ 01352000
*/*                                                                  */ 01353000
*/*      E N D L G O F F   SUBROUTINE                                */ 01354000
*/*                                                                  */ 01355000
*/********************************************************************/ 01356000
*/*                                                                  */ 01357000
*/*      THIS SUBROUTINE COMPLETES NORMAL TCAM-TASK SESSION  @YM08450*/ 01358000
*/*      TERMINATION PROCESSING FOR AN SLU AFTER A CLEAR     @YM08450*/ 01359000
*/*      RESPONSE PIU IS RECEIVED FROM IEDSCLU.              @YM08450*/ 01360000
*/*                                                                  */ 01361000
*/********************************************************************/ 01362000
*                                                                  0906 01363000
*                                   /* SEND SESSION-ENDED PIU TO SSCP*/ 01364000
*   RPARM=CD1XSND;                  /* SESSION-ENDED REQUEST CODE    */ 01365000
         L     RPARM,@CF00751                                      0906 01366000
*   CALL BLDMULTI;                  /* BUILD PIU & TPOST IT TO SSCP  */ 01367000
         BAL   @14,BLDMULTI                                        0907 01368000
*   CALL SECLNUP;                   /* DO END-OF-SESSION CTL BLOCK 0908 01369000
*                                      CLEAN-UP              @YM08450*/ 01370000
         BAL   @14,SECLNUP                                         0908 01371000
*   END ENDLGOFF;                   /* RETURN TO CALLER              */ 01372000
@EL00017 DS    0H                                                  0909 01373000
@EF00017 DS    0H                                                  0909 01374000
@ER00017 L     @14,@SA00017                                        0909 01375000
         BR    @14                                                 0909 01376000
         EJECT                                                          01377000
*ENDLGON:                                                          0910 01378000
*   PROC OPTIONS(SAVE(R14));                                       0910 01379000
*                                                                  0910 01380000
ENDLGON  ST    @14,@SA00018                                        0910 01381000
*/********************************************************************/ 01382000
*/*                                                                  */ 01383000
*/*      E N D L G O N   SUBROUTINE                                  */ 01384000
*/*                                                                  */ 01385000
*/********************************************************************/ 01386000
*/*                                                                  */ 01387000
*/*      THIS SUBROUTINE IS CALLED FROM PIURTN WHEN A        @YM08450*/ 01388000
*/*      BIND OR SDT RESPONSE PIU IS RECEIVED FROM IEDSCLU.          */ 01389000
*/*                                                                  */ 01390000
*/*      IF THE RESPONSE IS GOOD:                                    */ 01391000
*/*      .  A SESSION-STARTED PIU IS BUILT AND TPOSTED TO THE SSCP.  */ 01392000
*/*      .  NORMAL SCHEDULING IS BEGUN BY EITHER TPOSTING A LOGON    */ 01393000
*/*         DATA BUFFER TO THE INPUT MH, OR TPOSTING THE PLCB TO     */ 01394000
*/*         ITSELF TO PRIME THE SCHEDULERS.                          */ 01395000
*/*      .  LOGOFF PROCESSING IS STARTED IF THERE WAS A PREVIOUSLY   */ 01396000
*/*         PROCESSED CTERM PIU OR DEACTIVATE REQUEST.               */ 01397000
*/*                                                                  */ 01398000
*/*      IF THE RESPONSE SHOWS AN ERROR:                             */ 01399000
*/*      .  ERROR PROCESSING THROUGH THE MH IS BEGUN BY TPOSTING     */ 01400000
*/*         AN ERROR BUFFER TO THE INPUT MH OR BY TPOSTING THE       */ 01401000
*/*         PLCB TO ITSELF TO DRIVE SCHEDULING OF A SEND OPERATION.  */ 01402000
*/*                                                                  */ 01403000
*/********************************************************************/ 01404000
*                                                                  0911 01405000
*   IF TRHSDI=OFF THEN              /* GOOD RESPONSE?                */ 01406000
         TM    TRHSDI+10(RPIU),B'00000100'                         0911 01407000
         BNZ   @RF00911                                            0911 01408000
*     DO;                           /* YES                           */ 01409000
*       RPARM=CD1XSST;              /* SESSION-STARTED REQUEST CODE  */ 01410000
         L     RPARM,@CF00761                                      0913 01411000
*       CALL BLDMULTI;              /* BLD & TPOST SESSION-STARTD PIU*/ 01412000
         BAL   @14,BLDMULTI                                        0914 01413000
*       IF LCBWAITC>0&              /* IS THERE LOGON DATA SAVED?    */ 01414000
*           LCBWAITC->PRFPRI='FF'X THEN                            0915 01415000
         L     @09,LCBWAITC-1(,RLCB)                               0915 01416000
         LA    @09,0(,@09)                                         0915 01417000
         LTR   @09,@09                                             0915 01418000
         BNP   @RF00915                                            0915 01419000
         CLI   PRFPRI(@09),X'FF'                                   0915 01420000
         BNE   @RF00915                                            0915 01421000
*         DO;                       /* YES, TPOST IT TO MH           */ 01422000
*           RPRF=LCBWAITC;          /* LOGON DATA'S BUFFER ADDRESS   */ 01423000
         LR    RPRF,@09                                            0917 01424000
*           LCBWAITC=PRFLINK;       /* DELINK IT FROM WAIT CHAIN     */ 01425000
         MVC   LCBWAITC(3,RLCB),PRFLINK(RPRF)                      0918 01426000
*           PRFPRI=PRIMHBFR;        /* SET NO-ERROR PRIORITY         */ 01427000
         MVI   PRFPRI(RPRF),X'E4'                                  0919 01428000
*           CALL MHBUF;             /* FINISH BUILDING BFR & TPOST IT*/ 01429000
         BAL   @14,MHBUF                                           0920 01430000
*         END;                                                     0921 01431000
*       ELSE                        /* NO LOGON DATA                 */ 01432000
*         DO;                       /* TPOST PLCB TO ITSELF          */ 01433000
         B     @RC00915                                            0922 01434000
@RF00915 DS    0H                                                  0923 01435000
*           LCBQCBA=RLCB;           /* QCB ADDRESS                   */ 01436000
         STCM  RLCB,7,LCBQCBA(RLCB)                                0923 01437000
*           LCBPRI=PRILNFRE;        /* PRIORITY                      */ 01438000
         MVI   LCBPRI(RLCB),X'E0'                                  0924 01439000
*           RPARM=RLCB;             /* ELEMENT ADDRESS               */ 01440000
         LR    RPARM,RLCB                                          0925 01441000
*           CALL DSPPOSTR;          /* TPOST THE PLCB                */ 01442000
         LA    @15,DSPPOSTR(,RDISP)                                0926 01443000
         BALR  @14,@15                                             0926 01444000
*         END;                                                     0927 01445000
*       IF SIBTERM=ON|              /* ANY PREVIOUSLY-RECEIVED       */ 01446000
*           SIBRQEND=ON|            /* ... SESSION-TERMINATION       */ 01447000
*           SIBRQSHT=ON THEN        /* ... REQUESTS WAITING?         */ 01448000
@RC00915 L     @09,SIBPTR                                          0928 01449000
         TM    SIBTERM(@09),B'01000000'                            0928 01450000
         BO    @RT00928                                            0928 01451000
         TM    SIBRQEND(@09),B'11000000'                           0928 01452000
         BZ    @RF00928                                            0928 01453000
@RT00928 DS    0H                                                  0929 01454000
*         CALL DFRDTERM;            /* YES, GO PROCESS THEM          */ 01455000
         BAL   @14,DFRDTERM                                        0929 01456000
*     END;                                                         0930 01457000
         EJECT                                                          01458000
*   ELSE                            /* BAD RESPONSE                  */ 01459000
*     CALL ERRORPST;                /* CAUSE ERROR BUFFER TPOST TO 0931 01460000
*                                      USER'S MH             @YM08450*/ 01461000
         B     @RC00911                                            0931 01462000
@RF00911 BAL   @14,ERRORPST                                        0931 01463000
*   END ENDLGON;                    /* RETURN TO CALLER              */ 01464000
@EL00018 DS    0H                                                  0932 01465000
@EF00018 DS    0H                                                  0932 01466000
@ER00018 L     @14,@SA00018                                        0932 01467000
         BR    @14                                                 0932 01468000
         EJECT                                                          01469000
*ENDNOSIB:                                                         0933 01470000
*   PROC OPTIONS(SAVE(R14));                                       0933 01471000
*                                                                  0933 01472000
ENDNOSIB ST    @14,@SA00019                                        0933 01473000
*/********************************************************************/ 01474000
*/*                                                                  */ 01475000
*/*      E N D N O S I B   SUBROUTINE                                */ 01476000
*/*                                                                  */ 01477000
*/********************************************************************/ 01478000
*/*                                                                  */ 01479000
*/*      THIS SUBROUTINE FINISHES THE TCAM ERROR PROCESSING FOR      */ 01480000
*/*      A SESSION-INITIATION ATTEMPT WHICH FAILED BECAUSE THE       */ 01481000
*/*      SSCP COULD NOT GET A SIB.                           @YM08450*/ 01482000
*/*                                                                  */ 01483000
*/********************************************************************/ 01484000
*                                                                  0934 01485000
*   TRMINERR=OFF;                   /* INITIATION-ERROR BIT. TURNING    01486000
*                                      OFF THIS BIT ALLOWS IGCA210D     01487000
*                                      TO PROCESS NEW SESSION-     0934 01488000
*                                      INITIATION REQUESTS   @YM08450*/ 01489000
         LA    @05,TRMINERR(,RTRM)                                 0934 01490000
         LR    @03,@05                                             0934 01491000
         LA    @09,3                                               0934 01492000
         NR    @03,@09                                             0934 01493000
         LCR   @03,@03                                             0934 01494000
         ALR   @05,@03                                             0934 01495000
         L     @09,0(,@05)                                         0934 01496000
@GL00015 LR    @07,@09                                             0934 01497000
         N     @07,@CB03391+3(@03)                                 0934 01498000
         CS    @09,@07,0(@05)                                      0934 01499000
         BNE   @GL00015                                            0934 01500000
*   CALL SECLNUP;                   /* DO END-OF-SESSION CTL BLOCK 0935 01501000
*                                      CLEAN-UP              @YM08450*/ 01502000
         BAL   @14,SECLNUP                                         0935 01503000
*   END ENDNOSIB;                   /* RETURN TO CALLER              */ 01504000
@EL00019 DS    0H                                                  0936 01505000
@EF00019 DS    0H                                                  0936 01506000
@ER00019 L     @14,@SA00019                                        0936 01507000
         BR    @14                                                 0936 01508000
         EJECT                                                          01509000
*ERBRTN:                                                           0937 01510000
*   PROC OPTIONS(SAVE(R14));                                       0937 01511000
*                                                                  0937 01512000
ERBRTN   ST    @14,@SA00020                                        0937 01513000
*/********************************************************************/ 01514000
*/*                                                                  */ 01515000
*/*      E R B R T N   SUBROUTINE                                    */ 01516000
*/*                                                                  */ 01517000
*/********************************************************************/ 01518000
*/*                                                                  */ 01519000
*/*      THIS SUBROUTINE PROCESSES ERBS TPOSTED TO THIS MODULE       */ 01520000
*/*      WITH EMPTY BUFFERS ATTACHED.  SOME SET-UP COMMON TO BOTH    */ 01521000
*/*      TYPES OF ERB (INTERNAL AND EXTERNAL) IS DONE.  THE          */ 01522000
*/*      SPECIFIC ERB TYPE IS THEN IDENTIFIED, AND THE PROPER        */ 01523000
*/*      SUBROUTINE IS CALLED TO PROCESS IT.                         */ 01524000
*/*                                                                  */ 01525000
*/********************************************************************/ 01526000
*                                                                  0938 01527000
*   RLCB=RPARM-LCBERBFS;            /* BACK UP TO START OF PLCB      */ 01528000
         LR    RLCB,RPARM                                          0938 01529000
         SL    RLCB,@CF01909                                       0938 01530000
*   IF RPARM=ADDR(LUSERB) THEN      /* DETERMINE ERB TYPE            */ 01531000
         LA    @09,LUSERB                                          0939 01532000
         CR    RPARM,@09                                           0939 01533000
         BNE   @RF00939                                            0939 01534000
*     CALL ERBRTNI;                 /* INTERNAL ERB                  */ 01535000
         BAL   @14,ERBRTNI                                         0940 01536000
*   ELSE                                                           0941 01537000
*     CALL ERBRTNX;                 /* EXTERNAL (WITHIN A PLCB) ERB  */ 01538000
         B     @RC00939                                            0941 01539000
@RF00939 BAL   @14,ERBRTNX                                         0941 01540000
*   END ERBRTN;                     /* RETURN TO CALLER              */ 01541000
@EL00020 DS    0H                                                  0942 01542000
@EF00020 DS    0H                                                  0942 01543000
@ER00020 L     @14,@SA00020                                        0942 01544000
         BR    @14                                                 0942 01545000
         EJECT                                                          01546000
*ERBRTNI:                                                          0943 01547000
*   PROC OPTIONS(SAVE(R14));                                       0943 01548000
*                                                                  0943 01549000
ERBRTNI  ST    @14,@SA00021                                        0943 01550000
*/********************************************************************/ 01551000
*/*                                                                  */ 01552000
*/*      E R B R T N I   SUBROUTINE                                  */ 01553000
*/*                                                                  */ 01554000
*/********************************************************************/ 01555000
*/*                                                                  */ 01556000
*/*      THIS SUBROUTINE PROCESSES THE INTERNAL ERB, HARD-CODED      */ 01557000
*/*      WITHIN THIS MODULE, WHEN IT RETURNS FROM IEDQGA WITH AN     */ 01558000
*/*      EMPTY BUFFER.  THREE PROCESSING STEPS ARE INVOLVED:         */ 01559000
*/*                                                                  */ 01560000
*/*      .  THE EMPTY BUFFER IS REMOVED FROM THE ERB AND ITS         */ 01561000
*/*         ADDRESS STORED IN A LOCAL POINTER.                       */ 01562000
*/*                                                                  */ 01563000
*/*      .  THE ELEMENT WHICH WAS WAITING FOR THE BUFFER IS          */ 01564000
*/*         REPROCESSED THROUGH THE SAME SUBROUTINE WHICH ISSUED     */ 01565000
*/*         THE ORIGINAL BUFFER REQUEST.                             */ 01566000
*/*                                                                  */ 01567000
*/*      .  THE FIRST ELEMENT WAITING IN LINE FOR THE ERB TO BECOME  */ 01568000
*/*         AVAILABLE IS REMOVED FROM THE CHAIN, AND THE ERB IS      */ 01569000
*/*         TPOSTED TO IEDQGA TO HONOR THAT NEW ELEMENT'S REQUEST.   */ 01570000
*/*                                                                  */ 01571000
*/********************************************************************/ 01572000
*                                                                  0944 01573000
*   BUFPTR=LCBERBCH;                /* SAVE BUFFER ADDR IN LOCAL PTR */ 01574000
         L     @09,LCBERBCH-1(,RLCB)                               0944 01575000
         LA    @09,0(,@09)                                         0944 01576000
         ST    @09,BUFPTR                                          0944 01577000
*   LCBERBCH=0;                     /* CLEAR ERB'S BUFFER PTR        */ 01578000
         SLR   @09,@09                                             0945 01579000
         STCM  @09,7,LCBERBCH(RLCB)                                0945 01580000
*   RPARM=LCBBFRSV;                 /* ADDR OF ELEMENT NEEDING BUFFER*/ 01581000
         L     RPARM,LCBBFRSV-1(,RLCB)                             0946 01582000
         LA    RPARM,0(,RPARM)                                     0946 01583000
*                                   /* IDENTIFY THE ELEMENT TYPE & 0947 01584000
*                                      CALL PROPER SUBROUTINE        */ 01585000
*   IF RECBPRI=PRILUSQB THEN                                       0947 01586000
         CLI   RECBPRI(RPARM),226                                  0947 01587000
         BNE   @RF00947                                            0947 01588000
*     CALL QCBRTN;                  /* DESTINATION QCB               */ 01589000
         BAL   @14,QCBRTN                                          0948 01590000
*   ELSE                                                           0949 01591000
*     CALL DACTRTN;                 /* SHUTDOWN REQUEST FROM DEACT   */ 01592000
         B     @RC00947                                            0949 01593000
@RF00947 BAL   @14,DACTRTN                                         0949 01594000
*   RLCB=ADDR(LUSLCB);              /* ADDR OF LOCALLY DECLARED PLCB */ 01595000
@RC00947 LA    RLCB,LUSLCB                                         0950 01596000
*   LCBDLNKN=ON;                    /* MARK THE ERB AVAILABLE FOR USE*/ 01597000
         OI    LCBDLNKN(RLCB),B'00000001'                          0951 01598000
*   IF SAVTLUSB>0 THEN              /* ANY ELEMENTS WAITING FOR ERB? */ 01599000
         L     @09,AVTSAVTP(,RAVT)                                 0952 01600000
         L     @07,SAVTLUSB(,@09)                                  0952 01601000
         LTR   @07,@07                                             0952 01602000
         BNP   @RF00952                                            0952 01603000
*     DO;                           /* YES                           */ 01604000
*       RPARM=SAVTLUSB;             /* ADDR OF 1ST WAITING ELEMENT   */ 01605000
         LR    RPARM,@07                                           0954 01606000
*       SAVTLUSB=RECBLINK;          /* DELINK IT                     */ 01607000
         L     @07,RECBLINK-1(,RPARM)                              0955 01608000
         LA    @07,0(,@07)                                         0955 01609000
         ST    @07,SAVTLUSB(,@09)                                  0955 01610000
*       R0=RECBUNCT;                /* UNIT COUNT (SAVED IN 1ST WORD    01611000
*                                      OF ELEMENT)                   */ 01612000
         SLR   R0,R0                                               0956 01613000
         IC    R0,RECBUNCT(,RPARM)                                 0956 01614000
*       CALL GETBUFI;               /* REQUEST A BUFFER              */ 01615000
         BAL   @14,GETBUFI                                         0957 01616000
*     END;                                                         0958 01617000
*   END ERBRTNI;                    /* RETURN TO CALLER              */ 01618000
@EL00021 DS    0H                                                  0959 01619000
@EF00021 DS    0H                                                  0959 01620000
@ER00021 L     @14,@SA00021                                        0959 01621000
         BR    @14                                                 0959 01622000
         EJECT                                                          01623000
*ERBRTNX:                                                          0960 01624000
*   PROC OPTIONS(SAVE(R14));                                       0960 01625000
*                                                                  0960 01626000
ERBRTNX  ST    @14,@SA00022                                        0960 01627000
*/********************************************************************/ 01628000
*/*                                                                  */ 01629000
*/*      E R B R T N X   SUBROUTINE                                  */ 01630000
*/*                                                                  */ 01631000
*/********************************************************************/ 01632000
*/*                                                                  */ 01633000
*/*      THIS SUBROUTINE PROCESSES AN EXTERNAL ERB (A NORMAL ERB     */ 01634000
*/*      WITHIN A PLCB) WHEN IT RETURNS FROM IEDQGA WITH AN EMPTY    */ 01635000
*/*      BUFFER.  THE PROCESSING CONSISTS OF REDRIVING THE           */ 01636000
*/*      SUBROUTINE WHICH ORIGINALLY REQUESTED THE BUFFER.  THAT     */ 01637000
*/*      IS DETERMINED BY CHECKING THE CURRENT SESSION STATE.        */ 01638000
*/*                                                                  */ 01639000
*/********************************************************************/ 01640000
*                                                                  0961 01641000
*   TNTSLU=LCBTTCIN;                /* SLU'S TNT INDEX               */ 01642000
         LH    @09,LCBTTCIN(,RLCB)                                 0961 01643000
         N     @09,@CF03378                                        0961 01644000
         STH   @09,TNTSLU                                          0961 01645000
*   CALL CTLBLKS;                   /* SET UP CONTROL BLOCK POINTERS */ 01646000
         BAL   @14,CTLBLKS                                         0962 01647000
*   LCBDLNKN=ON;                    /* MARK ERB AVAILABLE            */ 01648000
         OI    LCBDLNKN(RLCB),B'00000001'                          0963 01649000
*                                   /* DETERMINE PROCESSING BASED ON    01650000
*                                      CURRENT SESSION STATE         */ 01651000
*   IF SIBSSTAT=SIBSINIT THEN       /* SESSION STATE = 'INITIAL'?    */ 01652000
         L     @09,SIBPTR                                          0964 01653000
         TM    SIBSSTAT(@09),B'00011100'                           0964 01654000
         BNZ   @RF00964                                            0964 01655000
*     DO;                           /* YES                           */ 01656000
*       RPRF=LCBBFRSV;              /* ADDR OF SAVED CINIT PIU'S BFR */ 01657000
         L     RPRF,LCBBFRSV-1(,RLCB)                              0966 01658000
         LA    RPRF,0(,RPRF)                                       0966 01659000
*       INPUTBUF=RPRF;              /* SAVE ADDRESS IN LOCAL POINTER */ 01660000
         ST    RPRF,INPUTBUF                                       0967 01661000
*       RPIU=ADDR(PRFPIUO)+PRFPIUO; /* START-OF-PIU ADDRESS          */ 01662000
         LA    RPIU,PRFPIUO(,RPRF)                                 0968 01663000
         SLR   @09,@09                                             0968 01664000
         IC    @09,PRFPIUO(,RPRF)                                  0968 01665000
         ALR   RPIU,@09                                            0968 01666000
*       INPUTPIU=RPIU;              /* SAVE ADDRESS IN LOCAL POINTER */ 01667000
         ST    RPIU,INPUTPIU                                       0969 01668000
*       CALL CINITRTN;              /* RESUME CINIT PIU PROCESSING   */ 01669000
         BAL   @14,CINITRTN                                        0970 01670000
*     END;                                                         0971 01671000
*   ELSE                                                           0972 01672000
*     IF SIBSSTAT=SIBSACTV THEN     /* SESSION STATE = 'ACTIVE'?     */ 01673000
         B     @RC00964                                            0972 01674000
@RF00964 L     @09,SIBPTR                                          0972 01675000
         TM    SIBSSTAT(@09),B'00011100'                           0972 01676000
         BNO   @RF00972                                            0972 01677000
*       CALL LGOFF;                 /* YES, RESUME LOGOFF PROCESSING */ 01678000
         BAL   @14,LGOFF                                           0973 01679000
*     ELSE                          /* SESSION STATE = 'RESET'       */ 01680000
*       CALL BINDFAIL;              /* RESUME BIND-FAILED PROCESSING */ 01681000
         B     @RC00972                                            0974 01682000
@RF00972 BAL   @14,BINDFAIL                                        0974 01683000
*   END ERBRTNX;                    /* RETURN TO CALLER              */ 01684000
@EL00022 DS    0H                                                  0975 01685000
@EF00022 DS    0H                                                  0975 01686000
@ER00022 L     @14,@SA00022                                        0975 01687000
         BR    @14                                                 0975 01688000
         EJECT                                                          01689000
*ERRORPST:                                                         0976 01690000
*   PROC OPTIONS(SAVE(R14));        /*                       @YM08450*/ 01691000
ERRORPST ST    @14,@SA00023                                        0976 01692000
*/********************************************************** @YM08450*/ 01693000
*/*                                                          @YM08450*/ 01694000
*/*      E R R O R P S T   SUBROUTINE                        @YM08450*/ 01695000
*/*                                                          @YM08450*/ 01696000
*/********************************************************** @YM08450*/ 01697000
*/*                                                          @YM08450*/ 01698000
*/*      THIS SUBROUTINE DOES THE PROCESSING NEEDED TO       @YM08450*/ 01699000
*/*      NOTIFY THE USER'S MH OF A SESSION-INITIATION        @YM08450*/ 01700000
*/*      ERROR.  THERE ARE TWO POSSIBILITIES:                @YM08450*/ 01701000
*/*                                                          @YM08450*/ 01702000
*/*      1. IF THIS IS A TCAM-INITIATED SESSION AND          @YM08450*/ 01703000
*/*         CONDITIONS ARE SUCH THAT THE SEND SCHEDULER      @YM08450*/ 01704000
*/*         WILL ATTEMPT A SEND OPERATION IF GIVEN CONTROL,  @YM08450*/ 01705000
*/*         THEN WE TPOST THE PLCB TO ITSELF.  THIS WILL     @YM08450*/ 01706000
*/*         ULTIMATELY CAUSE AN ERROR BUFFER TO BE PASSED    @YM08450*/ 01707000
*/*         THRU THE USER'S OUTPUT MH.                       @YM08450*/ 01708000
*/*                                                          @YM08450*/ 01709000
*/*      2. IF THE ABOVE CONDITIONS ARE NOT TRUE, THEN WE    @YM08450*/ 01710000
*/*         TPOST AN ERROR BUFFER TO THE USER'S INPUT MH.    @YM08450*/ 01711000
*/*         A SAVED LOGON-DATA BUFFER IS USED IF AVAILABLE.  @YM08450*/ 01712000
*/*         OTHERWISE, A ZERO-LENGTH BUFFER IS USED.         @YM08450*/ 01713000
*/*                                                          @YM08450*/ 01714000
*/********************************************************** @YM08450*/ 01715000
*                                                                  0977 01716000
*   LCBLUS=ON;                      /* SET BIT TO CAUSE PLCB TO BE 0977 01717000
*                                      RETURNED TO THIS MODULE WHEN     01718000
*                                      MH ERROR PROCESSING ENDS    0977 01719000
*                                                            @YM08450*/ 01720000
         OI    LCBLUS(RLCB),B'00100000'                            0977 01721000
*   LCBBFSNS=PIURU(1:4);            /* SAVE SENSE DATA FROM RESPONSE    01722000
*                                                            @YM08450*/ 01723000
         MVC   LCBBFSNS(4,RLCB),PIURU(RPIU)                        0978 01724000
*   SENDSW=OFF;                     /* INIT SEND SWITCH      @YM08450*/ 01725000
         NI    SENDSW,B'11110111'                                  0979 01726000
*   IF TRMLUREQ=OFF THEN            /* TCAM-INITIATED LOGON? @YM08450*/ 01727000
         TM    TRMLUREQ(RTRM),B'00000100'                          0980 01728000
         BNZ   @RF00980                                            0980 01729000
*     CALL CHKSEND;                 /* YES, SET SENDSW IF MSGS TO BE    01730000
*                                      SENT                  @YM08450*/ 01731000
         BAL   @14,CHKSEND                                         0981 01732000
*   IF SENDSW=ON THEN               /* SHOULD WE TRY A SEND? @YM08450*/ 01733000
@RF00980 TM    SENDSW,B'00001000'                                  0982 01734000
         BNO   @RF00982                                            0982 01735000
*     DO;                           /* YES                   @YM08450*/ 01736000
*       RPARM=RPRF;                 /* FREE THE              @YM08450*/ 01737000
         LR    RPARM,RPRF                                          0984 01738000
*       CALL FREEBUF;               /* ... RESPONSE BUFFER   @YM08450*/ 01739000
         BAL   @14,FREEBUF                                         0985 01740000
*       LCBQCBA=RLCB;               /* TPOST THE PLCB TO     @YM08450*/ 01741000
         STCM  RLCB,7,LCBQCBA(RLCB)                                0986 01742000
*       LCBPRI=PRILNFRE;            /* ... ITSELF TO DRIVE   @YM08450*/ 01743000
         MVI   LCBPRI(RLCB),X'E0'                                  0987 01744000
*       RPARM=RLCB;                 /* ... SEND SCHEDULER    @YM08450*/ 01745000
         LR    RPARM,RLCB                                          0988 01746000
*       CALL DSPPOSTR;              /* ... ROUTINE           @YM08450*/ 01747000
         LA    @15,DSPPOSTR(,RDISP)                                0989 01748000
         BALR  @14,@15                                             0989 01749000
*     END;                          /*                       @YM08450*/ 01750000
*   ELSE                            /* SENDSW IS OFF         @YM08450*/ 01751000
*     DO;                           /* TPOST TO INPUT MH     @YM08450*/ 01752000
         B     @RC00982                                            0991 01753000
@RF00982 DS    0H                                                  0992 01754000
*       IF LCBWAITC>0&              /* LOGON DATA SAVED?     @YM08450*/ 01755000
*           LCBWAITC->PRFPRI='FF'X THEN/*                    @YM08450*/ 01756000
         L     @09,LCBWAITC-1(,RLCB)                               0992 01757000
         LA    @09,0(,@09)                                         0992 01758000
         LTR   @09,@09                                             0992 01759000
         BNP   @RF00992                                            0992 01760000
         CLI   PRFPRI(@09),X'FF'                                   0992 01761000
         BNE   @RF00992                                            0992 01762000
*         DO;                       /* YES                   @YM08450*/ 01763000
*           RPARM=RPRF;             /* FREE THE RESPONSE     @YM08450*/ 01764000
         LR    RPARM,RPRF                                          0994 01765000
*           CALL FREEBUF;           /* ... BUFFER            @YM08450*/ 01766000
         BAL   @14,FREEBUF                                         0995 01767000
*           RPRF=LCBWAITC;          /* DELINK THE LOGON DATA @YM08450*/ 01768000
         L     RPRF,LCBWAITC-1(,RLCB)                              0996 01769000
         LA    RPRF,0(,RPRF)                                       0996 01770000
*           LCBWAITC=PRFLINK;       /* ... FROM WAIT CHAIN   @YM08450*/ 01771000
         MVC   LCBWAITC(3,RLCB),PRFLINK(RPRF)                      0997 01772000
*         END;                      /*                       @YM08450*/ 01773000
*       ELSE                        /* NO LOGON DATA         @YM08450*/ 01774000
*         DO;                       /* INIT 0-LENGTH BUFFER  @YM08450*/ 01775000
         B     @RC00992                                            0999 01776000
@RF00992 DS    0H                                                  1000 01777000
*           PRFNBUNT=1;             /* NUMBER OF UNITS       @YM08450*/ 01778000
         MVI   PRFNBUNT(RPRF),X'01'                                1000 01779000
*           PRFSIZE=0;              /* SIZE                  @YM08450*/ 01780000
         SLR   @09,@09                                             1001 01781000
         STH   @09,PRFSIZE(,RPRF)                                  1001 01782000
*           PRF1FLG1=0;             /* CLEAR FLAG BYTE       @YM08450*/ 01783000
         LR    @09,RPRF                                            1002 01784000
         AL    @09,@CF03396                                        1002 01785000
         MVI   PRF1FLG1-3(@09),X'00'                               1002 01786000
*           PRFDEST=SIBINDEX;       /* DESTINATION = PLU     @YM08450*/ 01787000
         L     @09,SIBPTR                                          1003 01788000
         LH    @09,SIBINDEX(,@09)                                  1003 01789000
         N     @09,@CF03378                                        1003 01790000
         STH   @09,PRFDEST(,RPRF)                                  1003 01791000
*         END;                      /*                       @YM08450*/ 01792000
*       PRFPRI=PRIERBFR;            /* SET ERROR PRIORITY    @YM08450*/ 01793000
@RC00992 MVI   PRFPRI(RPRF),X'E3'                                  1005 01794000
*       SCBSLCTN=ON;                /* SET SELECTION         @OX21004*/ 01795000
*       SCBCONNN=ON;                /* CONNECTION ERRORS     @OX21004*/ 01796000
         L     @09,LCBSCBA-1(,RLCB)                                1007 01797000
         OI    SCBSLCTN(@09),B'10100000'                           1007 01798000
*       LCBLUSNS=LCBBFSNS;          /* SET SENSE             @OX21004*/ 01799000
         MVC   LCBLUSNS(4,RLCB),LCBBFSNS(RLCB)                     1008 01800000
*       CALL MHBUF;                 /* TPOST ERROR BFR TO MH @YM08450*/ 01801000
         BAL   @14,MHBUF                                           1009 01802000
*     END;                          /*                       @YM08450*/ 01803000
*   END ERRORPST;                   /* RETURN TO CALLER      @YM08450*/ 01804000
@EL00023 DS    0H                                                  1011 01805000
@EF00023 DS    0H                                                  1011 01806000
@ER00023 L     @14,@SA00023                                        1011 01807000
         BR    @14                                                 1011 01808000
         EJECT                                                          01809000
*FNDLGDTA:                                                         1012 01810000
*   PROC OPTIONS(SAVE(R14));                                       1012 01811000
*                                                                  1012 01812000
FNDLGDTA ST    @14,@SA00024                                        1012 01813000
*/********************************************************************/ 01814000
*/*                                                                  */ 01815000
*/*      F N D L G D T A   SUBROUTINE                                */ 01816000
*/*                                                                  */ 01817000
*/********************************************************************/ 01818000
*/*                                                                  */ 01819000
*/*      THIS SUBROUTINE LOCATES THE USER-DATA FIELD WITHIN A        */ 01820000
*/*      CINIT PIU AND STORES ITS ADDRESS IN THE LOCAL POINTER,      */ 01821000
*/*      DATAPTR.                                                    */ 01822000
*/*                                                                  */ 01823000
*/*      (NOTE THAT THIS SUBROUTINE USES IEDMVBFR IN AN UNUSUAL      */ 01824000
*/*      WAY BY SETTING THE TARGET AND SOURCE ADDRESSES EQUAL.       */ 01825000
*/*      THIS CAUSES IEDMVBFR TO SKIP THE POINTERS PAST VARIABLE-    */ 01826000
*/*      LENGTH FIELDS WITHOUT DOING ANY ACTUAL DATA MOVING.)        */ 01827000
*/*                                                                  */ 01828000
*/********************************************************************/ 01829000
*                                                                  1013 01830000
*   RPARM=ADDR(PLMVPARM);           /* ADDRESS OF PARAMETER LIST     */ 01831000
         LA    RPARM,PLMVPARM                                      1013 01832000
*   PLMVTU=RPRF;                    /* TARGET UNIT ADDRESS           */ 01833000
         ST    RPRF,PLMVTU(,RPARM)                                 1014 01834000
*   PLMVTD=ADDR(XCINBLEN);          /* TARGET DATA ADDRESS           */ 01835000
         LA    @09,13                                              1015 01836000
         ALR   @09,RPIU                                            1015 01837000
         LA    @07,XCINBLEN(,@09)                                  1015 01838000
         ST    @07,PLMVTD(,RPARM)                                  1015 01839000
*   PLMVSU=PLMVTU;                  /* SOURCE UNIT ADDRESS           */ 01840000
         L     @05,PLMVTU(,RPARM)                                  1016 01841000
         ST    @05,PLMVSU(,RPARM)                                  1016 01842000
*   PLMVSD=PLMVTD;                  /* SOURCE DATA ADDRESS           */ 01843000
         ST    @07,PLMVSD(,RPARM)                                  1017 01844000
*   PLMVLNTH=XCINBLEN+LENGTH(XCINBLEN)+LENGTH(XCINNTYP);/* LENGTH    */ 01845000
         LA    @07,3                                               1018 01846000
         SLR   @05,@05                                             1018 01847000
         ICM   @05,3,XCINBLEN(@09)                                 1018 01848000
         ALR   @07,@05                                             1018 01849000
         STH   @07,PLMVLNTH(,RPARM)                                1018 01850000
*   PLMVFLG1=0;                     /* CLEAR FLAG BYTE               */ 01851000
         MVI   PLMVFLG1(RPARM),X'00'                               1019 01852000
*   CALL IEDMVBFR;                  /* SKIP TO NETWORK NAME LENGTH 1020 01853000
*                                      BYTE (XCINNLEN)               */ 01854000
         L     @09,AVTSAVTP(,RAVT)                                 1020 01855000
         L     @15,SAVTMVBF(,@09)                                  1020 01856000
         BALR  @14,@15                                             1020 01857000
*                                   /* LOOP TO SKIP OVER 3 VARIABLE-    01858000
*                                      LENGTH FIELDS (NETWORK NAME,     01859000
*                                      REQUESTOR ID, AND PASSWORD)   */ 01860000
*   R0=3;                           /* LOOP COUNTER                  */ 01861000
         LA    R0,3                                                1021 01862000
*   DO UNTIL(R0=0);                                                1022 01863000
@DL01022 DS    0H                                                  1023 01864000
*     PLMVLNTH=PLMVSD->VARLNGTH+LENGTH(VARLNGTH);/* BYTES TO SKIP    */ 01865000
         L     @09,PLMVSD(,RPARM)                                  1023 01866000
         LA    @07,1                                               1023 01867000
         SLR   @05,@05                                             1023 01868000
         IC    @05,VARLNGTH(,@09)                                  1023 01869000
         ALR   @07,@05                                             1023 01870000
         STH   @07,PLMVLNTH(,RPARM)                                1023 01871000
*     CALL IEDMVBFR;                /* SKIP TO NEXT FIELD            */ 01872000
         L     @09,AVTSAVTP(,RAVT)                                 1024 01873000
         L     @15,SAVTMVBF(,@09)                                  1024 01874000
         BALR  @14,@15                                             1024 01875000
*     R0=R0-1;                      /* DECREMENT LOOP COUNT          */ 01876000
         BCTR  R0,0                                                1025 01877000
*   END;                                                           1026 01878000
@DE01022 LTR   R0,R0                                               1026 01879000
         BNZ   @DL01022                                            1026 01880000
*   DATAPTR=PLMVSD;                 /* ADDRESS OF LOGON DATA FIELD   */ 01881000
         L     @09,PLMVSD(,RPARM)                                  1027 01882000
         ST    @09,DATAPTR                                         1027 01883000
*   END FNDLGDTA;                   /* RETURN TO CALLER              */ 01884000
@EL00024 DS    0H                                                  1028 01885000
@EF00024 DS    0H                                                  1028 01886000
@ER00024 L     @14,@SA00024                                        1028 01887000
         BR    @14                                                 1028 01888000
         EJECT                                                          01889000
*FREEBUF:                                                          1029 01890000
*   PROC OPTIONS(SAVE(R14));                                       1029 01891000
*                                                                  1029 01892000
FREEBUF  ST    @14,@SA00025                                        1029 01893000
*/********************************************************************/ 01894000
*/*                                                                  */ 01895000
*/*      F R E E B U F   SUBROUTINE                                  */ 01896000
*/*                                                                  */ 01897000
*/********************************************************************/ 01898000
*/*                                                                  */ 01899000
*/*      THIS SUBROUTINE FREES A BUFFER BY TPOSTING IT TO THE        */ 01900000
*/*      BUFFER-RETURN QCB.                                          */ 01901000
*/*                                                                  */ 01902000
*/********************************************************************/ 01903000
*                                                                  1030 01904000
*   RPARM->PRFQCBA=ADDR(AVTBFRTB);  /* BUFFER-RETURN QCB             */ 01905000
         LA    @09,AVTBFRTB(,RAVT)                                 1030 01906000
         STCM  @09,7,PRFQCBA(RPARM)                                1030 01907000
*   RPARM->PRFPRI=PRIBFRTB;         /* PRIORITY                      */ 01908000
         MVI   PRFPRI(RPARM),X'E0'                                 1031 01909000
*   RPARM->PRFNBUNT=0;              /* SET TO LET IEDQGA DETERMINE 1032 01910000
*                                      NUMBER OF UNITS               */ 01911000
         MVI   PRFNBUNT(RPARM),X'00'                               1032 01912000
*   CALL DSPPOSTR;                  /* TPOST THE BUFFER              */ 01913000
         LA    @15,DSPPOSTR(,RDISP)                                1033 01914000
         BALR  @14,@15                                             1033 01915000
*   END FREEBUF;                    /* RETURN TO CALLER              */ 01916000
@EL00025 DS    0H                                                  1034 01917000
@EF00025 DS    0H                                                  1034 01918000
@ER00025 L     @14,@SA00025                                        1034 01919000
         BR    @14                                                 1034 01920000
         EJECT                                                          01921000
*GETBUFI:                                                          1035 01922000
*   PROC OPTIONS(SAVE(R14));                                       1035 01923000
*                                                                  1035 01924000
GETBUFI  ST    @14,@SA00026                                        1035 01925000
*/********************************************************************/ 01926000
*/*                                                                  */ 01927000
*/*      G E T B U F I   SUBROUTINE                                  */ 01928000
*/*                                                                  */ 01929000
*/********************************************************************/ 01930000
*/*                                                                  */ 01931000
*/*      THIS SUBROUTINE IS CALLED BY THE QCBRTN AND DACTSHTD        */ 01932000
*/*      SUBROUTINES WHEN A BUFFER IS NEEDED BUT THERE IS NO PLCB    */ 01933000
*/*      AVAILABLE WHOSE ERB CAN BE USED.  THE CALLER PROVIDES THE   */ 01934000
*/*      ADDRESS OF THE ELEMENT NEEDING THE BUFFER AND THE NUMBER    */ 01935000
*/*      OF UNITS NEEDED.   PROCESSING DEPENDS ON WHETHER THE        */ 01936000
*/*      SINGLE INTERNAL ERB IS AVAILABLE:                           */ 01937000
*/*                                                                  */ 01938000
*/*      .  IF THE ERB IS AVAILABLE IT IS TPOSTED IMMEDIATELY TO     */ 01939000
*/*         IEDQGA TO REQUEST A BUFFER.  THE ADDRESS OF THE ELEMENT  */ 01940000
*/*         NEEDING THE BUFFER IS SAVED FOR LATER USE BY THE         */ 01941000
*/*         ERBRTNI SUBROUTINE.                                      */ 01942000
*/*                                                                  */ 01943000
*/*      .  IF THE ERB IS BUSY, THE ELEMENT IS PLACED ON A CHAIN.    */ 01944000
*/*         WHEN THE ERB LATER BECOMES AVAILABLE, AND THE ELEMENT    */ 01945000
*/*         REACHES THE TOP OF THE CHAIN, THE BUFFER REQUEST WILL    */ 01946000
*/*         BE MADE AT THAT TIME.                                    */ 01947000
*/*                                                                  */ 01948000
*/********************************************************************/ 01949000
*                                                                  1036 01950000
*   RLCB=ADDR(LUSLCB);              /* ADDR OF LOCALLY-DECLARED PLCB */ 01951000
         LA    RLCB,LUSLCB                                         1036 01952000
*   IF LCBDLNKN=ON THEN             /* IS INTERNAL ERB AVAILABLE?    */ 01953000
         TM    LCBDLNKN(RLCB),B'00000001'                          1037 01954000
         BNO   @RF01037                                            1037 01955000
*     DO;                           /* YES                           */ 01956000
*       LCBRBCT2=R0;                /* SET UNIT COUNT                */ 01957000
         STC   R0,LCBRBCT2(,RLCB)                                  1039 01958000
*       LCBBFRSV=RPARM;             /* ADDR OF ELEMENT NEEDING BUFFER*/ 01959000
         STCM  RPARM,7,LCBBFRSV(RLCB)                              1040 01960000
*       CALL TPOSTERB;              /* REQUEST A BUFFER              */ 01961000
         BAL   @14,TPOSTERB                                        1041 01962000
*     END;                                                         1042 01963000
*   ELSE                            /* INTERNAL ERB IS BUSY          */ 01964000
*     DO;                           /* ELEMENT MUST WAIT UNTIL LATER */ 01965000
         B     @RC01037                                            1043 01966000
@RF01037 DS    0H                                                  1044 01967000
*       RECBUNCT=R0;                /* SAVE UNIT COUNT IN 1ST WORD OF   01968000
*                                      ELEMENT                       */ 01969000
         STC   R0,RECBUNCT(,RPARM)                                 1044 01970000
*       CURRENT=ADDR(SAVTLUSB)-LINKOFFS;/* INITIALIZE FOR SEARCH     */ 01971000
         L     @09,AVTSAVTP(,RAVT)                                 1045 01972000
         LA    @09,SAVTLUSB(,@09)                                  1045 01973000
         SLR   @07,@07                                             1045 01974000
         IC    @07,LINKOFFS                                        1045 01975000
         SLR   @09,@07                                             1045 01976000
         ST    @09,CURRENT                                         1045 01977000
*       DO WHILE(CURRENT->RECBLINK>0);/* FIND END OF CHAIN           */ 01978000
         B     @DE01046                                            1046 01979000
@DL01046 DS    0H                                                  1047 01980000
*         CURRENT=CURRENT->RECBLINK;                               1047 01981000
         L     @09,CURRENT                                         1047 01982000
         L     @09,RECBLINK-1(,@09)                                1047 01983000
         LA    @09,0(,@09)                                         1047 01984000
         ST    @09,CURRENT                                         1047 01985000
*       END;                                                       1048 01986000
@DE01046 L     @09,CURRENT                                         1048 01987000
         L     @07,RECBLINK-1(,@09)                                1048 01988000
         LA    @07,0(,@07)                                         1048 01989000
         LTR   @07,@07                                             1048 01990000
         BP    @DL01046                                            1048 01991000
*       CURRENT->RECBLINK=RPARM;    /* ADD NEW ELEMENT AT END        */ 01992000
         STCM  RPARM,7,RECBLINK(@09)                               1049 01993000
*       RECBLINK=0;                 /* MARK IT AS LAST               */ 01994000
         SLR   @09,@09                                             1050 01995000
         STCM  @09,7,RECBLINK(RPARM)                               1050 01996000
*     END;                                                         1051 01997000
*   END GETBUFI;                    /* RETURN TO CALLER              */ 01998000
@EL00026 DS    0H                                                  1052 01999000
@EF00026 DS    0H                                                  1052 02000000
@ER00026 L     @14,@SA00026                                        1052 02001000
         BR    @14                                                 1052 02002000
         EJECT                                                          02003000
*LGOFF:                                                            1053 02004000
*   PROC OPTIONS(SAVE(R14));                                       1053 02005000
*                                                                  1053 02006000
LGOFF    ST    @14,@SA00027                                        1053 02007000
*/********************************************************************/ 02008000
*/*                                                                  */ 02009000
*/*      L G O F F   SUBROUTINE                                      */ 02010000
*/*                                                                  */ 02011000
*/********************************************************************/ 02012000
*/*                                                                  */ 02013000
*/*      THIS SUBROUTINE TPOSTS A PLCB TO IEDSCLU AS THE SIGNAL      */ 02014000
*/*      TO SEND CLEAR AND UNBIND PIUS TO A SLU.  SOME PRELIMINARY   */ 02015000
*/*      OPERATIONS MAY NEED TO BE DONE FIRST, HOWEVER:              */ 02016000
*/*                                                                  */ 02017000
*/*      .  IF THE SLU IS CURRENTLY IN MIDDLE-OF-MESSAGE IT IS       */ 02018000
*/*         FORCED OUT OF THAT STATE BY MEANS OF A 0-LENGTH ERROR    */ 02019000
*/*         BUFFER TPOSTED TO THE MH.                                */ 02020000
*/*                                                                  */ 02021000
*/*      .  THE SLU IS REMOVED FROM LOCK MODE, IF NECESSARY.         */ 02022000
*/*                                                                  */ 02023000
*/*      .  AN EMPTY BUFFER IS OBTAINED FOR USE BY IEDSCLU.  THIS    */ 02024000
*/*         AVOIDS THE NEED FOR BUFFER-REQUEST LOGIC IN IEDSCLU.     */ 02025000
*/*                                                                  */ 02026000
*/********************************************************************/ 02027000
*                                                                  1054 02028000
*   IF LCBERBCH=0 THEN              /* NEED TO REQUEST A BUFFER?     */ 02029000
         L     @09,LCBERBCH-1(,RLCB)                               1054 02030000
         LA    @09,0(,@09)                                         1054 02031000
         LTR   @09,@09                                             1054 02032000
         BNZ   @RF01054                                            1054 02033000
*     DO;                           /* YES                           */ 02034000
*       LCBRBCT2=1;                 /* SET NUMBER OF UNITS NEEDED    */ 02035000
         MVI   LCBRBCT2(RLCB),X'01'                                1056 02036000
*       CALL TPOSTERB;              /* REQUEST A BUFFER              */ 02037000
         BAL   @14,TPOSTERB                                        1057 02038000
*     END;                                                         1058 02039000
*   ELSE                            /* BUFFER ALREADY OBTAINED       */ 02040000
*     DO;                                                          1059 02041000
         B     @RC01054                                            1059 02042000
@RF01054 DS    0H                                                  1060 02043000
*       IF SCBBFMM=ON THEN          /* IS SLU IN MIDDLE-OF-MSG?      */ 02044000
         L     @09,LCBSCBA-1(,RLCB)                                1060 02045000
         LA    @09,0(,@09)                                         1060 02046000
         TM    SCBBFMM(@09),B'00000001'                            1060 02047000
         BNO   @RF01060                                            1060 02048000
*         DO;                       /* YES, SEND ERROR BUFFER TO MH  */ 02049000
*           RPRF=LCBERBCH;          /* GET BUFFER ADDRESS            */ 02050000
         L     RPRF,LCBERBCH-1(,RLCB)                              1062 02051000
         LA    RPRF,0(,RPRF)                                       1062 02052000
*           LCBERBCH=0;             /* CLEAR ERB'S BUFFER POINTER  1063 02053000
*                                      PARTIALLY SET UP BUFFER       */ 02054000
         SLR   @07,@07                                             1063 02055000
         STCM  @07,7,LCBERBCH(RLCB)                                1063 02056000
*           PRFPRI=PRIERBFR;        /* ERROR PRIORITY                */ 02057000
         MVI   PRFPRI(RPRF),X'E3'                                  1064 02058000
*           PRFNBUNT=1;             /* NUMBER OF UNITS               */ 02059000
         MVI   PRFNBUNT(RPRF),X'01'                                1065 02060000
*           PRF1FLG1=0;             /* CLEAR FLAG BYTE               */ 02061000
         LR    @05,RPRF                                            1066 02062000
         AL    @05,@CF03396                                        1066 02063000
         MVI   PRF1FLG1-3(@05),X'00'                               1066 02064000
*           PRFDEST=SIBINDEX;       /* DESTINATION = PLU             */ 02065000
         L     @05,SIBPTR                                          1067 02066000
         LH    @05,SIBINDEX(,@05)                                  1067 02067000
         N     @05,@CF03378                                        1067 02068000
         STH   @05,PRFDEST(,RPRF)                                  1067 02069000
*           PRFSIZE=0;              /* SIZE                          */ 02070000
         STH   @07,PRFSIZE(,RPRF)                                  1068 02071000
*           SCBTXTTN=ON;            /* SET TEXT ERROR        @OX21004*/ 02072000
         OI    SCBTXTTN(@09),B'01000000'                           1069 02073000
*           CALL MHBUF;             /* TPOST ERROR BUFFER TO MH      */ 02074000
         BAL   @14,MHBUF                                           1070 02075000
*         END;                                                     1071 02076000
*       ELSE                        /* NOT IN MIDDLE-OF-MSG          */ 02077000
*         DO;                                                      1072 02078000
         B     @RC01060                                            1072 02079000
@RF01060 DS    0H                                                  1073 02080000
*           SCBLCK1N=OFF;           /* TAKE SLU OUT OF LOCK MODE (IF    02081000
*                                      IT IS IN THAT STATE)          */ 02082000
         L     @09,LCBSCBA-1(,RLCB)                                1073 02083000
         NI    SCBLCK1N(@09),B'11011111'                           1073 02084000
*           LCBQCBA=SAVTSCLU;       /* QCB ADDRESS                   */ 02085000
         L     @09,AVTSAVTP(,RAVT)                                 1074 02086000
         MVC   LCBQCBA(3,RLCB),SAVTSCLU+1(@09)                     1074 02087000
*           LCBPRI=PRILNFRE;        /* PRIORITY                      */ 02088000
         MVI   LCBPRI(RLCB),X'E0'                                  1075 02089000
*           RPARM=RLCB;             /* PLCB ADDRESS                  */ 02090000
         LR    RPARM,RLCB                                          1076 02091000
*           CALL DSPPOSTR;          /* TPOST PLCB TO SESSION CONTROL */ 02092000
         LA    @15,DSPPOSTR(,RDISP)                                1077 02093000
         BALR  @14,@15                                             1077 02094000
*         END;                                                     1078 02095000
*     END;                                                         1079 02096000
*   END LGOFF;                      /* RETURN TO CALLER              */ 02097000
@EL00027 DS    0H                                                  1080 02098000
@EF00027 DS    0H                                                  1080 02099000
@ER00027 L     @14,@SA00027                                        1080 02100000
         BR    @14                                                 1080 02101000
         EJECT                                                          02102000
*MHBUF:                                                            1081 02103000
*   PROC OPTIONS(SAVE(R14));                                       1081 02104000
*                                                                  1081 02105000
MHBUF    ST    @14,@SA00028                                        1081 02106000
*/********************************************************************/ 02107000
*/*                                                                  */ 02108000
*/*      M H B U F   SUBROUTINE                                      */ 02109000
*/*                                                                  */ 02110000
*/********************************************************************/ 02111000
*/*                                                                  */ 02112000
*/*      THIS SUBROUTINE FINISHES BUILDING THE HEADER PREFIX FOR     */ 02113000
*/*      A BUFFER, SETS STATUS BITS AS NEEDED, AND TPOSTS THE        */ 02114000
*/*      BUFFER TO THE PROPER MH.                                    */ 02115000
*/*                                                                  */ 02116000
*/*      THE CALLER SUPPLIES A PARTIALLY-BUILT BUFFER WITH THE       */ 02117000
*/*      FOLLOWING FIELDS INITIALIZED:                               */ 02118000
*/*      .  PRFPRI           .  PRFNBUNT          .  PRFSIZE         */ 02119000
*/*      .  PRFDEST          .  PRF1LOGD                             */ 02120000
*/*                                                                  */ 02121000
*/********************************************************************/ 02122000
*                                                                  1082 02123000
*                                   /* SET UP BFR'S NEGATIVE PREFIX  */ 02124000
*   PRF1RH=DUMMYRH;                 /* SET UP RH IN NEG. PREFIX      */ 02125000
         LR    @09,RPRF                                            1082 02126000
         SL    @09,@CF00044                                        1082 02127000
         MVC   PRF1RH(3,@09),@CB03127                              1082 02128000
*   PRF1FIDI=ON;                    /* SET FID-1 INDICATOR           */ 02129000
         OI    PRF1FIDI(@09),B'10000000'                           1083 02130000
*   PRF1SEQP=0;                     /* SEQUENCE POINTER              */ 02131000
         SLR   @07,@07                                             1084 02132000
         ST    @07,PRF1SEQP(,@09)                                  1084 02133000
*                                   /* SET UP QCB ADDRESS FOR TPOST  */ 02134000
*   PRFQCBA=LCBMHA;                 /* ASSUME LCB POINTS TO PROPER 1085 02135000
*                                      MESSAGE HANDLER QCB   @YM06085*/ 02136000
         MVC   PRFQCBA(3,RPRF),LCBMHA(RLCB)                        1085 02137000
*                                   /* SET UP REST OF BUFFER PREFIX  */ 02138000
*   PRFLCB=RLCB;                    /* PLCB ADDRESS                  */ 02139000
         STCM  RLCB,7,PRFLCB(RPRF)                                 1086 02140000
*   PRFSRCE=TNTSLU;                 /* SOURCE TNT INDEX              */ 02141000
         LH    @09,TNTSLU                                          1087 02142000
         N     @09,@CF03378                                        1087 02143000
         STH   @09,PRFSRCE(,RPRF)                                  1087 02144000
*   PRFSTAT1=0;                     /* MARK AS HEADER, LAST          */ 02145000
         MVI   PRFSTAT1(RPRF),X'00'                                1088 02146000
*                                   /* CLEAR UNUSED HEADER FIELDS    */ 02147000
*   PRFXTRA=0;                      /*                               */ 02148000
         STCM  @07,7,PRFXTRA(RPRF)                                 1089 02149000
*   PRFSCAN=0;                      /*                               */ 02150000
         STH   @07,PRFSCAN(,RPRF)                                  1090 02151000
*   PRFNTXT=0;                      /*                               */ 02152000
         STCM  @07,7,PRFNTXT(RPRF)                                 1091 02153000
*   PRFCRCD=0;                      /*                               */ 02154000
         STCM  @07,7,PRFCRCD(RPRF)                                 1092 02155000
*   PRFNHDR=0;                      /*                               */ 02156000
         STCM  @07,7,PRFNHDR(RPRF)                                 1093 02157000
*   PRFHQBCK=0;                     /*                               */ 02158000
         STCM  @07,7,PRFHQBCK(RPRF)                                1094 02159000
*   PRFISEQ=0;                      /*                               */ 02160000
         STH   @07,PRFISEQ(,RPRF)                                  1095 02161000
*   PRFDEST=0;                      /*                               */ 02162000
         STH   @07,PRFDEST(,RPRF)                                  1096 02163000
*                                   /* SET ERROR STATUS BITS         */ 02164000
*                                   /* SET UP PLCB FOR INPUT MH      */ 02165000
*   LCBUSUE=ON;                     /* SET UNIT EXCEPTION            */ 02166000
         OI    LCBUSUE(RLCB),B'00000001'                           1097 02167000
*   LCBSENDN=OFF;                   /* MARK THE PLCB                 */ 02168000
*   LCBRECVN=ON;                    /* ... RECEIVING                 */ 02169000
         OI    LCBRECVN(RLCB),B'00000010'                          1099 02170000
         NI    LCBSENDN(RLCB),B'11111110'                          1099 02171000
*   RPARM=RPRF;                     /* BUFFER ADDRESS FOR TPOST      */ 02172000
         LR    RPARM,RPRF                                          1100 02173000
*   CALL DSPPOSTR;                  /* TPOST BUFFER TO MH            */ 02174000
         LA    @15,DSPPOSTR(,RDISP)                                1101 02175000
         BALR  @14,@15                                             1101 02176000
*   END MHBUF;                      /* RETURN TO CALLER              */ 02177000
@EL00028 DS    0H                                                  1102 02178000
@EF00028 DS    0H                                                  1102 02179000
@ER00028 L     @14,@SA00028                                        1102 02180000
         BR    @14                                                 1102 02181000
         EJECT                                                          02182000
*NAMESRCH:                                                         1103 02183000
*   PROC OPTIONS(SAVE(R14));                                       1103 02184000
NAMESRCH ST    @14,@SA00029                                        1103 02185000
*   RFY                                                            1104 02186000
*     R15 RSTD;                                                    1104 02187000
*                                                                  1104 02188000
*/********************************************************************/ 02189000
*/*                                                                  */ 02190000
*/*      N A M E S R C H   SUBROUTINE                                */ 02191000
*/*                                                                  */ 02192000
*/********************************************************************/ 02193000
*/*                                                                  */ 02194000
*/*      THIS SUBROUTINE TAKES A PRIMARY LU (PLU) NAME AND           */ 02195000
*/*      ATTEMPTS TO CONVERT IT INTO A TNT INDEX VALUE.  SINCE       */ 02196000
*/*      THE PLU NAME MAY REPRESENT EITHER TCAM, A PARTICULAR MH,    */ 02197000
*/*      OR A TPROCESS ENTRY, BOTH THE MH TABLE AND THE TERMINAL     */ 02198000
*/*      NAME TABLE MAY HAVE TO BE SEARCHED.                         */ 02199000
*/*                                                                  */ 02200000
*/********************************************************************/ 02201000
*                                                                  1105 02202000
*                                   /* LOOK FOR NAME IN MH TABLE     */ 02203000
*   TNTCTR=SAVTMHDX;                /* TNT INDEX FOR 1ST MH TBL ENTRY*/ 02204000
         L     @14,AVTSAVTP(,RAVT)                                 1105 02205000
         LH    @09,SAVTMHDX(,@14)                                  1105 02206000
         N     @09,@CF03378                                        1105 02207000
         STH   @09,TNTCTR                                          1105 02208000
*   CURRENT=SAVTMHTB;               /* ADDR OF 1ST MH TABLE ENTRY    */ 02209000
         L     @14,SAVTMHTB(,@14)                                  1106 02210000
         ST    @14,CURRENT                                         1106 02211000
*   FOUNDSW=OFF;                                                   1107 02212000
         NI    FOUNDSW,B'10111111'                                 1107 02213000
*   DO WHILE(FOUNDSW=OFF&MHNAME(1:1)<MHFLGE);                      1108 02214000
         B     @DE01108                                            1108 02215000
@DL01108 DS    0H                                                  1109 02216000
*     IF MHNAME=NAMECON THEN        /* NAME MATCH FOUND              */ 02217000
         L     @14,CURRENT                                         1109 02218000
         CLC   MHNAME(8,@14),NAMECON                               1109 02219000
         BNE   @RF01109                                            1109 02220000
*       FOUNDSW=ON;                 /* YES, MARK FOR EXIT FROM LOOP  */ 02221000
         OI    FOUNDSW,B'01000000'                                 1110 02222000
*     ELSE                                                         1111 02223000
*       DO;                         /* NO, KEEP LOOKING              */ 02224000
         B     @RC01109                                            1111 02225000
@RF01109 DS    0H                                                  1112 02226000
*         CURRENT=CURRENT+LENGTH(IEDMHTD);/* BUMP TO NEXT ENTRY      */ 02227000
         LA    @14,12                                              1112 02228000
         AL    @14,CURRENT                                         1112 02229000
         ST    @14,CURRENT                                         1112 02230000
*         TNTCTR=TNTCTR+1;          /* INCREMENT TNT VALUE           */ 02231000
         LH    @14,TNTCTR                                          1113 02232000
         LA    @14,1(,@14)                                         1113 02233000
         STH   @14,TNTCTR                                          1113 02234000
*       END;                                                       1114 02235000
*   END;                                                           1115 02236000
@RC01109 DS    0H                                                  1115 02237000
@DE01108 TM    FOUNDSW,B'01000000'                                 1115 02238000
         BNZ   @DC01108                                            1115 02239000
         L     @14,CURRENT                                         1115 02240000
         CLI   MHNAME(@14),X'FF'                                   1115 02241000
         BL    @DL01108                                            1115 02242000
@DC01108 DS    0H                                                  1116 02243000
*   IF FOUNDSW=OFF THEN             /* NO MATCH FOUND?               */ 02244000
         TM    FOUNDSW,B'01000000'                                 1116 02245000
         BNZ   @RF01116                                            1116 02246000
*     DO;                           /* TRUE, SEARCH THE TRM NAME TBL */ 02247000
*                                   /* SET UP PARAMETERS FOR IEDQA1  */ 02248000
*       R0=TNTENLEN;                /* NAME LENGTH                   */ 02249000
         L     @14,AVTRNMPT(,RAVT)                                 1118 02250000
         SLR   R0,R0                                               1118 02251000
         IC    R0,TNTENLEN(,@14)                                   1118 02252000
*       RPARM=ADDR(NAMECON);        /* ADDRESS OF NAME               */ 02253000
         LA    RPARM,NAMECON                                       1119 02254000
*       R15=A1VCON;                 /* TNT-SEARCH RTN (IEDQA1) ADDR  */ 02255000
         L     @09,AVTMSGS-1(,RAVT)                                1120 02256000
         L     R15,A1VCON(,@09)                                    1120 02257000
*       GEN(BAL  R14,4(R15));       /* SEARCH TERMINAL NAME TABLE    */ 02258000
         BAL  R14,4(R15)                                                02259000
*       IF R15>0 THEN               /* WAS A MATCH FOUND?            */ 02260000
         LTR   R15,R15                                             1122 02261000
         BNP   @RF01122                                            1122 02262000
*         DO;                       /* YES                           */ 02263000
*           TNTCTR=R15;             /* SAVE TNT VALUE IN LOCAL FIELD */ 02264000
         STH   R15,TNTCTR                                          1124 02265000
*           RPARM=R15;              /* TNT VALUE FOR PLU             */ 02266000
         LR    RPARM,R15                                           1125 02267000
*           CALL IEDQTNT;           /* CONVERT IT TO TTE ADDRESS     */ 02268000
         L     @15,AVTRNMPT(,RAVT)                                 1126 02269000
         BALR  @14,@15                                             1126 02270000
*           R0=TRMTPINM;            /* MASK FOR DEVICE DEPENDNT FIELD*/ 02271000
         LA    R0,4                                                1127 02272000
*           CALL IEDQTL;            /* GET DEVICE DEPENDNT FIELD ADDR*/ 02273000
         L     @15,AVTDDFT(,RAVT)                                  1128 02274000
         BALR  @14,@15                                             1128 02275000
*           IF R15>0&               /* FIELD FOUND? &                */ 02276000
*               R15->PROLU=ON THEN  /* ... LU=YES SPEC'D ON TPROCESS?*/ 02277000
         LTR   R15,R15                                             1129 02278000
         BNP   @RF01129                                            1129 02279000
         TM    PROLU(R15),B'01000000'                              1129 02280000
         BNO   @RF01129                                            1129 02281000
*             FOUNDSW=ON;           /* SET 'FOUND' INDICATOR         */ 02282000
         OI    FOUNDSW,B'01000000'                                 1130 02283000
*         END;                                                     1131 02284000
*     END;                                                         1132 02285000
*   END NAMESRCH;                   /* RETURN TO CALLER              */ 02286000
@EL00029 DS    0H                                                  1133 02287000
@EF00029 DS    0H                                                  1133 02288000
@ER00029 L     @14,@SA00029                                        1133 02289000
         BR    @14                                                 1133 02290000
*   RFY                                                            1134 02291000
*     R15 UNRSTD;                                                  1134 02292000
         EJECT                                                          02293000
*NOSIBRTN:                                                         1135 02294000
*   PROC OPTIONS(SAVE(R14));                                       1135 02295000
*                                                                  1135 02296000
NOSIBRTN ST    @14,@SA00030                                        1135 02297000
*/********************************************************************/ 02298000
*/*                                                                  */ 02299000
*/*      N O S I B R T N   SUBROUTINE                                */ 02300000
*/*                                                                  */ 02301000
*/********************************************************************/ 02302000
*/*                                                                  */ 02303000
*/*      THIS SUBROUTINE PROCESSES AN INIT-SELF ERROR RESPONSE       */ 02304000
*/*      PIU RECEIVED FROM THE SSCP.  SUCH A RESPONSE IS SENT        */ 02305000
*/*      WHEN THE SSCP WAS UNABLE TO OBTAIN A SIB FOR AN             */ 02306000
*/*      INTERNALLY-GENERATED INIT-SELF PIU.                         */ 02307000
*/*                                                                  */ 02308000
*/*      AFTER OBTAINING A PLCB THE SUBROUTINE CAUSES        @YM08450*/ 02309000
*/*      AN ERROR BUFFER TO BE TPOSTED TO THE USER'S MH.     @YM08450*/ 02310000
*/*                                                                  */ 02311000
*/********************************************************************/ 02312000
*                                                                  1136 02313000
*   IF QCBPLCBN=OFF THEN            /* DO WE NEED A PLCB?            */ 02314000
         L     @09,IEDQQPTR                                        1136 02315000
         SL    @09,@CF00044                                        1136 02316000
         TM    QCBPLCBN(@09),B'10000000'                           1136 02317000
         BNZ   @RF01136                                            1136 02318000
*     DO;                           /* YES                           */ 02319000
*       QCBNDXRQ=ON;                /* SPECIAL INDEXED REQUEST       */ 02320000
         OI    QCBNDXRQ(@09),B'00000100'                           1138 02321000
*       CALL RQSTPLCB;              /* REQUEST A PLCB                */ 02322000
         BAL   @14,RQSTPLCB                                        1139 02323000
*     END;                                                         1140 02324000
*   IF QCBPLCBN=OFF THEN            /* WAS OUR PLCB REQUEST QUEUED?  */ 02325000
@RF01136 L     @09,IEDQQPTR                                        1141 02326000
         AL    @09,@CF03385                                        1141 02327000
         TM    QCBPLCBN-4(@09),B'10000000'                         1141 02328000
         BNZ   @RF01141                                            1141 02329000
*     DO;                           /* YES, WE'LL GET PLCB LATER     */ 02330000
*       PRFLINK=SAVTLUSP;           /* ADD INIT-SELF PIU'S           */ 02331000
         L     @09,AVTSAVTP(,RAVT)                                 1143 02332000
         MVC   PRFLINK(3,RPRF),SAVTLUSP+1(@09)                     1143 02333000
*       SAVTLUSP=RPRF;              /* ... AT TOP OF CHAIN           */ 02334000
         ST    RPRF,SAVTLUSP(,@09)                                 1144 02335000
*     END;                                                         1145 02336000
*   ELSE                            /* WE HAVE A PLCB ASSIGNED       */ 02337000
*     DO;                                                          1146 02338000
         B     @RC01141                                            1146 02339000
@RF01141 DS    0H                                                  1147 02340000
*       QCBNDXRQ=OFF;               /* RESET INDEXED-REQUEST FLAG    */ 02341000
         L     @09,IEDQQPTR                                        1147 02342000
         AL    @09,@CF03385                                        1147 02343000
         NI    QCBNDXRQ-4(@09),B'11111011'                         1147 02344000
*       IF TTHDAF>SAVTMHDX THEN     /* IF IN SESSION WITH A MESSAGE     02345000
*                                      HANDLER, GET MH QCB   @YM06085*/ 02346000
         L     @09,AVTSAVTP(,RAVT)                                 1148 02347000
         CLC   TTHDAF(2,RPIU),SAVTMHDX(@09)                        1148 02348000
         BNH   @RF01148                                            1148 02349000
*         DO;                       /*                       @YM06085*/ 02350000
*           RPARM=((TTHDAF-SAVTMHDX)*LENGTH(IEDMHTD))+SAVTMHTB;/* GET   02351000
*                                      MH TABLE ENTRY        @YM06085*/ 02352000
         SLR   RPARM,RPARM                                         1150 02353000
         ICM   RPARM,3,TTHDAF(RPIU)                                1150 02354000
         LH    @07,SAVTMHDX(,@09)                                  1150 02355000
         N     @07,@CF03378                                        1150 02356000
         SLR   RPARM,@07                                           1150 02357000
         MH    RPARM,@CH00244                                      1150 02358000
         AL    RPARM,SAVTMHTB(,@09)                                1150 02359000
*           LCBMHA=RPARM->MHENTRY;  /* SET MH QCB ADDRESS    @YM06085*/ 02360000
         MVC   LCBMHA(3,RLCB),MHENTRY+1(RPARM)                     1151 02361000
*         END;                      /*                       @YM06085*/ 02362000
*       CALL ERRORPST;              /* CAUSE ERROR BUFFER TPOST TO 1153 02363000
*                                      USER'S MH             @YM08450*/ 02364000
@RF01148 BAL   @14,ERRORPST                                        1153 02365000
*     END;                                                         1154 02366000
*   END NOSIBRTN;                   /* RETURN TO CALLER              */ 02367000
@EL00030 DS    0H                                                  1155 02368000
@EF00030 DS    0H                                                  1155 02369000
@ER00030 L     @14,@SA00030                                        1155 02370000
         BR    @14                                                 1155 02371000
         EJECT                                                          02372000
*PIURTN:                                                           1156 02373000
*   PROC OPTIONS(SAVE(R14));                                       1156 02374000
*                                                                  1156 02375000
PIURTN   ST    @14,@SA00031                                        1156 02376000
