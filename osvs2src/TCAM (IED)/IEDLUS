*/*      T P O S T E R B   SUBROUTINE                                */ 03279000
*/*                                                                  */ 03280000
*/********************************************************************/ 03281000
*/*                                                                  */ 03282000
*/*      THIS SUBROUTINE COMPLETES THE SETTING UP OF AN ERB AND      */ 03283000
*/*      TPOSTS THE ERB TO REQUEST A BUFFER.                         */ 03284000
*/*                                                                  */ 03285000
*/********************************************************************/ 03286000
*                                                                  1405 03287000
*   LCBERBQB=ADDR(AVTBFREB);        /* QCB ADDRESS                   */ 03288000
         LA    @09,AVTBFREB(,RAVT)                                 1405 03289000
         STCM  @09,7,LCBERBQB(RLCB)                                1405 03290000
*   LCBERBPY=PRIINTRQ;              /* PRIORITY                      */ 03291000
         MVI   LCBERBPY(RLCB),X'E4'                                1406 03292000
*   LCBRBCT1=1;                     /* NUMBER OF BUFFERS REQUESTED   */ 03293000
         MVI   LCBRBCT1(RLCB),X'01'                                1407 03294000
*   LCBPRCPG=ON;                    /* SET UP ERB TO BE TPOSTED      */ 03295000
         OI    LCBPRCPG(RLCB),B'00000010'                          1408 03296000
*   LCBRCQCB=ADDR(SAVTLUSQ);        /* ... BACK TO THIS MODULE       */ 03297000
         L     @09,AVTSAVTP(,RAVT)                                 1409 03298000
         LA    @09,SAVTLUSQ(,@09)                                  1409 03299000
         ST    @09,LCBRCQCB(,RLCB)                                 1409 03300000
*   LCBDLNKN=OFF;                   /* MARK THE ERB IN USE           */ 03301000
         NI    LCBDLNKN(RLCB),B'11111110'                          1410 03302000
*   RPARM=ADDR(LCBERB);             /* ELEMENT ADDRESS FOR TPOST     */ 03303000
         LA    RPARM,LCBERB(,RLCB)                                 1411 03304000
*   CALL DSPPOSTR;                  /* TPOST THE ERB                 */ 03305000
         LA    @15,DSPPOSTR(,RDISP)                                1412 03306000
         BALR  @14,@15                                             1412 03307000
*   END TPOSTERB;                   /* RETURN TO CALLER              */ 03308000
@EL00040 DS    0H                                                  1413 03309000
@EF00040 DS    0H                                                  1413 03310000
@ER00040 L     @14,@SA00040                                        1413 03311000
         BR    @14                                                 1413 03312000
         EJECT                                                          03313000
*TPOSTPIU:                                                         1414 03314000
*   PROC OPTIONS(SAVE(R14));                                       1414 03315000
*                                                                  1414 03316000
