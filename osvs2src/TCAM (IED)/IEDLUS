         CLI   CHAR1(@09),X'A0'                                    1168 02425000
         BNE   @RF01168                                            1168 02426000
@RT01168 DS    0H                                                  1169 02427000
*         CALL ENDLGON;             /* YES, FINISH LOGON PROCESSING  */ 02428000
         BAL   @14,ENDLGON                                         1169 02429000
*       ELSE                        /* NO, MUST BE UNBIND RESPONSE   */ 02430000
*         CALL ENDLGOFF;            /* FINISH LOGOFF PROCESSING      */ 02431000
         B     @RC01168                                            1170 02432000
@RF01168 BAL   @14,ENDLGOFF                                        1170 02433000
*     END;                                                         1171 02434000
*   ELSE                            /* NETWORK SERVICES RU           */ 02435000
*     DO;                                                          1172 02436000
         B     @RC01164                                            1172 02437000
@RF01164 DS    0H                                                  1173 02438000
*       TNTSLU=TTHTAG;              /* GET SLU'S TNT INDEX           */ 02439000
         MVC   TNTSLU(2),TTHTAG(RPIU)                              1173 02440000
*       CALL CTLBLKS;               /* SET UP CONTROL BLOCK POINTERS */ 02441000
         BAL   @14,CTLBLKS                                         1174 02442000
*       IF DATAPTR->CHAR3=CD1XCIN THEN                             1175 02443000
         L     @09,DATAPTR                                         1175 02444000
