         L     @07,AVTRNMPT(,RAVT)                                 1296 02879000
         LH    @07,TNTLEN(,@07)                                    1296 02880000
         N     @07,@CF03378                                        1296 02881000
         CR    @09,@07                                             1296 02882000
         BNH   @RF01296                                            1296 02883000
         SLR   @03,@03                                             1296 02884000
         IC    @03,TCXTOTE(,TCXPTR)                                1296 02885000
         ALR   @07,@03                                             1296 02886000
         CR    @09,@07                                             1296 02887000
         BH    @RF01296                                            1296 02888000
*     DO;                           /*                       @YM08096*/ 02889000
*       RPARM=AVTCSTCS+(TRMCHCIN-1)*AVTDCTLN;/* DCT ADDRESS  @YM08096*/ 02890000
         SLR   @15,@15                                             1298 02891000
         IC    @15,TRMCHCIN(,RTRM)                                 1298 02892000
         BCTR  @15,0                                               1298 02893000
         MH    @15,AVTDCTLN(,RAVT)                                 1298 02894000
         AL    @15,AVTCSTCS(,RAVT)                                 1298 02895000
         LR    RPARM,@15                                           1298 02896000
*       RFY                                                        1299 02897000
*         IEDDCT BASED(RPARM);                                     1299 02898000
*       IF DCTL3270=ON THEN         /* CHECK FOR 3270 LUNT   @YM08096*/ 02899000
         TM    DCTL3270(RPARM),B'01000000'                         1300 02900000
         BNO   @RF01300                                            1300 02901000
*         XINSMODE=IEDTOTB2;        /* SET MODE NAME FOR 3270@YM08096*/ 02902000
         MVC   XINSMODE(8),@CC03117                                1301 02903000
*       ELSE                                                       1302 02904000
*         XINSMODE=IEDTOTB1;        /* SET ^3270 MODE NAME   @YM08096*/ 02905000
         B     @RC01300                                            1302 02906000
@RF01300 MVC   XINSMODE(8),@CC03115                                1302 02907000
*     END;                          /* END TOTE CHECK        @YM08096*/ 02908000
*   ELSE                                                           1304 02909000
*     DO;                           /* DO NORMAL PROCESSING  @YM08096*/ 02910000
         B     @RC01296                                            1304 02911000
@RF01296 DS    0H                                                  1305 02912000
*       R0=ADDR(LMODECON);          /* ADDR OF 'IEDLMODE' CONSTANT   */ 02913000
         LA    R0,@CC03113                                         1305 02914000
*       RPARM=ADDR(IEDQTRM);        /* SLU'S TERMINAL ENTRY ADDRESS  */ 02915000
         LA    RPARM,IEDQTRM(,RTRM)                                1306 02916000
*       CALL IEDOPTN;               /* FIND IEDLMODE OPTION FIELD    */ 02917000
         L     @15,AVTOPTN(,RAVT)                                  1307 02918000
         BALR  @14,@15                                             1307 02919000
*       IF R15=0 THEN               /* OPTION FIELD FOUND?           */ 02920000
         LTR   R15,R15                                             1308 02921000
         BNZ   @RF01308                                            1308 02922000
*         DO;                       /* YES                           */ 02923000
*           IF R0>LENGTH(XINSMODE) THEN/* OPTION FIELD TOO LONG?     */ 02924000
         LA    @09,8                                               1310 02925000
         CR    R0,@09                                              1310 02926000
         BNH   @RF01310                                            1310 02927000
*             R0=LENGTH(XINSMODE);  /* YES, TRUNCATE IT              */ 02928000
         LR    R0,@09                                              1311 02929000
*           XINSMODE(1:R0)=RPARM->CHAR8(1:R0);/* GET MODE NAME       */ 02930000
@RF01310 LR    @09,R0                                              1312 02931000
         BCTR  @09,0                                               1312 02932000
         EX    @09,@SM03398                                        1312 02933000
*         END;                                                     1313 02934000
*                                   /* GET SLU'S NETWORK NAME        */ 02935000
*       TNTCTR=TNTSLU-1;            /* NO. OF ENTRIES FROM TBL START */ 02936000
@RF01308 LH    @09,TNTSLU                                          1314 02937000
         N     @09,@CF03378                                        1314 02938000
         BCTR  @09,0                                               1314 02939000
         STH   @09,TNTCTR                                          1314 02940000
*       ENTRYSZ=TNTENLEN+3;         /* SIZE OF EACH TABLE ENTRY      */ 02941000
         L     @07,AVTRNMPT(,RAVT)                                 1315 02942000
         SLR   @05,@05                                             1315 02943000
         IC    @05,TNTENLEN(,@07)                                  1315 02944000
         LA    ENTRYSZ,3(,@05)                                     1315 02945000
*       CURRENT=ADDR(TNTFIRST);     /* ADDRESS OF START OF TABLE     */ 02946000
         AL    @07,@CF03369                                        1316 02947000
         ST    @07,CURRENT                                         1316 02948000
*       RPARM=CURRENT+(TNTCTR*ENTRYSZ);/* CALULATE ENTRY ADDRESS     */ 02949000
         LR    @15,@09                                             1317 02950000
         MR    @14,ENTRYSZ                                         1317 02951000
         ALR   @07,@15                                             1317 02952000
         LR    RPARM,@07                                           1317 02953000
*       XINSNAME=BLANKS;            /* SET UP SLU-NAME FIELD         */ 02954000
         MVC   XINSNAME(8),@CC03119                                1318 02955000
*       XINSNAME(1:TNTENLEN)=RPARM->CHAR8(1:TNTENLEN);             1319 02956000
         BCTR  @05,0                                               1319 02957000
         EX    @05,@SM03400                                        1319 02958000
*                                   /* MOVE IN INIT-SELF RU DATA     */ 02959000
*     END;                          /*                       @YM08096*/ 02960000
*   RPARM=ADDR(PLMVPARM);           /* PARAMETER LIST ADDRESS        */ 02961000
@RC01296 LA    RPARM,PLMVPARM                                      1321 02962000
*   PLMVTU=RPRF;                    /* TARGET UNIT ADDR              */ 02963000
         ST    RPRF,PLMVTU(,RPARM)                                 1322 02964000
*   PLMVTD=ADDR(PIURU);             /* TARGET DATA ADDR              */ 02965000
         LA    @09,PIURU(,RPIU)                                    1323 02966000
         ST    @09,PLMVTD(,RPARM)                                  1323 02967000
*   PLMVSU=ADDR(IEDXINSD);          /* SOURCE 'UNIT' ADDR            */ 02968000
         LA    @09,IEDXINSD                                        1324 02969000
         ST    @09,PLMVSU(,RPARM)                                  1324 02970000
*   PLMVSD=ADDR(IEDXINSD);          /* SOURCE DATA ADDR              */ 02971000
         ST    @09,PLMVSD(,RPARM)                                  1325 02972000
*   PLMVLNTH=LENGTH(IEDXINSD);      /* NUMBER OF BYTES TO BE MOVED   */ 02973000
         MVC   PLMVLNTH(2,RPARM),@CH01521                          1326 02974000
*   PLMVFLG1=0;                     /* CLEAR FLAG BYTE               */ 02975000
         MVI   PLMVFLG1(RPARM),X'00'                               1327 02976000
*   PLMVSCTG=ON;                    /* INDICATE THAT SOURCE DATA IS     02977000
*                                      IN A CONTIGUOUS-CORE AREA     */ 02978000
         OI    PLMVSCTG(RPARM),B'01000000'                         1328 02979000
*   CALL IEDMVBFR;                  /* MOVE THE DATA                 */ 02980000
         L     @09,AVTSAVTP(,RAVT)                                 1329 02981000
         L     @15,SAVTMVBF(,@09)                                  1329 02982000
         BALR  @14,@15                                             1329 02983000
*   END RUINITS;                    /* RETURN TO CALLER              */ 02984000
@EL00036 DS    0H                                                  1330 02985000
@EF00036 DS    0H                                                  1330 02986000
@ER00036 L     @14,@SA00036                                        1330 02987000
         BR    @14                                                 1330 02988000
         EJECT                                                          02989000
*SAVLGDTA:                                                         1331 02990000
*   PROC OPTIONS(SAVE(R14));                                       1331 02991000
*                                                                  1331 02992000
SAVLGDTA ST    @14,@SA00037                                        1331 02993000
*/********************************************************************/ 02994000
*/*                                                                  */ 02995000
*/*      S A V L G D T A   SUBROUTINE                                */ 02996000
*/*                                                                  */ 02997000
*/********************************************************************/ 02998000
*/*                                                                  */ 02999000
*/*      THIS SUBROUTINE MOVES THE LOGON DATA (THE USER-DATA FIELD)  */ 03000000
*/*      OUT OF A CINIT PIU AND SAVES IT IN AN EMPTY BUFFER.         */ 03001000
*/*      A PARTIAL HEADER PREFIX IS BUILT, AND THE BUFFER IS         */ 03002000
*/*      IS SAVED FOR A LATER TPOST TO MH.                           */ 03003000
*/*                                                                  */ 03004000
*/*      (NOTE THAT THE DATA MOVE DEPENDS ON SOURCE UNIT & SOURCE    */ 03005000
*/*      DATA POINTERS SET UP BY THE FNDLGDTA SUBROUTINE.)           */ 03006000
*/*                                                                  */ 03007000
*/********************************************************************/ 03008000
*                                                                  1332 03009000
*   RPRF=LCBERBCH;                  /* BUFFER ADDRESS                */ 03010000
         L     RPRF,LCBERBCH-1(,RLCB)                              1332 03011000
         LA    RPRF,0(,RPRF)                                       1332 03012000
*   LCBERBCH=0;                     /* CLEAR ERB'S BUFFER POINTER    */ 03013000
         SLR   @09,@09                                             1333 03014000
         STCM  @09,7,LCBERBCH(RLCB)                                1333 03015000
*                                   /* MOVE LOGON DATA INTO EMPTY BFR*/ 03016000
*   RPARM=ADDR(PLMVPARM);           /* PARAMETER LIST ADDRESS        */ 03017000
         LA    RPARM,PLMVPARM                                      1334 03018000
*   PLMVTU=RPRF;                    /* TARGET UNIT ADDRESS           */ 03019000
         ST    RPRF,PLMVTU(,RPARM)                                 1335 03020000
*   PLMVTD=ADDR(PRFSHDR)-1;         /* TARGET DATA ADDRESS           */ 03021000
         LA    @09,PRFSHDR(,RPRF)                                  1336 03022000
         BCTR  @09,0                                               1336 03023000
         ST    @09,PLMVTD(,RPARM)                                  1336 03024000
*   PLMVLNTH=DATAPTR->VARLNGTH+LENGTH(VARLNGTH);/* NUMBER OF BYTES   */ 03025000
         LA    @09,1                                               1337 03026000
         L     @07,DATAPTR                                         1337 03027000
         SLR   @05,@05                                             1337 03028000
         IC    @05,VARLNGTH(,@07)                                  1337 03029000
         ALR   @09,@05                                             1337 03030000
         STH   @09,PLMVLNTH(,RPARM)                                1337 03031000
*   CALL IEDMVBFR;                  /* MOVE THE DATA                 */ 03032000
         L     @09,AVTSAVTP(,RAVT)                                 1338 03033000
         L     @15,SAVTMVBF(,@09)                                  1338 03034000
         BALR  @14,@15                                             1338 03035000
*                                   /* BUILD A PARTIAL HEADER PREFIX */ 03036000
*   PRF1FLG1=0;                     /* INITIALIZE FLAG BYTE          */ 03037000
         LR    @09,RPRF                                            1339 03038000
         SL    @09,@CF00044                                        1339 03039000
         MVI   PRF1FLG1(@09),X'00'                                 1339 03040000
*   PRF1LOGD=ON;                    /* MARK AS LOGON DATA            */ 03041000
         OI    PRF1LOGD(@09),B'00000100'                           1340 03042000
*   PRFDEST=TTHDAF;                 /* PLU'S TNT INDEX               */ 03043000
         MVC   PRFDEST(2,RPRF),TTHDAF(RPIU)                        1341 03044000
*   PRFSIZE=DATAPTR->VARLNGTH+AVTHDRSZ;/* SIZE                       */ 03045000
         LA    @09,30                                              1342 03046000
         L     @07,DATAPTR                                         1342 03047000
         SLR   @05,@05                                             1342 03048000
         IC    @05,VARLNGTH(,@07)                                  1342 03049000
         ALR   @09,@05                                             1342 03050000
         STH   @09,PRFSIZE(,RPRF)                                  1342 03051000
*                                   /* SAVE LOGON-DATA BUFFER ON WAIT   03052000
*                                      CHAIN WITH HIGHEST PRIORITY   */ 03053000
*   PRFPRI='FF'X;                   /* PRIORITY                      */ 03054000
         MVI   PRFPRI(RPRF),X'FF'                                  1343 03055000
*   PRFLINK=0;                      /* END-OF-CHAIN                  */ 03056000
         SLR   @09,@09                                             1344 03057000
         STCM  @09,7,PRFLINK(RPRF)                                 1344 03058000
*   LCBWAITC=RPRF;                  /* PUT BUFFER ON WAIT CHAIN      */ 03059000
         STCM  RPRF,7,LCBWAITC(RLCB)                               1345 03060000
*   RPRF=INPUTBUF;                  /* RESTORE ORIGINAL REG VALUE    */ 03061000
         L     RPRF,INPUTBUF                                       1346 03062000
*   END SAVLGDTA;                   /* RETURN TO CALLER              */ 03063000
@EL00037 DS    0H                                                  1347 03064000
@EF00037 DS    0H                                                  1347 03065000
@ER00037 L     @14,@SA00037                                        1347 03066000
         BR    @14                                                 1347 03067000
         EJECT                                                          03068000
*SECLNUP:                                                          1348 03069000
*   PROC OPTIONS(SAVE(R14));        /*                       @YM08450*/ 03070000
SECLNUP  ST    @14,@SA00038                                        1348 03071000
*/********************************************************** @YM08450*/ 03072000
*/*                                                          @YM08450*/ 03073000
*/*      S E C L N U P   SUBROUTINE                          @YM08450*/ 03074000
*/*                                                          @YM08450*/ 03075000
*/********************************************************** @YM08450*/ 03076000
*/*                                                          @YM08450*/ 03077000
*/*      THIS SUBROUTINE TAKES CARE OF END-OF-SESSION        @YM08450*/ 03078000
*/*      CLEAN-UP FUNCTIONS COMMON TO BOTH NORMAL AND        @YM08450*/ 03079000
*/*      ERROR SITUATIONS.                                   @YM08450*/ 03080000
*/*                                                          @YM08450*/ 03081000
*/********************************************************** @YM08450*/ 03082000
*                                                                  1349 03083000
*   TRMSESRQ=OFF;                   /* ALLOW TCAM TO REQUEST NEW   1349 03084000
*                                      SESSION               @YM08450*/ 03085000
         LA    @05,TRMSESRQ(,RTRM)                                 1349 03086000
         LR    @03,@05                                             1349 03087000
         LA    @09,3                                               1349 03088000
         NR    @03,@09                                             1349 03089000
         LCR   @03,@03                                             1349 03090000
         ALR   @05,@03                                             1349 03091000
         L     @09,0(,@05)                                         1349 03092000
@GL00021 LR    @07,@09                                             1349 03093000
         N     @07,@CB03402+3(@03)                                 1349 03094000
         CS    @09,@07,0(@05)                                      1349 03095000
         BNE   @GL00021                                            1349 03096000
*   LCBLUS=OFF;                     /* RESET LUS BIT SO QEVENT WON'T    03097000
*                                      RETURN PLCB TO IEDLUS @YM08450*/ 03098000
         NI    LCBLUS(RLCB),B'11011111'                            1350 03099000
*   IF SIBPTR>0 THEN                /* LU-LU SIB ASSIGNED?   @YM08450*/ 03100000
         L     @09,SIBPTR                                          1351 03101000
         LTR   @09,@09                                             1351 03102000
         BNP   @RF01351                                            1351 03103000
*     DO;                           /* YES, CLEAN UP SIB     @YM08450*/ 03104000
*       SIBSSTAT=SIBSFINL;          /* SESSION STATE = FINAL @YM08450*/ 03105000
         NI    SIBSSTAT(@09),B'11110011'                           1353 03106000
         OI    SIBSSTAT(@09),B'00010000'                           1353 03107000
*       SIBLGOFF=OFF;               /* LOGOFF BIT            @YM08450*/ 03108000
*       SIBSHUTC=OFF;               /* SHUTDOWN-COMPLETE BIT @YM08450*/ 03109000
         NI    SIBLGOFF(@09),B'00111111'                           1355 03110000
*       IF SIBRQEND=ON|             /* ANY DEACT REQUESTS    @YM08450*/ 03111000
*           SIBRQSHT=ON THEN        /* ... ENQUEUED FOR LU?  @YM08450*/ 03112000
         TM    SIBRQEND(@09),B'11000000'                           1356 03113000
         BZ    @RF01356                                            1356 03114000
*         CALL DACTLOOP;            /* YES, DELINK & RETURN  @YM08450*/ 03115000
         BAL   @14,DACTLOOP                                        1357 03116000
*     END;                          /*                       @YM08450*/ 03117000
*   ELSE                            /* NO LU-LU SIB          @YM08450*/ 03118000
*     CALL DACTLOOP;                /* IF ANY DEACT REQUESTS ARE   1359 03119000
*                                      ENQUEUED FOR THIS LU DELINK &    03120000
*                                      RETURN THEM           @YM08450*/ 03121000
         B     @RC01351                                            1359 03122000
@RF01351 BAL   @14,DACTLOOP                                        1359 03123000
*   SENDSW=OFF;                     /* INIT SEND SWITCH      @YM08450*/ 03124000
@RC01351 NI    SENDSW,B'11110111'                                  1360 03125000
*   CALL CHKSEND;                   /* SEE IF THERE ARE ANY MSGS   1361 03126000
*                                      WHICH CAN BE SENT NOW @YM08450*/ 03127000
         BAL   @14,CHKSEND                                         1361 03128000
*   IF SENDSW=ON&                   /* MESSAGES TO BE SENT   @YM08450*/ 03129000
*       QCBSTPRI^=QCBCLK THEN       /* ... & NOT CLOCK?      @YM08450*/ 03130000
         TM    SENDSW,B'00001000'                                  1362 03131000
         BNO   @RF01362                                            1362 03132000
         L     @09,IEDQQPTR                                        1362 03133000
         CLI   QCBSTPRI(@09),X'80'                                 1362 03134000
         BE    @RF01362                                            1362 03135000
*     DO;                           /* TRUE                  @YM08450*/ 03136000
*       QCBSNDRQ=ON;                /* BIT TO CAUSE QEVENT TO REQUEST   03137000
*                                      A NEW SESSION         @YM08450*/ 03138000
         LR    @07,@09                                             1364 03139000
         AL    @07,@CF03385                                        1364 03140000
         OI    QCBSNDRQ-4(@07),B'00100000'                         1364 03141000
*       IF LCBSTCBA=ADDR(QCBSTVTO) THEN/* SND SCHED IN PLCB? @YM08450*/ 03142000
         LA    @07,QCBSTVTO(,@09)                                  1365 03143000
         L     @05,LCBSTCBA-1(,RLCB)                               1365 03144000
         LA    @05,0(,@05)                                         1365 03145000
         CR    @07,@05                                             1365 03146000
         BNE   @RF01365                                            1365 03147000
*         DO;                       /* YES                   @YM08450*/ 03148000
*           LCBSTCBA=QCBSLINK;      /* REMOVE SEND SCHEDULER @YM08450*/ 03149000
         MVC   LCBSTCBA(3,RLCB),QCBSLINK(@09)                      1367 03150000
*           QCBSLINK=QCBSTCHN;      /* ... STCB FROM PLCB &  @YM08450*/ 03151000
         MVC   QCBSLINK(3,@09),QCBSTCHN(@09)                       1368 03152000
*           QCBSTCHN=ADDR(QCBSTVTO);/* ... PUT IT BACK INTO  @YM08450*/ 03153000
         STCM  @07,7,QCBSTCHN(@09)                                 1369 03154000
*         END;                      /* ... QCB'S CHAIN       @YM08450*/ 03155000
*     END;                          /*                       @YM08450*/ 03156000
@RF01365 DS    0H                                                  1372 03157000
*   BYPASSSW=ON;                    /* CAUSE PLCB TO BE PASSED TO  1372 03158000
*                                      QEVENT VIA DSPBYPAS   @YM08450*/ 03159000
@RF01362 OI    BYPASSSW,B'10000000'                                1372 03160000
*   END SECLNUP;                    /* RETURN TO CALLER      @YM08450*/ 03161000
@EL00038 DS    0H                                                  1373 03162000
@EF00038 DS    0H                                                  1373 03163000
@ER00038 L     @14,@SA00038                                        1373 03164000
         BR    @14                                                 1373 03165000
         EJECT                                                          03166000
*THINITS:                                                          1374 03167000
*   PROC OPTIONS(SAVE(R14));                                       1374 03168000
*                                                                  1374 03169000
THINITS  ST    @14,@SA00039                                        1374 03170000
*/********************************************************************/ 03171000
*/*                                                                  */ 03172000
*/*      T H I N I T S   SUBROUTINE                                  */ 03173000
*/*                                                                  */ 03174000
*/********************************************************************/ 03175000
*/*                                                                  */ 03176000
*/*      THIS SUBROUTINE BUILDS THE TH FOR AN INIT-SELF REQUEST.     */ 03177000
*/*                                                                  */ 03178000
*/********************************************************************/ 03179000
*                                                                  1375 03180000
*                                   /* INITIALIZE THE TH             */ 03181000
*   TTHBYTE0=NRMFLOTH;              /* NORMAL-FLOW TH                */ 03182000
         MVI   TTHBYTE0(RPIU),X'1C'                                1375 03183000
*   TTHBYTE1=0;                                                    1376 03184000
         MVI   TTHBYTE1(RPIU),X'00'                                1376 03185000
*   TTHDAF=SAVTSCPT;                /* SSCP'S TNT INDEX              */ 03186000
         L     @09,AVTSAVTP(,RAVT)                                 1377 03187000
         MVC   TTHDAF(2,RPIU),SAVTSCPT(@09)                        1377 03188000
*   TCXPTR=CVTAQAVT;                /* GET TCX ADDRESS       @YM08096*/ 03189000
         L     @09,CVTPTR                                          1378 03190000
         L     TCXPTR,CVTAQAVT(,@09)                               1378 03191000
*   IF(QCBTTCIN>TNTLEN)&            /* IF THIS IS A TOTE ENTRY       */ 03192000
*       (QCBTTCIN=<TNTLEN+TCXTOTE) THEN                            1379 03193000
         L     @09,IEDQQPTR                                        1379 03194000
         BCTR  @09,0                                               1379 03195000
         BCTR  @09,0                                               1379 03196000
         LH    @09,QCBTTCIN-6(,@09)                                1379 03197000
         L     @07,AVTRNMPT(,RAVT)                                 1379 03198000
         LH    @07,TNTLEN(,@07)                                    1379 03199000
         N     @07,@CF03378                                        1379 03200000
         CR    @09,@07                                             1379 03201000
         BNH   @RF01379                                            1379 03202000
         SLR   @03,@03                                             1379 03203000
         IC    @03,TCXTOTE(,TCXPTR)                                1379 03204000
         ALR   @07,@03                                             1379 03205000
         CR    @09,@07                                             1379 03206000
         BH    @RF01379                                            1379 03207000
*     DO;                           /*                       @YM08096*/ 03208000
*       RPARM=AVTCWTOT;             /* RESIDENT TOTE CTL BLK @YM08096*/ 03209000
         L     RPARM,AVTCWTOT(,RAVT)                               1381 03210000
*       TTHOAF=RPARM->RESMHSRC;     /* TOTE MH TTCIN         @YM08096*/ 03211000
         MVC   TTHOAF(2,RPIU),RESMHSRC(RPARM)                      1382 03212000
*     END;                          /* END OF TOTE CHECK     @YM08096*/ 03213000
*   ELSE                                                           1384 03214000
*     DO;                           /* CONTINUE NORMAL PROC  @YM08096*/ 03215000
         B     @RC01379                                            1384 03216000
@RF01379 DS    0H                                                  1385 03217000
*                                   /* GET THE PLU'S TNT INDEX       */ 03218000
*       R0=ADDR(LUNAMCON);          /* ADDR OF 'IEDLUNAM' CONSTANT   */ 03219000
         LA    R0,@CC03111                                         1385 03220000
*       RPARM=ADDR(IEDQTRM);        /* TTE ADDRESS FOR SLU           */ 03221000
         LA    RPARM,IEDQTRM(,RTRM)                                1386 03222000
*       CALL IEDOPTN;               /* GET ADDRESS OF SLU'S IEDLUNAM    03223000
*                                      OPTION FIELD                  */ 03224000
         L     @15,AVTOPTN(,RAVT)                                  1387 03225000
         BALR  @14,@15                                             1387 03226000
*       IF R15=0 THEN               /* GOOD RETURN CODE?             */ 03227000
         LTR   R15,R15                                             1388 03228000
         BNZ   @RF01388                                            1388 03229000
*         DO;                       /* YES, OPTION FIELD WAS FOUND   */ 03230000
*           IF R0>LENGTH(NAMECON) THEN/* OPTION FIELD TOO LONG?      */ 03231000
         LA    @09,8                                               1390 03232000
         CR    R0,@09                                              1390 03233000
         BNH   @RF01390                                            1390 03234000
*             R0=LENGTH(NAMECON);   /* YES, TRUNCATE IT              */ 03235000
         LR    R0,@09                                              1391 03236000
*           NAMECON=BLANKS;         /* GET PLU NAME FROM OPTION FIELD*/ 03237000
@RF01390 MVC   NAMECON(8),@CC03119                                 1392 03238000
*           NAMECON(1:R0)=RPARM->CHAR8(1:R0);                      1393 03239000
         LR    @09,R0                                              1393 03240000
         BCTR  @09,0                                               1393 03241000
         EX    @09,@SM03407                                        1393 03242000
*           CALL NAMESRCH;          /* FIND TNT INDEX FOR PLU NAME   */ 03243000
         BAL   @14,NAMESRCH                                        1394 03244000
*         END;                                                     1395 03245000
*       ELSE                        /* BAD RETURN CODE               */ 03246000
*         FOUNDSW=OFF;              /* INDICATE DEFAULT PLU          */ 03247000
         B     @RC01388                                            1396 03248000
@RF01388 NI    FOUNDSW,B'10111111'                                 1396 03249000
*                                   /* FINISH BUILDING TH            */ 03250000
*       IF FOUNDSW=ON THEN          /* DID WE FIND A MATCH?          */ 03251000
@RC01388 TM    FOUNDSW,B'01000000'                                 1397 03252000
         BNO   @RF01397                                            1397 03253000
*         TTHOAF=TNTCTR;            /* YES, USE THE TNT VALUE FOUND  */ 03254000
         MVC   TTHOAF(2,RPIU),TNTCTR                               1398 03255000
*       ELSE                                                       1399 03256000
*         TTHOAF=SAVTMHDX;          /* NO, USE THE DEFAULT TNT VALUE */ 03257000
         B     @RC01397                                            1399 03258000
@RF01397 L     @09,AVTSAVTP(,RAVT)                                 1399 03259000
         MVC   TTHOAF(2,RPIU),SAVTMHDX(@09)                        1399 03260000
*     END;                          /* END NORMAL PATH       @YM08096*/ 03261000
@RC01397 DS    0H                                                  1401 03262000
*   TTHTAG=TNTSLU;                  /* SLU'S TNT INDEX               */ 03263000
@RC01379 MVC   TTHTAG(2,RPIU),TNTSLU                               1401 03264000
*   TTHDCF=LENGTH(IEDRH)+LENGTH(IEDXINSD);/* DATA COUNT FIELD        */ 03265000
         MVC   TTHDCF(2,RPIU),@CH02302                             1402 03266000
*   END THINITS;                    /* RETURN TO CALLER              */ 03267000
@EL00039 DS    0H                                                  1403 03268000
@EF00039 DS    0H                                                  1403 03269000
@ER00039 L     @14,@SA00039                                        1403 03270000
         BR    @14                                                 1403 03271000
         EJECT                                                          03272000
*TPOSTERB:                                                         1404 03273000
*   PROC OPTIONS(SAVE(R14));                                       1404 03274000
*                                                                  1404 03275000
TPOSTERB ST    @14,@SA00040                                        1404 03276000
