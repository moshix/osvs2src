         TITLE 'IEDLUS, LOGICAL UNIT SERVICES                          *00001000
                        '                                               00002000
IEDLUS   CSECT ,                                                   0001 00003000
@PROLOG  DS    0H                                                  0002 00004000
         USING *,RBASE1,RBASE2                                          00005000
         DC    XL2'0400'            STCB FOR THIS MODULE                00006000
*                                                                       00007000
         LR    RBASE1,RSTCB         SET UP BASE REGISTERS 1 & 2         00008000
         LA    RBASE2,X'FFF'(RBASE1)                                    00009000
         LA    RBASE2,1(RBASE2)                                         00010000
*                                                                       00011000
         LA    RPARM,0(RPARM)       CLEAR HIGH-ORDER BYTE               00012000
*                                                                       00013000
IEDLUS   IEDHJN  START                                                  00014000
         EJECT                                                          00015000
*                                                                  0547 00016000
*/********************************************************************/ 00017000
*/*                                                                  */ 00018000
*/*      M A I N   S E G M E N T                                     */ 00019000
*/*                                                                  */ 00020000
*/********************************************************************/ 00021000
*                                                                  0547 00022000
*   BYPASSSW=OFF;                   /* INITIALIZE FOR NORMAL EXIT    */ 00023000
         NI    BYPASSSW,B'01111111'                                0547 00024000
*                                   /* DETERMINE INPUT ELEMENT TYPE  */ 00025000
*   IF RECBPRI=PRILUSQB THEN        /* DESTINATION QCB?              */ 00026000
         CLI   RECBPRI(RPARM),226                                  0548 00027000
         BNE   @RF00548                                            0548 00028000
*     CALL QCBRTN;                  /* YES                           */ 00029000
         BAL   @14,QCBRTN                                          0549 00030000
*   ELSE                                                           0550 00031000
*     IF RECBPRI=PRILNFRE THEN      /* PLCB?                         */ 00032000
         B     @RC00548                                            0550 00033000
@RF00548 CLI   RECBPRI(RPARM),224                                  0550 00034000
         BNE   @RF00550                                            0550 00035000
*       CALL PLCBRTN;               /* YES                           */ 00036000
         BAL   @14,PLCBRTN                                         0551 00037000
*     ELSE                                                         0552 00038000
*       IF RECBPRI=PRIACTIV THEN    /* ERB?                          */ 00039000
         B     @RC00550                                            0552 00040000
@RF00550 CLI   RECBPRI(RPARM),228                                  0552 00041000
         BNE   @RF00552                                            0552 00042000
*         CALL ERBRTN;              /* YES                           */ 00043000
         BAL   @14,ERBRTN                                          0553 00044000
*       ELSE                                                       0554 00045000
*         IF RECBPRI=PRISSPIU THEN  /* PIU BUFFER?                   */ 00046000
         B     @RC00552                                            0554 00047000
@RF00552 CLI   RECBPRI(RPARM),229                                  0554 00048000
         BNE   @RF00554                                            0554 00049000
*           CALL PIURTN;            /* YES                           */ 00050000
         BAL   @14,PIURTN                                          0555 00051000
*         ELSE                                                     0556 00052000
*           IF RECBPRI=PRILUSR THEN /* DEACTIVATE REQUEST ELEMENT?   */ 00053000
         B     @RC00554                                            0556 00054000
@RF00554 CLI   RECBPRI(RPARM),218                                  0556 00055000
         BNE   @RF00556                                            0556 00056000
*             CALL DACTRTN;         /* YES                           */ 00057000
         BAL   @14,DACTRTN                                         0557 00058000
*   IF BYPASSSW=OFF THEN            /* NORMAL EXIT?                  */ 00059000
@RF00556 DS    0H                                                  0558 00060000
@RC00554 DS    0H                                                  0558 00061000
@RC00552 DS    0H                                                  0558 00062000
@RC00550 DS    0H                                                  0558 00063000
@RC00548 TM    BYPASSSW,B'10000000'                                0558 00064000
         BNZ   @RF00558                                            0558 00065000
*     CALL DSPDISP;                 /* YES, EXIT TO DISPATCHER       */ 00066000
         LA    @15,DSPDISP(,RDISP)                                 0559 00067000
         BALR  @14,@15                                             0559 00068000
*   ELSE                            /* NO                            */ 00069000
*     DO;                           /* BYPASS TO QEVENT WITH PLCB    */ 00070000
         B     @RC00558                                            0560 00071000
@RF00558 DS    0H                                                  0561 00072000
*       RPARM=RLCB;                 /* ELEMENT ADDRESS               */ 00073000
         LR    RPARM,RLCB                                          0561 00074000
*       RSTCB=LCBRSLNK;             /* STCB ADDRESS                  */ 00075000
         L     RSTCB,LCBRSLNK-1(,RLCB)                             0562 00076000
         LA    RSTCB,0(,RSTCB)                                     0562 00077000
*       RQCB=RLCB;                  /* QCB ADDRESS                   */ 00078000
         LR    RQCB,RLCB                                           0563 00079000
*       CALL DSPBYPAS;              /* BYPASS TO QEVENT              */ 00080000
         LA    @15,DSPBYPAS(,RDISP)                                0564 00081000
         BALR  @14,@15                                             0564 00082000
*     END;                                                         0565 00083000
*                                                                  0566 00084000
*/**************  E N D   O F   M A I N   S E G M E N T  *************/ 00085000
*                                                                  0566 00086000
         EJECT                                                          00087000
*BINDFAIL:                                                         0566 00088000
*   PROC OPTIONS(SAVE(R14));                                       0566 00089000
*                                                                  0566 00090000
@EL00001 DS    0H                                                  0566 00091000
@EF00001 DS    0H                                                  0566 00092000
@ER00001 BR    @14                                                 0566 00093000
BINDFAIL ST    @14,@SA00002                                        0566 00094000
*/********************************************************************/ 00095000
*/*                                                                  */ 00096000
*/*      B I N D F A I L   SUBROUTINE                                */ 00097000
*/*                                                                  */ 00098000
*/********************************************************************/ 00099000
*/*                                                                  */ 00100000
*/*      THIS SUBROUTINE FINISHES BIND-FAILURE PROCESSING AFTER      */ 00101000
*/*      ALL TCAM MH ERROR PROCESSING CONNECTED WITH THE FAILURE     */ 00102000
*/*      HAS ENDED.  THE ROUTINE IS ENTERED TWICE.                   */ 00103000
*/*                                                                  */ 00104000
*/*      1ST ENTRY (CALLED BY PLCBRTN):                              */ 00105000
*/*         THE PLCB'S ERB IS TPOSTED TO REQUEST A BUFFER.           */ 00106000
*/*                                                                  */ 00107000
*/*      2ND ENTRY (CALLED BY ERBRTNX):                              */ 00108000
*/*         .  A BIND-FAILED PIU IS BUILT & SENT TO THE SSCP.        */ 00109000
*/*         .  END-OF-SESSION CLEAN-UP IS DONE.              @YM08450*/ 00110000
*/*                                                                  */ 00111000
*/********************************************************************/ 00112000
*                                                                  0567 00113000
*   IF LCBERBCH=0 THEN              /* NEED TO REQUEST A BUFFER?     */ 00114000
         L     @09,LCBERBCH-1(,RLCB)                               0567 00115000
         LA    @09,0(,@09)                                         0567 00116000
         LTR   @09,@09                                             0567 00117000
         BNZ   @RF00567                                            0567 00118000
*     DO;                           /* YES                           */ 00119000
*       LCBRBCT2=1;                 /* NUMBER OF UNITS NEEDED        */ 00120000
         MVI   LCBRBCT2(RLCB),X'01'                                0569 00121000
*       CALL TPOSTERB;              /* REQUEST A BUFFER              */ 00122000
         BAL   @14,TPOSTERB                                        0570 00123000
*     END;                                                         0571 00124000
*   ELSE                            /* WE ALREADY HAVE A BUFFER      */ 00125000
*     DO;                           /* SEND BIND-FAILED PIU TO SSCP  */ 00126000
         B     @RC00567                                            0572 00127000
@RF00567 DS    0H                                                  0573 00128000
*       RPRF=LCBERBCH;              /* BUFFER ADDRESS                */ 00129000
         L     RPRF,LCBERBCH-1(,RLCB)                              0573 00130000
         LA    RPRF,0(,RPRF)                                       0573 00131000
*       LCBERBCH=0;                 /* CLEAR ERB'S BUFFER POINTER    */ 00132000
         SLR   @09,@09                                             0574 00133000
         STCM  @09,7,LCBERBCH(RLCB)                                0574 00134000
*       RPARM=CD1XBFL;              /* BIND-FAILED REQUEST CODE      */ 00135000
         L     RPARM,@CF00759                                      0575 00136000
*       CALL BLDMULTI;              /* BUILD & TPOST PIU             */ 00137000
         BAL   @14,BLDMULTI                                        0576 00138000
*       CALL SECLNUP;               /* DO END-OF-SESSION CTL BLOCK 0577 00139000
*                                      CLEAN-UP              @YM08450*/ 00140000
         BAL   @14,SECLNUP                                         0577 00141000
*     END;                                                         0578 00142000
*   END BINDFAIL;                   /* RETURN TO CALLER              */ 00143000
@EL00002 DS    0H                                                  0579 00144000
@EF00002 DS    0H                                                  0579 00145000
@ER00002 L     @14,@SA00002                                        0579 00146000
         BR    @14                                                 0579 00147000
         EJECT                                                          00148000
*BLDINITS:                                                         0580 00149000
*   PROC OPTIONS(SAVE(R14));                                       0580 00150000
*                                                                  0580 00151000
BLDINITS ST    @14,@SA00003                                        0580 00152000
*/********************************************************************/ 00153000
*/*                                                                  */ 00154000
*/*      B L D I N I T S   SUBROUTINE                                */ 00155000
*/*                                                                  */ 00156000
*/********************************************************************/ 00157000
*/*                                                                  */ 00158000
*/*      THIS SUBROUTINE BUILDS AN INIT-SELF PIU AND TPOSTS IT       */ 00159000
*/*      TO THE SSCP.                                                */ 00160000
*/*                                                                  */ 00161000
*/*                                                                  */ 00162000
*/********************************************************************/ 00163000
*                                                                  0581 00164000
*   PRFPIUO=1;                      /* OFFSET TO START OF PIU        */ 00165000
         LA    @09,1                                               0581 00166000
         STC   @09,PRFPIUO(,RPRF)                                  0581 00167000
*   RPIU=ADDR(PRFPIUO)+PRFPIUO;     /* PIU ADDRESS                   */ 00168000
         LA    RPIU,PRFPIUO(,RPRF)                                 0582 00169000
         ALR   RPIU,@09                                            0582 00170000
*   CALL THINITS;                   /* BUILD THE TH                  */ 00171000
         BAL   @14,THINITS                                         0583 00172000
*   PIURH=INITSRH;                  /* MOVE IN THE RH                */ 00173000
         MVC   PIURH(3,RPIU),@CB03121                              0584 00174000
*   CALL RUINITS;                   /* BUILD THE RU                  */ 00175000
         BAL   @14,RUINITS                                         0585 00176000
*   CALL TPOSTPIU;                  /* TPOST THE PIU BUFFER TO SSCP  */ 00177000
         BAL   @14,TPOSTPIU                                        0586 00178000
*   END BLDINITS;                   /* RETURN TO CALLER              */ 00179000
@EL00003 DS    0H                                                  0587 00180000
@EF00003 DS    0H                                                  0587 00181000
@ER00003 L     @14,@SA00003                                        0587 00182000
         BR    @14                                                 0587 00183000
         EJECT                                                          00184000
*BLDMULTI:                                                         0588 00185000
*   PROC OPTIONS(SAVE(R14));                                       0588 00186000
*                                                                  0588 00187000
BLDMULTI ST    @14,@SA00004                                        0588 00188000
*/********************************************************************/ 00189000
*/*                                                                  */ 00190000
*/*      B L D M U L T I   SUBROUTINE                                */ 00191000
*/*                                                                  */ 00192000
*/********************************************************************/ 00193000
*/*                                                                  */ 00194000
*/*      THIS SUBROUTINE BUILDS A SESSION-STARTED, SESSION-ENDED,    */ 00195000
*/*      OR BIND-FAILED PIU AND TPOSTS IT TO THE SSCP.               */ 00196000
*/*                                                                  */ 00197000
*/********************************************************************/ 00198000
*                                                                  0589 00199000
*   PRFPIUO=1;                      /* OFFSET TO START OF PIU        */ 00200000
         LA    @09,1                                               0589 00201000
         STC   @09,PRFPIUO(,RPRF)                                  0589 00202000
*   RPIU=ADDR(PRFPIUO)+PRFPIUO;     /* PIU ADDRESS                   */ 00203000
         LA    RPIU,PRFPIUO(,RPRF)                                 0590 00204000
         ALR   RPIU,@09                                            0590 00205000
*                                   /* FIELDS COMMON TO ALL PIU TYPES*/ 00206000
*   TTHBYTE0=NRMFLOTH;              /* NORMAL-FLOW TH                */ 00207000
         MVI   TTHBYTE0(RPIU),X'1C'                                0591 00208000
*   TTHBYTE1=0;                     /*                               */ 00209000
         MVI   TTHBYTE1(RPIU),X'00'                                0592 00210000
*   TTHDAF=SAVTSCPT;                /* SSCP'S TNT INDEX              */ 00211000
         L     @09,AVTSAVTP(,RAVT)                                 0593 00212000
         MVC   TTHDAF(2,RPIU),SAVTSCPT(@09)                        0593 00213000
*   TTHOAF=SIBINDEX;                /* PLU'S TNT INDEX               */ 00214000
         L     @03,SIBPTR                                          0594 00215000
         LH    @03,SIBINDEX(,@03)                                  0594 00216000
         N     @03,@CF03378                                        0594 00217000
         STCM  @03,3,TTHOAF(RPIU)                                  0594 00218000
*   TTHTAG=TNTSLU;                  /* SLU'S TNT INDEX               */ 00219000
         MVC   TTHTAG(2,RPIU),TNTSLU                               0595 00220000
*   PIURH=MULTIRH;                  /* MOVE IN RH CONSTANT           */ 00221000
         MVC   PIURH(3,RPIU),@CB03123                              0596 00222000
*   PIURU(1:3)=RPARM;               /* RU REQUEST CODE               */ 00223000
         STCM  RPARM,7,PIURU(RPIU)                                 0597 00224000
*                                   /* GET NETWORK ADDRESSES         */ 00225000
*   RPARM=SIBINDEX;                 /* PLU'S TNT INDEX               */ 00226000
         LR    RPARM,@03                                           0598 00227000
*   CALL IEDIAP04;                  /* CONVERT IT                    */ 00228000
         L     @15,SAVTTNTX(,@09)                                  0599 00229000
         BALR  @14,@15                                             0599 00230000
*   PLUNA=R15;                      /* SAVE PLU'S NETWORK ADDRESS    */ 00231000
         LR    PLUNA,R15                                           0600 00232000
*   RPARM=TNTSLU;                   /* SLU'S TNT INDEX               */ 00233000
         LH    RPARM,TNTSLU                                        0601 00234000
         N     RPARM,@CF03378                                      0601 00235000
*   CALL IEDIAP04;                  /* CONVERT IT                    */ 00236000
         L     @09,AVTSAVTP(,RAVT)                                 0602 00237000
         L     @15,SAVTTNTX(,@09)                                  0602 00238000
         BALR  @14,@15                                             0602 00239000
*   SLUNA=R15;                      /* SAVE SLU'S NETWORK ADDRESS    */ 00240000
         LR    SLUNA,R15                                           0603 00241000
*   IF PIURU(1:3)=CD1XBFL THEN      /* BIND-FAILED PIU?              */ 00242000
         CLC   PIURU(3,RPIU),@CB00759                              0604 00243000
         BNE   @RF00604                                            0604 00244000
*     DO;                           /* YES                           */ 00245000
*       TTHDCF=LENGTH(IEDXBFL)+LENGTH(PIURH);/* TH DATA COUNT FIELD  */ 00246000
         MVC   TTHDCF(2,RPIU),@CH00174                             0606 00247000
*       XBFLSNS=LCBBFSNS;           /* SENSE VALUE FROM BIND RESPONSE*/ 00248000
         LA    @09,13                                              0607 00249000
         ALR   @09,RPIU                                            0607 00250000
         MVC   XBFLSNS(4,@09),LCBBFSNS(RLCB)                       0607 00251000
*       XBFLRSN=BFLRSN;             /* REASON CODE                   */ 00252000
         MVI   XBFLRSN(@09),X'20'                                  0608 00253000
*       XBFLKEY=BFLKEY;             /* KEY                           */ 00254000
         MVI   XBFLKEY(@09),X'07'                                  0609 00255000
*       XBFLPNA=PLUNA;              /* PLU'S NETWORK ADDRESS         */ 00256000
         STCM  PLUNA,3,XBFLPNA(@09)                                0610 00257000
*       XBFLSNA=SLUNA;              /* SLU'S NETWORK ADDRESS         */ 00258000
         STCM  SLUNA,3,XBFLSNA(@09)                                0611 00259000
*     END;                                                         0612 00260000
*   ELSE                            /* SESSN-STARTED OR SESSN-ENDED  */ 00261000
*     DO;                           /* (BOTH HAVE SAME FORMAT)       */ 00262000
         B     @RC00604                                            0613 00263000
@RF00604 DS    0H                                                  0614 00264000
*       TTHDCF=LENGTH(IEDXSST)+LENGTH(PIURH);/* TH DATA COUNT FIELD  */ 00265000
         MVC   TTHDCF(2,RPIU),@CH03367                             0614 00266000
*       XSSTKEY=SSTKEY;             /* KEY                           */ 00267000
         LA    @09,13                                              0615 00268000
         ALR   @09,RPIU                                            0615 00269000
         MVI   XSSTKEY(@09),X'07'                                  0615 00270000
*       XSSTPNA=PLUNA;              /* PLU'S NETWORK ADDRESS         */ 00271000
         STCM  PLUNA,3,XSSTPNA(@09)                                0616 00272000
*       XSSTSNA=SLUNA;              /* SLU'S NETWORK ADDRESS         */ 00273000
         STCM  SLUNA,3,XSSTSNA(@09)                                0617 00274000
*     END;                                                         0618 00275000
*   CALL TPOSTPIU;                  /* TPOST PIU BUFFER TO SSCP      */ 00276000
@RC00604 BAL   @14,TPOSTPIU                                        0619 00277000
*   END BLDMULTI;                   /* RETURN TO CALLER              */ 00278000
@EL00004 DS    0H                                                  0620 00279000
@EF00004 DS    0H                                                  0620 00280000
@ER00004 L     @14,@SA00004                                        0620 00281000
         BR    @14                                                 0620 00282000
         EJECT                                                          00283000
*BLDSHUT:                                                          0621 00284000
*   PROC OPTIONS(SAVE(R14));                                       0621 00285000
*                                                                  0621 00286000
BLDSHUT  ST    @14,@SA00005                                        0621 00287000
*/********************************************************************/ 00288000
*/*                                                                  */ 00289000
*/*      B L D S H U T   SUBROUTINE                                  */ 00290000
*/*                                                                  */ 00291000
*/********************************************************************/ 00292000
*/*                                                                  */ 00293000
*/*      THIS SUBROUTINE BUILDS A SHUTDOWN PIU AND TPOSTS IT         */ 00294000
*/*      TO IEDDFOUT TO BE SENT TO THE SLU.                          */ 00295000
*/*                                                                  */ 00296000
*/********************************************************************/ 00297000
*                                                                  0622 00298000
*   PRFPIUO=1;                      /* OFFSET TO START OF PIU        */ 00299000
         LA    @09,1                                               0622 00300000
         STC   @09,PRFPIUO(,RPRF)                                  0622 00301000
*   RPIU=ADDR(PRFPIUO)+PRFPIUO;     /* START-OF-PIU ADDRESS          */ 00302000
         LA    RPIU,PRFPIUO(,RPRF)                                 0623 00303000
         ALR   RPIU,@09                                            0623 00304000
*                                   /* BUILD THE PIU                 */ 00305000
*   TTHBYTE0=EXPFLOTH;              /* EXPEDITED-FLOW TH             */ 00306000
         MVI   TTHBYTE0(RPIU),X'1D'                                0624 00307000
*   TTHBYTE1=0;                     /*                               */ 00308000
         MVI   TTHBYTE1(RPIU),X'00'                                0625 00309000
*   TTHDAF=TNTSLU;                  /* SLU'S TNT INDEX               */ 00310000
         MVC   TTHDAF(2,RPIU),TNTSLU                               0626 00311000
*   TTHOAF=SIBINDEX;                /* PLU'S TNT INDEX               */ 00312000
         L     @09,SIBPTR                                          0627 00313000
         MVC   TTHOAF(2,RPIU),SIBINDEX(@09)                        0627 00314000
*   TTHDCF=LENGTH(PIURH)+1;         /* DATA COUNT                    */ 00315000
         MVC   TTHDCF(2,RPIU),@CH00255                             0628 00316000
*   PIURH=SHUTDRH;                  /* SET UP RH                     */ 00317000
         MVC   PIURH(3,RPIU),@CB03125                              0629 00318000
*   PIURU(1:1)=CD1SHUTD;            /* RU REQUEST CODE               */ 00319000
         MVI   PIURU(RPIU),X'C0'                                   0630 00320000
*   PRF2TNTI=ON;                    /* INDICATE OAF & DAF ARE TNT  0631 00321000
*                                      OFFSETS               @YM08039   00322000
*                                      TPOST PIU BUFFER TO IEDDFOUT  */ 00323000
         LR    @09,RPRF                                            0631 00324000
         AL    @09,@CF03379                                        0631 00325000
         OI    PRF2TNTI(@09),B'10000000'                           0631 00326000
*   PRFQCBA=SAVTDFCO;               /* QCB ADDRESS                   */ 00327000
         L     @09,AVTSAVTP(,RAVT)                                 0632 00328000
         MVC   PRFQCBA(3,RPRF),SAVTDFCO+1(@09)                     0632 00329000
*   PRFPRI=PRIOUTBD;                /* PRIORITY                      */ 00330000
         MVI   PRFPRI(RPRF),X'E4'                                  0633 00331000
*   RPARM=RPRF;                     /* ELEMENT ADDRESS FOR TPOST     */ 00332000
         LR    RPARM,RPRF                                          0634 00333000
*   CALL DSPPOSTR;                  /* TPOST                         */ 00334000
         LA    @15,DSPPOSTR(,RDISP)                                0635 00335000
         BALR  @14,@15                                             0635 00336000
*   END BLDSHUT;                    /* RETURN TO CALLER              */ 00337000
@EL00005 DS    0H                                                  0636 00338000
@EF00005 DS    0H                                                  0636 00339000
@ER00005 L     @14,@SA00005                                        0636 00340000
         BR    @14                                                 0636 00341000
         EJECT                                                          00342000
*CHKSEND:                                                          0637 00343000
*   PROC OPTIONS(SAVE(R14));                                       0637 00344000
*                                                                  0637 00345000
CHKSEND  ST    @14,12(,@13)                                        0637 00346000
*/********************************************************************/ 00347000
*/*                                                                  */ 00348000
*/*      C H K S E N D   SUBROUTINE                                  */ 00349000
*/*                                                                  */ 00350000
*/********************************************************************/ 00351000
*/*                                                                  */ 00352000
*/*      THIS SUBROUTINE DETERMINES THE ANSWER TO ONE        @YM08450*/ 00353000
*/*      QUESTION:  "IF THE PLCB IS TPOSTED TO ITSELF,       @YM08450*/ 00354000
*/*      WILL THE SEND SCHEDULER TRY TO SEND A MESSAGE?".            */ 00355000
*/*      SENDSW IS SET TO INDICATE THE ANSWER (ON = YES).            */ 00356000
*/*                                                                  */ 00357000
*/********************************************************************/ 00358000
*                                                                  0638 00359000
*   NCPTTE=LCBNCPPT-LENGTH(IEDNTRM);/* GET NCP'S TTE ADDRESS         */ 00360000
         L     @09,LCBNCPPT-1(,RLCB)                               0638 00361000
         LA    @09,0(,@09)                                         0638 00362000
         SL    @09,@CF02842                                        0638 00363000
         ST    @09,NCPTTE                                          0638 00364000
*   IF(NCPTTE->TRMNCPCL=ON&NCPTTE->TRMQUCKN=ON)|/* NCP CLOSING?      */ 00365000
*       (AVTCLOSN=ON&AVTQUCKN=ON)|  /* ... OR TCAM CLOSING DOWN?     */ 00366000
*       QCBTRMHO=ON THEN            /* ... OR TERMINAL HELD?         */ 00367000
         TM    TRMNCPCL(@09),B'01001000'                           0639 00368000
         BO    @RT00639                                            0639 00369000
         TM    AVTCLOSN(RAVT),B'00000110'                          0639 00370000
         BO    @RT00639                                            0639 00371000
         L     @09,IEDQQPTR                                        0639 00372000
         TM    QCBTRMHO(@09),B'01000000'                           0639 00373000
         BO    @RT00639                                            0639 00374000
*     ;                             /* YES. LEAVE SEND SWITCH OFF    */ 00375000
*   ELSE                                                           0641 00376000
*     IF QCBTSQ=OFF&                /* IS THERE AN INITIATE-MODE     */ 00377000
*         QCBINSRC^=QCBPTR THEN     /* ... MESSAGE TO BE SENT?       */ 00378000
         L     @09,IEDQQPTR                                        0641 00379000
         TM    QCBTSQ(@09),B'10000000'                             0641 00380000
         BNZ   @RF00641                                            0641 00381000
         L     @09,QCBINSRC-1(,@09)                                0641 00382000
         LA    @09,0(,@09)                                         0641 00383000
         C     @09,QCBPTR                                          0641 00384000
         BE    @RF00641                                            0641 00385000
*       SENDSW=ON;                  /* YES, TURN ON SEND SWITCH      */ 00386000
         OI    SENDSW,B'00001000'                                  0642 00387000
*     ELSE                                                         0643 00388000
*       IF QCBHELD=ON THEN          /* REUS IN PROGRESS?             */ 00389000
         B     @RC00641                                            0643 00390000
@RF00641 L     @09,IEDQQPTR                                        0643 00391000
         TM    QCBHELD(@09),B'00000001'                            0643 00392000
         BO    @RT00643                                            0643 00393000
*         ;                         /* YES, LEAVE SEND SWITCH OFF    */ 00394000
*       ELSE                        /* SEE IF THERE ARE ANY MESSAGES */ 00395000
*         DO;                       /* ... QUEUED TO BE SENT         */ 00396000
*           EXITSW=OFF;             /* INITIALIZE FOR                */ 00397000
         NI    EXITSW,B'11011111'                                  0646 00398000
*           RPARM=ADDR(IEDQPQCB);   /* ... LOOP                      */ 00399000
         LA    RPARM,40                                            0647 00400000
         AL    RPARM,IEDQQPTR                                      0647 00401000
*           DO UNTIL(EXITSW=ON);    /* LOOP THRU PRIORITY QCBS       */ 00402000
@DL00648 DS    0H                                                  0649 00403000
*             IF RPARM->QCBFFEFO>0 THEN/* ANY MESSAGES TO BE SENT?   */ 00404000
         ICM   @09,7,QCBFFEFO(RPARM)                               0649 00405000
         BZ    @RF00649                                            0649 00406000
*               DO;                 /* YES                           */ 00407000
*                 SENDSW=ON;        /* TURN ON SEND SWITCH           */ 00408000
*                 EXITSW=ON;        /* PREPARE TO EXIT FROM LOOP     */ 00409000
         OI    SENDSW,B'00101000'                                  0652 00410000
*               END;                                               0653 00411000
*             ELSE                                                 0654 00412000
*               IF RPARM->QCBPRIPQ=0 THEN/* LAST PRI QCB?            */ 00413000
         B     @RC00649                                            0654 00414000
@RF00649 CLI   QCBPRIPQ(RPARM),0                                   0654 00415000
         BNE   @RF00654                                            0654 00416000
*                 EXITSW=ON;        /* YES, SET UP TO EXIT FROM LOOP */ 00417000
         OI    EXITSW,B'00100000'                                  0655 00418000
*               ELSE                                               0656 00419000
*                 RPARM=RPARM+LENGTH(IEDQPQCB);/* NO, GET NEXT ONE   */ 00420000
         B     @RC00654                                            0656 00421000
@RF00654 AL    RPARM,@CF02302                                      0656 00422000
*           END;                                                   0657 00423000
@RC00654 DS    0H                                                  0657 00424000
@RC00649 DS    0H                                                  0657 00425000
@DE00648 TM    EXITSW,B'00100000'                                  0657 00426000
         BNO   @DL00648                                            0657 00427000
*         END;                                                     0658 00428000
*   END CHKSEND;                    /* RETURN TO CALLER              */ 00429000
@EL00006 DS    0H                                                  0659 00430000
@EF00006 DS    0H                                                  0659 00431000
@ER00006 L     @14,12(,@13)                                        0659 00432000
         BR    @14                                                 0659 00433000
         EJECT                                                          00434000
*CINITRTN:                                                         0660 00435000
*   PROC OPTIONS(SAVE(R14));                                       0660 00436000
*                                                                  0660 00437000
CINITRTN ST    @14,@SA00007                                        0660 00438000
*/********************************************************************/ 00439000
*/*                                                                  */ 00440000
*/*      C I N I T R T N   SUBROUTINE                                */ 00441000
*/*                                                                  */ 00442000
*/********************************************************************/ 00443000
*/*                                                                  */ 00444000
*/*      THIS SUBROUTINE PROCESSES A CINIT PIU RECEIVED FROM THE     */ 00445000
*/*      SSCP.  THE SUBROUTINE ---                                   */ 00446000
*/*                                                                  */ 00447000
*/*         .  OBTAINS A PLCB.                                       */ 00448000
*/*                                                                  */ 00449000
*/*         .  (IF THERE IS LOGON DATA) GETS AN EMPTY BUFFER,        */ 00450000
*/*            AND SAVES THE LOGON DATA.                             */ 00451000
*/*                                                                  */ 00452000
*/*         .  TPOSTS THE CINIT PIU'S BUFFER TO IEDSCLU AS A         */ 00453000
*/*            SIGNAL TO SEND BIND AND SDT PIUS TO THE SLU.          */ 00454000
*/*                                                                  */ 00455000
*/*      AS MANY AS 3 SEPARATE ENTRIES TO THIS SUBROUTINE MAY BE     */ 00456000
*/*      NEEDED TO ACCOMPLISH THESE JOBS.  CALLS MAY OCCUR FROM      */ 00457000
*/*      PIURTN, PLCBRTN AND ERBRTNX.                                */ 00458000
*/*                                                                  */ 00459000
*/********************************************************************/ 00460000
*                                                                  0661 00461000
*   IF QCBPLCBN=OFF THEN            /* DO WE NEED A PLCB?            */ 00462000
         L     @09,IEDQQPTR                                        0661 00463000
         SL    @09,@CF00044                                        0661 00464000
         TM    QCBPLCBN(@09),B'10000000'                           0661 00465000
         BNZ   @RF00661                                            0661 00466000
*     DO;                           /* YES                           */ 00467000
*       QCBNDXRQ=ON;                /* SPECIAL INDEXED REQUEST       */ 00468000
         OI    QCBNDXRQ(@09),B'00000100'                           0663 00469000
*       CALL RQSTPLCB;              /* REQUEST A PLCB                */ 00470000
         BAL   @14,RQSTPLCB                                        0664 00471000
*     END;                                                         0665 00472000
*   IF QCBPLCBN=OFF THEN            /* WAS OUR PLCB REQUEST QUEUED?  */ 00473000
@RF00661 L     @09,IEDQQPTR                                        0666 00474000
         AL    @09,@CF03385                                        0666 00475000
         TM    QCBPLCBN-4(@09),B'10000000'                         0666 00476000
         BNZ   @RF00666                                            0666 00477000
*     DO;                           /* YES, WE'LL GET PLCB LATER     */ 00478000
*       PRFLINK=SAVTLUSP;           /* INSERT THE CINIT'S BUFFER     */ 00479000
         L     @09,AVTSAVTP(,RAVT)                                 0668 00480000
         MVC   PRFLINK(3,RPRF),SAVTLUSP+1(@09)                     0668 00481000
*       SAVTLUSP=RPRF;              /* ... ON TOP OF CHAIN           */ 00482000
         ST    RPRF,SAVTLUSP(,@09)                                 0669 00483000
*     END;                                                         0670 00484000
         EJECT                                                          00485000
*   ELSE                            /* WE HAVE A PLCB ASSIGNED       */ 00486000
*     DO;                                                          0671 00487000
         B     @RC00666                                            0671 00488000
@RF00666 DS    0H                                                  0672 00489000
*       QCBNDXRQ=OFF;               /* RESET INDEXED-REQUEST FLAG    */ 00490000
         L     @09,IEDQQPTR                                        0672 00491000
         AL    @09,@CF03385                                        0672 00492000
         NI    QCBNDXRQ-4(@09),B'11111011'                         0672 00493000
*       LCBBFRSV=0;                 /* CLEAR POINTER SAVE AREA       */ 00494000
         SLR   @09,@09                                             0673 00495000
         STCM  @09,7,LCBBFRSV(RLCB)                                0673 00496000
*       IF TRMLUREQ=ON THEN         /* EXTERNAL CINIT?               */ 00497000
         TM    TRMLUREQ(RTRM),B'00000100'                          0674 00498000
         BNO   @RF00674                                            0674 00499000
*         DO;                       /* YES                           */ 00500000
*           IF LCBERBCH=0 THEN      /* DO WE HAVE A BUFFER?          */ 00501000
         L     @09,LCBERBCH-1(,RLCB)                               0676 00502000
         LA    @09,0(,@09)                                         0676 00503000
         LTR   @09,@09                                             0676 00504000
         BNZ   @RF00676                                            0676 00505000
*             DO;                   /* NO                            */ 00506000
*               CALL FNDLGDTA;      /* LOCATE LOGON DATA (THE USER-     00507000
*                                      DATA FIELD) IN THE CINIT PIU  */ 00508000
         BAL   @14,FNDLGDTA                                        0678 00509000
*               IF DATAPTR->VARLNGTH>0 THEN/* IS THERE ANY DATA?     */ 00510000
         L     @09,DATAPTR                                         0679 00511000
         CLI   VARLNGTH(@09),0                                     0679 00512000
         BNH   @RF00679                                            0679 00513000
*                 DO;               /* YES                           */ 00514000
*                                   /* CALC NUMBER OF UNITS NEEDED   */ 00515000
*                   RPARMF=AVTHDRSZ+DATAPTR->VARLNGTH;/* BYTE COUNT  */ 00516000
         LA    RPARMF,30                                           0681 00517000
         SLR   @07,@07                                             0681 00518000
         IC    @07,VARLNGTH(,@09)                                  0681 00519000
         ALR   RPARMF,@07                                          0681 00520000
*                   CALL UNITCNT;   /* CONVERT BYTE COUNT TO UNITS   */ 00521000
         BAL   @14,UNITCNT                                         0682 00522000
*                   LCBRBCT2=R0;    /* UNIT COUNT                    */ 00523000
         STC   R0,LCBRBCT2(,RLCB)                                  0683 00524000
*                   CALL TPOSTERB;  /* REQUEST A BUFFER              */ 00525000
         BAL   @14,TPOSTERB                                        0684 00526000
*                   LCBBFRSV=RPRF;  /* SAVE PIU'S BUFFER ADDRESS FOR    00527000
*                                      LATER USE BY ERBRTNX          */ 00528000
         STCM  RPRF,7,LCBBFRSV(RLCB)                               0685 00529000
*                 END;                                             0686 00530000
*               IF TRMSESRQ=ON THEN /* INTERNAL INIT IN PROGRESS?    */ 00531000
@RF00679 TM    TRMSESRQ(RTRM),B'00001000'                          0687 00532000
         BNO   @RF00687                                            0687 00533000
*                 CALL DEQUEQCB;    /* YES, STOP THE QCB FROM      0688 00534000
*                                      GENERATING AN INIT-SELF (IF IT   00535000
*                                      HASN'T ALREADY DONE SO)       */ 00536000
         BAL   @14,DEQUEQCB                                        0688 00537000
*             END;                                                 0689 00538000
*           ELSE                    /* WE ALREADY HAVE A BUFFER      */ 00539000
*             DO;                                                  0690 00540000
         B     @RC00676                                            0690 00541000
@RF00676 DS    0H                                                  0691 00542000
*               CALL FNDLGDTA;      /* LOCATE THE LOGON DATA         */ 00543000
         BAL   @14,FNDLGDTA                                        0691 00544000
*               CALL SAVLGDTA;      /* MOVE THE LOGON DATA INTO THE     00545000
*                                      BUFFER & SAVE BUFFER ADDRESS  */ 00546000
         BAL   @14,SAVLGDTA                                        0692 00547000
*             END;                                                 0693 00548000
*         END;                                                     0694 00549000
@RC00676 DS    0H                                                  0695 00550000
*       IF LCBBFRSV=0 THEN          /* READY TO BIND?                */ 00551000
@RF00674 L     @09,LCBBFRSV-1(,RLCB)                               0695 00552000
         LA    @09,0(,@09)                                         0695 00553000
         LTR   @09,@09                                             0695 00554000
         BNZ   @RF00695                                            0695 00555000
*         DO;                       /* YES, TPOST CINIT TO IEDSCLU   */ 00556000
*           PRFQCBA=SAVTSCLU;       /* QCB ADDRESS                   */ 00557000
         L     @09,AVTSAVTP(,RAVT)                                 0697 00558000
         MVC   PRFQCBA(3,RPRF),SAVTSCLU+1(@09)                     0697 00559000
*           PRFPRI=PRISCPIN;        /* PRIORITY                      */ 00560000
         MVI   PRFPRI(RPRF),X'E5'                                  0698 00561000
*           RPARM=RPRF;             /* ELEMENT ADDRESSS              */ 00562000
         LR    RPARM,RPRF                                          0699 00563000
*           CALL DSPPOSTR;          /* TPOST THE ELEMENT             */ 00564000
         LA    @15,DSPPOSTR(,RDISP)                                0700 00565000
         BALR  @14,@15                                             0700 00566000
*         END;                                                     0701 00567000
*     END;                                                         0702 00568000
*   END CINITRTN;                   /* RETURN TO CALLER              */ 00569000
@EL00007 DS    0H                                                  0703 00570000
@EF00007 DS    0H                                                  0703 00571000
@ER00007 L     @14,@SA00007                                        0703 00572000
         BR    @14                                                 0703 00573000
         EJECT                                                          00574000
*CTERMRTN:                                                         0704 00575000
*   PROC OPTIONS(SAVE(R14));                                       0704 00576000
*                                                                  0704 00577000
CTERMRTN ST    @14,@SA00008                                        0704 00578000
*/********************************************************************/ 00579000
*/*                                                                  */ 00580000
*/*      C T E R M R T N   SUBROUTINE                                */ 00581000
*/*                                                                  */ 00582000
*/********************************************************************/ 00583000
*/*                                                                  */ 00584000
*/*      THIS SUBROUTINE PROCESSES A CTERM PIU RECEIVED FROM THE     */ 00585000
*/*      SSCP.  NOTE THAT THERE ARE TWO TYPES OF TERMINATION @YM08991*/ 00586000
*/*      WHICH MAY BE REQUESTED --- ORDERLY AND FORCED.      @YM08991*/ 00587000
*/*                                                          @YM08991*/ 00588000
*/*      ORDERLY TERMINATION:  IF THE LU IS CURRENTLY        @YM08991*/ 00589000
*/*      INVOLVED IN ANY MESSAGE TRAFFIC, WE CAN ALLOW THE   @YM08991*/ 00590000
*/*      CURRENT MESSAGE TO END NORMALLY BEFORE BEGINNING    @YM08991*/ 00591000
*/*      SESSION TERMINATION.                                @YM08991*/ 00592000
*/*                                                          @YM08991*/ 00593000
*/*      FORCED TERMINATION:  WE MUST FORCE THE CLEANING UP  @YM08991*/ 00594000
*/*      OF ANY IN-PROCESS MESSAGE TRAFFIC IMMEDIATELY       @YM08991*/ 00595000
*/*      WITHOUT WAITING FOR ANY CURRENT MESSAGE TO END.     @YM08991*/ 00596000
*/*                                                                  */ 00597000
*/********************************************************************/ 00598000
*                                                                  0705 00599000
*   IF XCTMTERM=XCTMTRMO THEN       /* ORDERLY TERMINATE?    @YM08991*/ 00600000
         TM    XCTMTERM+13(RPIU),B'00010000'                       0705 00601000
         BNO   @RF00705                                            0705 00602000
         TM    XCTMTERM+13(RPIU),B'11100000'                       0705 00603000
         BNZ   @RF00705                                            0705 00604000
*     DO;                           /* YES                   @YM08991   00605000
*                                      FREE CTERM PIU'S BUFFER       */ 00606000
*       RPARM=RPRF;                 /* BUFFER ADDRESS                */ 00607000
         LR    RPARM,RPRF                                          0707 00608000
*       CALL FREEBUF;               /* FREE THE BUFFER               */ 00609000
         BAL   @14,FREEBUF                                         0708 00610000
*       IF SIBPTR>0&                /* LU-LU SIB ASSIGNED? &         */ 00611000
*           SIBSSTAT=SIBSACTV THEN  /* ... SESSION STATE = 'ACTIVE'? */ 00612000
         L     @09,SIBPTR                                          0709 00613000
         LTR   @09,@09                                             0709 00614000
         BNP   @RF00709                                            0709 00615000
         TM    SIBSSTAT(@09),B'00011100'                           0709 00616000
         BNO   @RF00709                                            0709 00617000
*         DO;                       /* YES, PRIME LOGOFF     @YM09036*/ 00618000
*           SIBLGOFF=ON;            /* SET LOGOFF BIT        @YM09036*/ 00619000
         OI    SIBLGOFF(@09),B'10000000'                           0711 00620000
*           CALL PRMLGOFF;          /* CALL PRIMING ROUTINE  @YM09036*/ 00621000
         BAL   @14,PRMLGOFF                                        0712 00622000
*         END;                      /* END PRIME PROCESSING  @YM09036*/ 00623000
*     END;                          /*                       @YM08991*/ 00624000
*   ELSE                            /* FORCED TERMINATE      @YM08991*/ 00625000
*     DO;                           /*                       @YM08991*/ 00626000
         B     @RC00705                                            0715 00627000
@RF00705 DS    0H                                                  0716 00628000
*       BUSYSW=OFF;                 /* INITIALIZE SWITCH     @YM08991*/ 00629000
         NI    BUSYSW,B'11101111'                                  0716 00630000
*       IF SIBPTR>0 THEN            /* LU-LU SIB ASSIGNED?   @YM08991*/ 00631000
         L     @09,SIBPTR                                          0717 00632000
         LTR   @09,@09                                             0717 00633000
         BNP   @RF00717                                            0717 00634000
*         DO;                       /* YES                   @YM08991*/ 00635000
*           SIBFTERM=ON;            /* SET FORCED TERMINATE IN     0719 00636000
*                                      PROGRESS              @YM08991*/ 00637000
         OI    SIBFTERM(@09),B'00100000'                           0719 00638000
*           IF SIBSSTAT=SIBSACTV&   /* SESSION ACTIVE? &     @YM08991*/ 00639000
*               SIBDSTAT=SIBDACTV THEN/* DATA TRAFFIC ACTIVE?      0720 00640000
*                                                            @YM08991*/ 00641000
         TM    SIBSSTAT(@09),B'00011100'                           0720 00642000
         BNO   @RF00720                                            0720 00643000
         TM    SIBDSTAT(@09),B'00000011'                           0720 00644000
         BNO   @RF00720                                            0720 00645000
*             DO;                   /* YES                   @YM08991*/ 00646000
*               SIBLGOFF=ON;        /* SET LOGOFF FLAG       @YM08991*/ 00647000
         OI    SIBLGOFF(@09),B'10000000'                           0722 00648000
*               CALL PRMLGOFF;      /* PRIME LOGOFF PROCESS  @YM08991*/ 00649000
         BAL   @14,PRMLGOFF                                        0723 00650000
*             END;                  /*                       @YM08991*/ 00651000
*         END;                      /*                       @YM08991*/ 00652000
@RF00720 DS    0H                                                  0726 00653000
*       IF BUSYSW=ON THEN           /* PLCB-BUSY CONDITION FOUND BY     00654000
*                                      PRMLGOFF SUBOURTINE   @YM08991*/ 00655000
@RF00717 TM    BUSYSW,B'00010000'                                  0726 00656000
         BNO   @RF00726                                            0726 00657000
*         DO;                       /* YES, REQUEST IEDNCU TO CHECK     00658000
*                                      STATES & CLEAN UP     @YM08991   00659000
*                                      BLD CLEAN-UP ELEMENT  @YM08991*/ 00660000
*           IEDCUREQ=''B;           /* CLEAR THE BUFFER      @YM08991*/ 00661000
         XC    IEDCUREQ(44,RPRF),IEDCUREQ(RPRF)                    0728 00662000
*           CURQCB=SAVTCLUP;        /* NCU QCB ADDR          @YM08991*/ 00663000
         L     @09,AVTSAVTP(,RAVT)                                 0729 00664000
         MVC   CURQCB(3,RPRF),SAVTCLUP+1(@09)                      0729 00665000
*           CURPRI=PRICUREQ;        /* CLEANUP PRIORITY      @YM08991*/ 00666000
         MVI   CURPRI(RPRF),X'C4'                                  0730 00667000
*           CURLINK=PRFINTIC;       /* INCALID TIC           @YM08991*/ 00668000
         MVC   CURLINK(4,RPRF),@CF02080                            0731 00669000
*           CURTERM=ON;             /* SET TERMINATE FLAG    @YM08991*/ 00670000
         OI    CURTERM(RPRF),B'00000001'                           0732 00671000
*           CURTTCIN=TNTSLU;        /* SLU'S TNT INDEX       @YM08991*/ 00672000
         LH    @09,TNTSLU                                          0733 00673000
         N     @09,@CF03378                                        0733 00674000
         STH   @09,CURTTCIN(,RPRF)                                 0733 00675000
*           CURTRM=ADDR(IEDQTRM);   /* SLU'S TTE ADDR        @YM08991*/ 00676000
         LA    @09,IEDQTRM(,RTRM)                                  0734 00677000
         ST    @09,CURTRM(,RPRF)                                   0734 00678000
*           RPARM=RPRF;             /* TPOST THE REQUEST     @YM08991*/ 00679000
         LR    RPARM,RPRF                                          0735 00680000
*           CALL DSPPOSTR;          /* ELEMENT TO IEDNCU     @YM08991*/ 00681000
         LA    @15,DSPPOSTR(,RDISP)                                0736 00682000
         BALR  @14,@15                                             0736 00683000
*         END;                      /*                       @YM08991*/ 00684000
*       ELSE                        /*                       @YM08991*/ 00685000
*         DO;                       /*                       @YM08991*/ 00686000
         B     @RC00726                                            0738 00687000
@RF00726 DS    0H                                                  0739 00688000
*           RPARM=RPRF;             /* FREE THE CTERM'S      @YM08991*/ 00689000
         LR    RPARM,RPRF                                          0739 00690000
*           CALL FREEBUF;           /* ... BUFFER            @YM08991*/ 00691000
         BAL   @14,FREEBUF                                         0740 00692000
*         END;                      /*                       @YM08991*/ 00693000
*     END;                          /*                       @YM08991*/ 00694000
*   END CTERMRTN;                   /* RETURN TO CALLER              */ 00695000
@EL00008 DS    0H                                                  0743 00696000
@EF00008 DS    0H                                                  0743 00697000
@ER00008 L     @14,@SA00008                                        0743 00698000
         BR    @14                                                 0743 00699000
         EJECT                                                          00700000
*CTLBLKS:                                                          0744 00701000
*   PROC OPTIONS(SAVE(R14));                                       0744 00702000
*                                                                  0744 00703000
CTLBLKS  ST    @14,@SA00009                                        0744 00704000
