*/********************************************************************/ 03326000
*                                                                  1415 03327000
*   PRFQCBA=ADDR(AVTOPCOB);         /* OP CONTROL QCB ADDRESS        */ 03328000
         LA    @09,AVTOPCOB(,RAVT)                                 1415 03329000
         STCM  @09,7,PRFQCBA(RPRF)                                 1415 03330000
*   PRFPRI=PRISSPIU;                /* PRIORITY                      */ 03331000
         MVI   PRFPRI(RPRF),X'E5'                                  1416 03332000
*   RPARM=RPRF;                     /* ELEMENT ADDRESS FOR TPOST     */ 03333000
         LR    RPARM,RPRF                                          1417 03334000
*   CALL DSPPOSTR;                  /* TPOST THE PIU BUFFER          */ 03335000
         LA    @15,DSPPOSTR(,RDISP)                                1418 03336000
         BALR  @14,@15                                             1418 03337000
*   END TPOSTPIU;                   /* RETURN TO CALLER              */ 03338000
