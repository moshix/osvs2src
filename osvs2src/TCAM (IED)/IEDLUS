*/********************************************************************/ 02472000
*/*                                                                  */ 02473000
*/*      P L C B R T N   SUBROUTINE                                  */ 02474000
*/*                                                                  */ 02475000
*/********************************************************************/ 02476000
*/*                                                                  */ 02477000
*/*      THIS SUBROUTINE PROCESSES INPUT PLCBS.                      */ 02478000
*/*                                                                  */ 02479000
*/*      IF NO ELEMENT IS ENQUEUED WAITING FOR THIS PLCB, THE        */ 02480000
*/*      PROCESSING IS DETERMINED BY CHECKING THE SESSION STATE.     */ 02481000
*/*                                                                  */ 02482000
*/*      IF A PIU'S BUFFER IS ENQUEUED WAITING FOR THE PLCB,         */ 02483000
*/*      DEQUEUE THE WAITING PIU BUFFER, AND CALL THE SAME           */ 02484000
*/*      SUBROUTINE WHICH ORIGINALLY REQUESTED THE PLCB.             */ 02485000
*/*                                                                  */ 02486000
*/********************************************************************/ 02487000
*                                                                  1183 02488000
*   RLCB=RPARM;                     /* PLCB ADDRESS                  */ 02489000
         LR    RLCB,RPARM                                          1183 02490000
*   TNTSLU=LCBTTCIN;                /* SLU'S TNT INDEX               */ 02491000
         LH    @09,LCBTTCIN(,RLCB)                                 1184 02492000
         N     @09,@CF03378                                        1184 02493000
         STH   @09,TNTSLU                                          1184 02494000
*   CALL CTLBLKS;                   /* SET UP CONTROL BLOCK POINTERS */ 02495000
         BAL   @14,CTLBLKS                                         1185 02496000
*                                   /* SEE IF A PIU IS ENQUEUED    1186 02497000
*                                      WAITING FOR THIS PLCB         */ 02498000
*   IF QCBNDXRQ=OFF THEN            /* NO PIU ENQUEUED?              */ 02499000
         L     @09,IEDQQPTR                                        1186 02500000
         AL    @09,@CF03385                                        1186 02501000
         TM    QCBNDXRQ-4(@09),B'00000100'                         1186 02502000
         BNZ   @RF01186                                            1186 02503000
*     DO;                           /* TRUE, CHECK SESSION STATE     */ 02504000
*       IF SIBPTR>0 THEN            /* DO WE HAVE AN LU-LU SIB?      */ 02505000
         L     @09,SIBPTR                                          1188 02506000
         LTR   @09,@09                                             1188 02507000
         BNP   @RF01188                                            1188 02508000
*         DO;                       /* YES                           */ 02509000
*           IF SIBSSTAT=SIBSACTV THEN/* SESSION STATE = 'ACTIVE'?    */ 02510000
         TM    SIBSSTAT(@09),B'00011100'                           1190 02511000
         BNO   @RF01190                                            1190 02512000
*             DO;                   /* YES                           */ 02513000
*               IF SIBLGOFF=ON THEN /* LOGOFF BIT SET?               */ 02514000
         TM    SIBLGOFF(@09),B'10000000'                           1192 02515000
         BNO   @RF01192                                            1192 02516000
*                 CALL LGOFF;       /* YES, BEGIN LOGOFF PROCESSING  */ 02517000
         BAL   @14,LGOFF                                           1193 02518000
*               ELSE                                               1194 02519000
*                 DO;               /* NO                            */ 02520000
         B     @RC01192                                            1194 02521000
@RF01192 DS    0H                                                  1195 02522000
*                   SIBLGOFF=ON;    /* SET LOGOFF BIT                */ 02523000
         L     @09,SIBPTR                                          1195 02524000
         OI    SIBLGOFF(@09),B'10000000'                           1195 02525000
*                   LCBQCBA=RLCB;   /* TPOST PLCB TO ITSELF          */ 02526000
         STCM  RLCB,7,LCBQCBA(RLCB)                                1196 02527000
*                   LCBPRI=PRILNFRE;/* ... TO LET SCHEDULERS 'SEE'   */ 02528000
         MVI   LCBPRI(RLCB),X'E0'                                  1197 02529000
*                   RPARM=RLCB;     /* ... THE LOGOFF BIT (THIS LETS */ 02530000
         LR    RPARM,RLCB                                          1198 02531000
*                   CALL DSPPOSTR;  /* ... TSO SESSIONS BE ENDED     */ 02532000
         LA    @15,DSPPOSTR(,RDISP)                                1199 02533000
         BALR  @14,@15                                             1199 02534000
*                 END;              /* ... PROPERLY BEFORE LOGOFF)   */ 02535000
*             END;                                                 1201 02536000
*           ELSE                    /* NO                            */ 02537000
*             CALL BINDFAIL;        /* FINISH BIND-FAILED PROCESSING */ 02538000
         B     @RC01190                                            1202 02539000
@RF01190 BAL   @14,BINDFAIL                                        1202 02540000
*         END;                                                     1203 02541000
*       ELSE                        /* NO LU-LU SIB FOR THIS SLU     */ 02542000
*         CALL ENDNOSIB;            /* FINISH NO-SIB ERROR PROCESSING*/ 02543000
         B     @RC01188                                            1204 02544000
@RF01188 BAL   @14,ENDNOSIB                                        1204 02545000
*     END;                                                         1205 02546000
         EJECT                                                          02547000
*   ELSE                            /* PIU WAITING FOR THIS PLCB     */ 02548000
*     DO;                                                          1206 02549000
         B     @RC01186                                            1206 02550000
@RF01186 DS    0H                                                  1207 02551000
*       QCBNDXRQ=OFF;               /* TURN OFF INDEXED-REQUEST FLAG */ 02552000
         L     @09,IEDQQPTR                                        1207 02553000
         AL    @09,@CF03385                                        1207 02554000
         NI    QCBNDXRQ-4(@09),B'11111011'                         1207 02555000
*       PREVIOUS=ADDR(SAVTLUSP)-LINKOFFS;/* INITIALIZE FOR           */ 02556000
         L     @09,AVTSAVTP(,RAVT)                                 1208 02557000
         LA    @07,SAVTLUSP(,@09)                                  1208 02558000
         SLR   @05,@05                                             1208 02559000
         IC    @05,LINKOFFS                                        1208 02560000
         SLR   @07,@05                                             1208 02561000
         ST    @07,PREVIOUS                                        1208 02562000
*       RPRF=SAVTLUSP;              /* ... SEARCH                    */ 02563000
         L     RPRF,SAVTLUSP(,@09)                                 1209 02564000
*       FOUNDSW=OFF;                /* ... LOOP                      */ 02565000
         NI    FOUNDSW,B'10111111'                                 1210 02566000
*       DO UNTIL(FOUNDSW=ON);       /* FIND & DELINK THE WAITING PIU */ 02567000
@DL01211 DS    0H                                                  1212 02568000
*         RPIU=ADDR(PRFPIUO)+PRFPIUO;/* CURRENT PIU IN CHAIN         */ 02569000
         LA    RPIU,PRFPIUO(,RPRF)                                 1212 02570000
         SLR   @09,@09                                             1212 02571000
         IC    @09,PRFPIUO(,RPRF)                                  1212 02572000
         ALR   RPIU,@09                                            1212 02573000
*         IF TTHTAG=TNTSLU THEN     /* IS PIU FOR THIS SLU?          */ 02574000
         CLC   TTHTAG(2,RPIU),TNTSLU                               1213 02575000
         BNE   @RF01213                                            1213 02576000
*           DO;                     /* YES, DELINK & SET FOUND SWITCH*/ 02577000
*             FOUNDSW=ON;                                          1215 02578000
         OI    FOUNDSW,B'01000000'                                 1215 02579000
*             PREVIOUS->PRFLINK=PRFLINK;                           1216 02580000
         L     @09,PREVIOUS                                        1216 02581000
         MVC   PRFLINK(3,@09),PRFLINK(RPRF)                        1216 02582000
*           END;                                                   1217 02583000
*         ELSE                      /* NO MATCH FOUND YET            */ 02584000
*           DO;                     /* BUMP TO NEXT ELEMENT ON CHAIN */ 02585000
         B     @RC01213                                            1218 02586000
@RF01213 DS    0H                                                  1219 02587000
*             PREVIOUS=RPRF;                                       1219 02588000
         ST    RPRF,PREVIOUS                                       1219 02589000
*             RPRF=PRFLINK;                                        1220 02590000
         L     @09,PRFLINK-1(,RPRF)                                1220 02591000
         LA    @09,0(,@09)                                         1220 02592000
         LR    RPRF,@09                                            1220 02593000
*           END;                                                   1221 02594000
*       END;                                                       1222 02595000
@RC01213 DS    0H                                                  1222 02596000
@DE01211 TM    FOUNDSW,B'01000000'                                 1222 02597000
         BNO   @DL01211                                            1222 02598000
*       INPUTBUF=RPRF;              /* PIU'S BUFFER ADDRESS          */ 02599000
         ST    RPRF,INPUTBUF                                       1223 02600000
*       INPUTPIU=RPIU;              /* START OF PIU                  */ 02601000
         ST    RPIU,INPUTPIU                                       1224 02602000
*                                   /* REPROCESS THE WAITING PIU     */ 02603000
*       IF TRHTYPE=OFF THEN         /* IS PIU A REQUEST?             */ 02604000
         TM    TRHTYPE+10(RPIU),B'10000000'                        1225 02605000
         BNZ   @RF01225                                            1225 02606000
*         CALL CINITRTN;            /* YES, MUST BE A CINIT          */ 02607000
         BAL   @14,CINITRTN                                        1226 02608000
*       ELSE                        /* NO, IT'S A RESPONSE           */ 02609000
*         CALL NOSIBRTN;            /* MUST BE AN INIT-SELF          */ 02610000
         B     @RC01225                                            1227 02611000
@RF01225 BAL   @14,NOSIBRTN                                        1227 02612000
*     END;                                                         1228 02613000
*   END PLCBRTN;                    /* RETURN TO CALLER              */ 02614000
@EL00032 DS    0H                                                  1229 02615000
@EF00032 DS    0H                                                  1229 02616000
@ER00032 L     @14,@SA00032                                        1229 02617000
         BR    @14                                                 1229 02618000
         EJECT                                                          02619000
*PRMLGOFF:                                                         1230 02620000
*   PROC OPTIONS(SAVE(R14));                                       1230 02621000
*                                                                  1230 02622000
PRMLGOFF ST    @14,@SA00033                                        1230 02623000
*/********************************************************************/ 02624000
*/*                                                                  */ 02625000
*/*      P R M L G O F F   SUBROUTINE                                */ 02626000
*/*                                                                  */ 02627000
*/********************************************************************/ 02628000
*/*                                                                  */ 02629000
*/*      THIS SUBROUTINE 'PRIMES' LOGOFF PROCESSING FOR AN LU-LU     */ 02630000
*/*      SESSION.  THE LOGOFF BIT OR THE SHUTDOWN COMPLETE   @YM09036*/ 02631000
*/*      BIT HAS ALREADY BEEN SET, AND THE CURRENT STATE OF  @YM09036*/ 02632000
*/*      THE PLCB WILL BE DETERMINED.                        @YM09036*/ 02633000
*/*                                                                  */ 02634000
*/*      .  IF THE PLCB IS BUSY, NO FURTHER PROCESSING TAKES PLACE   */ 02635000
*/*         NOW.  THE LOGOFF OR SHUTDOWN-COMPLETE BIT WILL   @YM09036*/ 02636000
*/*         CAUSE THE PLCB TO BE TPOSTED TO THIS MODULE LATER WHEN   */ 02637000
*/*         CURRENT OPERATIONS ARE COMPLETE.                         */ 02638000
*/*                                                                  */ 02639000
*/*      .  IF A NON-BUSY PLCB CAN BE OBTAINED IMMEDIATELY, THE      */ 02640000
*/*         LOGOFF SUBROUTINE IS CALLED.                             */ 02641000
*/*                                                                  */ 02642000
*/*      .  OTHERWISE A PLCB IS REQUESTED.  WHEN THE REQUEST IS      */ 02643000
*/*         SATISFIED THE LOGOFF OR SHUTDOWN-COMPLETE BIT    @YM09036*/ 02644000
*/*         WILL CAUSE THE PLCB TO BE TPOSTED TO THIS MODULE.        */ 02645000
*/*                                                                  */ 02646000
*/********************************************************************/ 02647000
*                                                                  1231 02648000
*   IF QCBPLCBN=ON THEN             /* IS A PLCB ASSIGNED TO SLU?    */ 02649000
         L     @09,IEDQQPTR                                        1231 02650000
         AL    @09,@CF03385                                        1231 02651000
         TM    QCBPLCBN-4(@09),B'10000000'                         1231 02652000
         BNO   @RF01231                                            1231 02653000
*     DO;                           /* YES                           */ 02654000
*       IF TRMINPTP=ON THEN         /* IS IT WAITING FOR INPUT?      */ 02655000
         TM    TRMINPTP(RTRM),B'01000000'                          1233 02656000
         BNO   @RF01233                                            1233 02657000
*         DO;                       /* YES                           */ 02658000
*           TRMINPTP=OFF;           /* TURN OFF INPUT-PENDING BIT    */ 02659000
         NI    TRMINPTP(RTRM),B'10111111'                          1235 02660000
*           CALL LGOFF;             /* BEGIN LOGOFF PROCESSING       */ 02661000
         BAL   @14,LGOFF                                           1236 02662000
*         END;                                                     1237 02663000
*       ELSE                        /* NO                    @YM08991*/ 02664000
*         BUSYSW=ON;                /* INDICATE PLCB IS BUSY @YM08991*/ 02665000
         B     @RC01233                                            1238 02666000
@RF01233 OI    BUSYSW,B'00010000'                                  1238 02667000
*     END;                                                         1239 02668000
*   ELSE                            /* NO PLCB CURRENTLY ASSIGNED    */ 02669000
*     DO;                                                          1240 02670000
         B     @RC01231                                            1240 02671000
@RF01231 DS    0H                                                  1241 02672000
*       IF QCBRCVRQ=OFF THEN        /* HAS A PLCB BEEN REQUESTED?    */ 02673000
         L     @09,IEDQQPTR                                        1241 02674000
         AL    @09,@CF03385                                        1241 02675000
         TM    QCBRCVRQ-4(@09),B'00010000'                         1241 02676000
         BNZ   @RF01241                                            1241 02677000
*         DO;                       /* NO                            */ 02678000
*           CALL RQSTPLCB;          /* REQUEST A PLCB                */ 02679000
         BAL   @14,RQSTPLCB                                        1243 02680000
*           IF R15>0 THEN           /* DID WE GET ONE?       @YM05705*/ 02681000
         LTR   R15,R15                                             1244 02682000
         BNP   @RF01244                                            1244 02683000
*             DO;                   /* YES                           */ 02684000
*               LCBQCBA=RLCB;       /* TPOST THE PLCB TO ITSELF      */ 02685000
         STCM  RLCB,7,LCBQCBA(RLCB)                                1246 02686000
*               LCBPRI=PRILNFRE;    /* ... TO DRIVE THE              */ 02687000
         MVI   LCBPRI(RLCB),X'E0'                                  1247 02688000
*               RPARM=RLCB;         /* ... RECEIVE                   */ 02689000
         LR    RPARM,RLCB                                          1248 02690000
*               CALL DSPPOSTR;      /* ... SCHEDULER                 */ 02691000
         LA    @15,DSPPOSTR(,RDISP)                                1249 02692000
         BALR  @14,@15                                             1249 02693000
*             END;                                                 1250 02694000
*         END;                                                     1251 02695000
*     END;                                                         1252 02696000
*   END PRMLGOFF;                   /* RETURN TO CALLER              */ 02697000
@EL00033 DS    0H                                                  1253 02698000
@EF00033 DS    0H                                                  1253 02699000
@ER00033 L     @14,@SA00033                                        1253 02700000
         BR    @14                                                 1253 02701000
         EJECT                                                          02702000
*QCBRTN:                                                           1254 02703000
*   PROC OPTIONS(SAVE(R14));                                       1254 02704000
*                                                                  1254 02705000
QCBRTN   ST    @14,@SA00034                                        1254 02706000
*/********************************************************************/ 02707000
*/*                                                                  */ 02708000
*/*      Q C B R T N   SUBROUTINE                                    */ 02709000
*/*                                                                  */ 02710000
*/********************************************************************/ 02711000
*/*                                                                  */ 02712000
*/*      THIS SUBROUTINE PROCESSES A DESTINATION QCB TPOSTED TO      */ 02713000
*/*      IEDLUS AS INPUT.  THE SUBROUTINE IS ENTERED TWICE.          */ 02714000
*/*                                                                  */ 02715000
*/*      1ST ENTRY (CALLED FROM MAIN SEGMENT):                       */ 02716000
*/*         .  REQUESTS A BUFFER.                                    */ 02717000
*/*                                                                  */ 02718000
*/*      2ND ENTRY (CALLED FROM ERBRTNI):                            */ 02719000
*/*         .  BUILDS AN INIT-SELF PIU & TPOSTS IT TO THE SSCP       */ 02720000
*/*            TO REQUEST THE STARTING OF AN LU-LU SESSION.          */ 02721000
*/*                                                                  */ 02722000
*/********************************************************************/ 02723000
*                                                                  1255 02724000
*   QCBPTR=RPARM;                   /* QCB ADDRESS                   */ 02725000
         ST    RPARM,QCBPTR                                        1255 02726000
*   IF QCBPLCBN=ON THEN             /* IF PLCB ADDRESS THEN  @XM05703*/ 02727000
         L     @09,IEDQQPTR                                        1256 02728000
         SL    @09,@CF00044                                        1256 02729000
         TM    QCBPLCBN(@09),B'10000000'                           1256 02730000
         BNO   @RF01256                                            1256 02731000
*     TNTSLU=QCBPLCBA->LCBTTCIN;    /* GET SLU TNT FROM LCB  @XM05703*/ 02732000
         L     @09,QCBPLCBA-1(,@09)                                1257 02733000
         LH    @09,LCBTTCIN(,@09)                                  1257 02734000
         N     @09,@CF03378                                        1257 02735000
         STH   @09,TNTSLU                                          1257 02736000
*   ELSE                                                           1258 02737000
*     IF QCBWAITC=ON THEN           /* IF CHAIN OF PIUS THEN @XM05703*/ 02738000
         B     @RC01256                                            1258 02739000
@RF01256 L     @09,IEDQQPTR                                        1258 02740000
         SL    @09,@CF00044                                        1258 02741000
         TM    QCBWAITC(@09),B'01000000'                           1258 02742000
         BNO   @RF01258                                            1258 02743000
*       DO;                         /*                       @XM05703*/ 02744000
*         RPARM=QCBPLCBA;           /* GET FIRST PIU         @XM05703*/ 02745000
         L     RPARM,QCBPLCBA-1(,@09)                              1260 02746000
         LA    RPARM,0(,RPARM)                                     1260 02747000
*         RFY                                                      1261 02748000
*           IEDQPRF BASED(RPARM);   /*                       @XM05703*/ 02749000
*         RPARM=ADDR(PRFPIUO)+PRFPIUO;/* GET TH              @XM05703*/ 02750000
         LA    @09,PRFPIUO(,RPARM)                                 1262 02751000
         SLR   @07,@07                                             1262 02752000
         IC    @07,PRFPIUO(,RPARM)                                 1262 02753000
         ALR   @09,@07                                             1262 02754000
         LR    RPARM,@09                                           1262 02755000
*         TNTSLU=RPARM->TTHOAF;     /* GET SLU TNT FROM PIU  @XM05703*/ 02756000
         MVC   TNTSLU(2),TTHOAF(RPARM)                             1263 02757000
*         RFY                                                      1264 02758000
*           IEDQPRF BASED(RPRF);    /*                       @XM05703*/ 02759000
*       END;                        /*                       @XM05703*/ 02760000
*     ELSE                                                         1266 02761000
*       TNTSLU=QCBTTCIN;            /* ELSE QCB HAS SLU TNT  @XM05703*/ 02762000
         B     @RC01258                                            1266 02763000
@RF01258 L     @09,IEDQQPTR                                        1266 02764000
         BCTR  @09,0                                               1266 02765000
         BCTR  @09,0                                               1266 02766000
         LH    @09,QCBTTCIN-6(,@09)                                1266 02767000
         STH   @09,TNTSLU                                          1266 02768000
*   CALL CTLBLKS;                   /* SET UP CONTROL BLOCK POINTERS */ 02769000
@RC01258 DS    0H                                                  1267 02770000
@RC01256 BAL   @14,CTLBLKS                                         1267 02771000
*   IF BUFPTR=0 THEN                /* DO WE NEED TO GET A BUFFER?   */ 02772000
         L     @09,BUFPTR                                          1268 02773000
         LTR   @09,@09                                             1268 02774000
         BNZ   @RF01268                                            1268 02775000
*     DO;                           /* YES                           */ 02776000
*       RPARMF=LENGTH(PIUTH)+LENGTH(PIURH)+LENGTH(IEDXINSD)+LENGTH(     02777000
*           PRFPIUO);               /* CALC NUMBER OF BYTES IN PIU   */ 02778000
         LA    RPARMF,39                                           1270 02779000
*       CALL UNITCNT;               /* CONVERT TO UNIT COUNT (IN R0) */ 02780000
