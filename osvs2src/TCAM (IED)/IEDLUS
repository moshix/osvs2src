         BNO   @RF01164                                            1164 02414000
*     DO;                           /* YES                           */ 02415000
*       TNTSLU=TTHOAF;              /* GET SLU'S TNT INDEX           */ 02416000
         MVC   TNTSLU(2),TTHOAF(RPIU)                              1166 02417000
*       CALL CTLBLKS;               /* SET UP CONTROL BLOCK POINTERS */ 02418000
         BAL   @14,CTLBLKS                                         1167 02419000
*       IF DATAPTR->CHAR1=CD1BIND|  /* BIND? OR                      */ 02420000
*           DATAPTR->CHAR1=CD1SDT THEN/* ... SDT?                    */ 02421000
         L     @09,DATAPTR                                         1168 02422000
         CLI   CHAR1(@09),X'31'                                    1168 02423000
