*   DO UNTIL(RPARMF<=0);                                           1423 03363000
@DL01423 DS    0H                                                  1424 03364000
*     RPARMF=RPARMF-AVTKEYLE;       /* SUBTRACT SIZE OF 1 UNIT       */ 03365000
         SH    RPARMF,AVTKEYLE(,RAVT)                              1424 03366000
*     R0=R0+1;                      /* BUMP UNIT COUNT               */ 03367000
         AL    R0,@CF00060                                         1425 03368000
*   END;                                                           1426 03369000
@DE01423 LTR   RPARMF,RPARMF                                       1426 03370000
         BP    @DL01423                                            1426 03371000
*   END UNITCNT;                    /* RETURN TO CALLER              */ 03372000
