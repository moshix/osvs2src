*/********************************************************************/ 00766000
*/*                                                                  */ 00767000
*/*      D A C T L O O P   SUBROUTINE                                */ 00768000
*/*                                                                  */ 00769000
*/********************************************************************/ 00770000
*/*                                                                  */ 00771000
*/*      THIS SUBROUTINE SEARCHES THE CHAIN OF DEACTIVATE REQUEST    */ 00772000
*/*      ELEMENTS.  ALL SUCH ELEMENTS FOR A GIVEN SLU ARE REMOVED    */ 00773000
*/*      AND TPOSTED BACK TO THEIR ORIGINATORS.                      */ 00774000
*/*                                                                  */ 00775000
*/********************************************************************/ 00776000
*                                                                  0757 00777000
*   PREVIOUS=ADDR(SAVTLUSR)-LINKOFFS;/* INITIALIZE POINTERS          */ 00778000
         L     @09,AVTSAVTP(,RAVT)                                 0757 00779000
         LA    @07,SAVTLUSR(,@09)                                  0757 00780000
         SLR   @05,@05                                             0757 00781000
         IC    @05,LINKOFFS                                        0757 00782000
         SLR   @07,@05                                             0757 00783000
         ST    @07,PREVIOUS                                        0757 00784000
*   CURRENT=SAVTLUSR;               /* ... FOR SEARCH                */ 00785000
         L     @09,SAVTLUSR(,@09)                                  0758 00786000
         ST    @09,CURRENT                                         0758 00787000
*   DO WHILE(CURRENT>0);                                           0759 00788000
         B     @DE00759                                            0759 00789000
@DL00759 DS    0H                                                  0760 00790000
*     IF CURRENT->REQLTNT=TNTSLU THEN/* ELEMENT FOR THIS SLU?        */ 00791000
         L     @09,CURRENT                                         0760 00792000
         CLC   REQLTNT(2,@09),TNTSLU                               0760 00793000
         BNE   @RF00760                                            0760 00794000
*       DO;                         /* YES                           */ 00795000
*         DACTPTR=CURRENT;          /* SAVE ELEMENT ADDRESS          */ 00796000
         ST    @09,DACTPTR                                         0762 00797000
*         PREVIOUS->REQLINK=CURRENT->REQLINK;/* DELINK THE ELEMENT   */ 00798000
         L     @07,PREVIOUS                                        0763 00799000
         MVC   REQLINK(3,@07),REQLINK(@09)                         0763 00800000
*         CALL DACTPOST;            /* RETURN ELEMENT TO REQUESTOR   */ 00801000
         BAL   @14,DACTPOST                                        0764 00802000
*       END;                                                       0765 00803000
*     ELSE                          /* NOT ELEMENT FOR THIS SLU      */ 00804000
*       PREVIOUS=CURRENT;           /* BUMP TO NEXT ELEMENT          */ 00805000
         B     @RC00760                                            0766 00806000
@RF00760 L     @09,CURRENT                                         0766 00807000
         ST    @09,PREVIOUS                                        0766 00808000
*     CURRENT=PREVIOUS->REQLINK;    /* BUMP TO NEXT ELEMENT          */ 00809000
@RC00760 L     @09,PREVIOUS                                        0767 00810000
         L     @09,REQLINK-1(,@09)                                 0767 00811000
         LA    @09,0(,@09)                                         0767 00812000
         ST    @09,CURRENT                                         0767 00813000
*   END;                                                           0768 00814000
@DE00759 L     @09,CURRENT                                         0768 00815000
         LTR   @09,@09                                             0768 00816000
         BP    @DL00759                                            0768 00817000
*   END DACTLOOP;                   /* RETURN TO CALLER              */ 00818000
