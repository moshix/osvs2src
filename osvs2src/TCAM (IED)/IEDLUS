*   TCXPTR=CVTAQAVT;                /* GET TCX ADDRESS       @YM08096*/ 02870000
         L     @09,CVTPTR                                          1295 02871000
         L     TCXPTR,CVTAQAVT(,@09)                               1295 02872000
*   IF(QCBTTCIN>TNTLEN)&            /* CHECK FOR TOTE ENTRY  @YM08096*/ 02873000
*       (QCBTTCIN=<TNTLEN+TCXTOTE) THEN                            1296 02874000
