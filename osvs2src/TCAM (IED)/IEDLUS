         BNO   @RF01284                                            1284 02829000
*     R0=LUSNDX;                    /* YES, SET INDEX FOR POOL MGR 1285 02830000
*                                      (CAUSES PLCB TPOST TO IEDLUS) */ 02831000
         LA    R0,1                                                1285 02832000
*   CALL IEDIAP01;                  /* REQUEST A PLCB                */ 02833000
@RF01284 L     @09,AVTSAVTP(,RAVT)                                 1286 02834000
         L     @15,SAVTPREQ(,@09)                                  1286 02835000
         BALR  @14,@15                                             1286 02836000
*   IF R15>0 THEN                   /* GOOD RETURN CODE?             */ 02837000
         LTR   R15,R15                                             1287 02838000
         BNP   @RF01287                                            1287 02839000
*     DO;                           /* YES, PLCB HAS BEEN ASSIGNED   */ 02840000
*       RLCB=R15;                   /* SET UP PLCB ADDRESS POINTER   */ 02841000
         LR    RLCB,R15                                            1289 02842000
*       TRMINPTP=OFF;               /* TURN OFF INPUT-PENDING FLAG   */ 02843000
         NI    TRMINPTP(RTRM),B'10111111'                          1290 02844000
*     END;                                                         1291 02845000
*   END RQSTPLCB;                   /* RETURN TO CALLER              */ 02846000
@EL00035 DS    0H                                                  1292 02847000
@EF00035 DS    0H                                                  1292 02848000
@ER00035 L     @14,@SA00035                                        1292 02849000
         BR    @14                                                 1292 02850000
