*/*                                                                  */ 00706000
*/*      C T L B L K S   SUBROUTINE                                  */ 00707000
*/*                                                                  */ 00708000
*/********************************************************************/ 00709000
*/*                                                                  */ 00710000
*/*      THIS SUBROUTINE INITIALIZES CONTROL BLOCK POINTERS FOR      */ 00711000
*/*      THE SLU WHOSE TNT INDEX IS IN TNTSLU.                       */ 00712000
*/*                                                                  */ 00713000
*/********************************************************************/ 00714000
*                                                                  0745 00715000
*                                   /* GET TERMINAL ENTRY ADDRESS    */ 00716000
*   RPARM=TNTSLU;                   /* TNT INDEX                     */ 00717000
         LH    RPARM,TNTSLU                                        0745 00718000
         N     RPARM,@CF03378                                      0745 00719000
*   CALL IEDQTNT;                   /* CONVERT TNT INDEX TO A        */ 00720000
         L     @15,AVTRNMPT(,RAVT)                                 0746 00721000
         BALR  @14,@15                                             0746 00722000
*   RTRM=RPARM;                     /* ... TERMINAL ENTRY ADDRESS    */ 00723000
         LR    RTRM,RPARM                                          0747 00724000
*   RTRM=RTRM-LENGTH(IEDNTRM);      /* BACK UP TO TERM ENTRY PREFIX  */ 00725000
         SL    RTRM,@CF02842                                       0748 00726000
*                                   /* SET UP SIB POINTER            */ 00727000
*   IF TRMSIBPT>0&                  /* DO WE HAVE AN LU-LU SIB?      */ 00728000
*       TRMSIBPT->SIBINDEX^=SAVTSCPT THEN                          0749 00729000
         SLR   @09,@09                                             0749 00730000
         ICM   @09,7,TRMSIBPT(RTRM)                                0749 00731000
         LTR   @09,@09                                             0749 00732000
         BNP   @RF00749                                            0749 00733000
         L     @07,AVTSAVTP(,RAVT)                                 0749 00734000
         CLC   SIBINDEX(2,@09),SAVTSCPT(@07)                       0749 00735000
         BE    @RF00749                                            0749 00736000
*     SIBPTR=TRMSIBPT;              /* YES, GET ITS ADDRESS          */ 00737000
         ST    @09,SIBPTR                                          0750 00738000
*   ELSE                                                           0751 00739000
*     SIBPTR=0;                     /* NO, CLEAR SIB POINTER         */ 00740000
         B     @RC00749                                            0751 00741000
@RF00749 SLR   @09,@09                                             0751 00742000
         ST    @09,SIBPTR                                          0751 00743000
*   QCBPTR=TRMDESTQ;                /* GET DESTINATION QCB ADDRESS   */ 00744000
