*       RPARM=QCBPTR;               /* ADDR OF ELEMENT NEEDING BUFFER*/ 02782000
         L     RPARM,QCBPTR                                        1272 02783000
*       CALL GETBUFI;               /* REQUEST A BUFFER              */ 02784000
         BAL   @14,GETBUFI                                         1273 02785000
*     END;                                                         1274 02786000
*   ELSE                            /* WE ALREADY HAVE A BUFFER      */ 02787000
*     DO;                                                          1275 02788000
         B     @RC01268                                            1275 02789000
@RF01268 DS    0H                                                  1276 02790000
*       RPRF=BUFPTR;                /* GET BUFFER ADDRESS            */ 02791000
         L     RPRF,BUFPTR                                         1276 02792000
*       BUFPTR=0;                   /* CLEAR THE POINTER             */ 02793000
         SLR   @09,@09                                             1277 02794000
