         TITLE 'IEDMVBFR, MOVE-BUFFER-DATA MODULE                      *00001000
                        '                                               00002000
IEDMVBFR CSECT ,                                                   0001 00003000
@PROLOG  DS    0H                                                  0002 00004000
         USING   *,RBASE                                                00005000
*                                                                       00006000
IEDMVBFR IEDHJN  START,HJN                                              00007000
*                                   /* SAVE CALLER'S REGISTERS       */ 00008000
*   REGSV0=RZERO;                   /* REGISTER 0                    */ 00009000
         ST    RZERO,REGSV0(,RSAVE)                                0068 00010000
*   STM(R2,RC,REGSV2C);             /* REGISTERS 2-12                */ 00011000
         STM   R2,RC,REGSV2C(RSAVE)                                0069 00012000
*   REGSVE=RE;                      /* REGISTER 14                   */ 00013000
         ST    RE,REGSVE(,RSAVE)                                   0070 00014000
*   CALL INIT;                      /* INITIALIZE REGISTERS & FIELDS    00015000
*                                      FROM USER'S PARAMETER LIST    */ 00016000
         BAL   @14,INIT                                            0071 00017000
         EJECT                                                          00018000
*                                                                  0072 00019000
*/********************************************************************/ 00020000
*/*                                                                  */ 00021000
*/*      D A T A - M O V I N G   L O O P                             */ 00022000
*/*                                                                  */ 00023000
*/********************************************************************/ 00024000
*/*                                                                  */ 00025000
*/*      THIS LOOP MOVES THE DATA FROM THE SOURCE BUFFER TO THE      */ 00026000
*/*      TARGET BUFFER.  ON EACH PASS THRU THE LOOP THE NUMBER OF    */ 00027000
*/*      BYTES MOVED IS THE SMALLER OF ---                           */ 00028000
*/*        A. RTLENGTH - THE NUMBER OF BYTES OF SPACE REMAINING      */ 00029000
*/*           IN THE CURRENT TARGET UNIT,     OR                     */ 00030000
*/*        B. RSLENGTH - THE NUMBER OF DATA BYTES NOT YET MOVED      */ 00031000
*/*           OUT OF THE CURRENT SOURCE UNIT.                        */ 00032000
*/*                                                                  */ 00033000
*/*      WHENEVER EITHER THE TARGET DATA POINTER (RTDATA) OR THE     */ 00034000
*/*      SOURCE DATA POINTER (RSDATA) REACHES THE END OF ITS CURRENT */ 00035000
*/*      UNIT, WE SKIP TO THE NEXT UNIT IN THAT BUFFER AND ADJUST    */ 00036000
*/*      OUR ADDRESSES AND COUNTS ACCORDINGLY.                       */ 00037000
*/*                                                                  */ 00038000
*/*      WE EXIT FROM THE LOOP WHEN ALL THE DATA HAS BEEN MOVED      */ 00039000
*/*      OR WHEN WE HAVE NO MORE TARGET UNITS TO MOVE INTO.          */ 00040000
*/*                                                                  */ 00041000
*/********************************************************************/ 00042000
*                                                                  0072 00043000
*   DO WHILE(RCOUNT>0&((RTUNIT&ADDRMSK)=0));/*               @YM08496*/ 00044000
         B     @DE00072                                            0072 00045000
@DL00072 DS    0H                                                  0073 00046000
*     IF RTLENGTH<RSLENGTH THEN                                    0073 00047000
         CLR   RTLENGTH,RSLENGTH                                   0073 00048000
         BNL   @RF00073                                            0073 00049000
*       N=RTLENGTH;                 /* USE TARGET UNIT'S COUNT       */ 00050000
         ST    RTLENGTH,N(,RSAVE)                                  0074 00051000
*     ELSE                                                         0075 00052000
*       N=RSLENGTH;                 /* USE SOURCE UNIT'S COUNT       */ 00053000
         B     @RC00073                                            0075 00054000
@RF00073 ST    RSLENGTH,N(,RSAVE)                                  0075 00055000
*     RTDATA->CHARS(1:N)=RSDATA->CHARS(1:N);/* MOVE N BYTES OF DATA     00056000
*                                      CHANGE COUNTS, ADDRESSES BY N */ 00057000
@RC00073 L     @12,N(,RSAVE)                                       0076 00058000
         LR    @11,@12                                             0076 00059000
         BCTR  @11,0                                               0076 00060000
         EX    @11,@SM01463                                        0076 00061000
*     RCOUNT=RCOUNT-N;              /* TOTAL BYTES TO BE MOVED       */ 00062000
         SLR   RCOUNT,@12                                          0077 00063000
*     RTLENGTH=RTLENGTH-N;          /* TARGET LENGTH                 */ 00064000
         SLR   RTLENGTH,@12                                        0078 00065000
*     RSLENGTH=RSLENGTH-N;          /* SOURCE LENGTH                 */ 00066000
         SLR   RSLENGTH,@12                                        0079 00067000
*     RTDATA=RTDATA+N;              /* TARGET DATA                   */ 00068000
         ALR   RTDATA,@12                                          0080 00069000
*     RSDATA=RSDATA+N;              /* SOURCE DATA                   */ 00070000
         ALR   RSDATA,@12                                          0081 00071000
*     IF RTDATA=TUNITEND THEN       /* END OF TARGET UNIT?           */ 00072000
         C     RTDATA,TUNITEND(,RSAVE)                             0082 00073000
         BNE   @RF00082                                            0082 00074000
*       CALL TNEXT;                 /* YES, STEP TO NEXT TARGET UNIT */ 00075000
         BAL   @14,TNEXT                                           0083 00076000
*     IF RSDATA=SUNITEND THEN       /* END OF SOURCE UNIT?           */ 00077000
@RF00082 C     RSDATA,SUNITEND(,RSAVE)                             0084 00078000
         BNE   @RF00084                                            0084 00079000
*       CALL SNEXT;                 /* YES, STEP TO NEXT SOURCE UNIT */ 00080000
         BAL   @14,SNEXT                                           0085 00081000
*   END;                                                           0086 00082000
@RF00084 DS    0H                                                  0086 00083000
@DE00072 SLR   @12,@12                                             0086 00084000
         CLR   RCOUNT,@12                                          0086 00085000
         BNH   @DC00072                                            0086 00086000
         LR    @00,RTUNIT                                          0086 00087000
         N     @00,ADDRMSK(,RSAVE)                                 0086 00088000
         CLR   @00,@12                                             0086 00089000
         BE    @DL00072                                            0086 00090000
@DC00072 DS    0H                                                  0087 00091000
         EJECT                                                          00092000
*                                                                  0087 00093000
*/********************************************************************/ 00094000
*/*                                                                  */ 00095000
*/*      A F T E R - M O V E   P R O C E S S I N G                   */ 00096000
*/*                                                                  */ 00097000
*/********************************************************************/ 00098000
*                                                                  0087 00099000
*   IF PLMVTCTG=OFF&                /* TARGET = A TCAM BUFFER? & ... */ 00100000
*       PLMVUNCF=ON THEN            /* DID CALLER ASK US TO UNCHAIN     00101000
*                                      EMPTY TARGET UNITS LEFT OVER     00102000
*                                      AT THE END OF THE MOVE?       */ 00103000
         TM    PLMVUNCF(IEDPLMVP),B'10000000'                      0087 00104000
         BNO   @RF00087                                            0087 00105000
         TM    PLMVTCTG(IEDPLMVP),B'00100000'                      0087 00106000
         BNZ   @RF00087                                            0087 00107000
*     CALL UNCHAIN;                 /* YES, DO UNCHAINING IF NEEDED  */ 00108000
         BAL   @14,UNCHAIN                                         0088 00109000
*   ELSE                                                           0089 00110000
*     PLMVUNCA=0;                   /* NO, INDICATE NO UNCHAINING    */ 00111000
         B     @RC00087                                            0089 00112000
@RF00087 SLR   @12,@12                                             0089 00113000
         ST    @12,PLMVUNCA(,IEDPLMVP)                             0089 00114000
*   CALL UPDTPRMS;                  /* UPDATE USER'S PARAMETER LIST     00115000
*                                      TO REFLECT CONDITIONS AT THE     00116000
*                                      END OF THE MOVE               */ 00117000
@RC00087 BAL   @14,UPDTPRMS                                        0090 00118000
*                                   /* RESTORE REGISTERS & RETURN    */ 00119000
*   RE=REGSVE;                      /* REGISTER 14                   */ 00120000
         L     RE,REGSVE(,RSAVE)                                   0091 00121000
*   LM(R2,RC,REGSV2C);              /* REGISTERS 2-12                */ 00122000
         LM    R2,RC,REGSV2C(RSAVE)                                0092 00123000
*   RZERO=REGSV0;                   /* REGISTER 0                    */ 00124000
         L     RZERO,REGSV0(,RSAVE)                                0093 00125000
*   RETURN;                         /* RETURN TO CALLER              */ 00126000
@EL00001 DS    0H                                                  0094 00127000
@EF00001 DS    0H                                                  0094 00128000
@ER00001 BR    @14                                                 0094 00129000
         EJECT                                                          00130000
*                                                                  0095 00131000
*/********************************************************************/ 00132000
