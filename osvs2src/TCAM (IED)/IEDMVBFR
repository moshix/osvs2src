*/*      I N I T   SUBROUTINE                                        */ 00134000
*/*                                                                  */ 00135000
*/********************************************************************/ 00136000
*/*                                                                  */ 00137000
*/*      THIS SUBROUTINE INITIALIZES REGISTERS USED BY THIS MODULE   */ 00138000
*/*      AND SETS UP FIELDS WITHIN THE USER-SUPPLIED SAVE AREA       */ 00139000
*/*                                                                  */ 00140000
*/********************************************************************/ 00141000
*                                                                  0095 00142000
*INIT:                                                             0095 00143000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0095 00144000
INIT     DS    0H                                                  0096 00145000
*   RAVT=TCXAVT;                    /* AVT ADDRESS (AVTSAVE1)        */ 00146000
         L     @12,CVTPTR                                          0096 00147000
         L     @12,TCXPTR(,@12)                                    0096 00148000
         L     RAVT,TCXAVT(,@12)                                   0096 00149000
*                                   /* PICK UP PARAMETER LIST VALUES */ 00150000
*   RTUNIT=PLMVTU;                  /* TARGET UNIT ADDRESS           */ 00151000
         L     RTUNIT,PLMVTU(,IEDPLMVP)                            0097 00152000
*   RTDATA=PLMVTD;                  /* START-OF-DATA ADDRESS (WITHIN    00153000
*                                      TARGET UNIT)                  */ 00154000
         L     RTDATA,PLMVTD(,IEDPLMVP)                            0098 00155000
*   RSUNIT=PLMVSU;                  /* SOURCE UNIT ADDRESS           */ 00156000
         L     RSUNIT,PLMVSU(,IEDPLMVP)                            0099 00157000
*   RSDATA=PLMVSD;                  /* START-OF-DATA ADDR (WITHIN  0100 00158000
*                                      SOURCE UNIT)                  */ 00159000
         L     RSDATA,PLMVSD(,IEDPLMVP)                            0100 00160000
*   RCOUNT=PLMVLNTH;                /* NUMBER OF BYTES TO BE MOVED   */ 00161000
         LH    RCOUNT,PLMVLNTH(,IEDPLMVP)                          0101 00162000
         N     RCOUNT,@CF01465                                     0101 00163000
*                                   /* CALCULATE END-OF-UNIT ADDRESS    00164000
*                                      & SPACE REMAINING IN UNITS    */ 00165000
*   IF PLMVTCTG=ON THEN             /* TARGET = CONTIGUOUS-CORE AREA?*/ 00166000
         TM    PLMVTCTG(IEDPLMVP),B'00100000'                      0102 00167000
         BNO   @RF00102                                            0102 00168000
*     DO;                           /* YES                   @YM08496*/ 00169000
*       TUNITEND=RTDATA+RCOUNT+1;   /* SET BEYOND AREA END   @YM08496*/ 00170000
         LR    @12,RTDATA                                          0104 00171000
         ALR   @12,RCOUNT                                          0104 00172000
         AL    @12,@CF00060                                        0104 00173000
         ST    @12,TUNITEND(,RSAVE)                                0104 00174000
*       ADDRMSK=0;                  /* NULLIFY CHECK FOR     @YM08496*/ 00175000
         SLR   @12,@12                                             0105 00176000
         ST    @12,ADDRMSK(,RSAVE)                                 0105 00177000
*     END;                          /* ... INVALID TIC ADDR  @YM08496*/ 00178000
*   ELSE                            /*                       @YM08496*/ 00179000
*     DO;                           /* NO, TARGET = BUFFER   @YM08496*/ 00180000
         B     @RC00102                                            0107 00181000
@RF00102 DS    0H                                                  0108 00182000
*       TUNITEND=ADDR(RTUNIT->PRFSUNIT)+AVTKEYLE;/* UNIT END @YM08496*/ 00183000
         LA    @12,PRFSUNIT(,RTUNIT)                               0108 00184000
         AH    @12,AVTKEYLE(,IEDQAPTR)                             0108 00185000
         ST    @12,TUNITEND(,RSAVE)                                0108 00186000
*       ADDRMSK=INVALMSK;           /* SET UP FOR CHECKING   @YM08496*/ 00187000
         MVC   ADDRMSK(4,RSAVE),@CB01428                           0109 00188000
*     END;                          /* ... INVALID TIC ADDR  @YM08496*/ 00189000
*   RTLENGTH=TUNITEND-RTDATA;       /* SPACE REMAINING IN TARGET UNIT*/ 00190000
@RC00102 L     RTLENGTH,TUNITEND(,RSAVE)                           0111 00191000
         SLR   RTLENGTH,RTDATA                                     0111 00192000
*   IF PLMVSCTG=ON THEN             /* SOURCE = CONTIGUOUS-CORE AREA?*/ 00193000
         TM    PLMVSCTG(IEDPLMVP),B'01000000'                      0112 00194000
         BNO   @RF00112                                            0112 00195000
*     SUNITEND=RSDATA+RCOUNT+1;     /* YES, SET BEYOND END OF AREA   */ 00196000
         LR    @12,RSDATA                                          0113 00197000
         ALR   @12,RCOUNT                                          0113 00198000
         AL    @12,@CF00060                                        0113 00199000
         ST    @12,SUNITEND(,RSAVE)                                0113 00200000
*   ELSE                            /* NO, SET TO SOURCE UNIT END    */ 00201000
*     SUNITEND=ADDR(RSUNIT->PRFSUNIT)+AVTKEYLE;                    0114 00202000
         B     @RC00112                                            0114 00203000
@RF00112 LA    @12,PRFSUNIT(,RSUNIT)                               0114 00204000
         AH    @12,AVTKEYLE(,IEDQAPTR)                             0114 00205000
         ST    @12,SUNITEND(,RSAVE)                                0114 00206000
*   RSLENGTH=SUNITEND-RSDATA;       /* SPACE REMAINING IN SOURCE UNIT*/ 00207000
@RC00112 L     RSLENGTH,SUNITEND(,RSAVE)                           0115 00208000
         SLR   RSLENGTH,RSDATA                                     0115 00209000
*                                   /* IF NECESSARY ADJUST CALCULATED   00210000
*                                      VALUES TO BE NO GREATER THAN     00211000
*                                      NUMBER OF BYTES TO BE MOVED   */ 00212000
*   IF RTLENGTH>RCOUNT THEN                                        0116 00213000
         CLR   RTLENGTH,RCOUNT                                     0116 00214000
         BNH   @RF00116                                            0116 00215000
*     RTLENGTH=RCOUNT;              /* ADJUST TARGET LENGTH          */ 00216000
         LR    RTLENGTH,RCOUNT                                     0117 00217000
*   IF RSLENGTH>RCOUNT THEN                                        0118 00218000
@RF00116 CLR   RSLENGTH,RCOUNT                                     0118 00219000
         BNH   @RF00118                                            0118 00220000
*     RSLENGTH=RCOUNT;              /* ADJUST SOURCE LENGTH          */ 00221000
         LR    RSLENGTH,RCOUNT                                     0119 00222000
*   PRVTUNIT=0;                     /* INIT PREVIOUS-TARGET-UNIT ADDR*/ 00223000
@RF00118 SLR   @12,@12                                             0120 00224000
         ST    @12,PRVTUNIT(,RSAVE)                                0120 00225000
*   END INIT;                       /* RETURN TO CALLER              */ 00226000
@EL00002 DS    0H                                                  0121 00227000
@EF00002 DS    0H                                                  0121 00228000
@ER00002 BR    @14                                                 0121 00229000
         EJECT                                                          00230000
*                                                                  0122 00231000
*/********************************************************************/ 00232000
*/*                                                                  */ 00233000
*/*      S N E X T   SUBROUTINE                                      */ 00234000
*/*                                                                  */ 00235000
*/********************************************************************/ 00236000
*/*                                                                  */ 00237000
*/*      THIS SUBROUTINE BUMPS THE SOURCE UNIT ADDRESS TO THE        */ 00238000
*/*      START OF THE NEXT UNIT, AND INITIALIZES THE SOURCE DATA     */ 00239000
*/*      ADDRESS, END-OF-UNIT ADDRESS, AND BYTE COUNT FOR THE        */ 00240000
*/*      NEW UNIT.                                                   */ 00241000
*/*                                                                  */ 00242000
*/********************************************************************/ 00243000
*                                                                  0122 00244000
*SNEXT:                                                            0122 00245000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0122 00246000
SNEXT    DS    0H                                                  0123 00247000
*   RSUNIT=RSUNIT->PRFTI0;          /* NEXT SOURCE UNIT ADDRESS      */ 00248000
         L     @12,PRFTI0-1(,RSUNIT)                               0123 00249000
         LA    @12,0(,@12)                                         0123 00250000
         LR    RSUNIT,@12                                          0123 00251000
*   RSDATA=ADDR(RSUNIT->PRFSUNIT);  /* DATA ADDR IN NEXT UNIT        */ 00252000
         LA    RSDATA,PRFSUNIT(,RSUNIT)                            0124 00253000
*   SUNITEND=RSDATA+AVTKEYLE;       /* END OF NEXT SOURCE UNIT       */ 00254000
         LH    @12,AVTKEYLE(,IEDQAPTR)                             0125 00255000
         LR    @00,RSDATA                                          0125 00256000
         ALR   @00,@12                                             0125 00257000
         ST    @00,SUNITEND(,RSAVE)                                0125 00258000
*   IF RCOUNT<AVTKEYLE THEN         /* LESS THAN FULL UNIT TO MOVE?  */ 00259000
         CLR   RCOUNT,@12                                          0126 00260000
         BNL   @RF00126                                            0126 00261000
*     RSLENGTH=RCOUNT;              /* YES, SET SOURCE-UNIT COUNT =     00262000
*                                      TOTAL BYTES LEFT TO BE MOVED  */ 00263000
         LR    RSLENGTH,RCOUNT                                     0127 00264000
*   ELSE                                                           0128 00265000
*     RSLENGTH=AVTKEYLE;            /* NO, SET SOURCE-UNIT COUNT = 0128 00266000
*                                      UNIT SIZE                     */ 00267000
         B     @RC00126                                            0128 00268000
@RF00126 LH    RSLENGTH,AVTKEYLE(,IEDQAPTR)                        0128 00269000
*   END SNEXT;                      /* RETURN TO CALLER              */ 00270000
@EL00003 DS    0H                                                  0129 00271000
@EF00003 DS    0H                                                  0129 00272000
@ER00003 BR    @14                                                 0129 00273000
         EJECT                                                          00274000
*                                                                  0130 00275000
*/********************************************************************/ 00276000
*/*                                                                  */ 00277000
*/*      T N E X T   SUBROUTINE                                      */ 00278000
*/*                                                                  */ 00279000
*/********************************************************************/ 00280000
*/*                                                                  */ 00281000
*/*      THIS SUBROUTINE SETS THE PREVIOUS-TARGET-UNIT ADDRESS       */ 00282000
*/*      FIELD TO THE CURRENT TARGET UNIT ADDRESS.  IT THEN BUMPS    */ 00283000
*/*      THE TARGET UNIT ADDRESS TO THE START OF THE NEXT UNIT,      */ 00284000
*/*      AND INITIALIZES THE TARGET DATA ADDRESS, END-OF-UNIT        */ 00285000
*/*      ADDRESS AND BYTE COUNT FOR THE NEW UNIT.                    */ 00286000
*/*                                                                  */ 00287000
*/********************************************************************/ 00288000
*                                                                  0130 00289000
*TNEXT:                                                            0130 00290000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0130 00291000
TNEXT    DS    0H                                                  0131 00292000
*   PRVTUNIT=RTUNIT;                /* PREVIOUS-TARGET-UNIT ADDRESS  */ 00293000
         ST    RTUNIT,PRVTUNIT(,RSAVE)                             0131 00294000
*   RTUNIT=RTUNIT->PRFTI0;          /* NEXT TARGET UNIT ADDRESS      */ 00295000
         L     @12,PRFTI0-1(,RTUNIT)                               0132 00296000
         LA    @12,0(,@12)                                         0132 00297000
         LR    RTUNIT,@12                                          0132 00298000
*   RTDATA=ADDR(RTUNIT->PRFSUNIT);  /* TARGET DATA ADDRESS           */ 00299000
         LA    RTDATA,PRFSUNIT(,RTUNIT)                            0133 00300000
*   TUNITEND=RTDATA+AVTKEYLE;       /* END-OF-TARGET-UNIT ADDRESS    */ 00301000
         LH    @12,AVTKEYLE(,IEDQAPTR)                             0134 00302000
         LR    @00,RTDATA                                          0134 00303000
         ALR   @00,@12                                             0134 00304000
         ST    @00,TUNITEND(,RSAVE)                                0134 00305000
*   IF RCOUNT<AVTKEYLE THEN         /* LESS THAN FULL UNIT TO MOVE?  */ 00306000
         CLR   RCOUNT,@12                                          0135 00307000
         BNL   @RF00135                                            0135 00308000
*     RTLENGTH=RCOUNT;              /* YES, SET TARGET-UNIT COUNT =     00309000
*                                      TOTAL BYTES LEFT TO BE MOVED  */ 00310000
         LR    RTLENGTH,RCOUNT                                     0136 00311000
*   ELSE                                                           0137 00312000
*     RTLENGTH=AVTKEYLE;            /* NO, SET TARGET-UNIT COUNT = 0137 00313000
*                                      UNIT SIZE                     */ 00314000
         B     @RC00135                                            0137 00315000
@RF00135 LH    RTLENGTH,AVTKEYLE(,IEDQAPTR)                        0137 00316000
*   END TNEXT;                      /* RETURN TO CALLER              */ 00317000
@EL00004 DS    0H                                                  0138 00318000
@EF00004 DS    0H                                                  0138 00319000
@ER00004 BR    @14                                                 0138 00320000
         EJECT                                                          00321000
*                                                                  0139 00322000
*/********************************************************************/ 00323000
*/*                                                                  */ 00324000
*/*      U N C H A I N   SUBROUTINE                                  */ 00325000
*/*                                                                  */ 00326000
*/********************************************************************/ 00327000
*/*                                                                  */ 00328000
*/*      THIS SUBROUTINE CHECKS THE TARGET BUFFER AT THE COMPLETION  */ 00329000
*/*      OF THE MOVE TO SEE IF THERE ARE ANY EMPTY UNITS AT THE END  */ 00330000
*/*      OF THE BUFFER.  THERE ARE THREE POSSIBILITIES ---           */ 00331000
*/*                                                                  */ 00332000
*/*        1. THE LAST DATA BYTE MOVED FILLED THE LAST BYTE OF ONE   */ 00333000
*/*           TARGET UNIT, AND THE TARGET UNIT AND TARGET DATA       */ 00334000
*/*           REGISTERS NOW POINT TO THE START OF A NEW EMPTY UNIT.  */ 00335000
*/*           ACTIONS ---                                            */ 00336000
*/*             A. SET PLMVUNCA = THE ADDRESS OF THE 1ST EMPTY UNIT. */ 00337000
*/*             B. SET THE TIC-ADDRESS FIELD IN THE LAST TARGET      */ 00338000
*/*                UNIT CONTAINING ANY DATA TO AN INVALID VALUE.     */ 00339000
*/*             C. SET RTUNIT AND RTDATA EQUAL TO THE INVALID TIC    */ 00340000
*/*                ADDRESS & THAT ADDRESS + 12.  (THESE ARE THE      */ 00341000
*/*                VALUES THAT WOULD HAVE BEEN IN THESE REGISTERS    */ 00342000
*/*                IF THE EMPTY UNIT HAD NEVER BEEN THERE AT ALL.)   */ 00343000
*/*                                                                  */ 00344000
*/*        2. THE LAST BYTE OF DATA DID NOT COMPLETELY FILL THE      */ 00345000
*/*           CURRENT TARGET UNIT, AND THE TIC FIELD IN THAT UNIT    */ 00346000
*/*           CONTAINS A VALID ADDRESS.                              */ 00347000
*/*           ACTIONS ---                                            */ 00348000
*/*             A. SET PLMVUNCA = THE TIC-ADDRESS FIELD FROM THE     */ 00349000
*/*                CURRENT TARGET UNIT.  THIS IS THE ADDRESS OF THE  */ 00350000
*/*                FIRST EMPTY UNIT TO BE UNCHAINED.                 */ 00351000
*/*             B. SET THE TIC-ADDRESS FIELD IN THE CURRENT TARGET   */ 00352000
*/*                UNIT TO AN INVALID VALUE, UNCHAINING THE EMPTY    */ 00353000
*/*                UNIT.                                             */ 00354000
*/*                                                                  */ 00355000
*/*        3. THE CURRENT TARGET UNIT CONTAINS SOME DATA, AND THE    */ 00356000
*/*           TIC-ADDRESS FIELD IN THAT UNIT IS INVALID.             */ 00357000
*/*           ACTIONS ---                                            */ 00358000
*/*             A. SET PLMVUNCA = 0 TO INDICATE THERE WERE NO        */ 00359000
*/*                EMPTY UNITS TO UNCHAIN.                           */ 00360000
*/*                                                                  */ 00361000
*/********************************************************************/ 00362000
*                                                                  0139 00363000
         EJECT                                                          00364000
*UNCHAIN:                                                          0139 00365000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0139 00366000
UNCHAIN  DS    0H                                                  0140 00367000
*                                   /* TEST FOR CASE 1               */ 00368000
*   IF PRVTUNIT>0&                  /* TARGET UNIT ADDR BUMPED? &    */ 00369000
*       RTUNIT>INVALMSK&            /* ...TARGET UNIT ADDR VALID? &  */ 00370000
*       RTDATA=ADDR(RTUNIT->PRFSUNIT) THEN/* ...NO DATA IN THIS UNIT?*/ 00371000
         L     @12,PRVTUNIT(,RSAVE)                                0140 00372000
         LTR   @12,@12                                             0140 00373000
         BNP   @RF00140                                            0140 00374000
         C     RTUNIT,@CF01428                                     0140 00375000
         BNH   @RF00140                                            0140 00376000
         LA    @00,PRFSUNIT(,RTUNIT)                               0140 00377000
         CR    RTDATA,@00                                          0140 00378000
         BNE   @RF00140                                            0140 00379000
*     DO;                           /* YES                           */ 00380000
*       PLMVUNCA=RTUNIT;            /* EMPTY-TARGET-UNIT ADDRESS     */ 00381000
         ST    RTUNIT,PLMVUNCA(,IEDPLMVP)                          0142 00382000
*       PRVTUNIT->PRFTI0=INVALTIC;  /* UNCHAIN THE EMPTY TARGET UNIT */ 00383000
         LA    @11,2                                               0143 00384000
         STCM  @11,7,PRFTI0(@12)                                   0143 00385000
*       RTUNIT=INVALTIC;            /* ADJUST TARGET-UNIT ADDRESS ...*/ 00386000
         LR    RTUNIT,@11                                          0144 00387000
*       RTDATA=ADDR(RTUNIT->PRFSUNIT);/* ... & TARGET-DATA ADDRESS   */ 00388000
         LA    RTDATA,PRFSUNIT(,RTUNIT)                            0145 00389000
*     END;                                                         0146 00390000
*   ELSE                            /* TEST FOR CASE 2               */ 00391000
*     IF RTUNIT>INVALMSK&           /* TARGET UNIT ADDR VALID? &     */ 00392000
*         (RTUNIT->PRFTI0)>INVALMSK THEN/* ...HAS VALID TIC ADDR?    */ 00393000
         B     @RC00140                                            0147 00394000
@RF00140 C     RTUNIT,@CF01428                                     0147 00395000
         BNH   @RF00147                                            0147 00396000
         L     @12,PRFTI0-1(,RTUNIT)                               0147 00397000
         LA    @12,0(,@12)                                         0147 00398000
         C     @12,@CF01428                                        0147 00399000
         BNH   @RF00147                                            0147 00400000
*       DO;                         /* YES                           */ 00401000
*         PLMVUNCA=RTUNIT->PRFTI0;  /* EMPTY-TARGET-UNIT ADDR        */ 00402000
         ST    @12,PLMVUNCA(,IEDPLMVP)                             0149 00403000
*         RTUNIT->PRFTI0=INVALTIC;  /* UNCHAIN THE EMPTY TARGET UNIT */ 00404000
         MVC   PRFTI0(3,RTUNIT),@CF01426+1                         0150 00405000
*       END;                                                       0151 00406000
*     ELSE                          /* MUST BE CASE 3.               */ 00407000
*       PLMVUNCA=0;                 /* INDICATE NO UNCHAINED UNITS   */ 00408000
         B     @RC00147                                            0152 00409000
@RF00147 SLR   @12,@12                                             0152 00410000
         ST    @12,PLMVUNCA(,IEDPLMVP)                             0152 00411000
*   END UNCHAIN;                    /* RETURN TO CALLER              */ 00412000
@EL00005 DS    0H                                                  0153 00413000
@EF00005 DS    0H                                                  0153 00414000
@ER00005 BR    @14                                                 0153 00415000
         EJECT                                                          00416000
*                                                                  0154 00417000
*/********************************************************************/ 00418000
*/*                                                                  */ 00419000
*/*      U P D T P R M S   SUBROUTINE                                */ 00420000
*/*                                                                  */ 00421000
*/********************************************************************/ 00422000
*/*                                                                  */ 00423000
*/*      THIS SUBROUTINE UPDATES THE CALLER'S PARAMETER LIST TO      */ 00424000
*/*      REFLECT CONDITIONS AT THE END OF THE DATA MOVE.  THE        */ 00425000
*/*      UPDATED VERSION OF PLMVLNTH SERVES AS A RETURN CODE AS      */ 00426000
*/*      WELL AS A COUNT.  IF THAT FIELD IS ZERO, ALL OF THE DATA    */ 00427000
*/*      HAS BEEN MOVED.  IF THAT FIELD IS NON-ZERO, THEN SOME OF    */ 00428000
*/*      THE DATA COULD NOT BE MOVED BECAUSE WE RAN OUT OF SPACE     */ 00429000
*/*      IN THE TARGET BUFFER.                                       */ 00430000
*/*                                                                  */ 00431000
*/********************************************************************/ 00432000
*                                                                  0154 00433000
*UPDTPRMS:                                                         0154 00434000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0154 00435000
UPDTPRMS DS    0H                                                  0155 00436000
*   PLMVTU=RTUNIT;                  /* TARGET UNIT ADDRESS           */ 00437000
         ST    RTUNIT,PLMVTU(,IEDPLMVP)                            0155 00438000
*   PLMVTD=RTDATA;                  /* TARGET DATA ADDRESS (NEXT BYTE   00439000
*                                      AFTER LAST BYTE MOVED)        */ 00440000
         ST    RTDATA,PLMVTD(,IEDPLMVP)                            0156 00441000
*   PLMVSU=RSUNIT;                  /* SOURCE UNIT ADDRESS           */ 00442000
         ST    RSUNIT,PLMVSU(,IEDPLMVP)                            0157 00443000
*   PLMVSD=RSDATA;                  /* SOURCE DATA ADDRESS (NEXT BYTE   00444000
*                                      AFTER LAST BYTE MOVED)        */ 00445000
         ST    RSDATA,PLMVSD(,IEDPLMVP)                            0158 00446000
*   PLMVLNTH=RCOUNT;                /* NUMBER OF BYTES NOT MOVED     */ 00447000
         STH   RCOUNT,PLMVLNTH(,IEDPLMVP)                          0159 00448000
*   END UPDTPRMS;                   /* RETURN TO CALLER              */ 00449000
@EL00006 DS    0H                                                  0160 00450000
@EF00006 DS    0H                                                  0160 00451000
@ER00006 BR    @14                                                 0160 00452000
         EJECT                                                          00453000
*   END IEDMVBFR                                                   0161 00454000
*                                                                  0161 00455000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 00456000
*/*%INCLUDE SYSLIB  (TAVTD   )                                       */ 00457000
*/*%INCLUDE SYSLIB  (CVT     )                                       */ 00458000
*/*%INCLUDE SYSLIB  (TPLMVD  )                                       */ 00459000
*/*%INCLUDE SYSLIB  (TPRFD   )                                       */ 00460000
*/*%INCLUDE SYSLIB  (TTCXD   )                                       */ 00461000
*                                                                  0161 00462000
*       ;                                                          0161 00463000
@DATA    DS    0H                                                       00464000
@SM01463 MVC   CHARS(0,RTDATA),CHARS(RSDATA)                            00465000
