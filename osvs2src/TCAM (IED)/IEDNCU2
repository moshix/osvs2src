         TM    SIBSSTAT(RSIB),B'00011100'                       03S0838 00339000
         BNO   @GL00007                                         03S0838 00340000
         TM    SIBDSTAT(RSIB),B'00000001'                       03S0838 00341000
         BNO   @GL00007                                         03S0838 00342000
         TM    SIBDSTAT(RSIB),B'00000010'                       03S0838 00343000
         BZ    @RT00838                                         03S0838 00344000
@GL00007 TM    SIBSSTAT(RSIB),B'00000100'                       03S0838 00345000
         BNO   @GL00006                                         03S0838 00346000
         TM    SIBSSTAT(RSIB),B'00011000'                       03S0838 00347000
         BZ    @RT00838                                         03S0838 00348000
