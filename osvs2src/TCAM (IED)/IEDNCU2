         LTR   REG15,REG15                                      03S0842 00361000
         BZ    @RF00842                                         03S0842 00362000
*                     DO;                                       03S0843 00363000
*                                                               04S0844 00364000
*                       /*********************************************/ 00365000
*                       /*                                           */ 00366000
*                       /* PROCESS.SNAPUNCP.DAS: THIS SEGMENT FURTHER*/ 00367000
*                       /* INITIALIZES THE RESPONSE FOR SDT,ACTPU, OR*/ 00368000
*                       /* DACTPU                                    */ 00369000
*                       /*                                           */ 00370000
*                       /*********************************************/ 00371000
*                                                               04S0844 00372000
*                       TRHRUCAT=TRHSC;/* SET SESSION CONTROL TYPE   */ 00373000
         OI    TRHRUCAT+10(RTH),B'01100000'                     04S0844 00374000
