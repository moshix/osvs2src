         TITLE 'IEDNTR - PIU TRACE ROUTINE                             *00001000
                        '                                               00002000
IEDNTR   CSECT ,                                                   0001 00003000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00004000
*   RBASE=RENTRY;                   /* SET MODULE BASE       @Y17XASI*/ 00005000
         LR    RBASE,RENTRY                                        0082 00006000
*   RFY                                                            0083 00007000
*     RENTRY UNRSTD;                /* FREE REGISTER 15 FOR PLS    0083 00008000
*                                                            @Y17XASI*/ 00009000
*   GEN NOREFS NOSETS NOEXIT SEQFLOW;                              0084 00010000
*                                 /*                         @Y17XASI*/ 00011000
         USING IEDNTR,RBASE             ADDRESSIBILITY         @Y17XASI 00012000
IEDNTR   IEDHJN AFTERHJN,HJN            MODULE IDENTIFIER      @Y17XASI 00013000
*   RTBL=SAVTPIUT;                  /* GET TRACE TABLE ADDRESS     0085 00014000
*                                                            @Y17XASI*/ 00015000
         L     @07,AVTSAVTP(,RAVT)                                 0085 00016000
         L     RTBL,SAVTPIUT(,@07)                                 0085 00017000
*   RENT=CENTRY;                    /* GET CURRENT ENTRY ADDRESS   0086 00018000
*                                                            @Y17XASI*/ 00019000
         L     RENT,CENTRY(,RTBL)                                  0086 00020000
*   TRTNTDAF=PLTNTDAF;              /* TRACE DESTINATION TNT INDEX 0087 00021000
*                                                            @Y17XASI*/ 00022000
         MVC   TRTNTDAF(2,RENT),PLTNTDAF(RPARM)                    0087 00023000
*   TRTNTOAF=PLTNTOAF;              /* TRACE SOURCE TNT INDEX      0088 00024000
*                                                            @Y17XASI*/ 00025000
         MVC   TRTNTOAF(2,RENT),PLTNTOAF(RPARM)                    0088 00026000
*   RPIU=PLPIUPTR;                  /* POINT TO PIU TO BE TRACED   0089 00027000
*                                                            @Y17XASI*/ 00028000
         L     RPIU,PLPIUPTR(,RPARM)                               0089 00029000
*   RFY                                                            0090 00030000
*     RPARM UNRSTD;                 /* FREE REGISTER 1 FOR PLS     0090 00031000
*                                                            @Y17XASI*/ 00032000
*   RTH=ADDR(PRFSUNIT)+PRFPIUO;     /* POINT TO TRANSMISSION HEADER     00033000
*                                                            @Y17XASI*/ 00034000
         LA    RTH,PRFSUNIT(,RPIU)                                 0091 00035000
         SLR   @07,@07                                             0091 00036000
         IC    @07,PRFPIUO(,RPIU)                                  0091 00037000
         ALR   RTH,@07                                             0091 00038000
*   RLEN=LENGTH(IEDTH)+TTHDCF;      /* COMPUTE TOTAL PIU LENGTH    0092 00039000
*                                                            @Y17XASI*/ 00040000
         LA    RLEN,10                                             0092 00041000
         SLR   @07,@07                                             0092 00042000
         ICM   @07,3,TTHDCF(RTH)                                   0092 00043000
         ALR   RLEN,@07                                            0092 00044000
*   IF TTHFTYPE=TTHFID0 THEN        /* IF FID0 PIU           @Y17XASI*/ 00045000
         TM    TTHFTYPE(RTH),B'11110000'                           0093 00046000
         BNZ   @RF00093                                            0093 00047000
*     RLEN=RLEN-1;                  /* ADJUST LENGTH FOR FID0 PAD  0094 00048000
*                                                            @Y17XASI*/ 00049000
         BCTR  RLEN,0                                              0094 00050000
*   IF RLEN>LENGTH(TRPIU) THEN      /* IF TOTAL PIU SIZE IS GREATER     00051000
*                                      THAN TRACE ENTRY SIZE @Y17XASI*/ 00052000
@RF00093 LA    @07,28                                              0095 00053000
         CR    RLEN,@07                                            0095 00054000
         BNH   @RF00095                                            0095 00055000
*     RLEN=LENGTH(TRPIU);           /* MUST DEFAULT TO TRACE ENTRY 0096 00056000
*                                      SIZE                  @Y17XASI*/ 00057000
         LR    RLEN,@07                                            0096 00058000
*   TRPIU=TRPIUSTR;                 /* ASSUME FULL ENTRY EXISTS    0097 00059000
*                                                            @Y17XASI*/ 00060000
@RF00095 MVC   TRPIU(28,RENT),TRPIUSTR(RTH)                        0097 00061000
*   IF PRF2TERM=ON THEN             /* IF NOT GOING TO SEND  @OY18021*/ 00062000
         CLC   PRF2TERM(4),@CF00811                                0098 00063000
         BNE   @RF00098                                            0098 00064000
*     TRNOSEND=ON;                  /* MARK TRACE            @OY18021*/ 00065000
         OI    TRNOSEND(RENT),B'10000000'                          0099 00066000
*   IF TTHFTYPE=TTHFID0 THEN        /* FOR FID0 PIUS, MUST CONSIDER     00067000
*                                      PAD CHARACTER WHICH FOLLOWS 0100 00068000
*                                      REQUEST HEADER        @Y17XASI*/ 00069000
@RF00098 TM    TTHFTYPE(RTH),B'11110000'                           0100 00070000
         BNZ   @RF00100                                            0100 00071000
*     DO;                           /* SET UP TO TRACE TRUE FID0   0101 00072000
*                                      REQUEST UNIT          @Y17XASI*/ 00073000
*       RTH=RTH+1;                  /* ADJUST TH BASE ADDRESS FOR  0102 00074000
*                                      EXTRA BYTE            @Y17XASI*/ 00075000
         AL    RTH,@CF00059                                        0102 00076000
*       TRRU=RTH->TRPIURU;          /* TRACE FID0 REQUEST UNIT     0103 00077000
*                                                            @YM04615*/ 00078000
         MVC   TRRU(15,RENT),TRPIURU(RTH)                          0103 00079000
*       RTH=RTH-1;                  /* RESTORE REAL TH BASE ADDRESS     00080000
*                                                            @Y17XASI*/ 00081000
         BCTR  RTH,0                                               0104 00082000
*     END;                                                         0105 00083000
*   RFY                                                            0106 00084000
*    (RWRK8,                                                       0106 00085000
*     RREM) RSTD;                   /* RESTRICT WORK REGISTERS     0106 00086000
*                                                            @Y17XASI*/ 00087000
@RF00100 DS    0H                                                  0107 00088000
*   RWRK8=ADDR(PRFSUNIT)+AVTKEYLE-RTH;/* COMPUTE LENGTH OF PIU IN  0107 00089000
*                                      FIRST UNIT            @Y17XASI*/ 00090000
         LA    RWRK8,PRFSUNIT(,RPIU)                               0107 00091000
         AH    RWRK8,AVTKEYLE(,RAVT)                               0107 00092000
         SLR   RWRK8,RTH                                           0107 00093000
*   IF TTHFTYPE=TTHFID0 THEN        /* IF FID0 PIU           @Y17XASI*/ 00094000
         TM    TTHFTYPE(RTH),B'11110000'                           0108 00095000
         BNZ   @RF00108                                            0108 00096000
*     RWRK8=RWRK8-1;                /* ADJUST LENGTH FOR FID0 PAD  0109 00097000
*                                                            @Y17XASI*/ 00098000
         BCTR  RWRK8,0                                             0109 00099000
*   IF RLEN>RWRK8 THEN              /* IF LENGTH TO BE TRACED EXCEEDS   00100000
*                                      DATA COUNT IN FIRST UNIT, THEN   00101000
*                                      DATA MUST BE TRACED IN SECOND    00102000
*                                      UNIT                  @Y17XASI*/ 00103000
@RF00108 CR    RLEN,RWRK8                                          0110 00104000
         BNH   @RF00110                                            0110 00105000
*     DO;                           /* TRACE DATA IN NEXT UNIT     0111 00106000
*                                                            @Y17XASI*/ 00107000
*       RREM=RLEN-RWRK8;            /* LENGTH REMAINING TO TRACE   0112 00108000
*                                                            @Y17XASI*/ 00109000
         LR    RREM,RLEN                                           0112 00110000
         SLR   RREM,RWRK8                                          0112 00111000
*       RENT=RENT+RWRK8+LENGTH(TRTTCINS);/* NEW DATA OFFSET INTO   0113 00112000
*                                      TRACE ENTRY           @Y17XASI*/ 00113000
         LR    @09,RENT                                            0113 00114000
         ALR   @09,RWRK8                                           0113 00115000
         AL    @09,@CF00326                                        0113 00116000
         LR    RENT,@09                                            0113 00117000
*       RTH=PRFTI0;                 /* POINT TO NEXT UNIT IN THIS PIU   00118000
*                                                            @Y17XASI*/ 00119000
         L     RTH,PRFTI0-1(,RPIU)                                 0114 00120000
         LA    RTH,0(,RTH)                                         0114 00121000
*       RTH=ADDR(RTH->PRFSUNIT);    /* GET DATA ADDRESS IN THIS UNIT    00122000
*                                                            @Y17XASI*/ 00123000
         LA    @09,PRFSUNIT(,RTH)                                  0115 00124000
         LR    RTH,@09                                             0115 00125000
*       GEN NOSETS NOREFS;                                         0116 00126000
*                  /*                                        @Y17XASI*/ 00127000
         BCTR  RREM,0                   DECREMENT FOR EXECUTE  @Y17XASI 00128000
         EX    RREM,MOVEDATA            MOVE VARIABLE DATA     @Y17XASI 00129000
*       RENT=CENTRY;                /* RESTORE CURRENT ENTRY ADDRESS    00130000
*                                                            @Y17XASI*/ 00131000
         L     RENT,CENTRY(,RTBL)                                  0117 00132000
*     END;                          /*                       @Y17XASI*/ 00133000
*   RFY                                                            0119 00134000
*    (RWRK8,                                                       0119 00135000
*     RREM) UNRSTD;                 /* FREE UP WORK REGISTERS FOR PLS   00136000
*                                                            @Y17XASI*/ 00137000
@RF00110 DS    0H                                                  0120 00138000
*   IF RLEN<LENGTH(TRPIU) THEN      /* IF TOTAL PIU LENGTH IS LESS 0120 00139000
*                                      THAN TRACE ENTRY SIZE THE   0120 00140000
*                                      TRACE ENTRY MUST BE TRUNCATED    00141000
*                                                            @Y17XASI*/ 00142000
         LA    @07,28                                              0120 00143000
         CR    RLEN,@07                                            0120 00144000
         BNL   @RF00120                                            0120 00145000
*     DO;                           /* TRUNCATE SHORT ENTRY  @Y17XASI*/ 00146000
*       RENT=RENT+RLEN+LENGTH(TRTTCINS);/* POINT TO END OF DATA IN 0122 00147000
*                                      CURRENT ENTRY         @Y17XASI*/ 00148000
         LR    @15,RENT                                            0122 00149000
         ALR   @15,RLEN                                            0122 00150000
         AL    @15,@CF00326                                        0122 00151000
         LR    RENT,@15                                            0122 00152000
*       RLEN=LENGTH(TRPIU)-RLEN;    /* COMPUTE UNUSED LENGTH @Y17XASI*/ 00153000
         SLR   @07,RLEN                                            0123 00154000
         LR    RLEN,@07                                            0123 00155000
*       GEN NOREFS NOSETS;                                         0124 00156000
*                  /*                                        @Y17XASI*/ 00157000
         BCTR  RLEN,0                                        @YM05745   00158000
         EX    RLEN,CLRDATA             CLEAR VARIABLE DATA  @Y17XASI   00159000
*     END;                          /*                       @Y17XASI*/ 00160000
*   RFY                                                            0126 00161000
*     RPARM RSTD;                   /* RESTRICT REGISTER 1   @Y17XASI*/ 00162000
@RF00120 DS    0H                                                  0127 00163000
*   IF CENTRY=MENTRY THEN           /* 1ST HALF OF TABLE FULL?       */ 00164000
         L     @07,MENTRY(,RTBL)                                   0127 00165000
         C     @07,CENTRY(,RTBL)                                   0127 00166000
         BNE   @RF00127                                            0127 00167000
*     DO;                           /* YES                           */ 00168000
*       COUNTER=COUNTER+ONE;        /* INCREMENT COUNTER             */ 00169000
         LH    @00,ONE                                             0129 00170000
         AL    @00,COUNTER                                         0129 00171000
         ST    @00,COUNTER                                         0129 00172000
*       IF PARMCWOK=ON THEN         /* PREV WRITE-OUT COMPLETED?     */ 00173000
         TM    PARMCWOK,B'01000000'                                0130 00174000
         BNO   @RF00130                                            0130 00175000
*         DO;                       /* YES                           */ 00176000
*           TBL1FULL=ON;            /* MARK 1ST HALF-TABLE FULL      */ 00177000
*           TBL2FULL=OFF;           /* MARK 2ND HALF-TABLE EMPTY     */ 00178000
         OI    TBL1FULL(RTBL),B'10000000'                          0133 00179000
         NI    TBL2FULL(RTBL),B'10111111'                          0133 00180000
*           RPARM=FENTRY;           /* ADDR OF 1ST HALF-TABLE'S    0134 00181000
*                                      PREFIX                        */ 00182000
         L     RPARM,FENTRY(,RTBL)                                 0134 00183000
*           RPARM->TRCOUNT=COUNTER; /* SEQUENCE NUMBER               */ 00184000
         ST    @00,TRCOUNT(,RPARM)                                 0135 00185000
*           CENTRY=MENTRY+ENTRYSZ+HDRSZ;/* SET TO 2ND HALF           */ 00186000
         AH    @07,ENTRYSZ                                         0136 00187000
         AH    @07,HDRSZ                                           0136 00188000
         ST    @07,CENTRY(,RTBL)                                   0136 00189000
*         END;                                                     0137 00190000
*       ELSE                        /* 2ND-HALF WRITE NOT DONE,..    */ 00191000
*         CENTRY=FENTRY+HDRSZ;      /* ... REUSE 1ST HALF            */ 00192000
         B     @RC00130                                            0138 00193000
@RF00130 LH    @07,HDRSZ                                           0138 00194000
         AL    @07,FENTRY(,RTBL)                                   0138 00195000
         ST    @07,CENTRY(,RTBL)                                   0138 00196000
*     END;                                                         0139 00197000
*   ELSE                                                           0140 00198000
*     IF CENTRY=LENTRY THEN         /* 2ND HALF-TABLE FULL?          */ 00199000
         B     @RC00127                                            0140 00200000
@RF00127 CLC   CENTRY(4,RTBL),LENTRY(RTBL)                         0140 00201000
         BNE   @RF00140                                            0140 00202000
*       DO;                         /* YES                           */ 00203000
*         COUNTER=COUNTER+ONE;      /* INCREMENT COUNTER             */ 00204000
         LH    @07,ONE                                             0142 00205000
         AL    @07,COUNTER                                         0142 00206000
         ST    @07,COUNTER                                         0142 00207000
*         IF PARMCWOK=ON THEN       /* PREV WRITE-OUT COMPLETED?     */ 00208000
         TM    PARMCWOK,B'01000000'                                0143 00209000
         BNO   @RF00143                                            0143 00210000
*           DO;                     /* YES                           */ 00211000
*             TBL2FULL=ON;          /* MARK 2ND HALF-TABLE FULL      */ 00212000
*             TBL1FULL=OFF;         /* MARK 1ST HALF-TABLE EMPTY     */ 00213000
         OI    TBL2FULL(RTBL),B'01000000'                          0146 00214000
         NI    TBL1FULL(RTBL),B'01111111'                          0146 00215000
*             RPARM=MENTRY+ENTRYSZ; /* ADDR OF 2ND HALF-TBL'S PREFIX */ 00216000
         LH    RPARM,ENTRYSZ                                       0147 00217000
         AL    RPARM,MENTRY(,RTBL)                                 0147 00218000
*             RPARM->TRCOUNT=COUNTER;/* SEQUENCE NUMBER              */ 00219000
         ST    @07,TRCOUNT(,RPARM)                                 0148 00220000
*             CENTRY=FENTRY+HDRSZ;  /* SET TO 1ST HALF               */ 00221000
         LH    @07,HDRSZ                                           0149 00222000
         AL    @07,FENTRY(,RTBL)                                   0149 00223000
         ST    @07,CENTRY(,RTBL)                                   0149 00224000
*           END;                                                   0150 00225000
*         ELSE                      /* 1ST-HALF WRITE NOT DONE,..    */ 00226000
*           CENTRY=MENTRY+ENTRYSZ+HDRSZ;/* ..REUSE 2ND HALF          */ 00227000
         B     @RC00143                                            0151 00228000
@RF00143 LH    @07,ENTRYSZ                                         0151 00229000
         AL    @07,MENTRY(,RTBL)                                   0151 00230000
         AH    @07,HDRSZ                                           0151 00231000
         ST    @07,CENTRY(,RTBL)                                   0151 00232000
*       END;                                                       0152 00233000
*     ELSE                          /* NEITHER HALF-TABLE FULL       */ 00234000
*       CENTRY=CENTRY+ENTRYSZ;      /* BUMP POINTER TO NEXT ENTRY    */ 00235000
         B     @RC00140                                            0153 00236000
@RF00140 LH    @07,ENTRYSZ                                         0153 00237000
         AL    @07,CENTRY(,RTBL)                                   0153 00238000
         ST    @07,CENTRY(,RTBL)                                   0153 00239000
*   IF TRCOMWRT=ON&                 /* ARE WE USING COMWRITE? ...    */ 00240000
*       (TBL1FULL=ON|TBL2FULL=ON) THEN/* ... AND IS EITHER... ...  0154 00241000
*                                      HALF-TABLE FULL?              */ 00242000
@RC00140 DS    0H                                                  0154 00243000
@RC00127 TM    TRCOMWRT(RTBL),B'00001000'                          0154 00244000
         BNO   @RF00154                                            0154 00245000
         TM    TBL1FULL(RTBL),B'11000000'                          0154 00246000
         BZ    @RF00154                                            0154 00247000
*     DO;                           /* YES                           */ 00248000
*/********************************************************************/ 00249000
*/*                                                                  */ 00250000
*/*      THIS SUBROUTINE TESTS WHETHER EITHER OF THE 2 COMWRITE ECBS */ 00251000
*/*      IS AVAILABLE FOR USE.  IF AN ECB IS AVAILABLE, THE SWITCH   */ 00252000
*/*      ECBAVAIL IS TURNED ON, THE ECB ADDRESS IS PLACED IN RPARM   */ 00253000
*/*      AND THE CORRESPONDING COMWRITE PARAMETER LIST ADDRESS IS    */ 00254000
*/*      INITIALIZED.                                                */ 00255000
*/*                                                                  */ 00256000
*/********************************************************************/ 00257000
*                                                                  0156 00258000
*TESTECBS:                                                         0156 00259000
*       ECBAVAIL=OFF;               /* ASSUME NO ECB AVAILABLE       */ 00260000
TESTECBS NI    ECBAVAIL(RTBL),B'11011111'                          0156 00261000
*       TS(AVTCWTS1);               /* COMWRITE ECB 1 AVAILABLE?     */ 00262000
         TS    AVTCWTS1(RAVT)                                      0157 00263000
*       GEN(BZ ECB1);               /* BRANCH IF YES                 */ 00264000
         BZ ECB1                                                        00265000
*       TS(AVTCWTS2);               /* COMWRITE ECB 2 AVAILABLE?     */ 00266000
         TS    AVTCWTS2(RAVT)                                      0159 00267000
*       GEN(BNZ ENDTEST);           /* BRANCH IF NO, CAN'T PASS TO 0160 00268000
*                                      COMWRITE NOW                  */ 00269000
         BNZ ENDTEST                                                    00270000
*ECB2:                                                             0161 00271000
*       AVTCWPM2=ADDR(PARMLIST);    /* PARAMETER LIST ADDRESS        */ 00272000
ECB2     LA    @07,PARMLIST                                        0161 00273000
         ST    @07,AVTCWPM2(,RAVT)                                 0161 00274000
*       RPARM=ADDR(AVTCWEC2);       /* ADDR OF COMWRITE ECB 2        */ 00275000
         LA    RPARM,AVTCWEC2(,RAVT)                               0162 00276000
*       ECBAVAIL=ON;                /* INDICATE ECB AVAILABLE        */ 00277000
         OI    ECBAVAIL(RTBL),B'00100000'                          0163 00278000
*       GO TO ENDTEST;              /* EXIT                          */ 00279000
         B     ENDTEST                                             0164 00280000
*ECB1:                                                             0165 00281000
*       AVTCWPM1=ADDR(PARMLIST);    /* PARAMETER LIST ADDRESS        */ 00282000
ECB1     LA    @07,PARMLIST                                        0165 00283000
         ST    @07,AVTCWPM1(,RAVT)                                 0165 00284000
*       RPARM=ADDR(AVTCWEC1);       /* ADDR OF COMWRITE ECB 1        */ 00285000
         LA    RPARM,AVTCWEC1(,RAVT)                               0166 00286000
*       ECBAVAIL=ON;                /* INDICATE ECB AVAILABLE        */ 00287000
         OI    ECBAVAIL(RTBL),B'00100000'                          0167 00288000
*ENDTEST:                                                          0168 00289000
*       IF ECBAVAIL=ON THEN         /* IS AN ECB AVAILABLE?          */ 00290000
ENDTEST  TM    ECBAVAIL(RTBL),B'00100000'                          0168 00291000
         BNO   @RF00168                                            0168 00292000
*         DO;                       /* YES                           */ 00293000
*           AQECB=RPARM;            /* COMWRITE ECB ADDRESS          */ 00294000
         STCM  RPARM,7,AQECB                                       0170 00295000
*           AQTCB=AVTCWTCB;         /* COMWRITE TCB ADDRESS          */ 00296000
         L     @07,AVTCWTCB(,RAVT)                                 0171 00297000
         ST    @07,AQTCB                                           0171 00298000
*           IF TBL1FULL=ON THEN     /* 1ST HALF-TABLE FULL?          */ 00299000
         TM    TBL1FULL(RTBL),B'10000000'                          0172 00300000
         BNO   @RF00172                                            0172 00301000
*             DO;                   /* YES, SET 1ST-HALF PARMS       */ 00302000
*               PARMAREA=FENTRY;    /* DATA ADDRESS                  */ 00303000
         L     @07,FENTRY(,RTBL)                                   0174 00304000
         ST    @07,PARMAREA                                        0174 00305000
*               PARMLNTH=TLENGTH;   /* DATA LENGTH                   */ 00306000
         L     @07,TLENGTH(,RTBL)                                  0175 00307000
         STH   @07,PARMLNTH                                        0175 00308000
*               TBL1FULL=OFF;       /* RESET TABLE-FULL SWITCH       */ 00309000
         NI    TBL1FULL(RTBL),B'01111111'                          0176 00310000
*             END;                                                 0177 00311000
*           ELSE                    /* 2ND HALF-TABLE IS FULL        */ 00312000
*             DO;                   /* SET 2ND-HALF PARAMETERS       */ 00313000
         B     @RC00172                                            0178 00314000
@RF00172 DS    0H                                                  0179 00315000
*               PARMAREA=MENTRY+ENTRYSZ;/* DATA ADDRESS              */ 00316000
         LH    @07,ENTRYSZ                                         0179 00317000
         AL    @07,MENTRY(,RTBL)                                   0179 00318000
         ST    @07,PARMAREA                                        0179 00319000
*               PARMLNTH=TLENGTH;   /* DATA LENGTH                   */ 00320000
         L     @07,TLENGTH(,RTBL)                                  0180 00321000
         STH   @07,PARMLNTH                                        0180 00322000
*               TBL2FULL=OFF;       /* RESET TABLE-FULL SWITCH       */ 00323000
         NI    TBL2FULL(RTBL),B'10111111'                          0181 00324000
*             END;                                                 0182 00325000
*           PARMFLAG=''B;           /* RESET COMPLETION FLAG         */ 00326000
@RC00172 MVI   PARMFLAG,X'00'                                      0183 00327000
*           RPARM=ADDR(AQPARM);     /* ADDR OF SVC PARAMETER LIST    */ 00328000
         LA    RPARM,AQPARM                                        0184 00329000
*           GEN(AQCTL);             /* SVC TO POST COMWRITE ECB      */ 00330000
         AQCTL                                                          00331000
*         END;                                                     0186 00332000
*     END;                                                         0187 00333000
*   RETURN;                         /* RETURN TO CALLER      @Y17XASI*/ 00334000
@EL00001 DS    0H                                                  0188 00335000
@EF00001 DS    0H                                                  0188 00336000
@ER00001 LM    @14,@12,12(@13)                                     0188 00337000
         BR    @14                                                 0188 00338000
*   RFY                                                            0189 00339000
*     RPARM UNRSTD;                 /* FREE REGISTER 1 FOR PLS     0189 00340000
*                                                            @Y17XASI*/ 00341000
*   GEN DATA;                                                      0190 00342000
*   END IEDNTR                      /*                       @Y17XASI*/ 00343000
