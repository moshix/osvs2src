*       ;                                                          0191 00351000
         B     @EL00001                                            0191 00352000
@DATA    DS    0H                                                       00353000
         DS    0F                                                       00354000
         DS    0F                                                       00355000
@CF00059 DC    F'1'                                                     00356000
@CF00326 DC    F'4'                                                     00357000
@CF00811 DC    BL4'1'                                                   00358000
         DS    0D                                                       00359000
COUNTER  DC    F'0'                                                     00360000
PRF2TERM DS    F                                                        00361000
TJID     DC    H'0'                                                     00362000
ONE      DC    H'1'                                                     00363000
HDRSZ    DC    AL2(16)                                                  00364000
ENTRYSZ  DC    AL2(32)                                                  00365000
PARMLIST DS    CL16                                                     00366000
         ORG   PARMLIST                                                 00367000
PARMAREA DS    AL4                                                      00368000
@NM00126 DC    AL1(0)                                                   00369000
PARMFLAG DC    X'40'                                                    00370000
         ORG   PARMFLAG                                                 00371000
@NM00127 DS    BL1                                                      00372000
PARMCWOK EQU   PARMFLAG+0                                               00373000
@NM00128 EQU   PARMFLAG+0                                               00374000
         ORG   PARMLIST+6                                               00375000
PARMLNTH DS    FL2                                                      00376000
PARMCT   DC    F'0'                                                     00377000
PARMID   DC    CL4'PIUT'                                                00378000
         ORG   PARMLIST+16                                              00379000
AQPARM   DS    CL12                                                     00380000
         ORG   AQPARM                                                   00381000
@NM00129 DC    X'20'                                                    00382000
AQECB    DS    AL3                                                      00383000
@NM00130 DC    X'80'                                                    00384000
