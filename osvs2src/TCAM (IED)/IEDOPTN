         TITLE 'IEDOPTN, FIND A SPECIFIED OPTION FIELD                 *00001000
                        '                                               00002000
IEDOPTN  CSECT ,                                                   0001 00003000
@PROLOG  STM   @02,@12,28(@13)                                     0001 00004000
         BALR  @12,0                                               0001 00005000
@PSTART  DS    0H                                                  0001 00006000
         USING @PSTART,@12                                         0001 00007000
*                                                                  0075 00008000
IEDOPTN  IEDHJN START,HJN                                               00009000
*/*                                                                  */ 00010000
*                                                                  0076 00011000
*                                   /* INITIALIZATION                */ 00012000
*   RAVT=TCXAVT;                    /* ADDRESS OF TCAM AVT           */ 00013000
         L     @11,CVTPTR                                          0076 00014000
         L     @11,TCXPTR(,@11)                                    0076 00015000
         L     RAVT,TCXAVT(,@11)                                   0076 00016000
*   RTRM=RPARM;                     /* ADDRESS OF TTE                */ 00017000
         LR    RTRM,RPARM                                          0077 00018000
*   RF=NOTFOUND;                    /* SET BAD RETURN CODE           */ 00019000
         LA    RF,255                                              0078 00020000
*   ROPSWTCH=OFF;                   /* SET OPTION SWITCH OFF         */ 00021000
         SLR   ROPSWTCH,ROPSWTCH                                   0079 00022000
*   IF TRMOPTFN=ON THEN             /* OPTION FIELDS USED            */ 00023000
         TM    TRMOPTFN(IEDQTPTR),B'00000010'                      0080 00024000
         BNO   @RF00080                                            0080 00025000
*     DO;                                                          0081 00026000
*       RCOUNT=0;                   /* SET COUNTER TO ZERO           */ 00027000
         SLR   RCOUNT,RCOUNT                                       0082 00028000
*       RTBLPTR=OPTCHPTR;           /* POINTER TO OPTION           0083 00029000
*                                      CHARACTERISTICS TABLE         */ 00030000
         L     @11,AVTOPTPT(,RAVT)                                 0083 00031000
         L     RTBLPTR,OPTCHPTR(,@11)                              0083 00032000
*       DO WHILE ROPSWTCH=OFF&OPTCHLEN<NOTFOUND;/* NOT END OF TABLE     00033000
*                                      AND MATCH NOT FOUND...        */ 00034000
         B     @DE00084                                            0084 00035000
@DL00084 DS    0H                                                  0085 00036000
*         RCOUNT=RCOUNT+1;          /* ADD ONE TO COUNTER            */ 00037000
         LA    @11,1                                               0085 00038000
         ALR   RCOUNT,@11                                          0085 00039000
*         IF RZERO->PARMNAME=RTBLPTR->OPTCHNAM THEN/* OPTION NAME  0086 00040000
*                                      FOUND                         */ 00041000
         LR    @10,RZERO                                           0086 00042000
         CLC   PARMNAME(8,@10),OPTCHNAM(RTBLPTR)                   0086 00043000
         BNE   @RF00086                                            0086 00044000
*           DO;                                                    0087 00045000
*             ROPSWTCH=ON;          /* SET SWITCH ON                 */ 00046000
         LA    ROPSWTCH,1                                          0088 00047000
*             IF RCOUNT<=TRMOPNO&CUROPTN(RCOUNT)^=NOTFOUND THEN/*  0089 00048000
*                                      OPTION SPECIFIED FOR THIS   0089 00049000
*                                      TERMINAL AND THE OPTION DATA     00050000
*                                      IS VALID                      */ 00051000
         SLR   @10,@10                                             0089 00052000
         IC    @10,TRMOPNO(,IEDQTPTR)                              0089 00053000
         CR    RCOUNT,@10                                          0089 00054000
         BH    @RF00089                                            0089 00055000
         LA    @10,20                                              0089 00056000
         ALR   @10,IEDQTPTR                                        0089 00057000
         LR    @07,@10                                             0089 00058000
         ALR   @07,RCOUNT                                          0089 00059000
         BCTR  @07,0                                               0089 00060000
         CLI   CUROPTN(@07),X'FF'                                  0089 00061000
         BE    @RF00089                                            0089 00062000
*               DO;                                                0090 00063000
*                 RZERO=RTBLPTR->OPTCHLEN+1;/* OPTION DATA LENGTH    */ 00064000
         SLR   RZERO,RZERO                                         0091 00065000
         IC    RZERO,OPTCHLEN(,RTBLPTR)                            0091 00066000
         ALR   @11,RZERO                                           0091 00067000
         LR    RZERO,@11                                           0091 00068000
*                 RPARM=AVTOPTPT+TRMOPTBL+CUROPTN(RCOUNT);/* ADDRESS    00069000
*                                      OF OPTION DATA FIELD          */ 00070000
         LH    RPARM,TRMOPTBL(,IEDQTPTR)                           0092 00071000
         N     RPARM,@CF01148                                      0092 00072000
         AL    RPARM,AVTOPTPT(,RAVT)                               0092 00073000
         BCTR  @10,0                                               0092 00074000
         SLR   @11,@11                                             0092 00075000
         IC    @11,CUROPTN(RCOUNT,@10)                             0092 00076000
         ALR   RPARM,@11                                           0092 00077000
*                 RF=0;             /* SET GOOD RETURN CODE          */ 00078000
         SLR   RF,RF                                               0093 00079000
*               END;                                               0094 00080000
*           END;                                                   0095 00081000
*         ELSE                                                     0096 00082000
*           RTBLPTR=RTBLPTR+LENGTH(OPTCHENT);/* BUMP TO NEXT ENTRY   */ 00083000
         B     @RC00086                                            0096 00084000
@RF00086 AL    RTBLPTR,@CF01142                                    0096 00085000
*       END;                                                       0097 00086000
@RC00086 DS    0H                                                  0097 00087000
@DE00084 LTR   ROPSWTCH,ROPSWTCH                                   0097 00088000
         BNZ   @DC00084                                            0097 00089000
         CLI   OPTCHLEN(RTBLPTR),X'FF'                             0097 00090000
         BL    @DL00084                                            0097 00091000
@DC00084 DS    0H                                                  0098 00092000
*     END;                                                         0098 00093000
*   END IEDOPTN                                                    0099 00094000
*                                                                  0099 00095000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 00096000
*/*%INCLUDE SYSLIB  (TAVTD   )                                       */ 00097000
*/*%INCLUDE SYSLIB  (CVT     )                                       */ 00098000
*/*%INCLUDE SYSLIB  (TTCXD   )                                       */ 00099000
*/*%INCLUDE SYSLIB  (TTRMD   )                                       */ 00100000
*                                                                  0099 00101000
*       ;                                                          0099 00102000
@EL00001 DS    0H                                                  0099 00103000
@EF00001 DS    0H                                                  0099 00104000
@ER00001 LM    @02,@12,28(@13)                                     0099 00105000
         BR    @14                                                 0099 00106000
