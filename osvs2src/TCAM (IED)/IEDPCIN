         L     @15,SAVTLCBS(,@10)                                  0312 00060000
         BALR  @14,@15                                             0312 00061000
*                     RPARM=LCBTTCIN;/* GET NCP TTCIN        @YM09076*/ 00062000
         LH    RPARM,LCBTTCIN(,IEDQLPTR)                           0313 00063000
         N     RPARM,@CF01667                                      0313 00064000
*                     CALL IEDQTNT; /* GET NCP TERM ADDR     @YM09076*/ 00065000
