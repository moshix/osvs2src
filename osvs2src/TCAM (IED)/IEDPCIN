*                     RFY                                          0315 00068000
*                       IEDQTRM BASED(RPARM);/*              @YM09076*/ 00069000
*                     IF TRMRSACT=ON THEN/* NCP ACTIVE       @YM09076*/ 00070000
         LR    @10,RPARM                                           0316 00071000
         AL    @10,@CF01668                                        0316 00072000
         TM    TRMRSACT-2(@10),B'10000000'                         0316 00073000
         BNO   @RF00316                                            0316 00074000
*                       DO;         /* YES, NOT TRICKLE TRAFFIC    0317 00075000
*                                                            @YM09076*/ 00076000
*                         RFY                                      0318 00077000
*                           IEDQTRM BASED(IEDQTPTR);/*       @YM09076*/ 00078000
*                         PRFQCBA=SAVTSAI;/* FID0 STATUS ANALYSIS  0319 00079000
*                                                            @YM09076*/ 00080000
