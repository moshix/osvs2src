*                 PLTNTDAF=R15RC;   /* SET UP DAF TNT OFFSET         */ 01262000
         STH   R15RC,PLTNTDAF                                      0685 01263000
*               ELSE                /* DAF CONVERSION NOT SUCESSFUL  */ 01264000
*                 PLTNTDAF=0;       /* SET DAF TNT OFFSET TO 0       */ 01265000
         B     @RC00684                                            0686 01266000
@RF00684 SLR   @09,@09                                             0686 01267000
         STH   @09,PLTNTDAF                                        0686 01268000
*/*                                                                  */ 01269000
*                                                                  0687 01270000
*               CALL RSPBUILD;      /* BUILD AN ERROR RESPONSE       */ 01271000
@RC00684 BAL   @14,RSPBUILD                                        0687 01272000
*               CALL POSTPCO;       /* POST IT TO PATH CONTROL     0688 01273000
*                                      OUTBOUND                      */ 01274000
         BAL   @14,POSTPCO                                         0688 01275000
*             END;                                                 0689 01276000
*           ELSE                    /* INVALID OAF                   */ 01277000
*             DO;                                                  0690 01278000
