         BALR  @14,@15                                             0322 00090000
*                       END;        /*                       @YM09076*/ 00091000
*                     ELSE          /* DISPOSE OF            @YM09076*/ 00092000
*                       DO;         /* TRICKLE TRAFFIC       @YM09076*/ 00093000
         B     @RC00316                                            0324 00094000
@RF00316 DS    0H                                                  0325 00095000
*                         RPARM=CURPIUP;/* ADDR OF PIU       @YM08527*/ 00096000
         L     RPARM,CURPIUP                                       0325 00097000
*                         CALL DISCARD;/* DISCARD PIU        @YM08527*/ 00098000
         BAL   @14,DISCARD                                         0326 00099000
*                       END;        /*                       @YM08527*/ 00100000
*                   END;            /*                       @YM09076*/ 00101000
*                 ELSE              /* NOT 'TRICKLE OR 'DRIBBLE'   0329 00102000
*                                                            @YM08527*/ 00103000
*                   DO;             /*                       @YM08527*/ 00104000
         B     @RC00309                                            0329 00105000
@RF00309 DS    0H                                                  0330 00106000
*                     PRFQCBA=SAVTSAI;/* ADDR OF FID0 STATUS ANALYSIS*/ 00107000
