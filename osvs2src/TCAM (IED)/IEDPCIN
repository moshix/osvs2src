         TITLE 'IEDPCIN, PATH CONTROL INBOUND                          *00001000
                        '                                               00002000
IEDPCIN  CSECT ,                                                   0001 00003000
@PROLOG  DS    0H                                                  0002 00004000
         DC    A(0)                START OF IEDPCIN QCB                 00005000
         DC    A(0)                                                     00006000
         DC    X'06'               STCB                                 00007000
         DC    AL3(*-1)                                                 00008000
         USING *,RBASE                                                  00009000
         LR    RBASE,R15           BASE ADDRESSABILITY                  00010000
IEDPCIN  IEDHJN START,HJN                                               00011000
*                                                                  0298 00012000
*/********************************************************************/ 00013000
*/*                                                                  */ 00014000
*/*     M A I N     R O U T I N E                                    */ 00015000
*/*                                                                  */ 00016000
*/********************************************************************/ 00017000
*                                                                  0298 00018000
*   NEXTPIUP=RPARM;                 /* SET NEXT PIU TO BE DEBLOCKED     00019000
*                                      =TO 1ST PIU IN PASSED BTU     */ 00020000
         ST    RPARM,NEXTPIUP                                      0298 00021000
*   DO WHILE(NEXTPIUP^=0);          /* DEBLOCK LOOP                  */ 00022000
         B     @DE00299                                            0299 00023000
@DL00299 DS    0H                                                  0300 00024000
*/*                                                                  */ 00025000
*                                                                  0300 00026000
*     CALL GETPIU;                  /* DEBLOCK NEXT PIU              */ 00027000
         BAL   @14,GETPIU                                          0300 00028000
*/*  CURPIUP HAS A PTR TO THE PIU JUST DEBLOCKED                     */ 00029000
*                                                                  0301 00030000
*     CALL OAFDAFCK;                /* CHECK OAF & DAF. TRACE PIU.   */ 00031000
         BAL   @14,OAFDAFCK                                        0301 00032000
*     IF RC=GOOD THEN               /* O.K. SO FAR                   */ 00033000
         C     RC,@CF00466                                         0302 00034000
         BNE   @RF00302                                            0302 00035000
*       DO;                         /* YES                           */ 00036000
*         CALL DCFCK;               /* CHECK DATA COUNT FIELD        */ 00037000
         BAL   @14,DCFCK                                           0304 00038000
*         IF RC=GOOD THEN           /* DATA COUNT O.K.               */ 00039000
         C     RC,@CF00466                                         0305 00040000
         BNE   @RF00305                                            0305 00041000
*           DO;                     /* YES                           */ 00042000
*             IF TTHFTYPE=TTHFID0 THEN/* FID0 PIU?                   */ 00043000
         TM    TTHFTYPE(PIUSTART),B'11110000'                      0307 00044000
         BNZ   @RF00307                                            0307 00045000
*               DO;                 /* YES                           */ 00046000
*                 IF TRMSPACT=OFF THEN/* SUPERIOR RESOURCE NOT ACTIVE   00047000
*                                                            @YM08527*/ 00048000
         LR    @10,IEDQTPTR                                        0309 00049000
         AL    @10,@CF01666                                        0309 00050000
         TM    TRMSPACT(@10),B'00010000'                           0309 00051000
         BNZ   @RF00309                                            0309 00052000
*                   DO;             /* YES-THIS IS 'TRICKLE' OR    0310 00053000
*                                      'DRIBBLE' TRAFFIC     @YM08527*/ 00054000
*                     RPARM=PLTNTNET;/* GET NETWORK ADDR     @YM09076*/ 00055000
