*                     PRFPRI=PRIFIDIN;/* POST PRIORITY               */ 00111000
         MVI   PRFPRI(@10),X'E4'                                   0331 00112000
*                     RPARM=CURPIUP;/* PIU TO BE POSTED              */ 00113000
         L     RPARM,CURPIUP                                       0332 00114000
*                     CALL DSPPOSTR;/* POST PIU TO IEDSAI            */ 00115000
         LA    @15,DSPPOSTR(,RDISP)                                0333 00116000
         BALR  @14,@15                                             0333 00117000
*                   END;            /*                       @YM08527*/ 00118000
*               END;                                               0335 00119000
*             ELSE                  /* FID1 PIU                      */ 00120000
*               DO;                                                0336 00121000
         B     @RC00307                                            0336 00122000
@RF00307 DS    0H                                                  0337 00123000
*                 CALL SEGASSEM;    /* ASSEMBLE SEGMENTS             */ 00124000
         BAL   @14,SEGASSEM                                        0337 00125000
*                 IF RC=GO THEN     /* WHOLE PIU READY TO GO         */ 00126000
         C     RC,@CF00466                                         0338 00127000
         BNE   @RF00338                                            0338 00128000
*                   CALL POSTCSCI;  /* POST WHOLE PIU TO COMMON    0339 00129000
*                                      SESSION CONTROL INBOUND     0339 00130000
*                                      (IEDCSCIN) QCB                */ 00131000
         BAL   @14,POSTCSCI                                        0339 00132000
*               END;                                               0340 00133000
@RF00338 DS    0H                                                  0341 00134000
*           END;                                                   0341 00135000
@RC00307 DS    0H                                                  0342 00136000
*       END;                                                       0342 00137000
@RF00305 DS    0H                                                  0343 00138000
*   END;                            /* END OF LOOP                   */ 00139000
@RF00302 DS    0H                                                  0343 00140000
@DE00299 L     @10,NEXTPIUP                                        0343 00141000
         LTR   @10,@10                                             0343 00142000
         BNZ   @DL00299                                            0343 00143000
*/*                                                                  */ 00144000
*                                                                  0344 00145000
*   CALL DSPDISP;                   /* WHEN LOOP DONE-EXIT           */ 00146000
         LA    @15,DSPDISP(,RDISP)                                 0344 00147000
         BALR  @14,@15                                             0344 00148000
*                                                                  0345 00149000
*/********************************************************************/ 00150000
*/*                                                                  */ 00151000
*/* THIS ROUTINE COMBINES THE CURRENT PIU WITH THE ALREADY           */ 00152000
*/* PARTIALLY ACCUMULATED SEGMENT.                                   */ 00153000
*/*                                                                  */ 00154000
*/********************************************************************/ 00155000
*                                                                  0345 00156000
*COMBINE:                                                          0345 00157000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0345 00158000
@EL00001 DS    0H                                                  0345 00159000
@EF00001 DS    0H                                                  0345 00160000
@ER00001 BR    @14                                                 0345 00161000
COMBINE  DS    0H                                                  0346 00162000
*   R14SV10=R14;                    /* SAVE RETURN ADDRESS           */ 00163000
         ST    R14,R14SV10                                         0346 00164000
*/*                                                                  */ 00165000
*                                                                  0347 00166000
*   UNITPTR=SIBRUSEG;               /* ADDR OF PARTIALLY ACCUMULATED    00167000
*                                      SEGMENT                       */ 00168000
         L     @09,SIBRUSEG(,SIBPTR)                               0347 00169000
         ST    @09,UNITPTR                                         0347 00170000
*   DO WHILE(UNITPTR->PRFTIC^=INVALTIC);/* FIND LAST UNIT OF       0348 00171000
*                                      PARTIALLY ACCUMULATED SEGMENT */ 00172000
         B     @DE00348                                            0348 00173000
@DL00348 DS    0H                                                  0349 00174000
*     UNITPTR=UNITPTR->PRFTIC;      /* GET NEXT UNIT                 */ 00175000
         L     @09,UNITPTR                                         0349 00176000
         L     @09,PRFTIC(,@09)                                    0349 00177000
         ST    @09,UNITPTR                                         0349 00178000
*   END;                            /* END OF LOOP                   */ 00179000
@DE00348 L     @09,UNITPTR                                         0350 00180000
         CLC   PRFTIC(4,@09),@CB01097                              0350 00181000
         BNE   @DL00348                                            0350 00182000
*/*                                                                  */ 00183000
*                                                                  0351 00184000
*   UNITPTR->PRFTI0=CURPIUP;        /* CHAIN 1ST UNIT OF CURRENT PIU    00185000
*                                      TO LAST UNIT OF THE PARTIALLY    00186000
*                                      ACCUMULATED SEGMENT           */ 00187000
         L     @05,CURPIUP                                         0351 00188000
         STCM  @05,7,PRFTI0(@09)                                   0351 00189000
*/* CALCULATE ADDRESS OF TH IN THE PARTIALLY ACCUMULATED SEGMENT     */ 00190000
*                                                                  0352 00191000
*   ASEGTH=ADDR(SIBRUSEG->PRFPIUO)+SIBRUSEG->PRFPIUO;              0352 00192000
*                                                                  0352 00193000
         L     @09,SIBRUSEG(,SIBPTR)                               0352 00194000
         LA    @00,PRFPIUO(,@09)                                   0352 00195000
         SLR   @04,@04                                             0352 00196000
         IC    @04,PRFPIUO(,@09)                                   0352 00197000
         ALR   @00,@04                                             0352 00198000
         ST    @00,ASEGTH                                          0352 00199000
*/* CALCULATE AMOUNT OF DATA IN LAST UNIT OF PART. ACCUM. SEGMENT    */ 00200000
*                                                                  0353 00201000
*   REM=(ASEGTH->TTHDCF+LENGTH(IEDTH)+SIBRUSEG->PRFPIUO)//AVTKEYLE;     00202000
         LA    @09,10                                              0353 00203000
         LR    @05,@00                                             0353 00204000
         SLR   @00,@00                                             0353 00205000
         ICM   @00,3,TTHDCF(@05)                                   0353 00206000
         ALR   @09,@00                                             0353 00207000
         ALR   @09,@04                                             0353 00208000
         LR    @04,@09                                             0353 00209000
         SRDA  @04,32                                              0353 00210000
         LH    @09,AVTKEYLE(,IEDQAPTR)                             0353 00211000
         DR    @04,@09                                             0353 00212000
         STH   @04,REM                                             0353 00213000
*   IF REM=0 THEN                   /* LAST UNIT COMPLETELY FULL     */ 00214000
         LTR   @04,@04                                             0354 00215000
         BNZ   @RF00354                                            0354 00216000
*     DO;                           /* YES                           */ 00217000
*       PLMVTU=CURPIUP;             /* SET TARGET UNIT ADDRESS(1ST 0356 00218000
*                                      UNIT OF CURRENT PIU)          */ 00219000
         L     @09,CURPIUP                                         0356 00220000
         LA    @05,PLMV                                            0356 00221000
         ST    @09,PLMVTU(,@05)                                    0356 00222000
*       PLMVTD=ADDR(PRFPIUO);       /* SET TARGET DATA ADDRESS       */ 00223000
         L     @09,IEDQPPTR                                        0357 00224000
         LA    @09,PRFPIUO(,@09)                                   0357 00225000
         ST    @09,PLMVTD(,@05)                                    0357 00226000
*     END;                                                         0358 00227000
*   ELSE                            /* LAST UNIT NOT COMPLETELY FULL */ 00228000
*     DO;                                                          0359 00229000
         B     @RC00354                                            0359 00230000
@RF00354 DS    0H                                                  0360 00231000
*       PLMVTU=UNITPTR;             /* SET TARGET UNIT ADDRESS(LAST     00232000
*                                      UNIT OF PART. ACCUM. SEGMENT) */ 00233000
         L     @09,UNITPTR                                         0360 00234000
         LA    @05,PLMV                                            0360 00235000
         ST    @09,PLMVTU(,@05)                                    0360 00236000
*       PLMVTD=ADDR(UNITPTR->PRFPIUO)+REM;/* TARGET DATA ADDRESS     */ 00237000
         LA    @09,PRFPIUO(,@09)                                   0361 00238000
         LH    @00,REM                                             0361 00239000
         N     @00,@CF01667                                        0361 00240000
         ALR   @09,@00                                             0361 00241000
         ST    @09,PLMVTD(,@05)                                    0361 00242000
*     END;                                                         0362 00243000
*                                                                  0362 00244000
*/*                                                                  */ 00245000
*                                                                  0363 00246000
*   PLMVSU=CURPIUP;                 /* SET SOURCE UNIT ADDRESS       */ 00247000
@RC00354 LA    @09,PLMV                                            0363 00248000
         L     @00,CURPIUP                                         0363 00249000
         ST    @00,PLMVSU(,@09)                                    0363 00250000
*   PLMVSD=ADDR(IEDRH);             /* SET SOURCE DATA ADDRESS(1ST 0364 00251000
*                                      BYTE AFTER TH). NOTE:       0364 00252000
*                                      SUBSEQUENT SEGMENTS HAVE NO 0364 00253000
*                                      RH. THE RU FOLLOWS THE TH.    */ 00254000
         LA    @00,10                                              0364 00255000
         ALR   @00,PIUSTART                                        0364 00256000
         ST    @00,PLMVSD(,@09)                                    0364 00257000
*   PLMVLNTH=TTHDCF;                /* LENGTH OF DATA TO BE MOVED    */ 00258000
         MVC   PLMVLNTH(2,@09),TTHDCF(PIUSTART)                    0365 00259000
*   SAVEDCF=TTHDCF;                 /* SAVE DATA COUNT FIELD         */ 00260000
         MVC   SAVEDCF(2),TTHDCF(PIUSTART)                         0366 00261000
*   PLMVFLG1=0;                     /* INIT FLAG BYTE TO ZEROS       */ 00262000
         MVI   PLMVFLG1(@09),X'00'                                 0367 00263000
*   PLMVUNCF=ON;                    /* TURN ON 'UNCHAIN' FLAG        */ 00264000
         OI    PLMVUNCF(@09),B'10000000'                           0368 00265000
*/*                                                                  */ 00266000
*                                                                  0369 00267000
*   RPARM=ADDR(PLMV);               /* ADDRESS OF PARAMETER LIST     */ 00268000
         LR    RPARM,@09                                           0369 00269000
*   CALL IEDMVBFR;                  /* ADD THE CURRENT PIU TO THE  0370 00270000
*                                      PARTIALLY ACCUMULATED SEGMENT */ 00271000
         L     @10,AVTSAVTP(,IEDQAPTR)                             0370 00272000
         L     @15,SAVTMVBF(,@10)                                  0370 00273000
         BALR  @14,@15                                             0370 00274000
*   IF PLMVUNCA>0 THEN              /* ARE THERE ANY               0371 00275000
*                                      UNUSED(UNCHAINED) UNITS       */ 00276000
         LA    @10,PLMV                                            0371 00277000
         L     @10,PLMVUNCA(,@10)                                  0371 00278000
         LTR   @10,@10                                             0371 00279000
         BNP   @RF00371                                            0371 00280000
*     DO;                           /* YES                           */ 00281000
*       RPARM=PLMVUNCA;             /* ADDRESS OF 1ST UNCHAINED UNIT */ 00282000
         LR    RPARM,@10                                           0373 00283000
*       CALL DISCARD;               /* DISCARD UNCHAINED UNITS       */ 00284000
         BAL   @14,DISCARD                                         0374 00285000
*     END;                                                         0375 00286000
*                                                                  0375 00287000
*/*                                                                  */ 00288000
*                                                                  0376 00289000
*   ASEGTH->TTHDCF=ASEGTH->TTHDCF+SAVEDCF;/* UPDATE DATA COUNT OF THE   00290000
*                                      ACCUMULATED SEGMENT           */ 00291000
@RF00371 L     @10,ASEGTH                                          0376 00292000
         SLR   @09,@09                                             0376 00293000
         ICM   @09,3,TTHDCF(@10)                                   0376 00294000
         LH    @00,SAVEDCF                                         0376 00295000
         N     @00,@CF01667                                        0376 00296000
         ALR   @09,@00                                             0376 00297000
         STCM  @09,3,TTHDCF(@10)                                   0376 00298000
*   R14=R14SV10;                    /* RESTORE RETURN ADDRESS        */ 00299000
         L     R14,R14SV10                                         0377 00300000
*   RETURN;                         /* RETURN TO CALLER              */ 00301000
@EL00002 DS    0H                                                  0378 00302000
@EF00002 DS    0H                                                  0378 00303000
@ER00002 BR    @14                                                 0378 00304000
*   END COMBINE;                                                   0379 00305000
*                                                                  0380 00306000
*/********************************************************************/ 00307000
*/*                                                                  */ 00308000
*/* THIS ROUTINE VALIDITY CHECKS THE TH DATA COUNT FIELD.            */ 00309000
*/*                                                                  */ 00310000
*/********************************************************************/ 00311000
*                                                                  0380 00312000
*DCFCK:                                                            0380 00313000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0380 00314000
DCFCK    DS    0H                                                  0381 00315000
*   R14SV04=R14;                    /* SAVE RETURN REG               */ 00316000
         ST    R14,R14SV04                                         0381 00317000
*/*                                                                  */ 00318000
*                                                                  0382 00319000
*   RC=GOOD;                        /* INIT RETURN CODE TO GOOD      */ 00320000
         SLR   RC,RC                                               0382 00321000
*/*                                                                  */ 00322000
*                                                                  0383 00323000
*   IF TTHFTYPE=TTHFID1 THEN        /* FID1 PIU                      */ 00324000
         TM    TTHFTYPE(PIUSTART),B'00010000'                      0383 00325000
         BNO   @RF00383                                            0383 00326000
         TM    TTHFTYPE(PIUSTART),B'11100000'                      0383 00327000
         BNZ   @RF00383                                            0383 00328000
*     DO;                           /* YES                           */ 00329000
*       IF TTHBBIU=ON THEN          /* BEGINNING OF BIU              */ 00330000
         TM    TTHBBIU(PIUSTART),B'00001000'                       0385 00331000
         BNO   @RF00385                                            0385 00332000
*         DO;                       /* YES                           */ 00333000
*           IF TTHDCF>=3 THEN       /* IS RH PRESENT                 */ 00334000
         CLC   TTHDCF(2,PIUSTART),@CH00065                         0387 00335000
         BL    @RF00387                                            0387 00336000
*             DO;                   /* YES                           */ 00337000
*               IF TTHEBIU=ON THEN  /* END OF BIU                    */ 00338000
         TM    TTHEBIU(PIUSTART),B'00000100'                       0389 00339000
         BNO   @RF00389                                            0389 00340000
*                 DO;               /* YES                           */ 00341000
*                   IF TRHRUCAT=TRHFMD THEN/* FM DATA                */ 00342000
         LA    @09,10                                              0391 00343000
         ALR   @09,PIUSTART                                        0391 00344000
         TM    TRHRUCAT(@09),B'01100000'                           0391 00345000
         BNZ   @RF00391                                            0391 00346000
*                     DO;           /* YES                           */ 00347000
*                       IF TRHSDI=ON&/* SENSE DATA INCLUDED AND...   */ 00348000
*                           TTHDCF<7 THEN/* DATA COUNT < 7           */ 00349000
         TM    TRHSDI(@09),B'00000100'                             0393 00350000
         BNO   @RF00393                                            0393 00351000
         CLC   TTHDCF(2,PIUSTART),@CH00568                         0393 00352000
         BNL   @RF00393                                            0393 00353000
*                         DO;       /* YES                           */ 00354000
*                           SNC=SNSNGDCF;/* INCONSISTANT DATA COUNT  */ 00355000
         MVC   SNC(4),@CB01564                                     0395 00356000
*                           CALL SNDERRSP;/* SEND -RSP               */ 00357000
         BAL   @14,SNDERRSP                                        0396 00358000
*                           RC=BAD; /* SET BAD RETURN CODE           */ 00359000
         LA    RC,4                                                0397 00360000
*                         END;                                     0398 00361000
*                     END;                                         0399 00362000
*                   ELSE            /* NOT FM DATA                   */ 00363000
*                     DO;                                          0400 00364000
         B     @RC00391                                            0400 00365000
@RF00391 DS    0H                                                  0401 00366000
*                       IF TRHSDI=ON THEN/* SENSE DATA INCLUDED      */ 00367000
         TM    TRHSDI+10(PIUSTART),B'00000100'                     0401 00368000
         BNO   @RF00401                                            0401 00369000
*                         DO;       /* YES                           */ 00370000
*                           IF TTHDCF<8 THEN/* DATA COUNT < 8        */ 00371000
         CLC   TTHDCF(2,PIUSTART),@CH00044                         0403 00372000
         BNL   @RF00403                                            0403 00373000
*                             DO;   /* YES                           */ 00374000
*                               SNC=SNSNGDCF;/* INCONSISTENT DATA CNT*/ 00375000
         MVC   SNC(4),@CB01564                                     0405 00376000
*                               CALL SNDERRSP;/* SEND -RSP           */ 00377000
         BAL   @14,SNDERRSP                                        0406 00378000
*                               RC=BAD;/* SET BAD RETURN CODE        */ 00379000
         LA    RC,4                                                0407 00380000
*                             END;                                 0408 00381000
*                         END;                                     0409 00382000
*                       ELSE        /* SENSE DATA NOT INCLUDED       */ 00383000
*                         DO;                                      0410 00384000
         B     @RC00401                                            0410 00385000
@RF00401 DS    0H                                                  0411 00386000
*                           IF TTHDCF<4 THEN/* DATA COUNT < 4        */ 00387000
         CLC   TTHDCF(2,PIUSTART),@CH00326                         0411 00388000
         BNL   @RF00411                                            0411 00389000
*                             DO;   /* YES                           */ 00390000
*                               SNC=SNSNGDCF;/* INCONSISTENT DATA CNT*/ 00391000
         MVC   SNC(4),@CB01564                                     0413 00392000
*                               CALL SNDERRSP;/* SEND - RSP          */ 00393000
         BAL   @14,SNDERRSP                                        0414 00394000
*                               RC=BAD;/* SET BAD RETURN             */ 00395000
         LA    RC,4                                                0415 00396000
*                             END;                                 0416 00397000
*                         END;                                     0417 00398000
*                     END;                                         0418 00399000
*                 END;                                             0419 00400000
*               ELSE                /* NOT END OF BIU                */ 00401000
*                 DO;                                              0420 00402000
         B     @RC00389                                            0420 00403000
@RF00389 DS    0H                                                  0421 00404000
*                   IF TTHDCF<10 THEN/* DATA COUNT < 10              */ 00405000
         CLC   TTHDCF(2,PIUSTART),@CH00983                         0421 00406000
         BNL   @RF00421                                            0421 00407000
*                     DO;           /* YES                           */ 00408000
*                       RPARM=CURPIUP;/* PIU TO BE DISCARDED         */ 00409000
         L     RPARM,CURPIUP                                       0423 00410000
*                       CALL DISCARD;/* DISCARD PIU                  */ 00411000
         BAL   @14,DISCARD                                         0424 00412000
*                       RC=BAD;     /* SET BAD RETURN CODE           */ 00413000
         LA    RC,4                                                0425 00414000
*                     END;                                         0426 00415000
*                 END;                                             0427 00416000
*             END;                                                 0428 00417000
*           ELSE                    /* NO RH PRESENT                 */ 00418000
*             DO;                                                  0429 00419000
         B     @RC00387                                            0429 00420000
@RF00387 DS    0H                                                  0430 00421000
*               RPARM=CURPIUP;      /* PIU TO BE DISCARDED           */ 00422000
         L     RPARM,CURPIUP                                       0430 00423000
*               CALL DISCARD;       /* DISCARD PIU                   */ 00424000
         BAL   @14,DISCARD                                         0431 00425000
*               RC=BAD;             /* SET RETURN CODE AS BAD        */ 00426000
         LA    RC,4                                                0432 00427000
*             END;                                                 0433 00428000
*         END;                                                     0434 00429000
*     END;                                                         0435 00430000
*   ELSE                            /* FID0 PIU                      */ 00431000
*     DO;                                                          0436 00432000
         B     @RC00383                                            0436 00433000
@RF00383 DS    0H                                                  0437 00434000
*       IF TTHDCF<10 THEN           /* DATA COUNT < 10               */ 00435000
         CLC   TTHDCF(2,PIUSTART),@CH00983                         0437 00436000
         BNL   @RF00437                                            0437 00437000
*         DO;                       /* YES                           */ 00438000
*           RPARM=CURPIUP;          /* PIU TO BE DISCARDED           */ 00439000
         L     RPARM,CURPIUP                                       0439 00440000
*           CALL DISCARD;           /* DISCARD PIU                   */ 00441000
         BAL   @14,DISCARD                                         0440 00442000
*           RC=BAD;                 /* SET BAD RETURN                */ 00443000
         LA    RC,4                                                0441 00444000
*         END;                                                     0442 00445000
*     END;                                                         0443 00446000
*                                                                  0443 00447000
@RF00437 DS    0H                                                  0444 00448000
*/*                                                                  */ 00449000
*                                                                  0444 00450000
*   R14=R14SV04;                    /* RESTORE RETURN REG            */ 00451000
@RC00383 L     R14,R14SV04                                         0444 00452000
*   RETURN;                         /* RETURN TO CALLER              */ 00453000
@EL00003 DS    0H                                                  0445 00454000
@EF00003 DS    0H                                                  0445 00455000
@ER00003 BR    @14                                                 0445 00456000
*   END DCFCK;                                                     0446 00457000
*                                                                  0447 00458000
*/********************************************************************/ 00459000
*/*                                                                  */ 00460000
*/* THIS ROUTINE DISCARDS ALL THE UNITS IN A PIU(BUFFER). IT POSTS   */ 00461000
*/* THEM TO BUF  RETURN(IEDQGB).                                     */ 00462000
*/*       INPUT= REG1 POINTS TO PIU TO BE DISCARDED                  */ 00463000
*/*                                                                  */ 00464000
*/********************************************************************/ 00465000
*                                                                  0447 00466000
*DISCARD:                                                          0447 00467000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0447 00468000
DISCARD  DS    0H                                                  0448 00469000
*   R14SV07=R14;                    /* SAVE RETURN REG               */ 00470000
         ST    R14,R14SV07                                         0448 00471000
*/*                                                                  */ 00472000
*                                                                  0449 00473000
*   RPARM->PRFNBUNT=0;              /* SET NUMBER OF UNITS = 0 (THIS    00474000
*                                      MEANS IEDQGB WILL CALCULATE 0449 00475000
*                                      THE NUMBER OF UNITS TO BE   0449 00476000
*                                      FREED)                        */ 00477000
         MVI   PRFNBUNT(RPARM),X'00'                               0449 00478000
*   RPARM->PRFQCBA=ADDR(AVTBFRTB);  /* ADDR OF BUFFER RETURN QCB     */ 00479000
         LA    @09,AVTBFRTB(,IEDQAPTR)                             0450 00480000
         STCM  @09,7,PRFQCBA(RPARM)                                0450 00481000
*   RPARM->PRFPRI=PRIBFRTB;         /* POST PRIORITY                 */ 00482000
         MVI   PRFPRI(RPARM),X'E0'                                 0451 00483000
*   CALL DSPPOSTR;                  /* FREE UNITS                    */ 00484000
         LA    @15,DSPPOSTR(,RDISP)                                0452 00485000
         BALR  @14,@15                                             0452 00486000
*/*                                                                  */ 00487000
*                                                                  0453 00488000
*   R14=R14SV07;                    /* RESTORE RETURN REG            */ 00489000
         L     R14,R14SV07                                         0453 00490000
*   RETURN;                         /* RETURN TO CALLER              */ 00491000
@EL00004 DS    0H                                                  0454 00492000
@EF00004 DS    0H                                                  0454 00493000
@ER00004 BR    @14                                                 0454 00494000
*   END DISCARD;                                                   0455 00495000
*                                                                  0456 00496000
*/********************************************************************/ 00497000
*/*                                                                  */ 00498000
*/* THIS ROUTINE DEBLOCKS A PIU FROM THE BTU.                        */ 00499000
*/*    INPUT=NEXTPIUP(A PTR TO THE PIU TO BE DEBLOCKED)              */ 00500000
*/*    OUTPUT=                                                       */ 00501000
*/*            CURPIUP-PTR TO THE PIU WHICH JUST GOT DEBLOCKED       */ 00502000
*/*            PRFTIC FIELD (OF PIU JUST DEBLOCKED)-CONTAINS         */ 00503000
*/*                   INVALID TIC TO INDICATE LAST UNIT IN PIU       */ 00504000
*/*            NEXTPIUP CONTAINS EITHER                              */ 00505000
*/*             1) A POINTER TO THE NEXT PIU IN THE BTU (THE ONE     */ 00506000
*/*                AFTER THE ONE JUST DEBLOCKED) OR                  */ 00507000
*/*             2) 0 IF THERE ARE NO MORE PIUS IN THE BTU            */ 00508000
*/*                                                                  */ 00509000
*/********************************************************************/ 00510000
*                                                                  0456 00511000
*   RFY                                                            0456 00512000
*    (REG0,                                                        0456 00513000
*     R2) RSTD;                                                    0456 00514000
*GETPIU:                                                           0457 00515000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0457 00516000
GETPIU   DS    0H                                                  0458 00517000
*   CURPIUP=NEXTPIUP;               /* SET CURRENT PIU PTR TO NEXT 0458 00518000
*                                      PIU PTR                       */ 00519000
         L     @09,NEXTPIUP                                        0458 00520000
         ST    @09,CURPIUP                                         0458 00521000
*   PIUSTART=ADDR(PRFPIUO)+PRFPIUO; /* ADDRESS OF PIU(TH-RH-RU)      */ 00522000
         L     @09,IEDQPPTR                                        0459 00523000
         LA    PIUSTART,PRFPIUO(,@09)                              0459 00524000
         SLR   @06,@06                                             0459 00525000
         IC    @06,PRFPIUO(,@09)                                   0459 00526000
         ALR   PIUSTART,@06                                        0459 00527000
*/*                                                                  */ 00528000
*                                                                  0460 00529000
*   IEDPF2=IEDPF2&&IEDPF2;          /* ZERO OUT THE 8 BYTE PREFIX    */ 00530000
         AL    @09,@CF01670                                        0460 00531000
         XC    IEDPF2(8,@09),IEDPF2(@09)                           0460 00532000
*/*                                                                  */ 00533000
*/* CALCULATE THE NUMBER OF UNITS IN THE PIU TO BE DEBLOCKED         */ 00534000
*                                                                  0461 00535000
*   R2=AVTKEYLE;                    /* TCAM BUFFER UNIT LENGTH       */ 00536000
         LH    R2,AVTKEYLE(,IEDQAPTR)                              0461 00537000
*   REG0=0;                         /* ZERO LEFT PART OF DIVIDEND    */ 00538000
         SLR   REG0,REG0                                           0462 00539000
*   RPARM=TTHDCF+LENGTH(IEDTH)+PRFPIUO;/* SET UP DIVIDEND            */ 00540000
         LA    RPARM,10                                            0463 00541000
         SLR   @09,@09                                             0463 00542000
         ICM   @09,3,TTHDCF(PIUSTART)                              0463 00543000
         ALR   RPARM,@09                                           0463 00544000
         ALR   RPARM,@06                                           0463 00545000
*   GEN(DR  REG0,R2);               /* CALC MINIMUM # UNITS IN THIS     00546000
*                                      PIU                           */ 00547000
         DR  REG0,R2                                                    00548000
*/*AFTER DIVIDE REG0 CONTAINS REMAINDER AND RPARM CONTAINS QUOTIENT  */ 00549000
*                                                                  0465 00550000
*   IF REG0^=0 THEN                 /* NON 0 REMAINDER               */ 00551000
         LTR   REG0,REG0                                           0465 00552000
         BZ    @RF00465                                            0465 00553000
*     RPARM=RPARM+1;                /* ADD THE UNIT (CONTAINING THE     00554000
*                                      REMAINDER) TO THE COUNT       */ 00555000
         AL    RPARM,@CF00059                                      0466 00556000
*   UNITPTR=CURPIUP;                /* SET UNIT PTR TO 1ST UNIT OF 0467 00557000
*                                      PIU BUFFER                    */ 00558000
@RF00465 L     @09,CURPIUP                                         0467 00559000
         ST    @09,UNITPTR                                         0467 00560000
*   COUNTER=1;                      /* SET COUNTER TO 1              */ 00561000
         MVC   COUNTER(2),@CH00059                                 0468 00562000
*   DO WHILE(COUNTER<RPARM);        /* FIND LAST UNIT IN PIU BUFFER  */ 00563000
         B     @DE00469                                            0469 00564000
@DL00469 DS    0H                                                  0470 00565000
*     UNITPTR=UNITPTR->PRFTIC;      /* GET NEXT UNIT                 */ 00566000
         L     @09,UNITPTR                                         0470 00567000
         L     @09,PRFTIC(,@09)                                    0470 00568000
         ST    @09,UNITPTR                                         0470 00569000
*     COUNTER=COUNTER+1;            /* INCREMENT COUNTER             */ 00570000
         LH    @09,COUNTER                                         0471 00571000
         LA    @09,1(,@09)                                         0471 00572000
         STH   @09,COUNTER                                         0471 00573000
*   END;                                                           0472 00574000
*                                                                  0472 00575000
@DE00469 LH    @09,COUNTER                                         0472 00576000
         N     @09,@CF01667                                        0472 00577000
         CR    RPARM,@09                                           0472 00578000
         BH    @DL00469                                            0472 00579000
*/*                                                                  */ 00580000
*                                                                  0473 00581000
*   IF UNITPTR->PRFTIC=INVALTIC THEN/* LAST PIU IN BTU?              */ 00582000
         L     @09,UNITPTR                                         0473 00583000
         CLC   PRFTIC(4,@09),@CB01097                              0473 00584000
         BNE   @RF00473                                            0473 00585000
*     NEXTPIUP=0;                   /* YES - SET NEXT PIU POINTER=0  */ 00586000
         SLR   @09,@09                                             0474 00587000
         ST    @09,NEXTPIUP                                        0474 00588000
*   ELSE                            /* NOT LAST PIU IN BTU           */ 00589000
*     DO;                                                          0475 00590000
         B     @RC00473                                            0475 00591000
@RF00473 DS    0H                                                  0476 00592000
*       NEXTPIUP=UNITPTR->PRFTIC;   /* SET NEXT PIU PTR TO NEXT PIU     00593000
*                                      IN BTU                        */ 00594000
         L     @09,UNITPTR                                         0476 00595000
         L     @06,PRFTIC(,@09)                                    0476 00596000
         ST    @06,NEXTPIUP                                        0476 00597000
*       UNITPTR->PRFTIC=INVALTIC;   /* SET LAST UNIT OF PIU JUST   0477 00598000
*                                      DEBLOCKED TO INVALID TIC      */ 00599000
         MVC   PRFTIC(4,@09),@CB01097                              0477 00600000
*     END;                                                         0478 00601000
*                                                                  0478 00602000
*/*                                                                  */ 00603000
*                                                                  0479 00604000
*   RETURN;                         /* RETURN TO CALLER              */ 00605000
@EL00005 DS    0H                                                  0479 00606000
@EF00005 DS    0H                                                  0479 00607000
@ER00005 BR    @14                                                 0479 00608000
*   END GETPIU;                                                    0480 00609000
         B     @EL00005                                            0480 00610000
*   RFY                                                            0481 00611000
*    (REG0,                                                        0481 00612000
*     R2) UNRSTD;                                                  0481 00613000
*                                                                  0482 00614000
*/********************************************************************/ 00615000
*/*                                                                  */ 00616000
*/* THIS ROUTINE LOCATES THE CORRECT SIB FOR THIS OAF/DAF PAIR.      */ 00617000
*/*                                                                  */ 00618000
*/********************************************************************/ 00619000
*                                                                  0482 00620000
*GETSIB:                                                           0482 00621000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0482 00622000
GETSIB   DS    0H                                                  0483 00623000
*   R14SV01=R14;                    /* SAVE RETURN ADDRESS           */ 00624000
         ST    R14,R14SV01                                         0483 00625000
*/*                                                                  */ 00626000
*                                                                  0484 00627000
*   SIBPTR=TRMSIBPT;                /* GET 1ST SIB ADDRESS (NOTE-TTE    00628000
*                                      ADDRESS IS ALREADY SET UP)    */ 00629000
         LR    @09,IEDQTPTR                                        0484 00630000
         AL    @09,@CF01671                                        0484 00631000
         SLR   SIBPTR,SIBPTR                                       0484 00632000
         ICM   SIBPTR,7,TRMSIBPT-7(@09)                            0484 00633000
*   FOUNDSW=OFF;                    /* INIT FOUND SWITCH TO OFF      */ 00634000
         NI    FOUNDSW,B'01111111'                                 0485 00635000
*   DO WHILE(SIBPTR>0&FOUNDSW=OFF); /* SIB SEARCH LOOP               */ 00636000
         B     @DE00486                                            0486 00637000
@DL00486 DS    0H                                                  0487 00638000
*     IF SIBINDEX=TTHDAF THEN       /* IS THIS THE SIB FOR THIS    0487 00639000
*                                      OAF/DAF PAIR?                 */ 00640000
         CLC   SIBINDEX(2,SIBPTR),TTHDAF(PIUSTART)                 0487 00641000
         BNE   @RF00487                                            0487 00642000
*       FOUNDSW=ON;                 /* YES- SET 'FOUND' INDICATOR    */ 00643000
         OI    FOUNDSW,B'10000000'                                 0488 00644000
*     ELSE                          /* NO                            */ 00645000
*       SIBPTR=SIBCHAIN;            /* GET NEXT SIB IN CHAIN         */ 00646000
         B     @RC00487                                            0489 00647000
@RF00487 L     SIBPTR,SIBCHAIN(,SIBPTR)                            0489 00648000
*   END;                            /* END OF LOOP                   */ 00649000
@RC00487 DS    0H                                                  0490 00650000
@DE00486 LTR   SIBPTR,SIBPTR                                       0490 00651000
         BNP   @DC00486                                            0490 00652000
         TM    FOUNDSW,B'10000000'                                 0490 00653000
         BZ    @DL00486                                            0490 00654000
@DC00486 DS    0H                                                  0491 00655000
*/*                                                                  */ 00656000
*                                                                  0491 00657000
*   R14=R14SV01;                    /* RESTORE RETURN ADDRESS        */ 00658000
         L     R14,R14SV01                                         0491 00659000
*   RETURN;                         /* RETURN TO CALLER              */ 00660000
@EL00006 DS    0H                                                  0492 00661000
@EF00006 DS    0H                                                  0492 00662000
@ER00006 BR    @14                                                 0492 00663000
*   END GETSIB;                                                    0493 00664000
*                                                                  0494 00665000
*/********************************************************************/ 00666000
*/*                                                                  */ 00667000
*/* THIS ROUTINE VALIDITY CHECKS THE OAF AND DAF IN THE TH.          */ 00668000
*/* IT ALSO CONVERTS THE OAF AND DAF TO TNT OFFST VALUES.            */ 00669000
*/* IF OAF AND DAF ARE VALID THEIR TNT OFFSETS ARE STORED IN THE     */ 00670000
*/* TH.                                                              */ 00671000
*/*                                                                  */ 00672000
*/* THIS ROUTINE ALSO TRACES THE PIU IF REQUIRED.                    */ 00673000
*/*                                                                  */ 00674000
*/********************************************************************/ 00675000
*                                                                  0494 00676000
*OAFDAFCK:                                                         0494 00677000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0494 00678000
OAFDAFCK DS    0H                                                  0495 00679000
*   R14SV03=R14;                    /* SAVE RETURN REG               */ 00680000
         ST    R14,R14SV03                                         0495 00681000
*/*                                                                  */ 00682000
*                                                                  0496 00683000
*   RC=BAD;                         /* INIT RETURN CODE TO BAD       */ 00684000
         LA    RC,4                                                0496 00685000
*/* INITIALIZE THE TRACE PARAMETER LIST                              */ 00686000
*                                                                  0497 00687000
*   PLPIUPTR=CURPIUP;               /* ADDR OF PIU TO BE TRACED      */ 00688000
         L     @09,CURPIUP                                         0497 00689000
         ST    @09,PLPIUPTR                                        0497 00690000
*   PLTNTOAF=0;                     /* INIT OAF TNT OFFSET FIELD     */ 00691000
         SLR   @09,@09                                             0498 00692000
         STH   @09,PLTNTOAF                                        0498 00693000
*   PLTNTDAF=0;                     /* INIT DAF TNT OFFSET FIELD     */ 00694000
         STH   @09,PLTNTDAF                                        0499 00695000
*/*                                                                  */ 00696000
*                                                                  0500 00697000
*   RPARM=TTHDAF;                   /* GET DAF NETWORK ADDRESS       */ 00698000
         SLR   RPARM,RPARM                                         0500 00699000
         ICM   RPARM,3,TTHDAF(PIUSTART)                            0500 00700000
*   CALL IEDIAP03;                  /* CONVERT DAF TO TNT OFFSET R15    00701000
*                                      CONTAINS THE RETURN CODE      */ 00702000
         L     @09,AVTSAVTP(,IEDQAPTR)                             0501 00703000
         L     @15,SAVTNAX(,@09)                                   0501 00704000
         BALR  @14,@15                                             0501 00705000
*   IF R15RC^=0 THEN                /* VALID DAF SUBAREA             */ 00706000
         SLR   @09,@09                                             0502 00707000
         CR    R15RC,@09                                           0502 00708000
         BE    @RF00502                                            0502 00709000
*     DO;                           /* YES                           */ 00710000
*       TEMPNA=TTHDAF;              /* MOVE DAF NETWORK ADDRESS TO 0504 00711000
*                                      TEMPORARY AREA                */ 00712000
         MVC   TEMPNA(2),TTHDAF(PIUSTART)                          0504 00713000
*       TEMPNA1=TEMPNA1&SAVTSUBM;   /* 'AND' OFF ELEMENT PORTION OF     00714000
*                                      NETWORK ADDRESS               */ 00715000
         L     @02,AVTSAVTP(,IEDQAPTR)                             0505 00716000
         SLR   @00,@00                                             0505 00717000
         IC    @00,TEMPNA1                                         0505 00718000
         SLR   @05,@05                                             0505 00719000
         IC    @05,SAVTSUBM(,@02)                                  0505 00720000
         NR    @00,@05                                             0505 00721000
         STC   @00,TEMPNA1                                         0505 00722000
*       IF TEMPNA1=SAVTSUBV THEN    /* DAF SUBAREA=TCAM SUBAREA      */ 00723000
         CLC   TEMPNA1(1),SAVTSUBV(@02)                            0506 00724000
         BNE   @RF00506                                            0506 00725000
*         DO;                       /* YES                           */ 00726000
*           IF R15RC>0 THEN         /* VALID DAF ELEMENT ADDRESS     */ 00727000
         CR    R15RC,@09                                           0508 00728000
         BNH   @RF00508                                            0508 00729000
*             DO;                   /* YES                           */ 00730000
*               PLTNTDAF=R15RC;     /* SAVE DAF TNT OFFSET VALUE     */ 00731000
         STH   R15RC,PLTNTDAF                                      0510 00732000
*/*                                                                  */ 00733000
*                                                                  0511 00734000
*               RPARM=TTHOAF;       /* GET OAF NETWORK ADDRESS       */ 00735000
         SLR   @09,@09                                             0511 00736000
         ICM   @09,3,TTHOAF(PIUSTART)                              0511 00737000
         LR    RPARM,@09                                           0511 00738000
*               PLTNTNET=TTHOAF;    /* SAVE NETWORK ADDRESS  @YM09076*/ 00739000
         STH   @09,PLTNTNET                                        0512 00740000
*               CALL IEDIAP03;      /* CONVERT OAF NETWORK ADDRESS TO   00741000
*                                      TNT OFFSET                    */ 00742000
         L     @15,SAVTNAX(,@02)                                   0513 00743000
         BALR  @14,@15                                             0513 00744000
*               IF R15RC>0 THEN     /* VALID OAF NETWORK ADDRESS     */ 00745000
         LTR   R15RC,R15RC                                         0514 00746000
         BNP   @RF00514                                            0514 00747000
*                 DO;               /* YES                           */ 00748000
*                   PLTNTOAF=R15RC; /* PUT OAF TNT IN TRACE PARM LIST*/ 00749000
         STH   R15RC,PLTNTOAF                                      0516 00750000
*                   RPARM=R15RC;    /* OAF TNT OFFSET                */ 00751000
         LR    RPARM,R15RC                                         0517 00752000
*                   CALL IEDQTNT;   /* GET OAF TTE ADDRESS           */ 00753000
         L     @15,AVTRNMPT(,IEDQAPTR)                             0518 00754000
         BALR  @14,@15                                             0518 00755000
*                   RTRM=RPARM;     /* TTE ADDRESSABILITY            */ 00756000
         LR    RTRM,RPARM                                          0519 00757000
*/*                                                                  */ 00758000
*                                                                  0520 00759000
*                   IF TRMTRACE=ON THEN/* IS TRACE ACTIVE FOR THIS 0520 00760000
*                                      TERMINAL                      */ 00761000
         LR    @09,IEDQTPTR                                        0520 00762000
         AL    @09,@CF01673                                        0520 00763000
         TM    TRMTRACE-3(@09),B'00001000'                         0520 00764000
         BNO   @RF00520                                            0520 00765000
*                     CALL TRACE;   /* TRACE THE PIU                 */ 00766000
         BAL   @14,TRACE                                           0521 00767000
*/*                                                                  */ 00768000
*                                                                  0522 00769000
*                   TTHOAF=PLTNTOAF;/* MOVE OAF TNT OFFSET INTO TH   */ 00770000
@RF00520 MVC   TTHOAF(2,PIUSTART),PLTNTOAF                         0522 00771000
*                   TTHDAF=PLTNTDAF;/* MOVE DAF TNT OFFSET INTO TH   */ 00772000
         MVC   TTHDAF(2,PIUSTART),PLTNTDAF                         0523 00773000
*                   PRF2TNTI=ON;    /* SET 'TNT OFFSET' INDICATOR    */ 00774000
         L     @09,IEDQPPTR                                        0524 00775000
         AL    @09,@CF01670                                        0524 00776000
         OI    PRF2TNTI(@09),B'10000000'                           0524 00777000
*                   RC=GOOD;        /* SET GOOD RETURN CODE          */ 00778000
         SLR   RC,RC                                               0525 00779000
*                 END;                                             0526 00780000
*               ELSE                /* INVALID OAF NETWORK ADDRESS   */ 00781000
*                 DO;                                              0527 00782000
         B     @RC00514                                            0527 00783000
@RF00514 DS    0H                                                  0528 00784000
*                   CALL TRACE;     /* TRACE THE PIU                 */ 00785000
         BAL   @14,TRACE                                           0528 00786000
*                   RPARM=CURPIUP;  /* SET PTR TO CURRENT PIU        */ 00787000
         L     RPARM,CURPIUP                                       0529 00788000
*                   CALL DISCARD;   /* DISCARD THE PIU               */ 00789000
         BAL   @14,DISCARD                                         0530 00790000
*                 END;                                             0531 00791000
*             END;                                                 0532 00792000
*           ELSE                    /* INVALID DAF ELEMENT ADDRESS   */ 00793000
*             DO;                   /* YES                           */ 00794000
         B     @RC00508                                            0533 00795000
@RF00508 DS    0H                                                  0534 00796000
*               CALL TRACE;         /* TRACE PIU                     */ 00797000
         BAL   @14,TRACE                                           0534 00798000
*               SNC=SNSNGDAF;       /* SET NO GOOD DAF SENSE         */ 00799000
         MVC   SNC(4),@CB01562                                     0535 00800000
*               CALL SNDERRSP;      /* SEND -RSP                     */ 00801000
         BAL   @14,SNDERRSP                                        0536 00802000
*             END;                                                 0537 00803000
*         END;                                                     0538 00804000
*       ELSE                        /* DAF SUBAREA NOT TCAM SUBAREA  */ 00805000
*         DO;                                                      0539 00806000
         B     @RC00506                                            0539 00807000
@RF00506 DS    0H                                                  0540 00808000
*           CALL TRACE;             /* TRACE PIU                     */ 00809000
         BAL   @14,TRACE                                           0540 00810000
*           CALL POSTPCO;           /* POST PIU TO PATH CONTROL    0541 00811000
*                                      OUTBOUND                      */ 00812000
         BAL   @14,POSTPCO                                         0541 00813000
*         END;                                                     0542 00814000
*     END;                                                         0543 00815000
*   ELSE                            /* INVALID DAF SUBAREA           */ 00816000
*     DO;                           /* YES                           */ 00817000
         B     @RC00502                                            0544 00818000
@RF00502 DS    0H                                                  0545 00819000
*       CALL TRACE;                 /* TRACE PIU                     */ 00820000
         BAL   @14,TRACE                                           0545 00821000
*       SNC=SNSNGDAF;               /* UNRECOGNIZED DAF SENSE        */ 00822000
         MVC   SNC(4),@CB01562                                     0546 00823000
*       CALL SNDERRSP;              /* SEND -RSP                     */ 00824000
         BAL   @14,SNDERRSP                                        0547 00825000
*     END;                                                         0548 00826000
*                                                                  0548 00827000
*/*                                                                  */ 00828000
*                                                                  0549 00829000
*   R14=R14SV03;                    /* RESTORE RETURN REG            */ 00830000
@RC00502 L     R14,R14SV03                                         0549 00831000
*   RETURN;                         /* RETURN TO CALLER              */ 00832000
@EL00007 DS    0H                                                  0550 00833000
@EF00007 DS    0H                                                  0550 00834000
@ER00007 BR    @14                                                 0550 00835000
*   END OAFDAFCK;                                                  0551 00836000
*                                                                  0552 00837000
*/********************************************************************/ 00838000
*/*                                                                  */ 00839000
*/* THIS ROUTINE POST THE CURRENT PIU TO COMMON SESSION CONTROL      */ 00840000
*/* INBOUND(IEDCSCIN).                                               */ 00841000
*/*                                                                  */ 00842000
*/********************************************************************/ 00843000
*                                                                  0552 00844000
*POSTCSCI:                                                         0552 00845000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0552 00846000
POSTCSCI DS    0H                                                  0553 00847000
*   R14SV09=R14;                    /* SAVE RETURN ADDRESS           */ 00848000
         ST    R14,R14SV09                                         0553 00849000
*/*                                                                  */ 00850000
*                                                                  0554 00851000
*   PRFQCBA=SAVTCSCI;               /* ADDRESS OF COMMON SESSION   0554 00852000
*                                      CONTROL INBOUND QCB           */ 00853000
         L     @10,IEDQPPTR                                        0554 00854000
         L     @09,AVTSAVTP(,IEDQAPTR)                             0554 00855000
         MVC   PRFQCBA(3,@10),SAVTCSCI+1(@09)                      0554 00856000
*   PRFPRI=PRIFIDIN;                /* POST PRIORITY                 */ 00857000
         MVI   PRFPRI(@10),X'E4'                                   0555 00858000
*   RPARM=CURPIUP;                  /* CURRENT PIU ADDRESS           */ 00859000
         L     RPARM,CURPIUP                                       0556 00860000
*   CALL DSPPOSTR;                  /* POST THE PIU TH IEDCSCIN      */ 00861000
         LA    @15,DSPPOSTR(,RDISP)                                0557 00862000
         BALR  @14,@15                                             0557 00863000
*/*                                                                  */ 00864000
*                                                                  0558 00865000
*   R14=R14SV09;                    /* RESTORE RETURN REG            */ 00866000
         L     R14,R14SV09                                         0558 00867000
*   RETURN;                         /* RETURN TO CALLER              */ 00868000
@EL00008 DS    0H                                                  0559 00869000
@EF00008 DS    0H                                                  0559 00870000
@ER00008 BR    @14                                                 0559 00871000
*   END POSTCSCI;                                                  0560 00872000
*                                                                  0561 00873000
*/********************************************************************/ 00874000
*/*                                                                  */ 00875000
*/* THIS ROUTINE POST THE CURRENT PIU TO PATH CONTROL OUTBOUND       */ 00876000
*/* (IEDPCOUT).                                                      */ 00877000
*/*                                                                  */ 00878000
*/********************************************************************/ 00879000
*                                                                  0561 00880000
*POSTPCO:                                                          0561 00881000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0561 00882000
POSTPCO  DS    0H                                                  0562 00883000
*   R14SV08=R14;                    /* SAVE RETURN ADDRESS           */ 00884000
         ST    R14,R14SV08                                         0562 00885000
*/*                                                                  */ 00886000
*                                                                  0563 00887000
*   PRFQCBA=ADDR(IEDPCOUT);         /* ADDRESS OF IEDPCOUT'S QCB     */ 00888000
         L     @09,IEDQPPTR                                        0563 00889000
         L     @02,@CV01568                                        0563 00890000
         STCM  @02,7,PRFQCBA(@09)                                  0563 00891000
*   PRFPRI=PRIPCOUT;                /* POST PRIORITY                 */ 00892000
         MVI   PRFPRI(@09),X'E4'                                   0564 00893000
*   RPARM=CURPIUP;                  /* CURRENT PIU ADDRESS           */ 00894000
         L     RPARM,CURPIUP                                       0565 00895000
*   CALL DSPPOSTR;                  /* POST PIU TO IEDPCOUT QCB      */ 00896000
         LA    @15,DSPPOSTR(,RDISP)                                0566 00897000
         BALR  @14,@15                                             0566 00898000
*/*                                                                  */ 00899000
*                                                                  0567 00900000
*   R14=R14SV08;                    /* RESTORE RETURN REG            */ 00901000
         L     R14,R14SV08                                         0567 00902000
*   RETURN;                         /* RETURN TO CALLER              */ 00903000
@EL00009 DS    0H                                                  0568 00904000
@EF00009 DS    0H                                                  0568 00905000
@ER00009 BR    @14                                                 0568 00906000
*   END POSTPCO;                                                   0569 00907000
*                                                                  0570 00908000
*/********************************************************************/ 00909000
*/*                                                                  */ 00910000
*/* THIS ROUTINE BUILDS AN EXCEPTION RESPONSE(-RSP)                  */ 00911000
*/*   INPUT=                                                         */ 00912000
*/*         CURPIUP-CONTAINS A PTR TO THE PIU IN WHICH THE           */ 00913000
*/*                 EXCEPTION RESPONSE IS TO BE BUILT                */ 00914000
*/*         SNC- CONTAINS THE 4 SNA SENSE BYTES TO BE USED IN        */ 00915000
*/*              THE RESPONSE                                        */ 00916000
*/*         PLTNTDAF-CONTAINS THE DAF TNT OFFSET                     */ 00917000
*/*                                                                  */ 00918000
*/********************************************************************/ 00919000
*                                                                  0570 00920000
*RSPBUILD:                                                         0570 00921000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0570 00922000
RSPBUILD DS    0H                                                  0571 00923000
*   R14SV05=R14;                    /* SAVE RETURN REG               */ 00924000
         ST    R14,R14SV05                                         0571 00925000
*/*                                                                  */ 00926000
*                                                                  0572 00927000
*   IF PRFTIC^=INVALTIC THEN        /* LAST UNIT IN PIU              */ 00928000
         L     @09,IEDQPPTR                                        0572 00929000
         CLC   PRFTIC(4,@09),@CB01097                              0572 00930000
         BE    @RF00572                                            0572 00931000
*     DO;                           /* NO                            */ 00932000
*       RPARM=PRFTIC;               /* GET 2ND UNIT OF PIU           */ 00933000
         L     RPARM,PRFTIC(,@09)                                  0574 00934000
*       CALL DISCARD;               /* DISCARD ALL BUT 1ST UNIT OF 0575 00935000
*                                      PIU                           */ 00936000
         BAL   @14,DISCARD                                         0575 00937000
*       PRFTIC=INVALTIC;            /* MARK 1ST UNIT OF PIU 'LAST'   */ 00938000
         L     @09,IEDQPPTR                                        0576 00939000
         MVC   PRFTIC(4,@09),@CB01097                              0576 00940000
*     END;                                                         0577 00941000
*                                                                  0577 00942000
*/*                                                                  */ 00943000
*                                                                  0578 00944000
*   IF TRHRUCAT=TRHFMD THEN         /* FM DATA                       */ 00945000
@RF00572 LA    @09,10                                              0578 00946000
         LR    @02,PIUSTART                                        0578 00947000
         ALR   @02,@09                                             0578 00948000
         TM    TRHRUCAT(@02),B'01100000'                           0578 00949000
         BNZ   @RF00578                                            0578 00950000
*     DO;                           /* YES                           */ 00951000
*       IF PLTNTDAF=SAVTSCPT THEN   /* IS DAF SSCP                   */ 00952000
         L     @05,AVTSAVTP(,IEDQAPTR)                             0580 00953000
         CLC   PLTNTDAF(2),SAVTSCPT(@05)                           0580 00954000
         BNE   @RF00580                                            0580 00955000
*         DO;                       /* YES                           */ 00956000
*           TTHDCF=10;              /* SET DATA COUNT                */ 00957000
         STCM  @09,3,TTHDCF(PIUSTART)                              0582 00958000
*           IF TRHSDI=OFF THEN      /* SENSE DATA INCLUDED           */ 00959000
         TM    TRHSDI(@02),B'00000100'                             0583 00960000
         BNZ   @RF00583                                            0583 00961000
*             RU(5:7)=RU(1:3);      /* MOVE COMMAND BYTES OVER TO  0584 00962000
*                                      LEAVE ROOM FOE SENSE          */ 00963000
         LA    @09,13                                              0584 00964000
         ALR   @09,PIUSTART                                        0584 00965000
         MVC   RU+4(3,@09),RU(@09)                                 0584 00966000
*         END;                                                     0585 00967000
*       ELSE                        /* NOT SSCP                      */ 00968000
*         TTHDCF=7;                 /* SET DATA COUNT = 7            */ 00969000
         B     @RC00580                                            0586 00970000
@RF00580 MVC   TTHDCF(2,PIUSTART),@CH00568                         0586 00971000
*     END;                                                         0587 00972000
*   ELSE                            /* NOT FMD                       */ 00973000
*     DO;                           /* YES                           */ 00974000
         B     @RC00578                                            0588 00975000
@RF00578 DS    0H                                                  0589 00976000
*       IF TRHSDI=OFF THEN          /* NO SENSE INCLUDED             */ 00977000
         TM    TRHSDI+10(PIUSTART),B'00000100'                     0589 00978000
         BNZ   @RF00589                                            0589 00979000
*         RU(5)=RU(1);              /* YES-MOVE COMMAND CODE TO 5TH     00980000
*                                      BYTE (LEAVES 4 BYTES FOR    0590 00981000
*                                      SENSE)                        */ 00982000
         LA    @09,13                                              0590 00983000
         ALR   @09,PIUSTART                                        0590 00984000
         MVC   RU+4(1,@09),RU(@09)                                 0590 00985000
*       TTHDCF=8;                   /* SET DATA COUNT TO 8           */ 00986000
@RF00589 MVC   TTHDCF(2,PIUSTART),@CH00044                         0591 00987000
*     END;                                                         0592 00988000
*                                                                  0592 00989000
*/*                                                                  */ 00990000
*/* SWITCH OAF AND DAF                                               */ 00991000
*                                                                  0593 00992000
*   PLTNTOAF=TTHOAF;                /* SAVE OAF FOR A SECOND         */ 00993000
@RC00578 MVC   PLTNTOAF(2),TTHOAF(PIUSTART)                        0593 00994000
*   TTHOAF=TTHDAF;                  /* MOVE DAF INTO OAF             */ 00995000
         MVC   @TS00001(2),TTHDAF(PIUSTART)                        0594 00996000
         MVC   TTHOAF(2,PIUSTART),@TS00001                         0594 00997000
*   TTHDAF=PLTNTOAF;                /* MOVE OAF INTO DAF             */ 00998000
         MVC   TTHDAF(2,PIUSTART),PLTNTOAF                         0595 00999000
*/*                                                                  */ 01000000
*                                                                  0596 01001000
*   TTHBBIU=ON;                     /* MARK BEGINNING OF BIU         */ 01002000
*   TTHEBIU=ON;                     /* MARK END OF BIU               */ 01003000
         OI    TTHBBIU(PIUSTART),B'00001100'                       0597 01004000
*/*                                                                  */ 01005000
*                                                                  0598 01006000
*   TRHTYPE=ON;                     /* INDICATE RESPONSE             */ 01007000
*   TRHSDI=ON;                      /* SET SENSE DATA INCLUDED       */ 01008000
         LA    @09,10                                              0599 01009000
         ALR   @09,PIUSTART                                        0599 01010000
         OI    TRHTYPE(@09),B'10000100'                            0599 01011000
*   TRHER=ON;                       /* SET EXCEPTION RESPONSE FLAG   */ 01012000
         OI    TRHER(@09),B'00010000'                              0600 01013000
*   TRHBC=ON;                       /* SET BEGIN CHAIN               */ 01014000
*   TRHEC=ON;                       /* SET END CHAIN                 */ 01015000
         OI    TRHBC(@09),B'00000011'                              0602 01016000
*   TRHPI=OFF;                      /* INDICATE NOT PACING RESPONSE  */ 01017000
         NI    TRHPI(@09),B'11111110'                              0603 01018000
*/*                                                                  */ 01019000
*                                                                  0604 01020000
*   RU(1:4)=SNC;                    /* MOVE SENSE BYTES INTO RU      */ 01021000
         MVC   RU+13(4,PIUSTART),SNC                               0604 01022000
*/*                                                                  */ 01023000
*                                                                  0605 01024000
*   R14=R14SV05;                    /* RESTORE RETURN REG            */ 01025000
         L     R14,R14SV05                                         0605 01026000
*   RETURN;                         /* RETURN TO CALLER              */ 01027000
@EL00010 DS    0H                                                  0606 01028000
@EF00010 DS    0H                                                  0606 01029000
@ER00010 BR    @14                                                 0606 01030000
*   END RSPBUILD;                                                  0607 01031000
*                                                                  0608 01032000
*/********************************************************************/ 01033000
*/*                                                                  */ 01034000
*/* THIS ROUTINE ASSEMBLES BIU SEGMENTS(APPLIES TO FID1 ONLY)        */ 01035000
*/*                                                                  */ 01036000
*/********************************************************************/ 01037000
*                                                                  0608 01038000
*SEGASSEM:                                                         0608 01039000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0608 01040000
SEGASSEM DS    0H                                                  0609 01041000
*   R14SV02=R14;                    /* SAVE RETURN ADDRESS           */ 01042000
         ST    R14,R14SV02                                         0609 01043000
*/*                                                                  */ 01044000
*                                                                  0610 01045000
*   CALL GETSIB;                    /* LOCATE SIB FOR THIS OAF/DAF 0610 01046000
*                                      PAIR                          */ 01047000
         BAL   @14,GETSIB                                          0610 01048000
*   IF FOUNDSW=ON THEN              /* WAS SIB LOCATED?              */ 01049000
         TM    FOUNDSW,B'10000000'                                 0611 01050000
         BNO   @RF00611                                            0611 01051000
*     DO;                           /* YES                           */ 01052000
*       RC=GO;                      /* INIT RETURN CODE TO 'GO'      */ 01053000
         SLR   RC,RC                                               0613 01054000
*       IF TTHBBIU=ON THEN          /* BEGIN BIU                     */ 01055000
         TM    TTHBBIU(PIUSTART),B'00001000'                       0614 01056000
         BNO   @RF00614                                            0614 01057000
*         DO;                       /* YES                           */ 01058000
*           IF TTHEBIU=ON THEN      /* END BIU                       */ 01059000
         TM    TTHEBIU(PIUSTART),B'00000100'                       0616 01060000
         BNO   @RF00616                                            0616 01061000
*             DO;                   /* YES                           */ 01062000
*               IF SIBINBIU=ON THEN /* 'IN BIU' STATE                */ 01063000
         TM    SIBINBIU(SIBPTR),B'00100000'                        0618 01064000
         BNO   @RF00618                                            0618 01065000
*                 DO;               /* YES                           */ 01066000
*                   SIBINBIU=OFF;   /* GO TO 'BETWEEN BIU' STATE     */ 01067000
         NI    SIBINBIU(SIBPTR),B'11011111'                        0620 01068000
*                   RPARM=SIBRUSEG; /* ADDR OF ACCUMULATING SEGMENT  */ 01069000
         L     RPARM,SIBRUSEG(,SIBPTR)                             0621 01070000
*                   CALL DISCARD;   /* DISCARD PARTIALLY ACCUMULATED    01071000
*                                      SEGMENT                       */ 01072000
         BAL   @14,DISCARD                                         0622 01073000
*                 END;                                             0623 01074000
*             END;                                                 0624 01075000
*           ELSE                    /* NOT END OF BIU                */ 01076000
*             DO;                                                  0625 01077000
         B     @RC00616                                            0625 01078000
@RF00616 DS    0H                                                  0626 01079000
*               IF SIBINBIU=ON THEN /* 'IN BIU' STATE                */ 01080000
         TM    SIBINBIU(SIBPTR),B'00100000'                        0626 01081000
         BNO   @RF00626                                            0626 01082000
*                 DO;               /* YES                           */ 01083000
*                   RPARM=SIBRUSEG; /* ADDR OF PARTIALLY ACCUMULATED    01084000
*                                      SEGMENT                       */ 01085000
         L     RPARM,SIBRUSEG(,SIBPTR)                             0628 01086000
*                   CALL DISCARD;   /* DISCARD PART. ACCUM. SEGMENT  */ 01087000
         BAL   @14,DISCARD                                         0629 01088000
*                 END;                                             0630 01089000
*               ELSE                /* 'BETWEEN BIU' STATE           */ 01090000
*                 SIBINBIU=ON;      /* GO TO 'IN BIU' STATE          */ 01091000
         B     @RC00626                                            0631 01092000
@RF00626 OI    SIBINBIU(SIBPTR),B'00100000'                        0631 01093000
*/*                                                                  */ 01094000
*                                                                  0632 01095000
*               SIBRUSEG=CURPIUP;   /* SET PARTIALLY ACCUMULATED   0632 01096000
*                                      SEGMENT EQUAL CURRENT PIU     */ 01097000
@RC00626 L     @10,CURPIUP                                         0632 01098000
         ST    @10,SIBRUSEG(,SIBPTR)                               0632 01099000
*               RC=NOGO;            /* SET 'NOGO' RETURN CODE        */ 01100000
         LA    RC,4                                                0633 01101000
*             END;                                                 0634 01102000
*         END;                                                     0635 01103000
*       ELSE                        /* NOT BEGIN BIU                 */ 01104000
*         DO;                                                      0636 01105000
         B     @RC00614                                            0636 01106000
@RF00614 DS    0H                                                  0637 01107000
*           IF SIBINBIU=ON THEN     /* 'IN BIU' STATE                */ 01108000
         TM    SIBINBIU(SIBPTR),B'00100000'                        0637 01109000
         BNO   @RF00637                                            0637 01110000
*             DO;                   /* YES                           */ 01111000
*               IF TTHEBIU=ON THEN  /* END BIU                       */ 01112000
         TM    TTHEBIU(PIUSTART),B'00000100'                       0639 01113000
         BNO   @RF00639                                            0639 01114000
*                 DO;               /* YES                           */ 01115000
*                   CALL COMBINE;   /* COMBINE CURRENT PIU WITH    0641 01116000
*                                      PARTIALLY ACCUMULATED SEGMENT */ 01117000
         BAL   @14,COMBINE                                         0641 01118000
*                   SIBINBIU=OFF;   /* GO TO 'BETWEEN BIU' STATE     */ 01119000
         NI    SIBINBIU(SIBPTR),B'11011111'                        0642 01120000
*                   CURPIUP=SIBRUSEG;/* SET CURRENT PIU PTR TO     0643 01121000
*                                      ACCUMULATED SEGMENT           */ 01122000
         L     @09,SIBRUSEG(,SIBPTR)                               0643 01123000
         ST    @09,CURPIUP                                         0643 01124000
*                   PIUSTART=ADDR(PRFPIUO)+PRFPIUO;/* CALCULATE START   01125000
*                                      OF PIU IN CURRENT PIU(NOW   0644 01126000
*                                      ACCUMULATED SEGMENT)          */ 01127000
         L     @09,IEDQPPTR                                        0644 01128000
         LA    PIUSTART,PRFPIUO(,@09)                              0644 01129000
         SLR   @02,@02                                             0644 01130000
         IC    @02,PRFPIUO(,@09)                                   0644 01131000
         ALR   PIUSTART,@02                                        0644 01132000
*                   TTHEBIU=ON;     /* MARK PIU AS LAST BIU          */ 01133000
         OI    TTHEBIU(PIUSTART),B'00000100'                       0645 01134000
*                 END;                                             0646 01135000
*               ELSE                /* NOT END BIU                   */ 01136000
*                 DO;                                              0647 01137000
         B     @RC00639                                            0647 01138000
@RF00639 DS    0H                                                  0648 01139000
*                   CALL COMBINE;   /* COMBINE CURRENT PIU AND     0648 01140000
*                                      PARTIALLY ACCUMULATED SEGMENT */ 01141000
         BAL   @14,COMBINE                                         0648 01142000
*                   RC=NOGO;        /* SET 'NOGO' RETURN CODE        */ 01143000
         LA    RC,4                                                0649 01144000
*                 END;                                             0650 01145000
*             END;                                                 0651 01146000
*           ELSE                    /* 'BETWEEN BIU' STATE           */ 01147000
*             DO;                                                  0652 01148000
         B     @RC00637                                            0652 01149000
@RF00637 DS    0H                                                  0653 01150000
*               RPARM=CURPIUP;      /* ADDR OF CURRENT PIU           */ 01151000
         L     RPARM,CURPIUP                                       0653 01152000
*               CALL DISCARD;       /* DISCARD CURRENT PIU           */ 01153000
         BAL   @14,DISCARD                                         0654 01154000
*               RC=NOGO;            /* SET 'NOGO' RETURN CODE        */ 01155000
         LA    RC,4                                                0655 01156000
*             END;                                                 0656 01157000
*         END;                                                     0657 01158000
*     END;                                                         0658 01159000
*   ELSE                            /* SIB NOT FOUND                 */ 01160000
*     DO;                                                          0659 01161000
         B     @RC00611                                            0659 01162000
@RF00611 DS    0H                                                  0660 01163000
*       SNC=SNSNOSES;               /* 'NO SESSION' SENSE BYTES      */ 01164000
         MVC   SNC(4),@CB01567                                     0660 01165000
*       CALL SNDERRSP;              /* SEND ERROR RESPONSE           */ 01166000
         BAL   @14,SNDERRSP                                        0661 01167000
*       RC=NOGO;                    /* SET 'NOGO' RETURN CODE        */ 01168000
         LA    RC,4                                                0662 01169000
*     END;                                                         0663 01170000
*                                                                  0663 01171000
*/*                                                                  */ 01172000
*                                                                  0664 01173000
*   R14=R14SV02;                    /* RESTORE RETURN ADDRESS        */ 01174000
@RC00611 L     R14,R14SV02                                         0664 01175000
*   RETURN;                         /* RETURN TO CALLER              */ 01176000
@EL00011 DS    0H                                                  0665 01177000
@EF00011 DS    0H                                                  0665 01178000
@ER00011 BR    @14                                                 0665 01179000
*   END SEGASSEM;                                                  0666 01180000
*                                                                  0667 01181000
*/********************************************************************/ 01182000
*/*                                                                  */ 01183000
*/* THIS ROUTINE SENDS AN EXCEPTION RESPONSE,IF POSSIBLE, TO THE     */ 01184000
*/* TH OAF ADDRESS. THE RESPONSE WILL BE FROM THE TH DAF.            */ 01185000
*/*                                                                  */ 01186000
*/********************************************************************/ 01187000
*                                                                  0667 01188000
*SNDERRSP:                                                         0667 01189000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0667 01190000
SNDERRSP DS    0H                                                  0668 01191000
*   R14SV06=R14;                    /* SAVE RETURN REG               */ 01192000
         ST    R14,R14SV06                                         0668 01193000
*/*                                                                  */ 01194000
*                                                                  0669 01195000
*   IF TTHFTYPE=TTHFID1&            /* FID1 PIU AND...               */ 01196000
*       TTHBBIU=ON&                 /* BEGIN BIU AND...              */ 01197000
*       TTHDCF>=3&                  /* RH IS PRESENT AND...          */ 01198000
*       TRHTYPE=OFF&                /* REQUEST                       */ 01199000
*       (TRHDR1=ON|TRHDR2=ON) THEN  /* REQUEST ASKING FOR A DEFINITE    01200000
*                                      OR EXCEPTION RESPONSE         */ 01201000
         TM    TTHFTYPE(PIUSTART),B'00010000'                      0669 01202000
         BNO   @RF00669                                            0669 01203000
         TM    TTHFTYPE(PIUSTART),B'11100000'                      0669 01204000
         BNZ   @RF00669                                            0669 01205000
         TM    TTHBBIU(PIUSTART),B'00001000'                       0669 01206000
         BNO   @RF00669                                            0669 01207000
         CLC   TTHDCF(2,PIUSTART),@CH00065                         0669 01208000
         BL    @RF00669                                            0669 01209000
         LA    @09,10                                              0669 01210000
         ALR   @09,PIUSTART                                        0669 01211000
         TM    TRHTYPE(@09),B'10000000'                            0669 01212000
         BNZ   @RF00669                                            0669 01213000
         TM    TRHDR1(@09),B'10100000'                             0669 01214000
         BZ    @RF00669                                            0669 01215000
*     DO;                           /* YES                           */ 01216000
*       IF PRF2TNTI=ON THEN         /* ARE OAF AND DAF TNT OFFSETS   */ 01217000
         L     @09,IEDQPPTR                                        0671 01218000
         AL    @09,@CF01670                                        0671 01219000
         TM    PRF2TNTI(@09),B'10000000'                           0671 01220000
         BNO   @RF00671                                            0671 01221000
*         DO;                       /* YES                           */ 01222000
*           PLTNTDAF=TTHDAF;        /* SETUP DAF TNT OFFSET FOR    0673 01223000
*                                      RSPBUILD ROUTINE              */ 01224000
         MVC   PLTNTDAF(2),TTHDAF(PIUSTART)                        0673 01225000
*           CALL RSPBUILD;          /* BUILD AN ERROR RESPONSE       */ 01226000
         BAL   @14,RSPBUILD                                        0674 01227000
*           CALL POSTPCO;           /* POST THE ERROR RSP TO PATH  0675 01228000
*                                      CONTROL OUTBOUND              */ 01229000
         BAL   @14,POSTPCO                                         0675 01230000
*         END;                                                     0676 01231000
*       ELSE                        /* OAF AND DAF ARE NETWORK     0677 01232000
*                                      ADDRESSES                     */ 01233000
*         DO;                                                      0677 01234000
         B     @RC00671                                            0677 01235000
@RF00671 DS    0H                                                  0678 01236000
*           RPARM=TTHOAF;           /* OAF NETWORK ADDRESS           */ 01237000
         SLR   RPARM,RPARM                                         0678 01238000
         ICM   RPARM,3,TTHOAF(PIUSTART)                            0678 01239000
*           CALL IEDIAP03;          /* CONVERT THE OAF NETWORK     0679 01240000
*                                      ADDRESS TO ITS TNT OFFSET.  0679 01241000
*                                      THIS IS DONE TO VALIDITY CHECK   01242000
*                                      THE OAF.                      */ 01243000
         L     @09,AVTSAVTP(,IEDQAPTR)                             0679 01244000
         L     @15,SAVTNAX(,@09)                                   0679 01245000
         BALR  @14,@15                                             0679 01246000
*           IF R15RC>0 THEN         /* IS THE OAF VALID              */ 01247000
         LTR   R15RC,R15RC                                         0680 01248000
         BNP   @RF00680                                            0680 01249000
*             DO;                   /* YES-SET UP DAF TNT FOR      0681 01250000
*                                      RSPBUILD ROUTINE              */ 01251000
*               RPARM=TTHDAF;       /* DAF NETWORK ADDRESS           */ 01252000
         SLR   RPARM,RPARM                                         0682 01253000
         ICM   RPARM,3,TTHDAF(PIUSTART)                            0682 01254000
*               CALL IEDIAP03;      /* CONVERT DAF TO ITS TNT OFFSET */ 01255000
         L     @09,AVTSAVTP(,IEDQAPTR)                             0683 01256000
         L     @15,SAVTNAX(,@09)                                   0683 01257000
         BALR  @14,@15                                             0683 01258000
*               IF R15RC>0 THEN     /* DAF CONVERSION SUCESSFUL      */ 01259000
         LTR   R15RC,R15RC                                         0684 01260000
