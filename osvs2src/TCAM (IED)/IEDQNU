*                   RPARM->BDUHEADR=ON;/* HEADER PREFIX              */ 00282000
         OI    BDUHEADR(RPARM),B'01000000'                      05S0571 00283000
*                   PLMVTU=ADDR(CNRDVRCD);/* SET TARGET TO RECEIVE      00284000
*                                      ...                           */ 00285000
         LA    @14,CNRDVRCD(,RCKPREC)                           05S0572 00286000
