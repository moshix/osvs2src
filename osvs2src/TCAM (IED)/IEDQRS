*                   TTHDCF=10;      /* SET DATA COUNT                */ 01002000
         LA    @10,10                                              0768 01003000
         STCM  @10,3,TTHDCF(PIUPTR)                                0768 01004000
*                 END;                                             0769 01005000
*               ELSE                /* NOT FORMATTED                 */ 01006000
