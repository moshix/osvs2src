         NI    EXCPRSP,B'01111111'                                 0668 00665000
*               CALL SENDRESP;      /* SEND +RSP TO CANCEL   @YM08571*/ 00666000
         BAL   @14,SENDRESP                                        0669 00667000
*             END;                  /*                       @YM08571*/ 00668000
*         END;                      /*                       @YM08571*/ 00669000
