*               DO;                                             04S0833 00727000
*                 CANCLFLG=ON;      /* SET CANCEL FLAG               */ 00728000
         OI    CANCLFLG,B'00100000'                             04S0834 00729000
*                 CTSRTADR=ADDR(CANLDATA);/* START OF CONTROL TERM      00730000
*                                      FIELD                 @YM07358*/ 00731000
         LA    CTSRTADR,CANLDATA(,@14)                          04S0835 00732000
*                 DO REG01=CTSRTADR TO ADDR(CANLLAST) WHILE(DATABYTE^=  00733000
*                       SLASH);     /* LOCATE SLASH          @YM07358*/ 00734000
         LR    REG01,CTSRTADR                                   04S0836 00735000
         B     @DE00836                                         04S0836 00736000
@DL00836 CLI   DATABYTE(REG01),C'/'                             04S0836 00737000
         BE    @DC00836                                         04S0836 00738000
*                 END;                                          04S0837 00739000
*                                                               04S0837 00740000
         AL    REG01,@CF00062                                   04S0837 00741000
@DE00836 L     @14,DATAPTR                                      04S0837 00742000
         LA    @14,CANLLAST(,@14)                               04S0837 00743000
         CR    REG01,@14                                        04S0837 00744000
         BNH   @DL00836                                         04S0837 00745000
@DC00836 DS    0H                                               04S0838 00746000
