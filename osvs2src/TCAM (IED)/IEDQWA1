         ST    @10,PLMVTD(,@14)                                 03S1161 01826000
*       PLMVSU=ADDR(WORKAREA);      /* SOURCE UNIT                   */ 01827000
         LA    @10,WORKAREA                                     03S1162 01828000
         ST    @10,PLMVSU(,@14)                                 03S1162 01829000
*       PLMVSD=PLMVSU;              /* SOURCE DATA                   */ 01830000
         ST    @10,PLMVSD(,@14)                                 03S1163 01831000
