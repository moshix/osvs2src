         BNO   @RF01120                                         03S1120 01709000
*         IF TRMSNA=ON&DCTL3270=ON THEN/* IF THIS IS A SNA 3270      */ 01710000
         LR    @14,RTRM                                         03S1121 01711000
         AL    @14,@CF03678                                     03S1121 01712000
         TM    TRMSNA-10(@14),B'00000010'                       03S1121 01713000
         BNO   @RF01121                                         03S1121 01714000
         TM    DCTL3270(RDCT),B'01000000'                       03S1121 01715000
         BNO   @RF01121                                         03S1121 01716000
*           DO;                                                 03S1122 01717000
*             MSGPTR=ADDR(WORKAREA);/* INIT POINTER FOR MSG MOVE     */ 01718000
         LA    MSGPTR,WORKAREA                                  03S1123 01719000
*             MSGAREA(1:LENGTH(S3270))=S3270;/* INSERT WRITE ERASE   */ 01720000
         MVC   MSGAREA(2,MSGPTR),S3270                          03S1124 01721000
*             MSGPTR=MSGPTR+LENGTH(S3270);/* UPDATE POINTER          */ 01722000
         AL    MSGPTR,@CF00133                                  03S1125 01723000
*             MSGAREA(1:LENGTH(MSG149I))=MSG149I;/* MOVE IN TOTE BUSY   01724000
*                                      MESSAGE                       */ 01725000
         MVC   MSGAREA(17,MSGPTR),MSG149I                       03S1126 01726000
*             RMSGLEN=S3270L;       /* LENGTH OF MESSAGE             */ 01727000
         LH    RMSGLEN,S3270L                                   03S1127 01728000
*           END;                    /* END OF SNA 3270 PROCESSING    */ 01729000
*         ELSE                                                  03S1129 01730000
*           DO;                     /* NCP, NON 3270 SNA             */ 01731000
         B     @RC01121                                         03S1129 01732000
@RF01121 DS    0H                                               03S1130 01733000
*             MSGPTR=ADDR(WORKAREA);/* INIT POINTER FOR MOVE         */ 01734000
         LA    MSGPTR,WORKAREA                                  03S1130 01735000
*             MSGAREA(1:LENGTH(MSG149I))=MSG149I;/* MOVE IN TOTE BUSY   01736000
*                                      MSG                           */ 01737000
         MVC   MSGAREA(17,MSGPTR),MSG149I                       03S1131 01738000
*             MSGPTR=MSGPTR+LENGTH(MSG149I);/* UPDATE POINTER        */ 01739000
         AL    MSGPTR,@CF03569                                  03S1132 01740000
*             IF DCT3270=ON THEN    /* IF 3270 DEVICE                */ 01741000
         TM    DCT3270(RDCT),B'00000100'                        03S1133 01742000
         BNO   @RF01133                                         03S1133 01743000
*               MSGAREA(1:1)='';    /* ADD BLANK                     */ 01744000
         MVI   MSGAREA(MSGPTR),C' '                             03S1134 01745000
*             ELSE                                              03S1135 01746000
*               MSGAREA(1:LENGTH(NLCHAR))=NLCHAR;/* NEW LINE    03S1135 01747000
*                                      CHARACTER                     */ 01748000
