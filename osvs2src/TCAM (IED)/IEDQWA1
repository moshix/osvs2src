*             RMSGLEN=EPSSL;        /* LENGTH OF EP START STOP MSG   */ 01793000
         LH    RMSGLEN,EPSSL                                    03S1153 01794000
*           END;                                                03S1154 01795000
*       IEDPLMV=''B;                /* CLEAR IEDMVBFR PARM LIST      */ 01796000
@RC01138 DS    0H                                               03S1155 01797000
@RC01120 LA    @14,PARMLSP                                      03S1155 01798000
         XC    IEDPLMV(24,@14),IEDPLMV(@14)                     03S1155 01799000
*       IF RMSGLEN>(PRFNBUNT*AVTKEYLE-AVTHDRSZ) THEN/* IF MSG TOO BIG   01800000
*                                      FOR BUF                       */ 01801000
