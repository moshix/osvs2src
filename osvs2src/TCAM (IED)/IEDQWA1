         STCM  @14,7,PRFLINK(RPREFIX)                           02S1072 01519000
*       RZERO=ADDR(WORKAREA);       /* WORKAREA USED BY FREEBUF      */ 01520000
         LA    RZERO,WORKAREA                                   02S1073 01521000
*       CALL FREEBUF;               /* POST BUFFER TO BUFFER RETURN  */ 01522000
         L     @15,RESFBUF(,RRESPL)                             02S1074 01523000
         BALR  @14,@15                                          02S1074 01524000
*     END;                                                      02S1075 01525000
*   BUFRTFLG=ON;                    /* SET BUFFER RETURN FLAG   02S1076 01526000
*                                      INDICATING NO FURTHER    02S1076 01527000
*                                      PROCESSING                    */ 01528000
@RC01055 OI    BUFRTFLG,B'00001000'                             02S1076 01529000
*   RFY                                                         02S1077 01530000
*     IEDQLCB BASED;                                            02S1077 01531000
*   END RTNBUF;                                                 02S1078 01532000
@EL00002 DS    0H                                               02S1078 01533000
@EF00002 DS    0H                                               02S1078 01534000
@ER00002 L     @14,@SA00002                                     02S1078 01535000
         BR    @14                                              02S1078 01536000
*                                                               02S1079 01537000
*   /*****************************************************************/ 01538000
*   /*                                                               */ 01539000
*   /* HEXCONV: THIS GENERAL PURPOSE ROUTINE CONVERTS A STRING OF    */ 01540000
*   /* HEXIDECIMAL DIGITS TO THEIR EBCDIC REPRESENTATION .           */ 01541000
*   /*                                                               */ 01542000
*   /*****************************************************************/ 01543000
*                                                               02S1079 01544000
*/* PARAMETERS:                                                      */ 01545000
*/* - CONVNUM = THE NUMBER OF HEXIDECIMAL DIGITS                     */ 01546000
*/* - CONVSRC = THE ADDRESS OF THE START OF THE HEXIDECIMAL DIGITS   */ 01547000
*/* - CONVTRG = THE ADDRESS OF LOCATION TO WHICH THE CONVERTED EBCDIC*/ 01548000
*/*             REPRESENTATIONS ARE TO BE PLACED                     */ 01549000
*/* RESTRICTIONS:                                                    */ 01550000
*/* - THE HEXIDECIMAL DIGITS MUST START ON A BYTE BOUNDRY            */ 01551000
*                                                               02S1079 01552000
*HEXCONV:                                                       02S1079 01553000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                            02S1079 01554000
HEXCONV  DS    0H                                               02S1080 01555000
*   RFY                                                         02S1080 01556000
*     RRET RSTD;                                                02S1080 01557000
*   HEXSAV14=RRET;                  /* SAVE RETURN ADDRESS           */ 01558000
         ST    RRET,HEXSAV14                                    02S1081 01559000
*   RFY                                                         02S1082 01560000
*     RRET UNRSTD;                                              02S1082 01561000
*   DO I=1 TO CONVNUM BY 2;                                     02S1083 01562000
         LA    I,1                                              02S1083 01563000
         B     @DE01083                                         02S1083 01564000
@DL01083 DS    0H                                               02S1084 01565000
*     CONVIDX=CONVSRC->BYTENUM;     /* GET FIRST TWO DIGITS          */ 01566000
         L     @14,CONVSRC                                      02S1084 01567000
         SLR   @07,@07                                          02S1084 01568000
         IC    @07,BYTENUM(,@14)                                02S1084 01569000
         STH   @07,CONVIDX                                      02S1084 01570000
*     SRL(CONVIDX,4);               /* ISOLATE FIRST DIGIT           */ 01571000
         LR    @14,@07                                          02S1085 01572000
         SRL   @14,4                                            02S1085 01573000
         STH   @14,CONVIDX                                      02S1085 01574000
*     CONVTRG->DATABYTE=HEXTCHAR(CONVIDX+1);/* MOVE CONVERTED VALUE  */ 01575000
         LH    @14,CONVIDX                                      02S1086 01576000
         N     @14,@CF03658                                     02S1086 01577000
         LA    @07,HEXTCHAR(@14)                                02S1086 01578000
         MVC   DATABYTE(1,CONVTRG),0(@07)                       02S1086 01579000
*     CONVTRG=CONVTRG+1;            /* BUMP TARGET PTR TO NEXT BYTE  */ 01580000
         AL    CONVTRG,@CF00062                                 02S1087 01581000
*     IF I<=CONVNUM-1 THEN          /* IF END NOT IN MIDDLE OF SRC      01582000
*                                      BYTE                          */ 01583000
         LH    @14,CONVNUM                                      02S1088 01584000
         BCTR  @14,0                                            02S1088 01585000
         CR    I,@14                                            02S1088 01586000
         BH    @RF01088                                         02S1088 01587000
*       DO;                         /* CONVERT NEXT DIGIT IN BYTE    */ 01588000
*         CONVIDX=CONVSRC->BYTENUM; /* GET CONTENTS OF BYTE          */ 01589000
         L     @14,CONVSRC                                      02S1090 01590000
         SLR   @07,@07                                          02S1090 01591000
         IC    @07,BYTENUM(,@14)                                02S1090 01592000
         STH   @07,CONVIDX                                      02S1090 01593000
*         CONVIDX=CONVIDX&HIMASK;   /* ISOLATE SECOND DIGIT          */ 01594000
         SLR   @14,@14                                          02S1091 01595000
         ICM   @14,3,HIMASK                                     02S1091 01596000
         NR    @07,@14                                          02S1091 01597000
         STH   @07,CONVIDX                                      02S1091 01598000
*         CONVTRG->DATABYTE=HEXTCHAR(CONVIDX+1);/* PLACE IN TARGET      01599000
*                                      LOCATION                      */ 01600000
         LA    @14,HEXTCHAR(@07)                                02S1092 01601000
         MVC   DATABYTE(1,CONVTRG),0(@14)                       02S1092 01602000
*       END;                                                    02S1093 01603000
*     ELSE                                                      02S1094 01604000
*       ;                           /* END IN MIDDLE OF SOURCE BYTE  */ 01605000
@RF01088 DS    0H                                               02S1095 01606000
*     CONVSRC=CONVSRC+1;            /* POINT TO NEXT SOURCE BYTE     */ 01607000
         LA    @14,1                                            02S1095 01608000
         L     @07,CONVSRC                                      02S1095 01609000
         ALR   @07,@14                                          02S1095 01610000
         ST    @07,CONVSRC                                      02S1095 01611000
*     CONVTRG=CONVTRG+1;            /* POINT TO NEXT TARGET BYTE     */ 01612000
         ALR   CONVTRG,@14                                      02S1096 01613000
*   END;                            /* END OF CONVERSION LOOP        */ 01614000
         AL    I,@CF00133                                       02S1097 01615000
@DE01083 CH    I,CONVNUM                                        02S1097 01616000
         BNH   @DL01083                                         02S1097 01617000
*   RFY                                                         02S1098 01618000
*     RRET RSTD;                                                02S1098 01619000
*   RRET=HEXSAV14;                  /* RESTORE RETURN REG            */ 01620000
         L     RRET,HEXSAV14                                    02S1099 01621000
*   RFY                                                         02S1100 01622000
*     RRET UNRSTD;                                              02S1100 01623000
*   END HEXCONV;                                                02S1101 01624000
@EL00003 DS    0H                                               02S1101 01625000
@EF00003 DS    0H                                               02S1101 01626000
@ER00003 BR    @14                                              02S1101 01627000
*                                                               02S1102 01628000
*   /*****************************************************************/ 01629000
*   /*                                                               */ 01630000
*   /* REQMSG: THIS SUBROUTINE CONTAINS THE CODE TO RETURN THE TOTE  */ 01631000
*   /* BUSY MESSAGE TO THE REQUESTING TERMINAL.                      */ 01632000
*   /*                                                               */ 01633000
*   /*****************************************************************/ 01634000
*                                                               02S1102 01635000
*REQMSG:                                                        02S1102 01636000
*   PROC;                                                       02S1102 01637000
REQMSG   STM   @14,@12,@SA00004                                 02S1102 01638000
*   RFY                                                         02S1103 01639000
*     IEDQQCB BASED(TRMDESTQ);                                  02S1103 01640000
*   RFY                                                         02S1104 01641000
*     IEDQLCB BASED(PRFLCB);                                    02S1104 01642000
*   RFY                                                         02S1105 01643000
*     IEDQSCB BASED(LCBSCBA);                                   02S1105 01644000
*   IF PRFKEY^=PRFSYS THEN          /* IF REQUEST NOT FROM SYS  02S1106 01645000
*                                      CONSOLE                       */ 01646000
         CLI   PRFKEY(RPREFIX),X'0C'                            02S1106 01647000
         BE    @RF01106                                         02S1106 01648000
*     DO;                                                       02S1107 01649000
*       RPARM=PRFSRCE;              /* USE SOURCE TTCIN              */ 01650000
         LH    RPARM,PRFSRCE(,RPREFIX)                          02S1108 01651000
         N     RPARM,@CF03658                                   02S1108 01652000
*       CALL IEDQTNT;               /* TO CALCULATE TTE ADDRESS      */ 01653000
         L     @15,AVTRNMPT(,RAVT)                              02S1109 01654000
         BALR  @14,@15                                          02S1109 01655000
*       RTRM=RPARM;                 /* TTE BASE                      */ 01656000
         LR    RTRM,RPARM                                       02S1110 01657000
*       RDCT=AVTCSTCS+(TRMCHCIN-1)*LENGTH(IEDDCT);/* ENTRY IN DCT    */ 01658000
         SLR   RDCT,RDCT                                        02S1111 01659000
         IC    RDCT,TRMCHCIN(,RTRM)                             02S1111 01660000
         BCTR  RDCT,0                                           02S1111 01661000
         MH    RDCT,@CH00073                                    02S1111 01662000
         AL    RDCT,AVTCSTCS(,RAVT)                             02S1111 01663000
*     END;                                                      02S1112 01664000
*   ELSE                                                        02S1113 01665000
*     ;                                                         02S1113 01666000
@RF01106 DS    0H                                               02S1114 01667000
*   IF PRFKEY=PRFSYS|               /* IF REQUEST FROM SYS CONSOLE   */ 01668000
*       TRMCONC=ON|                 /* REQ FROM TERM ON CONCENTRATOR */ 01669000
*       (AVTTSON=ON&AVTAQTAN=OFF)|  /* TSO ONLY SYSTEM               */ 01670000
*       (QCBTSSES=ON)|              /* TERM IN TSO SESSION           */ 01671000
*       (QCBREUS=OFF&QCBNREUS=OFF&QCBCORE=OFF)/* NO QUEUES           */ 01672000
*     THEN                          /* CAN'T SEND TO REQUEST TERM    */ 01673000
         CLI   PRFKEY(RPREFIX),X'0C'                            02S1114 01674000
         BE    @RT01114                                         02S1114 01675000
         TM    TRMCONC(RTRM),B'10000000'                        02S1114 01676000
         BO    @RT01114                                         02S1114 01677000
         TM    AVTTSON(RAVT),B'01000000'                        02S1114 01678000
         BNO   @GL00037                                         02S1114 01679000
         TM    AVTAQTAN(RAVT),B'00100000'                       02S1114 01680000
         BZ    @RT01114                                         02S1114 01681000
@GL00037 L     @14,TRMDESTQ-1(,RTRM)                            02S1114 01682000
