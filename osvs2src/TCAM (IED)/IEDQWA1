         BNE   @RF00658                                         03S0658 00113000
@RT00658 DS    0H                                               03S0659 00114000
*                                   /* IF TEMPORARY ERROR    @YM09016*/ 00115000
*             DO;                   /* NO ACTION NECESSARY - RETURN     00116000
*                                      BUF                           */ 00117000
*               PRFKEY=''B;         /* CLEAR                         */ 00118000
         MVI   PRFKEY(RPREFIX),X'00'                            03S0660 00119000
*               CALL RTNBUF;                                    03S0661 00120000
         BAL   @14,RTNBUF                                       03S0661 00121000
*             END;                  /* END OF TEMPORARY PROBLEM      */ 00122000
*           ELSE                    /* SEVERE ERROR ON OUTPUT TO        00123000
*                                      CT/AP                         */ 00124000
*             DO;                   /* SET PRFKEY AND PRINT MESSAGE  */ 00125000
         B     @RC00658                                         03S0663 00126000
@RF00658 DS    0H                                               03S0664 00127000
*               PRFKEY=PRFDEVFL;    /* SET DEVICE FAILURE INDICATOR  */ 00128000
         MVI   PRFKEY(RPREFIX),X'F0'                            03S0664 00129000
*               CALL DEVFLMSG;      /* PRINT MESSAGE                 */ 00130000
         BAL   @14,DEVFLMSG                                     03S0665 00131000
*             END;                  /* END OF SEVERE CT/AP PROBLEM      00132000
*                                      HDLG                          */ 00133000
*       ELSE                        /* INBOUND DATA FROM SNA DEVICE  */ 00134000
*         DO;                       /* MAY BE CECOM REPLY OR TEST       00135000
*                                      DATA                          */ 00136000
         B     @RC00655                                         03S0667 00137000
@RF00655 DS    0H                                               03S0668 00138000
*           RPARM=PRFSRCE;          /* OBTAIN TTCIN OF RESOURCE AND  */ 00139000
         LH    RPARM,PRFSRCE(,RPREFIX)                          03S0668 00140000
         N     RPARM,@CF03658                                   03S0668 00141000
*           CALL IEDQTNT;           /* GET TTE ADDR                  */ 00142000
         L     @15,AVTRNMPT(,RAVT)                              03S0669 00143000
         BALR  @14,@15                                          03S0669 00144000
*           RTRM=RPARM;             /* TTE BASE                      */ 00145000
         LR    RTRM,RPARM                                       03S0670 00146000
*           IF TRMTDIFF=ON&PRFDEST^=SAVTSCPT THEN/* IF TEST DATA        00147000
*                                                            @YM08575*/ 00148000
         LR    @14,RTRM                                         03S0671 00149000
         AL    @14,@CF03659                                     03S0671 00150000
         TM    TRMTDIFF-5(@14),B'00010000'                      03S0671 00151000
         BNO   @RF00671                                         03S0671 00152000
         L     @14,AVTSAVTP(,RAVT)                              03S0671 00153000
         CLC   PRFDEST(2,RPREFIX),SAVTSCPT(@14)                 03S0671 00154000
         BE    @RF00671                                         03S0671 00155000
*             DO;                                               03S0672 00156000
*               PRFKEY=PRFSNAT;     /* INDICATOR                     */ 00157000
         MVI   PRFKEY(RPREFIX),X'28'                            03S0673 00158000
*               IF(PRFSIZE-PRFSCAN)=1&RUBYTE1='00'X THEN/* FORMAT FOR   00159000
*                                      NULL RU                       */ 00160000
         LH    @14,PRFSCAN(,RPREFIX)                            03S0674 00161000
         LH    @10,PRFSIZE(,RPREFIX)                            03S0674 00162000
         SLR   @10,@14                                          03S0674 00163000
         C     @10,@CF00062                                     03S0674 00164000
         BNE   @RF00674                                         03S0674 00165000
         LA    @10,PRFSUNIT(,RPREFIX)                           03S0674 00166000
         ALR   @10,@14                                          03S0674 00167000
         CLI   RUBYTE1(@10),X'00'                               03S0674 00168000
         BNE   @RF00674                                         03S0674 00169000
*                 PRFSIZE=PRFSCAN;  /* SET AS NULL RU FOR RECEIVE MOD*/ 00170000
         STH   @14,PRFSIZE(,RPREFIX)                            03S0675 00171000
*               ELSE                                            03S0676 00172000
*                 ;                                             03S0676 00173000
@RF00674 DS    0H                                               03S0677 00174000
*             END;                                              03S0677 00175000
*           ELSE                    /* MUST BE CECOM REPLY           */ 00176000
*             IF PRFNHDRN=OFF THEN  /* IF THIS IS A HEADER BUFFER    */ 00177000
         B     @RC00671                                         03S0678 00178000
@RF00671 TM    PRFNHDRN(RPREFIX),B'00000001'                    03S0678 00179000
         BNZ   @RF00678                                         03S0678 00180000
*               PRFKEY=PRFSNACE;    /* SET INDICATOR FOR CE REPLY FOR   00181000
*                                      SNA RESOURCE (NO PLCB)        */ 00182000
         MVI   PRFKEY(RPREFIX),X'1C'                            03S0679 00183000
*             ELSE                  /* TEXT BUFFER, ALL CE MSGS ARE  */ 00184000
*               DO;                 /* RESTICTED TO 1 BUFFER, IGNORE */ 00185000
         B     @RC00678                                         03S0680 00186000
@RF00678 DS    0H                                               03S0681 00187000
*                 PRFKEY=''B;       /* CLEAR FOR CALL TO RTNBUF      */ 00188000
         MVI   PRFKEY(RPREFIX),X'00'                            03S0681 00189000
*                 CALL RTNBUF;      /* RETURN BUFFER TO TCAM         */ 00190000
         BAL   @14,RTNBUF                                       03S0682 00191000
*               END;                /* END OF TEXT BUFFER HANDLING   */ 00192000
*         END;                      /* END OF INBOUND SNA DATA  03S0684 00193000
*                                      HANDLING                      */ 00194000
*     END;                          /* END OF MH INPUT PROCESSING    */ 00195000
*   ELSE                            /* NOT INPUT FROM SSCP OR TOTE MH*/ 00196000
*     IF PRFKEY=PRFSRTMH THEN       /* IF FROM STARTMH MODULE        */ 00197000
         B     @RC00650                                         02S0686 00198000
@RF00650 CLI   PRFKEY(RPREFIX),X'04'                            02S0686 00199000
         BNE   @RF00686                                         02S0686 00200000
*       DO;                         /* SET PRFKEY AND SET LCB        */ 00201000
*                                                               03S0688 00202000
*         /***********************************************************/ 00203000
*         /*                                                         */ 00204000
*         /* PROCESS.STARTMH: THIS SEGMENT PROCESSES BUFFERS POSTED  */ 00205000
*         /* TO TOTE FROM THE STARTMH MODULE (IEDQAA). THESE BUFFERS */ 00206000
*         /* CONTAIN CEINPUT FROM START-STOP AND BI-SYNCH TERMINALS. */ 00207000
*         /* ONLY THE FIRST BUFFER OF THE MESSAGE IS ASSUMED TO HAVE */ 00208000
*         /* VALID INPUT, ALL OTHERS ARE RETURNED TO TCAM. PRFKEY IS */ 00209000
*         /* SET IN THE VALID BUFFERS.                               */ 00210000
*         /*                                                         */ 00211000
*         /***********************************************************/ 00212000
*                                                               03S0688 00213000
*         RFY                                                   03S0688 00214000
*           IEDQLCB BASED(PRFLCB);  /* BASE LCB ON BUF PREFIX FIELD  */ 00215000
*         IF PRFNHDRN=OFF THEN      /* IF THIS IS A HEADER BUFFER    */ 00216000
         TM    PRFNHDRN(RPREFIX),B'00000001'                    03S0689 00217000
         BNZ   @RF00689                                         03S0689 00218000
*           DO;                     /* THIS IS A VALID BUFFER        */ 00219000
*             IF PRFNLSTN=OFF THEN  /* IF LAST BUF IN MSG            */ 00220000
         TM    PRFNLSTN(RPREFIX),B'00000010'                    03S0691 00221000
         BNZ   @RF00691                                         03S0691 00222000
*               DO;                 /* TREAT AS HEADER LAST          */ 00223000
*                 IF LCBSYNC=OFF THEN/* IF START STOP DEVICE         */ 00224000
         L     @14,PRFLCB-1(,RPREFIX)                           03S0693 00225000
         TM    LCBSYNC(@14),B'00000100'                         03S0693 00226000
         BNZ   @RF00693                                         03S0693 00227000
*                   PRFKEY=PRFSS;   /* SET PRFKEY                    */ 00228000
         MVI   PRFKEY(RPREFIX),X'08'                            03S0694 00229000
*                 ELSE                                          03S0695 00230000
*                   ;               /* PRFKEY ALREADY SET FOR BSC    */ 00231000
@RF00693 DS    0H                                               03S0696 00232000
*                 LCBOLT=OFF;       /* RESET OLT BIT FOR NEXT I/O    */ 00233000
         L     @14,PRFLCB-1(,RPREFIX)                           03S0696 00234000
         NI    LCBOLT(@14),B'11111110'                          03S0696 00235000
*               END;                /* END OF HEADER LAST PROCESSING */ 00236000
*             ELSE                  /* HEADER,NOT LAST               */ 00237000
*               PRFKEY=PRFMULT;     /* SET MULTIPLE BUFFERED INPUT   */ 00238000
         B     @RC00691                                         03S0698 00239000
@RF00691 MVI   PRFKEY(RPREFIX),X'14'                            03S0698 00240000
*             LCBTSTSW=AVTEFF;      /* RESET                         */ 00241000
@RC00691 L     @14,PRFLCB-1(,RPREFIX)                           03S0699 00242000
         LA    @14,0(,@14)                                      03S0699 00243000
         MVI   LCBTSTSW(@14),X'FF'                              03S0699 00244000
*             LCBNEGRP=ON;          /* NEG RESP POLL                 */ 00245000
         OI    LCBNEGRP(@14),B'00001000'                        03S0700 00246000
*           END;                    /* END HEADER BUFFER HANDLING    */ 00247000
*         ELSE                      /* NOT HEADER BUFFER             */ 00248000
*           IF PRFNLSTN=ON THEN     /* IF NOT LAST BUFFER            */ 00249000
         B     @RC00689                                         03S0702 00250000
@RF00689 TM    PRFNLSTN(RPREFIX),B'00000010'                    03S0702 00251000
         BNO   @RF00702                                         03S0702 00252000
*             DO;                   /* RETURN BUFFER BUT NOT LCB     */ 00253000
*               PRFKEY=''B;         /* CLEAR TO INDICATE NO LCB      */ 00254000
         MVI   PRFKEY(RPREFIX),X'00'                            03S0704 00255000
*               CALL RTNBUF;        /* RETURN BUFFER                 */ 00256000
         BAL   @14,RTNBUF                                       03S0705 00257000
*             END;                  /* END ^FIRST,^LAST BUF HANDLING */ 00258000
*           ELSE                    /* LAST BUFFER IN MESSAGE        */ 00259000
*             DO;                   /* RETURN BUFFER AND LCB TO TCAM */ 00260000
         B     @RC00702                                         03S0707 00261000
@RF00702 DS    0H                                               03S0708 00262000
*               LCBOLT=OFF;         /* RESET OLT INDICATOR           */ 00263000
         L     @14,PRFLCB-1(,RPREFIX)                           03S0708 00264000
         NI    LCBOLT(@14),B'11111110'                          03S0708 00265000
*               CALL RTNBUF;        /* RETURN BUFFER AND LCB         */ 00266000
         BAL   @14,RTNBUF                                       03S0709 00267000
*             END;                  /* END OF LAST BUF HANDLING      */ 00268000
*         RFY                                                   03S0711 00269000
*           IEDQLCB BASED;          /* SET AS ON ENTRY TO SEGMENT    */ 00270000
@RC00702 DS    0H                                               03S0711 00271000
@RC00689 DS    0H                                               03S0712 00272000
*       END;                        /* END OF STARTMH PROCESSING     */ 00273000
*     ELSE                                                      02S0713 00274000
*       ;                           /* PRFKEY ALREADY SET CORRECTLY  */ 00275000
@RF00686 DS    0H                                               02S0714 00276000
*   IF BUFRTFLG=ON THEN                                         02S0714 00277000
@RC00650 TM    BUFRTFLG,B'00001000'                             02S0714 00278000
         BO    @RT00714                                         02S0714 00279000
*     ;                             /* IF BUFFER RETURNED - NO MORE     00280000
*                                      PROCESSING                    */ 00281000
*   ELSE                                                        02S0716 00282000
*     IF PRFKEY<PRFCEIN THEN        /* IF THIS IS CE INPUT           */ 00283000
