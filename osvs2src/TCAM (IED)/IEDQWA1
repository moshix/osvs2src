*                                                            @YM07336*/ 00018000
         L     @14,BACKSAVE(,RSAVE)                             02S0642 00019000
         ST    @14,BACK13                                       02S0642 00020000
*   RRESPL=RPARM;                   /* TOTE RESPL BASE               */ 00021000
         LR    RRESPL,RPARM                                     02S0643 00022000
*   FLAGS=''B;                      /* INIT MODULE CONTROL FLAGS     */ 00023000
