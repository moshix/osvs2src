         BO    @RT01138                                         03S1138 01759000
         TM    DCTNOIDL(RDCT),B'00000010'                       03S1138 01760000
         BNO   @RF01138                                         03S1138 01761000
@RT01138 DS    0H                                               03S1139 01762000
*           DO;                                                 03S1139 01763000
*             MSGPTR=ADDR(WORKAREA);/* INIT POINTER                  */ 01764000
         LA    MSGPTR,WORKAREA                                  03S1140 01765000
*             MSGAREA(1:LENGTH(STXCH))=STXCHAR;/* INSERT STX         */ 01766000
