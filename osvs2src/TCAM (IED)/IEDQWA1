*       IF LCBRSKEY=ANRKEY THEN     /* IF ANR                        */ 01480000
         CLI   LCBRSKEY(RPARM),X'1E'                            02S1059 01481000
         BNE   @RF01059                                         02S1059 01482000
*         LCBTSTSW=ANRSW;           /* SET SWITCH                    */ 01483000
         MVI   LCBTSTSW(RPARM),X'F0'                            02S1060 01484000
*       ELSE                                                    02S1061 01485000
*         ;                                                     02S1061 01486000
@RF01059 DS    0H                                               02S1062 01487000
*       SCBDESTQ=ADDR(AVTBFRTB);    /* SET SCB WITH ADDR OF BUFF RTN */ 01488000
         L     @14,LCBSCBA-1(,RPARM)                            02S1062 01489000
         LA    @14,0(,@14)                                      02S1062 01490000
         LA    @10,AVTBFRTB(,RAVT)                              02S1062 01491000
         STCM  @10,7,SCBDESTQ(@14)                              02S1062 01492000
*       SCBMACR=ADDR(AVTINOUT);     /* DUMMY IN-OUT PARM LIST        */ 01493000
         LA    @10,AVTINOUT(,RAVT)                              02S1063 01494000
         STCM  @10,7,SCBMACR(@14)                               02S1063 01495000
*       LCBPRI=PRILCB;              /* SET LCB POST PRIORITY         */ 01496000
         MVI   LCBPRI(RPARM),X'E7'                              02S1064 01497000
*       LCBQCBA=AVTMSGS->BDADDR;    /* ADDR OF BUFFER DISP           */ 01498000
         L     @14,AVTMSGS-1(,RAVT)                             02S1065 01499000
         MVC   LCBQCBA(3,RPARM),BDADDR+1(@14)                   02S1065 01500000
*       RZERO=ADDR(WORKAREA);       /* WORKAREA USED BY POSTWAIT     */ 01501000
         LA    RZERO,WORKAREA                                   02S1066 01502000
*       LCBLINK=''B;                /* INDICATES TO POSTWAIT THAT       01503000
*                                      THIS IS A SINGLE ELEMENT POST */ 01504000
         SLR   @14,@14                                          02S1067 01505000
         STCM  @14,7,LCBLINK(RPARM)                             02S1067 01506000
*       CALL POSTWAIT;              /* POST LCB TO BUFFER DISP       */ 01507000
         L     @15,RESPLRVT(,RRESPL)                            02S1068 01508000
         BALR  @14,@15                                          02S1068 01509000
*     END;                                                      02S1069 01510000
*   ELSE                                                        02S1070 01511000
*     DO;                                                       02S1070 01512000
         B     @RC01055                                         02S1070 01513000
@RF01055 DS    0H                                               02S1071 01514000
*       RPARM=RPREFIX;              /* ADDR OF BUFFER                */ 01515000
         LR    RPARM,RPREFIX                                    02S1071 01516000
*       PRFLINK=''B;                                            02S1072 01517000
