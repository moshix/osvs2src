         LH    RPARM,PRFSRCE(,RPREFIX)                          03S0720 00308000
         N     RPARM,@CF03658                                   03S0720 00309000
*             CALL IEDQTNT;         /* CONVERT TO TTE ADDR           */ 00310000
         L     @15,AVTRNMPT(,RAVT)                              03S0721 00311000
