         TM    QCBREUS(@14),B'01110000'                         02S1114 01686000
         BNZ   @RF01114                                         02S1114 01687000
@RT01114 DS    0H                                               02S1115 01688000
*     DO;                           /* SEND MSG TO SYS CONSOLE       */ 01689000
*       RPARM=ADDR(IED149I);        /* ADDR OF WTO PARM LIST         */ 01690000
         LA    RPARM,IED149I                                    02S1116 01691000
*       SVC(35);                    /* WTO                           */ 01692000
         SVC   35                                               02S1117 01693000
*     END;                                                      02S1118 01694000
*   ELSE                                                        02S1119 01695000
*     DO;                           /* SEND MESSAGE TO REQUEST TERM  */ 01696000
         B     @RC01114                                         02S1119 01697000
@RF01114 DS    0H                                               02S1120 01698000
*                                                               03S1120 01699000
*       /*************************************************************/ 01700000
*       /*                                                           */ 01701000
*       /* REQMSG.REQMSGMV:THIS SEGMENT MOVES THE TOTE BUSY MESSAGE  */ 01702000
*       /* INTO THE BUFFER AND INSERTS ALL NECESSARY LINE CONTROL.   */ 01703000
*       /*                                                           */ 01704000
*       /*************************************************************/ 01705000
*                                                               03S1120 01706000
*       IF TRMPREF=ON THEN          /* IF THIS IS AN NCP DEVICE      */ 01707000
