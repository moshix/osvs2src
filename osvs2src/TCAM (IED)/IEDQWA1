*             MSGPTR=MSGPTR+LENGTH(STXCH);/* UPDATE POINTER          */ 01768000
         AL    MSGPTR,@CF00062                                  03S1142 01769000
*             MSGAREA(1:LENGTH(MSG149I))=MSG149I;/* MOVE IN MESSAGE  */ 01770000
         MVC   MSGAREA(17,MSGPTR),MSG149I                       03S1143 01771000
*             MSGPTR=MSGPTR+LENGTH(IED149I);/* UPDATE POINTER        */ 01772000
         AL    MSGPTR,@CF03652                                  03S1144 01773000
*             MSGAREA(1:LENGTH(ENDSYNC))=ENDSYNC;/* ADD BSC CHARS AT    01774000
*                                      END                           */ 01775000
         MVC   MSGAREA(3,MSGPTR),ENDSYNC                        03S1145 01776000
*             RMSGLEN=EPBSCL;       /* LENGTH OF MESSAGE             */ 01777000
         LH    RMSGLEN,EPBSCL                                   03S1146 01778000
*           END;                                                03S1147 01779000
*         ELSE                                                  03S1148 01780000
*           DO;                     /* START STOP TERMINAL           */ 01781000
