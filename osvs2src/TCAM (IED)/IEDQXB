         LA    R4,STCBIELL(,R4)         POINT TO NEXT ELEMENT           12570082
         CLI   STCBIC,C')'              END OF OPTION                   12600082
         BE    DECLENTH                 YES , PROC NEXT PARM            12630082
         BCT   R9,STCBTEST              BRANCH IF NOT LAST              12660082
         B     PARMERR                  NO , ERROR                      12690082
         TITLE ' BLCKINIT AND LINEINIT ROUTINES'                        12720082
         SPACE 3                                                        12750082
*        LINEINIT ROUTINE - PROCESS LINECNT= PARM                       12780082
         SPACE 3                                                        12810082
BLCKINIT EQU   *                                                        12840082
         USING TIMEPARM,R2              USING TIMEPARM R2               12870082
         LA    R6,7                     INDICATE LENGTH                 12900082
         BAL   R14,SCAN                 SCAN FOR VALID INPUT            12930082
         OI    EXECSW,EXECSWSS          IND SEARCH START TIME           12960082
         PACK  PACK4,TIME1HMD           PACK START TIME                 12990082
         MVC   TIMESHMD,PACK4           MOVE START HH-MM-DDD            13020082
         LTR   R8,R8                    END OF PARAMETERS               13050082
         BZ    PARMEND                  IF THIS IS LAST                 13080082
         LA    R1,TIMEXSZE(,R1)         ADJUST COUNT                    13110082
         CLI   TIMECONT,C'-'            ENDING TIME REQUIRED            13140082
         BNE   DECLENTH                 NO, CONTINUE PARM SCAN          13170082
         LA    R1,1(,R1)                ADJUST COUNT                    13200082
         LA    R6,7                     SET LOOP COUNT                  13230082
         BAL   R14,SCAN                 SCAN FOR VALID INPUT            13260082
         OI    EXECSW,EXECSWSE          IND SEARCH END TIME             13290082
         PACK  PACK4,TIME2HMD           PACK END TIME                   13320082
         MVC   TIMEEHMD,PACK4           MOVE END HH-MM-DDD              13350082
         LA    R1,TIMEXSZE(,R1)         ADJUST COUNT                    13380082
         B     DECLENTH                 CONTINUE                        13410082
         SPACE 3                                                        13440082
*        LINEINIT ROUTINE - PROCESS LINECNT= PARM '                     13470082
         SPACE 3                                                        13500082
LINEINIT EQU   *                                                        13530082
         LA    R6,2                     SET UP LOOP COUNT               13560082
         BAL   R14,SCAN                 SCAN FOR VALID DATA             13590082
         PACK  PACK8,8(2,R2)            PACK FOR CONVERSION             13620082
         CVB   R6,PACK8                 CONVERT TO BINARY               13650082
         STH   R6,LINECNT               SAVE NEW COUNT                  13680082
         LA    R1,2(,R1)                ADJUST COUNT                    13710082
         B     DECLENTH                 CONTINUE                        13740082
         TITLE 'IEDQXB FORMAT ROUTINE'                                  13770082
PARMEND  EQU   *                                                        13800082
         NI    PARMSW,PARMALL           RESET PARM PROC SWITCH          13830082
         BNZ   OPEN                     IF NO, OPEN DCB'S               13860082
PARMDEF  EQU   *                                                        13890082
