         TITLE '- IEDQXB - TCAM TRACE FORMAT PROGRAM - INITIALIZATION'  05070082
IEDQXB   CSECT                                                          05100082
         SPACE 3                                                        05130082
*        REGISTER ASSIGNMENTS                                           05160082
R0       EQU   0                        REG 0                           05190082
R1       EQU   1                        PARM REG                        05220082
R2       EQU   2                        PACKED HEADER RECORD            05250082
R3       EQU   3                        PACKED DATA RECORD              05280082
R4       EQU   4                        UNPACKED RECORD                 05310082
R5       EQU   5                        LENTH OF REMAINING REC          05340082
R6       EQU   6                        REG 6                           05370082
R7       EQU   7                        OUTPUT AREA POINTER             05400082
R8       EQU   8                        REG 8                           05430082
R9       EQU   9                        REG 9                           05460082
R10      EQU   10                       WORK REG 10                     05490082
R11      EQU   11                       BASE REG 1                      05520082
R12      EQU   12                       BASE REG 2                      05550082
R13      EQU   13                       BASE REG 3 AND SAVE             05580082
R14      EQU   14                       REG 14                          05610082
R15      EQU   15                       REG 15                          05640082
         SPACE 1                                                        05670082
ALL      EQU   15                       MASK OF 15                      05700082
DCBLRECL EQU   82                       DISP IN DCB OF LRECL            05730082
DCBBUFNO EQU   20                       BUFNO DISPL IN DCB              05760082
DCBBLKSZ EQU   62                       BLKSIZE DISPL IN DCB            05790082
DOUBLE   EQU   C'0'                     DOUBLE SPACE                    05820082
EIGHT    EQU   8                        EQU 8                           05850082
EJECT    EQU   C'1'                     EJECT                           05880082
FOUR     EQU   4                        EQU 4                           05910082
ONE      EQU   1                        EQU 1                           05940082
THREE    EQU   3                        EQU 3                           05970082
TEN      EQU   10                       EQU 10                          06000082
TRIPPLE  EQU   C'-'                     TRIPPLE SPACE                   06030082
         EJECT                                                          06060082
         USING IEDQXB,R15               USING BASE REG 15               06090082
         B     INIT                     BRANCH AROUND DC'S              06120082
IEDQXB   IEDHJN ,HJN                    IEDQXB HJN                      06150082
ADCONS   DC    A(IEDQXB),A(IEDQXB+4096),A(SAVEAREA)                     06180082
         PRINT NOGEN                                                    06210082
INIT     EQU   *                                                        06240082
         SAVE  (14,12)                  SAVE REGS 14-12                 06270082
         LR    R2,R13                   SAVE REG 13                     06300082
         LM    R11,R13,ADCONS           LOAD BASE REGS                  06330082
         USING IEDQXB,R11,R12,R13       USING BASE REGS 11-13           06360082
         DROP  R15                      DROP TEMPORARY BASE             06390082
         ST    R2,FOUR(,R13)            SAVE OLD SAVE AREA ADD          06420082
         ST    R13,EIGHT(,R2)           SAVE NEW SAVE AREA ADD          06450082
         L     R10,0(,R1)               GET PARAMETERS                  06480082
         OPEN  (SYSPRINT,(OUTPUT))      OPEN SYSPRINT                   06510082
         TM    SYSPRINT+48,X'10'        OPEN COMPLETE                   06540082
         BO    PARMCONT                 YES , CONTINUE                  06570082
         OI    EXECSW,EXECSWFP          IND PRINT OPEN FAILURE          06600082
         B     ERROPEN                  B ERROR OPEN                    06630082
PARMCONT EQU   *                                                        06660082
         LH    R5,0(,R10)               LOAD PARM LENGTH                06690082
         LTR   R5,R5                    ANY PARMS SPECIFIED             06720082
         BZ    PARMDEF                  NO , SET DEFAULT                06750082
         LA    R2,UNPKWORK              ADDRESS OF UNPKWORK             06780082
         LA    R3,2(,R10)               GET FIRST PARM                  06810082
         OI    PARMSW,PARMPROC          INDICATE PARM PROCESSING        06840082
         BCTR  R5,R0                    DECREMENT FOR EXECUTE           06870082
         EX    R5,SAVEPARM              MOVE PARM TO UNPKWORK           06900082
         LA    R5,ONE(,R5)              RESTORE LENGTH                  06930082
         USING PARMXXXX,R2              USING PARMCOMP R2               06960082
PARMSCAN EQU   *                                                        06990082
         LA    R6,PARMCONS              GET COMPARE CONSTANTS           07020082
         USING XXXXSEQE,R6              USING XXXXCOMP R6               07050082
         SR    R1,R1                    CLEAR WORK REG 1                07080082
         SR    R8,R8                    CLEAR WORK REG 8                07110082
         IC    R8,XXXXFLAG              GET FLAG BYTE                   07140082
PARMLOOP EQU   *                                                        07170082
         IC    R1,XXXXLENE              GET CHAR COUNT - 1              07200082
         EX    R1,PARMCHEK              IS THIS A COMPARE EQUAL         07230082
         BE    PARMINIT                 YES , INIT PARM                 07260082
         LA    R6,XXXXCONL(,R6)         BUMP TO NEXT ENTRY              07290082
         IC    R8,XXXXFLAG              GET FLAG BYTE                   07320082
         CLI   XXXXFLAG,AVTEFF          END OF LIST                     07350082
         BNE   PARMLOOP                 NO , CHECK NEXT PARM            07380082
         TM    PARMSW,PARMPROC          ARE WE CHECKING PARAMETERS      07410082
         BZR   R14                      NO , UNKNOWN RECORD             07440082
         B     PARMERR                  YES , NOTE ERROR EXIT           07470082
PARMINIT EQU   *                                                        07500082
         TM    PARMSW,PARMPROC          ARE WE CHECKING PARAMETERS      07530082
         BZR   R14                      IF NO, MUST BE CHECKING BUFFERS 07560082
         EX    R8,SETPARM               ELSE, SET APPROPRIATE PARM SW   07590082
         ICM   R15,ALL,XXXXINIT         INIT ROUTINE EXIST              07620082
         BZ    DECLENTH                 NO , CONTINUE                   07650082
         CLI   XXXXFLAG,AVTEZERO        XXXXFLAG EQ 0                   07680082
         BE    XXXXENTX                 YES , CONTINUE                  07710082
         CLC   XXXXPARM,PARMXXXX        XXXX=(                          07740082
         BNE   DECLENTH                 NO , CONTINUE                   07770082
         LA    R1,2(,R1)                ADJUST LENGTH                   07800082
XXXXENTX EQU   *                                                        07830082
         LR    R8,R5                    GET RESIDUAL COUNT              07860082
         SR    R8,R1                    SUBT LENGTH                     07890082
         BNP   PARMERR                  ERROR                           07920082
         BR    R15                      ENTER ROUTINE                   07950082
DECLENTH EQU   *                                                        07980082
         SR    R5,R1                    SUBT LENGTH OF PARM-1           08010082
         SH    R5,HALF1                 SUBT 1                          08040082
         BM    PARMERR                  ERROR                           08070082
         BZ    PARMEND                  END OF PARMS                    08100082
         LA    R2,1(R1,R2)              BUMP TO NEXT PARAMETER          08130082
         CLI   PARMXXXX,C','            A PROPER DELIMITER              08160082
         BNE   PARMERR                  IF NO, NOTE ERROR EXIT          08190082
         LA    R2,1(,R2)                BUMP PAST COMMA                 08220082
         SH    R5,HALF1                 EXCLUDE COMMA FROM LEN          08250082
         BP    PARMSCAN                 NO , GET NEXT PARM              08280082
PARMERR  EQU   *                                                        08310082
         LR    R8,R1                    SAVE LENGTH OF PARM             08340082
         LH    R3,0(,R10)               INIT PARM LENGTH                08370082
         LA    R2,2(,R10)               INIT PARM ADDRESS               08400082
         BCTR  R3,R0                    DECREMENT FOR EXECUTE           08430082
         EX    R3,MOVEPARM              MOVE PARM  TO OUTBUF            08460082
         LA    R3,1(,R3)                RESTORE LENGTH                  08490082
         LA    R7,OUTBUF+L'PARMDC(R3)   END OF PARM                     08520082
         MVI   0(R7),C''''              INDICATE END OF PARM            08550082
        $PRNT  PARMDC                   PRINT ENTIRE PARM               08580082
         SR    R3,R5                    FIND PARM ERROR DISP            08610082
         LA    R7,OUTBUF+L'PARMDC(R3)   INVALID PARM ADDRESS            08640082
         MVI   0(R7),C'*'               INDICATE FAILURE                08670082
         EX    R8,MOVEINDX              PROPAGATE *'S                   08700082
        $PRNT  ,                        PRINT IDENTIFIER                08730082
         LA    R2,IED123I               SET UP TO ISSUE WTO             08760082
         OI    EXECSW,EXECSWPE          INDICATE PARM ERROR             08790082
         B     WTO                      WRITE ERR MSG AND EXIT          08820082
         SPACE 3                                                        08850082
PARMCHEK CLC   PARMXXXX,XXXXNAME        COMPARE PARM NAME               08880082
MOVEPARM MVC   OUTBUF+6(1),PARMXXXX     MOVE PARM TO OUTBUF             08910082
MOVEINDX MVC   1(1,7),0(7)              PROPAGATE *                     08940082
SAVEPARM MVC   0(1,2),0(3)              MOVE PARM TO SAVEAREA           08970082
         TITLE '- BUFF - BUFFINIT ROUTINE - PROCESS BUFF=PARM'          09000082
BUFFINIT EQU   *                                                        09030082
         OI    BUFFSW,BUFFSWSE          BUFF SELECTIVE REQ              09060082
         LA    R3,BUFFX                 BUFF TABLE                      09090082
         USING BUFFCX,R3                USING BUFFCX R3                 09120082
         LA    R4,5(,R2)                GET FIRST ADDRESS               09150082
         USING BUFFIC,R4                USING BUFFIC R4                 09180082
         LA    R9,TEN                   SET FOR MAX ENTRIES             09210082
         B     BUFFFRST                                                 09240082
BUFFTEST EQU   *                                                        09270082
         CLI   BUFFIC,C','              NEW ADDRESS                     09300082
         BNE   BUFFTHRU                 NO , CHECK THRU ADDR            09330082
BUFFFRST EQU   *                                                        09360082
         OI    BUFFCX,BUFFCS            INDICATE SINGLE ENTRY           09390082
         B     BUFFCONT                 CONTINUE                        09420082
BUFFTHRU EQU   *                                                        09450082
         CLI   BUFFIC,C'-'              THRU ADDRESS                    09480082
         BNE   PARMERR                  NO , ERROR                      09510082
         OI    BUFFCX,BUFFCT            INDICATE THRU ADDR              09540082
BUFFCONT EQU   *                                                        09570082
         LA    R6,L'BUFFICIN-1          INDICATE LENGTH                 09600082
         BAL   R14,SCAN                 SCAN DATA                       09630082
         PACK  PACK3,BUFFICIN           PACK XXCIN ADDRESS              09660082
         MVC   BUFFCCIN,PACK3           MOVE TO NEXT ENTRY              09690082
         LA    R1,BUFFICXL(,R1)         ADD LENGTH OF XXCIN             09720082
         LA    R3,BUFFXL(,R3)           GET NEXT ENTRY                  09750082
         LA    R4,BUFFICXL(,R4)         GET NEXT XXCIN                  09780082
         CLI   BUFFIC,C')'              END OF BUFF= PARM               09810082
         BE    DECLENTH                 YES , CONTINUE                  09840082
         BCT   R9,BUFFTEST              BRANCH IF NOT LAST              09870082
         B     PARMERR                  NO , ERROR                      09900082
         TITLE '-IOTR - IOTRINIT ROUTINE - PROCESS IOTR=PARM'           09930082
IOTRINIT EQU   *                                                        09960082
         OI    IOTRSW,IOTRSWSE          IOTR SELECTIVE REQ              09990082
         LA    R3,IOTRX                 IOTR TABLE                      10020082
         USING IOTRCX,R3                USING IOTRCX R3                 10050082
         LA    R4,5(,R2)                GET FIRST ADDRESS               10080082
         USING IOTRIC,R4                USING IOTRIC R4                 10110082
         LA    R9,TEN                   SET FOR MAXIMUM ENTRIES         10140082
         B     IOTRFRST                                                 10170082
IOTRTEST EQU   *                                                        10200082
         CLI   IOTRIC,C','              NEW ADDRESS                     10230082
         BNE   IOTRTHRU                 NO , CHECK THRU ADDR            10260082
IOTRFRST EQU   *                                                        10290082
         OI    IOTRCX,IOTRCS            INDICATE SINGLE ENTRY           10320082
         B     IOTRCONT                 CONTINUE                        10350082
IOTRTHRU EQU   *                                                        10380082
         CLI   IOTRIC,C'-'              THRU ADDRESS                    10410082
         BNE   PARMERR                  NO , ERROR                      10440082
         OI    IOTRCX,IOTRCT            INDICATE THRU ADDR              10470082
IOTRCONT EQU   *                                                        10500082
         LA    R6,L'IOTRICUU-1          INDICATE LENGTH                 10530082
         BAL   R14,SCAN                 SCAN DATA                       10560082
         PACK  PACK3,IOTRICUU           PACK CUU ADDRESS                10590082
         MVC   IOTRCCUU,PACK3           MOVE TO NEXT ENTRY              10620082
         LA    R1,IOTRICUL(,R1)         ADD LENGTH OF 0CUU              10650082
         LA    R3,IOTRXL(,R3)           POINT TO NEXT ENTRY             10680082
         LA    R4,IOTRICUL(,R4)         POINT TO NEXT 0CUU              10710082
         CLI   IOTRIC,C')'              END OF OPTION                   10740082
         BE    DECLENTH                 YES , PROC NEXT PARM            10770082
         BCT   R9,IOTRTEST              BRANCH IF NOT LAST              10800082
         B     PARMERR                  NO , ERROR                      10830082
         TITLE '- PIUT - PIUTINIT ROUTINE - PROCESS PIUT=PARM'          10860082
PIUTINIT EQU   *                                                        10890082
         OI    PIUTSW,PIUTSWSE          PIUT SELECTIVE REQ              10920082
         LA    R3,PIUTX                 PIUT TABLE                      10950082
         USING PIUTCX,R3                USING PIUTCX R3                 10980082
         LA    R4,5(,R2)                GET FIRST ADDRESS               11010082
         USING PIUTIC,R4                USING PIUTIC R4                 11040082
         LA    R9,TEN                   SET FOR MAXIMUM ENTRIES         11070082
         B     PIUTFRST                                                 11100082
PIUTTEST EQU   *                                                        11130082
         CLI   PIUTIC,C','              NEW ADDRESS                     11160082
         BNE   PIUTTHRU                 NO , CHECK THRU ADDR            11190082
PIUTFRST EQU   *                                                        11220082
         OI    PIUTCX,PIUTCS            INDICATE SINGLE ENTRY           11250082
         B     PIUTCONT                 CONTINUE                        11280082
PIUTTHRU EQU   *                                                        11310082
         CLI   PIUTIC,C'-'              THRU ADDRESS                    11340082
         BNE   PARMERR                  NO , ERROR                      11370082
         OI    PIUTCX,PIUTCT            INDICATE THRU ADDR              11400082
PIUTCONT EQU   *                                                        11430082
         LA    R6,L'PIUTIRSI-1          INDICATE LENGTH                 11460082
         BAL   R14,SCAN                 SCAN DATA                       11490082
         PACK  PACK3,PIUTIRSI           PACK RESOURCE ID                11520082
         MVC   PIUTCRSI,PACK3           MOVE TO NEXT ENTRY              11550082
         LA    R1,PIUTIRSL(,R1)                                         11580082
         LA    R3,PIUTXL(,R3)                                           11610082
         LA    R4,PIUTIRSL(,R4)                                         11640082
         CLI   PIUTIC,C')'              END OF OPTION                   11670082
         BE    DECLENTH                 YES , PROC NEXT PARM            11700082
         BCT   R9,PIUTTEST              BRANCH IF NOT LAST              11730082
         B     PARMERR                  NO , ERROR                      11760082
         TITLE '- STCB - STCBINIT ROUTINE - PROCESS STCB=PARM'          11790082
STCBINIT EQU   *                                                        11820082
         OI    STCBSW,STCBSWSE          STCB SELECTIVE REQ              11850082
         LA    R3,STCBX                 STCB TABLE                      11880082
         USING STCBCX,R3                USING STCBCX R3                 11910082
         LA    R4,5(,R2)                GET FIRST ADDRESS               11940082
         USING STCBIC,R4                USING STCBIC R4                 11970082
         LA    R9,TEN                   SET FOR MAX ENTRIES             12000082
         B     STCBFRST                                                 12030082
STCBTEST EQU   *                                                        12060082
         CLI   STCBIC,C','              NEW ADDRESS                     12090082
         BNE   STCBTHRU                 NO , CHECK THRU ADDR            12120082
STCBFRST EQU   *                                                        12150082
         OI    STCBCX,STCBCS            INDICATE SINGLE ENTRY           12180082
         B     STCBCONT                 CONTINUE                        12210082
STCBTHRU EQU   *                                                        12240082
         CLI   STCBIC,C'-'              THRU ADDRESS                    12270082
         BNE   PARMERR                  NO , ERROR                      12300082
         OI    STCBCX,STCBCT            INDICATE THRU ADDR              12330082
STCBCONT EQU   *                                                        12360082
         LA    R6,L'STCBIELE-1          INDICATE LENGTH                 12390082
         BAL   R14,SCAN                 SCAN DATA                       12420082
