*                                   BDUMODIF=CD0WT;/* REPLACE   04S0649 00529000
*                                      MODIFIER WITH EOT             */ 00530000
         MVI   BDUMODIF(BDUPTR),X'02'                           04S0649 00531000
*                                   DCTADDR=AVTCSTCS;/* NOW GO TO       00532000
*                                      DEVICE CHAR TABLE TO SEE IF      00533000
*                                      THIS IS A 3270                */ 00534000
         L     DCTADDR,AVTCSTCS(,IEDQAPTR)                      04S0650 00535000
*                                   DCTADDR=DCTADDR+(AVTDCTLN*(TRMCHCIN 00536000
*                                       -1));                   04S0651 00537000
