*                                      ATTN READ REQ?                */ 00346000
         TM    LCBSATRD(RLCB),B'00100000'                       04S0596 00347000
         BNO   @RF00596                                         04S0596 00348000
*                                                               05S0597 00349000
*                                 /***********************************/ 00350000
*                                 /*                                 */ 00351000
*                                 /* PROCESS.WRITE.MSGGEN.ERBAYM:    */ 00352000
*                                 /* POST ERB TO AYM                 */ 00353000
*                                 /*                                 */ 00354000
*                                 /***********************************/ 00355000
*                                                               05S0597 00356000
*                                 DO;                           05S0597 00357000
*                                   LCBERBPY=PRILNEND;/* PRIORITY       00358000
*                                      'E4'                          */ 00359000
         MVI   LCBERBPY(RLCB),X'E4'                             05S0598 00360000
*                                   LCBERBQB=AVTTSOPT->TSIMSGEN;/*      00361000
