         TITLE 'IEDSAC: WRITE, CONTACT, DISCONNECT AND RESET COMMAND PR*00001000
               ROCESSOR '                                               00002000
IEDSAC   CSECT ,                                                01S0001 00003000
@PROLOG  DS    0H                                               01S0002 00004000
         DC    A(0)        QCB                                          00005000
         DC    A(0)                                                     00006000
         DC    X'06'       STCB                                         00007000
         DC    AL3(*-1)                                                 00008000
         USING *,RBASE                                                  00009000
         LR    RBASE,RLINK                                              00010000
IEDSAC IEDHJN START,HJN                                                 00011000
*   RPRF=RPARM;                     /* ESTABLISH PRF ADDRESSABILITY  */ 00012000
         LR    RPRF,RPARM                                       02S0517 00013000
*   RPARM=TTHOAF;                   /* CALL TNT CODE TO              */ 00014000
         LA    @10,PRFPIUO(,RPRF)                               02S0518 00015000
         SLR   @07,@07                                          02S0518 00016000
         IC    @07,PRFPIUO(,RPRF)                               02S0518 00017000
         ALR   @10,@07                                          02S0518 00018000
         SLR   RPARM,RPARM                                      02S0518 00019000
         ICM   RPARM,3,TTHOAF(@10)                              02S0518 00020000
*   CALL IEDQTNT;                   /* DEVELOPE THE                  */ 00021000
         L     @15,AVTRNMPT(,IEDQAPTR)                          02S0519 00022000
         BALR  @14,@15                                          02S0519 00023000
*   RTRM=RPARM-LENGTH(IEDNTRM);     /* TRM ADDRESS           @YM04622*/ 00024000
         LR    RTRM,RPARM                                       02S0520 00025000
         SL    RTRM,@CF01995                                    02S0520 00026000
*   IEDQQPTR=TRMDESTQ;              /* ESTABLISH QCB ADDRESSABILITY  */ 00027000
         SLR   IEDQQPTR,IEDQQPTR                                02S0521 00028000
         ICM   IEDQQPTR,7,TRMDESTQ(RTRM)                        02S0521 00029000
*   IF QCBPLCBN=ON THEN             /* THERE WILL BE A PLCB FOR ALL     00030000
*                                      RESPONSES EXCEPT TSO     02S0522 00031000
*                                      UNSOLICITED INPUT             */ 00032000
         LR    @10,IEDQQPTR                                     02S0522 00033000
         SL    @10,@CF00044                                     02S0522 00034000
         TM    QCBPLCBN(@10),B'10000000'                        02S0522 00035000
         BNO   @RF00522                                         02S0522 00036000
*     DO;                                                       02S0523 00037000
*       RLCB=QCBPLCBA;              /* ESTABLISH PLCB ADDRESSABILITY */ 00038000
         L     RLCB,QCBPLCBA-1(,@10)                            02S0524 00039000
         LA    RLCB,0(,RLCB)                                    02S0524 00040000
*       IEDQSPTR=LCBSCBA;           /* ESTABLISH SCB ADDRESSABILITY  */ 00041000
         L     @10,LCBSCBA-1(,RLCB)                             02S0525 00042000
         LA    @10,0(,@10)                                      02S0525 00043000
         ST    @10,IEDQSPTR                                     02S0525 00044000
*     END;                                                      02S0526 00045000
*   BDUPTR=ADDR(PIU0RU);            /* ESTABLISH BDU ADDRESSABILITY  */ 00046000
@RF00522 LA    BDUPTR,PRFPIUO(,RPRF)                            02S0527 00047000
         SLR   @10,@10                                          02S0527 00048000
         IC    @10,PRFPIUO(,RPRF)                               02S0527 00049000
         ALR   BDUPTR,@10                                       02S0527 00050000
         LA    BDUPTR,PIU0RU(,BDUPTR)                           02S0527 00051000
*                                                               02S0528 00052000
*   /*****************************************************************/ 00053000
*   /*                                                               */ 00054000
*   /* PROCESS: THIS SEGMENT CALLS SUBROUTINES WHICH PERFORM THE     */ 00055000
*   /* FUNCTION OF THE MODULE.                                       */ 00056000
*   /*                                                               */ 00057000
*   /*****************************************************************/ 00058000
*                                                               02S0528 00059000
*   IF BDUCMND=CD0WRT THEN          /* CALL WRITE PROCESSOR IF THIS     00060000
*                                      IS A WRITE                    */ 00061000
         CLI   BDUCMND(BDUPTR),X'02'                            02S0528 00062000
         BNE   @RF00528                                         02S0528 00063000
*                                                               03S0529 00064000
*     /***************************************************************/ 00065000
*     /*                                                             */ 00066000
*     /* PROCESS.WRITE: THIS SEGMENT DOES THE WRITE COMMAND          */ 00067000
*     /* PROCESSING. INTERNAL PROCEDURES ARE CALLED TO PROCESS THESE */ 00068000
*     /* CONDITIONS: (1)WRITE TERMINATION. (2)SESSION INITIATION AND */ 00069000
*     /* TERMINATION, (3)RESET IMMEDIATE, (4)MSGGEN, (5)NULL WRITE,  */ 00070000
*     /* (6)ERROR WRITE, (7)NONERROR WRITE.                          */ 00071000
*     /*                                                             */ 00072000
*     /***************************************************************/ 00073000
*                                                               03S0529 00074000
*     DO;                                                       03S0529 00075000
*       LCBRESP=OFF;                /* ANY WRITE SATISFIES RESPONSE     00076000
*                                      OWED REQUIREMENT              */ 00077000
         NI    LCBRESP(RLCB),B'11111110'                        03S0530 00078000
*       RFY                                                     03S0531 00079000
*         RZERO RSTD;               /*                       @YM05678*/ 00080000
*       TRMPREP=OFF;                /* INDICATE THAT MONITOR IS NO      00081000
*                                      LONGER ACTIVE IN THE NCP.        00082000
*                                                            @YM05638*/ 00083000
*       TRMWRBRK=OFF;               /* INDICATE THAT A WRITE BREAK IS   00084000
*                                      NOT IN PROGRESS       @YM08156*/ 00085000
         LA    @15,TRMWRBRK(,RTRM)                              03S0533 00086000
         LR    @14,@15                                          03S0533 00087000
         LA    @10,3                                            03S0533 00088000
         NR    @14,@10                                          03S0533 00089000
         LCR   @14,@14                                          03S0533 00090000
         ALR   @15,@14                                          03S0533 00091000
         L     @10,0(,@15)                                      03S0533 00092000
@GL00001 LR    @05,@10                                          03S0533 00093000
         N     @05,@CB02345+3(@14)                              03S0533 00094000
         CS    @10,@05,0(@15)                                   03S0533 00095000
         BNE   @GL00001                                         03S0533 00096000
*       RFY                                                     03S0534 00097000
*         RZERO UNRSTD;             /*                       @YM05678*/ 00098000
*       IF TTHTAG2=TTHABORT&        /* IF NULL WRITE TO ABORT   03S0535 00099000
*                                                            @YM08030*/ 00100000
*           TTHTAG1=0 THEN          /* INPUT OPERATION THEN  @YM08038*/ 00101000
         LA    @10,PRFPIUO(,RPRF)                               03S0535 00102000
         SLR   @05,@05                                          03S0535 00103000
         IC    @05,PRFPIUO(,RPRF)                               03S0535 00104000
         ALR   @10,@05                                          03S0535 00105000
         CLI   TTHTAG2(@10),X'04'                               03S0535 00106000
         BNE   @RF00535                                         03S0535 00107000
         CLI   TTHTAG1(@10),0                                   03S0535 00108000
         BNE   @RF00535                                         03S0535 00109000
*         DO;                       /*                       @YM08038*/ 00110000
*           LCBUSUE=ON;             /* INDICATE END OF RCV OPERATION    00111000
*                                                            @YM08038*/ 00112000
         OI    LCBUSUE(RLCB),B'00000001'                        03S0537 00113000
*           CALL OUTDHPST;          /* RETURN RESPONSE TO IEDNKA        00114000
*                                                            @YM08038*/ 00115000
         BAL   @14,OUTDHPST                                     03S0538 00116000
*         END;                      /*                       @YM08038*/ 00117000
*       ELSE                        /*                       @YM08038*/ 00118000
*         DO;                       /*                       @YM08038*/ 00119000
         B     @RC00535                                         03S0540 00120000
@RF00535 DS    0H                                               03S0541 00121000
*           IF LCBABRTN=ON THEN     /* IF WRITE IS TO BE TERMINTAED  */ 00122000
         TM    LCBABRTN(RLCB),B'00000001'                       03S0541 00123000
         BNO   @RF00541                                         03S0541 00124000
*                                                               04S0542 00125000
*             /*******************************************************/ 00126000
*             /*                                                     */ 00127000
*             /* PROCESS.WRITE.ABORT: WHEN AN ERROR WRITE RESPONSES  */ 00128000
*             /* IS RECOGNIZED, LCBABRTN IS TURNED ON. A RESET       */ 00129000
*             /* IMMEDIATE IS BUILT IN THE FIRST ERROR RESPONSE      */ 00130000
*             /* BUFFER AND POSTED TO THE NCP (IEDPCOUT). THIS RESET */ 00131000
*             /* IMMEDIATE TURNS OFF THE NCP'S ERROR LOCK AND ON     */ 00132000
*             /* RETURN IS POSTED TO THE MH AS AN ERROR BUFFER. TO   */ 00133000
*             /* PREVENT A DOUBLE POST TO MH, THIS SUBROUTINE FREES  */ 00134000
*             /* ALL WRITE RESPONSE BUFFERS. ABORT PROCESSING IS NOT */ 00135000
*             /* DONE FOR MSGGEN BUFFERS. LCBABRTN IS ALSO TESTED IN */ 00136000
*             /* DEBLOCK OUTBOUND (IEDSAO) WRTIE PROCESSING. IF FOUND*/ 00137000
*             /* ON, WRITE BUFFERS ARE FREED                         */ 00138000
*             /*                                                     */ 00139000
*             /*******************************************************/ 00140000
*                                                               04S0542 00141000
*             DO;                                               04S0542 00142000
*               IF LCBMSGNN=ON THEN /* THIS IS A MSGGEN RESPONSE     */ 00143000
         TM    LCBMSGNN(RLCB),B'01000000'                       04S0543 00144000
         BNO   @RF00543                                         04S0543 00145000
*                 DO;                                           04S0544 00146000
*                   LCBMSGNN=OFF;   /* RESET MSGGEN FLAG             */ 00147000
         NI    LCBMSGNN(RLCB),B'10111111'                       04S0545 00148000
*                   CALL ERBBD;     /* POST ERB TO DRIVE IN/OUT MSG     00149000
*                                      PROCESSING                    */ 00150000
         BAL   @14,ERBBD                                        04S0546 00151000
*                 END;                                          04S0547 00152000
*               CALL FREEBFR;       /* FREE BUFFER                   */ 00153000
@RF00543 BAL   @14,FREEBFR                                      04S0548 00154000
*             END;                                              04S0549 00155000
*           ELSE                    /* FOR ALL ELSE DO               */ 00156000
*             DO;                                               03S0550 00157000
         B     @RC00541                                         03S0550 00158000
@RF00541 DS    0H                                               03S0551 00159000
*               IF BDUMODIF=>CD0WD THEN/* IF COMMAND CAN MODIFY SESSN*/ 00160000
         CLI   BDUMODIF(BDUPTR),X'03'                           03S0551 00161000
         BL    @RF00551                                         03S0551 00162000
*                                                               04S0552 00163000
*                 /***************************************************/ 00164000
*                 /*                                                 */ 00165000
*                 /* PROCESS.WRITE.SESSION: IF A WRITE COMMAND IS    */ 00166000
*                 /* CAPABLE OF MODIFING SESSION, THE RESPONSE IS    */ 00167000
*                 /* EXAMINED FOR NORMAL OR ERROR AND PHASE. THE     */ 00168000
*                 /* SESSION FLAG IS THEN SET AS REQUIRED.           */ 00169000
*                 /*                                                 */ 00170000
*                 /***************************************************/ 00171000
*                                                               04S0552 00172000
*                 DO;                                           04S0552 00173000
*                   IF BDUPHASE=RSP60 THEN/* IF PHASE IS 3           */ 00174000
         TM    BDUPHASE(BDUPTR),B'01100000'                     04S0553 00175000
         BNO   @RF00553                                         04S0553 00176000
*                     DO;                                       04S0554 00177000
*                       IF(BDUMODIF=CD0WD|/* IF MODIFIER IS .........*/ 00178000
*                           BDUMODIF=CD0WCD) THEN/* .....A DISCONNECT*/ 00179000
         CLI   BDUMODIF(BDUPTR),X'03'                           04S0555 00180000
         BE    @RT00555                                         04S0555 00181000
         CLI   BDUMODIF(BDUPTR),X'0B'                           04S0555 00182000
         BNE   @RF00555                                         04S0555 00183000
@RT00555 DS    0H                                               04S0556 00184000
*                         TRMSESSN=OFF;/* THEN END SESSION           */ 00185000
         NI    TRMSESSN(RTRM),B'10111111'                       04S0556 00186000
*                       ELSE                                    04S0557 00187000
*                         IF BDUMODIF=>CD0WC THEN/* IF          04S0557 00188000
*                                      MODIFIER=CONTACT              */ 00189000
         B     @RC00555                                         04S0557 00190000
@RF00555 CLI   BDUMODIF(BDUPTR),X'08'                           04S0557 00191000
         BL    @RF00557                                         04S0557 00192000
*                           TRMSESSN=ON;/* INITIATE SESSION          */ 00193000
         OI    TRMSESSN(RTRM),B'01000000'                       04S0558 00194000
*                     END;                                      04S0559 00195000
*                   ELSE            /* IT IS NOT PHASE 3             */ 00196000
*                     DO;           /* IT IS PHASE 1 OR 2    @YM08112*/ 00197000
         B     @RC00553                                         04S0560 00198000
@RF00553 DS    0H                                               04S0561 00199000
*                       IF(BDUMODIF=>CD0WC&BDUMODIF=<CD0WCD) THEN/*     00200000
*                                      IF CONTACT                    */ 00201000
         CLI   BDUMODIF(BDUPTR),X'08'                           04S0561 00202000
         BL    @RF00561                                         04S0561 00203000
         CLI   BDUMODIF(BDUPTR),X'0B'                           04S0561 00204000
         BH    @RF00561                                         04S0561 00205000
*                         IF(BDURESP^=RSPE09&/* AND NOT SESSN^STARTED*/ 00206000
*                             BDURESP^=RSPE1E) THEN/* AND ^CMD  04S0562 00207000
*                                      REJ/RESET                     */ 00208000
         TM    BDURESP(BDUPTR),B'00001001'                      04S0562 00209000
         BNO   @GL00005                                         04S0562 00210000
         TM    BDURESP(BDUPTR),B'00010110'                      04S0562 00211000
         BZ    @RF00562                                         04S0562 00212000
@GL00005 TM    BDURESP(BDUPTR),B'00011110'                      04S0562 00213000
         BNO   @RT00562                                         04S0562 00214000
         TM    BDURESP(BDUPTR),B'00000001'                      04S0562 00215000
         BZ    @RF00562                                         04S0562 00216000
@RT00562 DS    0H                                               04S0563 00217000
*                           TRMSESSN=ON;/* INDICATE SESSION          */ 00218000
         OI    TRMSESSN(RTRM),B'01000000'                       04S0563 00219000
*                     END;                                      04S0564 00220000
@RF00562 DS    0H                                               04S0564 00221000
@RF00561 DS    0H                                               04S0565 00222000
*                 END;                                          04S0565 00223000
@RC00553 DS    0H                                               04S0566 00224000
*               IF TRMASNXI=ON THEN /* IF RESET IMMEDIATE ISSUED     */ 00225000
@RF00551 TM    TRMASNXI(RTRM),B'00010000'                       03S0566 00226000
         BNO   @RF00566                                         03S0566 00227000
*                                                               04S0567 00228000
*                 /***************************************************/ 00229000
*                 /*                                                 */ 00230000
*                 /* PROCESS.WRITE.RESETIMM: PROCESS STOPLINE,I      */ 00231000
*                 /* DURING WRITE COMMAND. IF THIS IS MSGGEN, TURN   */ 00232000
*                 /* OFF MSGGEN INDICATOR, FREE BUFFER AND POST ERB  */ 00233000
*                 /* TO BD. IF THIS IS A NULL WRITE TX, TURN OFF NULL*/ 00234000
*                 /* WRITE INDICATOR, FREE BUFFER AND POST PLCB TO   */ 00235000
*                 /* BD. IF THIS IS A NULL WRITE BLOCK, POST BUFFER  */ 00236000
*                 /* TO OUTDH. (OUTDH WILL TEST FOR STOP LINE IN     */ 00237000
*                 /* PROGRESS BEFORE ATTEMPTING TO CONTINUE THE      */ 00238000
*                 /* SEND). IF THIS IS WRITE DATA, INDICATE WRITE    */ 00239000
*                 /* ABORT, DO WRITE STATUS ANALYSIS, TRANSFORM THIS */ 00240000
*                 /* RESPONSE TO A TAGGED RESET IMMEDIATE AND POST TO*/ 00241000
*                 /* PATH CONTROL OUTBOUND. THE PURPOSE OF THIS      */ 00242000
*                 /* TAGGED RESET IS TO RETURN ANY OUTSTANDING       */ 00243000
*                 /* COMMANDS. THE TAGGED RESET RESPONSE WILL BE     */ 00244000
*                 /* ERROR POSTED TO THE MH.                         */ 00245000
*                 /*                                                 */ 00246000
*                 /***************************************************/ 00247000
*                                                               04S0567 00248000
*                 DO;                                           04S0567 00249000
*                   IF LCBMSGNN=ON THEN/* IF THIS IS A MSGGEN        */ 00250000
         TM    LCBMSGNN(RLCB),B'01000000'                       04S0568 00251000
         BNO   @RF00568                                         04S0568 00252000
*                     DO;                                       04S0569 00253000
*                       LCBMSGNN=OFF;                           04S0570 00254000
         NI    LCBMSGNN(RLCB),B'10111111'                       04S0570 00255000
*                       CALL FREEBFR;                           04S0571 00256000
         BAL   @14,FREEBFR                                      04S0571 00257000
*                       CALL ERBBD; /* POST ERB TO BD                */ 00258000
         BAL   @14,ERBBD                                        04S0572 00259000
*                     END;                                      04S0573 00260000
*                   ELSE                                        04S0574 00261000
*                     IF TRMNLOUT=OFF THEN/* IF IT IS A DATA WRITE   */ 00262000
         B     @RC00568                                         04S0574 00263000
@RF00568 TM    TRMNLOUT(RTRM),B'00000100'                       04S0574 00264000
         BNZ   @RF00574                                         04S0574 00265000
*                       CALL ERRWRITE;/* THIS SUBROUTINE WILL ABORT     00266000
*                                      THE WRITE, SET SCB AND LCB       00267000
*                                      ERROR BITS AND PRESERVE STATUS   00268000
*                                      OF THE ERROR FOR THE TCAM        00269000
*                                      CONSOLE ERROR MESSAGE         */ 00270000
         BAL   @14,ERRWRITE                                     04S0575 00271000
*                     ELSE                                      04S0576 00272000
*                       IF BDUMODIF=CD0WT THEN/* IF THIS IS A NULL WR   00273000
*                                      EOT                           */ 00274000
         B     @RC00574                                         04S0576 00275000
@RF00574 CLI   BDUMODIF(BDUPTR),X'02'                           04S0576 00276000
         BNE   @RF00576                                         04S0576 00277000
*                         DO;                                   04S0577 00278000
*                           TRMNLOUT=OFF;/* TURN OFF NULL OUT FLAG   */ 00279000
         NI    TRMNLOUT(RTRM),B'11111011'                       04S0578 00280000
*                           CALL FREEBFR;/* FREE PIU BFR     @YM07714*/ 00281000
         BAL   @14,FREEBFR                                      04S0579 00282000
*                           CALL PLCBPOST;/* POST PLCB TO BD         */ 00283000
         BAL   @14,PLCBPOST                                     04S0580 00284000
*                         END;                                  04S0581 00285000
*                       ELSE        /* ELSE, IT IS A NULL WR BLK     */ 00286000
*                         CALL OUTDHPST;/* WHICH SHOULD BE RETURNED     00287000
*                                      TO OUTDH WHERE THIS WRITE WILL   00288000
*                                      BE TERMINATED                 */ 00289000
         B     @RC00576                                         04S0582 00290000
@RF00576 BAL   @14,OUTDHPST                                     04S0582 00291000
*                 END;                                          04S0583 00292000
*               ELSE                                            03S0584 00293000
*                 IF LCBMSGNN=ON THEN/* IF MSGGEN IN PROGRESS        */ 00294000
         B     @RC00566                                         03S0584 00295000
@RF00566 TM    LCBMSGNN(RLCB),B'01000000'                       03S0584 00296000
         BNO   @RF00584                                         03S0584 00297000
*                                                               04S0585 00298000
*                   /*************************************************/ 00299000
*                   /*                                               */ 00300000
*                   /* PROCESS.WRITE.MSGGEN: IF INPUT IS MARKED BSC  */ 00301000
*                   /* STATUS PENDING, THEN SET STATUS PENDING FLAG, */ 00302000
*                   /* FREE THE BUFFER AND EXIT TO DISPATCHER. ON THE*/ 00303000
*                   /* NEXT INPUT FROM SAME SOURCE, IEDSAI WILL POST */ 00304000
*                   /* ERB TO BD TO DRIVE OUTMSG. THIS PROCESS       */ 00305000
*                   /* PREVENTS A RESET IMMEDIATE FROM RESETTING THE */ 00306000
*                   /* PENDING STATUS. IN NORMAL AND ERROR CASES     */ 00307000
*                   /* OTHER THAN BSC STATUS PENDING AND TSO SIM ATTN*/ 00308000
*                   /* RD, THE ERB IS POSTED TO IEDQBD. IF INPUT IS  */ 00309000
*                   /* TSO SIM ATTN RD, THE BUFFER IS FREED AND THE  */ 00310000
*                   /* THE ERB IS POSTED TO IEDQBD                   */ 00311000
*                   /*                                               */ 00312000
*                   /*************************************************/ 00313000
*                                                               04S0585 00314000
*                   DO;                                         04S0585 00315000
*                     IF BDUSYRSP=RSPSTPND THEN/* IF BSC STATUS 04S0586 00316000
*                                      PENDING                       */ 00317000
         CLI   BDUSYRSP(BDUPTR),X'A5'                           04S0586 00318000
         BNE   @RF00586                                         04S0586 00319000
*                       DO;                                     04S0587 00320000
*                         TRMSTPND=ON;/* INDICATE STAT PENDING       */ 00321000
         OI    TRMSTPND(RTRM),B'10000000'                       04S0588 00322000
*                         CALL FREEBFR;                         04S0589 00323000
         BAL   @14,FREEBFR                                      04S0589 00324000
*                       END;                                    04S0590 00325000
*                     ELSE                                      04S0591 00326000
*                       DO;         /*                       @OY20320*/ 00327000
         B     @RC00586                                         04S0591 00328000
@RF00586 DS    0H                                               04S0592 00329000
*                         LCBMSGNN=OFF;/* MSGGEN NOT OUT     @OZ27777*/ 00330000
         NI    LCBMSGNN(RLCB),B'10111111'                       04S0592 00331000
*                         IF QCBTSSES=ON THEN/* FOR TSO SESSIONS        00332000
*                                                            @YM08420*/ 00333000
         TM    QCBTSSES(IEDQQPTR),B'10000000'                   04S0593 00334000
         BNO   @RF00593                                         04S0593 00335000
*                           IF^(QCBSIMRD=ON&/* IF NOT TSO SIM ATTN RD   00336000
*                                      RESP IN ERR                   */ 00337000
*                               BDUERROR=ON) THEN               04S0594 00338000
         TM    QCBSIMRD(IEDQQPTR),B'00001000'                   04S0594 00339000
         BNO   @RT00594                                         04S0594 00340000
