*                                      WRITE EOT TO TERMINAL TO 04S0657 00561000
*                                      UNLOCK ITS KEYBOARD           */ 00562000
@RF00652 BAL   @14,PCOUT                                        04S0657 00563000
*                                 END;                          04S0658 00564000
*                               ELSE/* THERE IS NO ERROR AND NOTHING */ 00565000
*                                 CALL OUTDHPST;/* ON WAIT CHAIN, SO    00566000
*                                      POST TO                       */ 00567000
         B     @RC00643                                         04S0659 00568000
@RF00643 BAL   @14,OUTDHPST                                     04S0659 00569000
*                           END;    /* OUTDH                         */ 00570000
*                     END;                                      04S0661 00571000
*                   ELSE                                        03S0662 00572000
*                     IF BDUERROR=ON THEN/* ERROR ON WRITE           */ 00573000
         B     @RC00613                                         03S0662 00574000
@RF00613 TM    BDUERROR(BDUPTR),B'10000000'                     03S0662 00575000
         BNO   @RF00662                                         03S0662 00576000
*                       CALL ERRWRITE;                          03S0663 00577000
         BAL   @14,ERRWRITE                                     03S0663 00578000
*                     ELSE                                      03S0664 00579000
*                                                               04S0664 00580000
*                       /*********************************************/ 00581000
*                       /*                                           */ 00582000
*                       /* PROCESS.WRITE.NORMWRIT: THIS SEGMENT DOES */ 00583000
*                       /* THE "NORMAL WRITE" PROCESSING. IF EOM,    */ 00584000
*                       /* BUFFER IS POSTED TO IEDQBD. IF NOT END    */ 00585000
*                       /* MESSAGE, ERB IS POSTED IF REQUIRED. ELSE, */ 00586000
*                       /* THE BFR IS FREED.                         */ 00587000
*                       /*                                           */ 00588000
*                       /*********************************************/ 00589000
*                                                               04S0664 00590000
*                       DO;                                     04S0664 00591000
