         MH    @09,AVTDCTLN(,IEDQAPTR)                          04S0651 00541000
         ALR   DCTADDR,@09                                      04S0651 00542000
*                                   IF DCT3270=ON THEN/* IF THIS IS A   00543000
*                                      3270                          */ 00544000
         TM    DCT3270(DCTADDR),B'00000100'                     04S0652 00545000
         BNO   @RF00652                                         04S0652 00546000
*                                    DO;                        04S0653 00547000
*                                     CTRLCHAR=CTLCHAR;/* INSERT        00548000
*                                      DEVICE CNTRL CHARS            */ 00549000
         LA    @09,PIU0RU(,@10)                                 04S0654 00550000
         MVC   CTRLCHAR+6(3,@09),@CB02274                       04S0654 00551000
*                                     TTHDCF=TTHDCF+LENGTH(DATAFLD);    00552000
         LA    @09,3                                            04S0655 00553000
         SLR   @07,@07                                          04S0655 00554000
         ICM   @07,3,TTHDCF(@10)                                04S0655 00555000
         ALR   @09,@07                                          04S0655 00556000
         STCM  @09,3,TTHDCF(@10)                                04S0655 00557000
*                                     /* AND ADD 3 TO DCF            */ 00558000
*                                    END;                       04S0656 00559000
