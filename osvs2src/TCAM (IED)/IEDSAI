@EF00013 DS    0H                                                  0831 01119000
@ER00013 L     @14,@SA00013                                        0831 01120000
         BR    @14                                                 0831 01121000
*                                                                  0832 01122000
*   /*****************************************************************/ 01123000
*   /*                                                               */ 01124000
*   /* WAITCHN: IF THE PLCB IS ASSIGNED, CURRENT INPUT IS PLACED ON  */ 01125000
*   /* PLCB'S WAIT CHAIN. ELSE, CURRENT INPUT IS PLACED ON QCB'S WAIT*/ 01126000
*   /* CHAIN AND A PLCB IS REQUESTED. IF THE PLCB REQUEST IS         */ 01127000
*   /* SATISFIED AND INPUT IS PENDING, CONTROL IS PASSED TO IEDSARI, */ 01128000
*   /* ELSE EXIT IS MADE VIA DSPDISP.                                */ 01129000
*   /*                                                               */ 01130000
*   /*****************************************************************/ 01131000
*                                                                  0832 01132000
*   RFY                                                            0832 01133000
*    (RLINK) RSTD;                                                 0832 01134000
*WAITCHN:                                                          0833 01135000
*   PROC OPTIONS(SAVE(14),RETREG);                                 0833 01136000
WAITCHN  ST    @14,@SA00014                                        0833 01137000
*   IF QCBPLCBN=ON THEN             /* IF A PLCB IS ASSIGNED         */ 01138000
         L     @14,IEDQQPTR                                        0834 01139000
         AL    @14,@CF02301                                        0834 01140000
         TM    QCBPLCBN-4(@14),B'10000000'                         0834 01141000
         BNO   @RF00834                                            0834 01142000
*     DO;                                                          0835 01143000
*       IF LCBWAITC=0 THEN          /* IF THERE IS NOTHING ON WAIT 0836 01144000
*                                      CHAIN                         */ 01145000
         L     @14,LCBWAITC-1(,RLCB)                               0836 01146000
