*         LCBWAITC=RPRF;            /* PUT CURRENT INPUT ON WAIT   0837 01150000
*                                      CHAIN                         */ 01151000
         STCM  RPRF,7,LCBWAITC(RLCB)                               0837 01152000
*       ELSE                        /* ELSE, THERE IS A WAIT CHAIN TO   01153000
*                                      WHICH THIS INPUT              */ 01154000
*         DO;                       /* MUST BE LINKED IN FIFO ORDER  */ 01155000
         B     @RC00836                                            0838 01156000
@RF00836 DS    0H                                                  0839 01157000
*           CURBFR=RPRF;            /* SAVE PTR TO THIS INPUT        */ 01158000
         LR    CURBFR,RPRF                                         0839 01159000
*           RPRF=LCBWAITC;          /* POINT TO WAIT CHAIN           */ 01160000
         L     RPRF,LCBWAITC-1(,RLCB)                              0840 01161000
