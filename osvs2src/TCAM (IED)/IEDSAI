         STCM  RPARM,7,PRFLINK(@14)                                0741 00817000
*       CS(RPARM,RZERO,LCBOBRCH);   /* TRY TO ADD NEW PIU TO CHAIN   */ 00818000
         CS    RPARM,@00,LCBOBRCH(RLINK)                           0742 00819000
*       BC(7,CSLOOP);               /* TRY AGAIN IF UNSUCCESSFUL     */ 00820000
         BC    7,CSLOOP                                            0743 00821000
*/*                                                                  */ 00822000
*                                                                  0744 00823000
*       RFY                                                        0744 00824000
*         IEDQLCB BASED(RLCB);                                     0744 00825000
*       RFY                                                        0745 00826000
*         RLINK UNRSTD;                                            0745 00827000
*     END;                                                         0746 00828000
*   END RCRDMDR;                                                   0747 00829000
@EL00009 DS    0H                                                  0747 00830000
@EF00009 DS    0H                                                  0747 00831000
@ER00009 L     @14,@SA00009                                        0747 00832000
         BR    @14                                                 0747 00833000
*   RFY                                                            0748 00834000
*     RZERO UNRSTD;                                                0748 00835000
*                                                                  0749 00836000
*   /*****************************************************************/ 00837000
*   /*                                                               */ 00838000
*   /* RDPRPROC: IF INPUT IS A READ OR INVITE, SUBROUTINES WILL BE   */ 00839000
*   /* CALLED TO PROCESS NEG RESP TO POLL-QUEUED, BSC STATUS AND WAIT*/ 00840000
*   /* CHAIN PROCESSING. ALL VALID RD/INVITES ARE PASSED TO IEDSARI. */ 00841000
*   /* REMAINING VALID INPUT IS PASSED TO IEDSAC. IF NO FURTHER      */ 00842000
*   /* PROCESSING IS REQUIRED CONTROL IS GIVEN TO THE DISPATCHER.    */ 00843000
*   /*                                                               */ 00844000
*   /*****************************************************************/ 00845000
*                                                                  0749 00846000
*RDPRPROC:                                                         0749 00847000
*   PROC OPTIONS(RETREG,SAVE(14));                                 0749 00848000
RDPRPROC ST    @14,@SA00010                                        0749 00849000
*   IF RESPSAVE=RSPQNEGR THEN       /* IF Q-NEGREP RCVD              */ 00850000
         CLI   RESPSAVE,X'03'                                      0750 00851000
         BNE   @RF00750                                            0750 00852000
*     CALL NEGREP;                  /* CALL NEG RESP PROCESSOR       */ 00853000
         BAL   @14,NEGREP                                          0751 00854000
*   ELSE                                                           0752 00855000
*     DO;                                                          0752 00856000
         B     @RC00750                                            0752 00857000
@RF00750 DS    0H                                                  0753 00858000
*       TRMWTNEG=OFF;               /* NOT WAITING FOR Q-NEGREP      */ 00859000
         NI    TRMWTNEG(RTRM),B'01111111'                          0753 00860000
*       TRMQNPL=OFF;                /* NOT PROCESSING A Q-NEGREP     */ 00861000
         NI    TRMQNPL(RTRM),B'11110111'                           0754 00862000
*     END;                                                         0755 00863000
*   IF RETCODE=INITVAL THEN         /* IF RET CODE STILL NOT SET     */ 00864000
@RC00750 CLI   RETCODE,X'FF'                                       0756 00865000
         BNE   @RF00756                                            0756 00866000
*     DO;                                                          0757 00867000
*       IF TRMSTPND=ON|             /* IF BSC STATUS PENDING         */ 00868000
*           (BDUPHASE^=RSP00&(BDURESP=RSPE0A|/* OR BSC ERR STAT RECVD*/ 00869000
*           BDURESP=RSPN05)) THEN   /* OR BSC NORM STAT RECVD        */ 00870000
         TM    TRMSTPND(RTRM),B'10000000'                          0758 00871000
