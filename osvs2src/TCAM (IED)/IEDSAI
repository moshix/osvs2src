         TM    TRMLYNCH(RTRM),B'00000001'                          0621 00353000
         BNO   @RF00621                                            0621 00354000
@RT00621 DS    0H                                                  0622 00355000
*         DO;                                                      0622 00356000
*           IF QCBPLCBN=ON THEN     /* HAVE A PLCB?                  */ 00357000
         L     @10,IEDQQPTR                                        0623 00358000
