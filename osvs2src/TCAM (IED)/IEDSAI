         LR    RPRF,@14                                            0842 01169000
*           END;                                                   0843 01170000
@DE00841 L     @14,PRFLINK-1(,IEDQPPTR)                            0843 01171000
         LA    @14,0(,@14)                                         0843 01172000
         LTR   @14,@14                                             0843 01173000
         BNZ   @DL00841                                            0843 01174000
*           PRFLINK=CURBFR;         /* LINK CURRENT INPUT TO WAIT  0844 01175000
*                                      CHAIN                         */ 01176000
         STCM  CURBFR,7,PRFLINK(IEDQPPTR)                          0844 01177000
*           RPRF=CURBFR;            /* RESTORE PTR TO CURRENT INPUT  */ 01178000
         LR    RPRF,CURBFR                                         0845 01179000
*         END;                                                     0846 01180000
*       IF LCBRECVN=ON&             /* IF PLCB IS RECEIVING          */ 01181000
*           (TRMRDOUT=ON|           /* AND IF A READ IS OUT          */ 01182000
