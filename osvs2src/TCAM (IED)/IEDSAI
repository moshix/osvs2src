*     CALL ERRLOCK;                                                0551 00060000
         BAL   @14,ERRLOCK                                         0551 00061000
*   ELSE                                                           0552 00062000
*     ;                             /*                       @YM07645*/ 00063000
@RF00550 DS    0H                                                  0553 00064000
*   IF(BDUCMND=CD0READ|             /* IF THIS IS READ OR            */ 00065000
*       BDUCMND=CD0INV) THEN        /* INVITE INPUT THEN DO          */ 00066000
