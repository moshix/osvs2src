*       CALL WAITCHN;               /* PUT THIS INPUT ON WAIT CHAIN  */ 00888000
@RF00758 BAL   @14,WAITCHN                                         0760 00889000
*     END;                                                         0761 00890000
*   END RDPRPROC;                                                  0762 00891000
@EL00010 DS    0H                                                  0762 00892000
@EF00010 DS    0H                                                  0762 00893000
@ER00010 L     @14,@SA00010                                        0762 00894000
         BR    @14                                                 0762 00895000
*                                                                  0763 00896000
*   /*****************************************************************/ 00897000
*   /*                                                               */ 00898000
*   /* REQPLCB: PLACE THE CURRENT INPUT ON QCB'S WAIT CHAIN AND      */ 00899000
*   /* REQUEST A PLCB                                                */ 00900000
*   /*                                                               */ 00901000
*   /*****************************************************************/ 00902000
*                                                                  0763 00903000
*   RFY                                                            0763 00904000
*    (RLINK) RSTD;                                                 0763 00905000
*REQPLCB:                                                          0764 00906000
*   PROC OPTIONS(RETREG,SAVE(14));                                 0764 00907000
REQPLCB  ST    @14,@SA00011                                        0764 00908000
*   IF QCBWAITC=OFF THEN            /* IF QCB HAS NO WAIT CHAIN      */ 00909000
         L     @14,IEDQQPTR                                        0765 00910000
         SL    @14,@CF00044                                        0765 00911000
         TM    QCBWAITC(@14),B'01000000'                           0765 00912000
         BNZ   @RF00765                                            0765 00913000
*     DO;                                                          0766 00914000
*       QCBWAITC=ON;                /* THEN INDICATE A WAIT CHAIN    */ 00915000
         OI    QCBWAITC(@14),B'01000000'                           0767 00916000
*       QCBPLCBA=RPRF;              /* AND PUT INPUT ON WAIT CHAIN   */ 00917000
         STCM  RPRF,7,QCBPLCBA(@14)                                0768 00918000
*     END;                                                         0769 00919000
*   ELSE                                                           0770 00920000
*     DO;                           /* IF THERE IS A QCB WAIT CHAIN  */ 00921000
         B     @RC00765                                            0770 00922000
@RF00765 DS    0H                                                  0771 00923000
*       CURBFR=RPRF;                /* SAVE PTR TO THIS INPUT        */ 00924000
         LR    CURBFR,RPRF                                         0771 00925000
*       RPRF=QCBPLCBA;              /* POINT TO WAIT CHAIN           */ 00926000
         L     @14,IEDQQPTR                                        0772 00927000
         AL    @14,@CF02301                                        0772 00928000
