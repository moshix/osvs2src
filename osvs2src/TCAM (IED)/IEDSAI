         L     @10,AVTSAVTP(,IEDQAPTR)                             0783 00970000
         L     @15,SAVTPREQ(,@10)                                  0783 00971000
         BALR  @14,@15                                             0783 00972000
*       IF RLINK=0 THEN             /* IF PLCB REQUEST WAS ENQUEUED  */ 00973000
         LTR   RLINK,RLINK                                         0784 00974000
         BNZ   @RF00784                                            0784 00975000
*         RETCODE=NOPST;            /* SET RET CODE FOR DSPDISP      */ 00976000
         MVI   RETCODE,X'00'                                       0785 00977000
*       ELSE                        /* ELSE, GOT A PLCB              */ 00978000
*         DO;                                                      0786 00979000
         B     @RC00784                                            0786 00980000
@RF00784 DS    0H                                                  0787 00981000
*           RLCB=RLINK;             /* SETUP ADDRESSABILITY TO PLCB  */ 00982000
         LR    RLCB,RLINK                                          0787 00983000
*           IF LCBOCNI=ON THEN      /* PRESERVE STATUS OF OCNI       */ 00984000
         TM    LCBOCNI(RLCB),B'00100000'                           0788 00985000
         BNO   @RF00788                                            0788 00986000
*             DO;                                                  0789 00987000
*               LCBSTAT1=0;         /* TURN ALL BITS OFF AND         */ 00988000
         MVI   LCBSTAT1(RLCB),X'00'                                0790 00989000
*               LCBOCNI=ON;         /* TURN OCNI BACK ON             */ 00990000
*               LCBRECVN=ON;        /* THEN INDICATE RECEIVE STATE   */ 00991000
         OI    LCBOCNI(RLCB),B'00100010'                           0792 00992000
*             END;                                                 0793 00993000
