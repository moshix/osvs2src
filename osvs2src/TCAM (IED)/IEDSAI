         LR    RPRF,@14                                            0774 00937000
*       END;                                                       0775 00938000
@DE00773 L     @14,PRFLINK-1(,IEDQPPTR)                            0775 00939000
         LA    @14,0(,@14)                                         0775 00940000
         LTR   @14,@14                                             0775 00941000
         BNZ   @DL00773                                            0775 00942000
*       PRFLINK=CURBFR;             /* LINK CURRENT INPUT TO QCB'S 0776 00943000
*                                      WAIT CHAIN                    */ 00944000
         STCM  CURBFR,7,PRFLINK(IEDQPPTR)                          0776 00945000
*       RPRF=CURBFR;                /* RESTORE PTR TO CURRENT INPUT  */ 00946000
         LR    RPRF,CURBFR                                         0777 00947000
*     END;                                                         0778 00948000
*                                                                  0779 00949000
*   /*****************************************************************/ 00950000
*   /*                                                               */ 00951000
*   /* IF NO PLCB REQ IS PENDING, CALL IEDIAP TO REQUEST ONE. UPON   */ 00952000
*   /* RETURN CHANGE THE STATE OF THE PLCB FROM SEND TO RECEIVE.     */ 00953000
*   /* PRESERVE THE STATUS OF THE LCBOCNI BIT DURING THE STATE       */ 00954000
*   /* CHANGE.                                                       */ 00955000
*   /*                                                               */ 00956000
*   /*****************************************************************/ 00957000
*                                                                  0779 00958000
*   IF QCBRCVRQ=OFF THEN            /* IF NO PLCB REQ IS PENDING     */ 00959000
@RC00765 L     @14,IEDQQPTR                                        0779 00960000
         SL    @14,@CF00044                                        0779 00961000
