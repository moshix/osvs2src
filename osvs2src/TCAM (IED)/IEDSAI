*   TTHOAF=TTHDAF;                  /* REVERSE OAF AND               */ 01036000
         MVC   @TS00001(2),TTHDAF(RPIU)                            0808 01037000
         MVC   TTHOAF(2,RPIU),@TS00001                             0808 01038000
*   TTHDAF=CUROAF;                  /* DAF PAIR                      */ 01039000
         MVC   TTHDAF(2,RPIU),CUROAF                               0809 01040000
