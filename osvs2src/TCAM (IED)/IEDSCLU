*   TTHDCF=XCINBLEN+LENGTH(PIURH);  /* BIND PIU LENGTH               */ 00679000
         LA    @14,3                                               0706 00680000
         SLR   @10,@10                                             0706 00681000
         ICM   @10,3,XCINBLEN+13(RPIU)                             0706 00682000
         ALR   @14,@10                                             0706 00683000
         STCM  @14,3,TTHDCF(RPIU)                                  0706 00684000
*   PIURH=SCRH;                     /* SET UP BIND'S RH              */ 00685000
         MVC   PIURH(3,RPIU),@CB02136                              0707 00686000
*   CALL GETNAME;                   /* GET PLU NAME          @YM07298   00687000
*                                      SET UP DATA-MOVE PARAMETERS   */ 00688000
         BAL   @14,GETNAME                                         0708 00689000
*   PLMVTU=RPRF;                    /* TARGET UNIT                   */ 00690000
