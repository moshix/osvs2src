*   TTHBYTE1=0;                     /*                               */ 00667000
         MVI   TTHBYTE1(RPIU),X'00'                                0702 00668000
*   TTHOAF=TTHDAF;                  /* PLU'S TNT INDEX               */ 00669000
         MVC   @TS00001(2),TTHDAF(RPIU)                            0703 00670000
         MVC   TTHOAF(2,RPIU),@TS00001                             0703 00671000
*   TTHDAF=TTHTAG;                  /* SLU'S TNT INDEX               */ 00672000
         SLR   @14,@14                                             0704 00673000
         ICM   @14,3,TTHTAG(RPIU)                                  0704 00674000
         STCM  @14,3,TTHDAF(RPIU)                                  0704 00675000
*   TTHTAG=0;                       /* TAG FIELD                     */ 00676000
