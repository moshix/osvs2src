*               TRMINPTP=OFF;       /* TURN OFF INPUT PENDING        */ 01346000
         NI    TRMINPTP(@14),B'10111111'                           0881 01347000
*               LCBQCBA=ADDR(SAVTLUSQ);/* TPOST THE PLCB TO          */ 01348000
         L     @14,AVTSAVTP(,RAVT)                                 0882 01349000
