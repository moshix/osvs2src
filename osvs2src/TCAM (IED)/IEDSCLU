*     DO;                           /* YES, RQR IS A VALID REQUEST   */ 01332000
*       SIBLGOFF=ON;                /* SET LOGOFF                    */ 01333000
         OI    SIBLGOFF(RSIB),B'10000000'                          0876 01334000
