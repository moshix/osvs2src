*   PLMVTD=ADDR(PIURU);             /* TARGET DATA                   */ 00693000
         LA    @10,PIURU(,RPIU)                                    0710 00694000
         ST    @10,PLMVTD(,@14)                                    0710 00695000
*   PLMVSU=RPRF;                    /* SOURCE UNIT                   */ 00696000
