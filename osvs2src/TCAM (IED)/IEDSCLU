         TITLE 'IEDSCLU, SESSION CONTROL                               *00001000
                        '                                               00002000
IEDSCLU  CSECT ,                                                   0001 00003000
@PROLOG  DS    0H                                                  0002 00004000
         USING *,RBASE                                                  00005000
*                                                                       00006000
         DC    XL8'00'              QCB FOR THIS MODULE                 00007000
         DC    XL1'06'              STCB KEY                            00008000
         DC    AL3(*-1)             STCB POINTER                        00009000
*                                                                       00010000
         LR    RBASE,RQCB           SET UP BASE REGISTER                00011000
*                                                                       00012000
         LA    RPARM,0(RPARM)       CLEAR HIGH-ORDER BYTE OF INPUT REG  00013000
IEDSCLU  IEDHJN  AFTERHJN,HJN                                           00014000
*   SWITCHES=0;                     /* INITIALIZE ALL SWITCHES TO OFF*/ 00015000
         MVI   SWITCHES,X'00'                                      0511 00016000
         EJECT                                                          00017000
*                                                                  0512 00018000
*/********************************************************************/ 00019000
*/*                                                                  */ 00020000
*/*      M A I N   S E G M E N T                                     */ 00021000
*/*                                                                  */ 00022000
*/********************************************************************/ 00023000
*/*                                                                  */ 00024000
*/*      THIS MAIN SEGMENT DETERMINES THE INPUT TYPE, SETS UP        */ 00025000
*/*      CONTROL BLOCK POINTERS, AND CALLS THE PROPER SUBROUTINE     */ 00026000
*/*      TO PROCESS THE INPUT.                                       */ 00027000
*/*                                                                  */ 00028000
*/********************************************************************/ 00029000
*                                                                  0512 00030000
*   IF RECBPRI=PRILNSCL THEN        /* PLCB FROM USER'S UNBIND EXIT? */ 00031000
         CLI   RECBPRI(RPARM),227                                  0512 00032000
         BNE   @RF00512                                            0512 00033000
*     DO;                           /* YES, PROCESS THE PREVIOUSLY   */ 00034000
*       RLCB=RPARM;                 /* ... SAVED CLEAR RESPONSE      */ 00035000
         LR    RLCB,RPARM                                          0514 00036000
*       RPARM=LCBBFRSV;             /* ... BUFFER AS THOUGH IT WERE  */ 00037000
         L     RPARM,LCBBFRSV-1(,RLCB)                             0515 00038000
         LA    RPARM,0(,RPARM)                                     0515 00039000
*       LCBBFRSV=0;                 /* ... THE ACTUAL INPUT ELEMENT  */ 00040000
         SLR   @14,@14                                             0516 00041000
         STCM  @14,7,LCBBFRSV(RLCB)                                0516 00042000
*       REENTRSW=ON;                /* MARK THIS AS A RE-ENTRY TO    */ 00043000
         OI    REENTRSW,B'10000000'                                0517 00044000
*     END;                          /* ... REPROCESS CLEAR RESPONSE  */ 00045000
*   IF RECBPRI=PRILNFRE THEN        /* PLCB?                         */ 00046000
@RF00512 CLI   RECBPRI(RPARM),224                                  0519 00047000
         BNE   @RF00519                                            0519 00048000
*     DO;                           /* YES                           */ 00049000
*       RLCB=RPARM;                 /* GET PLCB ADDRESS              */ 00050000
         LR    RLCB,RPARM                                          0521 00051000
*       TNTSLU=LCBTTCIN;            /* GET SLU'S TNT INDEX           */ 00052000
         LH    @14,LCBTTCIN(,RLCB)                                 0522 00053000
         N     @14,@CF02265                                        0522 00054000
         STH   @14,TNTSLU                                          0522 00055000
*       CALL CTLBLKS;               /* SET BASIC CONTROL BLOCK PTRS  */ 00056000
         BAL   @14,CTLBLKS                                         0523 00057000
*       CALL LOGOFF;                /* BEGIN LOGOFF PROCESSING       */ 00058000
         BAL   @14,LOGOFF                                          0524 00059000
*     END;                                                         0525 00060000
*   ELSE                            /* INPUT IS A PIU BUFFER         */ 00061000
*     DO;                                                          0526 00062000
         B     @RC00519                                            0526 00063000
@RF00519 DS    0H                                                  0527 00064000
*       RPRF=RPARM;                 /* BUFFER ADDRESS                */ 00065000
         LR    RPRF,RPARM                                          0527 00066000
*       RPIU=ADDR(PRFPIUO)+PRFPIUO; /* PIU ADDRESS                   */ 00067000
         LA    RPIU,PRFPIUO(,RPRF)                                 0528 00068000
         SLR   @14,@14                                             0528 00069000
         IC    @14,PRFPIUO(,RPRF)                                  0528 00070000
         ALR   RPIU,@14                                            0528 00071000
*       DATAPTR=ADDR(PIURU);        /* RU ADDRESS                    */ 00072000
         LA    @14,PIURU(,RPIU)                                    0529 00073000
         ST    @14,DATAPTR                                         0529 00074000
*       IF TRHSDI=ON THEN           /* SENSE INCLUDED?               */ 00075000
         TM    TRHSDI+10(RPIU),B'00000100'                         0530 00076000
         BNO   @RF00530                                            0530 00077000
*         DATAPTR=DATAPTR+LENGTH(IEDSNSD);/* YES, SKIP PAST SENSE    */ 00078000
         AL    @14,@CF00330                                        0531 00079000
         ST    @14,DATAPTR                                         0531 00080000
*       IF TRHRUCAT=TRHFMD THEN     /* NETWORK SERVICES PIU?         */ 00081000
@RF00530 TM    TRHRUCAT+10(RPIU),B'01100000'                       0532 00082000
         BNZ   @RF00532                                            0532 00083000
*         DO;                       /* YES, MUST BE CINIT FROM IEDLUS*/ 00084000
*           TNTSLU=TTHTAG;          /* GET SLU'S TNT INDEX           */ 00085000
         MVC   TNTSLU(2),TTHTAG(RPIU)                              0534 00086000
*           CALL CTLBLKS;           /* SET BASIC CONTROL BLOCK PTRS  */ 00087000
         BAL   @14,CTLBLKS                                         0535 00088000
*           CALL LOGON;             /* BEGIN LOGON PROCESSING        */ 00089000
         BAL   @14,LOGON                                           0536 00090000
*         END;                                                     0537 00091000
         EJECT                                                          00092000
*       ELSE                        /* INPUT IS A SESSION-CONTROL PIU*/ 00093000
*         DO;                                                      0538 00094000
         B     @RC00532                                            0538 00095000
@RF00532 DS    0H                                                  0539 00096000
*           TNTSLU=TTHOAF;          /* GET SLU'S TNT INDEX           */ 00097000
         MVC   TNTSLU(2),TTHOAF(RPIU)                              0539 00098000
*           CALL CTLBLKS;           /* SET BASIC CONTROL BLOCK PTRS  */ 00099000
         BAL   @14,CTLBLKS                                         0540 00100000
*           IF TRHTYPE=ON THEN      /* RESPONSE?                     */ 00101000
         TM    TRHTYPE+10(RPIU),B'10000000'                        0541 00102000
         BNO   @RF00541                                            0541 00103000
*             DO;                   /* YES, DETERMINE PIU TYPE       */ 00104000
*               IF RUCMD1=CD1BIND THEN                             0543 00105000
         L     @14,DATAPTR                                         0543 00106000
         CLI   RUCMD1(@14),X'31'                                   0543 00107000
         BNE   @RF00543                                            0543 00108000
*                 CALL BINDRSP;     /* BIND RESPONSE                 */ 00109000
         BAL   @14,BINDRSP                                         0544 00110000
*               ELSE                                               0545 00111000
*                 IF RUCMD1=CD1SDT THEN                            0545 00112000
         B     @RC00543                                            0545 00113000
@RF00543 L     @14,DATAPTR                                         0545 00114000
         CLI   RUCMD1(@14),X'A0'                                   0545 00115000
         BNE   @RF00545                                            0545 00116000
*                   CALL SDTRSP;    /* SDT RESPONSE                  */ 00117000
         BAL   @14,SDTRSP                                          0546 00118000
*                 ELSE                                             0547 00119000
*                   IF RUCMD1=CD1CLEAR THEN                        0547 00120000
         B     @RC00545                                            0547 00121000
@RF00545 L     @14,DATAPTR                                         0547 00122000
         CLI   RUCMD1(@14),X'A1'                                   0547 00123000
         BNE   @RF00547                                            0547 00124000
*                     CALL CLEARRSP;/* CLEAR RESPONSE                */ 00125000
         BAL   @14,CLEARRSP                                        0548 00126000
*                   ELSE                                           0549 00127000
*                     IF RUCMD1=CD1UBIND THEN                      0549 00128000
         B     @RC00547                                            0549 00129000
@RF00547 L     @14,DATAPTR                                         0549 00130000
         CLI   RUCMD1(@14),X'32'                                   0549 00131000
         BNE   @RF00549                                            0549 00132000
*                       CALL UNBNDRSP;/* UNBIND RESPONSE             */ 00133000
         BAL   @14,UNBNDRSP                                        0550 00134000
*                     ELSE          /* UNEXPECTED RESPONSE           */ 00135000
*                       DO;         /* THROW IT AWAY                 */ 00136000
         B     @RC00549                                            0551 00137000
@RF00549 DS    0H                                                  0552 00138000
*                         RPARM=RPRF;                              0552 00139000
         LR    RPARM,RPRF                                          0552 00140000
*                         CALL FREEBUF;                            0553 00141000
         BAL   @14,FREEBUF                                         0553 00142000
*                       END;                                       0554 00143000
*             END;                                                 0555 00144000
*           ELSE                    /* REQUEST                       */ 00145000
*             DO;                                                  0556 00146000
         B     @RC00541                                            0556 00147000
@RF00541 DS    0H                                                  0557 00148000
*               IF RUCMD1=CD1RQR THEN/* RQR REQUEST?                 */ 00149000
         L     @14,DATAPTR                                         0557 00150000
         CLI   RUCMD1(@14),X'A3'                                   0557 00151000
         BNE   @RF00557                                            0557 00152000
*                 CALL RQRRTN;      /* YES                           */ 00153000
         BAL   @14,RQRRTN                                          0558 00154000
*               ELSE                                               0559 00155000
*                 DO;               /* UNEXPECTED REQUEST            */ 00156000
         B     @RC00557                                            0559 00157000
@RF00557 DS    0H                                                  0560 00158000
*                   IEDSNSD=INVALSNS;/* SPECIFY SENSE TO BE SENT     */ 00159000
         LA    @14,SENSE                                           0560 00160000
         MVC   IEDSNSD(4,@14),INVALSNS                             0560 00161000
*                   CALL BLDRESP;   /* SEND AN ERROR RESPONSE        */ 00162000
         BAL   @14,BLDRESP                                         0561 00163000
*                 END;                                             0562 00164000
*             END;                                                 0563 00165000
@RC00557 DS    0H                                                  0564 00166000
*         END;                                                     0564 00167000
@RC00541 DS    0H                                                  0565 00168000
*     END;                                                         0565 00169000
@RC00532 DS    0H                                                  0566 00170000
*   IF BYPASSSW=OFF THEN            /* NORMAL EXIT TO BE USED?       */ 00171000
@RC00519 TM    BYPASSSW,B'01000000'                                0566 00172000
         BNZ   @RF00566                                            0566 00173000
*     CALL DSPDISP;                 /* YES, EXIT TO DISPATCHER       */ 00174000
         LA    @15,DSPDISP(,RDISP)                                 0567 00175000
         BALR  @14,@15                                             0567 00176000
*   ELSE                                                           0568 00177000
*     DO;                           /* NO, BYPASS TO UNBIND EXIT     */ 00178000
         B     @RC00566                                            0568 00179000
@RF00566 DS    0H                                                  0569 00180000
*       RFY                                                        0569 00181000
*        (RQCB,                                                    0569 00182000
*         RSTCB) RSTD;                                             0569 00183000
*       RPARM=RLCB;                 /* ELEMENT ADDRESS               */ 00184000
         LR    RPARM,RLCB                                          0570 00185000
*       RQCB=LCBQCBA;               /* QCB ADDRESS                   */ 00186000
         L     RQCB,LCBQCBA-1(,RLCB)                               0571 00187000
         LA    RQCB,0(,RQCB)                                       0571 00188000
*       RSTCB=RQCB->QCBSTCHN;       /* STCB ADDRESS                  */ 00189000
         L     RSTCB,QCBSTCHN-1(,RQCB)                             0572 00190000
         LA    RSTCB,0(,RSTCB)                                     0572 00191000
*       CALL DSPBYPAS;              /* BYPASS                        */ 00192000
         LA    @15,DSPBYPAS(,RDISP)                                0573 00193000
         BALR  @14,@15                                             0573 00194000
*       RFY                                                        0574 00195000
*        (RQCB,                                                    0574 00196000
*         RSTCB) UNRSTD;                                           0574 00197000
*     END;                                                         0575 00198000
*                                                                  0576 00199000
*/***************  E N D   O F   M A I N   S E G M E N T  ************/ 00200000
*                                                                  0576 00201000
         EJECT                                                          00202000
*BINDEXIT:                                                         0576 00203000
*   PROC OPTIONS(SAVE(R14));                                       0576 00204000
*                                                                  0576 00205000
@EL00001 DS    0H                                                  0576 00206000
@EF00001 DS    0H                                                  0576 00207000
@ER00001 BR    @14                                                 0576 00208000
BINDEXIT ST    @14,@SA00002                                        0576 00209000
*/********************************************************************/ 00210000
*/*                                                                  */ 00211000
*/*      B I N D E X I T   SUBROUTINE                                */ 00212000
*/*                                                                  */ 00213000
*/********************************************************************/ 00214000
*/*                                                                  */ 00215000
*/*      THIS SUBROUTINE SETS UP THE INTERFACE BETWEEN THIS          */ 00216000
*/*      MODULE AND THE USER-WRITTEN BIND EXIT, IF ONE HAS           */ 00217000
*/*      BEEN PROVIDED.                                              */ 00218000
*/*                                                                  */ 00219000
*/********************************************************************/ 00220000
*                                                                  0577 00221000
*   TNTPLU=TTHOAF;                  /* PLU'S TNT INDEX               */ 00222000
         MVC   TNTPLU(2),TTHOAF(RPIU)                              0577 00223000
*   MHPTR=LCBMHA;                   /* GET MH QCB ADDRESS    @YM06085*/ 00224000
         L     @14,LCBMHA-1(,RLCB)                                 0578 00225000
         LA    @14,0(,@14)                                         0578 00226000
         ST    @14,MHPTR                                           0578 00227000
*   R15=STMHBEXT;                   /* GET BIND EXIT ADDRESS         */ 00228000
         AL    @14,@CF02266                                        0579 00229000
         L     R15,STMHBEXT-9(,@14)                                0579 00230000
         LA    R15,0(,R15)                                         0579 00231000
*   IF R15>0 THEN                   /* IS A BIND EXIT SPECIFIED?     */ 00232000
         LTR   R15,R15                                             0580 00233000
         BNP   @RF00580                                            0580 00234000
*     DO;                           /* YES                           */ 00235000
*       RFY                                                        0582 00236000
*        (REG02,                                                   0582 00237000
*         REG03,                                                   0582 00238000
*         REG04) RSTD;              /*                       @YM07260*/ 00239000
*       STM(REG02,REG04,LOCALSAV);  /* SAVE REGISTERS USED AS      0583 00240000
*                                      PARAMETERS FOR THE USER'S BIND   00241000
*                                      EXIT                  @YM07260*/ 00242000
         STM   REG02,REG04,LOCALSAV                                0583 00243000
*       REG04=ADDR(BNDFORMT);       /* PASS ADDRESS OF BIND IMAGE IN    00244000
*                                      THE BIND PIU TO THE USER    0584 00245000
*                                                            @YM07260*/ 00246000
         LA    REG04,BNDFORMT+13(,RPIU)                            0584 00247000
*       RPARM=ADDR(TNTFIRST)+(TTHDAF-1)*(TNTENLEN+LENGTH(TNTTRMAD));    00248000
         L     @14,AVTRNMPT(,RAVT)                                 0585 00249000
         LA    RPARM,82                                            0585 00250000
         ALR   RPARM,@14                                           0585 00251000
         SLR   @09,@09                                             0585 00252000
         ICM   @09,3,TTHDAF(RPIU)                                  0585 00253000
         BCTR  @09,0                                               0585 00254000
         LA    @10,3                                               0585 00255000
         SLR   @07,@07                                             0585 00256000
         IC    @07,TNTENLEN(,@14)                                  0585 00257000
         ALR   @10,@07                                             0585 00258000
         MR    @08,@10                                             0585 00259000
         ALR   RPARM,@09                                           0585 00260000
*                                   /* CONVERT SECONDARY LU'S TTCIN     00261000
*                                      TO A TNT ENTRY ADDRESS      0586 00262000
*                                                            @YM07260*/ 00263000
*       IF TRMLUREQ=OFF THEN        /* TCAM-INITIATED SESSION?       */ 00264000
         L     @14,TRMPTR                                          0586 00265000
         TM    TRMLUREQ(@14),B'00000100'                           0586 00266000
         BNZ   @RF00586                                            0586 00267000
*         RPARM=RPARM|HIGHBIT;      /* YES, TURN ON HIGH ORDER BIT   */ 00268000
         O     RPARM,@CF02141                                      0587 00269000
*       REG02=TTHOAF;               /* GET PRIMARY LU'S TTCIN      0588 00270000
*                                                            @YM07260*/ 00271000
@RF00586 SLR   @14,@14                                             0588 00272000
         ICM   @14,3,TTHOAF(RPIU)                                  0588 00273000
         LR    REG02,@14                                           0588 00274000
*       IF REG02=SAVTMHDX           /* IF PRIMARY LU IS 'TCAM'     0589 00275000
*                                                            @YM07260*/ 00276000
*         THEN                                                     0589 00277000
         L     @14,AVTSAVTP(,RAVT)                                 0589 00278000
         LH    @14,SAVTMHDX(,@14)                                  0589 00279000
         N     @14,@CF02265                                        0589 00280000
         CR    REG02,@14                                           0589 00281000
         BNE   @RF00589                                            0589 00282000
*         REG02=0;                  /* ..THEN SO INDICATE FOR USER'S    00283000
*                                      BIND EXIT             @YM07260*/ 00284000
         SLR   REG02,REG02                                         0590 00285000
*       ELSE                                                       0591 00286000
*         IF TNTLEN<REG02           /* ..ELSE IF THE PRIMARY LU'S  0591 00287000
*                                      TTCIN EXCEEDS THE GREATEST TTE   00288000
*                                      TTCIN                 @YM07260*/ 00289000
*           THEN                                                   0591 00290000
         B     @RC00589                                            0591 00291000
@RF00589 L     @14,AVTRNMPT(,RAVT)                                 0591 00292000
         LH    @14,TNTLEN(,@14)                                    0591 00293000
         N     @14,@CF02265                                        0591 00294000
         CR    REG02,@14                                           0591 00295000
         BNH   @RF00591                                            0591 00296000
*           REG02=-1;               /* ..THEN INDICATE TO THE USER 0592 00297000
*                                      THAT THE PRIMARY LU IS AN MH     00298000
*                                                            @YM07260*/ 00299000
         SLR   REG02,REG02                                         0592 00300000
         BCTR  REG02,0                                             0592 00301000
*         ELSE                                                     0593 00302000
*           REG02=ADDR(TNTFIRST)+(REG02-1)*(TNTENLEN+LENGTH(TNTTRMAD)); 00303000
         B     @RC00591                                            0593 00304000
@RF00591 L     @14,AVTRNMPT(,RAVT)                                 0593 00305000
         LA    @10,82                                              0593 00306000
         ALR   @10,@14                                             0593 00307000
         LR    @09,REG02                                           0593 00308000
         BCTR  @09,0                                               0593 00309000
         LA    @07,3                                               0593 00310000
         SLR   @08,@08                                             0593 00311000
         IC    @08,TNTENLEN(,@14)                                  0593 00312000
         ALR   @07,@08                                             0593 00313000
         MR    @08,@07                                             0593 00314000
         ALR   @10,@09                                             0593 00315000
         LR    REG02,@10                                           0593 00316000
*                                   /* ..ELSE THE PRIMARY LU IS AN 0594 00317000
*                                      APPLICATION AND WE WILL PASS     00318000
*                                      TO THE USER'S BIND EXIT THE 0594 00319000
*                                      ADDRESS OF THE APPLICATION'S     00320000
*                                      TPROCESS TNT ENTRY    @YM07260*/ 00321000
*       REG03=RPRF;                 /* PASS BIND PIU BUFFER'S ADDRESS   00322000
*                                                            @YM07260*/ 00323000
@RC00591 DS    0H                                                  0594 00324000
@RC00589 LR    REG03,RPRF                                          0594 00325000
*       CALL USEREXIT;              /* INVOKE USER'S BIND EXIT       */ 00326000
         BALR  @14,@15                                             0595 00327000
*       LM(REG02,REG04,LOCALSAV);   /* RESTORE REGISTERS USED TO PASS   00328000
*                                      PARAMETERS TO THE USER'S BIND    00329000
*                                      EXIT                  @YM07260*/ 00330000
         LM    REG02,REG04,LOCALSAV                                0596 00331000
*       RFY                                                        0597 00332000
*        (REG02,                                                   0597 00333000
*         REG03,                                                   0597 00334000
*         REG04) UNRSTD;            /*                       @YM07260*/ 00335000
*     END;                                                         0598 00336000
*   END BINDEXIT;                   /* RETURN TO CALLER              */ 00337000
@EL00002 DS    0H                                                  0599 00338000
@EF00002 DS    0H                                                  0599 00339000
@ER00002 L     @14,@SA00002                                        0599 00340000
         BR    @14                                                 0599 00341000
         EJECT                                                          00342000
*BINDOPTN:                                                         0600 00343000
*   PROC OPTIONS(SAVE(R14));                                       0600 00344000
*                                                                  0600 00345000
BINDOPTN ST    @14,@SA00003                                        0600 00346000
*/********************************************************************/ 00347000
*/*                                                                  */ 00348000
*/*      B I N D O P T N   SUBROUTINE                                */ 00349000
*/*                                                                  */ 00350000
*/********************************************************************/ 00351000
*/*                                                                  */ 00352000
*/*      THIS SUBROUTINE LOCATES THE 'IEDBNDPM' OPTION FIELD         */ 00353000
*/*      (IF THERE IS ONE SPECIFIED) FOR THE SLU, AND MOVES THE      */ 00354000
*/*      BIND DATA INTO THAT OPTION FIELD.  THE BIND DATA MOVED      */ 00355000
*/*      STARTS WITH THE BYTE AFTER THE REQUEST CODE.                */ 00356000
*/*                                                                  */ 00357000
*/********************************************************************/ 00358000
*                                                                  0601 00359000
*   R0=ADDR(BNDPMCON);              /* ADDRESS OF 'IEDBNDPM' NAME    */ 00360000
         LA    R0,@CC02132                                         0601 00361000
*   RPARM=TRMPTR+LENGTH(IEDNTRM);   /* ADDR OF SLU'S TTE             */ 00362000
         LA    RPARM,14                                            0602 00363000
         AL    RPARM,TRMPTR                                        0602 00364000
*   CALL IEDOPTN;                   /* CHECK FOR IEDBNDPM OPTION FLD */ 00365000
         L     @15,AVTOPTN(,RAVT)                                  0603 00366000
         BALR  @14,@15                                             0603 00367000
*   IF R15=0 THEN                   /* GOOD RETURN CODE?             */ 00368000
         LTR   R15,R15                                             0604 00369000
         BNZ   @RF00604                                            0604 00370000
*     DO;                           /* YES                           */ 00371000
*       PLMVLNTH=TTHDCF-LENGTH(PIURH)-1;/* CALCULATE BYTES TO MOVE   */ 00372000
         LA    @14,PLMVPARM                                        0606 00373000
         SLR   @10,@10                                             0606 00374000
         ICM   @10,3,TTHDCF(RPIU)                                  0606 00375000
         SL    @10,@CF00330                                        0606 00376000
         STH   @10,PLMVLNTH(,@14)                                  0606 00377000
*       IF R0<PLMVLNTH THEN         /* OPTION FIELD TOO SHORT?       */ 00378000
         CR    R0,@10                                              0607 00379000
         BNL   @RF00607                                            0607 00380000
*         PLMVLNTH=R0;              /* YES, ADJUST BYTE COUNT        */ 00381000
         STH   R0,PLMVLNTH(,@14)                                   0608 00382000
*                                   /* SET OTHER PARAMETERS FOR MOVE */ 00383000
*       PLMVTU=RPARM;               /* TARGET 'UNIT' (THE OPTION FLD)*/ 00384000
@RF00607 LA    @14,PLMVPARM                                        0609 00385000
         ST    RPARM,PLMVTU(,@14)                                  0609 00386000
*       PLMVTD=RPARM;               /* TARGET DATA                   */ 00387000
         ST    RPARM,PLMVTD(,@14)                                  0610 00388000
*       PLMVSU=RPRF;                /* SOURCE UNIT                   */ 00389000
         ST    RPRF,PLMVSU(,@14)                                   0611 00390000
*       PLMVSD=ADDR(BNDFORMT);      /* SOURCE DATA                   */ 00391000
         LA    @10,BNDFORMT+13(,RPIU)                              0612 00392000
         ST    @10,PLMVSD(,@14)                                    0612 00393000
*       PLMVFLG1=0;                 /* INDICATE THAT TARGET IS A     */ 00394000
         MVI   PLMVFLG1(@14),X'00'                                 0613 00395000
*       PLMVTCTG=ON;                /* ... CONTIGUOUS-CORE AREA      */ 00396000
         OI    PLMVTCTG(@14),B'00100000'                           0614 00397000
*       RPARM=ADDR(PLMVPARM);       /* ADDR OF PARAMETER LIST        */ 00398000
         LR    RPARM,@14                                           0615 00399000
*       CALL IEDMVBFR;              /* MOVE THE DATA                 */ 00400000
         L     @10,AVTSAVTP(,RAVT)                                 0616 00401000
         L     @15,SAVTMVBF(,@10)                                  0616 00402000
         BALR  @14,@15                                             0616 00403000
*     END;                                                         0617 00404000
*   END BINDOPTN;                   /* RETURN TO CALLER              */ 00405000
@EL00003 DS    0H                                                  0618 00406000
@EF00003 DS    0H                                                  0618 00407000
@ER00003 L     @14,@SA00003                                        0618 00408000
         BR    @14                                                 0618 00409000
         EJECT                                                          00410000
