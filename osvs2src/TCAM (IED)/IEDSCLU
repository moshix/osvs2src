         BZ    @RF00795                                            0795 00993000
*         DO;                       /* YES                           */ 00994000
*           LCBQCBA=STMHUNBR;       /* ADDR OF IEDUNBND QCB          */ 00995000
         MVC   LCBQCBA(3,RLCB),STMHUNBR+1(@14)                     0797 00996000
*           LCBPRI=PRILNSCL;        /* PRIORITY                      */ 00997000
         MVI   LCBPRI(RLCB),X'E3'                                  0798 00998000
*           LCBSTART=STMHUBXT;      /* ADDR OF USER'S UNBIND EXIT    */ 00999000
         STCM  @10,7,LCBSTART(RLCB)                                0799 01000000
*           LCBBFRSV=RPRF;          /* ADDR OF CLEAR RESPONSE BUFFER */ 01001000
         STCM  RPRF,7,LCBBFRSV(RLCB)                               0800 01002000
*           BYPASSSW=ON;            /* SPECIFY BYPASS TO IEDUNBND    */ 01003000
         OI    BYPASSSW,B'01000000'                                0801 01004000
*         END;                                                     0802 01005000
*       ELSE                        /* NO UNBIND EXIT SPECIFIED      */ 01006000
*         CALL UNBIND;              /* BUILD & SEND UNBIND PIU       */ 01007000
         B     @RC00795                                            0803 01008000
@RF00795 BAL   @14,UNBIND                                          0803 01009000
*     END;                                                         0804 01010000
*   ELSE                                                           0805 01011000
*     IF REENTRSW=ON THEN           /* RE-ENTRY FROM UNBIND EXIT?    */ 01012000
         B     @RC00790                                            0805 01013000
@RF00790 TM    REENTRSW,B'10000000'                                0805 01014000
         BNO   @RF00805                                            0805 01015000
*       CALL UNBIND;                /* YES, BUILD & SEND UNBIND PIU  */ 01016000
         BAL   @14,UNBIND                                          0806 01017000
*     ELSE                          /* UNEXPECTED RESPONSE           */ 01018000
*       DO;                         /* THROW IT AWAY                 */ 01019000
         B     @RC00805                                            0807 01020000
@RF00805 DS    0H                                                  0808 01021000
*         RPARM=RPRF;               /* BUFFER ADDRESS                */ 01022000
         LR    RPARM,RPRF                                          0808 01023000
*         CALL FREEBUF;             /* FREE THE BUFFER               */ 01024000
         BAL   @14,FREEBUF                                         0809 01025000
*       END;                                                       0810 01026000
*   END CLEARRSP;                   /* RETURN TO CALLER              */ 01027000
@EL00009 DS    0H                                                  0811 01028000
@EF00009 DS    0H                                                  0811 01029000
