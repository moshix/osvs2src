*                                                                  0845 01200000
*   SIBDSTAT=SIBDRPND;              /* SET DATA TRAFFIC STATE =    0845 01201000
*                                      'RESET PENDING'               */ 01202000
         NI    SIBDSTAT(RSIB),B'11111110'                          0845 01203000
         OI    SIBDSTAT(RSIB),B'00000010'                          0845 01204000
*   DO WHILE(LCBWAITC>0);           /* CLEAR THE PLCB'S WAIT CHAIN   */ 01205000
