*               CALL DSPPOSTR;      /* ... PROCESSING                */ 01356000
         LA    @15,DSPPOSTR(,RDISP)                                0885 01357000
         BALR  @14,@15                                             0885 01358000
*             END;                                                 0886 01359000
*         END;                                                     0887 01360000
*       ELSE                        /* NO PLCB CURRENTLY ASSIGNED    */ 01361000
*         DO;                                                      0888 01362000
