*/*      V A L B I N D   SUBROUTINE                                  */ 01591000
*/*                                                                  */ 01592000
*/********************************************************************/ 01593000
*/*                                                                  */ 01594000
*/*      THIS SUBROUTINE GETS THE BIND PIU AFTER THE USER BIND       */ 01595000
*/*      EXIT HAS BEED CALLED.  THOSE MODES OF OPERATION THAT        */ 01596000
*/*      ARE CONSTANT IN TCAM AND NOT SUBJECT TO USER CHOICE         */ 01597000
*/*      ARE SET IN THE BIND PARAMETERS, OVERLAYING ANY              */ 01598000
*/*      CONFLICTING VALUES THAT THE USER MIGHT HAVE SET.            */ 01599000
*/*                                                                  */ 01600000
*/*      THE PLUNAME IS RESTORED WITH THE CORRECT LENGTH   @ZM46792  */ 01601000
*/*      IN THE BIND IMAGE IN THE BUFFER AND THE TH DATA   @ZM46792  */ 01602000
*/*      ADJUSTED IF NECESSARY. THE USERDATA LENGTH IS     @ZM46792  */ 01603000
*/*      ALSO ADJUSTED IF CHANGED IN THE EXIT AND THE TH   @ZM46792  */ 01604000
*/*      DATA COUNT CHANGED TO REFLECT THE CHANGED LENGTH  @ZM46792  */ 01605000
*/********************************************************************/ 01606000
*                                                                  0951 01607000
*   BNDPRQM=OFF;                    /* TCAM ALWAYS RUNS IN IMMEDIATE    01608000
*                                      REQUEST MODE                  */ 01609000
*   BNDPDEF=ON;                     /* TCAM ALWAYS REQUESTS A        */ 01610000
*   BNDPEXC=OFF;                    /* ... DEFINITE RESPONSE         */ 01611000
         OI    BNDPDEF+13(RPIU),B'00100000'                        0953 01612000
         NI    BNDPRQM+13(RPIU),B'10101111'                        0953 01613000
*   NAMCOUNT=0;                     /* ZERO PLU NAME COUNT   @ZM46792*/ 01614000
         MVI   NAMCOUNT,X'00'                                      0954 01615000
*   NAMEADDR=ADDR(NAMEPLU);         /* IST CHAR ADDR         @ZM46792*/ 01616000
         LA    @14,NAMEPLU                                         0955 01617000
         ST    @14,NAMEADDR                                        0955 01618000
*   NMSWITCH=OFF;                   /* INIT SWITCH           @ZM46792*/ 01619000
         NI    NMSWITCH,B'11011111'                                0956 01620000
*   DO WHILE NMSWITCH=OFF&NAMCOUNT<MAXNAME;/*                @ZM46792*/ 01621000
         B     @DE00957                                            0957 01622000
@DL00957 DS    0H                                                  0958 01623000
*     IF NAMEADDR->CHAR1=' ' THEN   /* BLANK FOUND ?         @ZM46792*/ 01624000
         L     @14,NAMEADDR                                        0958 01625000
         CLI   CHAR1(@14),C' '                                     0958 01626000
         BNE   @RF00958                                            0958 01627000
*       NMSWITCH=ON;                /* YES SET SWITCH TO EXIT      0959 01628000
*                                                            @ZM46792*/ 01629000
         OI    NMSWITCH,B'00100000'                                0959 01630000
*     ELSE                          /* NO                    @ZM46792*/ 01631000
*       DO;                         /*                       @ZM46792*/ 01632000
         B     @RC00958                                            0960 01633000
@RF00958 DS    0H                                                  0961 01634000
*         NAMEADDR=NAMEADDR+1;      /* BUMP TO NEXT CHAR     @ZM46792*/ 01635000
         LA    @14,1                                               0961 01636000
         L     @10,NAMEADDR                                        0961 01637000
         ALR   @10,@14                                             0961 01638000
         ST    @10,NAMEADDR                                        0961 01639000
*         NAMCOUNT=NAMCOUNT+1;      /* INCREASE COUNT OF CHARS     0962 01640000
*                                                            @ZM46792*/ 01641000
         SLR   @10,@10                                             0962 01642000
         IC    @10,NAMCOUNT                                        0962 01643000
         ALR   @14,@10                                             0962 01644000
         STC   @14,NAMCOUNT                                        0962 01645000
*       END;                        /*                       @ZM46792*/ 01646000
*   END;                            /*                       @ZM46792*/ 01647000
@RC00958 DS    0H                                                  0964 01648000
@DE00957 TM    NMSWITCH,B'00100000'                                0964 01649000
         BNZ   @DC00957                                            0964 01650000
         CLI   NAMCOUNT,8                                          0964 01651000
         BL    @DL00957                                            0964 01652000
@DC00957 DS    0H                                                  0965 01653000
*   IF NAMCOUNT<MAXNAME THEN        /* IS PLU NAME LESS THEN 8     0965 01654000
*                                                            @ZM46792*/ 01655000
         CLI   NAMCOUNT,8                                          0965 01656000
         BNL   @RF00965                                            0965 01657000
*     DO;                           /* YES                   @ZM46792*/ 01658000
*       NEWCOUNT=MAXNAME-NAMCOUNT;  /* GET DIFFERENCE        @ZM46792*/ 01659000
         LA    @14,8                                               0967 01660000
         SLR   @10,@10                                             0967 01661000
         IC    @10,NAMCOUNT                                        0967 01662000
         SLR   @14,@10                                             0967 01663000
         STC   @14,NEWCOUNT                                        0967 01664000
*       TTHDCF=TTHDCF-NEWCOUNT;     /* ADJUST DATA COUNT     @ZM46792*/ 01665000
         LCR   @14,@14                                             0968 01666000
         SLR   @10,@10                                             0968 01667000
         ICM   @10,3,TTHDCF(RPIU)                                  0968 01668000
         ALR   @14,@10                                             0968 01669000
         STCM  @14,3,TTHDCF(RPIU)                                  0968 01670000
*     END;                          /*                       @ZM46792*/ 01671000
*   PLMVTU=RPRF;                    /* POINT TO TARGET UNIT FOR MOVE    01672000
*                                      BUFFER                @ZM46792*/ 01673000
@RF00965 LA    @14,PLMVPARM                                        0970 01674000
         ST    RPRF,PLMVTU(,@14)                                   0970 01675000
*   PLMVTD=ADDR(PIURU);             /* POINT TO TARG DATA BEGIN OF 0971 01676000
*                                      BIND                  @ZM46792*/ 01677000
         LA    @10,PIURU(,RPIU)                                    0971 01678000
         ST    @10,PLMVTD(,@14)                                    0971 01679000
*   PLMVSU=RPRF;                    /* SOURCE SAME AS TARGET FOR SKIP   01680000
*                                                            @ZM46792*/ 01681000
         ST    RPRF,PLMVSU(,@14)                                   0972 01682000
*   PLMVSD=ADDR(PIURU);             /* SOURCE SAME AS TARGET @ZM46792*/ 01683000
         ST    @10,PLMVSD(,@14)                                    0973 01684000
*   PLMVLNTH=BNDNAMPL;              /* TO PLU NAME FIELD     @ZM46792*/ 01685000
         SLR   @10,@10                                             0974 01686000
         IC    @10,BNDNAMPL                                        0974 01687000
         STH   @10,PLMVLNTH(,@14)                                  0974 01688000
*   PLMVFLG1=0;                     /* INIT FLAGS            @ZM46792*/ 01689000
         MVI   PLMVFLG1(@14),X'00'                                 0975 01690000
*   RPARM=ADDR(PLMVPARM);           /* POINT TO MOVEPARM LIST      0976 01691000
*                                                            @ZM46792*/ 01692000
         LR    RPARM,@14                                           0976 01693000
*   CALL IEDMVBFR;                  /* SKIP TO PLUNAME IN BUFFER   0977 01694000
*                                                            @ZM46792*/ 01695000
         L     @10,AVTSAVTP(,RAVT)                                 0977 01696000
         L     @15,SAVTMVBF(,@10)                                  0977 01697000
         BALR  @14,@15                                             0977 01698000
*   PLMVSU=ADDR(NAMEFLD);           /* POINT SOURCE TO SAVED PLUNAME    01699000
*                                                            @ZM46792*/ 01700000
         LA    @14,PLMVPARM                                        0978 01701000
         LA    @10,NAMEFLD                                         0978 01702000
         ST    @10,PLMVSU(,@14)                                    0978 01703000
*   PLMVSD=ADDR(NAMEFLD);           /* POINT SOURCE TO SAVED PLUNAME    01704000
*                                                            @ZM46792*/ 01705000
         ST    @10,PLMVSD(,@14)                                    0979 01706000
*   PLMVLNTH=NAMCOUNT+LENGTH(NAMCOUNT);/* REAL LNGTH OF PLU  @ZM46792*/ 01707000
         LA    @10,1                                               0980 01708000
         SLR   @07,@07                                             0980 01709000
         IC    @07,NAMCOUNT                                        0980 01710000
         ALR   @10,@07                                             0980 01711000
         STH   @10,PLMVLNTH(,@14)                                  0980 01712000
*   PLMVFLG1=0;                     /* INIT FLAG FIELD       @ZM46792*/ 01713000
         MVI   PLMVFLG1(@14),X'00'                                 0981 01714000
*   PLMVSCTG=ON;                    /* SET SOURCE AS CONTIGUOUS    0982 01715000
*                                                            @ZM46792*/ 01716000
         OI    PLMVSCTG(@14),B'01000000'                           0982 01717000
*   RPARM=ADDR(PLMVPARM);           /* POINT TO PARM LIST FOR MOVE 0983 01718000
*                                                            @ZM46792*/ 01719000
         LR    RPARM,@14                                           0983 01720000
*   CALL IEDMVBFR;                  /* MOVE IN SAVED PLU NAME      0984 01721000
*                                                            @ZM46792*/ 01722000
         L     @10,AVTSAVTP(,RAVT)                                 0984 01723000
         L     @15,SAVTMVBF(,@10)                                  0984 01724000
         BALR  @14,@15                                             0984 01725000
*   PLMVSD=PLMVTD;                  /* POINT AFTER PLUNAME IN BUFF 0985 01726000
*                                                            @ZM46792*/ 01727000
         LA    @14,PLMVPARM                                        0985 01728000
         L     @10,PLMVTD(,@14)                                    0985 01729000
         ST    @10,PLMVSD(,@14)                                    0985 01730000
*   PLMVSU=PLMVTU;                  /* POINT AFTER PLUNAME IN BUFFER    01731000
*                                                            @ZM46792*/ 01732000
         L     @07,PLMVTU(,@14)                                    0986 01733000
         ST    @07,PLMVSU(,@14)                                    0986 01734000
*   IF NAMCOUNT<MAXNAME THEN        /* IS PLUNAME LESS THEN 8 ?    0987 01735000
*                                                            @ZM46792*/ 01736000
         CLI   NAMCOUNT,8                                          0987 01737000
         BNL   @RF00987                                            0987 01738000
*     DO;                           /* YES                   @ZM46792*/ 01739000
*       SAVEDATA=PLMVTD;            /* SAVE DATA ADDR AFTER PLUNAME     01740000
*                                                            @ZM46792*/ 01741000
         ST    @10,SAVEDATA                                        0989 01742000
*       SAVEUNIT=PLMVTU;            /* SAVE UNIT ADDR AFTER PLUNAME     01743000
*                                                            @ZM46792*/ 01744000
         ST    @07,SAVEUNIT                                        0990 01745000
*       PLMVFLG1=0;                 /* INIT FLAG FIELD       @ZM46792*/ 01746000
         MVI   PLMVFLG1(@14),X'00'                                 0991 01747000
*       PLMVLNTH=MAXNAME-NAMCOUNT;  /* 8 - REAL LENGTH       @ZM46792*/ 01748000
         LA    @10,8                                               0992 01749000
         SLR   @07,@07                                             0992 01750000
         IC    @07,NAMCOUNT                                        0992 01751000
         SLR   @10,@07                                             0992 01752000
         STH   @10,PLMVLNTH(,@14)                                  0992 01753000
*       RPARM=ADDR(PLMVPARM);       /* POIN TO PARM LIST     @ZM46792*/ 01754000
         LR    RPARM,@14                                           0993 01755000
*       CALL IEDMVBFR;              /* SKIP OVER REST OR PLUNAME   0994 01756000
*                                                            @ZM46792*/ 01757000
         L     @10,AVTSAVTP(,RAVT)                                 0994 01758000
         L     @15,SAVTMVBF(,@10)                                  0994 01759000
         BALR  @14,@15                                             0994 01760000
*       PLMVTD=SAVEDATA;            /* RESTORE TARGET        @ZM46792*/ 01761000
         LA    @14,PLMVPARM                                        0995 01762000
         L     @10,SAVEDATA                                        0995 01763000
         ST    @10,PLMVTD(,@14)                                    0995 01764000
*       PLMVTU=SAVEUNIT;            /* RESTORE TARGET        @ZM46792*/ 01765000
         L     @10,SAVEUNIT                                        0996 01766000
         ST    @10,PLMVTU(,@14)                                    0996 01767000
*     END;                          /*                       @ZM46792*/ 01768000
*   ELSE                                                           0998 01769000
*     ;                             /*                       @ZM46792*/ 01770000
@RF00987 DS    0H                                                  0999 01771000
*   IF LASTLNTH^=(PLMVSD->NEWUSLN)+LENGTH(NEWUSLN) THEN/* OLD USER 0999 01772000
*                                      LENGTH DIFFERENT FROM NEW   0999 01773000
*                                                            @ZM47842*/ 01774000
