*   PLMVLNTH=LASTLNTH;              /* REMAINING LENGTH      @YM07298*/ 00778000
         L     @10,LASTLNTH                                        0736 00779000
         STH   @10,PLMVLNTH(,@14)                                  0736 00780000
*   PLMVFLG1=0;                     /* CLEAR FLAGS           @YM07298*/ 00781000
         MVI   PLMVFLG1(@14),X'00'                                 0737 00782000
*   PLMVUNCF=ON;                    /* LEFT OVER TARG UNITS  @YM07298*/ 00783000
         OI    PLMVUNCF(@14),B'10000000'                           0738 00784000
*   RPARM=ADDR(PLMVPARM);           /* PARM LIST ADDRESS     @YM07298*/ 00785000
         LR    RPARM,@14                                           0739 00786000
*   CALL IEDMVBFR;                  /* MOVE LAST PART BIND   @YM07298*/ 00787000
         L     @10,AVTSAVTP(,RAVT)                                 0740 00788000
         L     @15,SAVTMVBF(,@10)                                  0740 00789000
         BALR  @14,@15                                             0740 00790000
*   IF PLMVUNCA>0 THEN              /* ANY LEFT-OVER TARGET UNITS?   */ 00791000
         LA    @14,PLMVPARM                                        0741 00792000
         L     @14,PLMVUNCA(,@14)                                  0741 00793000
         LTR   @14,@14                                             0741 00794000
         BNP   @RF00741                                            0741 00795000
*     DO;                           /* YES, IEDMVBFR HAS UNCHAINED 0742 00796000
*                                      THE LEFT-OVER UNITS AND PUT AN   00797000
*                                      INVALID TIC IN THE LAST UNIT     00798000
*                                      OF THE BIND PIU'S BUFFER      */ 00799000
*       RPARM=PLMVUNCA;             /* FREE THE LEFT-OVER            */ 00800000
         LR    RPARM,@14                                           0743 00801000
*       CALL FREEBUF;               /* ... BUFFER UNITS              */ 00802000
         BAL   @14,FREEBUF                                         0744 00803000
*     END;                                                         0745 00804000
*   END BLDBIND;                    /* RETURN TO CALLER              */ 00805000
@EL00006 DS    0H                                                  0746 00806000
@EF00006 DS    0H                                                  0746 00807000
@ER00006 L     @14,@SA00006                                        0746 00808000
         BR    @14                                                 0746 00809000
         EJECT                                                          00810000
*BLDREQST:                                                         0747 00811000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0747 00812000
BLDREQST DS    0H                                                  0748 00813000
*   RFY                                                            0748 00814000
*     R14 RSTD;                                                    0748 00815000
*                                                                  0748 00816000
*/********************************************************************/ 00817000
*/*                                                                  */ 00818000
*/*      B L D R E Q S T   SUBROUTINE                                */ 00819000
*/*                                                                  */ 00820000
*/********************************************************************/ 00821000
*/*                                                                  */ 00822000
*/*      THIS SUBROUTINE BUILDS 3 DIFFERENT TYPES OF REQUEST         */ 00823000
*/*      PIUS --- SDT, CLEAR, AND UNBIND.                            */ 00824000
*/*                                                                  */ 00825000
*/********************************************************************/ 00826000
*                                                                  0749 00827000
*   PRFPIUO=1;                      /* PIU OFFSET                    */ 00828000
         LA    @10,1                                               0749 00829000
         STC   @10,PRFPIUO(,RPRF)                                  0749 00830000
*   RPIU=ADDR(PRFPIUO)+PRFPIUO;     /* START-OF-PIU ADDRESS          */ 00831000
         LA    RPIU,PRFPIUO(,RPRF)                                 0750 00832000
         ALR   RPIU,@10                                            0750 00833000
*                                   /* INITIALIZE THE TH             */ 00834000
*   TTHBYTE0=EXPFLOTH;              /* EXPEDITED FLOW                */ 00835000
         MVI   TTHBYTE0(RPIU),X'1D'                                0751 00836000
*   TTHBYTE1=0;                     /*                               */ 00837000
         MVI   TTHBYTE1(RPIU),X'00'                                0752 00838000
*   TTHDAF=TNTSLU;                  /* DESTINATION = SLU'S TNT INDEX */ 00839000
         MVC   TTHDAF(2,RPIU),TNTSLU                               0753 00840000
*   TTHOAF=SIBINDEX;                /* ORIGIN = PLU'S TNT INDEX      */ 00841000
         MVC   TTHOAF(2,RPIU),SIBINDEX(RSIB)                       0754 00842000
*   TTHTAG=0;                       /* TAG FIELD                     */ 00843000
         SLR   @10,@10                                             0755 00844000
         STCM  @10,3,TTHTAG(RPIU)                                  0755 00845000
*   PIURH=SCRH;                     /* MOVE IN THE RH                */ 00846000
         MVC   PIURH(3,RPIU),@CB02136                              0756 00847000
*   PIURU(1:1)=RPARM;               /* PIU REQUEST CODE              */ 00848000
         STC   RPARM,PIURU(,RPIU)                                  0757 00849000
*   IF RPARM=CD1UBIND THEN          /* BUILDING UNBIND PIU?          */ 00850000
         C     RPARM,@CF00701                                      0758 00851000
         BNE   @RF00758                                            0758 00852000
*     DO;                           /* YES                           */ 00853000
*       PIURU(2:2)=UBINDTYP;        /* SET UNBIND TYPE               */ 00854000
         MVI   PIURU+1(RPIU),X'01'                                 0760 00855000
*       TTHDCF=LENGTH(PIURH)+2;     /* SET BYTE COUNT                */ 00856000
         MVC   TTHDCF(2,RPIU),@CH00156                             0761 00857000
*     END;                                                         0762 00858000
*   ELSE                            /* NOT AN UNBIND PIU             */ 00859000
*     TTHDCF=LENGTH(PIURH)+1;       /* SET BYTE COUNT                */ 00860000
         B     @RC00758                                            0763 00861000
@RF00758 MVC   TTHDCF(2,RPIU),@CH00330                             0763 00862000
*   END BLDREQST;                   /* RETURN TO CALLER              */ 00863000
@EL00007 DS    0H                                                  0764 00864000
@EF00007 DS    0H                                                  0764 00865000
@ER00007 BR    @14                                                 0764 00866000
*   RFY                                                            0765 00867000
*     R14 UNRSTD;                                                  0765 00868000
         EJECT                                                          00869000
*BLDRESP:                                                          0766 00870000
*   PROC OPTIONS(SAVE(R14));                                       0766 00871000
*                                                                  0766 00872000
BLDRESP  ST    @14,@SA00008                                        0766 00873000
*/********************************************************************/ 00874000
*/*                                                                  */ 00875000
*/*      B L D R E S P   SUBROUTINE                                  */ 00876000
*/*                                                                  */ 00877000
*/********************************************************************/ 00878000
*/*                                                                  */ 00879000
*/*      THIS SUBROUTINE IS GIVEN A BUFFER CONTAINING A              */ 00880000
*/*      REQUEST PIU.  IF A RESPONSE HAS BEEN REQUESTED, A           */ 00881000
*/*      RESPONSE IS BUILT AND SENT.  OTHERWISE THE BUFFER IS        */ 00882000
*/*      FREED, AND NO RESPONSE IS SENT.                             */ 00883000
*/*                                                                  */ 00884000
*/********************************************************************/ 00885000
*                                                                  0767 00886000
*   IF(TRHDR1=ON|                   /* ANY TYPE OR RESPONSE          */ 00887000
*       TRHDR2=ON)&                 /* ... REQUESTED? &              */ 00888000
*       (TRHER=OFF|                 /* DEFINITE RESPONSE REQUESTED   */ 00889000
*       IEDSNSD>0) THEN             /* ... OR WAS THERE AN ERROR?    */ 00890000
         LA    @14,10                                              0767 00891000
         ALR   @14,RPIU                                            0767 00892000
         TM    TRHDR1(@14),B'10100000'                             0767 00893000
         BZ    @RF00767                                            0767 00894000
         TM    TRHER(@14),B'00010000'                              0767 00895000
         BZ    @RT00767                                            0767 00896000
         LA    @14,SENSE                                           0767 00897000
         CLC   IEDSNSD(4,@14),@CF00652                             0767 00898000
         BNH   @RF00767                                            0767 00899000
@RT00767 DS    0H                                                  0768 00900000
*     DO;                           /* YES, A RESPONSE MUST BE BUILT */ 00901000
*       RPARM=TTHDAF;               /* REVERSE THE                   */ 00902000
         SLR   RPARM,RPARM                                         0769 00903000
         ICM   RPARM,3,TTHDAF(RPIU)                                0769 00904000
*       TTHDAF=TTHOAF;              /* ... DAF &                     */ 00905000
         MVC   @TS00001(2),TTHOAF(RPIU)                            0770 00906000
         MVC   TTHDAF(2,RPIU),@TS00001                             0770 00907000
*       TTHOAF=RPARM;               /* ... OAF FIELDS                */ 00908000
         STCM  RPARM,3,TTHOAF(RPIU)                                0771 00909000
*       TRHTYPE=ON;                 /* MARK THIS AS A RESPONSE       */ 00910000
         OI    TRHTYPE+10(RPIU),B'10000000'                        0772 00911000
*       IF IEDSNSD=0 THEN           /* + FME RESPONSE?               */ 00912000
         LA    @14,SENSE                                           0773 00913000
         ICM   @10,15,IEDSNSD(@14)                                 0773 00914000
         BNZ   @RF00773                                            0773 00915000
*         TTHDCF=LENGTH(PIURH)+1;   /* YES, SET COUNT FOR NO SENSE   */ 00916000
         MVC   TTHDCF(2,RPIU),@CH00330                             0774 00917000
*       ELSE                        /* ERROR RESPONSE NEEDED         */ 00918000
*         DO;                                                      0775 00919000
         B     @RC00773                                            0775 00920000
@RF00773 DS    0H                                                  0776 00921000
*           TRHSDI=ON;              /* SENSE-INCLUDED                */ 00922000
         LA    @14,10                                              0776 00923000
         ALR   @14,RPIU                                            0776 00924000
         OI    TRHSDI(@14),B'00000100'                             0776 00925000
*           TRHER=ON;               /* EXCEPTION RESPONSE            */ 00926000
         OI    TRHER(@14),B'00010000'                              0777 00927000
*           PIURU(5:5)=PIURU(1:1);  /* MAKE ROOM FOR SENSE           */ 00928000
         MVC   PIURU+4(1,RPIU),PIURU(RPIU)                         0778 00929000
*           PIURU(1:4)=IEDSNSD;     /* MOVE IN SENSE DATA            */ 00930000
         LA    @14,SENSE                                           0779 00931000
         MVC   PIURU(4,RPIU),IEDSNSD(@14)                          0779 00932000
*           TTHDCF=LENGTH(PIURH)+LENGTH(IEDSNSD)+1;/* DATA COUNT   0780 00933000
*                                      INCLUDES SENSE DATA           */ 00934000
         MVC   TTHDCF(2,RPIU),@CH00044                             0780 00935000
*         END;                                                     0781 00936000
*       CALL SENDPIU;               /* SEND THE RESPONSE PIU         */ 00937000
@RC00773 BAL   @14,SENDPIU                                         0782 00938000
*     END;                                                         0783 00939000
*   ELSE                            /* NO RESPONSE WAS BUILT         */ 00940000
*     DO;                           /* THROW THE BUFFER AWAY         */ 00941000
         B     @RC00767                                            0784 00942000
@RF00767 DS    0H                                                  0785 00943000
*       RPARM=RPRF;                 /* BUFFER ADDRESS                */ 00944000
         LR    RPARM,RPRF                                          0785 00945000
*       CALL FREEBUF;               /* FREE THE BUFFER               */ 00946000
         BAL   @14,FREEBUF                                         0786 00947000
*     END;                                                         0787 00948000
*   END BLDRESP;                    /* RETURN TO CALLER              */ 00949000
@EL00008 DS    0H                                                  0788 00950000
@EF00008 DS    0H                                                  0788 00951000
@ER00008 L     @14,@SA00008                                        0788 00952000
         BR    @14                                                 0788 00953000
         EJECT                                                          00954000
*CLEARRSP:                                                         0789 00955000
*   PROC OPTIONS(SAVE(R14));                                       0789 00956000
*                                                                  0789 00957000
CLEARRSP ST    @14,@SA00009                                        0789 00958000
*/********************************************************************/ 00959000
*/*                                                                  */ 00960000
*/*      C L E A R R S P   SUBROUTINE                                */ 00961000
*/*                                                                  */ 00962000
*/********************************************************************/ 00963000
*/*                                                                  */ 00964000
*/*      THIS SUBROUTINE PROCESSES THE RESPONSE TO A 'CLEAR'         */ 00965000
*/*      PIU.                                                        */ 00966000
*/*                                                                  */ 00967000
*/********************************************************************/ 00968000
*                                                                  0790 00969000
*   IF SIBDSTAT=SIBDRPND THEN       /* DATA TRAFFIC STATE =        0790 00970000
*                                      'RESET-PENDING'?              */ 00971000
         TM    SIBDSTAT(RSIB),B'00000010'                          0790 00972000
         BNO   @RF00790                                            0790 00973000
         TM    SIBDSTAT(RSIB),B'00000001'                          0790 00974000
         BNZ   @RF00790                                            0790 00975000
*     DO;                           /* YES, PROCESS THIS RESPONSE    */ 00976000
*       SIBDSTAT=SIBDRSET;          /* DATA TRAFFIC STATE = 'RESET'  */ 00977000
         NI    SIBDSTAT(RSIB),B'11111100'                          0792 00978000
*       TNTPLU=TTHDAF;              /* GET PLU'S TNT INDEX           */ 00979000
         MVC   TNTPLU(2),TTHDAF(RPIU)                              0793 00980000
*       MHPTR=LCBMHA;               /* FIND PROPER MH QCB    @YM06085*/ 00981000
         L     @14,LCBMHA-1(,RLCB)                                 0794 00982000
