*   RLCB=QCBPLCBA;                  /* PLCB ADDRESS                  */ 01067000
         AL    @10,@CF02266                                        0819 01068000
         L     RLCB,QCBPLCBA-5(,@10)                               0819 01069000
         LA    RLCB,0(,RLCB)                                       0819 01070000
*   END CTLBLKS;                    /* RETURN TO CALLER              */ 01071000
@EL00010 DS    0H                                                  0820 01072000
@EF00010 DS    0H                                                  0820 01073000
