         SLR   WKPARM,WKPARM                                       0828 01124000
         ICM   WKPARM,3,TTHOAF(RPIU)                               0828 01125000
*   IF WKPARM<SAVTMHDX THEN         /* TNT IN TERMINAL TABLE @YM07298*/ 01126000
         L     @14,AVTSAVTP(,RAVT)                                 0829 01127000
         LH    @14,SAVTMHDX(,@14)                                  0829 01128000
         N     @14,@CF02265                                        0829 01129000
         CLR   WKPARM,@14                                          0829 01130000
         BNL   @RF00829                                            0829 01131000
*     DO;                           /* ADDRESS OF ENTRY      @YM07298*/ 01132000
*       WKADDR=WKPARM-1;            /*                       @YM07298*/ 01133000
         LR    @14,WKPARM                                          0831 01134000
         BCTR  @14,0                                               0831 01135000
         ST    @14,WKADDR                                          0831 01136000
*       WKENTLEN=TNTENLEN+LENGTH(TNTTRMAD);/*                @YM07298*/ 01137000
