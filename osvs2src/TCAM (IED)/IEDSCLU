*   PLMVFLG1=0;                     /* CLEAR FLAGS           @YM07298*/ 00707000
         MVI   PLMVFLG1(@14),X'00'                                 0714 00708000
*   RPARM=ADDR(PLMVPARM);           /* PARM LIST ADDRESS             */ 00709000
         LR    RPARM,@14                                           0715 00710000
*   CALL IEDMVBFR;                  /* SKIP TO DATA AFTER NAME       */ 00711000
         L     @10,AVTSAVTP(,RAVT)                                 0716 00712000
         L     @15,SAVTMVBF(,@10)                                  0716 00713000
         BALR  @14,@15                                             0716 00714000
*   LASTUNIT=PLMVTU;                /* UNIT WITH LAST DATA   @YM07298*/ 00715000
