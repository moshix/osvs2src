         TITLE 'IEDSCPRQ, SSCP REQUEST HANDLER                         *00001000
                        '                                               00002000
IEDSCPRQ CSECT ,                                                   0001 00003000
@PROLOG  DS    0H                                                  0002 00004000
*                                                                  0388 00005000
         USING *,RBASE                                                  00006000
*                                                                       00007000
         DC    2F'0'                QCB                                 00008000
         DC    X'06'                STCB                                00009000
         DC    AL3(*-1)                                                 00010000
*                                                                       00011000
         LR    RBASE,RQCB           SET UP BASE REGISTER                00012000
*                                                                       00013000
IEDSCPRQ IEDHJN  START,HJN                                              00014000
*/*                                                                  */ 00015000
*                                                                  0389 00016000
*                                   /* SET UP POINTER VALUES         */ 00017000
*   RPRF=RPARM;                     /* PIU'S BUFFER ADDRESS          */ 00018000
         LR    RPRF,RPARM                                          0389 00019000
*   PIUPTR=ADDR(PRFPIUO)+PRFPIUO;   /* PIU ADDRESS                   */ 00020000
         LA    @15,PRFPIUO(,IEDQPPTR)                              0390 00021000
         SLR   @10,@10                                             0390 00022000
         IC    @10,PRFPIUO(,IEDQPPTR)                              0390 00023000
         ALR   @15,@10                                             0390 00024000
         ST    @15,PIUPTR                                          0390 00025000
*   DATAPTR=ADDR(PIURU);            /* PTR TO 1ST BYTE OF RU DATA    */ 00026000
         LA    @10,PIURU(,@15)                                     0391 00027000
         ST    @10,DATAPTR                                         0391 00028000
*   DATALNTH=TTHDCF-LENGTH(IEDRH);  /* RU DATA LENGTH                */ 00029000
         SLR   @09,@09                                             0392 00030000
         ICM   @09,3,TTHDCF(@15)                                   0392 00031000
         SL    @09,@CF00065                                        0392 00032000
         STH   @09,DATALNTH                                        0392 00033000
*/*                                                                  */ 00034000
*                                                                  0393 00035000
*   IF TRHSDI=ON THEN               /* SENSE DATA INCLUDED?          */ 00036000
         TM    TRHSDI+10(@15),B'00000100'                          0393 00037000
         BNO   @RF00393                                            0393 00038000
*     DO;                           /* YES, ADJUST FOR SENSE DATA    */ 00039000
*       DATAPTR=DATAPTR+LENGTH(SNSDATA);/* RU DATA ADDRESS           */ 00040000
         LA    @15,4                                               0395 00041000
         ALR   @10,@15                                             0395 00042000
         ST    @10,DATAPTR                                         0395 00043000
*       DATALNTH=DATALNTH-LENGTH(SNSDATA);/* RU DATA LENGTH          */ 00044000
         SLR   @09,@15                                             0396 00045000
         STH   @09,DATALNTH                                        0396 00046000
*     END;                                                         0397 00047000
*                                                                  0397 00048000
*/*                                                                  */ 00049000
*                                                                  0398 00050000
*                                   /* SET UP COMMAND-TYPE           */ 00051000
*   IF TRHRUCAT=TRHFMD&             /* FM DATA RU? AND               */ 00052000
*       TRHFI=ON&                   /* ... FORMATTED? AND            */ 00053000
*       DATALNTH>=3 THEN            /* ... AT LEAST 3 DATA BYTES?    */ 00054000
@RF00393 LA    @15,10                                              0398 00055000
         AL    @15,PIUPTR                                          0398 00056000
         TM    TRHRUCAT(@15),B'01100000'                           0398 00057000
         BNZ   @RF00398                                            0398 00058000
         TM    TRHFI(@15),B'00001000'                              0398 00059000
         BNO   @RF00398                                            0398 00060000
         LH    @15,DATALNTH                                        0398 00061000
         C     @15,@CF00065                                        0398 00062000
         BL    @RF00398                                            0398 00063000
*     CMDTYPE=DATAPTR->CHAR3;       /* YES, MOVE IN COMMAND-TYPE     */ 00064000
         L     @15,DATAPTR                                         0399 00065000
         MVC   CMDTYPE(3),CHAR3(@15)                               0399 00066000
*   ELSE                                                           0400 00067000
*     CMDTYPE=0;                    /* NO, CLEAR COMMAND-TYPE        */ 00068000
         B     @RC00398                                            0400 00069000
@RF00398 SLR   @15,@15                                             0400 00070000
         STCM  @15,7,CMDTYPE                                       0400 00071000
         EJECT                                                          00072000
*                                                                  0401 00073000
*/********************************************************************/ 00074000
*/*                                                                  */ 00075000
*/*      M A I N   P R O C E S S I N G   S E G M E N T               */ 00076000
*/*                                                                  */ 00077000
*/********************************************************************/ 00078000
*                                                                  0401 00079000
*   IF CMDTYPE=TRACDATA THEN        /* RECORD-TRACE-DATA PIU?        */ 00080000
@RC00398 CLC   CMDTYPE(3),TRACDATA                                 0401 00081000
         BNE   @RF00401                                            0401 00082000
*     CALL RCRDLTRC;                /* YES                           */ 00083000
         BAL   @14,RCRDLTRC                                        0402 00084000
*/*                                                                  */ 00085000
*                                                                  0403 00086000
*   ELSE                                                           0403 00087000
*     IF CMDTYPE=MDRDATA THEN       /* RECORD-MAINT-STATISTICS PIU?  */ 00088000
         B     @RC00401                                            0403 00089000
@RF00401 CLC   CMDTYPE(3),MDRDATA                                  0403 00090000
         BNE   @RF00403                                            0403 00091000
*       CALL RCRDMDR;               /* YES                           */ 00092000
         BAL   @14,RCRDMDR                                         0404 00093000
*/*                                                                  */ 00094000
*                                                                  0405 00095000
*     ELSE                                                         0405 00096000
*       IF CMDTYPE=TESTDATA THEN    /* RECORD-TEST-DATA PIU?         */ 00097000
         B     @RC00403                                            0405 00098000
@RF00403 CLC   CMDTYPE(3),TESTDATA                                 0405 00099000
         BNE   @RF00405                                            0405 00100000
*         CALL RCRDTEST;            /* YES                           */ 00101000
         BAL   @14,RCRDTEST                                        0406 00102000
*/*                                                                  */ 00103000
*                                                                  0407 00104000
*       ELSE                                                       0407 00105000
*         IF CMDTYPE=EXITSLDN THEN  /* EXIT-SLOWDOWN PIU?            */ 00106000
         B     @RC00405                                            0407 00107000
@RF00405 CLC   CMDTYPE(3),EXITSLDN                                 0407 00108000
         BNE   @RF00407                                            0407 00109000
*           CALL SLOWEXIT;          /* YES                           */ 00110000
         BAL   @14,SLOWEXIT                                        0408 00111000
*/*                                                                  */ 00112000
*                                                                  0409 00113000
*         ELSE                      /* PROCESS ALL OTHER PIU TYPES   */ 00114000
*           CALL WAITCHN;           /* INSERT ON WAIT CHAIN FOR    0409 00115000
*                                      NORMAL SSCP INPUT HANDLING    */ 00116000
         B     @RC00407                                            0409 00117000
@RF00407 BAL   @14,WAITCHN                                         0409 00118000
*/*                                                                  */ 00119000
*                                                                  0410 00120000
*   CALL DSPDISP;                   /* EXIT TO DISPATCHER (NO RETURN)*/ 00121000
@RC00407 DS    0H                                                  0410 00122000
@RC00405 DS    0H                                                  0410 00123000
@RC00403 DS    0H                                                  0410 00124000
@RC00401 LA    @15,DSPDISP(,RDISP)                                 0410 00125000
         BALR  @14,@15                                             0410 00126000
*/*                                                                  */ 00127000
*/*************   E N D   O F   M A I N   S E G M E N T   ************/ 00128000
*                                                                  0411 00129000
         EJECT                                                          00130000
*                                                                  0411 00131000
*/********************************************************************/ 00132000
*/*                                                                  */ 00133000
*/*      R C R D L T R C   SUBROUTINE                                */ 00134000
*/*                                                                  */ 00135000
*/********************************************************************/ 00136000
*/*                                                                  */ 00137000
*/*      THIS SUBROUTINE PROCESSES A RECORD-TRACE-DATA PIU.          */ 00138000
*/*      THE PIU IS PASSED TO THE LINE TRACE ROUTINE (IEDNLT)        */ 00139000
*/*      TO BE RECORDED.  UPON RETURN FROM THAT ROUTINE THERE ARE    */ 00140000
*/*      2 POSSIBILITIES:                                            */ 00141000
*/*                                                                  */ 00142000
*/*        A. IF THE RETURN CODE IS GOOD NO FURTHER PROCESSING IS    */ 00143000
*/*           NEEDED.  THE PIU BUFFER IS SIMPLY THROWN AWAY.         */ 00144000
*/*                                                                  */ 00145000
*/*        B. IF THE RETURN CODE IS BAD THE PIU MUST BE PASSED       */ 00146000
*/*           THRU THE MH SO THAT IEDQNX CAN ISSUE AN OPERATOR       */ 00147000
*/*           AWARNESS MESSAGE.  INSERT THE PIU ON THE WAIT          */ 00148000
*/*           CHAIN FOR NORMAL SSCP-FLOW INPUT PROCESSING.           */ 00149000
*/*                                                                  */ 00150000
*/********************************************************************/ 00151000
*                                                                  0411 00152000
*RCRDLTRC:                                                         0411 00153000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0411 00154000
*                                                                  0411 00155000
@EL00001 DS    0H                                                  0411 00156000
@EF00001 DS    0H                                                  0411 00157000
@ER00001 BR    @14                                                 0411 00158000
RCRDLTRC DS    0H                                                  0412 00159000
*/*                                                                  */ 00160000
*                                                                  0412 00161000
*   R14SAVE1=RE;                    /* SAVE RETURN ADDRESS           */ 00162000
         ST    RE,R14SAVE1                                         0412 00163000
*/*                                                                  */ 00164000
*                                                                  0413 00165000
*   RPARM=RPRF;                     /* PIU'S BUFFER ADDRESS          */ 00166000
         LR    RPARM,RPRF                                          0413 00167000
*   CALL IEDNLT;                    /* PASS PIU TO LINE-TRACE RTN    */ 00168000
         L     @10,AVTSAVTP(,IEDQAPTR)                             0414 00169000
         L     @15,SAVTRNTA(,@10)                                  0414 00170000
         BALR  @14,@15                                             0414 00171000
*/*                                                                  */ 00172000
*                                                                  0415 00173000
*   IF RF=0 THEN                    /* GOOD RETURN CODE?             */ 00174000
         LTR   RF,RF                                               0415 00175000
         BNZ   @RF00415                                            0415 00176000
*     CALL XFREEBFR;                /* YES, FREE THE BUFFER          */ 00177000
         BAL   @14,XFREEBFR                                        0416 00178000
*   ELSE                                                           0417 00179000
*     CALL WAITCHN;                 /* NO, CONTINUE WITH NORMAL    0417 00180000
*                                      SSCP-FLOW INPUT PROCESSING    */ 00181000
         B     @RC00415                                            0417 00182000
@RF00415 BAL   @14,WAITCHN                                         0417 00183000
*/*                                                                  */ 00184000
*                                                                  0418 00185000
*   RE=R14SAVE1;                    /* RESTORE RETURN ADDRESS        */ 00186000
@RC00415 L     RE,R14SAVE1                                         0418 00187000
*   END RCRDLTRC;                   /* RETURN TO CALLER              */ 00188000
@EL00002 DS    0H                                                  0419 00189000
@EF00002 DS    0H                                                  0419 00190000
@ER00002 BR    @14                                                 0419 00191000
         EJECT                                                          00192000
*                                                                  0420 00193000
*/********************************************************************/ 00194000
*/*                                                                  */ 00195000
*/*      R C R D M D R   SUBROUTINE                                  */ 00196000
*/*                                                                  */ 00197000
*/********************************************************************/ 00198000
*/*                                                                  */ 00199000
*/*      THIS SUBROUTINE PROCESSES A RECORD-MAINTENANCE-STATISTICS   */ 00200000
*/*      PIU.  THE PIU IS ADDED TO THE TOP OF THE MDR CHAIN          */ 00201000
*/*      ANCHORED IN THE 3705'S LCB, AND NO FURTHER PROCESSING       */ 00202000
*/*      IS DONE IN THIS MODULE.  (THE MDR DATA WILL BE RECORDED     */ 00203000
*/*      LATER WHEN THE NEXT CHANNEL INTERRUPT CAUSES AN ERP         */ 00204000
*/*      MODULE TO PROCESS THE MDR CHAIN.)                           */ 00205000
*/*                                                                  */ 00206000
*/********************************************************************/ 00207000
*                                                                  0420 00208000
*RCRDMDR:                                                          0420 00209000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0420 00210000
*                                                                  0420 00211000
RCRDMDR  DS    0H                                                  0421 00212000
*/*                                                                  */ 00213000
*                                                                  0421 00214000
*   R14SAVE1=RE;                    /* SAVE RETURN ADDRESS           */ 00215000
         ST    RE,R14SAVE1                                         0421 00216000
*/*                                                                  */ 00217000
*                                                                  0422 00218000
*   CALL X3705CB;                   /* GET 3705'S LCB & TTE ADDR     */ 00219000
         BAL   @14,X3705CB                                         0422 00220000
*/*                                                                  */ 00221000
*                                                                  0423 00222000
*                                   /* INSERT PIU AT TOP OF MDR CHAIN*/ 00223000
*   RPARM=LCBOBRCH;                 /* CURRENT 1ST PIU ON CHAIN      */ 00224000
         L     RPARM,LCBOBRCH(,IEDQLPTR)                           0423 00225000
*CSLOOP:                                                           0424 00226000
*   PRFLINK=RPARM;                  /* LINK NEW PIU TO 1ST PIU       */ 00227000
CSLOOP   STCM  RPARM,7,PRFLINK(IEDQPPTR)                           0424 00228000
*   CS(RPARM,RPRF,LCBOBRCH);        /* TRY TO ADD NEW PIU TO CHAIN   */ 00229000
         CS    RPARM,@06,LCBOBRCH(IEDQLPTR)                        0425 00230000
*   BC(NOTZERO,CSLOOP);             /* BRANCH IF NOT SUCCESSFUL      */ 00231000
         BC    7,CSLOOP                                            0426 00232000
*/*                                                                  */ 00233000
*                                                                  0427 00234000
*   RE=R14SAVE1;                    /* RESTORE RETURN ADDRESS        */ 00235000
         L     RE,R14SAVE1                                         0427 00236000
*   END RCRDMDR;                    /* RETURN TO CALLER              */ 00237000
@EL00003 DS    0H                                                  0428 00238000
@EF00003 DS    0H                                                  0428 00239000
@ER00003 BR    @14                                                 0428 00240000
         EJECT                                                          00241000
*                                                                  0429 00242000
*/********************************************************************/ 00243000
*/*                                                                  */ 00244000
*/*      R C R D T E S T   SUBROUTINE                                */ 00245000
*/*                                                                  */ 00246000
*/********************************************************************/ 00247000
*/*                                                                  */ 00248000
*/*      THIS SUBROUTINE PROCESSES A RECORD-TEST-DATA PIU.           */ 00249000
*/*      THE PIU IS CONVERTED INTO A BUFFER AND IS THEN TPOSTED      */ 00250000
*/*      TO TOTE.  NO FURTHER PROCESSING IS DONE BY THIS MODULE.     */ 00251000
*/*                                                                  */ 00252000
*/********************************************************************/ 00253000
*                                                                  0429 00254000
*RCRDTEST:                                                         0429 00255000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0429 00256000
*                                                                  0429 00257000
RCRDTEST DS    0H                                                  0430 00258000
*/*                                                                  */ 00259000
*                                                                  0430 00260000
*   R14SAVE1=RE;                    /* SAVE RETURN ADDRESS           */ 00261000
         ST    RE,R14SAVE1                                         0430 00262000
*/*                                                                  */ 00263000
*                                                                  0431 00264000
*                                   /* CONVERT PIU INTO A BUFFER     */ 00265000
*   RPARM=RPRF;                     /* PIU'S BFR UNIT ADDRESS        */ 00266000
         LR    RPARM,RPRF                                          0431 00267000
*   TRHBC=ON;                       /* SET CHAINING BITS TO GIVE     */ 00268000
*   TRHEC=ON;                       /* ... HEADER LAST BUFFER        */ 00269000
         L     @15,PIUPTR                                          0433 00270000
         OI    TRHBC+10(@15),B'00000011'                           0433 00271000
*   CALL IEDXFMI;                   /* DO THE CONVERSION             */ 00272000
         L     @10,AVTSAVTP(,IEDQAPTR)                             0434 00273000
         L     @15,SAVTXFMI(,@10)                                  0434 00274000
         BALR  @14,@15                                             0434 00275000
*/*                                                                  */ 00276000
*                                                                  0435 00277000
*                                   /* TPOST THE BUFFER TO TOTE      */ 00278000
*   PRFQCBA=ADDR(AVTOLTQB);         /* QCB ADDRESS                   */ 00279000
         LA    @15,AVTOLTQB(,IEDQAPTR)                             0435 00280000
         STCM  @15,7,PRFQCBA(IEDQPPTR)                             0435 00281000
*   PRFPRI=PRIONLT;                 /* PRIORITY                      */ 00282000
         MVI   PRFPRI(IEDQPPTR),X'DC'                              0436 00283000
*   PRFKEY='20'X;                   /* SET KEY FOR TOTE              */ 00284000
         MVI   PRFKEY(IEDQPPTR),X'20'                              0437 00285000
*   RPARM=RPRF;                     /* BUFFER ADDRESS                */ 00286000
         LR    RPARM,RPRF                                          0438 00287000
*   CALL DSPPOSTR;                  /* TPOST THE BUFFER              */ 00288000
         LA    @15,DSPPOSTR(,RDISP)                                0439 00289000
         BALR  @14,@15                                             0439 00290000
*/*                                                                  */ 00291000
*                                                                  0440 00292000
*   RE=R14SAVE1;                    /* RESTORE RETURN ADDRESS        */ 00293000
         L     RE,R14SAVE1                                         0440 00294000
*   END RCRDTEST;                   /* RETURN TO CALLER              */ 00295000
@EL00004 DS    0H                                                  0441 00296000
@EF00004 DS    0H                                                  0441 00297000
@ER00004 BR    @14                                                 0441 00298000
         EJECT                                                          00299000
*                                                                  0442 00300000
*/********************************************************************/ 00301000
*/*                                                                  */ 00302000
*/*      S L O W E X I T   SUBROUTINE                                */ 00303000
*/*                                                                  */ 00304000
*/********************************************************************/ 00305000
*/*                                                                  */ 00306000
*/*      THIS SUBROUTINE PROCESSES AN EXIT-SLOWDOWN PIU.             */ 00307000
*/*      THE SLOWDOWN BIT IN THE 3705'S TTE IS TURNED OFF.           */ 00308000
*/*      THEN, IF THE 3705'S LCB IS MARKED 'FREE', IT IS TPOSTED     */ 00309000
*/*      TO ITSELF TO PRIME THE SCHEDULERS.                          */ 00310000
*/*      THE PIU IS THEN PUT ON THE WAIT CHAIN FOR NORMAL SSCP-FLOW  */ 00311000
*/*      INPUT PROCESSING.                                           */ 00312000
*/*                                                                  */ 00313000
*/********************************************************************/ 00314000
*                                                                  0442 00315000
*SLOWEXIT:                                                         0442 00316000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0442 00317000
*                                                                  0442 00318000
SLOWEXIT DS    0H                                                  0443 00319000
*/*                                                                  */ 00320000
*                                                                  0443 00321000
*   R14SAVE1=RE;                    /* SAVE RETURN ADDRESS           */ 00322000
         ST    RE,R14SAVE1                                         0443 00323000
*/*                                                                  */ 00324000
*                                                                  0444 00325000
*   CALL X3705CB;                   /* GET 3705'S LCB & TTE ADDR     */ 00326000
         BAL   @14,X3705CB                                         0444 00327000
*/*                                                                  */ 00328000
*                                                                  0445 00329000
*   TRMSLOWN=OFF;                   /* TURN OFF SLOWDOWN BIT         */ 00330000
         LR    @05,IEDQTPTR                                        0445 00331000
         AL    @05,@CF01981                                        0445 00332000
         LA    @09,TRMSLOWN-1(,@05)                                0445 00333000
         LR    @07,@09                                             0445 00334000
         LA    @15,3                                               0445 00335000
         NR    @07,@15                                             0445 00336000
         LCR   @07,@07                                             0445 00337000
         ALR   @09,@07                                             0445 00338000
         L     @15,0(,@09)                                         0445 00339000
@GL00003 LR    @10,@15                                             0445 00340000
         N     @10,@CB01977+3(@07)                                 0445 00341000
         CS    @15,@10,0(@09)                                      0445 00342000
         BNE   @GL00003                                            0445 00343000
*/*                                                                  */ 00344000
*                                                                  0446 00345000
*   IF LCBFREEN=ON THEN             /* LCB 'FREE'?                   */ 00346000
         TM    LCBFREEN(IEDQLPTR),B'00000100'                      0446 00347000
         BNO   @RF00446                                            0446 00348000
*     DO;                           /* YES                           */ 00349000
*       TS(LCBTSTSW);               /* TEST & SET TO GET USE OF LCB  */ 00350000
         TS    LCBTSTSW(IEDQLPTR)                                  0448 00351000
*       BC(NOTZERO,NOTFREE);        /* BRANCH IF NOT SUCCESSFUL      */ 00352000
         BC    7,NOTFREE                                           0449 00353000
*       LCBFREEN=OFF;               /* MARK THE LCB ...              */ 00354000
*       LCBRECVN=ON;                /* ... 'RECEIVING'               */ 00355000
         OI    LCBRECVN(IEDQLPTR),B'00000010'                      0451 00356000
         NI    LCBFREEN(IEDQLPTR),B'11111011'                      0451 00357000
*/*                                                                  */ 00358000
*                                                                  0452 00359000
*                                   /* TPOST THE LCB TO ITSELF       */ 00360000
*       LCBQCBA=RLCB;               /* QCB ADDRESS                   */ 00361000
         STCM  RLCB,7,LCBQCBA(IEDQLPTR)                            0452 00362000
*       LCBPRI=PRILNFRE;            /* PRIORITY                      */ 00363000
         MVI   LCBPRI(IEDQLPTR),X'E0'                              0453 00364000
*       RPARM=RLCB;                 /* ELEMENT ADDRESS FOR TPOST     */ 00365000
         LR    RPARM,RLCB                                          0454 00366000
*       CALL DSPPOSTR;              /* TPOST THE LCB TO ITSELF       */ 00367000
         LA    @15,DSPPOSTR(,RDISP)                                0455 00368000
         BALR  @14,@15                                             0455 00369000
*/*                                                                  */ 00370000
*                                                                  0456 00371000
*       LCBTSTSW=0;                 /* CLEAR THE TEST-&-SET SWITCH   */ 00372000
         MVI   LCBTSTSW(IEDQLPTR),X'00'                            0456 00373000
*     END;                                                         0457 00374000
*                                                                  0457 00375000
*/*                                                                  */ 00376000
*                                                                  0458 00377000
*NOTFREE:                                                          0458 00378000
*   CALL WAITCHN;                   /* PROCEED WITH NORMAL SSCP-FLOW    00379000
*                                      INPUT PROCESSING              */ 00380000
@RF00446 DS    0H                                                  0458 00381000
NOTFREE  BAL   @14,WAITCHN                                         0458 00382000
*/*                                                                  */ 00383000
*                                                                  0459 00384000
*   RE=R14SAVE1;                    /* RESTORE RETURN ADDRESS        */ 00385000
         L     RE,R14SAVE1                                         0459 00386000
*   END SLOWEXIT;                   /* RETURN TO CALLER              */ 00387000
@EL00005 DS    0H                                                  0460 00388000
@EF00005 DS    0H                                                  0460 00389000
@ER00005 BR    @14                                                 0460 00390000
         EJECT                                                          00391000
*                                                                  0461 00392000
*/********************************************************************/ 00393000
*/*                                                                  */ 00394000
*/*      W A I T C H N   SUBROUTINE                                  */ 00395000
*/*                                                                  */ 00396000
*/********************************************************************/ 00397000
*/*                                                                  */ 00398000
*/*      THIS SUBROUTINE HANDLES ANY PIU THAT IS TO BE PASSED        */ 00399000
*/*      ON THRU THE SSCP INPUT MH.  THE PIU IS ADDED TO THE         */ 00400000
*/*      WAIT CHAIN ON THE SSCP'S PLCB.  THEN, IF THE PLCB IS        */ 00401000
*/*      MARKED 'FREE', IT IS TPOSTED TO ITSELF TO PRIME THE         */ 00402000
*/*      SCHEDULERS.                                                 */ 00403000
*/*                                                                  */ 00404000
*/********************************************************************/ 00405000
*                                                                  0461 00406000
*WAITCHN:                                                          0461 00407000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0461 00408000
*                                                                  0461 00409000
WAITCHN  DS    0H                                                  0462 00410000
*/*                                                                  */ 00411000
*                                                                  0462 00412000
*   R14SAVE2=RE;                    /* SAVE RETURN ADDRESS           */ 00413000
         ST    RE,R14SAVE2                                         0462 00414000
*/*                                                                  */ 00415000
*                                                                  0463 00416000
*   RLCB=SAVTSLCB;                  /* SSCP'S PLCB ADDRESS           */ 00417000
         L     @15,AVTSAVTP(,IEDQAPTR)                             0463 00418000
         L     RLCB,SAVTSLCB(,@15)                                 0463 00419000
*   PRFLINK=0;                      /* SET END-OF-CHAIN INDICATOR    */ 00420000
         SLR   @15,@15                                             0464 00421000
         STCM  @15,7,PRFLINK(IEDQPPTR)                             0464 00422000
*/*                                                                  */ 00423000
*                                                                  0465 00424000
*                                   /* ADD NEW PIU TO WAIT CHAIN     */ 00425000
*   IF LCBWAITC=0 THEN              /* WAIT CHAIN EMPTY?             */ 00426000
         L     @15,LCBWAITC-1(,IEDQLPTR)                           0465 00427000
         LA    @15,0(,@15)                                         0465 00428000
         LTR   @15,@15                                             0465 00429000
         BNZ   @RF00465                                            0465 00430000
*     LCBWAITC=RPRF;                /* YES, ADD NEW PIU AT TOP       */ 00431000
         STCM  RPRF,7,LCBWAITC(IEDQLPTR)                           0466 00432000
*   ELSE                                                           0467 00433000
*     DO;                           /* NO, ADD NEW PIU AT END        */ 00434000
         B     @RC00465                                            0467 00435000
@RF00465 DS    0H                                                  0468 00436000
*       RPARM=LCBWAITC;             /* 1ST PIU ON CHAIN              */ 00437000
         L     RPARM,LCBWAITC-1(,IEDQLPTR)                         0468 00438000
         LA    RPARM,0(,RPARM)                                     0468 00439000
*       DO WHILE(RPARM->PRFLINK^=0);/* FIND END OF CHAIN             */ 00440000
         B     @DE00469                                            0469 00441000
