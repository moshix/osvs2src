*/*      X 3 7 0 5 C B   SUBROUTINE                                  */ 00538000
*/*                                                                  */ 00539000
*/********************************************************************/ 00540000
*/*                                                                  */ 00541000
*/*      THIS SUBROUTINE FINDS THE ADDRESSES OF THE LCB AND          */ 00542000
*/*      TTE CONTROL BLOCKS FOR THE 3705 AND PLACES THEM IN          */ 00543000
*/*      RLCB AND RTRM.  (THE SUBROUTINE ASSUMES THAT PIUPTR         */ 00544000
*/*      POINTS TO A PIU WHOSE ORIGIN IS THE NCP WHOSE CONTROL       */ 00545000
*/*      BLOCK ADDRESSES ARE WANTED.)                                */ 00546000
*/*                                                                  */ 00547000
*/********************************************************************/ 00548000
*                                                                  0494 00549000
*X3705CB:                                                          0494 00550000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0494 00551000
*                                                                  0494 00552000
X3705CB  DS    0H                                                  0495 00553000
*/*                                                                  */ 00554000
*                                                                  0495 00555000
*   R14SAVE2=RE;                    /* SAVE RETURN ADDRESS           */ 00556000
         ST    RE,R14SAVE2                                         0495 00557000
*/*                                                                  */ 00558000
*                                                                  0496 00559000
*                                   /* GET TERMINAL ENTRY ADDRESS    */ 00560000
*   RPARM=TTHOAF;                   /* TNT INDEX                     */ 00561000
         L     @15,PIUPTR                                          0496 00562000
         SLR   RPARM,RPARM                                         0496 00563000
         ICM   RPARM,3,TTHOAF(@15)                                 0496 00564000
*   CALL IEDQTNT;                   /* CONVERT TNT INDEX TO TTE ADDR */ 00565000
         L     @15,AVTRNMPT(,IEDQAPTR)                             0497 00566000
         BALR  @14,@15                                             0497 00567000
*   RTRM=RPARM;                     /* TTE ADDRESS                   */ 00568000
         LR    RTRM,RPARM                                          0498 00569000
*/*                                                                  */ 00570000
*                                                                  0499 00571000
*   RLCB=DCBIOBAD+DCBEIOBX-LCBPRFSZ;/* LCB ADDR FOR 3705             */ 00572000
         L     @15,TRMDESTQ-1(,IEDQTPTR)                           0499 00573000
         LA    @10,20                                              0499 00574000
         L     @15,QCBDCBAD-1(,@15)                                0499 00575000
         LA    @15,0(,@15)                                         0499 00576000
         ALR   @10,@15                                             0499 00577000
         SLR   RLCB,RLCB                                           0499 00578000
         IC    RLCB,DCBEIOBX(,@10)                                 0499 00579000
         AL    RLCB,DCBIOBAD(,@10)                                 0499 00580000
         SL    RLCB,@CF00986                                       0499 00581000
*/*                                                                  */ 00582000
*                                                                  0500 00583000
*   RE=R14SAVE2;                    /* RESTORE RETURN ADDRESS        */ 00584000
