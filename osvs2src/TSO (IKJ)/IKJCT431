*   ECDAPTR=R1;                     /* SAVE ADDRESS OF COMMON DATA   */ 00024000
         LR    ECDAPTR,R1                                          0093 00025000
*   RFY                                                            0094 00026000
*     R1 UNRSTD;                                                   0094 00027000
*   LINELNG=RECLNG-CON4;            /* LINE LENGTH FOR FIRST RECORD  */ 00028000
