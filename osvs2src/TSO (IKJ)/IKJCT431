         ST    @15,VALEND                                          0960 03229000
*               VALSTR=CON0;                                       0961 03230000
         ST    @15,VALSTR                                          0961 03231000
*             END;                                                 0962 03232000
*         END;                                                     0963 03233000
*                                                                  0963 03234000
*       /*************************************************************/ 03235000
*       /*                                                           */ 03236000
*       /* CHARACTER STRING VALUE PROCESSING                         */ 03237000
*       /*                                                           */ 03238000
*       /*************************************************************/ 03239000
*                                                                  0964 03240000
*       ELSE                                                       0964 03241000
*         DO;                                                      0964 03242000
         B     @RC00912                                            0964 03243000
@RF00912 DS    0H                                                  0965 03244000
*           VALSTR=ICTR;            /* SAVE VALUE START              */ 03245000
         ST    ICTR,VALSTR                                         0965 03246000
*           CYCLE=CON1;                                            0966 03247000
         LA    CYCLE,1                                             0966 03248000
*           DO ICTR=ICTR TO LINELNG WHILE CYCLE=CON1;              0967 03249000
         B     @DE00967                                            0967 03250000
@DL00967 LA    @08,1                                               0967 03251000
         CR    CYCLE,@08                                           0967 03252000
         BNE   @DC00967                                            0967 03253000
*             IF RECORD(ICTR)=LFPAREN THEN/* IF LEFT PAREN FOUND UP  */ 03254000
         L     @03,ECDAIREC(,ECDAPTR)                              0968 03255000
         ALR   @03,ICTR                                            0968 03256000
         CLI   RECORD-1(@03),C'('                                  0968 03257000
         BNE   @RF00968                                            0968 03258000
*               LFPARCTR=LFPARCTR+CON1;/* COUNT                      */ 03259000
         ALR   LFPARCTR,@08                                        0969 03260000
*             ELSE                                                 0970 03261000
*               IF RECORD(ICTR)=RTPAREN THEN/* IF THIS IS A RIGHT    */ 03262000
         B     @RC00968                                            0970 03263000
@RF00968 L     @08,ECDAIREC(,ECDAPTR)                              0970 03264000
         ALR   @08,ICTR                                            0970 03265000
         CLI   RECORD-1(@08),C')'                                  0970 03266000
         BNE   @RF00970                                            0970 03267000
*                 DO;               /* PAREN THEN DETERMINE IF WE ARE   03268000
*                                      AT END OF THE VALUE           */ 03269000
*                   LFPARCTR=LFPARCTR-CON1;                        0972 03270000
         BCTR  LFPARCTR,0                                          0972 03271000
*                   IF LFPARCTR=CON0 THEN                          0973 03272000
         LTR   LFPARCTR,LFPARCTR                                   0973 03273000
         BNZ   @RF00973                                            0973 03274000
*                     DO;                                          0974 03275000
*                       VALEND=ICTR-CON1;                          0975 03276000
         LR    @08,ICTR                                            0975 03277000
         BCTR  @08,0                                               0975 03278000
         ST    @08,VALEND                                          0975 03279000
*                       CYCLE=CON0;                                0976 03280000
         SLR   CYCLE,CYCLE                                         0976 03281000
*                     END;                                         0977 03282000
*                 END;                                             0978 03283000
*               ELSE                /* IF THIS IS A DELIMITER THEN   */ 03284000
*                 DO;               /* IT IS END OF THE VALUE        */ 03285000
         B     @RC00970                                            0979 03286000
@RF00970 DS    0H                                                  0980 03287000
*                   IF RECORD(ICTR)=BLANK|RECORD(ICTR)=COMMA|RECORD(    03288000
*                       ICTR)=TAB|(RECORD(ICTR)=SLASH&(ICTR<LINELNG&    03289000
*                       RECORD(ICTR:ICTR+CON1)=SLASHAST)) THEN     0980 03290000
         L     @08,ECDAIREC(,ECDAPTR)                              0980 03291000
         LR    @03,@08                                             0980 03292000
         ALR   @03,ICTR                                            0980 03293000
         CLI   RECORD-1(@03),C' '                                  0980 03294000
         BE    @RT00980                                            0980 03295000
         LR    @03,@08                                             0980 03296000
         ALR   @03,ICTR                                            0980 03297000
         CLI   RECORD-1(@03),C','                                  0980 03298000
         BE    @RT00980                                            0980 03299000
         LR    @03,@08                                             0980 03300000
         ALR   @03,ICTR                                            0980 03301000
         CLI   RECORD-1(@03),X'05'                                 0980 03302000
         BE    @RT00980                                            0980 03303000
         LR    @03,@08                                             0980 03304000
         ALR   @03,ICTR                                            0980 03305000
         CLI   RECORD-1(@03),C'/'                                  0980 03306000
         BNE   @RF00980                                            0980 03307000
         C     ICTR,LINELNG                                        0980 03308000
         BNL   @RF00980                                            0980 03309000
         ALR   @08,ICTR                                            0980 03310000
         CLC   RECORD-1(2,@08),@CC01353                            0980 03311000
         BNE   @RF00980                                            0980 03312000
@RT00980 DS    0H                                                  0981 03313000
*                     DO;                                          0981 03314000
*                       CYCLE=CON0;                                0982 03315000
         SLR   CYCLE,CYCLE                                         0982 03316000
*                       VALEND=ICTR-CON1;                          0983 03317000
         LR    @08,ICTR                                            0983 03318000
         BCTR  @08,0                                               0983 03319000
         ST    @08,VALEND                                          0983 03320000
*                       CALL SKIPSEP;                              0984 03321000
         BAL   @14,SKIPSEP                                         0984 03322000
*                       IF ICTR<=LINELNG THEN                      0985 03323000
         C     ICTR,LINELNG                                        0985 03324000
         BH    @RF00985                                            0985 03325000
*                         DO;       /* IF NO ENDING PAREN COULD BE   */ 03326000
*                           IF RECORD(ICTR)^=RTPAREN THEN/* THEN   0987 03327000
*                                      NOTIFY                        */ 03328000
         L     @08,ECDAIREC(,ECDAPTR)                              0987 03329000
         LR    @03,@08                                             0987 03330000
         ALR   @03,ICTR                                            0987 03331000
         CLI   RECORD-1(@03),C')'                                  0987 03332000
         BE    @RF00987                                            0987 03333000
*                             DO;   /* USER AND RETURN TO SYNTAX ANY */ 03334000
*                               EXMSGID=M508;/* REMAINING PARMS      */ 03335000
         MVC   EXMSGID(4),@CC01387                                 0989 03336000
*                               MVAR(1)=PARMADR;                   0990 03337000
         L     @03,PARMADR                                         0990 03338000
