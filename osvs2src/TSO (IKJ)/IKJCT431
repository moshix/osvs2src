*   /*****************************************************************/ 03386000
*   /*                                                               */ 03387000
*   /* MSGRTN - THE MESSAGE ROUTINE BUILDS THE IKJEFF02 PARAMETER    */ 03388000
*   /* LIST AND THEN INVOKES IKJEFF02 TO ISSUE A MESSAGE             */ 03389000
*   /*                                                               */ 03390000
*   /*****************************************************************/ 03391000
*                                                                  1004 03392000
*MSGRTN:                                                           1004 03393000
*   PROC OPTIONS(SAVEAREA);                                        1004 03394000
MSGRTN   STM   @14,@12,12(@13)                                     1004 03395000
         ST    @13,@SA00009+4                                      1004 03396000
         LA    @14,@SA00009                                        1004 03397000
         ST    @14,8(,@13)                                         1004 03398000
         LR    @13,@14                                             1004 03399000
*   MSGTABLE=MSGTABLE&&MSGTABLE;    /* INITIALIZE IKJEFF02 PARMLIST  */ 03400000
         L     @06,ERRPTR                                          1005 03401000
         XC    MSGTABLE(52,@06),MSGTABLE(@06)                      1005 03402000
*   LISTPTR=ADDR(MSGCSECT);         /* START OF MESSAGE PARMS        */ 03403000
         LA    @15,MSGCSECT(,@06)                                  1006 03404000
         ST    @15,LISTPTR(,@06)                                   1006 03405000
*   TMCTPTR=CPPLPTR;                /* ADDRESS OF THE CPPL           */ 03406000
         L     @15,CPPLPTR(,ECDAPTR)                               1007 03407000
         ST    @15,TMCTPTR(,@06)                                   1007 03408000
*   ECBPTR=ADDR(ECB);               /* DUMMY ECB ADDRESS             */ 03409000
         LA    @15,ECB                                             1008 03410000
         ST    @15,ECBPTR(,@06)                                    1008 03411000
*   MSGCSECT=ADDR(IKJCT435);        /* IKJCT435 IS MESSAGE CSECT     */ 03412000
         L     @15,@CV01232                                        1009 03413000
         ST    @15,MSGCSECT(,@06)                                  1009 03414000
*   MTPUTLSW=YES;                   /* PUTLINE MESSAGE REUESTED      */ 03415000
*   MTHEXSW=YES;                    /* PRINT RETURN CODES IN HEX     */ 03416000
         OI    MTPUTLSW(@06),B'01010000'                           1011 03417000
*   MSGID=EXMSGID;                  /* ID OF THE MESSAGE TO BE ISSUED*/ 03418000
         MVC   MSGID(4,@06),EXMSGID                                1012 03419000
*   DO MCTR=1 TO DIM(MVAR);         /* INITIALIZE INSERTION VARIABLES*/ 03420000
         LA    MCTR,1                                              1013 03421000
@DL01013 DS    0H                                                  1014 03422000
*     MTINSRT(MCTR)=MVAR(MCTR);     /* MVAR HAS BEEN PREVIOUSLY SET  */ 03423000
         LR    @06,MCTR                                            1014 03424000
         SLA   @06,2                                               1014 03425000
         L     @03,MVAR-4(@06)                                     1014 03426000
         L     @01,ERRPTR                                          1014 03427000
         ST    @03,MTINSRT-4(@06,@01)                              1014 03428000
*   END;                                                           1015 03429000
         AL    MCTR,@CF00035                                       1015 03430000
         C     MCTR,@CF00064                                       1015 03431000
         BNH   @DL01013                                            1015 03432000
*   RFY                                                            1016 03433000
*     R1 RSTD;                                                     1016 03434000
*   R1=ERRPTR;                      /* ADDRESS OF MSGTABLE           */ 03435000
         L     R1,ERRPTR                                           1017 03436000
*   RFY                                                            1018 03437000
*     R1 UNRSTD;                                                   1018 03438000
*   DO;                             /* LINK EP(IKJEFF02)             */ 03439000
*     I00601='00'X;                 /* EP/DE FLAG                    */ 03440000
         MVI   I00601,X'00'                                        1020 03441000
*     I00602=ADDR(IKJEFF02);        /* ADDR OF EP/DE                 */ 03442000
         LA    @06,@CC01290                                        1021 03443000
         STCM  @06,7,I00602                                        1021 03444000
*     I00604=0;                     /* DCB PTR                       */ 03445000
