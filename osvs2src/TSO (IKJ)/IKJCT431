*                               MVARLEN(1)=ADDR(RECORD(ICTR))-PARMADR+  03340000
*                                   CON1;                          0991 03341000
         LA    @08,RECORD-1(ICTR,@08)                              0991 03342000
         SLR   @08,@03                                             0991 03343000
         AL    @08,@CF00035                                        0991 03344000
         STC   @08,MVARLEN                                         0991 03345000
*                               NOTEXEC=YES;                       0992 03346000
         OI    NOTEXEC(ECDAPTR),B'01000000'                        0992 03347000
*                               CALL MSGRTN;                       0993 03348000
         BAL   @14,MSGRTN                                          0993 03349000
*                             END;                                 0994 03350000
*                         END;                                     0995 03351000
@RF00987 DS    0H                                                  0996 03352000
*                     END;                                         0996 03353000
@RF00985 DS    0H                                                  0997 03354000
*                 END;                                             0997 03355000
@RF00980 DS    0H                                                  0998 03356000
*           END;                    /* IF WE DID NOT FIND END OF     */ 03357000
@RC00970 DS    0H                                                  0998 03358000
@RC00968 AL    ICTR,@CF00035                                       0998 03359000
@DE00967 C     ICTR,LINELNG                                        0998 03360000
         BNH   @DL00967                                            0998 03361000
@DC00967 DS    0H                                                  0999 03362000
*           IF CYCLE=CON1 THEN      /* VALUE THEN ASSUME END AT END  */ 03363000
         C     CYCLE,@CF00035                                      0999 03364000
         BNE   @RF00999                                            0999 03365000
*             VALEND=ICTR-CON1;     /* OF RECORD                     */ 03366000
         LR    @15,ICTR                                            1000 03367000
         BCTR  @15,0                                               1000 03368000
         ST    @15,VALEND                                          1000 03369000
*         END;                                                     1001 03370000
*     END;                                                         1002 03371000
*   END;                                                           1003 03372000
*                                                                  1003 03373000
@EL00008 DS    0H                                                  1003 03374000
@EF00008 DS    0H                                                  1003 03375000
@ER00008 LM    @14,@05,@SA00008                                    1003 03376000
         LM    @07,@12,@SA00008+32                                 1003 03377000
         BR    @14                                                 1003 03378000
