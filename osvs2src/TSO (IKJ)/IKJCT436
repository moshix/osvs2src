*             DCBBLKSI=DEFBLKSI;    /* SET BLKSI TO 3360             */ 03616000
         MVC   DCBBLKSI(2,@07),@CH00102                            1026 03617000
*             DCBLRECL=DEFLRECL;    /* SET LRECL TO 80               */ 03618000
         MVC   DCBLRECL(2,@06),@CH00104                            1027 03619000
*           END;                    /*                               */ 03620000
*         ELSE                                                     1029 03621000
*           DCBBLKSI=DCBLRECL+F4;   /* SET BLKSI TO LREC + 4         */ 03622000
