         CLI   OPCODE(@01),4                                       0226 00514000
         BNE   @RF00226                                            0226 00515000
*     N=F2;                         /* PRINT ASIS                    */ 00516000
         MVC   N(4),@CF00091                                       0227 00517000
*   ELSE                            /* OTHERWISE                     */ 00518000
*     N=F0;                         /* PRINT EDIT                    */ 00519000
