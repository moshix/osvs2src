*                   PTRA=PTRA+F4;   /* SKIP LL OO FIELDS             */ 03167000
         ALR   PTRA,@02                                            0902 03168000
*                 END;              /* END RECFM=V X X PROCESS       */ 03169000
*               ELSE                /* IF RECFM=F OR RECFM= U        */ 03170000
*                 SVTLNG=DCBLRECL;  /* SET LENGTH                    */ 03171000
         B     @RC00899                                            0904 03172000
