*               IF DCBRECL=V THEN   /* IF RECFM=V X X                */ 03154000
         TM    DCBRECL+28(DCBPTR),B'01000000'                      0899 03155000
         BNO   @RF00899                                            0899 03156000
         TM    DCBRECL+28(DCBPTR),B'10000000'                      0899 03157000
         BNZ   @RF00899                                            0899 03158000
*                 DO;               /* ACCOUNT FOR LL00              */ 03159000
*                   SVTLNG=DCBLRECL-4;/* LENGTH FOR MOVE             */ 03160000
         L     @05,SNTELPTR(,COMONPTR)                             0901 03161000
