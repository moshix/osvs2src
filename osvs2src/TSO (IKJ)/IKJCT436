         ST    @15,@AL00001+8                                      0229 00529000
         LA    @01,@AL00001                                        0229 00530000
         BAL   @14,COMLINK                                         0229 00531000
*   ERROR=E4;                       /* SET UP TO GET NEW LINE        */ 00532000
         MVC   ERROR(4),@CF00109                                   0230 00533000
*   END WRITERTN;                                                  0231 00534000
@EL00009 DS    0H                                                  0231 00535000
