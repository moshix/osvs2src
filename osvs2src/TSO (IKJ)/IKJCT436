         BR    @14                                                 0982 03480000
*                                                                  0983 03481000
*   /*****************************************************************/ 03482000
*   /*                                                               */ 03483000
*   /* CLOSE DOWN THE FILEDCB                                        */ 03484000
*   /*                                                               */ 03485000
*   /*****************************************************************/ 03486000
*                                                                  0983 03487000
*CLOSRTN:                                                          0983 03488000
*   PROC;                           /* CLOSE ROUTINE                 */ 03489000
CLOSRTN  STM   @14,@12,@SA00026                                    0983 03490000
*   CALL LOCFILE;                   /* GET FILENAME                  */ 03491000
         BAL   @14,LOCFILE                                         0984 03492000
*   IF ERROR=F0&MATCH=NO THEN       /* IF DDNAME NOT OPEN            */ 03493000
         L     @03,ERROR                                           0985 03494000
         LTR   @03,@03                                             0985 03495000
         BNZ   @RF00985                                            0985 03496000
         TM    MATCH(COMONPTR),B'00010000'                         0985 03497000
         BNZ   @RF00985                                            0985 03498000
*     ERROR=E368;                   /* SET ERROR CODE                */ 03499000
         MVC   ERROR(4),@CF00577                                   0986 03500000
*   IF ERROR=F0 THEN                /* IF OK SO FAR                  */ 03501000
@RF00985 L     @03,ERROR                                           0987 03502000
         LTR   @03,@03                                             0987 03503000
         BNZ   @RF00987                                            0987 03504000
*     DO;                           /* CONTINUE                      */ 03505000
*       PREVFILE->NEXTFILE=NEXTFILE;/* UNCHAIN FILEBLOK              */ 03506000
         L     @15,FILEPTR                                         0989 03507000
         L     @14,NEXTFILE(,@15)                                  0989 03508000
         ST    @14,NEXTFILE(,PREVFILE)                             0989 03509000
*       DCBPTR=ADDR(DCBAREA);       /* POINT TO DCB                  */ 03510000
         LA    DCBPTR,DCBAREA(,@15)                                0990 03511000
*       IF DCBOFOPN=YES THEN        /* IF THE DCB IS OPEN            */ 03512000
         TM    DCBOFOPN+40(DCBPTR),B'00010000'                     0991 03513000
         BNO   @RF00991                                            0991 03514000
*         DO;                       /*                               */ 03515000
*           CLOSL(2:4)=DCBPTR;      /* SET DCB ADDR IN CLOSE LIST    */ 03516000
         STCM  DCBPTR,7,CLOSL+1                                    0993 03517000
*           GEN(CLOSE    MF=(E,CLOSL));/* ISSUE CLOSE                */ 03518000
         CLOSE    MF=(E,CLOSL)                                          03519000
*         END;                      /*                               */ 03520000
*       IF DCBOFOPN=NO THEN         /* AS LONG AS DCB IS CLOSED      */ 03521000
@RF00991 TM    DCBOFOPN+40(DCBPTR),B'00010000'                     0996 03522000
         BNZ   @RF00996                                            0996 03523000
*         DO;                       /* FREE ALL STG FOR IT   @ZA09978*/ 03524000
*           RFY                                                    0998 03525000
*            (R2) RSTD;             /* RESERVE REGISTER      @ZA09978*/ 03526000
*           R2=DCBPTR;              /* POINT TO DCB          @ZA09978*/ 03527000
         LR    R2,DCBPTR                                           0999 03528000
*           GEN REFS(R2)(FREEPOOL  (R2));/* FREE BUFFER POOL @ZA09978*/ 03529000
         FREEPOOL  (R2)                                                 03530000
*           RFY                                                    1001 03531000
*            (R2) UNRSTD;           /* RELEASE REGISTER      @ZA09978*/ 03532000
*           DO;                     /* FREEMAIN                    1002 03533000
*                                      LV(LENGTH(FILEBLOK)+DCBLEN) 1002 03534000
*                                      A(FILEPTR) SP(78) MF(E,FRELST)*/ 03535000
*             RESPECIFY                                            1003 03536000
*              (GPR01P) RESTRICTED;                                1003 03537000
*             FRELST02=LENGTH(FILEBLOK)+DCBLEN;/* LENGTH             */ 03538000
         L     @15,DCBLEN                                          1004 03539000
         LA    @15,20(,@15)                                        1004 03540000
