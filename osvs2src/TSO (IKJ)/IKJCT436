         BNE   @RF00606                                            0606 02053000
*         DO;                       /*                               */ 02054000
*           OUTA1(1:3)=ANSWER(2:4); /* COPY ANSWER                   */ 02055000
         MVC   OUTA1(3),ANSWER+1(@14)                              0608 02056000
*           DO I=F3 TO F1 BY-F1;    /* RE-FOLD ANSWER                */ 02057000
         LA    @15,3                                               0609 02058000
         ST    @15,I(,COMONPTR)                                    0609 02059000
@DL00609 DS    0H                                                  0610 02060000
*             IF(OUTA1(I)<CC0|OUTA1(I)>CC9)&/* NOT ZERO TO NINE      */ 02061000
*                 (OUTA1(I)<CCA|OUTA1(I)>CCF) THEN/* NOT A-F         */ 02062000
         L     @15,I(,COMONPTR)                                    0610 02063000
         LA    @14,OUTA1-1(@15)                                    0610 02064000
