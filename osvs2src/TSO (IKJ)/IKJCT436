         LA    @01,@AL00001                                        0214 00459000
         BALR  @14,@15                                             0214 00460000
*   ERROR=R15;                      /* SAVE ERROR CODE               */ 00461000
         ST    R15,ERROR                                           0215 00462000
*   RFY                                                            0216 00463000
*    (R15) UNRSTD;                  /* RELEASE REG 15                */ 00464000
*   IF ERROR=E4 THEN                /* IF FALSE                      */ 00465000
         L     @15,ERROR                                           0217 00466000
         C     @15,@CF00109                                        0217 00467000
         BNE   @RF00217                                            0217 00468000
*     LSDANEXT=FLSEPATH;            /* BRANCH TO FALSE PATH          */ 00469000
