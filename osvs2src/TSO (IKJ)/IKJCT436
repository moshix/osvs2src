         BR    @14                                                 0954 03366000
*                                                                  0955 03367000
*   /*****************************************************************/ 03368000
*   /*                                                               */ 03369000
*   /* PUTXRTN: THE PUTXRTN TELLS THE ACCESS METHOD TO PLACE AN      */ 03370000
*   /* UPDATED RECORD BACK INTO THE DATASET. THIS OPTION IS USED WITH*/ 03371000
*   /* OPENFILE XXXXXXXX,UPDATE.                                     */ 03372000
*   /*                                                               */ 03373000
*   /*****************************************************************/ 03374000
*                                                                  0955 03375000
*PUTXRTN:                                                          0955 03376000
*   PROC;                           /* PUTXRTN                       */ 03377000
PUTXRTN  STM   @14,@12,@SA00024                                    0955 03378000
*   PTRA=CURREC;                    /* GET LAST RECORD READ          */ 03379000
         L     @02,FILEPTR                                         0956 03380000
         L     PTRA,CURREC(,@02)                                   0956 03381000
*   IF PTRA=0 THEN                  /* IF ZERO (NO LAST RECORD)      */ 03382000
         LTR   PTRA,PTRA                                           0957 03383000
         BNZ   @RF00957                                            0957 03384000
*     ERROR=E372;                   /* SET ERROR CODE                */ 03385000
         MVC   ERROR(4),@CF00579                                   0958 03386000
*   ELSE                            /* OTHERWISE                     */ 03387000
*     DO;                           /* PUT BACK RECORD               */ 03388000
         B     @RC00957                                            0959 03389000
@RF00957 DS    0H                                                  0960 03390000
*       IF DCBRECL=V THEN           /* FOR VARIABLE RECORDS          */ 03391000
         TM    DCBRECL+28(DCBPTR),B'01000000'                      0960 03392000
         BNO   @RF00960                                            0960 03393000
         TM    DCBRECL+28(DCBPTR),B'10000000'                      0960 03394000
         BNZ   @RF00960                                            0960 03395000
*         DO;                       /*                               */ 03396000
*           OUTLEN=DCBLRECL-4;      /* SET OUTPUT MOVE LENGTH        */ 03397000
         LA    @05,4                                               0962 03398000
         LH    @02,DCBLRECL+72(,DCBPTR)                            0962 03399000
         SLR   @02,@05                                             0962 03400000
         STH   @02,OUTLEN                                          0962 03401000
*           PTRA=PTRA+F4;           /* DONT OVERLAY LL/OO            */ 03402000
         ALR   PTRA,@05                                            0963 03403000
*         END;                      /*                               */ 03404000
*       ELSE                        /* FOR RECFM = FIXED             */ 03405000
*         OUTLEN=DCBLRECL;          /* SET MAX LENGTH                */ 03406000
         B     @RC00960                                            0965 03407000
@RF00960 LH    @05,DCBLRECL+72(,DCBPTR)                            0965 03408000
