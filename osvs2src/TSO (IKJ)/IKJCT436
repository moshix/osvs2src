         TM    EMPTY(@01),B'00010000'                              0879 03094000
         BNO   @RF00879                                            0879 03095000
*     ERROR=E400;                   /* SET END OF FILE       @ZA15726*/ 03096000
         MVC   ERROR(4),@CF00581                                   0880 03097000
*   IF ERROR=F0 THEN                /* IF ALL OK                     */ 03098000
@RF00879 L     @15,ERROR                                           0881 03099000
         LTR   @15,@15                                             0881 03100000
         BNZ   @RF00881                                            0881 03101000
*     DO;                           /* CONTINUE PROCESS              */ 03102000
*       DCBPTR=ADDR(DCBAREA);       /* POINT TO DCB TO USE           */ 03103000
         L     DCBPTR,FILEPTR                                      0883 03104000
         LA    DCBPTR,DCBAREA(,DCBPTR)                             0883 03105000
*       IF DCBOFOPN=NO THEN         /* IF DCB NOT OPEN               */ 03106000
         TM    DCBOFOPN+40(DCBPTR),B'00010000'                     0884 03107000
         BNZ   @RF00884                                            0884 03108000
*         ERROR=E356;               /* SET ERROR CODE                */ 03109000
         MVC   ERROR(4),@CF00571                                   0885 03110000
*       ELSE                        /* OTHERWISE                     */ 03111000
*         DO;                       /* CONTINUE                      */ 03112000
         B     @RC00884                                            0886 03113000
@RF00884 DS    0H                                                  0887 03114000
*           CALL COMLINK(LOCATE,NAMESAV,NAMELEN,F0);/* FIND NAME     */ 03115000
         LA    @02,@CF00121                                        0887 03116000
