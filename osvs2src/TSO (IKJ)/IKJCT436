         BNZ   @RF00888                                            0888 03129000
*             DO;                   /* CONTINUE                      */ 03130000
*               DCBEODA=ADDR(EOF);  /* SET EOF RETURN POINT          */ 03131000
         LA    @02,EOF                                             0890 03132000
         STCM  @02,7,DCBEODA+28(DCBPTR)                            0890 03133000
*               ERROR=E400;         /* SET END OF FILE RC            */ 03134000
         MVC   ERROR(4),@CF00581                                   0891 03135000
*               RFY                                                0892 03136000
*                (R1,                                              0892 03137000
*                 R2) RSTD;         /* RESTRICT REG 1,2              */ 03138000
*               R2=DCBPTR;          /* ADDR OF DCB                   */ 03139000
         LR    R2,DCBPTR                                           0893 03140000
*               GEN REFS(R2) SETS(R1) FLOWS(EOF,SYNADXT)(GET (R2));     03141000
         GET (R2)                                                       03142000
*               PTRA=R1;            /* SET BASE OF I/O BFRS          */ 03143000
         LR    PTRA,R1                                             0895 03144000
*               RFY                                                0896 03145000
*                (R1,                                              0896 03146000
*                 R2) UNRSTD;       /* RELEASE REGISTER 1,2          */ 03147000
*               ERROR=F0;           /* NOT END OF FILE               */ 03148000
         SLR   @05,@05                                             0897 03149000
