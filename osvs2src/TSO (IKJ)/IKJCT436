@ER00024 LM    @14,@12,@SA00024                                    0972 03433000
         BR    @14                                                 0972 03434000
*                                                                  0973 03435000
*   /*****************************************************************/ 03436000
*   /*                                                               */ 03437000
*   /* MOVE: THE MOVE ROUTINE IS USED TO MOVE I/O BUFFERS AROUND.    */ 03438000
*   /* THIS ROUTINE DECLARES ITS OWN REGISTERS TO KEEP THE SEPARATE  */ 03439000
*   /* FROM MAINLINE PROCESSING                                      */ 03440000
*   /*                                                               */ 03441000
*   /*****************************************************************/ 03442000
*                                                                  0973 03443000
*MOVE:                                                             0973 03444000
*   PROC(FROM,TO,LENFR,LENTO);      /* MOVE ROUTINE                  */ 03445000
MOVE     STM   @14,@12,@SA00025                                    0973 03446000
         MVC   @PC00025(16),0(@01)                                 0973 03447000
*   DCL                             /*                               */ 03448000
*     FROM CHAR(*),                 /* FROM BUFFER                   */ 03449000
*     TO CHAR(*),                   /* TO BUFFER                     */ 03450000
*     LENFR FIXED(15),              /* NUMBER OF BYTES TO MOVE       */ 03451000
*     LENTO FIXED(15);              /* NUMBER OF BYTES TO MOVE       */ 03452000
*   RFY                                                            0975 03453000
*    (R2,                                                          0975 03454000
*     R3,                                                          0975 03455000
*     R4,                                                          0975 03456000
*     R5) RSTD;                     /* RESTRICT REGS FOR MVCL        */ 03457000
*   R2=ADDR(TO(1));                 /* ADDRESS OF TO BUFFER          */ 03458000
         L     R2,@PC00025+4                                       0976 03459000
*   R4=ADDR(FROM(1));               /* ADDRESS OF FROM BUFFER        */ 03460000
         L     R4,@PC00025                                         0977 03461000
*   R3=LENTO;                       /* NUMBER OF BYTES TO MOVE       */ 03462000
         L     @15,@PC00025+12                                     0978 03463000
         LH    R3,LENTO(,@15)                                      0978 03464000
*   R5=BLANKS|LENFR;                /* SET LEN AND PADD CHAR         */ 03465000
         L     @15,@PC00025+8                                      0979 03466000
         LH    R5,LENFR(,@15)                                      0979 03467000
         O     R5,@CF00417                                         0979 03468000
*   GEN(MVCL  R2,R4);               /* ISSUE MVCL INSTRUCTION        */ 03469000
         MVCL  R2,R4                                                    03470000
*   RFY                                                            0981 03471000
*    (R2,                                                          0981 03472000
*     R3,                                                          0981 03473000
*     R4,                                                          0981 03474000
*     R5) UNRSTD;                   /* RELEASE REGS                  */ 03475000
*   END MOVE;                       /*                               */ 03476000
