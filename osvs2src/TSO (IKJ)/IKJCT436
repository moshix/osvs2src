         BNO   @RF00940                                            0940 03324000
         TM    DCBRECL+28(DCBPTR),B'10000000'                      0940 03325000
         BNZ   @RF00940                                            0940 03326000
*     DO;                           /* PROCESS VARIABLE              */ 03327000
*       PTRA=SNTVLPTR;              /* POINT TO LENGTH FIELD(SVTLNG) */ 03328000
         LR    PTRA,@07                                            0942 03329000
*       SVTLNG=SVTLNG+F4;           /* ADD FOUR FOR PUT              */ 03330000
         AL    @02,@CF00109                                        0943 03331000
         STH   @02,SVTLNG(,@07)                                    0943 03332000
*       SVTORIG=F0;                 /* SET SVTORIG TO ZERO FOR PUT   */ 03333000
         SLR   @02,@02                                             0944 03334000
         STH   @02,SVTORIG(,@07)                                   0944 03335000
*     END;                          /*                               */ 03336000
*   ELSE                            /* OTHERWISE                     */ 03337000
*     PTRA=ADDR(SVTDATA);           /* POINT DIRECTLY TO SVTDATA     */ 03338000
         B     @RC00940                                            0946 03339000
@RF00940 L     @07,SNTELPTR(,COMONPTR)                             0946 03340000
         L     PTRA,SNTVLPTR(,@07)                                 0946 03341000
         LA    PTRA,SVTDATA(,PTRA)                                 0946 03342000
*   RFY                                                            0947 03343000
*    (R0,                                                          0947 03344000
*     R2) RSTD;                     /* RESTRICT FOR PUT              */ 03345000
@RC00940 DS    0H                                                  0948 03346000
*   R0=PTRA;                        /* SET OUTAREA ADDRESS           */ 03347000
         LR    R0,PTRA                                             0948 03348000
*   R2=DCBPTR;                      /* LOAD DCB ADDR                 */ 03349000
         LR    R2,DCBPTR                                           0949 03350000
*   GEN REFS(R0,R2) FLOWS(SYNADXT)(PUT    (R2),(R0));/* ISSUE PUT    */ 03351000
         PUT    (R2),(R0)                                               03352000
*   RFY                                                            0951 03353000
*    (R0,                                                          0951 03354000
*     R2) UNRSTD;                   /* RELEASE REG ZERO              */ 03355000
*   SVTORIG=SAVOO;                  /* RESTORE ORIG                  */ 03356000
         L     @07,SNTELPTR(,COMONPTR)                             0952 03357000
         L     @07,SNTVLPTR(,@07)                                  0952 03358000
