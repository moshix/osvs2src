*             FRELST04=ADDR(FILEPTR);/* ADDR OF ADDR LIST            */ 03542000
         LA    @00,FILEPTR                                         1005 03543000
         STCM  @00,7,FRELST04                                      1005 03544000
*             FRELST06=78;          /* SUBPOOL VALUE                 */ 03545000
         MVI   FRELST06,X'4E'                                      1006 03546000
*             GPR01P=ADDR(FRELST);  /* REG1 POINTS TO LIST           */ 03547000
         LA    GPR01P,FRELST                                       1007 03548000
*             SVC(5);               /* STANDARD-FORM OF GETMAIN      */ 03549000
         SVC   5                                                   1008 03550000
*             RESPECIFY                                            1009 03551000
*              (GPR01P) UNRESTRICTED;                              1009 03552000
*           END;                    /* FREEMAIN                    1010 03553000
*                                      LV(LENGTH(FILEBLOK)+DCBLEN) 1010 03554000
