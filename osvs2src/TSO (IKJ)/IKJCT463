*       IF ERROR=YES THEN           /* ERROR IN MSG HANDLER          */ 01273000
         TM    ERROR(OCMTBPTR),B'00001000'                         0441 01274000
         BO    @RT00441                                            0441 01275000
*         GOTO ENDRETRN;            /* GOTO END AND RETURN TO CALLER */ 01276000
*       ELSE                        /* MSG HANDLER SUCCESSFULLY    0443 01277000
