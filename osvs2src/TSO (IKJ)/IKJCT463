*       KEY1=YES;                   /* INDICATE BACK IN KEY1         */ 01264000
         OI    KEY1(OCMTBPTR),B'10000000'                          0438 01265000
*       OUTMSGID=4;                 /* MSG FOR ERROR DURING ATTACH   */ 01266000
