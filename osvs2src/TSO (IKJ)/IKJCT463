         TITLE 'IKJCT463 - OUTPUT SUBCOMMAND HANDLER                   *00001000
                        '                                               00002000
IKJCT463 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKJCT463  77.006'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @11,8(,@13)                                         0001 00020000
         LR    @13,@11                                             0001 00021000
         XC    @ZTEMPS(@ZLEN),@ZTEMPS                                   00022000
         MVC   @PC00001(4),0(@01)                                  0001 00023000
*                                                                  0055 00024000
*/********************************************************************/ 00025000
*/*                                                                  */ 00026000
*/*                                                                  */ 00027000
*/*      B E G I N   C O D E                                         */ 00028000
*/*                                                                  */ 00029000
*/*                                                                  */ 00030000
*/********************************************************************/ 00031000
*                                                                  0055 00032000
*   OCMTBPTR=OUTP;                  /* SAVE PARAMETER FROM CALLER    */ 00033000
         L     @14,@PC00001                                        0055 00034000
         L     OCMTBPTR,OUTP(,@14)                                 0055 00035000
*   SUBCMODE=YES;                   /* INDICATE IN SUBCMD MODE       */ 00036000
         OI    SUBCMODE(OCMTBPTR),B'01000000'                      0056 00037000
*/********************************************************************/ 00038000
*/*       BUILD COMMAND SCAN AND PARSE PARAMETER LISTS               */ 00039000
*/********************************************************************/ 00040000
*                                                                  0057 00041000
*   CPPLPTR=OUTCPPL;                /* ADDR FOR CPPL DSECT           */ 00042000
         L     @14,OUTCPPL(,OCMTBPTR)                              0057 00043000
         ST    @14,CPPLPTR                                         0057 00044000
*   CSPLPTR=ADDR(CSPLAREA);         /* BASE CSPL ON CORE GOTTEN IN 63*/ 00045000
         LA    @10,CSPLAREA                                        0058 00046000
         ST    @10,CSPLPTR                                         0058 00047000
*   CSPLUPT=CPPLUPT;                /* FILL IN ADDR OF UPT           */ 00048000
         L     @09,CPPLUPT(,@14)                                   0059 00049000
         ST    @09,CSPLUPT(,@10)                                   0059 00050000
*   CSPLECT=CPPLECT;                /* FILL IN ADDR OF ECT           */ 00051000
         L     @14,CPPLECT(,@14)                                   0060 00052000
         ST    @14,CSPLECT(,@10)                                   0060 00053000
*   CSPLECB=ADDR(OUTATTN);          /* .....                         */ 00054000
         LA    @08,OUTATTN(,OCMTBPTR)                              0061 00055000
         ST    @08,CSPLECB(,@10)                                   0061 00056000
*   CSOAPTR=ADDR(CSOAAREA);         /* BASE CSOA ON DYNAMIC AREA     */ 00057000
         LA    CSOAPTR,CSOAAREA                                    0062 00058000
*   CSPLOA=ADDR(CSOA);              /* ADDR OF DATA AREA FOR PARMLIST*/ 00059000
         ST    CSOAPTR,CSPLOA(,@10)                                0063 00060000
*   CSOA=CSOA&&CSOA;                /* ZERO CMD SCAN OUTPUT AREA     */ 00061000
         XC    CSOA(8,CSOAPTR),CSOA(CSOAPTR)                       0064 00062000
*   PPLPTR=ADDR(PPLAREA);           /* BASE PARSE PARM LIST          */ 00063000
         LA    @10,PPLAREA                                         0065 00064000
         ST    @10,PPLPTR                                          0065 00065000
*   PPL=PPL&&PPL;                   /* ZERO PARSE PARM LIST          */ 00066000
         XC    PPL(28,@10),PPL(@10)                                0066 00067000
*   PPLUPT=CPPLUPT;                 /* .....                         */ 00068000
         ST    @09,PPLUPT(,@10)                                    0067 00069000
*   PPLECT=CPPLECT;                 /* .....                         */ 00070000
         ST    @14,PPLECT(,@10)                                    0068 00071000
*   PPLECB=ADDR(OUTATTN);           /* OUTATTN FIELD IN OCMTB IS ECB */ 00072000
         ST    @08,PPLECB(,@10)                                    0069 00073000
*   PPLANS=ADDR(OUTSBPDL);          /* SAVE PDE ADDR IN OUTCOMTB     */ 00074000
         LA    @14,OUTSBPDL(,OCMTBPTR)                             0070 00075000
         ST    @14,PPLANS(,@10)                                    0070 00076000
*/********************************************************************/ 00077000
*/*         SET BASES FOR CONTROL BLOCKS                             */ 00078000
*/********************************************************************/ 00079000
*                                                                  0071 00080000
*   DCBPTR=ADDR(DCBOUTD);           /* BASE IHADCB ON DYNAMIC CORE   */ 00081000
         LA    @14,DCBOUTD                                         0071 00082000
         ST    @14,DCBPTR                                          0071 00083000
*   DAPLPTR=ADDR(DAPLAREA);         /* BASE DAPL MAP ON DYNAMIC AREA */ 00084000
         LA    @14,DAPLAREA                                        0072 00085000
         ST    @14,DAPLPTR                                         0072 00086000
*   DAPB2PTR=ADDR(DAIRAREA);        /* BASE DAIR BLOCK ON DYNAMIC  0073 00087000
*                                      AREA                          */ 00088000
         LA    @14,DAIRAREA                                        0073 00089000
         ST    @14,DAPB2PTR                                        0073 00090000
*                                                                  0074 00091000
*/********************************************************************/ 00092000
*/*                                                                  */ 00093000
*/*                   SCAN THE SUBCOMMAND                            */ 00094000
*/*                                                                  */ 00095000
*/********************************************************************/ 00096000
*                                                                  0074 00097000
*SCANSUB:                           /* RETURN HERE TO SCAN ANY NEW 0074 00098000
*                                      SUBCOMMAND                    */ 00099000
*   CSPLCBUF=OUTSBBUF;              /* ADDR NEW SUBCMD FOR CMD SCAN  */ 00100000
SCANSUB  L     @14,CSPLPTR                                         0074 00101000
         MVC   CSPLCBUF(4,@14),OUTSBBUF(OCMTBPTR)                  0074 00102000
*   CSPLFLG=ADDR(OUTWORK);          /* ADDR OF REQUEST FLAG          */ 00103000
         LA    @10,OUTWORK(,OCMTBPTR)                              0075 00104000
         ST    @10,CSPLFLG(,@14)                                   0075 00105000
*   OUTWORK=ZERO;                   /* SET INDICATOR FLAG TO SYNTAX     00106000
*                                      CHECK REQUEST                 */ 00107000
         SLR   @14,@14                                             0076 00108000
         ST    @14,OUTWORK(,OCMTBPTR)                              0076 00109000
*   POSTED=NO;                      /* BE SURE BIT OFF. THEN IF IT 0077 00110000
*                                      SHOULD BE ON AFTER CMD SCAN,     00111000
*                                      IT'S DUE TO A NEW SUBCMD    0077 00112000
*                                      ENTERED AND NOT DUE TO THE  0077 00113000
*                                      LAST SUBCMD JUST SCANNED. A 0077 00114000
*                                      RESCAN OF A SUBCMD CAUSES   0077 00115000
*                                      PROBLEMS DUE TO BUFFER OFFSETS*/ 00116000
         NI    POSTED(OCMTBPTR),B'10111111'                        0077 00117000
*   KEY1=NO;                        /* INCICATE KEY0 FOR '67         */ 00118000
         NI    KEY1(OCMTBPTR),B'01111111'                          0078 00119000
*   GEN(MODESET EXTKEY=SUPR);       /* OBTAIN KEY0 FOR CMD SCAN      */ 00120000
         MODESET EXTKEY=SUPR                                            00121000
*   REG1=ADDR(CSPL);                /* PARM FOR CMD SCAN             */ 00122000
         L     REG1,CSPLPTR                                        0080 00123000
*   GEN(LINK EP=IKJSCAN);           /* LINK TO CMD SCAN              */ 00124000
         LINK EP=IKJSCAN                                                00125000
*   OUTRTCD=REG15;                  /* SAVE RETURN CODE              */ 00126000
         STH   REG15,OUTRTCD(,OCMTBPTR)                            0082 00127000
*   GEN(MODESET EXTKEY=SCHED);      /* RETURN TO KEY 1               */ 00128000
         MODESET EXTKEY=SCHED                                           00129000
*   KEY1=YES;                       /* INDICATE KEY 1 FOR '67        */ 00130000
         OI    KEY1(OCMTBPTR),B'10000000'                          0084 00131000
*                                                                  0085 00132000
*/********************************************************************/ 00133000
*/*          CHECK COMMAND SCAN RESULTS                              */ 00134000
*/********************************************************************/ 00135000
*                                                                  0085 00136000
*   IF POSTED=YES THEN              /* IF ATTN WAS STRUCK            */ 00137000
         TM    POSTED(OCMTBPTR),B'01000000'                        0085 00138000
         BNO   @RF00085                                            0085 00139000
*     DO;                                                          0086 00140000
*       CALL TCLRQSTK;              /* CALL RTN, RESET STACK @ZA15715*/ 00141000
         BAL   @14,TCLRQSTK                                        0087 00142000
*       GOTO SCANSUB;               /* GO LOOK FOR NEXT CMD          */ 00143000
         B     SCANSUB                                             0088 00144000
*     END;                                                         0089 00145000
*   IF OUTRTCD^=ZERO|CSOABAD='1'B THEN/* SOME KIND OF ERROR FROM   0090 00146000
*                                      CMDSCAN                       */ 00147000
@RF00085 LH    @14,OUTRTCD(,OCMTBPTR)                              0090 00148000
         LTR   @14,@14                                             0090 00149000
         BNZ   @RT00090                                            0090 00150000
         TM    CSOABAD(CSOAPTR),B'00001000'                        0090 00151000
         BNO   @RF00090                                            0090 00152000
@RT00090 DS    0H                                                  0091 00153000
*     DO;                                                          0091 00154000
*       IF CSOABAD='1'B THEN        /* IF SYNTACTICALLY INVALID CMD  */ 00155000
         TM    CSOABAD(CSOAPTR),B'00001000'                        0092 00156000
         BNO   @RF00092                                            0092 00157000
*         DO;                                                      0093 00158000
*           OUTMSGID=128;           /* INVALID SUBCMD MSG 323I       */ 00159000
         MVI   OUTMSGID(OCMTBPTR),X'80'                            0094 00160000
*           CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER               */ 00161000
         ST    OCMTBPTR,@AL00001                                   0095 00162000
         L     @15,@CV01609                                        0095 00163000
         LA    @01,@AL00001                                        0095 00164000
         BALR  @14,@15                                             0095 00165000
*           IF NONTERM=YES|ERROR=YES THEN/* INPUT NOT FROM TERMINAL     00166000
*                                      OR MSG HANDLER COULDN'T PUT 0096 00167000
*                                      MSG                           */ 00168000
         TM    NONTERM(OCMTBPTR),B'00100000'                       0096 00169000
         BO    @RT00096                                            0096 00170000
         TM    ERROR(OCMTBPTR),B'00001000'                         0096 00171000
         BNO   @RF00096                                            0096 00172000
@RT00096 DS    0H                                                  0097 00173000
*             DO;                                                  0097 00174000
*               ERROR=YES;          /* INDICATE ERROR CONDITION      */ 00175000
         OI    ERROR(OCMTBPTR),B'00001000'                         0098 00176000
*               GOTO ENDRETRN;      /* QUIT PROCESSING               */ 00177000
         B     ENDRETRN                                            0099 00178000
*             END;                                                 0100 00179000
*           ELSE                    /* WROTE MSG OK & INPUT FROM TERM*/ 00180000
*             DO;                   /* GIVE USER ANOTHER CHANCE      */ 00181000
@RF00096 DS    0H                                                  0102 00182000
*               GEN(TCLEARQ INPUT); /* FLUSH INPUT STACK             */ 00183000
         TCLEARQ INPUT                                                  00184000
*               OUTMSGID=0;         /* MODE MSG 300A                 */ 00185000
         MVI   OUTMSGID(OCMTBPTR),X'00'                            0103 00186000
*               CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER           */ 00187000
         ST    OCMTBPTR,@AL00001                                   0104 00188000
         L     @15,@CV01609                                        0104 00189000
         LA    @01,@AL00001                                        0104 00190000
         BALR  @14,@15                                             0104 00191000
*               GOTO SCANSUB;       /* GO SCAN NEW SUBCMD            */ 00192000
         B     SCANSUB                                             0105 00193000
*             END;                                                 0106 00194000
*         END;                                                     0107 00195000
*       ELSE                        /* IF RETURN CODE NON ZERO       */ 00196000
*         DO;                                                      0108 00197000
@RF00092 DS    0H                                                  0109 00198000
*           ERROR=YES;              /* SERIOUS ERROR                 */ 00199000
         OI    ERROR(OCMTBPTR),B'00001000'                         0109 00200000
*           OUTMSGID=20;            /* CMD SYS ERROR MSG             */ 00201000
         MVI   OUTMSGID(OCMTBPTR),X'14'                            0110 00202000
*           CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER               */ 00203000
         ST    OCMTBPTR,@AL00001                                   0111 00204000
         L     @15,@CV01609                                        0111 00205000
         LA    @01,@AL00001                                        0111 00206000
         BALR  @14,@15                                             0111 00207000
*           GOTO ENDRETRN;          /* RETURN                        */ 00208000
         B     ENDRETRN                                            0112 00209000
*         END;                                                     0113 00210000
*     END;                                                         0114 00211000
*   IF CSOANOC='1'B THEN            /* NULL LINE ENTERED             */ 00212000
@RF00090 TM    CSOANOC(CSOAPTR),B'00010000'                        0115 00213000
         BNO   @RF00115                                            0115 00214000
*     DO;                                                          0116 00215000
*       SUBCONT=YES;                /* INDICATE 'CONTINUE' WAS     0117 00216000
*                                      ENTERED                       */ 00217000
*       SUBHERE=YES;                /* DEFAULT IS 'HERE'             */ 00218000
*       SUBNEXT=NO;                 /* MAKE SURE BITS ARE OFF        */ 00219000
*       SUBBEGN=NO;                 /* .....                         */ 00220000
         OI    SUBCONT(OCMTBPTR),B'00110000'                       0120 00221000
         NI    SUBNEXT(OCMTBPTR),B'11110011'                       0120 00222000
*       GOTO ENDRETRN;              /* RETURN TO '62 & PROCESS 'CONT'*/ 00223000
         B     ENDRETRN                                            0121 00224000
*     END;                                                         0122 00225000
*   IF ERROR=YES THEN               /* IF ERRORS DETECTED ANYWHERE   */ 00226000
@RF00115 TM    ERROR(OCMTBPTR),B'00001000'                         0123 00227000
         BO    @RT00123                                            0123 00228000
*     GOTO ENDRETRN;                /* RETURN TO CALLER              */ 00229000
*                                                                  0125 00230000
*/********************************************************************/ 00231000
*/*                                                                  */ 00232000
*/*   DETERMINE WHICH SUBCMD IS IN THE BUFFER                        */ 00233000
*/*                                                                  */ 00234000
*/********************************************************************/ 00235000
*                                                                  0125 00236000
*   /*****************************************************************/ 00237000
*   /*                                                               */ 00238000
*   /* IF 'C' OR 'CONTINUE' ENTERED                                  */ 00239000
*   /*                                                               */ 00240000
*   /*****************************************************************/ 00241000
*                                                                  0125 00242000
*   IF(CSOALNM=1&SUBCMND='C')|(CSOALNM=8&SUBCMND='CONTINUE') THEN  0125 00243000
         CLC   CSOALNM(2,CSOAPTR),@CH00033                         0125 00244000
         BNE   @GL00003                                            0125 00245000
         L     @14,CSOACNM(,CSOAPTR)                               0125 00246000
         CLI   SUBCMND(@14),C'C'                                   0125 00247000
         BE    @RT00125                                            0125 00248000
@GL00003 CLC   CSOALNM(2,CSOAPTR),@CH00045                         0125 00249000
         BNE   @RF00125                                            0125 00250000
         L     @14,CSOACNM(,CSOAPTR)                               0125 00251000
         CLC   SUBCMND(8,@14),@CC01689                             0125 00252000
         BE    @RT00125                                            0125 00253000
*     GOTO CONTIN;                  /* PROCESS CONTINUE CMD          */ 00254000
*                                                                  0126 00255000
*   /*****************************************************************/ 00256000
*   /*                                                               */ 00257000
*   /* IF 'S' OR 'SAVE' ENTERED                                      */ 00258000
*   /*                                                               */ 00259000
*   /*****************************************************************/ 00260000
*                                                                  0127 00261000
*   IF(CSOALNM=1&SUBCMND='S')|(CSOALNM=4&SUBCMND='SAVE') THEN      0127 00262000
@RF00125 CLC   CSOALNM(2,CSOAPTR),@CH00033                         0127 00263000
         BNE   @GL00006                                            0127 00264000
         L     @14,CSOACNM(,CSOAPTR)                               0127 00265000
         CLI   SUBCMND(@14),C'S'                                   0127 00266000
         BE    @RT00127                                            0127 00267000
@GL00006 CLC   CSOALNM(2,CSOAPTR),@CH00051                         0127 00268000
         BNE   @RF00127                                            0127 00269000
         L     @14,CSOACNM(,CSOAPTR)                               0127 00270000
         CLC   SUBCMND(4,@14),@CC01692                             0127 00271000
         BE    @RT00127                                            0127 00272000
*     GOTO SAVE;                    /* GOTO SAVE CMD HANDLER         */ 00273000
*                                                                  0128 00274000
*   /*****************************************************************/ 00275000
*   /*                                                               */ 00276000
*   /* IF 'END' ENTERED                                              */ 00277000
*   /*                                                               */ 00278000
*   /*****************************************************************/ 00279000
*                                                                  0129 00280000
*   IF CSOALNM=3&SUBCMND='END' THEN                                0129 00281000
@RF00127 CLC   CSOALNM(2,CSOAPTR),@CH00268                         0129 00282000
         BNE   @RF00129                                            0129 00283000
         L     @14,CSOACNM(,CSOAPTR)                               0129 00284000
         CLC   SUBCMND(3,@14),@CC01694                             0129 00285000
         BE    @RT00129                                            0129 00286000
*     GOTO ENDCMD;                  /* GOTO END CMD HANDLER          */ 00287000
*                                                                  0130 00288000
*   /*****************************************************************/ 00289000
*   /*                                                               */ 00290000
*   /* IF 'H' OR 'HELP' ENTERED                                      */ 00291000
*   /*                                                               */ 00292000
*   /*****************************************************************/ 00293000
*                                                                  0131 00294000
*   IF(CSOALNM=1&SUBCMND='H')|(CSOALNM=4&SUBCMND='HELP') THEN      0131 00295000
@RF00129 CLC   CSOALNM(2,CSOAPTR),@CH00033                         0131 00296000
         BNE   @GL00010                                            0131 00297000
         L     @14,CSOACNM(,CSOAPTR)                               0131 00298000
         CLI   SUBCMND(@14),C'H'                                   0131 00299000
         BE    @RT00131                                            0131 00300000
@GL00010 CLC   CSOALNM(2,CSOAPTR),@CH00051                         0131 00301000
         BNE   @RF00131                                            0131 00302000
         L     @14,CSOACNM(,CSOAPTR)                               0131 00303000
         CLC   SUBCMND(4,@14),@CC01697                             0131 00304000
         BE    @RT00131                                            0131 00305000
*     GOTO HELP;                    /* GOTO THE HELP PROCESSOR       */ 00306000
*                                                                  0132 00307000
*   /*****************************************************************/ 00308000
*   /*                                                               */ 00309000
*   /* INVALID SUBCOMMAND ENTERED                                    */ 00310000
*   /*                                                               */ 00311000
*   /*****************************************************************/ 00312000
*                                                                  0133 00313000
*   OUTMSGID=128;                   /* MUST BE BAD OR MISSPELLED CMD */ 00314000
@RF00131 MVI   OUTMSGID(OCMTBPTR),X'80'                            0133 00315000
*   CALL IKJCT467(OUTCOMTB);        /* CALL MSG WRITER               */ 00316000
         ST    OCMTBPTR,@AL00001                                   0134 00317000
         L     @15,@CV01609                                        0134 00318000
         LA    @01,@AL00001                                        0134 00319000
         BALR  @14,@15                                             0134 00320000
*   IF NONTERM=YES THEN             /* YES - MUST BE CLIST OR      0135 00321000
*                                      NOPROMPT                      */ 00322000
         TM    NONTERM(OCMTBPTR),B'00100000'                       0135 00323000
         BNO   @RF00135                                            0135 00324000
*     DO;                                                          0136 00325000
*       ERROR=YES;                  /* SET ERROR INDICATERS          */ 00326000
         OI    ERROR(OCMTBPTR),B'00001000'                         0137 00327000
*       GOTO ENDRETRN;              /* RETURN TO CALLER              */ 00328000
         B     ENDRETRN                                            0138 00329000
*     END;                                                         0139 00330000
*   ELSE                            /* IF MSG WAS PUT OUT            */ 00331000
*     DO;                           /* GIVE USER ANOTHER CHANCE      */ 00332000
@RF00135 DS    0H                                                  0141 00333000
*       GEN(TCLEARQ INPUT);         /* FLUSH INPUT STACK             */ 00334000
         TCLEARQ INPUT                                                  00335000
*       OUTMSGID=0;                 /* ISSUE MODE MSG                */ 00336000
         MVI   OUTMSGID(OCMTBPTR),X'00'                            0142 00337000
*       CALL IKJCT467(OUTCOMTB);    /* CALL MSG WRITER               */ 00338000
         ST    OCMTBPTR,@AL00001                                   0143 00339000
         L     @15,@CV01609                                        0143 00340000
         LA    @01,@AL00001                                        0143 00341000
         BALR  @14,@15                                             0143 00342000
*       GOTO SCANSUB;               /* MUST HAVE ENTERED NEW CMD     */ 00343000
         B     SCANSUB                                             0144 00344000
*     END;                                                         0145 00345000
*                                                                  0146 00346000
*/********************************************************************/ 00347000
*/*                                                                  */ 00348000
*/*                                                                  */ 00349000
*/*                BEGIN  PROCESSING SUB COMMANDS                    */ 00350000
*/*                                                                  */ 00351000
