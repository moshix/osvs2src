*                                                                  0146 00361000
*SAVE:                                                             0146 00362000
*                                                                  0146 00363000
*/********************************************************************/ 00364000
*/*                      PARSE SAVE SUBCOMMAND                       */ 00365000
*/********************************************************************/ 00366000
*                                                                  0146 00367000
*   PPLCBUF=CSPLCBUF;               /* PASS CMD BUF W/ UPDATED OFFSET*/ 00368000
SAVE     L     @14,PPLPTR                                          0146 00369000
         L     @10,CSPLPTR                                         0146 00370000
         MVC   PPLCBUF(4,@14),CSPLCBUF(@10)                        0146 00371000
*   PPLPCL=VSAVEPCL;                /* PASS ADR OF SAVE PCL          */ 00372000
         MVC   PPLPCL(4,@14),VSAVEPCL                              0147 00373000
*   KEY1=NO;                        /* INDICATE KEY 0 FOR '67        */ 00374000
         NI    KEY1(OCMTBPTR),B'01111111'                          0148 00375000
*   GEN(MODESET EXTKEY=SUPR);       /* GOTO KEY 0 FOR PARSE          */ 00376000
         MODESET EXTKEY=SUPR                                            00377000
*   REG1=ADDR(PPL);                 /* PARAM FOR PARSE               */ 00378000
         L     REG1,PPLPTR                                         0150 00379000
*   GEN(LINK EP=IKJPARS);           /* LINK TO IKJPARS               */ 00380000
         LINK EP=IKJPARS                                                00381000
*   OUTRTCD=REG15;                  /* SAVE RC AROUND MODESET        */ 00382000
         STH   REG15,OUTRTCD(,OCMTBPTR)                            0152 00383000
*   GEN(MODESET EXTKEY=SCHED);      /* RETURN TO KEY 1               */ 00384000
         MODESET EXTKEY=SCHED                                           00385000
*   KEY1=YES;                       /* INDICATE BACK TO KEY1         */ 00386000
         OI    KEY1(OCMTBPTR),B'10000000'                          0154 00387000
*/********************************************************************/ 00388000
*/*              CHECK PARSE RETURN CODE                             */ 00389000
*/********************************************************************/ 00390000
*                                                                  0155 00391000
*   IF OUTRTCD^=ZERO THEN           /* IF PARSE RC IS NON ZERO       */ 00392000
         LH    @14,OUTRTCD(,OCMTBPTR)                              0155 00393000
         LTR   @14,@14                                             0155 00394000
         BZ    @RF00155                                            0155 00395000
*     DO;                                                          0156 00396000
*       IF OUTRTCD=8 THEN           /* ATTN DETECTED DURING PARSE    */ 00397000
         CH    @14,@CH00045                                        0157 00398000
         BNE   @RF00157                                            0157 00399000
*         DO;                                                      0158 00400000
*           CALL TCLRQSTK;          /* CALL RTN TO RESET STACK     0159 00401000
*                                                            @ZA15715*/ 00402000
         BAL   @14,TCLRQSTK                                        0159 00403000
*           GOTO SCANSUB;           /* GO SCAN NEW SUBCMD            */ 00404000
         B     SCANSUB                                             0160 00405000
*         END;                                                     0161 00406000
*       ELSE                        /* ANY OTHER ERROR CONDITION     */ 00407000
*         IF OUTRTCD=4 THEN         /* 4 - USER IN NOPROMPT AND COULD   00408000
*                                      NOT BE PROMPTED               */ 00409000
@RF00157 CLC   OUTRTCD(2,OCMTBPTR),@CH00051                        0162 00410000
         BNE   @RF00162                                            0162 00411000
*           IF NONTERM=YES THEN     /* INPUT NOT FROM TERMINAL       */ 00412000
         TM    NONTERM(OCMTBPTR),B'00100000'                       0163 00413000
         BNO   @RF00163                                            0163 00414000
*             DO;                                                  0164 00415000
