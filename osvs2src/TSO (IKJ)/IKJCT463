         AH    REG1,@TF00001                                       0416 01194000
*   SP78LEN=REG1;                   /* SAVE LENGTH OF SP78 CORE      */ 01195000
         STH   REG1,SP78LEN                                        0417 01196000
*   GEN(GETMAIN R,LV=(1),SP=78);    /* GET CORE IN SP 78 (KEY8) FOR     01197000
*                                      HELP PARMLIST                 */ 01198000
         GETMAIN R,LV=(1),SP=78                                         01199000
*   SP78PTR=REG1;                   /* BASE MAP FOR SP78 CORE        */ 01200000
         LR    SP78PTR,REG1                                        0419 01201000
