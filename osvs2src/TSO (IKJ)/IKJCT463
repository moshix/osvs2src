*           GOTO SCANSUB;           /* GO RETRIEVE NEXT SUBCMD       */ 01290000
         B     SCANSUB                                             0446 01291000
*         END;                                                     0447 01292000
*     END;                                                         0448 01293000
*   TCBPTR=REG1;                    /* BASE HELP TCB                 */ 01294000
