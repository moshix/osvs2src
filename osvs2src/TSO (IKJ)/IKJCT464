         TITLE 'IKJCT464 OUTPUT ABNORMAL TERMINATION CLEAN UP          *00001000
                        '                                               00002000
IKJCT464 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKJCT464  78.116'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                  0074 00014000
*/********************************************************************/ 00015000
*/*  START CODE TO CLEAN UP                                          */ 00016000
*/********************************************************************/ 00017000
*                                                                  0074 00018000
*   OCMTBPTR=PLIST;                 /* INIT OUTCOMTB BASE            */ 00019000
         L     OCMTBPTR,PLIST(,REG1)                               0074 00020000
*   REG13=ADDR(EWASA2);             /* PROVIDE A SAVE AREA           */ 00021000
         L     REG13,EWAPTR(,OCMTBPTR)                             0075 00022000
         LA    REG13,EWASA2(,REG13)                                0075 00023000
*/********************************************************************/ 00024000
*/*                                                                  */ 00025000
*/*  THIS ROUTINE WILL CLOSE THE CURRENT SYSOUT DATA SET IF IT IS    */ 00026000
*/*  OPEN AT THE TIME OF ABNORMAL TERMINATION. IT WILL ALSO FREE     */ 00027000
*/*  INPUT BUFFERS                                                   */ 00028000
*/*                                                                  */ 00029000
*/********************************************************************/ 00030000
*                                                                  0076 00031000
*   IF OPENED=YES THEN              /* IS THERE A SYSOUT DS OPEN?    */ 00032000
         TM    OPENED(OCMTBPTR),B'01000000'                        0076 00033000
         BNO   @RF00076                                            0076 00034000
*     DO;                           /* YES                           */ 00035000
*       RFY                                                        0078 00036000
*         IFGRPL BASED(OUTRPL);                                    0078 00037000
*       REG4=RPLAREA;               /* ADDR OF AREA TO FREE          */ 00038000
         L     @14,OUTRPL(,OCMTBPTR)                               0079 00039000
         L     REG4,RPLAREA(,@14)                                  0079 00040000
*       REG8=RPLBUFL;               /* LENGTH OF AREA TO FREE        */ 00041000
