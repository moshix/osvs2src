*         SSSORBA=OUTBKAPX;         /* CKPT BACK, CAUSE TCAM BUFS  0163 00341000
*                                      LOST                          */ 00342000
         MVC   SSSORBA(8,@14),OUTBKAPX(OCMTBPTR)                   0163 00343000
*       ELSE                                                       0164 00344000
*         SSSORBA=OUTHOLD;          /* GIVE SUBSYS THE CKPT RBA      */ 00345000
         B     @RC00162                                            0164 00346000
@RF00162 L     @14,OUTSOBSO(,OCMTBPTR)                             0164 00347000
         MVC   SSSORBA(8,@14),OUTHOLD(OCMTBPTR)                    0164 00348000
*       SSSOUFLG=SSSOUFLG&&SSSOUFLG;/* ZERO GROUP REQUEST FLAGS FOR     00349000
*                                      FINAL CALL                    */ 00350000
@RC00162 L     @14,OUTSOBSO(,OCMTBPTR)                             0165 00351000
