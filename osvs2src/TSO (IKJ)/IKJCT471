*   OCMTBPTR=OUTP;                  /* BASE OUTCOMTB ON REG          */ 00033000
         L     @10,@PC00001                                        0027 00034000
         L     OCMTBPTR,OUTP(,@10)                                 0027 00035000
*   DCBPTR=OUTDCB(STRCTNUM);        /* BASE DCB DSECT                */ 00036000
         L     @10,STRCTNUM(,OCMTBPTR)                             0028 00037000
         MH    @10,@CH00519                                        0028 00038000
         L     DCBPTR,OUTDCB-20(@10,OCMTBPTR)                      0028 00039000
*   DCBSYNA=ADDR(SYNADEXT);         /* PUT SYNAD EXIT ADDR IN DCB    */ 00040000
         LA    @09,SYNADEXT                                        0029 00041000
         STCM  @09,7,DCBSYNA+52(DCBPTR)                            0029 00042000
*   SWITCHES=OFF;                   /* INITIALIZE SWITCHES TO ZERO   */ 00043000
         MVI   SWITCHES,B'00000000'                                0030 00044000
*   BUFADR=OUTBUFA(STRCTNUM);       /* SAVE PUT BUFFER ADDR          */ 00045000
         L     BUFADR,OUTBUFA-20(@10,OCMTBPTR)                     0031 00046000
*   BUFLEN=OUTBUFL(STRCTNUM);       /* SAVE PUT BUFFER LENGTH        */ 00047000
