*           RECRDPTR=OUTRECA;       /* INIT 'EVEN' REG WITH ADDR OF     00169000
*                                      AREA MOVING FROM (ADDR OF   0062 00170000
*                                      SYSOUT RCD)                   */ 00171000
         L     RECRDPTR,OUTRECA(,OCMTBPTR)                         0062 00172000
*           FROMLEN=OUTRECL|PADCHAR;/* INIT 'ODD' REG WITH LEN OF  0063 00173000
*                                      AREA MOVING FROM (SYSOUT RCD)    00174000
*                                      - PAD CHAR IN HI ORDER BYTE   */ 00175000
         LH    FROMLEN,OUTRECL(,OCMTBPTR)                          0063 00176000
         O     FROMLEN,@CF00496                                    0063 00177000
*           MVCL(BUFADR,RECRDPTR);  /* MOVE SYSOUT RECORD INTO BUFFER*/ 00178000
         MVCL  BUFADR,RECRDPTR                                     0064 00179000
*           BUFADR=OUTBUFA(STRCTNUM);/* RESTORE PUT BUFFER ADDR AFTER   00180000
*                                      MVCL INSTRUCTION              */ 00181000
         L     @10,STRCTNUM(,OCMTBPTR)                             0065 00182000
         MH    @10,@CH00519                                        0065 00183000
         L     BUFADR,OUTBUFA-20(@10,OCMTBPTR)                     0065 00184000
*           BUFLEN=OUTBUFL(STRCTNUM);/* RESTORE PUT BUFFER LENGTH  0066 00185000
*                                      AFTER MVCL INSTRUCTION        */ 00186000
         LH    BUFLEN,OUTBUFL-20(@10,OCMTBPTR)                     0066 00187000
*           FROMLEN=FROMLEN&&PADCHAR;/* REMOVE PADDING CHAR FROM HI     00188000
*                                      BYTE BEFORE TESTING FOR A   0067 00189000
*                                      REMAINDER                     */ 00190000
         X     FROMLEN,@CF00496                                    0067 00191000
*           IF FROMLEN>0 THEN       /* SYSOUT BUFFER NOT EMPTY       */ 00192000
         LTR   FROMLEN,FROMLEN                                     0068 00193000
         BNP   @RF00068                                            0068 00194000
*             DO;                   /* PREPARE FOR NEXT LOOP THROUGH    00195000
*                                      MVCL                          */ 00196000
*               OUTRECA=RECRDPTR;   /* SET INDEX TO POINT TO       0070 00197000
*                                      REMAINDER OF SYSOUT RECORD    */ 00198000
         ST    RECRDPTR,OUTRECA(,OCMTBPTR)                         0070 00199000
*               OUTRECL=FROMLEN;    /* RESET RECORD LENGTH EQUAL TO     00200000
*                                      REMAINING LENGTH              */ 00201000
         STH   FROMLEN,OUTRECL(,OCMTBPTR)                          0071 00202000
*             END;                                                 0072 00203000
*           ELSE                    /* SYSOUT BUFFER IS EMPTY        */ 00204000
*             AGAIN=NO;             /* DO NOT LOOP AGAIN             */ 00205000
         B     @RC00068                                            0073 00206000
@RF00068 NI    AGAIN,B'01111111'                                   0073 00207000
*         END;                                                     0074 00208000
@RC00068 DS    0H                                                  0075 00209000
*                                                                  0075 00210000
*     /***************************************************************/ 00211000
*     /*                                                             */ 00212000
*     /* SET UP TO ISSUE 'PUT'                                       */ 00213000
*     /*                                                             */ 00214000
*     /***************************************************************/ 00215000
*                                                                  0075 00216000
*     IF OUTRECUN(STRCTNUM)=YES THEN/* IF UNDEFINED RECORD LENGTH    */ 00217000
@RC00052 DS    0H                                                  0075 00218000
@RC00043 L     @10,STRCTNUM(,OCMTBPTR)                             0075 00219000
