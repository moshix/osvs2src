@RF00035 TM    EODSW(OCMTBPTR),B'00010000'                         0043 00101000
         BNO   @RF00043                                            0043 00102000
*       DO;                                                        0044 00103000
*         BLANKPTR=ADDR(BLANK);     /* INIT 'EVEN' REG WITH ADDR OF     00104000
*                                      AREA MOVING FROM              */ 00105000
         LA    BLANKPTR,@CC00493                                   0045 00106000
*         FROMLEN=LENGTH(BLANK)|PADCHAR;/* INIT 'ODD' REG WITH LENGTH   00107000
*                                      OF AREA MOVING FROM - PAD CHAR   00108000
*                                      IN HI ORDER BYTE              */ 00109000
         L     FROMLEN,@CF00527                                    0046 00110000
*         MVCL(BUFADR,BLANKPTR);    /* BLANK BUFFER                  */ 00111000
         MVCL  BUFADR,BLANKPTR                                     0047 00112000
*         AGAIN=NO;                 /* DON'T LOOP TO 'PUT' ANY OTHER    00113000
*                                      RECORDS ON THIS CALL          */ 00114000
         NI    AGAIN,B'01111111'                                   0048 00115000
*         BUFADR=OUTBUFA(STRCTNUM); /* RESTORE 'PUT' BUFFER ADDR   0049 00116000
*                                      AFTER MVCL INSTR              */ 00117000
         L     @10,STRCTNUM(,OCMTBPTR)                             0049 00118000
         MH    @10,@CH00519                                        0049 00119000
         L     BUFADR,OUTBUFA-20(@10,OCMTBPTR)                     0049 00120000
*         BUFLEN=OUTBUFL(STRCTNUM); /* RESTORE 'PUT' BUFFER LENGTH 0050 00121000
*                                      AFTER MVCL INSTR              */ 00122000
         LH    BUFLEN,OUTBUFL-20(@10,OCMTBPTR)                     0050 00123000
*       END;                                                       0051 00124000
*                                                                  0052 00125000
*     /***************************************************************/ 00126000
*     /*                                                             */ 00127000
*     /* IF OUTPUT INTERRUPTED - FILL 'PUT' BUFFER WITH MSG          */ 00128000
*     /*                                                             */ 00129000
*     /***************************************************************/ 00130000
*                                                                  0052 00131000
*     ELSE                                                         0052 00132000
*       IF INTRPMSG=YES THEN                                       0052 00133000
         B     @RC00043                                            0052 00134000
@RF00043 TM    INTRPMSG(OCMTBPTR),B'00000010'                      0052 00135000
         BNO   @RF00052                                            0052 00136000
*         DO;                                                      0053 00137000
*           MSGPTR=ADDR(PUTMSG);    /* INIT 'EVEN' REG WITH ADDR OF     00138000
*                                      AREA MOVING FROM (ADDR OF MSG)*/ 00139000
         LA    MSGPTR,PUTMSG                                       0054 00140000
*           FROMLEN=LENGTH(PUTMSG)|PADCHAR;/* INIT 'ODD' REG WITH LEN   00141000
*                                      OF AREA MOVING FROM (MSG LEN)    00142000
*                                      - PAD CHAR IN HI ORDER BYTE   */ 00143000
         L     FROMLEN,@CF00528                                    0055 00144000
*           MVCL(BUFADR,MSGPTR);    /* MOVE MSG INTO BUFFER          */ 00145000
         MVCL  BUFADR,MSGPTR                                       0056 00146000
*           INTRPMSG=NO;            /* TURN FLAG OFF                 */ 00147000
         NI    INTRPMSG(OCMTBPTR),B'11111101'                      0057 00148000
*           BUFADR=OUTBUFA(STRCTNUM);/* RESTORE 'PUT' BUFFER ADDR  0058 00149000
*                                      AFTER MVCL INSTRUCTION        */ 00150000
         L     @10,STRCTNUM(,OCMTBPTR)                             0058 00151000
         MH    @10,@CH00519                                        0058 00152000
         L     BUFADR,OUTBUFA-20(@10,OCMTBPTR)                     0058 00153000
*           BUFLEN=OUTBUFL(STRCTNUM);/* RESTORE 'PUT' BUFFER LEN   0059 00154000
*                                      AFTER MVCL INSTRUCTION        */ 00155000
         LH    BUFLEN,OUTBUFL-20(@10,OCMTBPTR)                     0059 00156000
*         END;                                                     0060 00157000
*                                                                  0061 00158000
*     /***************************************************************/ 00159000
*     /*                                                             */ 00160000
