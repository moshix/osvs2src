         ALR   @10,OCMTBPTR                                        0075 00221000
         TM    OUTRECUN-20(@10),B'00000001'                        0075 00222000
         BNO   @RF00075                                            0075 00223000
*       DCBLRECL=132;               /* MAKE THEM ALL 132 UNBLOCKED   */ 00224000
*                                                                  0076 00225000
         MVC   DCBLRECL+72(2,DCBPTR),@CH00524                      0076 00226000
*     /***************************************************************/ 00227000
*     /*                                                             */ 00228000
*     /* ISSUE 'PUT'                                                 */ 00229000
*     /*                                                             */ 00230000
*     /***************************************************************/ 00231000
*                                                                  0077 00232000
*     REG1=OUTDCB(STRCTNUM);        /* PARAMETER FOR 'PUT'           */ 00233000
@RF00075 L     @10,STRCTNUM(,OCMTBPTR)                             0077 00234000
