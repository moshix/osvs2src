*         BUFFER(THREE:FOUR)=ZERO;  /* ZERO SECOND 2 BYTES           */ 00081000
         SLR   @10,@10                                             0038 00082000
         STH   @10,BUFFER+2(,BUFADR)                               0038 00083000
*         BUFADR=BUFADR+FOUR;       /* ADJ BUFFER POSITION SO AS     */ 00084000
         LA    @10,4                                               0039 00085000
         ALR   BUFADR,@10                                          0039 00086000
*         BUFLEN=BUFLEN-FOUR;       /* NOT TO OVERLAY FIRST WORD     */ 00087000
