         L     REG2,OUTBUFA-20(@10,OCMTBPTR)                       0078 00238000
*     GEN(PUT (1),(2));             /* PUT                           */ 00239000
         PUT (1),(2)                                                    00240000
*                                                                  0080 00241000
*     /***************************************************************/ 00242000
*     /*                                                             */ 00243000
*     /* UPDATE CKPT CTRS IF PROCESSING FOR 'PRINT'                  */ 00244000
*     /*                                                             */ 00245000
*     /***************************************************************/ 00246000
*                                                                  0080 00247000
*     IF SUBCMODE=NO&SYNADERR=NO THEN/* PUT FOR PRINT, RATHER THAN 0080 00248000
*                                      SAVE PROCESSING, AND PUT WAS     00249000
*                                      SUCCESSFUL                    */ 00250000
         TM    SUBCMODE(OCMTBPTR),B'01000000'                      0080 00251000
         BNZ   @RF00080                                            0080 00252000
         TM    SYNADERR(OCMTBPTR),B'10000000'                      0080 00253000
         BNZ   @RF00080                                            0080 00254000
*       DO;                         /* FOR PRINT, UPDATE CTRS TO THE    00255000
*                                      RBA CORRESPONDING TO APPROX 10   00256000
*                                      RCDS BACK IN CASE AN ATTN   0081 00257000
*                                      OCCURS AND WIPES OUT RCDS IN     00258000
*                                      THE TCAM BUFFERS              */ 00259000
*         OUTBKCNT=OUTBKCNT+ONE;    /* INCRE CTR OF LINES PUT        */ 00260000
         LA    @10,1                                               0082 00261000
         AL    @10,OUTBKCNT(,OCMTBPTR)                             0082 00262000
         ST    @10,OUTBKCNT(,OCMTBPTR)                             0082 00263000
*         IF OUTBKCNT>TEN THEN      /* MORE THAN 10 RCDS PUT SINCE 0083 00264000
*                                      LAST APPROXIMATION            */ 00265000
         CH    @10,@CH00504                                        0083 00266000
         BNH   @RF00083                                            0083 00267000
*           DO;                                                    0084 00268000
*             OUTBKAPX=OUTBKNEW;    /* UPDATE OFFICIAL RBA OF 10   0085 00269000
*                                      LINES BACK.                   */ 00270000
         MVC   OUTBKAPX(8,OCMTBPTR),OUTBKNEW(OCMTBPTR)             0085 00271000
*             OUTBKNEW=OUTHOLD;     /* UPDATE NEW APPROXIMATION      */ 00272000
         MVC   OUTBKNEW(8,OCMTBPTR),OUTHOLD(OCMTBPTR)              0086 00273000
*             OUTBKCNT=ZERO;        /* SET RCD CTR BACK TO ZERO      */ 00274000
         SLR   @10,@10                                             0087 00275000
         ST    @10,OUTBKCNT(,OCMTBPTR)                             0087 00276000
*           END;                                                   0088 00277000
*         ELSE                                                     0089 00278000
*           ;                       /* HAVE NOT PUT 10 RCDS SINCE  0089 00279000
*                                      LAST APPROXIMATION            */ 00280000
@RF00083 DS    0H                                                  0090 00281000
*       END;                        /* END OF CTR UPDATES            */ 00282000
*     ELSE                                                         0091 00283000
*       ;                           /* SAVE IS PROCESSING, OR PUT  0091 00284000
*                                      ERROR, DO NOT FIGURE A NEW  0091 00285000
*                                      APPROXIMATION                 */ 00286000
@RF00080 DS    0H                                                  0092 00287000
