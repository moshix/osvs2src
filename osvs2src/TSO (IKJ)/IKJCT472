         TITLE 'IKJCT472 OUTPUT    ATTENTION EXIT                      *00001000
                        '                                               00002000
IKJCT472 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKJCT472  74.107'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN R,LV=(0)                                               00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @11,8(,@13)                                         0001 00020000
         LR    @13,@11                                             0001 00021000
         XC    @ZTEMPS(@ZLEN),@ZTEMPS                                   00022000
         MVC   @PC00001(12),0(@01)                                 0001 00023000
*                                                                  0021 00024000
*/********************************************************************/ 00025000
*/*                                                                  */ 00026000
*/*   THE BASIC FLOW OF IKJCT472, THE ATTENTION EXIT, CAN BE         */ 00027000
*/*   OBTAINED FROM THE FOLLOWING SECTION OF CODE:                   */ 00028000
*/*                                                                  */ 00029000
*/*          1. RETRIEVE THE SUBCMD                                  */ 00030000
*/*          2. STOP HELP TCB IF ACTIVE                              */ 00031000
*/*          3. POST ATTENTION ECB                                   */ 00032000
*/*          4. RETURN TO CALLER                                     */ 00033000
*/*                                                                  */ 00034000
*/********************************************************************/ 00035000
*                                                                  0021 00036000
*   POSTED=OFF;                     /* IN CASE A PREVIOUS ATTN IS AS    00037000
*                                      YET UNDETECTED, TURN OFF SO 0021 00038000
*                                      PUTGET WILL RETRIEVE THIS   0021 00039000
*                                      LATEST SUBCMD.                */ 00040000
         L     @14,@PC00001+8                                      0021 00041000
         NI    POSTED(@14),B'10111111'                             0021 00042000
*   SKIPATTN=OFF;                   /* INIT 'IGNORE ATTN' SWITCH     */ 00043000
         NI    SKIPATTN,B'01111111'                                0022 00044000
*   CALL GETSUBCM;                  /* CALL RTN TO GET SUBCMD        */ 00045000
         BAL   @14,GETSUBCM                                        0023 00046000
*   IF SKIPATTN=NO THEN             /* IF ATTN NOT TO BE IGNORED     */ 00047000
         TM    SKIPATTN,B'10000000'                                0024 00048000
         BNZ   @RF00024                                            0024 00049000
*     CALL CKHELP;                  /* CALL RTN TO STOP HELP         */ 00050000
         BAL   @14,CKHELP                                          0025 00051000
*   IF SKIPATTN=NO&ERROR=NO THEN    /* IF ATTN NOT TO BE IGNORED AND    00052000
*                                      NO ERROR OCCURRED             */ 00053000
@RF00024 TM    SKIPATTN,B'10000000'                                0026 00054000
         BNZ   @RF00026                                            0026 00055000
         L     @14,@PC00001+8                                      0026 00056000
         TM    ERROR(@14),B'00001000'                              0026 00057000
         BNZ   @RF00026                                            0026 00058000
*     POSTED=ON;                    /* POST ATTN ECB                 */ 00059000
         OI    POSTED(@14),B'01000000'                             0027 00060000
*   RETURN;                         /* CONTINUE PROCESSING AT POINT     00061000
*                                      OF INTERRUPTION             0028 00062000
*                                                                    */ 00063000
@EL00001 L     @13,4(,@13)                                         0028 00064000
@EF00001 L     @00,@SIZDATD                                        0028 00065000
         LR    @01,@11                                             0028 00066000
         FREEMAIN R,LV=(0),A=(1)                                        00067000
@ER00001 LM    @14,@12,12(@13)                                     0028 00068000
         BR    @14                                                 0028 00069000
*                                                                  0029 00070000
*/********************************************************************/ 00071000
*/*                                                                  */ 00072000
*/* THIS ROUTINE IS CALLED TO ISSUE PUTGET ATTN AND RETRIEVE THE     */ 00073000
*/* SUBCMD - THE ADDR OF THE SUBCMD IS SAVED IN OUTCOMTB.            */ 00074000
*/* AN INDICATION IS GIVEN AS TO THE SOURCE OF THE LINE (TERMINAL    */ 00075000
*/* OR NON TERMINAL).                                                */ 00076000
*/*                                                                  */ 00077000
*/********************************************************************/ 00078000
*                                                                  0029 00079000
*GETSUBCM:                                                         0029 00080000
*   PROC;                           /* INTERNAL PROC TO ISSUE PUTGET    00081000
*                                      AND RETRIEVE            SUBCMD*/ 00082000
GETSUBCM STM   @14,@12,@SA00002                                    0029 00083000
*/********************************************************************/ 00084000
*/*  INITIALIZE THE IOPL                                             */ 00085000
*/********************************************************************/ 00086000
*                                                                  0030 00087000
*   IOPLPTR=ADDR(IOPLAREA);         /* BASE IOPL                     */ 00088000
         L     @14,@PC00001+8                                      0030 00089000
         LA    @10,IOPLAREA(,@14)                                  0030 00090000
         ST    @10,IOPLPTR(,@14)                                   0030 00091000
*   IOPLUPT=CPPLUPT;                /* ADDR OF UPT IOPL              */ 00092000
         L     @09,CPPLPTR(,@14)                                   0031 00093000
         MVC   IOPLUPT(4,@10),CPPLUPT(@09)                         0031 00094000
*   IOPLECT=CPPLECT;                /* ADDR OF ECT IN IOPL           */ 00095000
         MVC   IOPLECT(4,@10),CPPLECT(@09)                         0032 00096000
*   IOPLECB=ADDR(OUTATTN);          /* ADDR OF ECB IN IOPL           */ 00097000
         LA    @09,OUTATTN(,@14)                                   0033 00098000
         ST    @09,IOPLECB(,@10)                                   0033 00099000
*   IOPLIOPB=ADDR(PTGTLSTA);        /* ADDR OF PGPB IN IOPL          */ 00100000
         LA    @09,PTGTLSTA                                        0034 00101000
         ST    @09,IOPLIOPB(,@10)                                  0034 00102000
*/********************************************************************/ 00103000
*/*  FILL IN OLD                                                     */ 00104000
*/********************************************************************/ 00105000
*                                                                  0035 00106000
*   NUMSEGS=ONE;                    /* ONE MSG SEGMENT               */ 00107000
         MVC   NUMSEGS(4),@CF00035                                 0035 00108000
*   SEG1ADDR=ADDR(SEGMENT1);        /* ADDR MSGID CONSTANT SEG       */ 00109000
         LA    @10,SEGMENT1                                        0036 00110000
         ST    @10,SEG1ADDR                                        0036 00111000
*/********************************************************************/ 00112000
*/*  FILL IN MSG SEGMENT 1                                           */ 00113000
*/********************************************************************/ 00114000
*                                                                  0037 00115000
*   SEG1LEN=LENGTH(SEG1TEXT)+4;     /* LEN CMDNM + HEADER            */ 00116000
         MVC   SEG1LEN(2),@CH00223                                 0037 00117000
*   SEG1OFST=SEG1OFST&&SEG1OFST;    /* SEG1 OFFSET 0                 */ 00118000
         LH    @10,SEG1OFST                                        0038 00119000
         LR    @09,@10                                             0038 00120000
         XR    @09,@10                                             0038 00121000
         STH   @09,SEG1OFST                                        0038 00122000
*   SEG1TXTA=' ';                                                  0039 00123000
         MVI   SEG1TXTA,C' '                                       0039 00124000
*   SEG1TXTB=OUTCMDNM;              /* MODE MSG IS CMD NAME          */ 00125000
         MVC   SEG1TXTB(8),OUTCMDNM(@14)                           0040 00126000
*   PTGTLSTA(1:PTGTLNGS)=PTGTLSTS(1:PTGTLNGS);/* LIST TO DYN CORE    */ 00127000
         L     @14,PTGTLNGS                                        0041 00128000
         BCTR  @14,0                                               0041 00129000
         EX    @14,@SM00228                                        0041 00130000
*   OLDADDR=ADDR(OLD);              /* PUT 'OLD' ADDR IN PGPB        */ 00131000
         LA    @14,OLD                                             0042 00132000
         ST    @14,OLDADDR                                         0042 00133000
*   GEN(MODESET EXTKEY=SUPR);       /* KEY 0 FOR I/O SERV. RTNS      */ 00134000
         MODESET EXTKEY=SUPR                                            00135000
*   REG1=ADDR(IOPL);                /* SET R1 W/ IOPL PTR FOR PUTGET */ 00136000
         L     @14,@PC00001+8                                      0044 00137000
         L     REG1,IOPLPTR(,@14)                                  0044 00138000
*   GEN(PUTGET MF=(E,(1)));         /* ISSUE PUTGET                  */ 00139000
         PUTGET MF=(E,(1))                                              00140000
*   RETNCODE=REG15;                 /* SAVE PUTGET RETURN CODE       */ 00141000
         LR    RETNCODE,REG15                                      0046 00142000
*   GEN(MODESET EXTKEY=SCHED);      /* RETURN TO KEY 1               */ 00143000
         MODESET EXTKEY=SCHED                                           00144000
*/********************************************************************/ 00145000
*/*  CHECK PUTGET RETURN CODE                                        */ 00146000
*/********************************************************************/ 00147000
*                                                                  0048 00148000
*   IF RETNCODE=ZERO|RETNCODE=FOUR THEN/* PUTGET COMPLETED NORMALLY  */ 00149000
         LTR   RETNCODE,RETNCODE                                   0048 00150000
         BZ    @RT00048                                            0048 00151000
         CH    RETNCODE,@CH00053                                   0048 00152000
         BNE   @RF00048                                            0048 00153000
@RT00048 DS    0H                                                  0049 00154000
*     DO;                                                          0049 00155000
*       IF SUBCLEN=FOUR THEN        /* NULL LINE ENTERED             */ 00156000
         L     @14,@PC00001+8                                      0050 00157000
         L     @10,IOPLPTR(,@14)                                   0050 00158000
         L     @10,PGPBPTR(,@10)                                   0050 00159000
         L     @10,PGPBIBUF(,@10)                                  0050 00160000
         CLC   SUBCLEN(2,@10),@CH00053                             0050 00161000
         BNE   @RF00050                                            0050 00162000
*         DO;                                                      0051 00163000
*           SKIPATTN=YES;           /* IGNORE ATTN PROCESSING        */ 00164000
         OI    SKIPATTN,B'10000000'                                0052 00165000
*           OUTHOLD=OUTBKAPX;       /* BACK UP TO AVOID LOSING INFO     00166000
*                                      IN BUFFERS WHEN ATTN HIT      */ 00167000
         MVC   OUTHOLD(8,@14),OUTBKAPX(@14)                        0053 00168000
*           OUTBKNEW=OUTBKAPX;      /* BACK UP ALL CTRS '70 USES     */ 00169000
         MVC   OUTBKNEW(8,@14),OUTBKAPX(@14)                       0054 00170000
*           OUTBKCNT=ZEROS;         /* ZERO RCD CTR FOR '70          */ 00171000
         SLR   @10,@10                                             0055 00172000
         ST    @10,OUTBKCNT(,@14)                                  0055 00173000
*           INTRPMSG=YES;           /* VIA MSG TELL USER DS        0056 00174000
*                                      INTERRUPTED                   */ 00175000
*           POINT=YES;              /* TELL IKJCT470 TO POINT BACK 0057 00176000
*                                      BEFORE GET                    */ 00177000
         OI    INTRPMSG(@14),B'10000010'                           0057 00178000
*         END;                                                     0058 00179000
*       ELSE                        /* SUBCMD ENTERED                */ 00180000
*         OUTEMPSB=PGPBIBUF;        /* SAVE SUBCMD ADDR FOR IKJCT462    00181000
*                                      AND IKJCT463                  */ 00182000
         B     @RC00050                                            0059 00183000
@RF00050 L     @14,@PC00001+8                                      0059 00184000
         L     @10,IOPLPTR(,@14)                                   0059 00185000
         L     @10,PGPBPTR(,@10)                                   0059 00186000
         MVC   OUTEMPSB(4,@14),PGPBIBUF(@10)                       0059 00187000
*       IF RETNCODE=FOUR THEN       /* LINE FROM OTHER THAN TERM.    */ 00188000
@RC00050 CH    RETNCODE,@CH00053                                   0060 00189000
         BNE   @RF00060                                            0060 00190000
*         NONTERM=ON;               /* INDICATE NONTERMINAL INPUT    */ 00191000
         L     @14,@PC00001+8                                      0061 00192000
         OI    NONTERM(@14),B'00100000'                            0061 00193000
*       ELSE                        /* LINE FROM TERMINAL            */ 00194000
*         NONTERM=OFF;                                             0062 00195000
         B     @RC00060                                            0062 00196000
@RF00060 L     @14,@PC00001+8                                      0062 00197000
         NI    NONTERM(@14),B'11011111'                            0062 00198000
*     END;                                                         0063 00199000
*   ELSE                            /* PUTGET FAILED - (SHOULD NOT 0064 00200000
*                                      GET ATTN RC=8, CAUSE POST BIT    00201000
*                                      CLEARED ON ENTRY AND AN ATTN     00202000
*                                      IN THIS EXIT CAUSES RETURN TO    00203000
*                                      TMP)                          */ 00204000
*     DO;                                                          0064 00205000
         B     @RC00048                                            0064 00206000
@RF00048 DS    0H                                                  0065 00207000
*       ERROR=YES;                  /* SET QUIT SWITCH               */ 00208000
         L     @14,@PC00001+8                                      0065 00209000
         OI    ERROR(@14),B'00001000'                              0065 00210000
*       OUTRTCD=RETNCODE;           /* PUTGET RC FOR MSG             */ 00211000
         STH   RETNCODE,OUTRTCD(,@14)                              0066 00212000
*       OUTMSGID=24;                /* ISSUE MSG                     */ 00213000
         MVI   OUTMSGID(@14),X'18'                                 0067 00214000
*       CALL IKJCT467(OUTCOMTB);    /* CALL MESSAGE HANDLER          */ 00215000
         ST    @14,@AL00001                                        0068 00216000
         L     @15,@CV00177                                        0068 00217000
         LA    @01,@AL00001                                        0068 00218000
         BALR  @14,@15                                             0068 00219000
*     END;                                                         0069 00220000
*   END GETSUBCM;                   /* END RTN TO CK TGET RC       0070 00221000
*                                                                    */ 00222000
@EL00002 DS    0H                                                  0070 00223000
@EF00002 DS    0H                                                  0070 00224000
@ER00002 LM    @14,@12,@SA00002                                    0070 00225000
         BR    @14                                                 0070 00226000
*                                                                  0071 00227000
*/********************************************************************/ 00228000
*/*                                                                  */ 00229000
*/*     THIS ROUTINE CHECKS TO SEE IF THE HELP CP WAS ACTIVE         */ 00230000
*/*                                                                  */ 00231000
*/********************************************************************/ 00232000
*                                                                  0071 00233000
*CKHELP:                                                           0071 00234000
*   PROC;                           /* INTERNAL PROCEDURE TO SEE IF     00235000
*                                      HELP WAS ACTIVE               */ 00236000
CKHELP   STM   @14,@12,@SA00003                                    0071 00237000
*   IF OUTTCBH^=ZEROTCB THEN        /* IF HELP WAS ACTIVE            */ 00238000
         L     @14,@PC00001+8                                      0072 00239000
         L     @14,OUTTCBH(,@14)                                   0072 00240000
         LTR   @14,@14                                             0072 00241000
         BZ    @RF00072                                            0072 00242000
*     DO;                                                          0073 00243000
*       REG2=OUTTCBH;               /* ADDR HELP TCB                 */ 00244000
         LR    REG2,@14                                            0074 00245000
*       GEN(STATUS    STOP,TCB=(2));/* STOP HELP TCB                 */ 00246000
         STATUS    STOP,TCB=(2)                                         00247000
*       RETNCODE=REG15;             /* SAVE STATUS RETURN CODE       */ 00248000
         LR    RETNCODE,REG15                                      0076 00249000
*       IF RETNCODE^=ZERO THEN      /* STATUS FAILED                 */ 00250000
         LTR   RETNCODE,RETNCODE                                   0077 00251000
         BZ    @RF00077                                            0077 00252000
*         DO;                                                      0078 00253000
*           OUTMSGID=48;            /* ISSUE MSG 313I                */ 00254000
         L     @14,@PC00001+8                                      0079 00255000
         MVI   OUTMSGID(@14),X'30'                                 0079 00256000
*           OUTRTCD=RETNCODE;       /* PASS STATUS RC                */ 00257000
         STH   RETNCODE,OUTRTCD(,@14)                              0080 00258000
*           CALL IKJCT467(OUTCOMTB);/* CALL MSG HANDLER              */ 00259000
         ST    @14,@AL00001                                        0081 00260000
         L     @15,@CV00177                                        0081 00261000
         LA    @01,@AL00001                                        0081 00262000
         BALR  @14,@15                                             0081 00263000
*         END;                                                     0082 00264000
*       GEN(MODESET EXTKEY=SUPR);   /* GOTO KEY0                     */ 00265000
@RF00077 DS    0H                                                  0083 00266000
         MODESET EXTKEY=SUPR                                            00267000
*       REG2=OHELPECB;              /* ADDR HELP ECB                 */ 00268000
         L     @14,@PC00001+8                                      0084 00269000
         L     REG2,OHELPECB(,@14)                                 0084 00270000
*       GEN(POST   (2));            /* POST HELP ECB                 */ 00271000
         POST   (2)                                                     00272000
*       GEN(MODESET EXTKEY=SCHED);  /* RETURN TO KEY1                */ 00273000
         MODESET EXTKEY=SCHED                                           00274000
*     END;                                                         0087 00275000
*   ELSE                                                           0088 00276000
*     ;                             /* HELP NOT ACTIVE               */ 00277000
@RF00072 DS    0H                                                  0089 00278000
*   END CKHELP;                     /* END RTN TO CK IF HELP ACT     */ 00279000
@EL00003 DS    0H                                                  0089 00280000
@EF00003 DS    0H                                                  0089 00281000
@ER00003 LM    @14,@12,@SA00003                                    0089 00282000
         BR    @14                                                 0089 00283000
*   END IKJCT472                    /* END ATTENTION EXIT          0090 00284000
*                                                                    */ 00285000
*                                                                  0090 00286000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 00287000
