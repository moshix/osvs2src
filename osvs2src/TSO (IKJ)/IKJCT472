         ORG   SEG1TEXT                                                 00325000
SEG1TXTA DS    CL1                                                      00326000
SEG1TXTB DS    CL8                                                      00327000
         ORG   SEGMENT1+13                                              00328000
IKJCT472 CSECT                                                          00329000
         DS    0F                                                       00330000
@SIZDATD DC    AL1(1)                                                   00331000
         DC    AL3(@ENDDATD-@DATD)                                      00332000
@CV00177 DC    V(IKJCT467)                                              00333000
         DS    0D                                                       00334000
PATCH72  DS    CL100                                                    00335000
         ORG   PATCH72                                                  00336000
PTCHINIT DC    20CL5'ZAP72'                                             00337000
         ORG   PATCH72+100                                              00338000
IKJCT472 CSECT                                                          00339000
*                                       /* LIST FORM PUTGET - LOCAL  */ 00340000
PTGTLSTS PUTGET OUTPUT=(0,SINGLE,ATTN),                                *00341000
               TERMPUT=(EDIT,WAIT,HOLD,NOBREAK),                       *00342000
               TERMGET=(EDIT,WAIT),MF=L                                 00343000
ENDPTGTL EQU   *                                                        00344000
PTGTLNGS DC     A(ENDPTGTL-PTGTLSTS)       LENGTH OF LIST               00345000
@DATD    DSECT                                                          00346000
*                                       /* LIST FORM PUTGET -AUTO    */ 00347000
PTGTLSTA PUTGET OUTPUT=(0,SINGLE,ATTN),                                *00348000
               TERMPUT=(EDIT,WAIT,HOLD,NOBREAK),                       *00349000
               TERMGET=(EDIT,WAIT),MF=L                                 00350000
@DATD    DSECT                                                          00351000
         ORG   *+1-(*-@DATD)/(*-@DATD) INSURE DSECT DATA                00352000
@ENDDATD EQU   *                                                        00353000
IKJCT472 CSECT                                                          00354000
