*           END;                    /* CONTINUE TERMINATION: ERRORS     00559000
*                                      WERE ENCOUNTERED OR IT WAS AN    00560000
*                                      INCORE PROC                   */ 00561000
*         END;                                                     0205 00562000
*       ELSE                                                       0206 00563000
*         R15=0;                    /* NO SDWA WAS PROVIDED          */ 00564000
         B     @RC00196                                            0206 00565000
@RF00196 SLR   R15,R15                                             0206 00566000
