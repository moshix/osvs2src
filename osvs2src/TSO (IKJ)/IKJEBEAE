*         DO;                                                      0197 00541000
*           R13=RTMSAREA;           /* SAVE AREA ADDRESS             */ 00542000
         L     R13,RTMSAREA(,COMMREG)                              0198 00543000
*           DO;                     /* SETRP RC(0)REGS(14,12)        */ 00544000
*             RESPECIFY                                            0200 00545000
