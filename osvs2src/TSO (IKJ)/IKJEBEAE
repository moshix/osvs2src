*         GEN(RETURN (14,12));                                     0186 00510000
         RETURN (14,12)                                                 00511000
*         RESPECIFY                                                0187 00512000
*          (GPR00P,                                                0187 00513000
*           GPR01P,                                                0187 00514000
*           GPR14P,                                                0187 00515000
*           GPR15P) UNRSTD;                                        0187 00516000
*       END;                                                       0188 00517000
*     END RETRY;                                                   0189 00518000
*   ELSE                                                           0190 00519000
*NORETRY:                                                          0190 00520000
*     DO;                           /* NO RETRY-CALL END TO FINISH 0190 00521000
*                                      CLEANUP                       */ 00522000
         B     @RC00178                                            0190 00523000
@RF00178 DS    0H                                                  0190 00524000
NORETRY  DS    0H                                                  0191 00525000
*       IF AERETRY='0'B THEN                                       0191 00526000
         TM    AERETRY,B'00010000'                                 0191 00527000
         BNZ   @RF00191                                            0191 00528000
*         DO;                       /* GO TO END IF ALL STILL OK     */ 00529000
*           PARMREG1=COMMREG;       /* GET COMM AREA PTR     @ZA09473*/ 00530000
