         SR    INS2REG,INS2REG NO SECOND LEVEL INSERTIONS.       A45714 68715021
         B     IA020001       BRANCH TO CALL IKJEBEMS.           A45714 68720021
         EJECT                                                          68725021
*********************************************************************** 68740020
*                                                                     * 68810020
*   INVOKE IKJEBEMS TO PUT THE MESSAGE TO THE TERMINAL USER.  UPON    * 68880020
*   RETURN FROM IKJEBEMS, RETURN TO THE CALLING ROUTINE.  REGISTER 15 * 68950020
*   ON EXIT FROM THIS ROUTINE WILL CONTAIN THE RETURN CODE PASSED     * 69020020
*   BACK BY IKJEBEMS.                                                 * 69090020
*                                                                     * 69160020
*********************************************************************** 69230020
IA020001 EQU   *                                                        69300020
         IKJEBEML (LEV1REG),(LEV2REG),(INS1REG),(INS2REG),             X69370020
               MF=(E,IAPRMLST) CALL IKJEBEMS.                           69440020
         SPACE 2                                                        69490020
IA020002 EQU   *                                                        69500020
         IKJEBERT (14,12),,RC=(15) RESTORE REGISTERS AND RETURN.        69510020
         EJECT                                                          69580020
*********************************************************************** 69630020
*                                                                       69640020
*    THE FOLLOWING SECTION IS EXECUTED FOR DECIMAL AND HEXADECIMAL      69642020
*    CONVERSIONS AND FORMATTING OF NUMERIC MESSAGE INSERTIONS.          69644020
*                                                                       69646020
*********************************************************************** 69648020
         SPACE 2                                                        69648420
IA100000 EQU   *                                                        69650020
         SR    EPLOCREG,EPLOCREG SPECIFY THAT INSERTION IS NOT A        69720020
*                                  RETURN CODE                          69790020
IA100003 EQU   *                                                        69860020
         CVD   PARMREG1,INDECBIN CONVERT DATA TO PACKED DECIMAL         69930020
         UNPK  D16(D10,LEV2REG),INDECBIN+D4(D5) AND TO EBCDIC.          70000020
         LTR   EPLOCREG,EPLOCREG DETERMINE IF INSERTION IS TO BE        70070020
*                                  PROCESSED AS 2-DIGIT CODE.           70140020
         BM    IA100005       IF SO, BRANCH TO PROCESS.                 70210020
         LTR   PARMREG1,PARMREG1 CHECK FOR NUMERIC INSERTION            70260020
*                                  EQUAL TO ZERO.                       70270020
         BNZ   IA100004       IF NOT ZERO, BRANCH TO PROCESS            70272020
*                                  LEADING ZEROES.                      70274020
         MVI   D8(LEV2REG),IACHRZRO MOVE CHARACTER ZERO TO              70276020
*                                  INSERTION LIST.                      70278020
         LA    INLENREG,D5    LENGTH OF INSERTION IS FIVE BYTES.        70278420
         B     IA100045       BRANCH TO COMPLETE INSERTION.             70278820
IA100004 EQU   *                                                        70279220
         LA    INS2REG,D16(,LEV2REG) ADDRESS OF CONVERTED DATA.         70280020
         LA    INLENREG,D8(,D0) SET COUNTER EQUAL TO DATA LENGTH.       70350020
         B     IA100020       BRANCH TO SUPPRESS LEADING ZEROES.        70420020
IA100005 EQU   *                                                        70490020
         LA    INS2REG,D22(,LEV2REG) ADDRESS OF CONVERTED DATA.         70560020
         LA    INLENREG,D1(,D0) TWO CHARACTER DIGITS TO BE MOVED.       70630020
         B     IA100040       BRANCH TO MOVE RC TO INSERTION.           70700020
IA100010 EQU   *                                                        70770020
         UNPK  D16(D9,LEV2REG),D0(D5,INS2REG) CONVERT DATA TO           70840020
         TR    D16(D8,LEV2REG),IATRNSTB-IACHRZRO PRINTABLE EBCDIC       70910020
         LA    INS2REG,D20(,LEV2REG) ADDRESS OF CONVERTED DATA.         70980020
         LA    INLENREG,D3(,D0) SET DATA LENGTH FOR MVC.                71050020
         B     IA100040       BRANCH TO MOVE 4-CHARACTER FIELD          71120020
*                                  TO INSERTION RECORD.                 71190020
IA100020 EQU   *                                                        71260020
         CLI   D0(INS2REG),IACHRZRO CHECK IF CHARACTER IS A ZERO.       71330020
         BNE   IA100030       IF NOT, BRANCH TO COMPLETE                71400020
*                                  INSERTION RECORD.                    71470020
         LA    INS2REG,D1(,INS2REG) UPDATE POINTER TO NEXT BYTE.        71540020
         BCT   INLENREG,IA100020   REDUCE COUNTER BY 1 AND BRANCH       71610020
*                             TO CHECK NEXT BYTE.                       71680020
IA100030 EQU   *                                                        71750020
         BCTR  INLENREG,D0    REDUCE COUNT BY ONE FOR MVC.              72030020
IA100040 EQU   *                                                        72100020
         EX    INLENREG,IAMVC005 MOVE DATA INTO INSERTION RECORD.       72170020
         LA    INLENREG,D5(,INLENREG) COMPUTE LENGTH OF INSERTION       72240020
*                                  RECORD.                              72310020
IA100045 EQU   *                                                        72360020
         XC    D0(D8,LEV2REG),D0(LEV2REG) ZERO CONTROL PORTION OF       72380020
*                             INSERTION RECORD.                         72450020
         STH   INLENREG,D4(,LEV2REG) SAVE LENGTH OF INSERTION           72520020
*                                  RECORD.                              72590020
         BR    SRRTNREG       RETURN TO CALLER.                         72660020
         EJECT                                                          72710020
*********************************************************************** 72720020
*                                                                       72722020
*    THE FOLLOWING SUBROUTINE IS EXECUTED TO PROCESS A PARTIALLY        72724020
*    QUALIFIED DATA SET NAME AS A MESSAGE INSERTION.                    72726020
*                                                                       72728020
*********************************************************************** 72728420
         SPACE 2                                                        72728820
IA100050 EQU   *                                                        72730020
         LA    DSNAMREG,CAEDDSN+D1 ADDRESS SECOND CHARACTER OF          72800020
*                                  DATA SET NAME.                       72870020
         LA    COUNTREG,D2    MINUMUM NUMBER OF CHARACTERS TO           72940020
*                                  BE STRIPPED IS TWO.                  73010020
IA100060 EQU   *                                                        73080020
         CLI   D0(DSNAMREG),INPERIOD CHECK FOR PERIOD FOLLOWING         73150020
*                                  USERID -                             73220020
         BE    IA100070       IF PERIOD ENCOUNTERED, BRANCH             73290020
*                             TO MOVE INSERTION TEXT.                   73360020
         LA    COUNTREG,D1(,COUNTREG) BUMP COUNT BY ONE.                73430020
         LA    DSNAMREG,D1(,DSNAMREG) ADDRESS NEXT CHARACTER            73500020
*                                  OF DSNAME.                           73570020
         B     IA100060       REPEAT LOOP UNTIL PERIOD FOUND.           73640020
IA100070 EQU   *                                                        73710020
         LH    LENREG,CAEDDSNL GET DSNAME LENGTH.                       73780020
         SR    LENREG,COUNTREG DETERMINE INSERTION TEXT LENGTH.         73850020
         BCTR  LENREG,D0      DECREMENT BY 1 FOR MVC.                   73920020
         EX    LENREG,IAMVC010 MOVE INSERTION TEXT TO INSERTION         73990020
*                                  RECORD.                              74060020
         LA    LENREG,D5(,LENREG) COMPUTE LENGTH FOR INSERTION          74130020
*                                  RECORD.                              74200020
         STH   LENREG,INDSNLEN SET LENGTH IN INSERTION RECORD.          74270020
         SR    LENREG,LENREG  SET INSERTION CHAIN WORD TO ZERO.         74340020
         ST    LENREG,INDSNPTR (THIS IS LAST MESSAGE INSERTION.)        74410020
         BR    SRRTNREG       RETURN TO MAINLINE ROUTINE.               74480020
         SPACE 5                                                        74550020
         EJECT                                                          74600020
*********************************************************************** 74610020
*    THE FOLLOWING SUBROUTINE IS EXECUTED TO STRIP TRAILING BLANKS      74612020
*    FROM MESSAGE INSERTIONS.                                           74614020
*                                                                       74616020
*                                                                       74618020
*********************************************************************** 74618420
         SPACE 2                                                        74618820
IA110010 EQU   *                                                        74620020
         LA    RETREG,D8(,D0) LOOP CONTROL COUNTER.                     74690020
         LA    EPLOCREG,D15(,INS1REG) ADDRESS OF LAST CHARACTER         74760020
*                                  IN DATA SET TYPE.                    74830020
IA110020 EQU   *                                                        74900020
         CLI   D0(EPLOCREG),IACHBLNK CHECK FOR BLANK.                   74970020
         BNE   IA110030       IF NOT A BLANK, ALL TRAILING BLANKS       75040020
*                                  HAVE BEEN SUPPRESSED.                75110020
         BCTR  EPLOCREG,D0    BACK POINTER UP ONE CHARACTER.            75180020
         BCT   RETREG,IA110020 REDUCE COUNTER BY ONE AND BRANCH         75250020
*                                  TO CHECK NEXT BYTE.                  75320020
IA110030 EQU   *                                                        75390020
         XC    D0(D4,INS1REG),D0(INS1REG) ZERO CONTROL FIELDS.          75460020
         LA    RETREG,D4(,RETREG) COMPUTE LENGTH OF INSERTION           75530020
         STH   RETREG,D4(,INS1REG) RECORD AND SAVE.                     75600020
         BR    SRRTNREG       RETURN TO CALLER.                         75670020
         EJECT                                                          75740020
*********************************************************************** 75810020
