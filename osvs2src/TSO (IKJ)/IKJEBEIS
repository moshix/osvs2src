*                                                                  0408 01033000
*        DECLARE /* COMMON VARIABLES */                            0409 01034000
*          I256C CHAR(256) BASED,                                  0409 01035000
*          I031F FIXED(31) BASED,                                  0409 01036000
*          I031P PTR(31)   BASED,                                  0409 01037000
*          I015F FIXED(15) BASED,                                  0409 01038000
*          I015P PTR(15)   BASED,                                  0409 01039000
*          I008P PTR(8)    BASED,                                  0409 01040000
*          I001C CHAR(1)   BASED;                                  0409 01041000
*        END IKJEBEIS                  /* END OF INSERT SUBCOMMAND    * 01042000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.      * 01043000
