*                                         WRITTEN                     * 00783000
         MVC   UTRECPT(4),WREG1                                    0327 00784000
*        LBIT2 = '1'B;                 /* INDICATE 2-WORD LIST        * 00785000
         OI    LBIT2,B'10000000'                                   0328 00786000
*        CALL IORTN;                   /* GO WRITE RECORD IN DS       * 00787000
*                                                                  0329 00788000
         BAL   @14,IORTN                                           0329 00789000
*        /* CHECK RETURN CODE FROM IKJEBEUT                           * 00790000
*        IF RCODEREG ^= 0                                          0330 00791000
*          THEN                        /* WRITE WAS UNSUCCESSFUL ,    * 00792000
         LTR   RCODEREG,RCODEREG                                   0330 00793000
         BZ    @RF00330                                            0330 00794000
*            DO;                                                   0331 00795000
*              RTREG = ADDR (PRSEXIT); /* SET RTURN ADDR              * 00796000
         L     @12,BACKREG(,SAVEREG)                               0332 00797000
         LA    @15,PRSEXIT                                         0332 00798000
         ST    @15,RTREG(,@12)                                     0332 00799000
*              RETURN;                 /* RETURN TO COMMAND MODE      * 00800000
@EL00002 DS    0H                                                  0333 00801000
@EF00002 DS    0H                                                  0333 00802000
@ER00002 BR    @14                                                 0333 00803000
*            END;                                                  0334 00804000
*          ELSE;                       /* WRITE WAS SUCCESSFUL        * 00805000
*                                                                  0335 00806000
@RF00330 DS    0H                                                  0336 00807000
*        /* CHECK FOR END-OF-LOOP                                     * 00808000
*        IF WREG2 = 0                                              0336 00809000
*          THEN DO;                    /* YES, FINISHED               * 00810000
         ICM   @15,15,WREG2                                        0336 00811000
         BNZ   @RF00336                                            0336 00812000
*                 RETREG = CLNPTR;     /* RESTORE RETURN POINT ADDRESS* 00813000
         LR    RETREG,CLNPTR                                       0338 00814000
*          /* RETURN TO CALLING ROUTINE                               * 00815000
*                 RETURN;              /* RETURN                      * 00816000
         B     @EL00002                                            0339 00817000
*               END;                                               0340 00818000
*          ELSE;                       /* NO, SLOT WAS NOT FOUND      * 00819000
*                                                                  0341 00820000
@RF00336 DS    0H                                                  0342 00821000
*        /* THE NEXT 3 INSTRUCTIONS SWAP THE BUFFER PTRS              * 00822000
*        GEN REFS(WREG1,WREG2);                                    0342 00823000
         XC    WREG1,WREG2                                              00824000
         XC    WREG2,WREG1                                              00825000
         XC    WREG1,WREG2                                              00826000
*        GOTO CLNLP;                   /* CONTINUE LOOPING            * 00827000
         B     CLNLP                                               0343 00828000
*        END CLNSAVE;                  /* END OF CLNSAVE SUBROUTINE   * 00829000
*IORTN:  /* ********************************************************* * 00830000
*        /* THIS ROUTINE PERFORMS ALL I/O REQUESTS ON THE UTILITY     * 00831000
*        /* DATA SET. UPON ENTRY, ALL PARAMETERS IN UTPRMLST MUST     * 00832000
*        /* BE INITIALIZED BY THE CALLER FOR THE DESIRED OPERATION.   * 00833000
*        /* THIS ROUTINE ALSO CHECKS END-OF-DATA FROM A READ NEXT     * 00834000
*        /* (RC=4) AND INITIALIZES EODSW TO 1 WHEN IT OCCURS. THIS    * 00835000
*        /* CONDITION ALSO CAUSES 'NEXTREC' TO BE INITIALIZED TO      * 00836000
*        /* 99999999 FOR A NUMBERED DATA SET OR 2,147,483,648         * 00837000
*        /* (2**31-1) FOR A NON-LINENUMBERED DATA SET. IF AN I/O      * 00838000
*        /* ERROR IS ENCOUNTERED, 'CCODE' IS SET TO 12 AND THIS       * 00839000
*        /* ROUTINE GOES DIRECTLY TO 'EXIT'. RETURN CODES FOR         * 00840000
*        /* CALLER ARE: FOR OP CODES 00-20 THE RETURN CODE IS 0 OR    * 00841000
*        /* 4 FOR OP CODE 20 A RETURN CODE OF 12 IS ADDED THESE       * 00842000
*        /* RETURN CODES ARE AS DESCRIBED FOR IKJEBEUT REGISTER 1     * 00843000
*        /* WILL BE LEFT INTACT FOLLOWING A READ.                     * 00844000
*        /*                                                           * 00845000
*        /* PERFORM I/O OPERATION BY CALLING IKJEBEUT.  CHECK FOR EOD * 00846000
*        /* ON PREVIOUS READ NEXT                                     * 00847000
*        /* ********************************************************* * 00848000
*        PROC OPTIONS (NOSAVE, NOSAVEAREA);                        0345 00849000
*                                                                  0345 00850000
IORTN    DS    0H                                                  0346 00851000
*        IORTNPTR = RETREG;            /* SAVE RETURN POINT ADDR      * 00852000
         ST    RETREG,IORTNPTR                                     0346 00853000
*        /* THESE REGISTERS ARE ALWAYS RESTRICTED                     * 00854000
*        RESPECIFY (COMMREG, BASEREG, DATAREG, RCODEREG) RSTD;     0347 00855000
*        RESPECIFY (PARMREG1) RSTD;    /* RESTRICTED ONLY FOR IORTN   * 00856000
*        CODESAVE = 0;                 /* INIT RETURN CODE            * 00857000
*                                                                  0349 00858000
         SLR   @14,@14                                             0349 00859000
         LR    CODESAVE,@14                                        0349 00860000
*        /* CHECK FOR PREVIOUS EOD FROM READ                          * 00861000
*        IF EODSW = '1'B                                           0350 00862000
*          THEN                        /* YES EOD HAS BEEN REACHED    * 00863000
         TM    EODSW,B'00100000'                                   0350 00864000
         BNO   @RF00350                                            0350 00865000
*            DO;                                                   0351 00866000
*                                                                  0351 00867000
*              /* IGNORE IF OPERATION WAS A WRITE                     * 00868000
*              IF UTOPTCD ^= '20'X                                 0352 00869000
*                THEN                  /* NO, OPERATION NOT WRITE     * 00870000
         CLI   UTOPTCD,X'20'                                       0352 00871000
         BE    @RF00352                                            0352 00872000
*                  DO;                                             0353 00873000
*                    RCODEREG = 0;       /* SET NORMAL RETURN CODE    * 00874000
         LR    RCODEREG,@14                                        0354 00875000
*                    GOTO IORET;       /* RETURN TO CALLER            * 00876000
         B     IORET                                               0355 00877000
*                  END;                                            0356 00878000
*            END;                                                  0357 00879000
*          ELSE;                       /* NO PREVIOUS EOD - ISSUE I/O   00880000
*                                         REQUEST                     * 00881000
*                                                                  0358 00882000
@RF00350 DS    0H                                                  0359 00883000
*        /* CALL UTILITY ACCESS METHOD                                * 00884000
*        GEN;                                                      0359 00885000
*                                                                  0359 00886000
@RC00350 DS    0H                                                  0359 00887000
         IKJEBESH (COMMREG),IKJEBEUT,PARAM=((COMMREG),UTPRMLST),       X00888000
               MF=(E,SRWORK)                                            00889000
*        /* CHECK RETURN CODE                                         * 00890000
*        IF RCODEREG = 0                                           0360 00891000
*          THEN                        /* OPERATION SUCCESSFUL        * 00892000
         LTR   RCODEREG,RCODEREG                                   0360 00893000
         BZ    @RT00360                                            0360 00894000
*            GOTO IORET;               /* RETURN TO CALLER            * 00895000
*          ELSE;                       /* OPERATION UNSUCCESSFUL      * 00896000
*        CODESAVE = RCODEREG;          /* SAVE RETURN CODE            * 00897000
*                                                                  0363 00898000
         LR    CODESAVE,RCODEREG                                   0363 00899000
*        /* CHECK FOR RECORD NOT FOUND INDICATION                     * 00900000
*        IF CODESAVE = 4                                           0364 00901000
*          THEN                        /* YES, RECORD NOT FOUND       * 00902000
         CH    CODESAVE,@CH00407                                   0364 00903000
         BNE   @RF00364                                            0364 00904000
*            /* DETERMINE IF OPERATION WAS 'READ NEXT'                * 00905000
*            DO;                                                   0365 00906000
*                                                                  0365 00907000
*              /* IF I/O WAS NOT READ NEXT, RETURN                    * 00908000
*              IF UTOPTCD = '02'X                                  0366 00909000
*                THEN                  /* I/O WAS READ NEXT           * 00910000
         CLI   UTOPTCD,X'02'                                       0366 00911000
         BNE   @RF00366                                            0366 00912000
*                  /* SET END-OF-DATA SWITCH AND RETURN TO CALLER     * 00913000
*                  DO;                                             0367 00914000
*                    EODSW = '1'B;     /* INDICATE END-OF-DATA        * 00915000
         OI    EODSW,B'00100000'                                   0368 00916000
*                    GOTO IORET;       /* RETURN TO CALLER            * 00917000
         B     IORET                                               0369 00918000
*                  END;                                            0370 00919000
*                ELSE                  /* CONTINUE CHECKING RET CODE  * 00920000
*                  GOTO IORET;         /* RETURN TO CALLER            * 00921000
*            END;                                                  0372 00922000
*          ELSE;                       /* CONTINUE CHECKING RET CODE  * 00923000
*                                                                  0373 00924000
@RF00364 DS    0H                                                  0374 00925000
*        /* CHECK FOR I/O ERROR ON UTILITY DATA SET                   * 00926000
*        IF CODESAVE = 8                                           0374 00927000
*          THEN                        /* YES I/O ERROR OCCURRED      * 00928000
         CH    CODESAVE,@CH00048                                   0374 00929000
         BNE   @RF00374                                            0374 00930000
*            DO;                                                   0375 00931000
*              CCODE = 12;             /* SET RETURN CODE             * 00932000
         LA    CCODE,12                                            0376 00933000
*              RFY SAVEREG RSTD;                                   0377 00934000
*              RTREG = ADDR (EXIT);    /* RTURN TO COMMAND MODE       * 00935000
         L     @14,BACKREG(,SAVEREG)                               0378 00936000
         LA    @00,EXIT                                            0378 00937000
         ST    @00,RTREG(,@14)                                     0378 00938000
*              RFY SAVEREG UNRSTD;                                 0379 00939000
*              RETURN;                 /* RETURN                      * 00940000
@EL00003 DS    0H                                                  0380 00941000
@EF00003 DS    0H                                                  0380 00942000
@ER00003 BR    @14                                                 0380 00943000
*            END;                                                  0381 00944000
*          ELSE;                       /* RETURN CODE IS NOT 8        * 00945000
@RF00374 DS    0H                                                  0383 00946000
*IORET: ;                              /* BEGIN EXIT PROCESSING       * 00947000
IORET    DS    0H                                                  0384 00948000
*        RCODEREG = CODESAVE;          /* FALLING THROUGH HERE ASSUMES  00949000
*                                         A RETURN CODE OF 12.  RESTORE 00950000
*                                         RETURN CODE                 * 00951000
         LR    RCODEREG,CODESAVE                                   0384 00952000
*        IOPTR = PARMREG1;             /* SET RECORD PTR IN REG 1     * 00953000
         L     @14,BACKREG(,SAVEREG)                               0385 00954000
         ST    PARMREG1,IOPTR(,@14)                                0385 00955000
*        RETREG = IORTNPTR;            /* RESTORE RETURN POINT ADDR   * 00956000
         L     RETREG,IORTNPTR                                     0386 00957000
*        RESPECIFY RCODEREG UNRSTD;                                0387 00958000
*        RETURN;                       /* RETURN                      * 00959000
         B     @EL00003                                            0388 00960000
*        END IORTN;                    /* END OF IO SUBROUTINE        * 00961000
*CVDRTN: /* ********************************************************* * 00962000
*        /* THIS ROUTINE CONVERTS A BINARY NUMBER TO ZONED DECIMAL    * 00963000
*        /* AND PUTS IT IN 'NUMTEMP'. THE NUMBER TO BE CONVERTED IS   * 00964000
*        /* ASSUMED TO BE IN CVDREG.                                  * 00965000
*        /* ********************************************************* * 00966000
*        PROC OPTIONS (NOSAVEAREA, NOSAVE);                        0390 00967000
*                                                                  0390 00968000
CVDRTN   DS    0H                                                  0391 00969000
*        /* THESE REGISTERS ARE ALWAYS RESTRICTED                     * 00970000
*        RESPECIFY (COMMREG, BASEREG, DATAREG) RSTD;               0391 00971000
*                                                                  0391 00972000
*        /* CONVERT INSTRUCTIONS                                      * 00973000
*        GEN (CVD   CVDREG,PACKTMP  CONVERT NUM TO PACKED DECIMAL);     00974000
         CVD   CVDREG,PACKTMP  CONVERT NUM TO PACKED DECIMAL            00975000
*        GEN (UNPK  NUMTEMP(PKLEN),PACKTMP(PKLEN)); /* UNPACK NUMBER  * 00976000
         UNPK  NUMTEMP(PKLEN),PACKTMP(PKLEN)                            00977000
*        GEN (OI    NUMTEMP+PKOFF,ZONESIGN); /* CHANGE SIGN TO ZONE   * 00978000
         OI    NUMTEMP+PKOFF,ZONESIGN                                   00979000
*        RETURN;                       /* RETURN                      * 00980000
@EL00004 DS    0H                                                  0395 00981000
@EF00004 DS    0H                                                  0395 00982000
@ER00004 BR    @14                                                 0395 00983000
*        END CVDRTN;                   /* END OF CVDRTN SUBROUTINE    * 00984000
*MSGRTN: /* ********************************************************* * 00985000
*        /* THIS ROUTINE IS USED TO PUT OUT MESSAGES WHICH REQUIRE    * 00986000
*        /* FIRST AND OR SECOND LEVEL MESSAGES WITH NO INSERTIONS     * 00987000
*        /* RELMSG1 CONTAINS THE MESSAGE NUMBER. RELMSG2 CONTAINS     * 00988000
*        /* THE RELATIVE SECOND LEVEL NUMBER. 0 IF NONE. LEV1INS      * 00989000
*        /* POINTS TO THE FIRST INSERTION FOR THE FIRST LEVEL MSG     * 00990000
*        /* . LEV2INS POINTS TO THE FIRST INSERTION FOR THE           * 00991000
*        /* SECOND-LEVEL MSG.                                         * 00992000
*        /* ********************************************************* * 00993000
*        PROC OPTIONS (NOSAVE, NOSAVEAREA);                        0397 00994000
*                                                                  0397 00995000
MSGRTN   DS    0H                                                  0398 00996000
*        RESPECIFY (BASEREG,DATAREG,SAVEREG,LEV1INS,LEV2INS) RSTD; 0398 00997000
*         MSGPTR = RETREG;            /* SAVE RETURN POINT ADDRESS    * 00998000
