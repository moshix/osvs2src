@EL00005 DS    0H                                                  0402 01005000
@EF00005 DS    0H                                                  0402 01006000
@ER00005 BR    @14                                                 0402 01007000
*        END MSGRTN;                   /* END OF MESSAGE ROUTINE      * 01008000
*                                                                  0403 01009000
*                                                                  0403 01010000
*        DECLARE /*GENERAL PURPOSE REGISTERS */                    0404 01011000
*          GPR00P PTR(31) REG(0),                                  0404 01012000
*          GPR01P PTR(31) REG(1),                                  0404 01013000
*          GPR02P PTR(31) REG(2),                                  0404 01014000
*          GPR03P PTR(31) REG(3),                                  0404 01015000
*          GPR04P PTR(31) REG(4),                                  0404 01016000
*          GPR05P PTR(31) REG(5),                                  0404 01017000
*          GPR06P PTR(31) REG(6),                                  0404 01018000
*          GPR07P PTR(31) REG(7),                                  0404 01019000
*          GPR08P PTR(31) REG(8),                                  0404 01020000
*          GPR09P PTR(31) REG(9),                                  0404 01021000
*          GPR12P PTR(31) REG(12),                                 0404 01022000
*          GPR13P PTR(31) REG(13),                                 0404 01023000
*          GPR14P PTR(31) REG(14),                                 0404 01024000
*          GPR15P PTR(31) REG(15);                                 0404 01025000
*                                                                  0404 01026000
*        /* VARIABLES REQUIRED BY PL/S SYSTEM MACROS */            0405 01027000
*          DECLARE CASAVPTR PTR(31) BASED;/*SAVE AREA PTR FOR IKJEBESV* 01028000
*          DECLARE COMMREG REG(9) PTR(31) RSTD;/*POINTER TO EDIT COMMO  01029000
*REA*/     DECLARE DATAREG REG(10) PTR(31) RSTD;/*BASEREG FOR EDIT SUBC 01030000
*MMAND WORK AREA*/                                                 0406 01031000
