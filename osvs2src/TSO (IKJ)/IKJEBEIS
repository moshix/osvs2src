*        GEN REFS(LEV1INS,LEV2INS);                                0400 01000000
         IKJEBEML ,,(LEV1INS),(LEV2INS),MF=(E,SRWORK)                   01001000
*        RETREG = MSGPTR;              /* RESTORE RETURN POINT ADDRESS* 01002000
         LR    RETREG,MSGPTR                                       0401 01003000
