         MVC   @ZT00001+1(3),USMNEXT(USMPTR)                       0285 00767000
         L     @10,@ZT00001                                        0285 00768000
         LTR   @10,@10                                             0285 00769000
         BZ    @RT00285                                            0285 00770000
*     GOTO BCEND;                   /* CLEAN UP AND RETURN     Y02676*/ 00771000
*   GO TO BCPRMSG;                  /* PROCESS NEXT MESSAGE          */ 00772000
         B     BCPRMSG                                             0287 00773000
*/* ******************************************************************/ 00774000
*/*     INFORM THE USER IF NO BROADCAST MESSAGES OF THE TYPE(S)      */ 00775000
*/*     REQUESTED HAVE BEEN FOUND.                                   */ 00776000
*/* ******************************************************************/ 00777000
*                                                                  0288 00778000
*BCEND:                                                            0288 00779000
*   IF BCNOTICE='0'B&BCMAIL='0'B    /* WERE ANY MSGS REQUESTED?      */ 00780000
