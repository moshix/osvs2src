*     BCMAIL='1'B;                  /* YES, INDICATE MAIL REQUESTED  */ 00241000
         OI    BCMAIL,B'00100000'                                  0128 00242000
*   IF BCNANS=1 THEN                /* WAS NOTICE MSGS REQUESTED?    */ 00243000
@RF00127 L     @10,BCANSPL                                         0129 00244000
         CLC   BCNANS(2,@10),@CH00034                              0129 00245000
         BNE   @RF00129                                            0129 00246000
*     BCNOTICE='1'B;                /* YES, INDICATE NOTICE REQUESTED*/ 00247000
         OI    BCNOTICE,B'00010000'                                0130 00248000
