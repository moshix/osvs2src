*   PPLPCL=ADDR(BCPARS1);           /* PTR TO GENERATED PCL          */ 00140000
         L     @15,@CV00260                                        0094 00141000
         ST    @15,PPLPCL(,@10)                                    0094 00142000
*   PARMRGR=PPLPTR;                 /* INITIALIZE PARM REGISTER      */ 00143000
         LR    PARMRGR,@10                                         0095 00144000
*   DO;                             /* CALLTSSR EP(IKJPARS)          */ 00145000
*     RESPECIFY                                                    0097 00146000
*       GPR01P RSTD;                                               0097 00147000
*     IF CVTPARS=0 THEN                                            0098 00148000
         L     @10,CVTPTR                                          0098 00149000
