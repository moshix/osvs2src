         BALR  @14,@15                                             0162 00354000
*     RESPECIFY                                                    0163 00355000
*       GPR01P UNRSTD;                                             0163 00356000
@RC00160 DS    0H                                                  0164 00357000
*   END;                            /* LINK TO DAIR YM2225           */ 00358000
*   RETCDC=RETCD;                   /* STORE REG 15 FOR TEST         */ 00359000
         ST    RETCD,RETCDC                                        0165 00360000
*   IF RETCDC>0                     /* TEST RETURN CODE FROM DAIR    */ 00361000
*     THEN                          /* ALLOCATION UNSUCCESSFUL?      */ 00362000
         L     @10,RETCDC                                          0166 00363000
         LTR   @10,@10                                             0166 00364000
         BNP   @RF00166                                            0166 00365000
*     DO;                           /* YES, DETERMINE CAUSE AND ISSUE   00366000
*                                      APPROPRIATE ERROR MESSAGE TO     00367000
*                                      THE TERMINAL                  */ 00368000
*       F18DAPL=ADDR(DAPL);         /* SET PTR TO DAPL         Y02676*/ 00369000
         MVC   F18DAPL(4),DAPLPTR                                  0168 00370000
*       F18RETCD=ADDR(RETCDC);      /* SET PTR TO RETURN CODE  Y02676*/ 00371000
         LA    @10,RETCDC                                          0169 00372000
         ST    @10,F18RETCD                                        0169 00373000
*       F18F02A=ADDR(F18F02);       /* SET PTR TO IKJEFF02 PTR Y02676*/ 00374000
         LA    @10,F18F02                                          0170 00375000
         ST    @10,F18F02A                                         0170 00376000
*       F18CLNOA=ADDR(F18CLNO);     /* SET PTR TO CALL NUMBER  Y02676*/ 00377000
         LA    @10,F18CLNO                                         0171 00378000
         ST    @10,F18CLNOA                                        0171 00379000
*       PARMRGR=ADDR(F18PARMS);     /* LOAD PARAMETER REGISTER Y02676*/ 00380000
         LA    PARMRGR,F18PARMS                                    0172 00381000
*       GEN(LINK EP=IKJEFF18);      /* LINK TO IKJEFF18 TO PROCESS 0173 00382000
*                                      ERROR                   Y02676*/ 00383000
         LINK EP=IKJEFF18                                               00384000
*       GOTO BCND;                  /* CLEAN UP AND EXIT             */ 00385000
