         BNZ   @RF00098                                            0098 00152000
*       GEN(LINK EP=IKJPARS);                                      0099 00153000
         LINK EP=IKJPARS                                                00154000
*     ELSE                                                         0100 00155000
*       CALL CVTPARS;                                              0100 00156000
         B     @RC00098                                            0100 00157000
@RF00098 L     @10,CVTPTR                                          0100 00158000
         L     @15,CVTPARS(,@10)                                   0100 00159000
         BALR  @14,@15                                             0100 00160000
*     RESPECIFY                                                    0101 00161000
*       GPR01P UNRSTD;                                             0101 00162000
@RC00098 DS    0H                                                  0102 00163000
*   END;                            /* PARSING ROUTINE YM2225        */ 00164000
*/****************************************************************** */ 00165000
*/*      UPON RETURN FROM PARSE, THE RETURN CODE WILL INDICATE       */ 00166000
*/*      NORMAL RETURN OR ERROR RETURN.  IF ANY ERROR CONDITION      */ 00167000
*/*      EXISTS, THE CAUSE OF THE ERROR IS DETERMINED, THEN THE      */ 00168000
*/*      MODULE IKJEES75 IS CALLED TO ISSUE THE APPROPRIATE          */ 00169000
*/*      ERROR MESSAGE TO THE TERMINAL.                              */ 00170000
*/*      UPON NORMAL COMPLETION, THE RESULTS RETURNED                */ 00171000
*/*      FROM PARSE ARE USED TO DETERMINE WHICH SET OR SETS OF       */ 00172000
*/*      MESSAGES FROM THE SYS1.BRODCAST DATA SET HAVE BEEN          */ 00173000
*/*      REQUESTED.                                                  */ 00174000
*/****************************************************************** */ 00175000
*                                                                  0103 00176000
*   RETCDC=RETCD;                   /* STORE REG 15 FOR TEST         */ 00177000
         ST    RETCD,RETCDC                                        0103 00178000
*   IF RETCDC=4                     /* TEST FOR NO PROMPT M4092      */ 00179000
*     THEN                          /* YES, M4092                    */ 00180000
         L     @10,RETCDC                                          0104 00181000
         CH    @10,@CH00188                                        0104 00182000
         BE    @RT00104                                            0104 00183000
*     GOTO BCND;                    /* GO TO FLUSH STACK & CLEAR QUE    00184000
*                                      M4092                         */ 00185000
*   IF RETCDC=8                     /* TEST RETURN CODE FROM PARSE   */ 00186000
*     THEN                          /* YES,                          */ 00187000
         LA    @15,8                                               0106 00188000
         CR    @10,@15                                             0106 00189000
         BE    @RT00106                                            0106 00190000
*     GO TO BCND;                   /* ATTENTION INTERRUPT           */ 00191000
*   IF RETCDC>0                     /* WAS PARSE SUCCESSFUL?         */ 00192000
*     THEN                          /* NO                            */ 00193000
         LTR   @10,@10                                             0108 00194000
         BNP   @RF00108                                            0108 00195000
*     DO;                           /* DETERMINE THE REASON          */ 00196000
*       BCERFLG='1'B;               /* INDICATE THIS IS AN ERROR   0110 00197000
*                                      CONDITION                     */ 00198000
         OI    BCERFLG,B'10000000'                                 0110 00199000
*       BCPUTL='1'B;                /* INDICATE PUTLINE TO BE USED   */ 00200000
*       BCDEQ='1'B;                 /* DEQ FROM BROADCAST DATA SET   */ 00201000
         OI    BCPUTL,B'00000101'                                  0112 00202000
*       IF RETCDC=16                /* WAS THERE INSUFFICIENT CORE TO   00203000
*                                      EXECUTE THE PARSE COMMAND ?   */ 00204000
*         THEN                      /* YES                           */ 00205000
         CH    @10,@CH00069                                        0113 00206000
         BNE   @RF00113                                            0113 00207000
*         DO;                                                      0114 00208000
*           BCERCDE=8;              /* INDICATE ERROR MSG INDEX      */ 00209000
         STH   @15,BCERCDE                                         0115 00210000
*           BC1LVL='1'B;            /* INDICATE ONE-LEVEL ERROR MSG  */ 00211000
         OI    BC1LVL,B'01000000'                                  0116 00212000
*         END;                                                     0117 00213000
*       ELSE                        /* PARSE UNSUCCESSFUL DUE TO A 0118 00214000
*                                      SYSTEM ERROR                  */ 00215000
*         DO;                                                      0118 00216000
         B     @RC00113                                            0118 00217000
@RF00113 DS    0H                                                  0119 00218000
*           BCTXTIN='1'B;           /* INDICATE TEXT INSERTION FOR 0119 00219000
*                                      THIS ERROR MESSAGE            */ 00220000
         OI    BCTXTIN,B'00100000'                                 0119 00221000
*           BCERCDE=13;             /* INDICATE ERROR MSG INDEX      */ 00222000
         MVC   BCERCDE(2),@CH00085                                 0120 00223000
*           BCRTCDE=RETCDC;         /* SAVE RETURN CODE FOR TEXT   0121 00224000
*                                      INSERTION INTO ERROR MSG      */ 00225000
         MVC   BCRTCDE(2),RETCDC+2                                 0121 00226000
*         END;                                                     0122 00227000
*       PARMRGR=ADDR(BCIOPARM);                                    0123 00228000
@RC00113 LA    PARMRGR,BCIOPARM                                    0123 00229000
*       CALL IKJEES75;              /* CALL IKJEES75 TO ISSUE      0124 00230000
*                                      APPROPRIATE ERROR MESSAGE     */ 00231000
         L     @15,@CV00933                                        0124 00232000
         BALR  @14,@15                                             0124 00233000
*       GOTO BCEND;                 /* TERMINATE               Y02676*/ 00234000
         B     BCEND                                               0125 00235000
*     END;                          /* END DO                        */ 00236000
*   IF BCMANS=1 THEN                /* WAS MAIL MSGS REQUESTED?      */ 00237000
@RF00108 L     @10,BCANSPL                                         0127 00238000
         CLC   BCMANS(2,@10),@CH00034                              0127 00239000
