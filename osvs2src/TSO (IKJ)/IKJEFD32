*           GENERATE;                                              0416 01245000
*                                      /* DELETE DAIRFAIL RTN @YM02616* 01246000
                     DELETE EP=IKJEFF18                                 01247000
*         END;                      /*                       @YM02616*/ 01248000
*       ELSE                        /* DS NOT CATALOGED      @ZA10491*/ 01249000
*         DISPPDE=3;                /* DEFAULT DISP=CATLG    @ZA10491*/ 01250000
         B     @RC00406                                            0418 01251000
@RF00406 L     @10,PTRPDL(,AWAPTR)                                 0418 01252000
         MVC   DISPPDE(2,@10),@CH00323                             0418 01253000
*     END;                                                         0419 01254000
*   END CHKDISP;                                                   0420 01255000
@EL00007 DS    0H                                                  0420 01256000
@EF00007 DS    0H                                                  0420 01257000
@ER00007 LM    @14,@12,@SA00007                                    0420 01258000
         BR    @14                                                 0420 01259000
*                                                                  0421 01260000
*/********************************************************************/ 01261000
*/*                                                                  */ 01262000
*/* SEGMENT NAME -                                                   */ 01263000
*/*      OLDDS                                                       */ 01264000
*/*                                                                  */ 01265000
*/* FUNCTION -                                                       */ 01266000
*/*      THIS ROUTINE PROCESSES REQUESTS FOR AN OLD DATA SET.        */ 01267000
*/*                                                                  */ 01268000
*/* OPERATION -                                                      */ 01269000
*/*      IF DSNAME IS NOT SPECIFIED, THE USER IS PROMPTED TO ENTER   */ 01270000
*/*      A DSNAME. EXTRANEOUS PARAMETERS ARE MARKED NOT ENTERED IN   */ 01271000
*/*      THE PDL. IF DISPOSITION WAS OMITTED, INDICATE KEEP IN THE   */ 01272000
*/*      PDL. INVOKE GENTRANS TO TRANSLATE THE PARAMETERS. IF        */ 01273000
*/*      GENTRANS SUCCESSFUL, INVOKE DYNAMIC ALLOCATION. IF DYNAMIC  */ 01274000
*/*      ALLOCATION INDICATES FILE IN USE, GO TO PROMPT THE USER     */ 01275000
*/*      FOR THE OPTION. IF DATA SET COULD NOT BE FOUND, PROMPT FOR  */ 01276000
*/*      A NEW DSNAME, AND RE-INVOKE DYNAMIC ALLOCATION.             */ 01277000
*/*                                                                  */ 01278000
*/* INPUT -                                                          */ 01279000
*/*      PTR TO PDL                                                  */ 01280000
*/*                                                                  */ 01281000
*/* OUTPUT -                                                         */ 01282000
*/*      RBCODE12 = 0 - ALLOCATION SUCCESSFUL                        */ 01283000
*/*                 1 - ERROR IN GENTRANS OR ALLOCATION              */ 01284000
*/*                                                                  */ 01285000
*/* NOTE:  THIS ROUTINE REPRESENTS A DIRECT LINE FUNCTION AND CAN    */ 01286000
*/*        BE INCORPORATED AS AN INCLUDED CODE SEGMENT WHEN THE      */ 01287000
*/*        INCLUDE FUNCTION HAS RECEIVED A STANDARD DEFINITION.      */ 01288000
*/********************************************************************/ 01289000
*                                                                  0421 01290000
*OLDDS:                                                            0421 01291000
*   PROC;                                                          0421 01292000
OLDDS    STM   @14,@12,@SA00008                                    0421 01293000
*   IF DSPDE=0 THEN                 /* IF DSN NOT ENTERED,           */ 01294000
         L     @10,PTRPDL(,AWAPTR)                                 0422 01295000
         LH    @10,DSPDE(,@10)                                     0422 01296000
         LTR   @10,@10                                             0422 01297000
         BNZ   @RF00422                                            0422 01298000
*     DO;                           /* THEN                          */ 01299000
*       CALL PRMPDSN;               /* PROMPT FOR NEW DSNAME         */ 01300000
         BAL   @14,PRMPDSN                                         0424 01301000
*       IF RBCODE12='0'B THEN       /* IF PROMPT SUCCESSFUL,         */ 01302000
         TM    RBCODE12(AWAPTR),B'10000000'                        0425 01303000
         BNZ   @RF00425                                            0425 01304000
*         DO;                       /* THEN,                         */ 01305000
*           DSPDE=1;                /* INDICATE DSN ENTERED          */ 01306000
         L     @10,PTRPDL(,AWAPTR)                                 0427 01307000
         MVC   DSPDE(2,@10),@CH00033                               0427 01308000
*           DSOVRLAY='1'B;          /* INDICATE PDE OVERLAID         */ 01309000
*                                                                  0428 01310000
         OI    DSOVRLAY(AWAPTR),B'01000000'                        0428 01311000
*           /*********************************************************/ 01312000
*           /*                                                       */ 01313000
*           /* DETERMINE LENGTH OF PDE TO BE COPIED, AND OVERLAY     */ 01314000
*           /* ORIGINAL PDE WITH PROMPT PDE.                         */ 01315000
*           /*                                                       */ 01316000
*           /*********************************************************/ 01317000
*                                                                  0429 01318000
*           MOVELEN=ADDR(DDNMEPDE)-ADDR(DSNPDE);                   0429 01319000
         LA    MOVELEN,28                                          0429 01320000
*           DSNPDE(1:MOVELEN)=DSNAMPDE(1:MOVELEN);                 0430 01321000
         LR    @07,MOVELEN                                         0430 01322000
         BCTR  @07,0                                               0430 01323000
         L     @05,PMPTPDL(,AWAPTR)                                0430 01324000
         EX    @07,@SM01515                                        0430 01325000
*         END;                                                     0431 01326000
*     END;                                                         0432 01327000
@RF00425 DS    0H                                                  0433 01328000
*   IF RBCODE12='0'B THEN           /* IF NO ERROR FOUND,            */ 01329000
@RF00422 TM    RBCODE12(AWAPTR),B'10000000'                        0433 01330000
         BNZ   @RF00433                                            0433 01331000
*     DO;                           /* CONTINUE                      */ 01332000
*       I=1;                        /* INITIALIZE SUBSCRIPT          */ 01333000
*                                                                  0435 01334000
         LA    I,1                                                 0435 01335000
*       /*************************************************************/ 01336000
*       /*                                                           */ 01337000
*       /* MARK EXTRANEOUS PARAMETERS NOT ENTERED IN PDL             */ 01338000
*       /*                                                           */ 01339000
*       /*************************************************************/ 01340000
*                                                                  0436 01341000
*       DO WHILE(CONT='0'B);                                       0436 01342000
         B     @DE00436                                            0436 01343000
@DL00436 DS    0H                                                  0437 01344000
*         IF OLDEXT(I)='00'X THEN   /* IF END OF LIST,               */ 01345000
         LA    @10,OLDEXT-1(I)                                     0437 01346000
         CLI   0(@10),X'00'                                        0437 01347000
         BNE   @RF00437                                            0437 01348000
*           CONT='1'B;              /* EXIT DO-WHILE                 */ 01349000
         OI    CONT(AWAPTR),B'00010000'                            0438 01350000
*         ELSE                      /* ELSE,                         */ 01351000
*           DO;                     /* GET ADDRESS OF PDE            */ 01352000
         B     @RC00437                                            0439 01353000
@RF00437 DS    0H                                                  0440 01354000
*             EXTRAN=ADDR(ALLOCPDL)+OLDEXT(I);                     0440 01355000
         SLR   EXTRAN,EXTRAN                                       0440 01356000
         IC    EXTRAN,OLDEXT-1(I)                                  0440 01357000
         AL    EXTRAN,PTRPDL(,AWAPTR)                              0440 01358000
*             PDEEXT=0;             /* SET PDE TO 0                  */ 01359000
         SLR   @10,@10                                             0441 01360000
         STH   @10,PDEEXT(,EXTRAN)                                 0441 01361000
*             I=I+1;                /* INCREMENT SUBSCRIPT           */ 01362000
         AL    I,@CF00033                                          0442 01363000
*           END;                                                   0443 01364000
*       END;                        /* END DO-WHILE                  */ 01365000
@RC00437 DS    0H                                                  0444 01366000
@DE00436 TM    CONT(AWAPTR),B'00010000'                            0444 01367000
         BZ    @DL00436                                            0444 01368000
*       CONT='0'B;                  /* RESET SWITCH                  */ 01369000
         NI    CONT(AWAPTR),B'11101111'                            0445 01370000
*       IF DISPPDE=0 THEN           /* IF DISP NOT ENTERED,          */ 01371000
         L     @10,PTRPDL(,AWAPTR)                                 0446 01372000
         LH    @07,DISPPDE(,@10)                                   0446 01373000
         LTR   @07,@07                                             0446 01374000
         BNZ   @RF00446                                            0446 01375000
*         DISPPDE=1;                /* INDICATE DEFAULT OF KEEP      */ 01376000
         MVC   DISPPDE(2,@10),@CH00033                             0447 01377000
*       CALL TRANSRTN;              /* TRANSLATE PARAMETERS          */ 01378000
@RF00446 BAL   @14,TRANSRTN                                        0448 01379000
*       IF RBCODE12='0'B THEN       /* IF TRANSLATE SUCCESSFUL       */ 01380000
         TM    RBCODE12(AWAPTR),B'10000000'                        0449 01381000
         BNZ   @RF00449                                            0449 01382000
*         DO;                       /* THEN                          */ 01383000
*           IF DSOVRLAY='1'B THEN   /* IF 2ND PDL TO FREE,           */ 01384000
         TM    DSOVRLAY(AWAPTR),B'01000000'                        0451 01385000
         BNO   @RF00451                                            0451 01386000
*             DO;                   /* THEN                          */ 01387000
*               DSOVRLAY='0'B;      /* TURN OFF INDICATOR            */ 01388000
         NI    DSOVRLAY(AWAPTR),B'10111111'                        0453 01389000
*               RFY                                                0454 01390000
*                 R5 RSTD;          /* RESTRICT REG 5                */ 01391000
*               R5=ADDR(PMPTPDL);   /* PDL ADDRESS IN REG 5          */ 01392000
         LA    R5,PMPTPDL(,AWAPTR)                                 0455 01393000
*               GEN(IKJRLSA (5));   /* RELEASE PDL                   */ 01394000
         IKJRLSA (5)                                                    01395000
*               RFY                                                0457 01396000
*                 R5 UNRSTD;        /* UNRESTRICT REG 5              */ 01397000
*             END;                                                 0458 01398000
*           CALL DYNSVC;            /* INVOKE DYNAMIC ALLOCATION     */ 01399000
@RF00451 BAL   @14,DYNSVC                                          0459 01400000
*           IF RCODESV>0 THEN       /* IF RETURN CODE NON-ZERO       */ 01401000
         L     @10,RCODESV(,AWAPTR)                                0460 01402000
         LTR   @10,@10                                             0460 01403000
         BNP   @RF00460                                            0460 01404000
*             DO;                   /* CHECK IF FILE IN USE          */ 01405000
*               IF S99ERROR='0410'X THEN                           0462 01406000
         L     @10,PTRS99RB(,AWAPTR)                               0462 01407000
         CLC   S99ERROR(2,@10),@CB01444                            0462 01408000
         BNE   @RF00462                                            0462 01409000
*                 DO;               /* YES -                         */ 01410000
*                   R1=AWAPTR;      /* WORKAREA PTR IN REG 1         */ 01411000
         LR    R1,AWAPTR                                           0464 01412000
*                   CALL IKJEFD36;  /* PROMPT FOR OPTION             */ 01413000
         L     @15,@CV00342                                        0465 01414000
         BALR  @14,@15                                             0465 01415000
*                 END;                                             0466 01416000
*               ELSE                /* NO - CHECK IF DSN NOT FOUND   */ 01417000
*                 IF S99ERROR='1708'X THEN                         0467 01418000
         B     @RC00462                                            0467 01419000
@RF00462 L     @10,PTRS99RB(,AWAPTR)                               0467 01420000
         CLC   S99ERROR(2,@10),@CB01467                            0467 01421000
         BNE   @RF00467                                            0467 01422000
*                   DO;             /* YES -                         */ 01423000
*                     ENTRYCD=1;    /* SET ENTRY CODE                */ 01424000
         MVC   ENTRYCD(2,AWAPTR),@CH00033                          0469 01425000
*                     R1=AWAPTR;    /* WORKAREA PTR IN REG 1 ISSUE 0470 01426000
*                                      NOT FOUND MSG                 */ 01427000
         LR    R1,AWAPTR                                           0470 01428000
*                     CALL IKJEFD35;                               0471 01429000
         L     @15,@CV00340                                        0471 01430000
         BALR  @14,@15                                             0471 01431000
*                     CALL PRMPDSN; /* PROMPT FOR DSNAME             */ 01432000
         BAL   @14,PRMPDSN                                         0472 01433000
*                     IF RBCODE12='0'B THEN                        0473 01434000
         TM    RBCODE12(AWAPTR),B'10000000'                        0473 01435000
         BNZ   @RF00473                                            0473 01436000
*                       CALL DSNADDT;/* PROCESS NEW DSNAME           */ 01437000
         BAL   @14,DSNADDT                                         0474 01438000
*                   END;                                           0475 01439000
*                 ELSE                                             0476 01440000
*                   DO;             /* OTHERWISE,                    */ 01441000
         B     @RC00467                                            0476 01442000
@RF00467 DS    0H                                                  0477 01443000
*                     RBCODE12='1'B;/* INDICATE ALLOC FAILED         */ 01444000
         OI    RBCODE12(AWAPTR),B'10000000'                        0477 01445000
*                     ENTRYCD=1;    /* SET ENTRY CODE                */ 01446000
         MVC   ENTRYCD(2,AWAPTR),@CH00033                          0478 01447000
*                     R1=AWAPTR;    /* WORKAREA PTR IN REG 1 ISSUE 0479 01448000
*                                      ERROR MESSAGE                 */ 01449000
         LR    R1,AWAPTR                                           0479 01450000
*                     CALL IKJEFD35;                               0480 01451000
         L     @15,@CV00340                                        0480 01452000
         BALR  @14,@15                                             0480 01453000
*                   END;                                           0481 01454000
*             END;                                                 0482 01455000
*         END;                                                     0483 01456000
*     END;                                                         0484 01457000
*   END OLDDS;                                                     0485 01458000
@EL00008 DS    0H                                                  0485 01459000
@EF00008 DS    0H                                                  0485 01460000
@ER00008 LM    @14,@12,@SA00008                                    0485 01461000
         BR    @14                                                 0485 01462000
*                                                                  0486 01463000
*/********************************************************************/ 01464000
*/*                                                                  */ 01465000
*/* SEGMENT NAME -                                                   */ 01466000
*/*      DSNADDT                                                     */ 01467000
*/*                                                                  */ 01468000
*/* FUNCTION -                                                       */ 01469000
*/*      THIS ROUTINE BUILDS THE TEXT UNIT FOR THE DSNAME ENTERED    */ 01470000
*/*      ON THE PROMPT AND RE-INVOKES DYNAMIC ALLOCATION.            */ 01471000
*/*                                                                  */ 01472000
*/* OPERATION -                                                      */ 01473000
*/*      INVOKE GENTRANS, PASSING THE SECONDARY PCL AND PDL. IF      */ 01474000
*/*      GENTRANS FAILED, ISSUE ERROR MESSAGE. ELSE, FREE THE        */ 01475000
*/*      SECONDARY PDL. COPY THE POINTERS TO THE NEW TEXT UNITS      */ 01476000
*/*      INTO THE ORIGINAL ADDRESS LIST. INVOKE DYNAMIC ALLOCATION.  */ 01477000
*/*      IF ALLOCATION INDICATES FILE IN USE, GO TO PROMPT FOR       */ 01478000
*/*      OPTION. IF DSNAME NOT FOUND, PROMPT FOR NEW DSNAME AND      */ 01479000
*/*      REPEAT ABOVE PROCEDURE. ELSE, ISSUE ERROR MSG. FREE THE     */ 01480000
*/*      STORAGE USED TO BUILD THE SECONDARY TEXT UNITS.             */ 01481000
*/*                                                                  */ 01482000
*/* INPUT -                                                          */ 01483000
*/*      PTR TO PROMPT PCL                                           */ 01484000
*/*      PTR TO PROMPT PDL                                           */ 01485000
*/*                                                                  */ 01486000
*/* OUTPUT -                                                         */ 01487000
*/*      RBCODE12 = 0 - ALLOCATION SUCCESSFUL                        */ 01488000
*/*                 1 - ERROR IN GENTRANS OR ALLOCATION              */ 01489000
*/*                                                                  */ 01490000
*/* NOTE:  THIS ROUTINE REPRESENTS A DIRECT LINE FUNCTION AND CAN    */ 01491000
*/*        BE INCORPORATED AS AN INCLUDED CODE SEGMENT WHEN THE      */ 01492000
*/*        INCLUDE FUNCTION HAS RECEIVED A STANDARD DEFINITION.      */ 01493000
*/*                                                                  */ 01494000
*/*        IF THE DSNAME COULD NOT BE FOUND, AND THE USER IS         */ 01495000
*/*        PROMPTED TO REENTER DSNAME, THEN THE TEXT UNITS FOR THE   */ 01496000
*/*        OTHER PARAMETERS HAVE ALREADY BEEN BUILT. GENTRANS IS     */ 01497000
*/*        INVOKED TO TRANSLATE ONLY THE DSNAME ENTERED ON THE       */ 01498000
*/*        PROMPT. THE TEXT UNITS BUILT ARE PUT INTO THE ORIGINAL    */ 01499000
*/*        LIST OF TEXT UNITS.                                       */ 01500000
*/********************************************************************/ 01501000
*                                                                  0486 01502000
*DSNADDT:                                                          0486 01503000
*   PROC;                                                          0486 01504000
DSNADDT  STM   @14,@12,@SA00009                                    0486 01505000
*DSNAD1:                                                           0487 01506000
*   S99ERROR=S99ERROR&&S99ERROR;    /* ZERO ERROR FIELD              */ 01507000
DSNAD1   L     @10,PTRS99RB(,AWAPTR)                               0487 01508000
         XC    S99ERROR(2,@10),S99ERROR(@10)                       0487 01509000
*   GTPLPDL=PMPTPDL;                /* SET PDL PTR                   */ 01510000
         L     @10,GTPLPTR(,AWAPTR)                                0488 01511000
         L     @07,PMPTPDL(,AWAPTR)                                0488 01512000
         ST    @07,GTPLPDL(,@10)                                   0488 01513000
*   GTPLPCL=PPLPCL;                 /* SET PCL PTR                   */ 01514000
         L     @07,PPLPTR(,AWAPTR)                                 0489 01515000
         L     @07,PPLPCL(,@07)                                    0489 01516000
         ST    @07,GTPLPCL(,@10)                                   0489 01517000
*   GTPLKLST=ADDR(DSNKLIST);        /* PTR TO KEYLIST                */ 01518000
         LA    @07,DSNKLIST                                        0490 01519000
         ST    @07,GTPLKLST(,@10)                                  0490 01520000
*   GTPLTBLE=0;                     /* NO KEYWORD TABLE              */ 01521000
         SLR   @07,@07                                             0491 01522000
         ST    @07,GTPLTBLE(,@10)                                  0491 01523000
*   GTPLABLK=0;                     /* NO ADDITIONAL BLOCK           */ 01524000
         ST    @07,GTPLABLK(,@10)                                  0492 01525000
*   GTPLOUTA=0;                     /* ZERO PTR TO TEXT              */ 01526000
         ST    @07,GTPLOUTA(,@10)                                  0493 01527000
*   R1=GTPLPTR;                     /* GTPL ADDR IN REG 1            */ 01528000
         LR    R1,@10                                              0494 01529000
*   GEN(LINK EP=IKJCB831);          /* INVOKE GENTRANS               */ 01530000
         LINK EP=IKJCB831                                               01531000
*   RCODESV=R15;                    /* SAVE RETURN CODE              */ 01532000
         ST    R15,RCODESV(,AWAPTR)                                0496 01533000
*   IF RCODESV>0 THEN               /* IF GENTRANS FAILED            */ 01534000
         L     @10,RCODESV(,AWAPTR)                                0497 01535000
         LTR   @10,@10                                             0497 01536000
         BNP   @RF00497                                            0497 01537000
*     DO;                           /* THEN                          */ 01538000
*       RBCODE12='1'B;              /* INDICATE ALLOC FAILED         */ 01539000
         OI    RBCODE12(AWAPTR),B'10000000'                        0499 01540000
*       ENTRYCD=3;                  /* SET ENTRY CODE                */ 01541000
         MVC   ENTRYCD(2,AWAPTR),@CH00323                          0500 01542000
*       R1=AWAPTR;                  /* WORKAREA PTR IN REG 1         */ 01543000
         LR    R1,AWAPTR                                           0501 01544000
*       CALL IKJEFD35;              /* ISSUE SYSTEM ERROR MSG        */ 01545000
         L     @15,@CV00340                                        0502 01546000
         BALR  @14,@15                                             0502 01547000
*     END;                                                         0503 01548000
*   ELSE                                                           0504 01549000
*     DO;                           /* IF GENTRANS SUCCESSFUL,       */ 01550000
         B     @RC00497                                            0504 01551000
@RF00497 DS    0H                                                  0505 01552000
*       RFY                                                        0505 01553000
*         R5 RSTD;                  /* RESTRICT REG 5                */ 01554000
*       R5=ADDR(PMPTPDL);           /* PDL PTR IN REG 5              */ 01555000
         LA    R5,PMPTPDL(,AWAPTR)                                 0506 01556000
*       GEN(IKJRLSA (5));           /* RELEASE 2ND PDL               */ 01557000
         IKJRLSA (5)                                                    01558000
*       RFY                                                        0508 01559000
*         R5 UNRSTD;                /* UNRESTRICT REG 5              */ 01560000
*       TEXTPTR2=GTPLOUTA;          /* SAVE PTR TO TEXT              */ 01561000
         L     @10,GTPLPTR(,AWAPTR)                                0509 01562000
         L     TEXTPTR2,GTPLOUTA(,@10)                             0509 01563000
*       TUDSN=ADDRLST2(3);          /* NEW DSN TEXT POINTER          */ 01564000
         L     @10,ADDRLST2+8(,TEXTPTR2)                           0510 01565000
         ST    @10,TUDSN(,GTPLOUT1)                                0510 01566000
*       TUMBR=ADDRLST2(4);          /* NEW MEMBER TEXT POINTER       */ 01567000
         L     @10,ADDRLST2+12(,TEXTPTR2)                          0511 01568000
         ST    @10,TUMBR(,GTPLOUT1)                                0511 01569000
*       TUPSWD=ADDRLST2(5);         /* NEW PSWORD TEXT POINTER       */ 01570000
         L     @10,ADDRLST2+16(,TEXTPTR2)                          0512 01571000
         ST    @10,TUPSWD(,GTPLOUT1)                               0512 01572000
*       TUPSWDND='0'B;              /* ZERO HIGH ORDER BIT           */ 01573000
         NI    TUPSWDND(GTPLOUT1),B'01111111'                      0513 01574000
*       CALL DYNSVC;                /* INVOKE DYN ALLOCATION         */ 01575000
         BAL   @14,DYNSVC                                          0514 01576000
*       IF RCODESV>0 THEN           /* IF ALLOCATION FAILED,         */ 01577000
         L     @10,RCODESV(,AWAPTR)                                0515 01578000
         LTR   @10,@10                                             0515 01579000
         BNP   @RF00515                                            0515 01580000
*         DO;                       /* THEN CHECK IF FILE IN USE,    */ 01581000
*           IF S99ERROR='0410'X THEN                               0517 01582000
         L     @10,PTRS99RB(,AWAPTR)                               0517 01583000
         CLC   S99ERROR(2,@10),@CB01444                            0517 01584000
         BNE   @RF00517                                            0517 01585000
*             DO;                   /* YES -                         */ 01586000
*               R1=AWAPTR;          /* WORKAREA PTR IN REG 1 PROMPT     01587000
*                                      FOR OPTION                    */ 01588000
         LR    R1,AWAPTR                                           0519 01589000
*               CALL IKJEFD36;                                     0520 01590000
         L     @15,@CV00342                                        0520 01591000
         BALR  @14,@15                                             0520 01592000
*             END;                                                 0521 01593000
