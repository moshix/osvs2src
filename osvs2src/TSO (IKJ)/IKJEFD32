         LR    R1,AWAPTR                                           0221 00575000
*       CALL IKJEFD33;              /* PROCESS SPACE                 */ 00576000
         L     @15,@CV00339                                        0222 00577000
         BALR  @14,@15                                             0222 00578000
*       INC=ADDNUM+1;               /* SET ADDBLOCK SUBSCRIPT        */ 00579000
         L     @10,GTPLPTR(,AWAPTR)                                0223 00580000
