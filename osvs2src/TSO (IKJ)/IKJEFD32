         TITLE 'ALLOCATION REQUEST PROCESSOR                           *00001000
                        '                                               00002000
IKJEFD32 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKJEFD32  76.167'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @11,8(,@13)                                         0001 00020000
         LR    @13,@11                                             0001 00021000
         XC    @ZTEMPS(@ZLEN),@ZTEMPS                                   00022000
         MVC   @PC00001(4),0(@01)                                  0001 00023000
*/*                    END OF CVT MACRO                              */ 00024000
*                                                                  0093 00025000
*                                                                  0093 00026000
*/********************************************************************/ 00027000
*/*                                                                  */ 00028000
*/* SEGMENT NAME -                                                   */ 00029000
*/*      REQTYPE                                                     */ 00030000
*/*                                                                  */ 00031000
*/* FUNCTION -                                                       */ 00032000
*/*      THIS ROUTINE DETERMINES THE TYPE OF ALLOCATION REQUEST      */ 00033000
*/*      BEING MADE, AND ROUTES CONTROL TO THE APPROPRIATE           */ 00034000
*/*      PROCESSOR.                                                  */ 00035000
*/*                                                                  */ 00036000
*/* OPERATION -                                                      */ 00037000
*/*      IF THE REQUEST IS FOR A DUMMY DATA SET, INVOKE DMMYREQ.     */ 00038000
*/*      IF REQUEST IF FOR A TERMINAL, INVOKE TERMREQ.  IF REQUEST   */ 00039000
*/*      IF FOR SYSOUT DATA SET, INVOKE SYSOTREQ. OTHERWISE,         */ 00040000
*/*      DETERMINE DATA SET STATUS AND INVOKE APPROPRIATE ROUTINE.   */ 00041000
*/*                                                                  */ 00042000
*/* INPUT -                                                          */ 00043000
*/*      PTR TO PDL                                                  */ 00044000
*/*                                                                  */ 00045000
*/* OUTPUT -                                                         */ 00046000
*/*      REQUEST PROCESSED                                           */ 00047000
*/*                                                                  */ 00048000
*/* NOTE:  THIS ROUTINE REPRESENTS A DIRECT LINE FUNCTION AND CAN    */ 00049000
*/*        BE INCORPORATED AS AN INCLUDED CODE SEGMENT WHEN THE      */ 00050000
*/*        INCLUDE FUNCTION HAS RECEIVED A STANDARD DEFINITION.      */ 00051000
*/********************************************************************/ 00052000
*                                                                  0093 00053000
*REQTYPE:                                                          0093 00054000
*   AWAPTR=R1;                      /* PTR TO WORKAREA               */ 00055000
REQTYPE  LR    AWAPTR,R1                                           0093 00056000
*   INC=ADDNUM+1;                   /* ADDBLOCK SUBSCRIPT            */ 00057000
         L     @10,GTPLPTR(,AWAPTR)                                0094 00058000
         L     @10,GTPLABLK(,@10)                                  0094 00059000
         LA    INC,1                                               0094 00060000
         MVC   @TF00001(2),ADDNUM(@10)                             0094 00061000
         AH    INC,@TF00001                                        0094 00062000
*   IF DSPDE=2 THEN                 /* IF DUMMY SPECIFIED,           */ 00063000
         L     @10,PTRPDL(,AWAPTR)                                 0095 00064000
         CLC   DSPDE(2,@10),@CH00066                               0095 00065000
         BNE   @RF00095                                            0095 00066000
*     CALL DMMYREQ;                 /* INVOKE DUMMY PROCESSOR        */ 00067000
         BAL   @14,DMMYREQ                                         0096 00068000
*   ELSE                                                           0097 00069000
*     IF DSPDE=1&                   /* IF * ENTERED AS               */ 00070000
*         DSNPRES='1'B&             /* DSNAME,                       */ 00071000
*         DSNTERM='*' THEN          /* INVOKE TERMINAL               */ 00072000
         B     @RC00095                                            0097 00073000
@RF00095 L     @10,PTRPDL(,AWAPTR)                                 0097 00074000
         CLC   DSPDE(2,@10),@CH00033                               0097 00075000
         BNE   @RF00097                                            0097 00076000
         TM    DSNPRES(@10),B'10000000'                            0097 00077000
         BNO   @RF00097                                            0097 00078000
         L     @10,DSNPTR(,@10)                                    0097 00079000
         CLI   DSNTERM(@10),C'*'                                   0097 00080000
         BNE   @RF00097                                            0097 00081000
*       CALL TERMREQ;               /* PROCESSOR                     */ 00082000
         BAL   @14,TERMREQ                                         0098 00083000
*     ELSE                                                         0099 00084000
*       IF STATSPDE=5 THEN          /* IF SYSOUT ENTERED,            */ 00085000
         B     @RC00097                                            0099 00086000
@RF00097 L     @10,PTRPDL(,AWAPTR)                                 0099 00087000
         CLC   STATSPDE(2,@10),@CH00047                            0099 00088000
         BNE   @RF00099                                            0099 00089000
*         CALL SYSOTREQ;            /* INVOKE SYSOUT PROC.           */ 00090000
         BAL   @14,SYSOTREQ                                        0100 00091000
*       ELSE                                                       0101 00092000
*         DO;                       /*                       @Y30LPKH*/ 00093000
         B     @RC00099                                            0101 00094000
@RF00099 DS    0H                                                  0102 00095000
*           IF VOLPDE=2 THEN        /* IF MSVGP SPEC         @Y30LPKH*/ 00096000
         L     @10,PTRPDL(,AWAPTR)                                 0102 00097000
         CLC   VOLPDE(2,@10),@CH00066                              0102 00098000
         BNE   @RF00102                                            0102 00099000
*             KEYLIST(8)=DALMSVGP;  /* SET KEY               @Y30LPKH*/ 00100000
         L     @10,GTPLPTR(,AWAPTR)                                0103 00101000
         L     @10,GTPLKLST(,@10)                                  0103 00102000
         MVC   KEYLIST+14(2,@10),@CH00746                          0103 00103000
*           IF STATSPDE=<2 THEN     /* IF STATUS OLD OR SHR          */ 00104000
@RF00102 L     @10,PTRPDL(,AWAPTR)                                 0104 00105000
         LH    @10,STATSPDE(,@10)                                  0104 00106000
         C     @10,@CF00066                                        0104 00107000
         BH    @RF00104                                            0104 00108000
*             CALL OLDDS;           /* INVOKE PROCESSOR              */ 00109000
         BAL   @14,OLDDS                                           0105 00110000
*           ELSE                                                   0106 00111000
*             IF STATSPDE=3 THEN    /* STATUS MOD,                   */ 00112000
         B     @RC00104                                            0106 00113000
@RF00104 L     @10,PTRPDL(,AWAPTR)                                 0106 00114000
         CLC   STATSPDE(2,@10),@CH00323                            0106 00115000
         BNE   @RF00106                                            0106 00116000
*               CALL MODDS;         /* INVOKE MOD PROCESSOR          */ 00117000
         BAL   @14,MODDS                                           0107 00118000
*             ELSE                  /* ELSE,                         */ 00119000
*               CALL NEWDS;         /* INVOKE NEW PROCESSOR          */ 00120000
         B     @RC00106                                            0108 00121000
@RF00106 BAL   @14,NEWDS                                           0108 00122000
*         END;                                                     0109 00123000
@RC00106 DS    0H                                                  0109 00124000
@RC00104 DS    0H                                                  0110 00125000
*   IF RBCODE12='1'B THEN           /* IF ERROR OCCURRED,            */ 00126000
@RC00099 DS    0H                                                  0110 00127000
@RC00097 DS    0H                                                  0110 00128000
@RC00095 TM    RBCODE12(AWAPTR),B'10000000'                        0110 00129000
         BNO   @RF00110                                            0110 00130000
*     RETCODE=12;                   /* SET RETURN CODE TO 12         */ 00131000
         L     @10,REGSAVE(,R13)                                   0111 00132000
         MVC   RETCODE(4,@10),@CF01435                             0111 00133000
*   ELSE                            /* ELSE                          */ 00134000
*     RETCODE=0;                    /* RETURN CODE 0                 */ 00135000
         B     @RC00110                                            0112 00136000
@RF00110 L     @10,REGSAVE(,R13)                                   0112 00137000
         SLR   @15,@15                                             0112 00138000
         ST    @15,RETCODE(,@10)                                   0112 00139000
*   RETURN;                                                        0113 00140000
@EL00001 L     @13,4(,@13)                                         0113 00141000
@EF00001 L     @00,@SIZDATD                                        0113 00142000
         LR    @01,@11                                             0113 00143000
         FREEMAIN R,LV=(0),A=(1)                                        00144000
@ER00001 LM    @14,@12,12(@13)                                     0113 00145000
         BR    @14                                                 0113 00146000
*                                                                  0114 00147000
*/********************************************************************/ 00148000
*/*                                                                  */ 00149000
*/* SEGMENT NAME -                                                   */ 00150000
*/*      NEWDS                                                       */ 00151000
*/*                                                                  */ 00152000
*/* FUNCTION -                                                       */ 00153000
*/*      THIS ROUTINE ALLOCATES A DATA SET HAVING A STATUS OF NEW.   */ 00154000
*/*                                                                  */ 00155000
*/* OPERATION -                                                      */ 00156000
*/*      EXTRANEOUS PARAMETERS ARE IGNORED. IF DISPOSITION WAS NOT   */ 00157000
*/*      SPECIFIED, AND DSNAME WAS NOT SPECIFIED, SET DISPOSITION TO */ 00158000
*/*      DELETE. ELSE, SET DISPOSITION TO CATALOG. INVOKE IKJEFD33   */ 00159000
*/*      TO PROCESS SPACE PARAMETERS. SET TRACKS OR CYLINDERS KEY IN */ 00160000
*/*      KEY LIST, WHICHEVER IS APPROPRIATE. INVOKE GENTRANS. IF     */ 00161000
*/*      GENTRANS SUCCESSFUL, INVOKE DYNAMIC ALLOCATION.             */ 00162000
*/*                                                                  */ 00163000
*/* INPUT -                                                          */ 00164000
*/*      PTR TO PDL                                                  */ 00165000
*/*      GENTRANS PARAMETER LIST                                     */ 00166000
*/*      DYNAMIC ALLOCATION REQUEST BLOCK                            */ 00167000
*/* OUTPUT -                                                         */ 00168000
*/*      RBCODE12 = 0 - ALLOCATION SUCCESSFUL                        */ 00169000
*/*                 1 - FAILURE IN GENTRANS OR ALLOCATION            */ 00170000
*/*                                                                  */ 00171000
*/* NOTE:  THIS ROUTINE REPRESENTS A DIRECT LINE FUNCTION AND CAN    */ 00172000
*/*        BE INCORPORATED AS AN INCLUDED CODE SEGMENT WHEN THE      */ 00173000
*/*        INCLUDE FUNCTION HAS RECEIVED A STANDARD DEFINITION.      */ 00174000
*/*                                                                  */ 00175000
*/*        IF TRACKS WAS SPECIFIED, THE KEY PASSED TO GENTRANS IS    */ 00176000
*/*        THE TRACKS KEY. OTHERWISE, THE KEY IS FOR CYLINDERS. IF   */ 00177000
*/*        BLOCK OR AVBLOCK WAS ENTERED, THE CYLINDERS KEY WILL NOT  */ 00178000
*/*        BE LOOKED AT.                                             */ 00179000
*/********************************************************************/ 00180000
*                                                                  0114 00181000
*NEWDS:                                                            0114 00182000
*   PROC;                                                          0114 00183000
         B     @EL00001                                            0114 00184000
NEWDS    STM   @14,@12,@SA00002                                    0114 00185000
*   I=1;                            /* INITIALIZE SUBSCRIPT          */ 00186000
*                                                                  0115 00187000
         LA    I,1                                                 0115 00188000
*   /*****************************************************************/ 00189000
*   /*                                                               */ 00190000
*   /* MARK EXTRANEOUS PARAMETERS NOT ENTERED IN PDL                 */ 00191000
*   /*                                                               */ 00192000
*   /*****************************************************************/ 00193000
*                                                                  0116 00194000
*   DO WHILE(CONT='0'B);                                           0116 00195000
         B     @DE00116                                            0116 00196000
@DL00116 DS    0H                                                  0117 00197000
*     IF NEWEXT(I)='00'X THEN       /* IF END OF LIST,               */ 00198000
         LA    @10,NEWEXT-1(I)                                     0117 00199000
         CLI   0(@10),X'00'                                        0117 00200000
         BNE   @RF00117                                            0117 00201000
*       CONT='1'B;                  /* EXIT DO-WHILE                 */ 00202000
         OI    CONT(AWAPTR),B'00010000'                            0118 00203000
*     ELSE                          /* ELSE,                         */ 00204000
*       DO;                         /* GET ADDRESS OF PDE            */ 00205000
         B     @RC00117                                            0119 00206000
@RF00117 DS    0H                                                  0120 00207000
*         EXTRAN=ADDR(ALLOCPDL)+NEWEXT(I);                         0120 00208000
         SLR   EXTRAN,EXTRAN                                       0120 00209000
         IC    EXTRAN,NEWEXT-1(I)                                  0120 00210000
         AL    EXTRAN,PTRPDL(,AWAPTR)                              0120 00211000
*         PDEEXT=0;                 /* SET PDE TO 0                  */ 00212000
         SLR   @10,@10                                             0121 00213000
         STH   @10,PDEEXT(,EXTRAN)                                 0121 00214000
*         I=I+1;                    /* INCREMENT SUBSCRIPT           */ 00215000
         AL    I,@CF00033                                          0122 00216000
*       END;                                                       0123 00217000
*   END;                            /* END DO-WHILE                  */ 00218000
@RC00117 DS    0H                                                  0124 00219000
@DE00116 TM    CONT(AWAPTR),B'00010000'                            0124 00220000
         BZ    @DL00116                                            0124 00221000
*   CONT='0'B;                      /* RE-SET SWITCH                 */ 00222000
         NI    CONT(AWAPTR),B'11101111'                            0125 00223000
*   IF DISPPDE=0 THEN               /* IF DISP NOT SPECIFIED         */ 00224000
         L     @10,PTRPDL(,AWAPTR)                                 0126 00225000
         LH    @07,DISPPDE(,@10)                                   0126 00226000
         LTR   @07,@07                                             0126 00227000
         BNZ   @RF00126                                            0126 00228000
*     DO;                           /* AND                           */ 00229000
*       IF DSPDE=1 THEN             /* IF DSNAME ENTERED,            */ 00230000
         CLC   DSPDE(2,@10),@CH00033                               0128 00231000
         BNE   @RF00128                                            0128 00232000
*         DISPPDE=3;                /* DEFAULT CATALOG               */ 00233000
         MVC   DISPPDE(2,@10),@CH00323                             0129 00234000
*       ELSE                        /* IF DSNAME NOT ENTERED         */ 00235000
*         DISPPDE=2;                /* DEFAULT DELETE                */ 00236000
         B     @RC00128                                            0130 00237000
@RF00128 L     @10,PTRPDL(,AWAPTR)                                 0130 00238000
         MVC   DISPPDE(2,@10),@CH00066                             0130 00239000
*     END;                                                         0131 00240000
@RC00128 DS    0H                                                  0132 00241000
*   R1=AWAPTR;                      /* WORKAREA PTR IN REG 1         */ 00242000
@RF00126 LR    R1,AWAPTR                                           0132 00243000
*   CALL IKJEFD33;                  /* PROCESS SPACE PARAMETERS      */ 00244000
         L     @15,@CV00339                                        0133 00245000
         BALR  @14,@15                                             0133 00246000
*   IF RBCODE12='0'B THEN           /* IF ERROR OCCURRED,            */ 00247000
         TM    RBCODE12(AWAPTR),B'10000000'                        0134 00248000
         BNZ   @RF00134                                            0134 00249000
*     DO;                                                          0135 00250000
*       INC=ADDNUM+1;               /* SET ADDBLOCK SUBSCRIPT        */ 00251000
         L     @10,GTPLPTR(,AWAPTR)                                0136 00252000
         L     @07,GTPLABLK(,@10)                                  0136 00253000
         LA    INC,1                                               0136 00254000
         MVC   @TF00001(2),ADDNUM(@07)                             0136 00255000
         AH    INC,@TF00001                                        0136 00256000
*       IF BLOKPDE=3 THEN           /* IF TRK ENTERED, GET           */ 00257000
         L     @07,PTRPDL(,AWAPTR)                                 0137 00258000
         CLC   BLOKPDE(2,@07),@CH00323                             0137 00259000
         BNE   @RF00137                                            0137 00260000
*         KEYLIST(9)=DALTRK;        /* KEY IN KEYLIST                */ 00261000
         L     @10,GTPLKLST(,@10)                                  0138 00262000
         MVC   KEYLIST+16(2,@10),@CH00572                          0138 00263000
*       ELSE                        /* ELSE                          */ 00264000
*         KEYLIST(9)=DALCYL;        /* CYL KEY IN KEYLIST            */ 00265000
         B     @RC00137                                            0139 00266000
@RF00137 L     @10,GTPLPTR(,AWAPTR)                                0139 00267000
         L     @10,GTPLKLST(,@10)                                  0139 00268000
         MVC   KEYLIST+16(2,@10),@CH00574                          0139 00269000
*       CALL TRANSRTN;              /* TRANSLATE PARAMETERS          */ 00270000
@RC00137 BAL   @14,TRANSRTN                                        0140 00271000
*       IF RBCODE12='0'B THEN       /* IF SUCCESSFUL,                */ 00272000
         TM    RBCODE12(AWAPTR),B'10000000'                        0141 00273000
         BNZ   @RF00141                                            0141 00274000
*         DO;                                                      0142 00275000
*           CALL DYNSVC;            /* INVOKE ALLOCATION             */ 00276000
         BAL   @14,DYNSVC                                          0143 00277000
*           IF RCODESV>0 THEN       /* IF ALLOC FAILED,              */ 00278000
         L     @10,RCODESV(,AWAPTR)                                0144 00279000
         LTR   @10,@10                                             0144 00280000
         BNP   @RF00144                                            0144 00281000
*             DO;                   /* AND                           */ 00282000
*               IF S99ERROR='0410'X THEN/* IF FILE IN USE,           */ 00283000
         L     @10,PTRS99RB(,AWAPTR)                               0146 00284000
         CLC   S99ERROR(2,@10),@CB01444                            0146 00285000
         BNE   @RF00146                                            0146 00286000
*                 DO;                                              0147 00287000
*                   R1=AWAPTR;      /* WORKAREA PTR IN REG 1         */ 00288000
         LR    R1,AWAPTR                                           0148 00289000
*                   CALL IKJEFD36;  /* PROMPT FOR OPTION             */ 00290000
         L     @15,@CV00342                                        0149 00291000
         BALR  @14,@15                                             0149 00292000
*                 END;                                             0150 00293000
*               ELSE                /* OTHER ERROR,                  */ 00294000
*                 DO;                                              0151 00295000
         B     @RC00146                                            0151 00296000
@RF00146 DS    0H                                                  0152 00297000
*                   RBCODE12='1'B;  /* INDICATE ALLOC FAILED         */ 00298000
         OI    RBCODE12(AWAPTR),B'10000000'                        0152 00299000
*                   ENTRYCD=1;      /* CODE FOR DYNALLOC ERR         */ 00300000
         MVC   ENTRYCD(2,AWAPTR),@CH00033                          0153 00301000
*                   R1=AWAPTR;      /* WORKAREA PTR IN REG 1         */ 00302000
         LR    R1,AWAPTR                                           0154 00303000
*                   CALL IKJEFD35;  /* ISSUE ERROR MESSAGE           */ 00304000
         L     @15,@CV00340                                        0155 00305000
         BALR  @14,@15                                             0155 00306000
*                 END;                                             0156 00307000
*             END;                                                 0157 00308000
*         END;                                                     0158 00309000
*     END;                                                         0159 00310000
*   END NEWDS;                                                     0160 00311000
@EL00002 DS    0H                                                  0160 00312000
@EF00002 DS    0H                                                  0160 00313000
@ER00002 LM    @14,@12,@SA00002                                    0160 00314000
         BR    @14                                                 0160 00315000
*                                                                  0161 00316000
*/********************************************************************/ 00317000
*/*                                                                  */ 00318000
*/* SEGMENT NAME -                                                   */ 00319000
*/*      TERMREQ                                                     */ 00320000
*/*                                                                  */ 00321000
*/* FUNCTION -                                                       */ 00322000
*/*      THIS ROUTINE HANDLES REQUESTS FOR TERMINAL DATA SET         */ 00323000
*/*      ALLOCATION.                                                 */ 00324000
*/*                                                                  */ 00325000
*/* OPERATION -                                                      */ 00326000
*/*      EXTRANEOUS PARAMETERS ARE MARKED NOT ENTERED IN THE PDL.    */ 00327000
*/*      IF BLOCK WAS ENTERED INVOKE IKJEFD33 TO SET UP DCB BLOCK    */ 00328000
*/*      SIZE TEXT UNIT.  SET UP THE TEXT UNIT WHICH                 */ 00329000
*/*      INDICATES TERMINAL ALLOCATION, AND MARK DATASET NOT         */ 00330000
*/*      ENTERED IN THE PDL. INVOKE GENTRANS. IF GENTRANS SUCCESSFUL */ 00331000
*/*      INVOKE DYNAMIC ALLOCATION.                                  */ 00332000
*/*                                                                  */ 00333000
*/* INPUT -                                                          */ 00334000
*/*      PTR TO PDL                                                  */ 00335000
*/*                                                                  */ 00336000
*/* OUTPUT -                                                         */ 00337000
*/*      RBCODE12 = 0 - ALLOCATION SUCCESSFUL                        */ 00338000
*/*                 1 - FAILURE IN GENTRANS OR ALLOCATION            */ 00339000
*/*                                                                  */ 00340000
*/* NOTE:  THIS ROUTINE REPRESENTS A DIRECT LINE FUNCTION AND CAN    */ 00341000
*/*        BE INCORPORATED AS AN INCLUDED CODE SEGMENT WHEN THE      */ 00342000
*/*        INCLUDE FUNCTION HAS RECEIVED A STANDARD DEFINITION.      */ 00343000
*/*                                                                  */ 00344000
*/*        THE TEXT UNIT SPECIFYING TERMINAL ALLOCATION MUST BE SET  */ 00345000
*/*        UP SINCE GENTRANS DOES NOT RECOGNIZE A DATA SET NAME OF   */ 00346000
*/*        ASTERISK (*).                                             */ 00347000
*/********************************************************************/ 00348000
*                                                                  0161 00349000
*TERMREQ:                                                          0161 00350000
*   PROC;                                                          0161 00351000
TERMREQ  STM   @14,@12,@SA00003                                    0161 00352000
*   I=1;                            /* INITIALIZE SUBSCRIPT          */ 00353000
*                                                                  0162 00354000
         LA    I,1                                                 0162 00355000
*   /*****************************************************************/ 00356000
*   /*                                                               */ 00357000
*   /* MARK EXTRANEOUS PARAMETERS NOT ENTERED IN PDL                 */ 00358000
*   /*                                                               */ 00359000
*   /*****************************************************************/ 00360000
*                                                                  0163 00361000
*   DO WHILE(CONT='0'B);                                           0163 00362000
         B     @DE00163                                            0163 00363000
@DL00163 DS    0H                                                  0164 00364000
*     IF TERMEXT(I)='00'X THEN      /* IF END OF LIST,               */ 00365000
         LA    @10,TERMEXT-1(I)                                    0164 00366000
         CLI   0(@10),X'00'                                        0164 00367000
         BNE   @RF00164                                            0164 00368000
*       CONT='1'B;                  /* EXIT DO-WHILE                 */ 00369000
         OI    CONT(AWAPTR),B'00010000'                            0165 00370000
*     ELSE                          /* ELSE,                         */ 00371000
*       DO;                         /* GET ADDRESS OF PDE            */ 00372000
         B     @RC00164                                            0166 00373000
@RF00164 DS    0H                                                  0167 00374000
*         EXTRAN=ADDR(ALLOCPDL)+TERMEXT(I);                        0167 00375000
         SLR   EXTRAN,EXTRAN                                       0167 00376000
         IC    EXTRAN,TERMEXT-1(I)                                 0167 00377000
         AL    EXTRAN,PTRPDL(,AWAPTR)                              0167 00378000
*         PDEEXT=0;                 /* SET PDE TO 0                  */ 00379000
         SLR   @10,@10                                             0168 00380000
         STH   @10,PDEEXT(,EXTRAN)                                 0168 00381000
*         I=I+1;                    /* INCREMENT SUBSCRIPT           */ 00382000
         AL    I,@CF00033                                          0169 00383000
*       END;                                                       0170 00384000
*   END;                                                           0171 00385000
@RC00164 DS    0H                                                  0171 00386000
@DE00163 TM    CONT(AWAPTR),B'00010000'                            0171 00387000
         BZ    @DL00163                                            0171 00388000
*   CONT='0'B;                                                     0172 00389000
         NI    CONT(AWAPTR),B'11101111'                            0172 00390000
*   IF BLOKPDE=1 THEN               /* IF BLOCK ENTERED,             */ 00391000
         L     @10,PTRPDL(,AWAPTR)                                 0173 00392000
         CLC   BLOKPDE(2,@10),@CH00033                             0173 00393000
         BNE   @RF00173                                            0173 00394000
*     DO;                                                          0174 00395000
*       R1=AWAPTR;                  /* WORKAREA PTR IN REG 1         */ 00396000
         LR    R1,AWAPTR                                           0175 00397000
*       CALL IKJEFD33;              /* PROCESS PARM                  */ 00398000
         L     @15,@CV00339                                        0176 00399000
         BALR  @14,@15                                             0176 00400000
*       INC=ADDNUM+1;               /* SET ADDBLOCK SUBSCRIPT        */ 00401000
         L     @10,GTPLPTR(,AWAPTR)                                0177 00402000
         L     @10,GTPLABLK(,@10)                                  0177 00403000
         LA    INC,1                                               0177 00404000
         MVC   @TF00001(2),ADDNUM(@10)                             0177 00405000
         AH    INC,@TF00001                                        0177 00406000
*     END;                                                         0178 00407000
*   BLOKPDE=0;                      /* MARK BLOCK NOT ENTERED        */ 00408000
@RF00173 SLR   @10,@10                                             0179 00409000
         L     @07,PTRPDL(,AWAPTR)                                 0179 00410000
         STH   @10,BLOKPDE(,@07)                                   0179 00411000
*   ADDNUM=ADDNUM+1;                /* INCREMENT ENTRY CTR           */ 00412000
         L     @05,GTPLPTR(,AWAPTR)                                0180 00413000
         L     @05,GTPLABLK(,@05)                                  0180 00414000
         LA    @02,1                                               0180 00415000
         MVC   @TF00001(2),ADDNUM(@05)                             0180 00416000
         LH    @15,@TF00001                                        0180 00417000
         ALR   @15,@02                                             0180 00418000
