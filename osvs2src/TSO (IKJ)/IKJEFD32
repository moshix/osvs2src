*       IF(RCODESV^=8)|             /* IF NOT CATLG ERROR    @OZ05962*/ 01211000
*           (RCODESV=8&DA04CTRC^=8) THEN/*                   @OZ05962*/ 01212000
         LA    @10,8                                               0406 01213000
         L     @05,RCODESV(,AWAPTR)                                0406 01214000
         CR    @05,@10                                             0406 01215000
         BNE   @RT00406                                            0406 01216000
         CR    @05,@10                                             0406 01217000
         BNE   @RF00406                                            0406 01218000
         CH    @10,DA04CTRC(,DAPB0PTR)                             0406 01219000
         BE    @RF00406                                            0406 01220000
@RT00406 DS    0H                                                  0407 01221000
*         DO;                       /* INVOKE DAIRFAIL       @OZ05962*/ 01222000
*           RBCODE12='1'B;          /* INDICATE ERROR        @YM02616   01223000
*                                      GET ADDR OF DAPB      @YM02616*/ 01224000
         OI    RBCODE12(AWAPTR),B'10000000'                        0408 01225000
*           DFDAPLP=ADDR(DAPB04);   /* GET ADDR OF RET CODE@YM02616  */ 01226000
         ST    DAPB0PTR,DFDAPLP                                    0409 01227000
*           DFRCP=ADDR(RCODESV);    /* GET ADDR OF MSG RTN   @YM02616*/ 01228000
         LA    @10,RCODESV(,AWAPTR)                                0410 01229000
         ST    @10,DFRCP                                           0410 01230000
*           DFJEFF02=ADDR(ADMSG);   /* GET ADDR OF ID        @YM02616*/ 01231000
         LA    @10,ADMSG                                           0411 01232000
         ST    @10,DFJEFF02                                        0411 01233000
*           DFIDP=ADDR(CALLERNO);                                  0412 01234000
         LA    @10,CALLERNO                                        0412 01235000
         ST    @10,DFIDP                                           0412 01236000
*           GENERATE;                                              0413 01237000
*                                      /* LOAD DAIRFAIL RTN   @YM02616* 01238000
                     LOAD EP=IKJEFF18                                   01239000
*           R1=ADDR(DFPARMS);                                      0414 01240000
         LA    R1,DFPARMS                                          0414 01241000
*           CALL R0;                /* INVOKE DAIRFAIL       @YM02616*/ 01242000
         LR    @15,R0                                              0415 01243000
