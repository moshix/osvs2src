*/********************************************************************/ 00027000
*/*                                                                  */ 00028000
*/* SEGMENT NAME -                                                   */ 00029000
*/*      PRMPTFLE                                                    */ 00030000
*/*                                                                  */ 00031000
*/* FUNCTION -                                                       */ 00032000
*/*      THIS ROUTINE NOTIFIES THE USER THAT THE REQUESTED FILENAME  */ 00033000
*/*      IS IN USE, AND PROMPTS THE USER TO ENTER EITHER FREE OR     */ 00034000
*/*      CARRIAGE RETURN. IF FREE IS ENTERED, THE FILE IS FREED AND  */ 00035000
*/*      RE-ALLOCATED. ELSE, ALLOCATE TERMINATES.                    */ 00036000
*/*                                                                  */ 00037000
*/* OPERATION -                                                      */ 00038000
*/*      DAIRFAIL IS INVOKED TO ISSUE THE FILE IN USE MESSAGE. THE   */ 00039000
*/*      PROMPT MESSAGE IS SET UP AND ROUTINE PRMOPT IS INVOKED TO   */ 00040000
*/*      ISSUE THE PUTGET AND ANALYZE THE RESPONSE.                  */ 00041000
*/*                                                                  */ 00042000
*/* INPUT -                                                          */ 00043000
*/*      DDNAME PDE                                                  */ 00044000
*/*                                                                  */ 00045000
*/* OUTPUT -                                                         */ 00046000
*/*      FILE FREED AND RE-ALLOCATED, OR                             */ 00047000
*/*      ALLOCATE TERMINATION INDICATED                              */ 00048000
*/*                                                                  */ 00049000
*/* NOTE:  THIS ROUTINE REPRESENTS A DIRECT LINE FUNCTION AND CAN    */ 00050000
*/*        BE INCORPORATED AS AN INCLUDED CODE SEGMENT WHEN THE      */ 00051000
*/*        INCLUDE FUNCTION HAS RECEIVED A STANDARD DEFINITION.      */ 00052000
*/********************************************************************/ 00053000
*    PRMPTFLE:                                                     0069 00054000
*        AWAPTR = R1;                  /* PTR TO WORKAREA            */ 00055000
PRMPTFLE LR    AWAPTR,R1                                           0069 00056000
*        MSGTABLE = MSGTABLE && MSGTABLE;                          0070 00057000
         L     @10,MSGPTR(,AWAPTR)                                 0070 00058000
         XC    MSGTABLE(64,@10),MSGTABLE(@10)                      0070 00059000
*        ENTRYCD = 1;                  /* SET ENTRY CODE             */ 00060000
         MVC   ENTRYCD(2,AWAPTR),@CH00033                          0071 00061000
*        R1 = AWAPTR;                  /* WORKAREA PTR IN REG 1      */ 00062000
         LR    R1,AWAPTR                                           0072 00063000
*        CALL IKJEFD35;                /* ISSUE FILE IN USE MSG      */ 00064000
*                                                                  0073 00065000
         L     @15,@CV00329                                        0073 00066000
         BALR  @14,@15                                             0073 00067000
*        IF RBCODE12 = '0'B THEN       /* IF NO ERROR,               */ 00068000
         TM    RBCODE12(AWAPTR),B'10000000'                        0074 00069000
         BNZ   @RF00074                                            0074 00070000
*          DO;                         /* THEN                       */ 00071000
*            MSGID = '113A';           /* SET MESSAGE ID             */ 00072000
         L     @10,MSGPTR(,AWAPTR)                                 0076 00073000
         MVC   MSGID(4,@10),@CC01352                               0076 00074000
*            MTPUTLSW = '0'B;          /* INDICATE PUTGET            */ 00075000
         NI    MTPUTLSW(@10),B'10111111'                           0077 00076000
*            VAR1 = DDNPTR;            /* PTR TO DDNAME VARIABLE     */ 00077000
         L     @02,PTRPDL(,AWAPTR)                                 0078 00078000
         MVC   VAR1(3,@10),DDNPTR+1(@02)                           0078 00079000
*            L1 = DDNLEN;              /* DDNAME VARIABLE LENGTH     */ 00080000
         MVC   L1(1,@10),DDNLEN+1(@02)                             0079 00081000
*            CALL PRMOPT;              /* ISSUE MSG AND PROCESS    0080 00082000
*                                         RESPONSE                   */ 00083000
         BAL   @14,PRMOPT                                          0080 00084000
*          END;                                                    0081 00085000
*        IF RBCODE12 = '1'B THEN                                   0082 00086000
@RF00074 TM    RBCODE12(AWAPTR),B'10000000'                        0082 00087000
         BNO   @RF00082                                            0082 00088000
*          RETCODE = 12;                                           0083 00089000
         L     @10,REGSAVE(,R13)                                   0083 00090000
         MVC   RETCODE(4,@10),@CF01355                             0083 00091000
*        ELSE                                                      0084 00092000
*          RETCODE = 0;                                            0084 00093000
*                                                                  0084 00094000
         B     @RC00082                                            0084 00095000
@RF00082 L     @10,REGSAVE(,R13)                                   0084 00096000
         SLR   @15,@15                                             0084 00097000
         ST    @15,RETCODE(,@10)                                   0084 00098000
*        RETURN;                       /* RETURN TO CALLER           */ 00099000
@EL00001 L     @13,4(,@13)                                         0085 00100000
@EF00001 L     @00,@SIZDATD                                        0085 00101000
         LR    @01,@11                                             0085 00102000
         FREEMAIN R,LV=(0),A=(1)                                        00103000
@ER00001 LM    @14,@12,12(@13)                                     0085 00104000
         BR    @14                                                 0085 00105000
*/********************************************************************/ 00106000
*/*                                                                  */ 00107000
*/* SEGMENT NAME -                                                   */ 00108000
*/*      PRMOPT                                                      */ 00109000
*/*                                                                  */ 00110000
*/* FUNCTION -                                                       */ 00111000
*/*      THIS ROUTINE PROMPTS THE USER FOR A TERMINAL RESPONSE, AND  */ 00112000
*/*      INVOKES COMMAND SCAN TO CHECK THE RESPONSE.                 */ 00113000
*/*                                                                  */ 00114000
*/* OPERATION -                                                      */ 00115000
*/*      ISSUE PROMPT MESSAGE. SET UP THE                            */ 00116000
*/*      PARAMETER LIST FOR COMMAND SCAN, PASSING THE BUFFER         */ 00117000
*/*      RETURNED FROM PUTGET. INVOKE COMMAND SCAN. SAVE THE RETURN  */ 00118000
*/*      CODE AND ANALYZE COMMAND SCAN OUTPUT.                       */ 00119000
*/*                                                                  */ 00120000
*/* INPUT -                                                          */ 00121000
*/*      PROMPT MESSAGE TO BE ISSUED                                 */ 00122000
*/*                                                                  */ 00123000
*/* OUTPUT -                                                         */ 00124000
*/*      PROMPT MESSAGE ISSUED                                       */ 00125000
*/*      RESPONSE SCANNED FOR VALIDITY                               */ 00126000
*/*                                                                  */ 00127000
*/* NOTE:  THIS ROUTINE REPRESENTS A DIRECT LINE FUNCTION AND CAN    */ 00128000
*/*        BE INCORPORATED AS AN INCLUDED CODE SEGMENT WHEN THE      */ 00129000
*/*        INCLUDE FUNCTION HAS RECEIVED A STANDARD DEFINITION.      */ 00130000
*/********************************************************************/ 00131000
