*            L1 = LENGTH(BLKVAR);           /* AND VARIABLE LENGTH   */ 01670000
         MVI   L1(@10),X'0A'                                       0462 01671000
*          END;                                                    0463 01672000
*        ELSE                               /* ELSE,                 */ 01673000
*          DO;                              /* ASSUME AVBLOCK        */ 01674000
         B     @RC00457                                            0464 01675000
@RF00457 DS    0H                                                  0465 01676000
*            Y = 8-PABLKLEN+ADDR(PKAREA);   /* PUT VALUE IN PACK AREA*/ 01677000
