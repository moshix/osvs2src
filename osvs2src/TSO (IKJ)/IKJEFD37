*/*                                                                  */ 01174000
*/* NOTE:  THIS ROUTINE REPRESENTS A DIRECT LINE FUNCTION AND CAN    */ 01175000
*/*        BE INCORPORATED AS AN INCLUDED CODE SEGMENT WHEN THE      */ 01176000
*/*        INCLUDE FUNCTION HAS RECEIVED A STANDARD DEFINITION.      */ 01177000
*/********************************************************************/ 01178000
*                                      /* ENTRY FOR VOLUME           */ 01179000
*    SYSVLIST: PROC OPTIONS(NOSAVEAREA);                           0324 01180000
SYSVLIST STM   @14,@12,12(@13)                                     0324 01181000
*        R15 = R1->VALWORK;            /* GET STORED REGISTERS       */ 01182000
         L     R15,VALWORK(,R1)                                    0325 01183000
*        GEN (LM 2,12,0(15));          /* RESTORE ALLOC REGISTERS    */ 01184000
         LM 2,12,0(15)                                                  01185000
*        VCKPTR = R1;                  /* PARM LIST FROM REG 1       */ 01186000
         LR    VCKPTR,R1                                           0327 01187000
*        PARSRTC = 0;                  /* INITIALIZE RETURN CODE     */ 01188000
         SLR   @10,@10                                             0328 01189000
         ST    @10,PARSRTC(,R13)                                   0328 01190000
*        RCODESV = 0;                  /* INIT RET CODE IN WORKAREA  */ 01191000
         ST    @10,RCODESV(,AWAPTR)                                0329 01192000
*        VLISTCTR = VLISTCTR + 1;      /* INCREMENT COUNTER          */ 01193000
         LA    @10,1                                               0330 01194000
         AH    @10,VLISTCTR(,AWAPTR)                               0330 01195000
         STH   @10,VLISTCTR(,AWAPTR)                               0330 01196000
*   END SYSVLIST;                                                  0331 01197000
@EL00012 DS    0H                                                  0331 01198000
@EF00012 DS    0H                                                  0331 01199000
@ER00012 LM    @14,@12,12(@13)                                     0331 01200000
         BR    @14                                                 0331 01201000
*/********************************************************************/ 01202000
*/*                                                                  */ 01203000
*/* SEGMENT NAME -                                                   */ 01204000
*/*      SYSTATS                                                     */ 01205000
*/*                                                                  */ 01206000
*/* FUNCTION -                                                       */ 01207000
*/*      THIS VALIDITY CHECK ROUTINE CHECKS THE STATUS ENTERED ON    */ 01208000
*/*      THE PROMPT FOR A VALID KEYWORD.                             */ 01209000
*/*                                                                  */ 01210000
*/* OPERATION -                                                      */ 01211000
*/*      IF MORE THAN ONE SUBSCRIPT IS ENTERED, PARSE RETURN CODE    */ 01212000
*/*      IS SET TO 4. ELSE, SET UP A NEW COMMAND BUFFER WITH THE     */ 01213000
*/*      PARAMETERS ENTERED ON THE PROMPT. INVOKE PARSE, PASSING     */ 01214000
*/*      A LIST OF KEYWORDS IN THE PCL. IF PARSE FINDS A MATCH,      */ 01215000
*/*      MOVE THE PARAMETER INTO THE ORIGINAL PDE.                   */ 01216000
*/*                                                                  */ 01217000
*/* INPUT -                                                          */ 01218000
*/*      PTR TO PDE                                                  */ 01219000
*/*      THIRD LEVEL PCL                                             */ 01220000
*/*                                                                  */ 01221000
*/* OUTPUT -                                                         */ 01222000
*/*      RETURN CODE = 0 - VALID PARAMETER                           */ 01223000
*/*                    4 - INVALID PARAMETER, PARSE PROMPTS          */ 01224000
*/********************************************************************/ 01225000
*                                      /* ENTRY FOR STATUS           */ 01226000
*    SYSTATS: PROC OPTIONS(NOSAVEAREA);                            0332 01227000
SYSTATS  STM   @14,@12,12(@13)                                     0332 01228000
*        R15 = R1->VALWORK;            /* GET STORED REGISTERS       */ 01229000
         L     R15,VALWORK(,R1)                                    0333 01230000
*        GENERATE;                     /* SET UP ADDRESSABILITY      */ 01231000
         LM  2,12,0(15)                   RESTORE ALLOC REGISTERS       01232000
         LA  14,SAVEVC                    VC SAVER IN REG 14            01233000
         ST  14,8(13)                     VC SAVER IN PARSE SAVER       01234000
         ST  13,4(14)                     PARSE SAVER IN VC SAVER       01235000
         LR  13,14                        VC SAVER IN REG 14            01236000
*        VCKPTR = R1;                  /* PARM LIST FROM REG 1       */ 01237000
         LR    VCKPTR,R1                                           0335 01238000
*        PRCODE = 0;                   /* INIT PARSE RETURN CODE     */ 01239000
         SLR   @10,@10                                             0336 01240000
         ST    @10,PRCODE(,AWAPTR)                                 0336 01241000
*        IF SUBNMBR > 1 THEN           /* IF MORE THAN 1 SUBSCRIPT,  */ 01242000
         L     @10,VALPDE(,VCKPTR)                                 0337 01243000
         CLI   SUBNMBR(@10),1                                      0337 01244000
         BNH   @RF00337                                            0337 01245000
*          PRCODE = 4;                 /* INDICATE PARSE PROMPT      */ 01246000
         MVC   PRCODE(4,AWAPTR),@CF00337                           0338 01247000
*        ELSE                                                      0339 01248000
*          DO;                         /* ELSE,                      */ 01249000
         B     @RC00337                                            0339 01250000
@RF00337 DS    0H                                                  0340 01251000
*            SAVPPL = PPLPTR;                                      0340 01252000
         L     SAVPPL,PPLPTR(,AWAPTR)                              0340 01253000
*            CALL PARSLINK;            /* SET UP FOR CALL TO PARSE   */ 01254000
         BAL   @14,PARSLINK                                        0341 01255000
*            R1 = PPLPTR;              /* PPL IN REG 1               */ 01256000
         L     R1,PPLPTR(,AWAPTR)                                  0342 01257000
*              DO; /*CALLTSSR EP(IKJPARS)*/                        0343 01258000
*                RESPECIFY GPR01P RSTD;                            0344 01259000
*                IF CVTPARS = 0                                    0345 01260000
*                  THEN                                            0345 01261000
         L     @10,CVTPTR                                          0345 01262000
         L     @10,CVTPARS(,@10)                                   0345 01263000
         LTR   @10,@10                                             0345 01264000
         BNZ   @RF00345                                            0345 01265000
*                    GEN(LINK EP=IKJPARS);                         0346 01266000
         LINK EP=IKJPARS                                                01267000
*                  ELSE                                            0347 01268000
*                    CALL CVTPARS;                                 0347 01269000
         B     @RC00345                                            0347 01270000
@RF00345 L     @10,CVTPTR                                          0347 01271000
         L     @15,CVTPARS(,@10)                                   0347 01272000
         BALR  @14,@15                                             0347 01273000
*                RESPECIFY GPR01P UNRSTD;                          0348 01274000
@RC00345 DS    0H                                                  0349 01275000
*              END;   /* INVOKE PARSE       @Y30NQKK*/             0349 01276000
*            RCODESV = R15;            /* SAVE RETURN CODE           */ 01277000
         ST    R15,RCODESV(,AWAPTR)                                0350 01278000
*            IF RCODESV > 0 THEN       /* IF PARSE FAILED,           */ 01279000
         L     @10,RCODESV(,AWAPTR)                                0351 01280000
         LTR   @10,@10                                             0351 01281000
         BNP   @RF00351                                            0351 01282000
*                PRCODE = 12;          /* INDICATE PARSE TERMINATE   */ 01283000
         MVC   PRCODE(4,AWAPTR),@CF01223                           0352 01284000
*            ELSE                      /* OTHERWISE                  */ 01285000
*              DO;                                                 0353 01286000
         B     @RC00351                                            0353 01287000
@RF00351 DS    0H                                                  0354 01288000
*                IF STAT2PDE = 0 THEN  /* IF NO STATUS ENTERED,      */ 01289000
         L     @10,PDLADDR3(,AWAPTR)                               0354 01290000
         LH    @10,STAT2PDE(,@10)                                  0354 01291000
         LTR   @10,@10                                             0354 01292000
         BNZ   @RF00354                                            0354 01293000
*                  PRCODE = 4;         /* INDICATE PARSE PROMPT      */ 01294000
         MVC   PRCODE(4,AWAPTR),@CF00337                           0355 01295000
*                ELSE                  /* IF STATUS WAS ENTERED,     */ 01296000
*                   DO;                /* SAVE STATUS IN PDE         */ 01297000
         B     @RC00354                                            0356 01298000
@RF00354 DS    0H                                                  0357 01299000
*                     STATSPDE = STAT2PDE;                         0357 01300000
         L     @10,PTRPDL(,AWAPTR)                                 0357 01301000
         L     @07,PDLADDR3(,AWAPTR)                               0357 01302000
         LH    @03,STAT2PDE(,@07)                                  0357 01303000
         STH   @03,STATSPDE(,@10)                                  0357 01304000
*                     IF STATSPDE=5 &  /* IF SYSOUT AND              */ 01305000
*                                      /* CLASS SPECIFIED THEN       */ 01306000
*                       PCLASPRS = '1'B THEN                       0358 01307000
         C     @03,@CF00097                                        0358 01308000
         BNE   @RF00358                                            0358 01309000
         TM    PCLASPRS(@07),B'10000000'                           0358 01310000
         BNO   @RF00358                                            0358 01311000
*                       DO;            /* OVERLAY PDE -              */ 01312000
*                         CLASPTR = ADDR(NCLASBUF);                0360 01313000
         LA    @03,NCLASBUF(,AWAPTR)                               0360 01314000
         ST    @03,CLASPTR(,@10)                                   0360 01315000
*                                      /* CLASS VALUE IN PDE         */ 01316000
*                         NCLASBUF(1) = CLASBUF1(1);               0361 01317000
         L     @03,PCLASPTR(,@07)                                  0361 01318000
         MVC   NCLASBUF(1,AWAPTR),CLASBUF1(@03)                    0361 01319000
*                                      /* LENGTH IN PDE              */ 01320000
*                         CLASLEN = PCLASLEN;                      0362 01321000
         LH    @03,PCLASLEN(,@07)                                  0362 01322000
         STH   @03,CLASLEN(,@10)                                   0362 01323000
*                                      /* FLAGS IN PDE               */ 01324000
*                         CLASFLG = PCLASFLG;                      0363 01325000
         MVC   CLASFLG(1,@10),PCLASFLG(@07)                        0363 01326000
*                       END;                                       0364 01327000
*                  END;                                            0365 01328000
@RF00358 DS    0H                                                  0366 01329000
*                RFY R5 RSTD;          /* RESTRICT REG 5             */ 01330000
@RC00354 DS    0H                                                  0367 01331000
*                R5 = ADDR(PDLADDR3);  /* PDLADDR IN REG 5           */ 01332000
         LA    R5,PDLADDR3(,AWAPTR)                                0367 01333000
*                GEN (IKJRLSA (5));    /* RELEASE PDL                */ 01334000
         IKJRLSA (5)                                                    01335000
*                RFY R5 UNRSTD;        /* UNRESTRICT REG 5           */ 01336000
*              END;                                                0370 01337000
*            PPLPTR = SAVPPL;          /* RESTORE PPL PTR            */ 01338000
@RC00351 ST    SAVPPL,PPLPTR(,AWAPTR)                              0371 01339000
*          END;                                                    0372 01340000
*        GEN (L 13,4(13));             /* RESTORE PARSE REGISTERS    */ 01341000
@RC00337 DS    0H                                                  0373 01342000
         L 13,4(13)                                                     01343000
*        PARSRTC = PRCODE;             /* RETURN CODE IN REG 15      */ 01344000
         L     @10,PRCODE(,AWAPTR)                                 0374 01345000
         ST    @10,PARSRTC(,R13)                                   0374 01346000
*        RETURN;                                                   0375 01347000
@EL00013 DS    0H                                                  0375 01348000
@EF00013 DS    0H                                                  0375 01349000
@ER00013 LM    @14,@12,12(@13)                                     0375 01350000
         BR    @14                                                 0375 01351000
*   END SYSTATS;                                                   0376 01352000
*/********************************************************************/ 01353000
*/*                                                                  */ 01354000
*/* SEGMENT NAME -                                                   */ 01355000
*/*      PARSLINK                                                    */ 01356000
*/*                                                                  */ 01357000
*/* FUNCTION -                                                       */ 01358000
*/*      THIS ROUTINE SETS UP THE COMMAND BUFFER AND PARSE PARAMETER */ 01359000
*/*      LIST.                                                       */ 01360000
*/*                                                                  */ 01361000
*/* OPERATION -                                                      */ 01362000
*/*      THE PARAMETER ENTERED ON THE PROMPT IS PLACED IN A          */ 01363000
*/*      BUFFER. THE PARSE PARAMETER LIST IS SET UP.                 */ 01364000
*/*                                                                  */ 01365000
*/* INPUT -                                                          */ 01366000
*/*      PTR TO PDE                                                  */ 01367000
*/*                                                                  */ 01368000
*/* OUTPUT -                                                         */ 01369000
*/*      PPL                                                         */ 01370000
*/*                                                                  */ 01371000
*/********************************************************************/ 01372000
*        RFY I RSTD;                                               0377 01373000
*    PARSLINK: PROC OPTIONS(NOSAVEAREA);                           0378 01374000
PARSLINK STM   @14,@12,12(@13)                                     0378 01375000
*        CMDTWOBF(1) = ' ';                 /* BLANK IN 1ST POSITION */ 01376000
         MVI   CMDTWOBF(AWAPTR),C' '                               0379 01377000
*        I = STATLEN;                       /* SET BUFFER PTR        */ 01378000
         L     @10,VALPDE(,VCKPTR)                                 0380 01379000
         SLR   I,I                                                 0380 01380000
         IC    I,STATLEN(,@10)                                     0380 01381000
*        CMDTWOBF(2:I+1) = STATBUF(1:I);    /* MOVE IN STATUS        */ 01382000
         LR    @04,I                                               0381 01383000
         BCTR  @04,0                                               0381 01384000
         L     @01,STATPTR(,@10)                                   0381 01385000
         EX    @04,@SM01311                                        0381 01386000
*        I = I + 1;                         /* INCREMENT BUFFER PTR  */ 01387000
         LA    @04,1                                               0382 01388000
         ALR   I,@04                                               0382 01389000
*        IF SUBPRES = '1'B THEN             /* IF SUBSCRIPT ENTERED, */ 01390000
         TM    SUBPRES(@10),B'10000000'                            0383 01391000
         BNO   @RF00383                                            0383 01392000
*          DO;                              /* THEN                  */ 01393000
*            I = I + 1;                     /* INCREMENT BUFFER PTR  */ 01394000
         ALR   I,@04                                               0385 01395000
*            CMDTWOBF(I) = '(';             /* MOVE IN LEFT PAREN    */ 01396000
         LR    @01,AWAPTR                                          0386 01397000
         ALR   @01,I                                               0386 01398000
         MVI   CMDTWOBF-1(@01),C'('                                0386 01399000
*            I = I+1;                       /* INCREMENT BUFFER PTR  */ 01400000
         ALR   I,@04                                               0387 01401000
*            IF CNSTSUB = '1'B THEN         /* IF SUBSCRIPT A CNST,  */ 01402000
         TM    CNSTSUB(@10),B'01000000'                            0388 01403000
         BNO   @RF00388                                            0388 01404000
*              DO;                          /* MOVE IN CONSTANT      */ 01405000
*                CMDTWOBF(I:I+CNSTLEN-1) = CNSTBUF(1:CNSTLEN);     0390 01406000
         LR    @04,AWAPTR                                          0390 01407000
         ALR   @04,I                                               0390 01408000
         SLR   @15,@15                                             0390 01409000
         IC    @15,CNSTLEN(,@10)                                   0390 01410000
         LR    @14,@15                                             0390 01411000
         BCTR  @14,0                                               0390 01412000
         L     @10,CNSTPTR(,@10)                                   0390 01413000
         EX    @14,@SM01313                                        0390 01414000
*                I = I + CNSTLEN;           /* INCREMENT BUFFER PTR  */ 01415000
         ALR   I,@15                                               0391 01416000
*              END;                                                0392 01417000
*            ELSE                           /* ELSE,                 */ 01418000
*              DO;                          /* MOVE IN VARIABLE      */ 01419000
         B     @RC00388                                            0393 01420000
@RF00388 DS    0H                                                  0394 01421000
*                CMDTWOBF(I:I+VARLEN-1) = VARBUF(1:VARLEN);        0394 01422000
         LR    @10,AWAPTR                                          0394 01423000
         ALR   @10,I                                               0394 01424000
         L     @04,VALPDE(,VCKPTR)                                 0394 01425000
         SLR   @15,@15                                             0394 01426000
         IC    @15,VARLEN(,@04)                                    0394 01427000
         LR    @14,@15                                             0394 01428000
         BCTR  @14,0                                               0394 01429000
         L     @04,VARPTR(,@04)                                    0394 01430000
         EX    @14,@SM01315                                        0394 01431000
*                I = I + VARLEN;            /* INCREMENT BUFFER PTR  */ 01432000
         ALR   I,@15                                               0395 01433000
*              END;                                                0396 01434000
*            CMDTWOBF(I) = ')';             /* MOVE IN RIGHT PAREN   */ 01435000
@RC00388 LR    @10,AWAPTR                                          0397 01436000
         ALR   @10,I                                               0397 01437000
         MVI   CMDTWOBF-1(@10),C')'                                0397 01438000
*          END;                                                    0398 01439000
*        CMDTWOLN = I + 4;                  /* SET BUFFER LENGTH     */ 01440000
@RF00383 LA    @10,4                                               0399 01441000
         ALR   @10,I                                               0399 01442000
         STH   @10,CMDTWOLN(,AWAPTR)                               0399 01443000
*        CMDTWOOF = 0;                      /* BUFFER OFFSET ZERO    */ 01444000
         SLR   @10,@10                                             0400 01445000
         STH   @10,CMDTWOOF(,AWAPTR)                               0400 01446000
*        PPLPTR = ADDR(PPLTWO);             /* PTR TO PPL            */ 01447000
         LA    @07,PPLTWO(,AWAPTR)                                 0401 01448000
         ST    @07,PPLPTR(,AWAPTR)                                 0401 01449000
*        PPLUPT = CPPLUPT;                  /* UPT                   */ 01450000
         L     @15,CPPLPTR(,AWAPTR)                                0402 01451000
         L     @14,CPPLUPT(,@15)                                   0402 01452000
         ST    @14,PPLUPT(,@07)                                    0402 01453000
*        PPLECT = CPPLECT;                  /* ECT                   */ 01454000
         L     @15,CPPLECT(,@15)                                   0403 01455000
         ST    @15,PPLECT(,@07)                                    0403 01456000
*        PPLECB = ADDR(COMMECB);            /* ECB                   */ 01457000
         LA    @15,COMMECB(,AWAPTR)                                0404 01458000
         ST    @15,PPLECB(,@07)                                    0404 01459000
*        COMMECB = 0;                       /* ECB SET TO 0          */ 01460000
         ST    @10,COMMECB(,AWAPTR)                                0405 01461000
*        PPLPCL = ADCNPCLX;                 /* PCL ADDRESS           */ 01462000
         L     @15,ADCNPCLX                                        0406 01463000
