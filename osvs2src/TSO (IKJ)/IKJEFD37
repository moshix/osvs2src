         LA    Y,8                                                 0465 01680000
         SLR   Y,@07                                               0465 01681000
         LA    @04,PKAREA                                          0465 01682000
         ALR   Y,@04                                               0465 01683000
*            PKAREA1(1:PABLKLEN)=PABLKBUF(1:PABLKLEN);             0466 01684000
         BCTR  @07,0                                               0466 01685000
         L     @10,PABLKPTR(,@10)                                  0466 01686000
         EX    @07,@SM01319                                        0466 01687000
*            VAR1 = ADDR(AVBLOCK);          /* AVBLOCK VARIABLE PTR  */ 01688000
         L     @10,MSGPTR(,AWAPTR)                                 0467 01689000
         LA    @07,AVBLOCK                                         0467 01690000
         ST    @07,@TF00001                                        0467 01691000
         MVC   VAR1(3,@10),@TF00001+1                              0467 01692000
*            L1 = LENGTH(AVBLOCK);          /* AND VARIABLE LENGTH   */ 01693000
         MVI   L1(@10),X'07'                                       0468 01694000
*          END;                                                    0469 01695000
*                                                                  0469 01696000
*        GENERATE REFS(CVBAREA);            /* CONVERT TO BINARY     */ 01697000
@RC00457 DS    0H                                                  0470 01698000
         PACK CVBAREA(8),PKAREA(8)             PACK VALUE               01699000
