         LA    @15,CMDTWO(,AWAPTR)                                 0408 01469000
         ST    @15,PPLCBUF(,@07)                                   0408 01470000
*        PPLUWA = 0;                        /* USER WORD             */ 01471000
         ST    @10,PPLUWA(,@07)                                    0409 01472000
*   END PARSLINK;                                                  0410 01473000
@EL00014 DS    0H                                                  0410 01474000
@EF00014 DS    0H                                                  0410 01475000
@ER00014 LM    @14,@12,12(@13)                                     0410 01476000
         BR    @14                                                 0410 01477000
*        RFY I UNRSTD;                                             0411 01478000
*/********************************************************************/ 01479000
*/*                                                                  */ 01480000
*/* SEGMENT NAME -                                                   */ 01481000
*/*      SYSPBLK                                                     */ 01482000
*/*                                                                  */ 01483000
*/* FUNCTION -                                                       */ 01484000
*/*      THIS VALIDITY CHECK ROUTINE CHECKS THE VALUE ENTERED ON     */ 01485000
*/*      THE PROMPT FOR A VALID KEYWORD OF BLOCK, AVBLOCK, TRACKS,   */ 01486000
*/*      OR CYLINDERS.                                               */ 01487000
*/*                                                                  */ 01488000
*/* OPERATION -                                                      */ 01489000
*/*      IF MORE THAN ONE SUBSCRIPT IS ENTERED, PARSE RETURN CODE    */ 01490000
*/*      IS SET TO 4. ELSE, SET UP A NEW COMMAND BUFFER WITH THE     */ 01491000
*/*      PARAMETERS ENTERED ON THE PROMPT. INVOKE PARSE, PASSING     */ 01492000
*/*      A LIST OF KEYWORDS IN THE PCL. IF PARSE FINDS A MATCH,      */ 01493000
*/*      MOVE THE PARAMETER INTO THE ORIGINAL PDE.                   */ 01494000
*/*                                                                  */ 01495000
*/* INPUT -                                                          */ 01496000
*/*      PTR TO PDE                                                  */ 01497000
*/*      THIRD LEVEL PCL                                             */ 01498000
*/*                                                                  */ 01499000
*/* OUTPUT -                                                         */ 01500000
*/*      RETURN CODE = 0 - VALID PARAMETER                           */ 01501000
*/*                    4 - INVALID PARAMETER, PARSE PROMPTS          */ 01502000
*/********************************************************************/ 01503000
*                                      /* ENTRY FOR BLOCK PROMPT     */ 01504000
*    SYSPBLK: PROC OPTIONS(NOSAVEAREA);                            0412 01505000
SYSPBLK  STM   @14,@12,12(@13)                                     0412 01506000
*        R15 = R1->VALWORK;            /* GET STORED REGISTERS       */ 01507000
         L     R15,VALWORK(,R1)                                    0413 01508000
*        GENERATE;                     /* SET UP ADDRESSABILITY      */ 01509000
         LM  2,12,0(15)                   RESTORE ALLOC REGISTERS       01510000
         LA  14,SAVEVC                    VC SAVER IN REG 14            01511000
         ST  14,8(13)                     VC SAVER IN PARSE SAVER       01512000
         ST  13,4(14)                     PARSE SAVER IN VC SAVER       01513000
         LR  13,14                        VC SAVER IN REG 13            01514000
*        VCKPTR = R1;                  /* PARM LIST FROM REG 1       */ 01515000
         LR    VCKPTR,R1                                           0415 01516000
*        PRCODE = 0;                   /* INIT RETURN CODE           */ 01517000
         SLR   @10,@10                                             0416 01518000
         ST    @10,PRCODE(,AWAPTR)                                 0416 01519000
*        IF SUBNUM2 > 1 THEN           /* IF MORE THAN ONE SUBSCRIPT,*/ 01520000
         L     @10,VALPDE(,VCKPTR)                                 0417 01521000
         CLI   SUBNUM2(@10),1                                      0417 01522000
         BNH   @RF00417                                            0417 01523000
*          PRCODE = 4;                 /* THEN PARSE PROMPTS         */ 01524000
         MVC   PRCODE(4,AWAPTR),@CF00337                           0418 01525000
*        ELSE                                                      0419 01526000
*          DO;                         /* ELSE                       */ 01527000
         B     @RC00417                                            0419 01528000
@RF00417 DS    0H                                                  0420 01529000
*            SAVPPL = PPLPTR;          /* SAVE PPL PTR               */ 01530000
         L     SAVPPL,PPLPTR(,AWAPTR)                              0420 01531000
*            CALL PARSLNK2;            /* SET UP FOR LINK TO PARSE   */ 01532000
         BAL   @14,PARSLNK2                                        0421 01533000
*            R1 = PPLPTR;              /* PARM LIST IN REGISTER 1    */ 01534000
         L     R1,PPLPTR(,AWAPTR)                                  0422 01535000
*              DO; /*CALLTSSR EP(IKJPARS)*/                        0423 01536000
*                RESPECIFY GPR01P RSTD;                            0424 01537000
*                IF CVTPARS = 0                                    0425 01538000
*                  THEN                                            0425 01539000
         L     @10,CVTPTR                                          0425 01540000
         L     @10,CVTPARS(,@10)                                   0425 01541000
         LTR   @10,@10                                             0425 01542000
         BNZ   @RF00425                                            0425 01543000
*                    GEN(LINK EP=IKJPARS);                         0426 01544000
         LINK EP=IKJPARS                                                01545000
*                  ELSE                                            0427 01546000
*                    CALL CVTPARS;                                 0427 01547000
         B     @RC00425                                            0427 01548000
@RF00425 L     @10,CVTPTR                                          0427 01549000
         L     @15,CVTPARS(,@10)                                   0427 01550000
         BALR  @14,@15                                             0427 01551000
*                RESPECIFY GPR01P UNRSTD;                          0428 01552000
@RC00425 DS    0H                                                  0429 01553000
*              END;   /* INVOKE PARSE       @Y30NQKK*/             0429 01554000
*            RCODESV = R15;            /* SAVER RETURN CODE          */ 01555000
         ST    R15,RCODESV(,AWAPTR)                                0430 01556000
*            IF RCODESV > 0 THEN       /* IF PARSE FAILED            */ 01557000
         L     @10,RCODESV(,AWAPTR)                                0431 01558000
         LTR   @10,@10                                             0431 01559000
         BNP   @RF00431                                            0431 01560000
*                PRCODE = 12;          /* PARSE TERMINATE            */ 01561000
         MVC   PRCODE(4,AWAPTR),@CF01223                           0432 01562000
*            ELSE                                                  0433 01563000
*              DO;                     /* OTHERWISE,                 */ 01564000
         B     @RC00431                                            0433 01565000
@RF00431 DS    0H                                                  0434 01566000
*                IF BLOKNBR = 0 THEN   /* IF PARM NOT ENTERED,       */ 01567000
         L     @10,PDLADDR3(,AWAPTR)                               0434 01568000
         LH    @10,BLOKNBR(,@10)                                   0434 01569000
         LTR   @10,@10                                             0434 01570000
         BNZ   @RF00434                                            0434 01571000
*                  PRCODE = 4;         /* INDICATE PARSE PROMPT      */ 01572000
         MVC   PRCODE(4,AWAPTR),@CF00337                           0435 01573000
*                ELSE                                              0436 01574000
*                  DO;                 /* IF PARM IS ENTERED,        */ 01575000
         B     @RC00434                                            0436 01576000
@RF00434 DS    0H                                                  0437 01577000
*                                      /* SAVE PARM IN PDE           */ 01578000
*                    BLOKPDE = BLOKNBR;                            0437 01579000
         L     @10,PTRPDL(,AWAPTR)                                 0437 01580000
         L     @07,PDLADDR3(,AWAPTR)                               0437 01581000
         LH    @07,BLOKNBR(,@07)                                   0437 01582000
