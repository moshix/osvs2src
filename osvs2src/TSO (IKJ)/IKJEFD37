*        RCODESV = 0;                  /* INIT RET CODE IN WORKAREA  */ 01044000
         ST    @10,RCODESV(,AWAPTR)                                0293 01045000
*        PKAREA(1) = 'F0'X;            /* 1ST BYTE IN PACKAREA F0    */ 01046000
         MVI   PKAREA,X'F0'                                        0294 01047000
*        P = ADDR(PKAREA(1));          /* POINT TO 1ST BYTE          */ 01048000
         LA    @10,PKAREA                                          0295 01049000
         ST    @10,P(,AWAPTR)                                      0295 01050000
*        Q = ADDR(PKAREA(2));          /* POINT TO 2ND BYTE          */ 01051000
         LA    @07,PKAREA+1                                        0296 01052000
         ST    @07,Q(,AWAPTR)                                      0296 01053000
*        Q->AREA1 = P->AREA2;          /* CLEAR PACKAREA TO ALL F0   */ 01054000
         MVC   AREA1(15,@07),AREA2(@10)                            0297 01055000
*        RFY Y RSTD;                   /* RESTRICT WORK REGISTER     */ 01056000
*        Y = 8-VCBLKLEN+ADDR(PKAREA);  /* MOVE VALUE TO PACKAREA     */ 01057000
         L     @10,VALPDE(,VCKPTR)                                 0299 01058000
         LH    @07,VCBLKLEN(,@10)                                  0299 01059000
         LA    Y,8                                                 0299 01060000
         SLR   Y,@07                                               0299 01061000
         LA    @04,PKAREA                                          0299 01062000
         ALR   Y,@04                                               0299 01063000
*        PKAREA1(1:VCBLKLEN)=VCBLKBUF(1:VCBLKLEN);                 0300 01064000
         BCTR  @07,0                                               0300 01065000
         L     @10,VCBLKPTR(,@10)                                  0300 01066000
         EX    @07,@SM01301                                        0300 01067000
*        GENERATE REFS(CVBAREA);       /* PACK & CONVERT TO BINARY   */ 01068000
         PACK  CVBAREA(8),PKAREA(8)                                     01069000
         CVB   Y,CVBAREA                                                01070000
*        IF Y> 255 THEN                /* IF VALUE EXCEEDS 255,      */ 01071000
         C     Y,@CF01242                                          0302 01072000
         BNH   @RF00302                                            0302 01073000
*          PARSRTC = 4;                /* INDICATE PARSE PROMPT      */ 01074000
         MVC   PARSRTC(4,R13),@CF00337                             0303 01075000
*   END SYSMAX;                                                    0304 01076000
@EL00010 DS    0H                                                  0304 01077000
@EF00010 DS    0H                                                  0304 01078000
@ER00010 LM    @14,@12,12(@13)                                     0304 01079000
         BR    @14                                                 0304 01080000
*        RFY Y UNRSTD;                                             0305 01081000
