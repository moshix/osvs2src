*                VAR1 = ADDR(SPC2);    /* PTR TO INCREMENT VARIABLE  */ 00812000
         L     @10,MSGPTR(,AWAPTR)                                 0229 00813000
         LA    @09,SPC2                                            0229 00814000
         ST    @09,@TF00001                                        0229 00815000
         MVC   VAR1(3,@10),@TF00001+1                              0229 00816000
*                L1 = LENGTH(SPC2);    /* AND LENGTH IN PARM LIST    */ 00817000
         MVI   L1(@10),X'0F'                                       0230 00818000
*              END;                                                0231 00819000
*            ELSE                      /* OTHERWISE,                 */ 00820000
*              DO;                     /* ASSUME DIR ENTRY -         */ 00821000
         B     @RC00227                                            0232 00822000
@RF00227 DS    0H                                                  0233 00823000
*                VAR1 = ADDR(DIR1);    /* PTR TO DIR VARIABLE        */ 00824000
         L     @10,MSGPTR(,AWAPTR)                                 0233 00825000
         LA    @09,DIR1                                            0233 00826000
         ST    @09,@TF00001                                        0233 00827000
         MVC   VAR1(3,@10),@TF00001+1                              0233 00828000
*                L1 = LENGTH(DIR1);    /* AND LENGTH IN PARM LIST    */ 00829000
         MVI   L1(@10),X'03'                                       0234 00830000
*              END;                                                0235 00831000
*            CALL VCPUTMSG;            /* ISSUE MESSAGE              */ 00832000
@RC00227 DS    0H                                                  0236 00833000
@RC00222 BAL   @14,VCPUTMSG                                        0236 00834000
*            IF RCODESV > 0 THEN       /* IF MSG PROCESSOR FAILED,   */ 00835000
         L     @10,RCODESV(,AWAPTR)                                0237 00836000
         LTR   @10,@10                                             0237 00837000
         BNP   @RF00237                                            0237 00838000
*              PRCODE = 12;            /* INDICATE PARSE TERMINATE   */ 00839000
         MVC   PRCODE(4,AWAPTR),@CF01223                           0238 00840000
*            ELSE                      /* ELSE,                      */ 00841000
*              VALMSG = ADDR(MSG01);   /* PASS 2ND LEVEL MSG         */ 00842000
         B     @RC00237                                            0239 00843000
@RF00237 LA    @10,MSG01                                           0239 00844000
         ST    @10,VALMSG(,VCKPTR)                                 0239 00845000
*          END;                                                    0240 00846000
*   END  SYSPACE;                      /* RETURN                     */ 00847000
@EL00008 DS    0H                                                  0241 00848000
@EF00008 DS    0H                                                  0241 00849000
@ER00008 LM    @14,@12,@SA00008                                    0241 00850000
         BR    @14                                                 0241 00851000
*        RFY Y UNRSTD;                                             0242 00852000
*/********************************************************************/ 00853000
*/*                                                                  */ 00854000
*/* SEGMENT NAME -                                                   */ 00855000
*/*      SYSDSN                                                      */ 00856000
*/*                                                                  */ 00857000
*/* FUNCTION -                                                       */ 00858000
*/*      THIS VALIDITY CHECK ROUTINE CHECKS THE DATASET PARAMETER    */ 00859000
*/*      FOR A MAXIMUM OF 255 DSNAMES IN THE LIST, AN ASTERISK       */ 00860000
*/*      ENTERED AS DSNAME WITHIN THE LIST, AND A MEMBER NAME        */ 00861000
*/*      SPECIFIED WITH NO DSNAME.                                   */ 00862000
*/*                                                                  */ 00863000
*/* OPERATION -                                                      */ 00864000
*/*      A COUNTER IS INCREMENTED UPON ENTRY TO THE ROUTINE. IF THE  */ 00865000
*/*      COUNTER EXCEEDS 255, THE RETURN CODE IS SET TO 12 AND AN    */ 00866000
*/*      ERROR MESSAGE IS ISSUED. IF THE COUNTER IS GREATER THAN 1   */ 00867000
*/*      AND AN ASTERISK HAS BEEN ENTERED AS DSNAME, THE RETURN CODE */ 00868000
*/*      IS SET TO 12, AND AN ERROR MESSAGE IS ISSUED. IF A MEMBER   */ 00869000
*/*      NAME HAS BEEN SPECIFIED WITH NO DSNAME, THE RETURN CODE IS  */ 00870000
*/*      SET TO 4, AND A SECOND LEVEL MESSAGE IS PASSED TO PARSE.    */ 00871000
*/*                                                                  */ 00872000
*/* INPUT -                                                          */ 00873000
*/*      PTR TO PDE                                                  */ 00874000
*/*      DSNAME COUNTER                                              */ 00875000
*/*                                                                  */ 00876000
*/* OUTPUT -                                                         */ 00877000
*/*      RETURN CODE = 0 - VALID PARAMETER                           */ 00878000
*/*                    4 - INVALID PARAMETER, PARSE PROMPTS          */ 00879000
*/*                   12 - INVALID PARAMETER, PARSE TERMINATES       */ 00880000
*/*                                                                  */ 00881000
*/* NOTE:  THIS ROUTINE REPRESENTS A DIRECT LINE FUNCTION AND CAN    */ 00882000
*/*        BE INCORPORATED AS AN INCLUDED CODE SEGMENT WHEN THE      */ 00883000
*/*        INCLUDE FUNCTION HAS RECEIVED A STANDARD DEFINITION.      */ 00884000
*/********************************************************************/ 00885000
*                                      /* ENTRY FOR DSNAME           */ 00886000
*    SYSDSN: PROC OPTIONS(NOSAVEAREA);                             0243 00887000
SYSDSN   STM   @14,@12,12(@13)                                     0243 00888000
*        R15 = R1->VALWORK;            /* GET STORED REGISTERS       */ 00889000
         L     R15,VALWORK(,R1)                                    0244 00890000
*        GENERATE;                     /* SET UP ADDRESSABILITY      */ 00891000
         LM  2,12,0(15)                   RESTORE ALLOC REGISTERS       00892000
         LA  14,SAVEVC                    VC SAVER IN REG 14            00893000
         ST  14,8(13)                     VC SAVER IN PARSE SAVER       00894000
         ST  13,4(14)                     PARSE SAVER IN VC SAVER       00895000
         LR  13,14                        VC SAVER IN REG 13            00896000
*        VCKPTR = R1;                  /* PARM LIST FROM REG 1       */ 00897000
         LR    VCKPTR,R1                                           0246 00898000
*        PRCODE = 0;                   /* INIT PARSE RETURN CODE     */ 00899000
         SLR   @10,@10                                             0247 00900000
         ST    @10,PRCODE(,AWAPTR)                                 0247 00901000
*        RCODESV = 0;                  /* INIT RET CODE IN WORKAREA  */ 00902000
         ST    @10,RCODESV(,AWAPTR)                                0248 00903000
*        IF FIRSTPDE = '0'B THEN       /* IS THIS 1ST DSNAME @YM05479*/ 00904000
         TM    FIRSTPDE(AWAPTR),B'00000001'                        0249 00905000
         BNZ   @RF00249                                            0249 00906000
*          DO;                         /* YES -              @YM05479*/ 00907000
*            FIRSTPDE = '1'B;          /* INDICATE SO        @YM05479*/ 00908000
         OI    FIRSTPDE(AWAPTR),B'00000001'                        0251 00909000
*            FIRSTPTR = VALPDE;        /* SAVE PDE ADDR      @YM05479*/ 00910000
         L     @10,VALPDE(,VCKPTR)                                 0252 00911000
         ST    @10,FIRSTPTR(,AWAPTR)                               0252 00912000
*          END;                        /*                    @YM05479*/ 00913000
*        ELSE                          /* NO -               @YM05479*/ 00914000
*          IF FIRSTPTR = VALPDE THEN   /* IS THIS PDE ADDR   @YM05479   00915000
*                                         IN 1ST LOCATION    @YM05479*/ 00916000
         B     @RC00249                                            0254 00917000
@RF00249 CLC   FIRSTPTR(4,AWAPTR),VALPDE(VCKPTR)                   0254 00918000
         BNE   @RF00254                                            0254 00919000
*            DO;                       /* YES -              @YM05479*/ 00920000
*              DSNCTR = 0;             /* RESET DSN COUNTER  @YM05479*/ 00921000
         SLR   @10,@10                                             0256 00922000
         STH   @10,DSNCTR(,AWAPTR)                                 0256 00923000
*              ASTRSK = '0'B;          /* RESET * INDICATOR  @YM05479*/ 00924000
         NI    ASTRSK(AWAPTR),B'11111011'                          0257 00925000
*            END;                      /*                    @YM05479*/ 00926000
*        IF ASTRSK = '1'B THEN         /* IF * FOUND AS PREVIOUS DSN */ 00927000
@RF00254 DS    0H                                                  0259 00928000
@RC00249 TM    ASTRSK(AWAPTR),B'00000100'                          0259 00929000
         BO    @RT00259                                            0259 00930000
*          GO TO INVASTK;              /* PROCESS ERROR              */ 00931000
*        DSNCTR = DSNCTR + 1;          /* INCREMENT COUNTER          */ 00932000
         LA    @10,1                                               0261 00933000
         AH    @10,DSNCTR(,AWAPTR)                                 0261 00934000
         STH   @10,DSNCTR(,AWAPTR)                                 0261 00935000
*        IF DSNCTR > 255 THEN          /* IF MORE THAN 255 DSNAMES,  */ 00936000
         C     @10,@CF01242                                        0262 00937000
         BNH   @RF00262                                            0262 00938000
*          DO;                         /* THEN                       */ 00939000
*            PRCODE  = 12;             /* INDICATE PARSE TERMINATE   */ 00940000
         MVC   PRCODE(4,AWAPTR),@CF01223                           0264 00941000
*            MSGID = '107A';           /* SET MESSAGE ID             */ 00942000
         L     @10,MSGPTR(,AWAPTR)                                 0265 00943000
         MVC   MSGID(4,@10),@CC01243                               0265 00944000
*            CALL VCPUTMSG;            /* ISSUE ERROR MESSAGE        */ 00945000
         BAL   @14,VCPUTMSG                                        0266 00946000
*            AWARSV1 = '1'B;           /* INDICATE MSG ISSUED        */ 00947000
         OI    AWARSV1(AWAPTR),B'00000010'                         0267 00948000
*          END;                                                    0268 00949000
*        ELSE                                                      0269 00950000
*          IF VCTERM = '*' THEN        /* IF THIS DSNAME IS *        */ 00951000
         B     @RC00262                                            0269 00952000
@RF00262 L     @10,VALPDE(,VCKPTR)                                 0269 00953000
         L     @10,VCDSNPTR(,@10)                                  0269 00954000
         CLI   VCTERM(@10),C'*'                                    0269 00955000
         BNE   @RF00269                                            0269 00956000
*            DO;                       /* THEN                       */ 00957000
*              IF DSNCTR = 1 THEN      /* IF 1ST DSNAME,             */ 00958000
         CLC   DSNCTR(2,AWAPTR),@CH00032                           0271 00959000
         BNE   @RF00271                                            0271 00960000
*                ASTRSK = '1'B;        /* SET INDICATOR              */ 00961000
         OI    ASTRSK(AWAPTR),B'00000100'                          0272 00962000
*              ELSE                    /* ELSE,                      */ 00963000
*                DO;                   /* IF * WITHIN DSN LIST -     */ 00964000
         B     @RC00271                                            0273 00965000
@RF00271 DS    0H                                                  0274 00966000
* INVASTK:                                                         0274 00967000
*                   PRCODE  = 12;      /* INDICATE PARSE TERMINATE   */ 00968000
INVASTK  MVC   PRCODE(4,AWAPTR),@CF01223                           0274 00969000
*                   MSGID = '111A';    /* SET MESSAGE ID             */ 00970000
         L     @10,MSGPTR(,AWAPTR)                                 0275 00971000
         MVC   MSGID(4,@10),@CC01246                               0275 00972000
*                   CALL VCPUTMSG;     /* ISSUE ERROR MESSAGE        */ 00973000
         BAL   @14,VCPUTMSG                                        0276 00974000
*                   AWARSV1 = '1'B;    /* INDICATE MSG ISSUED        */ 00975000
         OI    AWARSV1(AWAPTR),B'00000010'                         0277 00976000
*                END;                                              0278 00977000
*            END;                                                  0279 00978000
*          ELSE                                                    0280 00979000
*            IF VCDSNPRS = '0'B &      /* IF DSN NOT ENTERED AND     */ 00980000
*               VCMBRPRS = '1'B THEN   /* MEMBER NAME IS ENTERED,    */ 00981000
         B     @RC00269                                            0280 00982000
@RF00269 L     @10,VALPDE(,VCKPTR)                                 0280 00983000
         TM    VCDSNPRS(@10),B'10000000'                           0280 00984000
         BNZ   @RF00280                                            0280 00985000
         TM    VCMBRPRS(@10),B'10000000'                           0280 00986000
         BNO   @RF00280                                            0280 00987000
*              DO;                     /* THEN                       */ 00988000
*                PRCODE = 4;           /* INDICATE PARSE PROMPT      */ 00989000
         MVC   PRCODE(4,AWAPTR),@CF00337                           0282 00990000
*                VALMSG = ADDR(MSG02); /* PASS 2ND LEVEL MSG         */ 00991000
         LA    @10,MSG02                                           0283 00992000
         ST    @10,VALMSG(,VCKPTR)                                 0283 00993000
*              END;                                                0284 00994000
*        GEN (L 13,4(13));             /* RESTORE PARSE REGISTERS    */ 00995000
@RF00280 DS    0H                                                  0285 00996000
@RC00269 DS    0H                                                  0285 00997000
@RC00262 DS    0H                                                  0285 00998000
         L 13,4(13)                                                     00999000
*        PARSRTC = PRCODE;             /* RETURN CODE IN REG 15      */ 01000000
         L     @10,PRCODE(,AWAPTR)                                 0286 01001000
         ST    @10,PARSRTC(,R13)                                   0286 01002000
*   END  SYSDSN;                                                   0287 01003000
@EL00009 DS    0H                                                  0287 01004000
@EF00009 DS    0H                                                  0287 01005000
@ER00009 LM    @14,@12,12(@13)                                     0287 01006000
         BR    @14                                                 0287 01007000
*/********************************************************************/ 01008000
*/*                                                                  */ 01009000
*/* SEGMENT NAME -                                                   */ 01010000
*/*      SYSMAX                                                      */ 01011000
*/*                                                                  */ 01012000
*/* FUNCTION -                                                       */ 01013000
*/*      THIS VALIDITY CHECK ROUTINE CHECKS THE VOLUME COUNT AND     */ 01014000
*/*      VOLUME SEQUENCE NUMBER FOR THE MAXIMUM VALUE.               */ 01015000
