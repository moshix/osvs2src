         LH    @10,TERMID(,@10)                                    0529 01344000
         STH   @10,MPLID(,MPLPTR)                                  0529 01345000
*       MPLSTOP='1'B;               /* STOP THE MONITORS             */ 01346000
*       MPLTERM='1'B;               /* TERMINAL USER                 */ 01347000
         OI    MPLSTOP(MPLPTR),B'01010000'                         0531 01348000
*       CALL IEAVMNTR(MPLLIST);     /* CALL COM TASK ROUTINE         */ 01349000
         LA    @10,MPLLIST                                         0532 01350000
         ST    @10,@AL00001                                        0532 01351000
         L     @10,UCMBASPT(,CVTPTR)                               0532 01352000
         L     @15,UCMMNTR(,@10)                                   0532 01353000
         LA    @01,@AL00001                                        0532 01354000
         BALR  @14,@15                                             0532 01355000
*     END;                          /* END OF PROCESS TO STOP      0533 01356000
*                                      MONITORS                      */ 01357000
*   RFY                                                            0534 01358000
*    (DATAPTR,                                                     0534 01359000
*     CODEPTR,                                                     0534 01360000
*     LOCKRTNC,                                                    0534 01361000
*     SAVED11,                                                     0534 01362000
*     SAVED12,                                                     0534 01363000
*     SAVED13) RSTD;                                               0534 01364000
@RF00524 DS    0H                                                  0535 01365000
*   SAVED11=DATAPTR;                /* SAVE DATA PTR REGISTER        */ 01366000
