         TM    FSTFLG(@10),B'00001000'                             0524 01334000
         BNO   @RF00524                                            0524 01335000
*     DO;                           /* PREPARE VMNTR PARM LIST       */ 01336000
*       MPLPTR=ADDR(MPLLIST);       /* ADDRESSIBILITY TO MPL         */ 01337000
         LA    MPLPTR,MPLLIST                                      0526 01338000
*       MPLLIST=MPLLIST&&MPLLIST;   /* ZERO OUT THE PARM LIST        */ 01339000
         XC    MPLLIST(8),MPLLIST                                  0527 01340000
