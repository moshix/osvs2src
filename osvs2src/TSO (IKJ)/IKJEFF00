         LR    SAVED12,CODEPTR                                     0536 01369000
*   SAVED13=LOCKRTNC;               /* SAVE REGISTER THIRTEEN        */ 01370000
         LR    SAVED13,LOCKRTNC                                    0537 01371000
*   GEN(SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(TSB,IKJEFF00))   01372000
