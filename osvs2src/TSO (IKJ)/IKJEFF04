         TITLE 'IKJEFF04 - SUBMIT INPUT INITIALIZER                    *00001000
                        '                                               00002000
IKJEFF04 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKJEFF04  74.015'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN R,LV=(0)                                               00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @11,8(,@13)                                         0001 00020000
         LR    @13,@11                                             0001 00021000
         MVC   @PC00001(4),0(@01)                                  0001 00022000
DUMMYLBL DS    0H                                                  0003 00023000
DUMYLBLX DS    0H                                                  0031 00024000
JSCBDISP EQU 188                                                        00025000
JSCBAOS1 EQU  72                                                        00026000
*                                                                  0113 00027000
JSCBAOS2 EQU 192                                                        00028000
*   /*****************************************************************/ 00029000
*   /*                                                               */ 00030000
*   /* END OF JSCB                                                   */ 00031000
*   /*                                                               */ 00032000
*   /*****************************************************************/ 00033000
*                                                                  0114 00034000
*DUMYLBL1:                          /* SEPARATE TWO HEADER COMMENTS  */ 00035000
*                                                                  0114 00036000
*   /*****************************************************************/ 00037000
*   /*                                                               */ 00038000
*   /* THE IEZJSCB MAPPING MACRO IS INCLUDED LAST BECAUSE IT HAS     */ 00039000
*   /* GENERATE STATEMENTS WHICH SHOULD FOLLOW THE DECLARES, TO MEET */ 00040000
*   /* STANDARDS                                                     */ 00041000
*   /*                                                               */ 00042000
*   /*****************************************************************/ 00043000
*                                                                  0114 00044000
*DUMYLBL2:                                                         0114 00045000
*                                                                  0114 00046000
*   /*****************************************************************/ 00047000
*   /*                                                               */ 00048000
*   /* INITIALIZATION                                                */ 00049000
*   /*                                                               */ 00050000
*   /*****************************************************************/ 00051000
*                                                                  0114 00052000
*   FIBCMPTR=ADDR(FIBPARML);        /* USE LOCAL POINTER TO FIBCPARM    00053000
*                                      PARAMETER LIST (FROM SVC 100)    00054000
*                                      FOR EFFICIENCY                */ 00055000
DUMYLBL1 DS    0H                                                  0114 00056000
DUMYLBL2 L     FIBCMPTR,@PC00001                                   0114 00057000
*   INTSWITS=INTSWITS&&INTSWITS;    /* ZERO OUT AREA FOR INTERNAL  0115 00058000
*                                      SWITCHES                      */ 00059000
         XC    INTSWITS(1),INTSWITS                                0115 00060000
*/********************************************************************/ 00061000
*/*  INITSUBT SEGMENT - INITIALIZES INTERNAL TABLES USED BY          */ 00062000
*/*  THE SUBMIT COMMAND                                              */ 00063000
*/********************************************************************/ 00064000
*                                                                  0116 00065000
*   MSGTABLE=MSGTABLE&&MSGTABLE;    /* CLEAR THE MESSAGE AREA AND  0116 00066000
*                                      INITIALIZE THIS INTERFACE     */ 00067000
         XC    MSGTABLE(64),MSGTABLE                               0116 00068000
*   HISTORY=HISTORY&&HISTORY;       /* ZERO OUT HISTORY FIELD        */ 00069000
         XC    HISTORY(52),HISTORY                                 0117 00070000
*   CONTAB=CONTAB&&CONTAB;          /* ZERO OUT CONTROL TABLE - MUST    00071000
*                                      DO BEFORE FIBCUSER SET        */ 00072000
         XC    CONTAB(85),CONTAB                                   0118 00073000
*   EXAREA=EXAREA&&EXAREA;          /* ZERO THE AREA USED AS AN    0119 00074000
*                                      INTERFACE TO THE PARSE EXIT 0119 00075000
*                                      ROUTINE (IKJEFF16)            */ 00076000
*                                                                  0119 00077000
         XC    EXAREA(46),EXAREA                                   0119 00078000
*   /*****************************************************************/ 00079000
*   /*                                                               */ 00080000
*   /* INITIALIZE SUBMIT MESSAGE TABLE (IKJEFFMT)                    */ 00081000
*   /*                                                               */ 00082000
*   /*****************************************************************/ 00083000
*                                                                  0120 00084000
*   ECB=0;                          /* ZERO THE SUBMIT ECB - NOT   0120 00085000
*                                      POSTED BECAUSE SUBMIT DOESN'T    00086000
*                                      HAVE SUBCOMMANDS              */ 00087000
         SLR   @10,@10                                             0120 00088000
         ST    @10,ECB                                             0120 00089000
*   LISTPTR=ADDR(MSGCSECT);         /* ADDR OF MSG PARMLIST          */ 00090000
         LA    @10,MSGCSECT                                        0121 00091000
         ST    @10,LISTPTR                                         0121 00092000
*   MTCPPL=ADDR(FIBCPPLC);          /* ADDR OF COPY OF TMP'S CPPL C.B*/ 00093000
         LA    @10,FIBCPPLC(,FIBCMPTR)                             0122 00094000
         ST    @10,MTCPPL                                          0122 00095000
*   ECBPTR=ADDR(ECB);                                              0123 00096000
         LA    @04,ECB                                             0123 00097000
         ST    @04,ECBPTR                                          0123 00098000
*   MTPUTLSW=ON;                    /* INDICATE DOING PUTLINE        */ 00099000
         OI    MTPUTLSW,B'01000000'                                0124 00100000
*   MSGCSECT=ADDR(IKJEFF03);        /* ADDR OF SUBMIT'S MESSAGES     */ 00101000
         L     @15,@CV00100                                        0125 00102000
         ST    @15,MSGCSECT                                        0125 00103000
*   MSGRTN=ADDR(IKJEFF02);          /* ADDR OF MESSAGE ISSUER ROUTINE*/ 00104000
         L     @15,@CV00099                                        0126 00105000
         ST    @15,MSGRTN                                          0126 00106000
*   MTHIGH=ON;                      /* END OF MSG PARM LIST          */ 00107000
*                                                                  0127 00108000
         OI    MTHIGH,B'10000000'                                  0127 00109000
*   /*****************************************************************/ 00110000
*   /*                                                               */ 00111000
*   /* INITIALIZE SUBMIT HISTORY TABLE (IKJEFFHT)                    */ 00112000
*   /*                                                               */ 00113000
*   /*****************************************************************/ 00114000
*                                                                  0128 00115000
*   SUBTAB='HISTORY TAB ';          /* *** INIT HISTORY TABLE        */ 00116000
         MVC   SUBTAB(12),@CC02058                                 0128 00117000
*   FIRST='1'B;                     /* FIRST SWITCH ON INDICATES   0129 00118000
*                                      PROCESSING IS TAKING PLACE AND   00119000
*                                      NO ERROR STATE EXISTS AT THIS    00120000
*                                      TIME                          */ 00121000
         OI    FIRST,B'00100000'                                   0129 00122000
*   JOBX=ON;                        /* INDICATES THAT THE IBM EXIT 0130 00123000
*                                      ONLY WISHES TO LOOK AT JOB  0130 00124000
*                                      CARDS                         */ 00125000
*                                                                  0130 00126000
         OI    JOBX,B'10000000'                                    0130 00127000
*   /*****************************************************************/ 00128000
*   /*                                                               */ 00129000
*   /* INITIALIZE SUBMIT CONTROL TABLE (IKJEFFCT)                    */ 00130000
*   /*                                                               */ 00131000
*   /*****************************************************************/ 00132000
*                                                                  0131 00133000
*   FIBCUSER=ADDR(CONTAB);          /* IKJEFF20, THE SVC 100 STAI  0131 00134000
*                                      RTN, NEEDS THIS ADDRESS. IT 0131 00135000
*                                      USES THE CLOSERPT AND CTACBPT    00136000
*                                      FIELDS                        */ 00137000
         LA    @15,CONTAB                                          0131 00138000
         ST    @15,FIBCUSER(,FIBCMPTR)                             0131 00139000
*   TABID='SUBMIT TABLE';           /* *** INIT CONTROL TABLE        */ 00140000
         MVC   TABID(12),@CC02060                                  0132 00141000
*   CONTABPT=ADDR(CONTAB);          /* CONTROL TABLE HAS PTR TO SELF */ 00142000
         LA    @15,CONTAB                                          0133 00143000
         ST    @15,CONTABPT                                        0133 00144000
*   HISTPT=ADDR(HISTORY);           /* HISTORY TABLE HOLDS INFO ABOUT   00145000
*                                      INPUT RECORDS PASSING THRU    */ 00146000
         LA    @15,HISTORY                                         0134 00147000
         ST    @15,HISTPT                                          0134 00148000
*   JOBNAMES=' ';                   /* INITIALIZE JOBNAMES AREA--USED   00149000
*                                      LATER FOR MSG INSERTS         */ 00150000
         MVI   JOBNAMES+1,C' '                                     0135 00151000
         MVC   JOBNAMES+2(14),JOBNAMES+1                           0135 00152000
         MVI   JOBNAMES,C' '                                       0135 00153000
*   JOBNAMPT=ADDR(JOBNAMES);        /* SAVE PTR TO JOBNAMES IN     0136 00154000
*                                      CONTROL TABLE                 */ 00155000
         LA    @15,JOBNAMES                                        0136 00156000
         ST    @15,JOBNAMPT                                        0136 00157000
*   MSGLISPT=ADDR(MSGTABLE);        /* INTERFACE TO ISSUE MESSAGE    */ 00158000
         LA    @15,MSGTABLE                                        0137 00159000
         ST    @15,MSGLISPT                                        0137 00160000
*   USER=FIBPSCBU;                  /* COPY THE USERID TO AN 8-BYTE     00161000
*                                      FIELD                         */ 00162000
         MVI   USER+7,C' '                                         0138 00163000
         MVC   USER(7),FIBPSCBU(FIBCMPTR)                          0138 00164000
*   USERIDPT=ADDR(USER);            /* USE FOR GENERATED JOBNAMES    */ 00165000
         LA    @15,USER                                            0139 00166000
         ST    @15,USERIDPT                                        0139 00167000
*   CTIDLN=FIBPSCBL;                /* SAVE USERID LENGTH, TOO       */ 00168000
         MVC   CTIDLN(1),FIBPSCBL(FIBCMPTR)                        0140 00169000
*   TMCTPT=ADDR(FIBCPPLC);          /* CPPL ADDR FOR SERVICE ROUTINES*/ 00170000
         ST    @10,TMCTPT                                          0141 00171000
*   COMECBPT=ADDR(ECB);             /* COMMON ECB-NEVER POSTED     0142 00172000
*                                      BECAUSE NO SUBCOMMANDS        */ 00173000
         ST    @04,COMECBPT                                        0142 00174000
*   RFY                                                            0143 00175000
*     R13 RSTD;                                                    0143 00176000
*   INITSAVE=R13;                   /* SAVING THE ADDR OF THE SAVE 0144 00177000
*                                      AREA IN THE CONTROL TABLE     */ 00178000
         ST    R13,INITSAVE                                        0144 00179000
*   RFY                                                            0145 00180000
*     R13 UNRSTD;                                                  0145 00181000
*                                                                  0145 00182000
*   /*****************************************************************/ 00183000
*   /*                                                               */ 00184000
*   /* SAVE THE COMMAND NAME IN THE SUBMIT CONTROL TABLE             */ 00185000
*   /*                                                               */ 00186000
*   /*****************************************************************/ 00187000
*                                                                  0146 00188000
*   IF CMDNAMEL<=LENGTH(CTCMDNM) THEN                              0146 00189000
         L     @10,FIBCPPLC(,FIBCMPTR)                             0146 00190000
         LH    @04,CMDNAMEL(,@10)                                  0146 00191000
         CH    @04,@CH00071                                        0146 00192000
         BH    @RF00146                                            0146 00193000
*     CTCMDNM=CMDNAME(1:CMDNAMEL);  /* GET COMMAND NAME AS ENTERED BY   00194000
*                                      THE USER (GET FROM COMMAND  0147 00195000
*                                      BUFFER RATHER THAN ECTPCMD, 0147 00196000
*                                      SINCE COULD HAVE SUBMIT UNDER    00197000
*                                      EDIT COMMAND). NAME IS USED AS   00198000
*                                      INSERT IN ERROR MESSAGES.     */ 00199000
         MVI   CTCMDNM+1,C' '                                      0147 00200000
         MVC   CTCMDNM+2(6),CTCMDNM+1                              0147 00201000
         BCTR  @04,0                                               0147 00202000
         EX    @04,@SM02162                                        0147 00203000
*   ELSE                            /* TRUNCATE NAME IF NECESSARY  0148 00204000
*                                      (COMMAND NAME IS MAXIMUM OF 8    00205000
*                                      BUT CMDNAMEL COULD INCLUDE  0148 00206000
*                                      TRAILING BLANKS IN THE COMMAND   00207000
*                                      BUFFER)                       */ 00208000
*     CTCMDNM=CMDNAME(1:LENGTH(CTCMDNM));                          0148 00209000
         B     @RC00146                                            0148 00210000
@RF00146 L     @10,FIBCPPLC(,FIBCMPTR)                             0148 00211000
         MVC   CTCMDNM(8),CMDNAME(@10)                             0148 00212000
*                                                                  0149 00213000
*/********************************************************************/ 00214000
*/*                                                                  */ 00215000
*/* ACNTCODE SEGMENT - PUTS USER'S ACCOUNTING INFORMATION            */ 00216000
*/*                    IN THE SUBMIT HISTORY TABLE                   */ 00217000
*/*                                                                  */ 00218000
*/*   (AT LOGON THE USER'S ACCOUNT # IS OBTAINED FROM THE UADS AND   */ 00219000
*/*    PASSED TO THE READER INTERPRETER.  MODULES IEFVJA AND IEFVFA  */ 00220000
*/*    CREATE THE ACCOUNT CONTROL TABLE (ACT) AND PLACE THE ACCOUNT  */ 00221000
*/*    # IN THIS CONTROL BLOCK, WHICH IS IN THE SCHEDULER WORK AREA. */ 00222000
*/*                                                                  */ 00223000
*/*    THE LOGON INSTALLATION EXIT COULD SUPPLY ACCOUNTING           */ 00224000
*/*    INFORMATION FOR THE USER, INSTEAD OF JUST THE ACCOUNT NUMBER.)*/ 00225000
*/*                                                                  */ 00226000
*/********************************************************************/ 00227000
*                                                                  0149 00228000
*   RFY                                                            0149 00229000
*     IEFAACTB BASED(ACTPTR);       /* CHANGE BASE OF ACT C.B.       */ 00230000
*                                                                  0149 00231000
@RC00146 DS    0H                                                  0150 00232000
*   /*****************************************************************/ 00233000
*   /*                                                               */ 00234000
*   /* INITIALIZE SWA MANAGER PARAMETER LIST                         */ 00235000
*   /*                                                               */ 00236000
*   /*****************************************************************/ 00237000
*                                                                  0150 00238000
*   SWEPAPTR=ADDR(MYEPA);           /* PASS THIS PTR TO SWA MANAGER  */ 00239000
         LA    @10,MYEPA                                           0150 00240000
         ST    @10,SWEPAPTR                                        0150 00241000
*   MYEPA=MYEPA&&MYEPA;             /* ZERO SWAEPA PARM LIST         */ 00242000
*                                                                  0151 00243000
         XC    MYEPA(16),MYEPA                                     0151 00244000
*   /*****************************************************************/ 00245000
*   /*                                                               */ 00246000
*   /* HAVE SWA MANAGER GET ADDRESS OF JCT CONTROL BLOCK (WHICH HAS  */ 00247000
*   /* SWA TTR FOR ACT C.B.)                                         */ 00248000
*   /*                                                               */ 00249000
*   /*****************************************************************/ 00250000
*                                                                  0152 00251000
*   SWVA=JSCBJCTA;                  /* TTR OF JCT IN SWA             */ 00252000
*                                                                  0152 00253000
         L     @04,TCBPTR                                          0152 00254000
         L     @04,TCBJSCB(,@04)                                   0152 00255000
         MVC   SWVA(3,@10),JSCBJCTA(@04)                           0152 00256000
*   /*****************************************************************/ 00257000
*   /*                                                               */ 00258000
*   /* ?SWAREQ (RL) EPA (SWEPAPTR);                                  */ 00259000
*   /*                                                               */ 00260000
*   /*****************************************************************/ 00261000
*                                                                  0153 00262000
*   CALL SWAMGR(SWEPAPTR,'RL');     /* GET PTR TO JCT FROM SWA MGR   */ 00263000
         LA    @10,SWEPAPTR                                        0153 00264000
         ST    @10,@AL00001                                        0153 00265000
         LA    @10,@CC02064                                        0153 00266000
         ST    @10,@AL00001+4                                      0153 00267000
         L     @10,CVTPTR                                          0153 00268000
         L     @10,CVTJESCT(,@10)                                  0153 00269000
         L     @15,JESWAA(,@10)                                    0153 00270000
         LA    @01,@AL00001                                        0153 00271000
         BALR  @14,@15                                             0153 00272000
*   RFY                                                            0154 00273000
*     R15 RSTD;                                                    0154 00274000
*   IF R15^=0 THEN                  /* BAD R.C. FROM QMGR (ISSUE MSG)*/ 00275000
         LTR   R15,R15                                             0155 00276000
         BZ    @RF00155                                            0155 00277000
*     DO;                                                          0156 00278000
*       RETCODE=R15;                /* SAVE QMGR'S RETURN CODE       */ 00279000
         ST    R15,RETCODE                                         0157 00280000
*       RFY                                                        0158 00281000
*         R15 UNRSTD;                                              0158 00282000
*       CALL QMGRERR;               /* ISSUE COMMAND SYSTEM ERROR MSG*/ 00283000
         BAL   @14,QMGRERR                                         0159 00284000
*     END;                                                         0160 00285000
*   ELSE                            /* CONTINUE IF HAVE ADDR OF JCT     00286000
*                                      IN SWBLKPTR FIELD (SWBLKID  0161 00287000
*                                      SHOULD BE 64 AND SWLNGTH =  0161 00288000
*                                      176)                          */ 00289000
*HAVEJCT:                           /* LABEL FOR DO/END GROUP        */ 00290000
*     DO;                                                          0161 00291000
*                                                                  0161 00292000
         B     @RC00155                                            0161 00293000
@RF00155 DS    0H                                                  0161 00294000
HAVEJCT  DS    0H                                                  0162 00295000
*       /*************************************************************/ 00296000
*       /*                                                           */ 00297000
*       /* HAVE SWA MANAGER GET ADDRESS OF ACT CONTROL BLOCK         */ 00298000
*       /*                                                           */ 00299000
*       /*************************************************************/ 00300000
*                                                                  0162 00301000
*       TEMPHOLD=JCTACTAD;          /* JCT HAS TTR FOR ACT IN SWA AND   00302000
*                                      SWAEPA'S SWBLKPTR -> JCT      */ 00303000
         L     @10,SWEPAPTR                                        0162 00304000
         L     @01,JCTPTR(,@10)                                    0162 00305000
         SLR   TEMPHOLD,TEMPHOLD                                   0162 00306000
         ICM   TEMPHOLD,7,JCTACTAD(@01)                            0162 00307000
*       MYEPA=MYEPA&&MYEPA;         /* ZERO OUT SWAEPA PARMLIST AGAIN*/ 00308000
         XC    MYEPA(16),MYEPA                                     0163 00309000
*       SWVA=TEMPHOLD;              /* PUT TTR FOR ACT IN SWAEPA     */ 00310000
*                                                                  0164 00311000
         STCM  TEMPHOLD,7,SWVA(@10)                                0164 00312000
*       /*************************************************************/ 00313000
*       /*                                                           */ 00314000
*       /* ?SWAREQ (RL) EPA (SWEPAPTR);                              */ 00315000
*       /*                                                           */ 00316000
*       /*************************************************************/ 00317000
*                                                                  0165 00318000
*       CALL SWAMGR(SWEPAPTR,'RL'); /* GET PTR TO ACT FROM SWA MGR   */ 00319000
         LA    @10,SWEPAPTR                                        0165 00320000
         ST    @10,@AL00001                                        0165 00321000
         LA    @10,@CC02064                                        0165 00322000
         ST    @10,@AL00001+4                                      0165 00323000
         L     @10,CVTPTR                                          0165 00324000
         L     @10,CVTJESCT(,@10)                                  0165 00325000
         L     @15,JESWAA(,@10)                                    0165 00326000
         LA    @01,@AL00001                                        0165 00327000
         BALR  @14,@15                                             0165 00328000
*       RFY                                                        0166 00329000
*         R15 RSTD;                                                0166 00330000
*       IF R15^=0 THEN              /* BAD R.C. FROM QMGR (ISSUE MSG)*/ 00331000
         LTR   R15,R15                                             0167 00332000
         BZ    @RF00167                                            0167 00333000
*         DO;                                                      0168 00334000
*           RETCODE=R15;            /* SAVE QMGR'S RETURN CODE       */ 00335000
         ST    R15,RETCODE                                         0169 00336000
*           RFY                                                    0170 00337000
*             R15 UNRSTD;                                          0170 00338000
*           CALL QMGRERR;           /* ISSUE COMMAND SYSTEM ERROR MSG*/ 00339000
         BAL   @14,QMGRERR                                         0171 00340000
*         END;                                                     0172 00341000
*                                                                  0173 00342000
*       /*************************************************************/ 00343000
*       /*                                                           */ 00344000
*       /* NOW HAVE THE ACT, WHICH HOLDS THE TSO USER'S ACCOUNTING   */ 00345000
*       /* INFO. NOTE THAT THE GENERATE JOB CARD ROUTINE (IKJEFF08)  */ 00346000
*       /* AND INSTALLATION EXIT ROUTINES (IKJEFF09, IKJEFF10) WILL  */ 00347000
*       /* USE THIS INFORMATION.                                     */ 00348000
*       /*                                                           */ 00349000
*       /*************************************************************/ 00350000
*                                                                  0173 00351000
*       ELSE                        /* CONTINUE IF HAVE ACT ADDR IN     00352000
*                                      SWAEPA'S SWBLKPTR FIELD       */ 00353000
*HAVEACT:                           /* SWBLKID SHOULD BE 1           */ 00354000
*         DO;                                                      0173 00355000
         B     @RC00167                                            0173 00356000
@RF00167 DS    0H                                                  0173 00357000
HAVEACT  DS    0H                                                  0174 00358000
*           HTACCNTP=ADDR(HTACCNTI);/* SAVE ADDRESS OF ACCOUNTING  0174 00359000
*                                      INFORMATION                   */ 00360000
         LA    @10,HTACCNTI                                        0174 00361000
         ST    @10,HTACCNTP                                        0174 00362000
*           IF^(ACTJNFLD=0|(ACTJNFLD=1&ACNTLEN1=0)) THEN/* IF NOT  0175 00363000
*                                      TRIVIAL CASE (WITH HTACCNTL 0175 00364000
*                                      ALREADY INITIALIZED TO ZERO)  */ 00365000
         L     @10,SWEPAPTR                                        0175 00366000
         L     @10,ACTPTR(,@10)                                    0175 00367000
         CLI   ACTJNFLD(@10),0                                     0175 00368000
         BE    @RF00175                                            0175 00369000
         CLI   ACTJNFLD(@10),1                                     0175 00370000
         BNE   @RT00175                                            0175 00371000
         CLI   ACNTLEN1(@10),0                                     0175 00372000
         BE    @RF00175                                            0175 00373000
@RT00175 DS    0H                                                  0176 00374000
*             DO;                                                  0176 00375000
*               IF ACNTLEN1>MAXACNTI THEN/* IF TOO BIG ACCOUNT NUMBER*/ 00376000
         L     @10,SWEPAPTR                                        0177 00377000
         L     @10,ACTPTR(,@10)                                    0177 00378000
         CLI   ACNTLEN1(@10),142                                   0177 00379000
         BNH   @RF00177                                            0177 00380000
*                 DO;                                              0178 00381000
*                   MSGID='04A';                                   0179 00382000
         MVI   MSGID+3,C' '                                        0179 00383000
         MVC   MSGID(3),@CC02070                                   0179 00384000
*                   CALL AICALL02;  /* INTERNAL PROCEDURE WILL ISSUE    00385000
*                                      ERROR MESSAGE                 */ 00386000
         BAL   @14,AICALL02                                        0180 00387000
*                 END;                                             0181 00388000
*                                                                  0181 00389000
*               /*****************************************************/ 00390000
*               /*                                                   */ 00391000
*               /* REBUILD USER'S ACCOUNTING INFORMATION FROM FIELDS */ 00392000
*               /* IN ACT                                            */ 00393000
*               /*                                                   */ 00394000
*               /*****************************************************/ 00395000
*                                                                  0182 00396000
*               ELSE                                               0182 00397000
*                 CALL BUILDAI;     /* INTERNAL PROCEDURE TO REBUILD    00398000
*                                      ACCOUNTING INFO               */ 00399000
         B     @RC00177                                            0182 00400000
@RF00177 BAL   @14,BUILDAI                                         0182 00401000
*             END;                                                 0183 00402000
@RC00177 DS    0H                                                  0184 00403000
*         END HAVEACT;                                             0184 00404000
@RF00175 DS    0H                                                  0185 00405000
*     END HAVEJCT;                                                 0185 00406000
@RC00167 DS    0H                                                  0186 00407000
*   IF QUIT04=OFF THEN              /* IF NO ERROR SO FAR            */ 00408000
@RC00155 TM    QUIT04,B'10000000'                                  0186 00409000
         BNZ   @RF00186                                            0186 00410000
*     DO;                           /* ACCTOK DO/END GROUP           */ 00411000
*/********************************************************************/ 00412000
*/*  PARSCODE SEGMENT - CALLS PARSE TO CHECK COMMAND BUFFER          */ 00413000
*/********************************************************************/ 00414000
*                                                                  0188 00415000
*       PPLPTR=ADDR(INTFACE);       /* INITIALIZE THE INTERFACE AREA    00416000
*                                      USED FOR CALLING THE SERVICE     00417000
*                                      ROUTINES (PARSE AND DAIR)     */ 00418000
         LA    @10,INTFACE                                         0188 00419000
         ST    @10,PPLPTR                                          0188 00420000
*       PPLUPT=FIBCPPLU;                                           0189 00421000
         L     @04,FIBCPPLU(,FIBCMPTR)                             0189 00422000
         ST    @04,PPLUPT(,@10)                                    0189 00423000
*       PPLECT=FIBCPPLE;                                           0190 00424000
         L     @15,FIBCPPLE(,FIBCMPTR)                             0190 00425000
         ST    @15,PPLECT(,@10)                                    0190 00426000
*       PPLECB=ADDR(ECB);                                          0191 00427000
         LA    @14,ECB                                             0191 00428000
         ST    @14,PPLECB(,@10)                                    0191 00429000
*       PPLPCL=ADDR(IKJEFF16);      /* PTR TO PCL MODULE 16 -- THE 0192 00430000
