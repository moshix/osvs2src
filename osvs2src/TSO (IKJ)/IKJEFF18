             IKJTSMSG                                                   00302000
IKJEFF18 CSECT                                                          00303000
*   INTFPTR=R1;                     /* SET UP BASE FOR INPUT PARMS   */ 00304000
         LR    INTFPTR,R1                                          0108 00305000
*   RFY                                                            0109 00306000
*     R1 UNRSTD;                                                   0109 00307000
*   RCODE=DFRC;                     /* MAKE LOCAL, FULL WORD COPY OF    00308000
*                                      THE BASIC RETURN CODE --    0110 00309000
*                                      DYNAMIC ALLOCATION OR DAIR, 0110 00310000
*                                      WITH HIGH ORDER ZEROES        */ 00311000
         L     @05,DFRCP(,INTFPTR)                                 0110 00312000
         L     @05,DFRC(,@05)                                      0110 00313000
         ST    @05,RCODE                                           0110 00314000
*   MSGTABLE=MSGTABLE&&MSGTABLE;    /* INITIALIZE MSG WTR INTERFACE  */ 00315000
         XC    MSGTABLE(84),MSGTABLE                               0111 00316000
*   DARET=FCODE;                                                   0112 00317000
         MVC   DARET(4),FCODE                                      0112 00318000
*   IF DFWTP=YES THEN               /* IF A WTP HAS BEEN REQUESTED   */ 00319000
         L     @05,DFIDP(,INTFPTR)                                 0113 00320000
         TM    DFWTP(@05),B'10000000'                              0113 00321000
         BNO   @RF00113                                            0113 00322000
*     MTWTPSW=YES;                  /* INDICATE WTP TO IKJEFF02      */ 00323000
         OI    MTWTPSW,B'00000010'                                 0114 00324000
*   ELSE                            /* OTHERWISE USE THE DEFAULT OF  */ 00325000
*     MTPUTLSW=YES;                 /* A PUTLINE REQUEST             */ 00326000
         B     @RC00113                                            0115 00327000
@RF00113 OI    MTPUTLSW,B'01000000'                                0115 00328000
*   MTHEXSW=YES;                    /* INDICATE TO IKJEFF02 TO PRINT    00329000
*                                      RETURN CODES IN HEX           */ 00330000
@RC00113 OI    MTHEXSW,B'00010000'                                 0116 00331000
*   FLGS=CODE0000;                                                 0117 00332000
         MVC   FLGS(1),CODE0000                                    0117 00333000
*   DO I=1 TO DIM(VAR);                                            0118 00334000
         LA    I,1                                                 0118 00335000
@DL00118 DS    0H                                                  0119 00336000
*     VAR(I)=ZERO;                  /* ZERO OUT MESSAGE VARIABLE   0119 00337000
*                                      ARRAY                         */ 00338000
         LR    @05,I                                               0119 00339000
         SLA   @05,2                                               0119 00340000
         SLR   @03,@03                                             0119 00341000
         ST    @03,VAR-4(@05)                                      0119 00342000
*   END;                                                           0120 00343000
*                                                                  0120 00344000
         AL    I,@CF00033                                          0120 00345000
         C     I,@CF00089                                          0120 00346000
         BNH   @DL00118                                            0120 00347000
*/********************************************************************/ 00348000
*/*         INITIALIZE DEFAULT VALUES FOR MESSAGE INSERTS            */ 00349000
*/********************************************************************/ 00350000
*                                                                  0121 00351000
*   VAR(5)=ADDR(ERRCODE);           /* INSERT IF UNDEFINED   @ZA02550*/ 00352000
         LA    @05,ERRCODE                                         0121 00353000
         ST    @05,VAR+16                                          0121 00354000
*   VARLEN(5)=LENGTH(ERRCODE);      /*                       @ZA02550*/ 00355000
         MVI   VARLEN+16,X'04'                                     0122 00356000
*   VARHEX(5)=YES;                  /* WILL TELL IKJEFF02 TO CONVERT    00357000
*                                      R.C. TO PRINTABLE HEX @ZA02550*/ 00358000
         OI    VARHEX+16,B'10000000'                               0123 00359000
*   VAR(9)=ADDR(DADSM);                                            0124 00360000
         LA    @05,@CC00290                                        0124 00361000
         ST    @05,VAR+32                                          0124 00362000
*   VARLEN(9)=LENGTH(DADSM);                                       0125 00363000
         MVI   VARLEN+32,X'05'                                     0125 00364000
*   VAR(10)=ADDR(IKJ562);                                          0126 00365000
         LA    @05,@CC00275                                        0126 00366000
         ST    @05,VAR+36                                          0126 00367000
*   VARLEN(10)=LENGTH(IKJ562);                                     0127 00368000
         MVI   VARLEN+36,X'06'                                     0127 00369000
*   VAR(11)=ADDR(DATASET);                                         0128 00370000
         LA    @05,@CC00263                                        0128 00371000
         ST    @05,VAR+40                                          0128 00372000
*   VARLEN(11)=LENGTH(DATASET);                                    0129 00373000
         MVI   VARLEN+40,X'08'                                     0129 00374000
*   VAR(15)=ADDR(CATALOG);                                         0130 00375000
         LA    @05,@CC00256                                        0130 00376000
         ST    @05,VAR+56                                          0130 00377000
*   VARLEN(15)=LENGTH(CATALOG);                                    0131 00378000
         MVI   VARLEN+56,X'07'                                     0131 00379000
*   VAR(17)=ADDR(INVALID);                                         0132 00380000
         LA    @05,@CC00320                                        0132 00381000
         ST    @05,VAR+64                                          0132 00382000
*   VARLEN(17)=LENGTH(INVALID);                                    0133 00383000
         MVI   VARLEN+64,X'07'                                     0133 00384000
*   VAR(2)=ADDR(REQVAR);                                           0134 00385000
         LA    @05,@CC00292                                        0134 00386000
         ST    @05,VAR+4                                           0134 00387000
*   VARLEN(2)=LENGTH(REQVAR);                                      0135 00388000
         MVI   VARLEN+4,X'07'                                      0135 00389000
*   VAR(3)=ADDR(PERFORM);                                          0136 00390000
         LA    @05,@CC00294                                        0136 00391000
         ST    @05,VAR+8                                           0136 00392000
*   VARLEN(3)=LENGTH(PERFORM);                                     0137 00393000
         MVI   VARLEN+8,X'09'                                      0137 00394000
*   VAR(12)=ADDR(ATTRLIST);                                        0138 00395000
         LA    @05,@CC00286                                        0138 00396000
         ST    @05,VAR+44                                          0138 00397000
*   VARLEN(12)=LENGTH(ATTRLIST);                                   0139 00398000
         MVI   VARLEN+44,X'0E'                                     0139 00399000
*                                                                  0140 00400000
*/********************************************************************/ 00401000
*/*         IDENTIFY THE INTERFACE OF CALLER                         */ 00402000
*/********************************************************************/ 00403000
*                                                                  0140 00404000
*   IF IDNUM=ONE THEN               /* IF CALLER NUMBER IS ONE THE   */ 00405000
         L     @05,DFIDP(,INTFPTR)                                 0140 00406000
         CLI   IDNUM(@05),1                                        0140 00407000
         BE    @RT00140                                            0140 00408000
*     GO TO DAIRINIT;               /* USER HAS A DAIR ERROR         */ 00409000
*   IF IDNUM^=FIFTY&                /* IF CALLER NUMBER IS NOT A     */ 00410000
*       IDNUM^=CON51 THEN           /* VALID ID NUMBER FOR AN SVC 99 */ 00411000
         CLI   IDNUM(@05),50                                       0142 00412000
         BE    @RF00142                                            0142 00413000
         CLI   IDNUM(@05),51                                       0142 00414000
         BE    @RF00142                                            0142 00415000
*     DO;                                                          0143 00416000
*       DFRETCDE=FOUR;              /* ERROR, THEN RETURN WITH ERROR */ 00417000
