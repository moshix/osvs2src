@RF00627 DS    0H                                                  0632 02014000
@RF00626 L     @05,DCBTU                                           0632 02015000
         LTR   @05,@05                                             0632 02016000
         BZ    @RF00632                                            0632 02017000
*     DO;                           /* SAVE ITS NAME FOR POSSIBLE    */ 02018000
*       VAR(13)=ADDR(DCBTU->S99TUPAR);/* USE LATER                   */ 02019000
         LA    @15,S99TUPAR(,@05)                                  0634 02020000
         ST    @15,VAR+48                                          0634 02021000
*       VARLEN(13)=DCBTU->S99TULNG;                                0635 02022000
         MVC   VARLEN+48(1),S99TULNG+1(@05)                        0635 02023000
*       ATTRFLG=YES;                                               0636 02024000
         OI    ATTRFLG,B'00000100'                                 0636 02025000
*     END;                                                         0637 02026000
*   END;                            /* END OF VARINIT PROC           */ 02027000
@EL00003 DS    0H                                                  0638 02028000
@EF00003 DS    0H                                                  0638 02029000
@ER00003 L     @14,12(,@13)                                        0638 02030000
         BR    @14                                                 0638 02031000
*                                                                  0639 02032000
*/********************************************************************/ 02033000
*/*     FINDTU - THIS PROCEDURE LOCATES THE TEXT UNITS NEEDED FOR    */ 02034000
*/*              USE AS MESSAGE VARIABLES OR THOSE TEXT UNITS        */ 02035000
*/*              NECESSARY TO CHECK THE STATUS OF A GIVEN REQUEST.   */ 02036000
*/*              THE TUKEY TABLE CONTAINS ALL THE TEXT UNIT KEYS     */ 02037000
*/*              NEEDED BY IKJEFF18, HOWEVER ONLY CERTAIN KEYS MAY   */ 02038000
*/*              BE NEEDED DEPENDING ON THE DYNAMIC ALLOCATION VERB  */ 02039000
*/*              OPERATION CODE.  THE TUSELECT TABLE IS USED TO      */ 02040000
*/*              CONTROL THE SEARCH MECHANISM TO ONLY LOCATE         */ 02041000
*/*              PARTICULAR TEXT UNITS ACCORDING TO THE DYNAMIC      */ 02042000
*/*              ALLOCATION VERB OPERATION CODE (ALSO THIS ALLOWS    */ 02043000
*/*              EASE IN UPDATING WHEN MORE KEYS ARE REQUIRED BY     */ 02044000
*/*              IKJEFF18).  INDEX VARIABLE K IS SET TO THE PROPER   */ 02045000
*/*              SELECTION SEQUENCE IN THE TUSELECT TABLE UPON ENTRY */ 02046000
*/*              ACCORDING TO THE DYNAMIC ALLOCATION VERB OPERATION  */ 02047000
*/*              CODE.  EACH SELECTION SEQUENCE ENDS IN ZERO WHICH   */ 02048000
*/*              TERMINATES FINDTU PROC.  IF A TEXT UNIT IS          */ 02049000
*/*              IDENTIFIED AS ONE REQUIRED BY IKJEFF18, THE TEXT    */ 02050000
*/*              UNIT POINTER IS SAVED IN THE CORRESPONDING SLOT IN  */ 02051000
*/*              THE TUTAB TABLE THEN J IS SET TO ZERO.              */ 02052000
*/*       OUTPUT - THE TUTAB CONTAINS THE ADDRESSES OF THOSE TEXT    */ 02053000
*/*                UNITS NEEDED FOR USE AS MESSAGE VARIABLES OR      */ 02054000
*/*                THOSE TEXT UNITS NECESSARY TO DETERMINE THE       */ 02055000
*/*                STATUS OF A DYNAMIC ALLOCATION REQUEST.           */ 02056000
*/********************************************************************/ 02057000
*                                                                  0639 02058000
*FINDTU:                                                           0639 02059000
*   PROC OPTIONS(SAVE(14));                                        0639 02060000
FINDTU   ST    @14,12(,@13)                                        0639 02061000
*   CYCLE=ONE;                      /* VARIABLE USED FOR LOOP CONTROL*/ 02062000
         LA    @03,1                                               0640 02063000
         LR    CYCLE,@03                                           0640 02064000
*   I=ONE;                          /* INITIALIZE I TO ALWAYS GET  0641 02065000
*                                      FIRST TEXT UNIT POINTER       */ 02066000
         LR    I,@03                                               0641 02067000
*   SAVEINDX=K;                     /* SAVE PRESET SELECTION SEQUENCE   02068000
*                                      NUMBER                        */ 02069000
         LR    SAVEINDX,K                                          0642 02070000
*   RFY                                                            0643 02071000
*     S99TUNIT BASED(S99TUPTR(I));  /* SET BASE FOR THE TEXT UNITS   */ 02072000
*   DO WHILE(CYCLE=1);                                             0644 02073000
         B     @DE00644                                            0644 02074000
@DL00644 DS    0H                                                  0645 02075000
*     IF(S99TUPTR(I)&AND3)^=ZERO THEN/* MAKE SURE THE TEXT           */ 02076000
         LR    @03,I                                               0645 02077000
         SLA   @03,2                                               0645 02078000
         L     @15,S99RBPTR                                        0645 02079000
         L     @01,S99TXTPP(,@15)                                  0645 02080000
         AL    @01,@CF01933                                        0645 02081000
         L     @03,S99TUPTR(@03,@01)                               0645 02082000
         LR    @14,@03                                             0645 02083000
         N     @14,@CF00179                                        0645 02084000
         LTR   @14,@14                                             0645 02085000
         BZ    @RF00645                                            0645 02086000
*       DO;                         /* UNIT POINTER IS VALID         */ 02087000
*         IF BADTUFLG=YES THEN      /* IF THE ERROR IS AN INVALID    */ 02088000
         TM    BADTUFLG,B'01000000'                                0647 02089000
         BNO   @RF00647                                            0647 02090000
*           IF S99TUKEY=S99INFO THEN/* PARAMETER LIST CHECK          */ 02091000
         CLC   S99TUKEY(2,@03),S99INFO(@15)                        0648 02092000
         BNE   @RF00648                                            0648 02093000
*             DO;                   /* CURRENT TEXT UNIT KEY AGAINST    02094000
*                                      THE TEXT UNIT KEY NUMBER      */ 02095000
*               VAR(20)=ADDR(S99TUKEY);/* FOUND BY DYNAMIC ALLO-     */ 02096000
         ST    @03,VAR+76                                          0650 02097000
*               VARLEN(20)=LENGTH(S99TUKEY);/* CATION TO BE IN       */ 02098000
         MVI   VARLEN+76,X'02'                                     0651 02099000
*               VARHEX(20)=YES;     /* ERROR-WHEN THEY ARE THE SAME  */ 02100000
         OI    VARHEX+76,B'10000000'                               0652 02101000
*               RETURN;             /* RETURN BECAUSE ANY REMAINING     02102000
*                                      TEXT UNITS WERE NOT SYNTAX  0653 02103000
*                                      CHECKED BY DYNAMIC ALLOCATION */ 02104000
@EL00004 DS    0H                                                  0653 02105000
@EF00004 DS    0H                                                  0653 02106000
@ER00004 L     @14,12(,@13)                                        0653 02107000
         BR    @14                                                 0653 02108000
*             END;                                                 0654 02109000
*         J=TUSELECT(K);            /* J IS USED TO INDEX INTO THE   */ 02110000
@RF00648 DS    0H                                                  0655 02111000
@RF00647 SLR   J,J                                                 0655 02112000
         IC    J,TUSELECT-1(K)                                     0655 02113000
*         DO WHILE(J^=0);           /* TABLE OF KEY NUMBERS (THOSE 0656 02114000
*                                      KEYS THAT MAY BE USED AS    0656 02115000
*                                      MESSAGE VARIABLES OR THOSE  0656 02116000
*                                      NEEDED FOR CHECKING THE STATUS   02117000
*                                      OF A DYNAMIC ALLOC REQUEST) 0656 02118000
*                                      WHEN J GOES TO ZERO NO OTHER     02119000
*                                      KEYS ARE REQUIRED             */ 02120000
         B     @DE00656                                            0656 02121000
@DL00656 DS    0H                                                  0657 02122000
*           IF S99TUKEY=TUKEY(J) THEN/* CHECK FOR A REQUIRED         */ 02123000
         LR    @15,I                                               0657 02124000
         SLA   @15,2                                               0657 02125000
         L     @01,S99RBPTR                                        0657 02126000
         L     @01,S99TXTPP(,@01)                                  0657 02127000
         AL    @01,@CF01933                                        0657 02128000
         L     @15,S99TUPTR(@15,@01)                               0657 02129000
         LR    @14,J                                               0657 02130000
         ALR   @14,@14                                             0657 02131000
         LA    @01,TUKEY-2(@14)                                    0657 02132000
         CLC   S99TUKEY(2,@15),0(@01)                              0657 02133000
         BNE   @RF00657                                            0657 02134000
*             DO;                   /* KEY NUMBER                    */ 02135000
*               TUTAB(J)=S99TUPTR(I);                              0659 02136000
         LR    @14,J                                               0659 02137000
         SLA   @14,2                                               0659 02138000
         ST    @15,TUTAB-4(@14)                                    0659 02139000
*               J=ZERO;                                            0660 02140000
         SLR   J,J                                                 0660 02141000
*             END;                                                 0661 02142000
*           ELSE                    /* IF NOT A REQUIRED KEY THEN    */ 02143000
*             DO;                   /* CHECK NEXT TEXT UNIT          */ 02144000
         B     @RC00657                                            0662 02145000
@RF00657 DS    0H                                                  0663 02146000
*               K=ONE+K;                                           0663 02147000
         AL    K,@CF00033                                          0663 02148000
*               J=TUSELECT(K);                                     0664 02149000
         SLR   J,J                                                 0664 02150000
         IC    J,TUSELECT-1(K)                                     0664 02151000
*             END;                                                 0665 02152000
*         END;                                                     0666 02153000
@RC00657 DS    0H                                                  0666 02154000
@DE00656 LTR   J,J                                                 0666 02155000
         BNZ   @DL00656                                            0666 02156000
*       END;                                                       0667 02157000
*     IF S99TUPLN(I)=YES THEN       /* IF THIS IS THE LAST TEXT UNIT */ 02158000
@RF00645 LR    @05,I                                               0668 02159000
         SLA   @05,2                                               0668 02160000
         L     @03,S99RBPTR                                        0668 02161000
         L     @03,S99TXTPP(,@03)                                  0668 02162000
         ALR   @03,@05                                             0668 02163000
         AL    @03,@CF01933                                        0668 02164000
         TM    S99TUPLN(@03),B'10000000'                           0668 02165000
         BNO   @RF00668                                            0668 02166000
*       CYCLE=ZERO;                 /* THEN SET LOOP CONTROL TO      */ 02167000
         SLR   CYCLE,CYCLE                                         0669 02168000
*     I=ONE+I;                      /* EXIT FROM FINDTU              */ 02169000
@RF00668 AL    I,@CF00033                                          0670 02170000
*     K=SAVEINDX;                                                  0671 02171000
         LR    K,SAVEINDX                                          0671 02172000
*   END;                                                           0672 02173000
@DE00644 C     CYCLE,@CF00033                                      0672 02174000
         BE    @DL00644                                            0672 02175000
*   END;                            /* END OF FINDTU PROC            */ 02176000
         B     @EL00004                                            0673 02177000
*                                                                  0674 02178000
*/********************************************************************/ 02179000
*/*     INITALLO - THIS PROCEDURE IS USED TO INITIALIZE MESSAGE      */ 02180000
*/*                VARIABLE THREE TO 'ALLOCATED'                     */ 02181000
*/********************************************************************/ 02182000
*                                                                  0674 02183000
*INITALLO:                                                         0674 02184000
*   PROC OPTIONS(SAVE(14));                                        0674 02185000
INITALLO ST    @14,12(,@13)                                        0674 02186000
*   VAR(3)=ADDR(ALLOCTD);                                          0675 02187000
         LA    @07,@CC00266                                        0675 02188000
         ST    @07,VAR+8                                           0675 02189000
*   VARLEN(3)=LENGTH(ALLOCTD);                                     0676 02190000
         MVI   VARLEN+8,X'09'                                      0676 02191000
*   VAR(2)=VAR(11);                 /* 'DATA SET' IS DEFAULT FOR MOST   02192000
*                                      REQUESTS HOWEVER IF NOT IT  0677 02193000
*                                      WILL BE CHANGED WHEN THE    0677 02194000
*                                      REQUEST IS IDENTIFIED         */ 02195000
         L     @07,VAR+40                                          0677 02196000
         ST    @07,VAR+4                                           0677 02197000
*   END;                            /* END OF INITALLO INTERNAL PROC */ 02198000
@EL00005 DS    0H                                                  0678 02199000
@EF00005 DS    0H                                                  0678 02200000
@ER00005 L     @14,12(,@13)                                        0678 02201000
         BR    @14                                                 0678 02202000
*/********************************************************************/ 02203000
*/*     INITFILE - THIS PROCEDURE IS USED TO INITIALIZE MESSAGE      */ 02204000
*/*                VARIABLE ONE TO 'FILE'                            */ 02205000
*/********************************************************************/ 02206000
*                                                                  0679 02207000
*INITFILE:                                                         0679 02208000
*   PROC OPTIONS(SAVE(14));                                        0679 02209000
INITFILE ST    @14,12(,@13)                                        0679 02210000
*   VAR(1)=ADDR(FILE);                                             0680 02211000
         LA    @07,@CC00269                                        0680 02212000
         ST    @07,VAR                                             0680 02213000
*   VARLEN(1)=LENGTH(FILE);                                        0681 02214000
         MVI   VARLEN,X'04'                                        0681 02215000
*   END;                            /* END OF INITFILE INTERNAL PROC */ 02216000
@EL00006 DS    0H                                                  0682 02217000
@EF00006 DS    0H                                                  0682 02218000
@ER00006 L     @14,12(,@13)                                        0682 02219000
         BR    @14                                                 0682 02220000
*   END                             /* END OF IKJEFF18               */ 02221000
*                                                                  0683 02222000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 02223000
