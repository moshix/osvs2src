         BE    @RT00411                                            0411 01283000
*     GO TO SELECT;                 /* BYPASS ANY FURTHER PROCESSING */ 01284000
*   SAVEINDX=I;                                                    0413 01285000
         LR    SAVEINDX,I                                          0413 01286000
*   IF RCODE=ZERO THEN              /* IF RETURN CODE IS ZERO        */ 01287000
         LTR   @05,@05                                             0414 01288000
