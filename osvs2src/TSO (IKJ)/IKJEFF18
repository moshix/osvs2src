         TM    SECERR(@07),B'10000000'                             0416 01296000
         BNO   @RF00416                                            0416 01297000
*         UINFOFLG=YES;             /* SET BIT FOR TESTDAIR          */ 01298000
         OI    UINFOFLG,B'00100000'                                0417 01299000
*       ELSE                        /* OTHERWISE                     */ 01300000
*         DO;                       /* RETURN TO USER                */ 01301000
         B     @RC00416                                            0418 01302000
@RF00416 DS    0H                                                  0419 01303000
*           DFRETCDE=ZERO;          /* THERE IS NO ERROR             */ 01304000
         SLR   DFRETCDE,DFRETCDE                                   0419 01305000
*           GO TO EXIT;             /* TO PROCESS                    */ 01306000
         B     EXIT                                                0420 01307000
*         END;                                                     0421 01308000
*     END;                                                         0422 01309000
@RC00416 DS    0H                                                  0423 01310000
*   CALL INITALLO;                  /* INITIALIZE MESSAGE VARIABLE 0423 01311000
*                                      THREE TO 'ALLOCATED' AND    0423 01312000
