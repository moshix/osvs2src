         L     @09,@CV00076                                        0601 01470000
         ST    @09,MSGCSECT                                        0601 01471000
*       MTNPLMSW=ON;                /* WILL ISSUE OWN PUTLINE ERROR     01472000
*                                      MESSAGE                       */ 01473000
*       MTDOMSW=ON;                 /* IF ISSUE THIS MESSAGE, OR A 0603 01474000
*                                      LATER ERROR MESSAGE, AS A   0603 01475000
