*                                                                  0323 01081000
*STATMSG:                                                          0323 01082000
*   PROC OPTIONS(SAVE(14));         /* JUST SAVE REGISTER 14         */ 01083000
STATMSG  ST    @14,@SA00004                                        0323 01084000
*   VAR1=ADDR(JOBINSRT);            /* INITIALIZE POINTER TO JOBNAME    01085000
*                                      OR JOBNAME(JOBID) INSERT FOR     01086000
*                                      STATUS MESSAGE                */ 01087000
         LA    @05,JOBINSRT                                        0324 01088000
         STCM  @05,7,VAR1(MTPTR)                                   0324 01089000
*   IF JOBIINST=ALLBLNKS THEN       /* NO JOBID SO                   */ 01090000
         CLC   JOBIINST(8),ALLBLNKS                                0325 01091000
         BNE   @RF00325                                            0325 01092000
*     L1=LENGTH(JOBNINST);          /* JUST PUT JOBNAME IN MESSAGE   */ 01093000
         MVI   L1(MTPTR),X'08'                                     0326 01094000
*   ELSE                            /* JOBNAME(JOBID) INSERT LENGTH  */ 01095000
*     DO;                                                          0327 01096000
         B     @RC00325                                            0327 01097000
@RF00325 DS    0H                                                  0328 01098000
*       L1=LENGTH(JOBINSRT);                                       0328 01099000
         MVI   L1(MTPTR),X'12'                                     0328 01100000
*       MTJOBISW=ON;                /* TELL IKJEFF02 TO COMPRESS   0329 01101000
*                                      BLANKS OUT OF INSERT          */ 01102000
         OI    MTJOBISW(MTPTR),B'00000100'                         0329 01103000
*     END;                                                         0330 01104000
*   RFY                                                            0331 01105000
*    (R1,                                                          0331 01106000
*     R15) RSTD;                    /* DON'T USE CALL02 - THIS CODE     01107000
*                                      IS MORE EFFICIENT             */ 01108000
@RC00325 DS    0H                                                  0332 01109000
*   R1=ADDR(MSGTABLE);              /* PICK UP ADDR OF MSG PARAMETERS*/ 01110000
