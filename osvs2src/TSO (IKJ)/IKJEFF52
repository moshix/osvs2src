*   IF R15^=MSGRTOK THEN            /* CHECK FOR BAD PUTLINE R.C.    */ 01115000
         LTR   R15,R15                                             0334 01116000
         BZ    @RF00334                                            0334 01117000
*     DO;                           /* - IF BAD R.C. -               */ 01118000
*       ENDIDSW='1'B;               /* INDICATE GET OUT OF INNER   0336 01119000
*                                      (JOBID) LOOP                  */ 01120000
         OI    ENDIDSW,B'00001000'                                 0336 01121000
