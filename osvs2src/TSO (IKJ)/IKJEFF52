*   /*****************************************************************/ 00057000
*   /*                                                               */ 00058000
*   /* INITIALIZE SSOB AND SSCS C.B. FOR JOB ENTRY SUBSYSTEM         */ 00059000
*   /*                                                               */ 00060000
*   /*****************************************************************/ 00061000
*                                                                  0100 00062000
*   SSOB=SSOB&&SSOB;                /* ZERO OUT THE SSOB             */ 00063000
         XC    SSOB(20),SSOB                                       0100 00064000
*   SSOBINDV=ADDR(SSCSCORE);        /* PUT ADDR OF GOTTEN CORE FOR 0101 00065000
*                                      SSCS INTO SSOB                */ 00066000
         LA    @15,SSCSCORE                                        0101 00067000
         ST    @15,SSOBINDV                                        0101 00068000
*   HEADERLN=LENGTH(SSCS)-32*(LENGTH(SSCSARAY));/* LENGTH OF SSCS  0102 00069000
