*             CALL CALL02;          /* ISSUE THE ERROR MESSAGE       */ 00857000
         BAL   @14,CALL02                                          0275 00858000
*           END;                                                   0276 00859000
*         QUIT='1'B;                /* INDICATE ERROR EXIT           */ 00860000
@RC00250 L     @01,SWITPTR(,CSPLPTR)                               0277 00861000
         OI    QUIT(@01),B'00001000'                               0277 00862000
*       END BADRC;                                                 0278 00863000
*   END;                            /* END OF JOBNLOOP             0279 00864000
*                                                                    */ 00865000
@RC00150 LA    @15,1                                               0279 00866000
         AL    @15,T                                               0279 00867000
@DE00113 ST    @15,T                                               0279 00868000
         L     @14,NUMJOBSP(,CSPLPTR)                              0279 00869000
         C     @15,TBLCOUNT(,@14)                                  0279 00870000
         BNH   @DL00113                                            0279 00871000
@DC00113 DS    0H                                                  0280 00872000
*                                                                  0280 00873000
*   /*****************************************************************/ 00874000
*   /*                                                               */ 00875000
*   /* CLEAN-UP AND RETURN TO CALLER                                 */ 00876000
*   /*                                                               */ 00877000
*   /*****************************************************************/ 00878000
*                                                                  0280 00879000
*   FREESSCS='1'B;                  /* WILL DO FREEMAIN FOR SSCS     */ 00880000
         OI    FREESSCS,B'01000000'                                0280 00881000
*   CALL MORECORE;                  /* GO FREE SSCS                  */ 00882000
         BAL   @14,MORECORE                                        0281 00883000
*   IF QUIT='1'B THEN               /* IF ERROR WAS FOUND IN THIS  0282 00884000
*                                      MODULE                        */ 00885000
         L     @10,SWITPTR(,CSPLPTR)                               0282 00886000
         TM    QUIT(@10),B'00001000'                               0282 00887000
         BNO   @RF00282                                            0282 00888000
*     RETCODE=CMDRC12;              /* ERROR RETURN CODE             */ 00889000
         LA    RETCODE,12                                          0283 00890000
*   ELSE                            /* IF EVERYTHING WAS O.K.        */ 00891000
*     RETCODE=CMDRCOK;              /* NORMAL RETURN                 */ 00892000
         B     @RC00282                                            0284 00893000
@RF00282 SLR   RETCODE,RETCODE                                     0284 00894000
*   RETURN CODE(RETCODE);           /* RETURN TO CALLER (IKJEFF51) 0285 00895000
*                                                                    */ 00896000
@RC00282 L     @13,4(,@13)                                         0285 00897000
         L     @00,@SIZDATD                                        0285 00898000
         LR    @01,@11                                             0285 00899000
         FREEMAIN R,LV=(0),A=(1)                                        00900000
         LR    @15,@02                                             0285 00901000
         L     @14,12(,@13)                                        0285 00902000
         LM    @00,@12,20(@13)                                     0285 00903000
         BR    @14                                                 0285 00904000
*                                                                  0286 00905000
*/********************************************************************/ 00906000
*/*                                                                  */ 00907000
*/*  CALL02 INTERNAL PROCEDURE                                       */ 00908000
*/*                                                                  */ 00909000
*/*  - POINTS REGISTER 1 TO MSGTABLE PARAMETER LIST AND CALLS        */ 00910000
*/*    IKJEFF02 MESSAGE ISSUER SERVICE ROUTINE                       */ 00911000
*/*                                                                  */ 00912000
*/********************************************************************/ 00913000
*                                                                  0286 00914000
*CALL02:                                                           0286 00915000
*   PROC;                                                          0286 00916000
@EL00001 L     @13,4(,@13)                                         0286 00917000
@EF00001 L     @00,@SIZDATD                                        0286 00918000
         LR    @01,@11                                             0286 00919000
         FREEMAIN R,LV=(0),A=(1)                                        00920000
@ER00001 LM    @14,@12,12(@13)                                     0286 00921000
         BR    @14                                                 0286 00922000
@PB00001 DS    0H                                                  0286 00923000
CALL02   STM   @14,@12,@SA00002                                    0286 00924000
*   RFY                                                            0287 00925000
*    (R1,                                                          0287 00926000
*     R15) RSTD;                                                   0287 00927000
*   R1=ADDR(MSGTABLE);                                             0288 00928000
         LR    R1,MTPTR                                            0288 00929000
*   CALL IKJEFF02;                  /* ISSUE THE MESSAGE             */ 00930000
         L     @15,MSGRTNPT(,CSPLPTR)                              0289 00931000
         BALR  @14,@15                                             0289 00932000
*   IF R15^=MSGRTOK THEN            /* IF COULDN'T ISSUE THE MESSAGE,   00933000
*                                      STOP PROCESSING AND REMEMBER     00934000
*                                      TO FLUSH CLIST STACK          */ 00935000
         LTR   R15,R15                                             0290 00936000
         BZ    @RF00290                                            0290 00937000
*     QUIT=ON;                                                     0291 00938000
