*                                      BEING REQUESTED               */ 00080000
         MVC   SSOBFUNC(2),@CH00108                                0105 00081000
*   SSCSLEN=LENGTH(SSCS);           /* SAVE SSCS LENGTH              */ 00082000
         MVC   SSCSLEN(2,@15),@CH00969                             0106 00083000
*   CORELN=ZEROFLD;                 /* INITIALIZE THE AMOUNT OF    0107 00084000
*                                      EXPLICITLY GOTTEN CORE (FOR 0107 00085000
