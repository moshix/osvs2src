*   END CALL02;                                                    0293 00944000
@EL00002 DS    0H                                                  0293 00945000
@EF00002 DS    0H                                                  0293 00946000
@ER00002 LM    @14,@12,@SA00002                                    0293 00947000
         BR    @14                                                 0293 00948000
*                                                                  0294 00949000
*/********************************************************************/ 00950000
*/*                                                                  */ 00951000
*/* MORECORE INTERNAL PROCEDURE                                      */ 00952000
*/*                                                                  */ 00953000
*/* - GETS LARGER SSCS C.B. WHEN SUBSYSTEM NEEDS LARGER SSCSARAY     */ 00954000
*/*   FOR ITS REPLY                                                  */ 00955000
*/* - ALSO USE TO FREE SSCS EXTENSION                                */ 00956000
*/*                                                                  */ 00957000
*/********************************************************************/ 00958000
*                                                                  0294 00959000
*MORECORE:                          /* INTERNAL PROCEDURE            */ 00960000
*   PROC OPTIONS(SAVE(14));         /* IN ORDER TO GET MORE CORE FOR    00961000
*                                      SSOB                          */ 00962000
MORECORE ST    @14,@SA00003                                        0294 00963000
*   IF FREESSCS='0'B THEN           /* IF GOING TO GET MORE CORE     */ 00964000
         TM    FREESSCS,B'01000000'                                0295 00965000
         BNZ   @RF00295                                            0295 00966000
*     DO;                           /* THEN SAVE INFO FROM OLD SSCS  */ 00967000
*       HOLDDIMR=SSCSDIMR;          /* ARRAY SIZE NEEDED BY SUBSYSTEM*/ 00968000
         L     @15,SSOBINDV                                        0297 00969000
         LH    @14,SSCSDIMR(,@15)                                  0297 00970000
         ST    @14,HOLDDIMR                                        0297 00971000
*       HOLDSHDR=SSCS(1:HEADERLN);  /* SAVE HEADER SECTION OF SSCS   */ 00972000
         MVI   HOLDSHDR+1,C' '                                     0298 00973000
         MVC   HOLDSHDR+2(22),HOLDSHDR+1                           0298 00974000
         LR    @14,HEADERLN                                        0298 00975000
         BCTR  @14,0                                               0298 00976000
         EX    @14,@SM00982                                        0298 00977000
*     END;                                                         0299 00978000
*                                                                  0299 00979000
*   /*****************************************************************/ 00980000
*   /*                                                               */ 00981000
*   /* FREE PREVIOUS SSCS EXTENSION                                  */ 00982000
*   /*                                                               */ 00983000
*   /*****************************************************************/ 00984000
*                                                                  0300 00985000
*   RFY                                                            0300 00986000
*    (R0,                                                          0300 00987000
*     R1) RSTD;                                                    0300 00988000
@RF00295 DS    0H                                                  0301 00989000
*   IF CORELN>ZEROFLD THEN          /* DON'T FREE ORIGINAL CORE      */ 00990000
         ICM   @15,7,CORELN                                        0301 00991000
         BNP   @RF00301                                            0301 00992000
*     DO;                                                          0302 00993000
*       R1=SSOBINDV;                /* ADDRESS OF CORE TO FREE       */ 00994000
         L     R1,SSOBINDV                                         0303 00995000
*       R0=GETCORE;                 /* PICK UP AMOUNT OF CORE ALREADY   00996000
*                                      GOTTEN                        */ 00997000
         L     R0,GETCORE                                          0304 00998000
*       GEN(FREEMAIN R,LV=(0),A=(1));/* FREE PREVIOUS MORECORE       */ 00999000
         FREEMAIN R,LV=(0),A=(1)                                        01000000
*     END;                                                         0306 01001000
*   RFY                                                            0307 01002000
*    (R0,                                                          0307 01003000
*     R1) UNRSTD;                                                  0307 01004000
*                                                                  0307 01005000
@RF00301 DS    0H                                                  0308 01006000
*   /*****************************************************************/ 01007000
*   /*                                                               */ 01008000
*   /* ISSUE GETMAIN FOR NEW SSCS                                    */ 01009000
*   /*                                                               */ 01010000
*   /*****************************************************************/ 01011000
*                                                                  0308 01012000
*   IF FREESSCS='0'B THEN                                          0308 01013000
         TM    FREESSCS,B'01000000'                                0308 01014000
         BNZ   @RF00308                                            0308 01015000
*GETSCORE:                                                         0309 01016000
*     DO;                           /* DON'T CONTINUE IF JUST USE  0309 01017000
*                                      MORECORE TO FREE              */ 01018000
GETSCORE DS    0H                                                  0310 01019000
*       CORELN=LENGTH(SSCS)+HOLDDIMR/* FIGURE OUT TOTAL AMOUNT OF  0310 01020000
*                                      CORE NEEDED                   */ 01021000
*           -HOLDARSZ;              /* LENGTH(SSCS) ALSO HAS SOME  0310 01022000
*                                      ARRAY SPACE, SO SUBTRACT THAT */ 01023000
         LA    @15,536                                             0310 01024000
         AL    @15,HOLDDIMR                                        0310 01025000
         SL    @15,HOLDARSZ                                        0310 01026000
         STCM  @15,7,CORELN                                        0310 01027000
*       CORESP=ZEROFLD;             /* SUBPOOL 0                     */ 01028000
         MVI   CORESP,X'00'                                        0311 01029000
*       RFY                                                        0312 01030000
*        (R0,                                                      0312 01031000
*         R1) RSTD;                                                0312 01032000
*       R0=GETCORE;                 /* POINT TO SUBPOOL AND LENGTH 0313 01033000
*                                      FOR GETMAIN                   */ 01034000
         L     R0,GETCORE                                          0313 01035000
*       GEN(GETMAIN R,LV=(0));      /* GET THE MORE CORE             */ 01036000
         GETMAIN R,LV=(0)                                               01037000
*       SSOBINDV=R1;                /* PUT ADDRESS OF GOTTEN CORE  0315 01038000
*                                      (NEW SSCS) INTO SSOB          */ 01039000
         ST    R1,SSOBINDV                                         0315 01040000
*       RFY                                                        0316 01041000
*        (R0,                                                      0316 01042000
*         R1) UNRSTD;                                              0316 01043000
*       SSCS(1:HEADERLN)=HOLDSHDR;  /* FILL IN SSCSFLGS, SSCSULEN, 0317 01044000
*                                      SSCSJOBN, SSCSJOBI FIELDS IN     01045000
*                                      THE NEW SSCS'S HEADER         */ 01046000
         L     @15,SSOBINDV                                        0317 01047000
         LR    @14,HEADERLN                                        0317 01048000
         BCTR  @14,0                                               0317 01049000
         EX    @14,@SM00984                                        0317 01050000
*       SSCSLEN=CORELN;             /* PICK UP LENGTH OF CORE        */ 01051000
         SLR   @14,@14                                             0318 01052000
         ICM   @14,7,CORELN                                        0318 01053000
         STH   @14,SSCSLEN(,@15)                                   0318 01054000
*       SSCSDIMP=HOLDDIMR;          /* INDICATE AMOUNT OF ARRAY SPACE   01055000
*                                      BEING SENT                    */ 01056000
         L     @14,HOLDDIMR                                        0319 01057000
         STH   @14,SSCSDIMP(,@15)                                  0319 01058000
*       SSCSDIMR=ZEROFLD;           /* ZERO OUT LENGTH OF SUBSYSTEM     01059000
*                                      REPLY ARRAY                   */ 01060000
         SLR   @14,@14                                             0320 01061000
         STH   @14,SSCSDIMR(,@15)                                  0320 01062000
*     END GETSCORE;                                                0321 01063000
*   END MORECORE;                                                  0322 01064000
@EL00003 DS    0H                                                  0322 01065000
@EF00003 DS    0H                                                  0322 01066000
@ER00003 L     @14,@SA00003                                        0322 01067000
         BR    @14                                                 0322 01068000
*                                                                  0323 01069000
*/********************************************************************/ 01070000
*/*                                                                  */ 01071000
*/*  STATMSG INTERNAL PROCEDURE                                      */ 01072000
*/*                                                                  */ 01073000
*/*  - SETS UP JOBNAME OR JOBNAME(JOBID) INSERT FOR THE STATUS       */ 01074000
*/*   MESSAGE, CALLS THE MESSAGE ISSUER ROUTINE, AND CHECKS ITS      */ 01075000
*/*   RETURN CODE.                                                   */ 01076000
*/*  - NOTE JOBNINST, JOBIINST, LFTPAREN, RTPAREN, AND VAR1 ARE      */ 01077000
*/*   PREVIOUSLY SET.                                                */ 01078000
*/*                                                                  */ 01079000
