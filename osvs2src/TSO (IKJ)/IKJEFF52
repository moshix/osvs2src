         STH   @14,SSCSDIMP(,@15)                                  0109 00096000
*   SSOBID='SSOB';                  /* SSOB CONTAINS ITS OWN NAME FOR   00097000
*                                      DEBUGGING                     */ 00098000
         MVC   SSOBID(4),@CC00928                                  0110 00099000
*   SSOBPTR=ADDR(SSOB);                                            0111 00100000
