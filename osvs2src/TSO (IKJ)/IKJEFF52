         TITLE 'IKJEFF52 - STATUS COMMAND INTERFACE TO SUBSYSTEM       *00001000
                        '                                               00002000
IKJEFF52 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKJEFF52  74.093'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN R,LV=(0)                                               00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @11,8(,@13)                                         0001 00020000
         LR    @13,@11                                             0001 00021000
         MVC   @PC00001(8),0(@01)                                  0001 00022000
DUMMY    DS    0H                                                  0003 00023000
*                                                                    */ 00024000
*                                                                  0093 00025000
*   /*****************************************************************/ 00026000
*   /*                                                               */ 00027000
*   /* IKJEFF52 INITIALIZATION                                       */ 00028000
*   /*                                                               */ 00029000
*   /*****************************************************************/ 00030000
*                                                                  0093 00031000
*   FIBCMPTR=ADDR(FIBPARML);        /* LOCAL POINTER TO INPUT      0093 00032000
*                                      FIBCPARM                      */ 00033000
         L     FIBCMPTR,@PC00001                                   0093 00034000
*   CSPLPTR=ADDR(CSPARML);          /* LOCAL POINTER TO CANCEL/STATUS   00035000
*                                      PARMLIST - INSTEAD OF INPUT 0094 00036000
*                                      PTR                           */ 00037000
         L     CSPLPTR,@PC00001+4                                  0094 00038000
*   MTPTR=MSGPTR;                   /* USE LOCAL POINTER TO MSGTABLE    00039000
*                                      FOR MORE EFFICIENT CODE       */ 00040000
         L     MTPTR,MSGPTR(,CSPLPTR)                              0095 00041000
*   RESPECIFY                                                      0096 00042000
*    (MSGTABLE) BASED(MTPTR);                                      0096 00043000
