         TITLE 'IKJEFF57 - CANCEL INITIALIZATION                       *00001000
                        '                                               00002000
IKJEFF57 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKJEFF57  74.008'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @12,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@12                                         0001 00014000
         L     @00,@SIZDATD                                        0001 00015000
         GETMAIN R,LV=(0)                                               00016000
         LR    @11,@01                                             0001 00017000
         USING @DATD,@11                                           0001 00018000
         ST    @13,@SA00001+4                                      0001 00019000
         LM    @00,@01,20(@13)                                     0001 00020000
         ST    @11,8(,@13)                                         0001 00021000
         LR    @13,@11                                             0001 00022000
         MVC   @PC00001(4),0(@01)                                  0001 00023000
DUMMY    DS    0H                                                  0003 00024000
*                                                                    */ 00025000
*                                                                  0065 00026000
*   /*****************************************************************/ 00027000
*   /*                                                               */ 00028000
*   /* INITIALIZE LOCAL POINTERS AND SWITCHES                        */ 00029000
*   /*                                                               */ 00030000
*   /*****************************************************************/ 00031000
*                                                                  0065 00032000
*   FIBCMPTR=ADDR(FIBPARML);        /* USE LOCAL POINTER TO FIBCPARM    00033000
*                                      FOR EFFICIENCY                */ 00034000
         L     FIBCMPTR,@PC00001                                   0065 00035000
*   SWITCHES=0;                     /* ZERO SWITCH PARAMETERS        */ 00036000
         MVI   SWITCHES,X'00'                                      0066 00037000
*   CANCELSW='1'B;                  /* INDICATE CANCEL COMMAND       */ 00038000
*                                                                  0067 00039000
         OI    CANCELSW,B'10000000'                                0067 00040000
*   /*****************************************************************/ 00041000
*   /*                                                               */ 00042000
*   /* INITIALIZE PARSE INTERFACE LIST                               */ 00043000
*   /*                                                               */ 00044000
*   /*****************************************************************/ 00045000
*                                                                  0068 00046000
*   PPLPTR=ADDR(PPLLN);             /* OBTAIN STORAGE FOR PPL        */ 00047000
         LA    PPLPTR,PPLLN                                        0068 00048000
*   PPLPCL=ADDR(IKJEFF4A);          /* POINT TO PCL (DESCRIBES     0069 00049000
*                                      COMMAND SYNTAX TO PARSE)      */ 00050000
         L     @10,@CV00823                                        0069 00051000
         ST    @10,PPLPCL(,PPLPTR)                                 0069 00052000
*   PPLUPT=FIBCPPLU;                /* PICK UP UPT POINTER           */ 00053000
         L     @10,FIBCPPLU(,FIBCMPTR)                             0070 00054000
         ST    @10,PPLUPT(,PPLPTR)                                 0070 00055000
*   PPLECT=FIBCPPLE;                /* PICK UP ECT POINTER           */ 00056000
         L     @10,FIBCPPLE(,FIBCMPTR)                             0071 00057000
         ST    @10,PPLECT(,PPLPTR)                                 0071 00058000
*   CPECB=0;                        /* ZERO OUT COMMUNICATIONS ECB   */ 00059000
         SLR   @10,@10                                             0072 00060000
         ST    @10,CPECB                                           0072 00061000
*   PERMPDLP=0;                     /* WILL CONTAIN POINTER TO PDE -    00062000
*                                      INIT TO ZERO                  */ 00063000
         ST    @10,PERMPDLP                                        0073 00064000
*   PPLECB=ADDR(CPECB);             /* PICK UP ADDRESS OF          0074 00065000
*                                      COMMUNICATIONS ECB            */ 00066000
         LA    @10,CPECB                                           0074 00067000
         ST    @10,PPLECB(,PPLPTR)                                 0074 00068000
*   PPLANS=ADDR(PERMPDLP);          /* WITHIN THIS AREA PARSE WILL 0075 00069000
*                                      PLACE A POINTER TO PDL        */ 00070000
         LA    @07,PERMPDLP                                        0075 00071000
         ST    @07,PPLANS(,PPLPTR)                                 0075 00072000
*   PPLCBUF=FIBCPPLC;               /* POINT TO INPUT COMMAND BUFFER */ 00073000
         L     @07,FIBCPPLC(,FIBCMPTR)                             0076 00074000
         ST    @07,PPLCBUF(,PPLPTR)                                0076 00075000
*   MSGPTR=ADDR(MSGTABLE);          /* SAVE ADDR OF MSG PARMLIST IN     00076000
*                                      CANCEL PARMLIST               */ 00077000
         LA    @07,MSGTABLE                                        0077 00078000
         ST    @07,MSGPTR                                          0077 00079000
*   WORKAREA=MSGPTR;                /* PASS MESSAGE PARMLIST TO    0078 00080000
*                                      VALIDITY CHECK ROUTINE        */ 00081000
*                                                                  0078 00082000
         ST    @07,WORKAREA                                        0078 00083000
*   /*****************************************************************/ 00084000
*   /*                                                               */ 00085000
*   /* INITIALIZE MESSAGE PARAMETER LIST -- SO THE VALIDITY CHECK    */ 00086000
*   /* ROUTINE CAN USE IT TO INTERFACE TO THE MESSAGE ROUTINE        */ 00087000
*   /*                                                               */ 00088000
*   /*****************************************************************/ 00089000
*                                                                  0079 00090000
*   MSGTABLE=MSGTABLE&&MSGTABLE;    /* ZERO MESSAGE AREA             */ 00091000
         XC    MSGTABLE(64),MSGTABLE                               0079 00092000
*   LISTPTR=ADDR(MSGCSECT);         /* GET A POINTER TO MESSAGE    0080 00093000
*                                      SECTION IN MSGTABLE           */ 00094000
         LA    @07,MSGCSECT                                        0080 00095000
         ST    @07,LISTPTR                                         0080 00096000
*   MTCPPL=ADDR(FIBCPPLC);          /* PICK UP POINTER TO CPPL       */ 00097000
         LA    @07,FIBCPPLC(,FIBCMPTR)                             0081 00098000
         ST    @07,MTCPPL                                          0081 00099000
*   ECBPTR=ADDR(CPECB);             /* PICK UP ADDRESS OF          0082 00100000
*                                      COMMUNICATIONS ECB            */ 00101000
         ST    @10,ECBPTR                                          0082 00102000
*   MTPUTLSW='1'B;                  /* INDICATE WANT PUTLINE - NO  0083 00103000
*                                      CANCEL MESSAGES USE PROMPTING    00104000
*                                      (PUTGET) OTHER THAN IKJ56208A    00105000
*                                      IN IKJEFF51                   */ 00106000
         OI    MTPUTLSW,B'01000000'                                0083 00107000
*   RFY                                                            0084 00108000
*     R0 RSTD;                                                     0084 00109000
*   GEN(LOAD EP=IKJEFF02);          /* LOAD THE MESSAGE ROUTINE      */ 00110000
         LOAD EP=IKJEFF02                                               00111000
*   MSGRTN=R0;                      /* PICK UP ADDRESS OF MESSAGE  0086 00112000
*                                      ROUTINE                       */ 00113000
         ST    R0,MSGRTN                                           0086 00114000
*   RFY                                                            0087 00115000
*     R0 UNRSTD;                                                   0087 00116000
*   MSGRTNPT=MSGRTN;                /* ALSO PUT ADDRESS OF MSG RTN IN   00117000
*                                      CANCEL PARAMETER LIST         */ 00118000
         L     @10,MSGRTN                                          0088 00119000
         ST    @10,MSGRTNPT                                        0088 00120000
*   MTHIGH='1'B;                    /* STANDARD LINKAGE CONVENTION   */ 00121000
         OI    MTHIGH,B'10000000'                                  0089 00122000
*   PPLUWA=ADDR(WORKAREA);          /* PICK UP ADDRESS OF WORK AREA  */ 00123000
*                                                                  0090 00124000
         LA    @10,WORKAREA                                        0090 00125000
         ST    @10,PPLUWA(,PPLPTR)                                 0090 00126000
*   /*****************************************************************/ 00127000
*   /*                                                               */ 00128000
*   /* CALL PARSE (WILL USE IKJEFF49 VALIDITY CHECK ROUTINE)         */ 00129000
*   /*                                                               */ 00130000
*   /*****************************************************************/ 00131000
*                                                                  0091 00132000
*SETKEY0:                                                          0091 00133000
*   GEN(MODESET EXTKEY=SUPR,RELATED=(KEY0,IKJEFF57(SETKEY1)));/* SEE    00134000
*                                      SUBPOOL NOTES                 */ 00135000
SETKEY0  MODESET EXTKEY=SUPR,RELATED=(KEY0,IKJEFF57(SETKEY1))           00136000
*   RFY                                                            0092 00137000
*    (R0,                                                          0092 00138000
*     R1,                                                          0092 00139000
*     R15) RSTD;                                                   0092 00140000
*   R1=PPLPTR;                      /* PLACE POINTER TO PARSE      0093 00141000
*                                      PARAMETER LIST IN REGISTER ONE*/ 00142000
         LR    R1,PPLPTR                                           0093 00143000
*   DO;                             /* CALLTSSR EP(IKJPARS)          */ 00144000
*     RESPECIFY                                                    0095 00145000
*       GPR01P RSTD;                                               0095 00146000
*     IF CVTPARS=0 THEN                                            0096 00147000
         L     @14,CVTPTR                                          0096 00148000
         L     @14,CVTPARS(,@14)                                   0096 00149000
         LTR   @14,@14                                             0096 00150000
         BNZ   @RF00096                                            0096 00151000
*       GEN(LINK EP=IKJPARS);                                      0097 00152000
         LINK EP=IKJPARS                                                00153000
*     ELSE                                                         0098 00154000
*       CALL CVTPARS;                                              0098 00155000
         B     @RC00096                                            0098 00156000
@RF00096 L     @10,CVTPTR                                          0098 00157000
         L     @15,CVTPARS(,@10)                                   0098 00158000
         BALR  @14,@15                                             0098 00159000
*     RESPECIFY                                                    0099 00160000
*       GPR01P UNRSTD;                                             0099 00161000
@RC00096 DS    0H                                                  0100 00162000
*   END;                                                           0100 00163000
*   RETCODE=R15;                    /* SAVE RETURN CODE              */ 00164000
         LR    RETCODE,R15                                         0101 00165000
*SETKEY1:                                                          0102 00166000
*   GEN(MODESET EXTKEY=SCHED,RELATED=(KEY1,IKJEFF57(SETKEY0)));/* SEE   00167000
*                                      SUBPOOL NOTES                 */ 00168000
SETKEY1  MODESET EXTKEY=SCHED,RELATED=(KEY1,IKJEFF57(SETKEY0))          00169000
*   GEN(LOAD EP=IKJEFF55);          /* LOAD THE MESSAGE CSECT FOR THE   00170000
*                                      REST OF CANCEL (IKJEFF49 HAD     00171000
*                                      ITS OWN MESSAGE CSECT)        */ 00172000
         LOAD EP=IKJEFF55                                               00173000
*   MSGCSECT=R0;                    /* PICK UP ADDRESS OF MESSAGES 0104 00174000
*                                      BECAUSE VALIDITY CHECK EXIT 0104 00175000
*                                      USED A DIFFERENT MESSAGE CSECT*/ 00176000
         ST    R0,MSGCSECT                                         0104 00177000
*   RFY                                                            0105 00178000
*    (R0,                                                          0105 00179000
*     R1,                                                          0105 00180000
*     R15) UNRSTD;                                                 0105 00181000
*   MTKEY1SW='1'B;                  /* INDICATE IN KEY 1 (SCHEDULER)    00182000
*                                      - SEE SUBPOOL NOTES         0106 00183000
*                                                                    */ 00184000
         OI    MTKEY1SW,B'00001000'                                0106 00185000
*                                                                  0107 00186000
*   /*****************************************************************/ 00187000
*   /*                                                               */ 00188000
*   /* PROCESS CANCEL COMMAND OPERANDS, FORMATTED IN PARSE PDE'S     */ 00189000
*   /*                                                               */ 00190000
*   /*****************************************************************/ 00191000
*                                                                  0107 00192000
*   IF RETCODE=PARSER0 THEN         /* NO ERRORS, CONTINUE           */ 00193000
         LTR   RETCODE,RETCODE                                     0107 00194000
         BNZ   @RF00107                                            0107 00195000
*PARSEOK:                           /* LABEL FOR DO/END GROUP        */ 00196000
*     DO;                                                          0108 00197000
PARSEOK  DS    0H                                                  0109 00198000
*       PDEPTR=PERMPDLP+PDLHDRLN;   /* UPDATE BEYOND HEADER        0109 00199000
*                                      INFORMATION TO PICK UP POINTER   00200000
*                                      TO PDE CHAINS (FOR PARAMETERS    00201000
*                                      ENTERED ON CANCEL COMMAND)    */ 00202000
*                                                                  0109 00203000
         LA    PDEPTR,8                                            0109 00204000
         AL    PDEPTR,PERMPDLP                                     0109 00205000
*       /*************************************************************/ 00206000
*       /*                                                           */ 00207000
*       /* IF PURGE KEYWORD GIVEN ON COMMAND, SET BIT ON IN PARMLIST */ 00208000
*       /* SWITCHES                                                  */ 00209000
*       /*                                                           */ 00210000
*       /*************************************************************/ 00211000
*                                                                  0110 00212000
*       IF PURGEPDE=1 THEN          /* PDE FOR PURGE/NOPURGE KEYWORDS   00213000
*                                      IS SECOND PDE                 */ 00214000
         CLC   PURGEPDE+20(2,PDEPTR),@CH00043                      0110 00215000
         BNE   @RF00110                                            0110 00216000
*         PTPURGSW=ON;                                             0111 00217000
*                                                                  0111 00218000
         OI    PTPURGSW,B'00000100'                                0111 00219000
*       /*************************************************************/ 00220000
*       /*                                                           */ 00221000
*       /* CALCULATE SPACE NEEDED FOR JOBLIST TABLE AND GET IT       */ 00222000
*       /*                                                           */ 00223000
*       /*************************************************************/ 00224000
*                                                                  0112 00225000
*       TBLCOUNT=1;                 /* INITIALIZE COUNT OF JOBNAME 0112 00226000
*                                      PDE'S                         */ 00227000
*                                                                  0112 00228000
@RF00110 MVC   TBLCOUNT(4),@CF00043                                0112 00229000
*       /*************************************************************/ 00230000
*       /*                                                           */ 00231000
*       /* LOOP THRU PDE'S, COUNTING THEM                            */ 00232000
*       /*                                                           */ 00233000
*       /*************************************************************/ 00234000
*                                                                  0113 00235000
*       DO WHILE CHAINPTR^=0;       /* COUNT PDE'S ON CHAIN          */ 00236000
         B     @DE00113                                            0113 00237000
@DL00113 DS    0H                                                  0114 00238000
*         IF PRESENT='1'B THEN      /* IF JOBID IS PRESENT           */ 00239000
         TM    PRESENT(PDEPTR),B'10000000'                         0114 00240000
         BNO   @RF00114                                            0114 00241000
*           TBLCOUNT=TBLCOUNT+1;    /* THEN UPDATE COUNT             */ 00242000
         LA    @10,1                                               0115 00243000
         AL    @10,TBLCOUNT                                        0115 00244000
         ST    @10,TBLCOUNT                                        0115 00245000
*         PDEPTR=LAST;              /* UPDATE TO NEXT PDE            */ 00246000
@RF00114 L     PDEPTR,LAST(,PDEPTR)                                0116 00247000
*         TBLCOUNT=TBLCOUNT+1;      /* UPDATE COUNT OF PDE'S         */ 00248000
         LA    @10,1                                               0117 00249000
         AL    @10,TBLCOUNT                                        0117 00250000
         ST    @10,TBLCOUNT                                        0117 00251000
*       END;                                                       0118 00252000
@DE00113 ICM   @10,7,CHAINPTR(PDEPTR)                              0118 00253000
         BNZ   @DL00113                                            0118 00254000
*       IF PRESENT='1'B THEN        /* ALSO COUNT JOBID IF IN LAST 0119 00255000
*                                      PDE                           */ 00256000
         TM    PRESENT(PDEPTR),B'10000000'                         0119 00257000
         BNO   @RF00119                                            0119 00258000
*         TBLCOUNT=TBLCOUNT+1;      /* UPDATE COUNT OF PDE'S ONCE  0120 00259000
*                                      MORE BECAUSE ABOVE METHOD WILL   00260000
*                                      ALWAYS LEAVE ONE SHORT IF LAST   00261000
*                                      PDE CONTAINS A JOBID        0120 00262000
*                                                                    */ 00263000
         LA    @10,1                                               0120 00264000
         AL    @10,TBLCOUNT                                        0120 00265000
         ST    @10,TBLCOUNT                                        0120 00266000
*                                                                  0121 00267000
*       /*************************************************************/ 00268000
*       /*                                                           */ 00269000
*       /* GET CORE FOR JOBLIST TABLE                                */ 00270000
*       /*                                                           */ 00271000
*       /*************************************************************/ 00272000
*                                                                  0121 00273000
*       RFY                                                        0121 00274000
*        (R0,                                                      0121 00275000
*         R1) RSTD;                                                0121 00276000
@RF00119 DS    0H                                                  0122 00277000
*       R0=TBLCOUNT*LENGTH(JOBLIST);/* NOW KNOW HOW MANY ITEMS WERE     00278000
*                                      ENTERED SO MULTIPLY BY NINE IN   00279000
*                                      ORDER TO HAVE EACH          0122 00280000
*                                      JOBNAME(JOBID) FIT INTO     0122 00281000
*                                      PARMLIST TABLE                */ 00282000
         L     R0,TBLCOUNT                                         0122 00283000
         MH    R0,@CH00749                                         0122 00284000
*       CTR=R0;                     /* SAVE NUMBER OF BYTES GETMAINED*/ 00285000
         LR    CTR,R0                                              0123 00286000
*       GEN(GETMAIN R,LV=(0));      /* GET CORE                      */ 00287000
         GETMAIN R,LV=(0)                                               00288000
*       JOBLISTP=R1;                /* PLACE POINTER TO GOTTEN CORE     00289000
*                                      IN JOBLISTP                   */ 00290000
         ST    R1,JOBLISTP                                         0125 00291000
*       SAVELIST=R1;                /* SAVE PTR TO GOTTEN CORE FOR 0126 00292000
*                                      FREEING AND RESTORING         */ 00293000
         LR    SAVELIST,R1                                         0126 00294000
*       RFY                                                        0127 00295000
*        (R0,                                                      0127 00296000
*         R1) UNRSTD;                                              0127 00297000
*       PDEPTR=PERMPDLP+PDLHDRLN;   /* RESET PDE POINTER TO BEGINNING   00298000
*                                      OF PDE CHAIN                  */ 00299000
*                                                                  0128 00300000
         LA    PDEPTR,8                                            0128 00301000
         AL    PDEPTR,PERMPDLP                                     0128 00302000
*       /*************************************************************/ 00303000
*       /*                                                           */ 00304000
*       /* COPY THE JOBNAME(S) AND JOBID(S) FROM THE PDE CHAIN INTO  */ 00305000
*       /* THE JOBLIST TABLE                                         */ 00306000
*       /*                                                           */ 00307000
*       /*************************************************************/ 00308000
*                                                                  0129 00309000
*       DO K=1 TO TBLCOUNT;                                        0129 00310000
         LA    K,1                                                 0129 00311000
         B     @DE00129                                            0129 00312000
@DL00129 DS    0H                                                  0130 00313000
*         JOBNMID(K)=JOBNAME(1:NAMELEN);/* PICK UP JOBNAME FROM PDE  */ 00314000
         L     @10,JOBLISTP                                        0130 00315000
         LR    @09,K                                               0130 00316000
         MH    @09,@CH00749                                        0130 00317000
         ST    @09,@TF00001                                        0130 00318000
         ALR   @09,@10                                             0130 00319000
         AL    @09,@CF00871                                        0130 00320000
         MVI   JOBNMID(@09),C' '                                   0130 00321000
         MVC   JOBNMID+1(6,@09),JOBNMID(@09)                       0130 00322000
         LH    @15,NAMELEN(,PDEPTR)                                0130 00323000
         BCTR  @15,0                                               0130 00324000
         L     @01,JOBNAMEP(,PDEPTR)                               0130 00325000
         EX    @15,@SM00872                                        0130 00326000
*         IF PRESENT='0'B THEN      /* IF NO JOBID ON CANCEL COMMAND */ 00327000
         TM    PRESENT(PDEPTR),B'10000000'                         0131 00328000
         BNZ   @RF00131                                            0131 00329000
*           LEN1(K)=IDJOBNM;        /* INITIALIZE SWITCH BYTE        */ 00330000
         AL    @10,@TF00001                                        0132 00331000
         AL    @10,@CF00874                                        0132 00332000
         MVI   LEN1(@10),X'00'                                     0132 00333000
*         ELSE                      /* JOBNAME(JOBID) ON CANCEL    0133 00334000
*                                      COMMAND                       */ 00335000
*           DO;                                                    0133 00336000
         B     @RC00131                                            0133 00337000
@RF00131 DS    0H                                                  0134 00338000
*             LEN1(K)=IDJOBID;      /* SWITCH INDICATOR THAT JOBID IS   00339000
*                                      NEXT IN LIST                  */ 00340000
         L     @10,JOBLISTP                                        0134 00341000
         LR    @09,K                                               0134 00342000
         MH    @09,@CH00749                                        0134 00343000
         ST    @09,@TF00001                                        0134 00344000
         ALR   @09,@10                                             0134 00345000
         AL    @09,@CF00874                                        0134 00346000
         MVI   LEN1(@09),X'44'                                     0134 00347000
*             JOBNMID(K+1)=JOBID(1:JOBIDLN);/* PICK UP JOBID         */ 00348000
         AL    @10,@TF00001                                        0135 00349000
         MVI   JOBNMID+1(@10),C' '                                 0135 00350000
         MVC   JOBNMID+2(6,@10),JOBNMID+1(@10)                     0135 00351000
         LH    @09,JOBIDLN(,PDEPTR)                                0135 00352000
         BCTR  @09,0                                               0135 00353000
         L     @01,JOBIDP(,PDEPTR)                                 0135 00354000
         EX    @09,@SM00875                                        0135 00355000
*             K=K+1;                                               0136 00356000
         AH    K,@CH00043                                          0136 00357000
*           END;                                                   0137 00358000
*         PDEPTR=CHAINPTR;          /* UPDATE TO NEXT PDE            */ 00359000
@RC00131 SLR   @10,@10                                             0138 00360000
         ICM   @10,7,CHAINPTR(PDEPTR)                              0138 00361000
         LR    PDEPTR,@10                                          0138 00362000
*       END;                                                       0139 00363000
         AH    K,@CH00043                                          0139 00364000
@DE00129 C     K,TBLCOUNT                                          0139 00365000
         BNH   @DL00129                                            0139 00366000
*       LEN1(TBLCOUNT)=IDLASTJB;    /* INDICATE END OF LIST          */ 00367000
         L     @10,TBLCOUNT                                        0140 00368000
         MH    @10,@CH00749                                        0140 00369000
         L     @09,JOBLISTP                                        0140 00370000
         ALR   @09,@10                                             0140 00371000
         AL    @09,@CF00874                                        0140 00372000
         MVI   LEN1(@09),X'80'                                     0140 00373000
*       GEN(IKJRLSA PERMPDLP);      /* FREE THE PDE CHAIN          0141 00374000
*                                                                    */ 00375000
         IKJRLSA PERMPDLP                                               00376000
*                                                                  0142 00377000
*       /*************************************************************/ 00378000
*       /*                                                           */ 00379000
*       /* SET UP PARAMETERS TO PASS INTO IKJEFF51                   */ 00380000
*       /*                                                           */ 00381000
*       /*************************************************************/ 00382000
*                                                                  0142 00383000
*       CSPTR1=ADDR(FIBCPARM);      /* R1 -> CSPTR1 -> FIBCPARM      */ 00384000
         ST    FIBCMPTR,CSPTR1                                     0142 00385000
*       CSPTR2=ADDR(PARMLIST);      /* PASS A POINTER TO           0143 00386000
*                                      CANCEL/STATUS PARAMETER LIST,    00387000
*                                      ALSO                          */ 00388000
         LA    @10,PARMLIST                                        0143 00389000
         ST    @10,CSPTR2                                          0143 00390000
*       CSHIGH='1'B;                /* STANDARD LINKAGE CONVENTION   */ 00391000
         OI    CSHIGH,B'10000000'                                  0144 00392000
*       JOBLISTP=SAVELIST;          /* RESTORE POINTER TO TOP OF JOB    00393000
*                                      NAME LIST                     */ 00394000
         ST    SAVELIST,JOBLISTP                                   0145 00395000
*       NUMJOBSP=ADDR(TBLCOUNT);    /* PICK UP ADDRESS OF COUNT OF 0146 00396000
*                                      NUMBER OF JOBNAMES/JOBIDS TO     00397000
*                                      PROCESS                       */ 00398000
         LA    @10,TBLCOUNT                                        0146 00399000
         ST    @10,NUMJOBSP                                        0146 00400000
*       MTINSRTS=MTINSRTS&&MTINSRTS;/* CLEAR INSERT AREA OF MSGTABLE    00401000
*                                      - IKJEFF49 MAY HAVE USED IT   */ 00402000
         XC    MTINSRTS(16),MTINSRTS                               0147 00403000
*       SWITPTR=ADDR(SWITCHES);     /* PUT ADDRESS OF SWITCHES IN  0148 00404000
*                                      PARAMETER LIST                */ 00405000
         LA    @10,SWITCHES                                        0148 00406000
         ST    @10,SWITPTR                                         0148 00407000
*       PTHIGH='1'B;                /* INDICATE END OF CANCEL/STATUS    00408000
*                                      JOB LIST                      */ 00409000
         OI    PTHIGH,B'10000000'                                  0149 00410000
*       RFY                                                        0150 00411000
*        (R0,                                                      0150 00412000
*         R1,                                                      0150 00413000
*         R15) RSTD;                                               0150 00414000
*       R1=ADDR(CSPTRS);            /* POINT TO PARAMETER LIST       */ 00415000
         LA    R1,CSPTRS                                           0151 00416000
*       GEN(LINK EP=IKJEFF51);      /* BRANCH TO INSTALLATION EXIT 0152 00417000
*                                      INTERFACE ROUTINE             */ 00418000
*                                                                  0152 00419000
         LINK EP=IKJEFF51                                               00420000
*       /*************************************************************/ 00421000
*       /*                                                           */ 00422000
*       /* CLEAN UP WHEN RETURN                                      */ 00423000
*       /*                                                           */ 00424000
*       /*************************************************************/ 00425000
*                                                                  0153 00426000
*       RETCODE=R15;                /* SAVE RETURN CODE              */ 00427000
         LR    RETCODE,R15                                         0153 00428000
*       IF SAVELIST^=0 THEN         /* IF 57 HAD GOTTEN ANY CORE     */ 00429000
