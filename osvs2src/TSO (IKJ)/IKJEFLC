         CLC   PSCBUSER(7,@10),ZEROFELD                            0158 00303000
         BE    @RF00158                                            0158 00304000
*       DO;                        /*YES, GET IT OUT OF PSCB         */ 00305000
*         MSGUSID=PSCBUSER;        /*GET USER ID                     */ 00306000
         MVC   MSGUSID(7),PSCBUSER(@10)                            0160 00307000
*         SEGWTO3=ADDR(STRTUSID);  /*SET PTR TO USER ID TXT          */ 00308000
         LA    @10,STRTUSID                                        0161 00309000
         ST    @10,SEGWTO3                                         0161 00310000
*       END;                                                       0162 00311000
*                                                                  0162 00312000
*   SEGWTO4=0;                     /* ASSUME PROC NAME UNKNOWN       */ 00313000
*                                                                  0163 00314000
@RF00158 SLR   @10,@10                                             0163 00315000
         ST    @10,SEGWTO4                                         0163 00316000
*   IF JSELCSCB^=0                 /* IF WE HAVE A CSCB AND          */ 00317000
*     THEN                                                         0164 00318000
         L     @09,LWAJSEL(,LWAPTR)                                0164 00319000
         L     @09,JSELCSCB(,@09)                                  0164 00320000
         CLR   @09,@10                                             0164 00321000
         BE    @RF00164                                            0164 00322000
*       IF JSELCSCB->CHCLS^=ZEROFELD&JSELCSCB->CHCLS(1)^=' '  /*IF 0165 00323000
*                                            PROC NAME AVAILABLE */     00324000
*         THEN                                                     0165 00325000
         CLC   CHCLS(8,@09),ZEROFELD                               0165 00326000
         BE    @RF00165                                            0165 00327000
