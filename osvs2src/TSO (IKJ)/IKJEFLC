         TITLE 'IKJEFLC, LOGON PROMPTING MONITOR                       *00001000
                        '                                               00002000
IKJEFLC  CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKJEFLC   77.235'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         LR    @03,@01                                             0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         LR    @01,@03                                             0001 00018000
         ST    @13,@SA00001+4                                      0001 00019000
         LR    @13,@11                                             0001 00020000
         XC    @ZTEMPS(@ZLEN),@ZTEMPS                                   00021000
         MVC   @PC00001(4),0(@01)                                  0001 00022000
LWASWAR  EQU   4                                                        00023000
LWAMSPE  EQU   16                                                       00024000
INITDONE EQU  4                                                         00025000
NQUSERID EQU  8                                                         00026000
DQUSERID EQU  12                                                        00027000
SCHEDULE EQU  16                                                        00028000
BCDONE   EQU  20                                                        00029000
TERMINAT EQU  24                                                        00030000
NQDQ0    EQU  0                                                         00031000
NQDQ4    EQU  4                                                         00032000
NQDQ8    EQU  8                                                         00033000
STARTP2  EQU  16                                                        00034000
INITRDY  EQU  20                                                        00035000
ENDTASK  EQU  24                                                        00036000
JSCBDISP EQU 188                                                        00037000
JSCBAOS1 EQU  72                                                        00038000
JSCBAOS2 EQU 192                                                        00039000
CSCBLEN1 EQU 176                                                        00040000
JCLSLEN0 EQU 88                                                         00041000
UNPKLEN  EQU 8                                                          00042000
