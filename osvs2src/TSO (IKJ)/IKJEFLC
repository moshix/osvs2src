*/*                      CSCBINIT                                    */ 00377000
*/* DELETE INPUT CSCB - THE MEMORY CSCB IF INITIAL LOGON OR THE      */ 00378000
*/* PREVIOUS USER FOR RELOGON/LOGOFF. CREATE NEW CSCB AND PUT ON     */ 00379000
*/* CHAIN.                                                           */ 00380000
*/*                                                                  */ 00381000
*/********************************************************************/ 00382000
*CSCBINIT:                                                         0185 00383000
*   PROCEDURE OPTIONS(NOSAVEAREA,NOSAVE);                          0185 00384000
CSCBINIT DS    0H                                                  0186 00385000
*   CSITRET=R14;                   /* SAVE RETURN POINT              */ 00386000
         ST    R14,CSITRET                                         0186 00387000
*   GEN(GETMAIN R,SP=245,LV=CSCBLEN1); /* GET CORE FOR NEW CSCB      */ 00388000
         GETMAIN R,SP=245,LV=CSCBLEN1                                   00389000
*   TEMCSCBP = GMAPTR;             /* STORE ADDR OF NEW CSCB         */ 00390000
         ST    GMAPTR,TEMCSCBP                                     0188 00391000
*   CSCBPTR = GMAPTR;              /* POINT TO NEW CSCB              */ 00392000
         LR    CSCBPTR,GMAPTR                                      0189 00393000
*   CHAIN=CHAIN && CHAIN;          /* ZERO OUT NEW CSCB              */ 00394000
         XC    CHAIN(176,CSCBPTR),CHAIN(CSCBPTR)                   0190 00395000
*   CHVCD='98'X;                   /*PUT COMMAND VERB CODE   @YM00040*/ 00396000
         MVI   CHVCD(CSCBPTR),X'98'                                0191 00397000
*                                  /* FOR LOGON IN CSCB      @YM00040*/ 00398000
*   CHSZE = 22;                    /*SET LEN OF CSCB IN DOUBLE WORDS */ 00399000
         MVI   CHSZE(CSCBPTR),X'16'                                0192 00400000
*   CHASID = ASCBASID;             /* SET ASID IN CSCB               */ 00401000
         L     @10,ASCBPTR                                         0193 00402000
         MVC   CHASID(2,CSCBPTR),ASCBASID(@10)                     0193 00403000
*   CHSWAP = '1'B;                 /* SET SWAPPABLE PGM FLAG         */ 00404000
*   CHTERM = '1'B;                 /* SET TERMINAL JOB FLAG          */ 00405000
*   CHCL = '1'B;                   /* SET CANCELABLE JOB STEP FLAG   */ 00406000
         OI    CHSWAP(CSCBPTR),B'11001000'                         0196 00407000
