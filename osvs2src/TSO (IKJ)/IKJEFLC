*        CALL CSCBINIT;            /*GET NEW CSCB                    */ 00082000
@RF00075 BAL   @14,CSCBINIT                                        0081 00083000
*      END;                                                        0082 00084000
*                                                                  0082 00085000
*   IF LWADISC='0'B                /*IF DISCONNECT WAS NOT SPECIFIED */ 00086000
*     THEN                         /* THEN                           */ 00087000
@RF00073 TM    LWADISC(LWAPTR),B'00010000'                         0083 00088000
