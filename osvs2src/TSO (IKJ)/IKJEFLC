         MVC   PROCNAME(8),CHCLS(@09)                              0167 00332000
*             SEGWTO4=ADDR(STRTPROC); /* PTR TO PROC INSERTION TEXT  */ 00333000
         LA    @10,STRTPROC                                        0168 00334000
         ST    @10,SEGWTO4                                         0168 00335000
*           END;                                                   0169 00336000
*                                                                  0169 00337000
*   LWADISC='1'B;                  /*SET CANCEL BIT                  */ 00338000
@RF00165 DS    0H                                                  0170 00339000
@RF00164 OI    LWADISC(LWAPTR),B'00010000'                         0170 00340000
*   LWCMSGNO=20;                   /*SET NUMBER OF DESIRED MESSAGE   */ 00341000
         MVC   LWCMSGNO(2),@CH00142                                0171 00342000
*                                  /*SR HAS GIVEN INDICATED RET. CODE*/ 00343000
*   RETCODE=RTNCODE;               /*GET THE RETURN CODE TO PRINT    */ 00344000
         L     RETCODE,RTNCODE                                     0172 00345000
*   GEN(CVD   RETCODE,PACKDEC);    /* CONVERT RET. CODE TO DECIMAL   */ 00346000
