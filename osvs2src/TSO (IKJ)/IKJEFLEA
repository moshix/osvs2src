*     THEN                                                         0610 01929000
CHECKNXT LR    @01,CHECKPTR                                        0610 01930000
         ALR   @01,QUOTESCN                                        0610 01931000
         BCTR  @01,0                                               0610 01932000
         CLI   ACCTNM(@01),C''''                                   0610 01933000
         BE    @RT00610                                            0610 01934000
*       GOTO INVADACT;                                             0611 01935000
*                                                                  0611 01936000
*                                                                  0611 01937000
*   QUOTESCN = QUOTESCN + 1;      /* BUMP SCAN INDEX TO NEXT       0612 01938000
*                                    CHARACTER OF ACCOUNT NUMBER     */ 01939000
*                                                                  0612 01940000
*                                                                  0612 01941000
         AH    QUOTESCN,@CH00032                                   0612 01942000
*   IF QUOTESCN ^> ACCTNMLN       /* IF MORE CHARACTERS TO BE      0613 01943000
*                                    CHECKED, GO TO CHECK NEXT ONE   */ 01944000
*     THEN                                                         0613 01945000
