*         2 PRFMLN FIXED(15);      /*LENGTH OF CHAR STRING     Y02669*/ 01996000
*                                                                  0628 01997000
*   DCL EBCDICPG CHAR(3) BASED(PRFMP);  /*PERFORMANCE GROUP    Y02669*/ 01998000
*                                  /*ENTERED IN EBCDIC         Y02669*/ 01999000
*                                                                  0630 02000000
*   GENERATE;                                                /*Y02669*/ 02001000
*                                                                  0630 02002000
      L  3,4(1)                      GET ADDR OF USERWORD      Y02669   02003000
      LM 10,12,0(3)                  SET UP ADDRESS. TO DATA   Y02669   02004000
*   RESPECIFY (VADPRMPT) RSTD;                               /*Y02669*/ 02005000
*   VADPRMPT=PARMPTR;              /* GET ADDRESS OF PARM      Y02669*/ 02006000
         LR    VADPRMPT,PARMPTR                                    0632 02007000
*                                  /* LIST PASSED BY PARSE     Y02669*/ 02008000
