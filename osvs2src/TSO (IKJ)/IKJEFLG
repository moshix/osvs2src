*        TOCHAR = LENGTH (PTPB) + LENGTH (GTPB)                    0060 00059000
*                                          /*ESTABLISH LENGTH OF   0060 00060000
*                                          INITIALIZED CONTROL BLCKS*/; 00061000
         LA    TOCHAR,20                                           0060 00062000
*        SRVCPARM(FROMCHAR:TOCHAR) = PARMBLOK                      0061 00063000
*                                          /*COPY INITIALIZED BLOCKS*/  00064000
*                                          /*INTO DYNAMIC STORAGE   */; 00065000
         LA    @07,SRVCPARM-1(FROMCHAR)                            0061 00066000
