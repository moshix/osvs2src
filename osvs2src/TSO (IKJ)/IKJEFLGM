*   IF SVEMSGNM ^= 0              /* IF RELATED MESSAGES,          0171 00393000
*                                    GO TO SEND NEXT ONE             */ 00394000
*     THEN                                                         0171 00395000
         LH    @10,SVEMSGNM                                        0171 00396000
         LTR   @10,@10                                             0171 00397000
         BNZ   @RT00171                                            0171 00398000
*       GOTO NEXTMSG;                                              0172 00399000
*                                                                  0172 00400000
*WANTDUMP:                                                         0173 00401000
*                                                                  0173 00402000
*   /*****************************************************************/ 00403000
*   /*    IF SVC DUMP IS TO BE ISSUED AS PART OF THIS TRANSACTION,   */ 00404000
*   /*    SET UP FOR AND INVOKE SVC DUMP.                            */ 00405000
*   /*****************************************************************/ 00406000
*                                                                  0173 00407000
*   IF DUMPCORE ^= '1'B           /* IF A DUMP IS NOT NEEDED, RETURN */ 00408000
*     THEN                                                         0173 00409000
WANTDUMP TM    DUMPCORE,B'10000000'                                0173 00410000
         BNO   @RT00173                                            0173 00411000
*       GOTO RTNTCLR;                                              0174 00412000
*                                                                  0174 00413000
*                                                                  0174 00414000
*   RESPECIFY  PARMREG RSTD;      /*                            M1786*/ 00415000
*   GEN(MVC SDUMPA(SDUMPSE-SDUMPS),SDUMPS); /* L-FORM SDUMP MOVE   0176 00416000
*                                                              Y02669*/ 00417000
         MVC SDUMPA(SDUMPSE-SDUMPS),SDUMPS                              00418000
*   PARMREG = ADDR(SDUMPA);         /* ADDR OF SDUMP LIST      Y02669*/ 00419000
         LA    PARMREG,SDUMPA                                      0177 00420000
*   GEN(SDUMP MF=(E,(1)));          /* CALL SVC DUMP ROUTINE.  Y02669*/ 00421000
         SDUMP MF=(E,(1))                                               00422000
*   RESPECIFY PARMREG UNRSTD;                                      0179 00423000
*   GO TO RTNTCLR;                                                 0180 00424000
         B     RTNTCLR                                             0180 00425000
*WTOMSG:                                                           0181 00426000
*   WTOTXT(1:80) = MAINSEG -> MSGTXT(1:80); /* MOVE THE MAIN SEGMENT    00427000
*                                    OF THE 'WTO' MESSAGE TO A     0181 00428000
*                                    TEMPORARY BUFFER                */ 00429000
