         TITLE 'IKJEFLGM, LOGON MESSAGE SENDER MODULE                  *00001000
                        '                                               00002000
IKJEFLGM CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKJEFLGM  78.173'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @11,8(,@13)                                         0001 00020000
         LR    @13,@11                                             0001 00021000
         XC    @ZTEMPS(@ZLEN),@ZTEMPS                                   00022000
         MVC   @PC00001(4),0(@01)                                  0001 00023000
         EJECT                                                          00024000
         EJECT                                                          00025000
         EJECT                                                          00026000
         EJECT                                                          00027000
         EJECT                                                          00028000
JSCBDISP EQU 188                                                        00029000
JSCBAOS1 EQU  72                                                        00030000
JSCBAOS2 EQU 192                                                        00031000
         EJECT                                                          00032000
         EJECT                                                          00033000
         EJECT                                                          00034000
         EJECT                                                          00035000
*                                                                  0078 00036000
*                                                                  0078 00037000
*   PARMPTR = PARMREG;            /* GET ADDRESS OF PARAMETER LIST 0079 00038000
*                                    PASSED BY CALLER                */ 00039000
*                                                                  0079 00040000
         LR    PARMPTR,PARMREG                                     0079 00041000
*   SRUPT = PTRUPT;               /* INITIALIZE UPT POINTER.         */ 00042000
         MVC   SRUPT(4),PTRUPT(PARMPTR)                            0080 00043000
*   SRECT = PTRECT;               /* INITIALIZE ECT POINTER.         */ 00044000
         MVC   SRECT(4),PTRECT(PARMPTR)                            0081 00045000
*   SRECB = PTRECB;               /* INITIALIZE CANCEL ECB POINTER.  */ 00046000
*                                                                  0082 00047000
         MVC   SRECB(4),PTRECB(PARMPTR)                            0082 00048000
*   RTNCODE = 0;                  /* INITIALIZE RETURN CODE TO ZERO  */ 00049000
*                                                                  0083 00050000
*                                                                  0083 00051000
         SLR   @10,@10                                             0083 00052000
         L     @03,BACKPTR+4(,SAVEREG)                             0083 00053000
         ST    @10,RTNCODE+16(,@03)                                0083 00054000
*   /*****************************************************************/ 00055000
*   /*      IF THE MESSAGE NUMBER IS ZERO, A GETLINE                 */ 00056000
*   /*      HAS BEEN REQUESTED.                                      */ 00057000
*   /*****************************************************************/ 00058000
*                                                                  0084 00059000
*   IF MSGNM = 0                                                   0084 00060000
*     THEN                                                         0084 00061000
         L     @03,MSGNMPTR(,PARMPTR)                              0084 00062000
         CH    @10,MSGNM(,@03)                                     0084 00063000
         BNE   @RF00084                                            0084 00064000
*       DO;                                                        0085 00065000
*         GEN(MVC   GTPBDYN(GTPBLEN),GTPBDNW);                     0086 00066000
         MVC   GTPBDYN(GTPBLEN),GTPBDNW                                 00067000
*         GOTO CKGET;             /* GO TO CHECK RETURN CODE FROM  0087 00068000
*                                    GETLINE.                        */ 00069000
         B     CKGET                                               0087 00070000
*       END;                                                       0088 00071000
*GEN(EJECT);                                                       0089 00072000
@RF00084 DS    0H                                                  0089 00073000
         EJECT                                                          00074000
*   RESPECIFY  MSGDPTR RSTD;                                       0090 00075000
*                                                                  0090 00076000
*   SVEMSGNM = MSGNM;             /* SAVE NUMBER OF MESSAGE        0091 00077000
*                                    TO BE SENT TO TERMINAL          */ 00078000
         L     @10,MSGNMPTR(,PARMPTR)                              0091 00079000
         MVC   SVEMSGNM(2),MSGNM(@10)                              0091 00080000
*   CNTLSWC = CNTLSWC && CNTLSWC; /* SET COMMUNICATIONS SWITCHES OFF */ 00081000
         XC    CNTLSWC(1),CNTLSWC                                  0092 00082000
*   PTRNEXT = SEGS1PTR;           /* GET ADDRESS OF FIRST SET OF   0093 00083000
*                                    TEXT INSERTION BUFFERS PASSED 0093 00084000
*                                    BY CALLER                       */ 00085000
*                                                                  0093 00086000
         MVC   PTRNEXT(4),SEGS1PTR(PARMPTR)                        0093 00087000
*NEXTMSG:                                                          0094 00088000
*                                                                  0094 00089000
*   MSGDPTR = IKJLGZ + MSGDOFST(SVEMSGNM); /* GET ADDRESS OF MESSAGE    00090000
*                                    DESCRIPTOR BLOCK.               */ 00091000
*                                                                  0094 00092000
NEXTMSG  L     @10,IKJLGZ                                          0094 00093000
         LH    @15,SVEMSGNM                                        0094 00094000
         ALR   @15,@15                                             0094 00095000
         BCTR  @15,0                                               0094 00096000
         BCTR  @15,0                                               0094 00097000
         LH    MSGDPTR,MSGDOFST(@15,@10)                           0094 00098000
         ALR   MSGDPTR,@10                                         0094 00099000
*   /*****************************************************************/ 00100000
*   /*     IF SVC DUMP TO BE ISSUED, SET COMMUNICATION SWITCH SO     */ 00101000
*   /*     THAT DUMP WILL BE TAKEN BEFORE RETURNING TO CALLER        */ 00102000
*   /*****************************************************************/ 00103000
*                                                                  0095 00104000
*   IF DUMP = '1'B                                                 0095 00105000
*     THEN                                                         0095 00106000
         TM    DUMP(MSGDPTR),B'00000010'                           0095 00107000
         BNO   @RF00095                                            0095 00108000
*       DUMPCORE = '1'B;                                           0096 00109000
*                                                                  0096 00110000
         OI    DUMPCORE,B'10000000'                                0096 00111000
*   IF TEXTI = '1'B               /* IF MESSAGE REQUIRES TEXT      0097 00112000
*                                    INSERTION, SET UP FOR IT.       */ 00113000
*     THEN                                                         0097 00114000
@RF00095 TM    TEXTI(MSGDPTR),B'00000100'                          0097 00115000
         BNO   @RF00097                                            0097 00116000
*       DO;                                                        0098 00117000
*         SEG1(1) = SEGMOVE;      /* MOVE CALLER SUPPLIED TEXT     0099 00118000
*                                    INSERTION BUFFER POINTERS TO  0099 00119000
*                                    WORK AREA                       */ 00120000
*                                                                  0099 00121000
         L     @10,PTRNEXT                                         0099 00122000
         MVC   SEG1(16),SEGMOVE(@10)                               0099 00123000
*         /***********************************************************/ 00124000
*         /*     LOOP TO INSERT OFFSETS INTO USER SUPPLIED TEXT      */ 00125000
*         /*     INSERTION BUFFERS.                                  */ 00126000
*         /*     IF THE USER DID NOT SUPPLY A BUFFER, ONE CONTAIN-   */ 00127000
*         /*     NING THE TEXT 'UNKNOWN' WILL BE CONSTRUCTED AND     */ 00128000
*         /*     THE OFFSET PLACED IN IT.                            */ 00129000
*         /***********************************************************/ 00130000
*                                                                  0100 00131000
*         DO LOOPCNT = 1 TO INSNUM;                                0100 00132000
*                                                                  0100 00133000
         LA    LOOPCNT,1                                           0100 00134000
         B     @DE00100                                            0100 00135000
@DL00100 DS    0H                                                  0101 00136000
*           IF SEGPTR1(LOOPCNT) = 0 /* IF SEGMENT NOT SUPPLIED,    0101 00137000
*                                    CONSTRUCT DUMMY SEGMENT.        */ 00138000
*             THEN                                                 0101 00139000
         LR    @10,LOOPCNT                                         0101 00140000
         SLA   @10,2                                               0101 00141000
         L     @04,SEGPTR1-4(@10)                                  0101 00142000
         LTR   @04,@04                                             0101 00143000
         BNZ   @RF00101                                            0101 00144000
*               DO;                                                0102 00145000
*                                                                  0102 00146000
*                 /***************************************************/ 00147000
*                 /*      CONSTRUCT 'UNKNOWN' TEXT BUFFER.           */ 00148000
*                 /**************************************************/  00149000
*                                                                  0103 00150000
*                 DUMMYLNG(LOOPCNT) = 11; /* SET SEGMENT LENGTH.     */ 00151000
         LR    @04,LOOPCNT                                         0103 00152000
         MH    @04,@CH01368                                        0103 00153000
         LA    @15,11                                              0103 00154000
         STH   @15,DUMMYLNG-12(@04)                                0103 00155000
*                 DUMMYOFT(LOOPCNT) = INSOFST(LOOPCNT); /* SET OFFSET   00156000
*                                    OF THIS TEXT IN MESSAGE.        */ 00157000
         SLR   @15,@15                                             0104 00158000
         IC    @15,INSOFST-1(LOOPCNT,MSGDPTR)                      0104 00159000
         STH   @15,DUMMYOFT-12(@04)                                0104 00160000
*                 DUMMYTXT(LOOPCNT) = 'UNKNOWN'; /* INDICATE TEXT  0105 00161000
*                                    IS UNKNOWN                      */ 00162000
         LA    @01,DUMMYTXT-12(@04)                                0105 00163000
         MVC   0(7,@01),@CC01379                                   0105 00164000
*                 SEGPTR1(LOOPCNT) = ADDR(DUMMYLNG(LOOPCNT));      0106 00165000
         LA    @04,DUMMYLNG-12(@04)                                0106 00166000
         ST    @04,SEGPTR1-4(@10)                                  0106 00167000
*               END;                                               0107 00168000
*             ELSE                                                 0108 00169000
*                                                                  0108 00170000
*               /*****************************************************/ 00171000
*               /*    PLACE OFFSET INTO CALLER SUPPLIED BUFFER       */ 00172000
*               /*****************************************************/ 00173000
*                                                                  0108 00174000
*               DO;                                                0108 00175000
         B     @RC00101                                            0108 00176000
@RF00101 DS    0H                                                  0109 00177000
*                 RESPECIFY SEGPTR RSTD;                           0109 00178000
*                 SEGPTR = SEGPTR1(LOOPCNT); /* GET ADDRESS OF     0110 00179000
*                                    SUPPLIED SEGMENT */           0110 00180000
*                                                                  0110 00181000
         LR    @10,LOOPCNT                                         0110 00182000
         SLA   @10,2                                               0110 00183000
         L     SEGPTR,SEGPTR1-4(@10)                               0110 00184000
*                   STOROFST = INSOFST(LOOPCNT); /* SET OFFSET OF  0111 00185000
*                                    THIS SEGMENT INTO MESSAGE.      */ 00186000
         SLR   @10,@10                                             0111 00187000
         IC    @10,INSOFST-1(LOOPCNT,MSGDPTR)                      0111 00188000
         STH   @10,STOROFST(,SEGPTR)                               0111 00189000
*               END;                                               0112 00190000
*                                                                  0112 00191000
*         END;                                                     0113 00192000
@RC00101 AH    LOOPCNT,@CH00033                                    0113 00193000
@DE00100 MVC   @ZT00003+3(1),INSNUM(MSGDPTR)                       0113 00194000
         C     LOOPCNT,@ZT00003                                    0113 00195000
         BNH   @DL00100                                            0113 00196000
*         NUMSEG1 = INSNUM + 1;   /* TOTAL NUMBER OF SEGMENTS IN THIS   00197000
*                                    MESSAGE, INCLUDING BASE SEGMENT.*/ 00198000
         LA    @10,1                                               0114 00199000
         MVC   @ZT00003+3(1),INSNUM(MSGDPTR)                       0114 00200000
         AL    @10,@ZT00003                                        0114 00201000
         ST    @10,NUMSEG1                                         0114 00202000
*         PTRNEXT = SEGS2PTR;     /* GET ADDRESS OF NEXT SET OF    0115 00203000
*                                    CALLER SUPPLIED TEXT INSERTION     00204000
*                                    BUFFERS                         */ 00205000
         MVC   PTRNEXT(4),SEGS2PTR(PARMPTR)                        0115 00206000
*       END;                                                       0116 00207000
*     ELSE                                                         0117 00208000
*                                                                  0117 00209000
*       NUMSEG1 = 1;              /* IF NO TEXT TO BE INSERTED,    0117 00210000
*                                    THERE IS ONLY ONE SEGMENT.      */ 00211000
*                                                                  0117 00212000
         B     @RC00097                                            0117 00213000
@RF00097 MVC   NUMSEG1(4),@CF00033                                 0117 00214000
*   MAINSEG = IKJLGZ + MSGOFST;   /* GET ACTUAL ADDRESS OF MESSAGE.  */ 00215000
@RC00097 LH    @10,MSGOFST(,MSGDPTR)                               0118 00216000
         AL    @10,IKJLGZ                                          0118 00217000
         ST    @10,MAINSEG                                         0118 00218000
*   SVEMSGNM = MSGOFSTR;          /* SAVE MESSAGE NUMBER OF RELATED     00219000
*                                    MESSAGE.                        */ 00220000
