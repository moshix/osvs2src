         LH    @10,MSGOFSTR(,MSGDPTR)                              0119 00222000
         STH   @10,SVEMSGNM                                        0119 00223000
*   IF WTOM = '1'B                /* IF WTO MESSAGE, NEEDS SPECIAL 0120 00224000
*                                    SET UP. BRANCH TO PROCESS.      */ 00225000
*     THEN                                                         0120 00226000
         TM    WTOM(MSGDPTR),B'00010000'                           0120 00227000
