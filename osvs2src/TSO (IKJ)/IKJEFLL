*/*          CONVERT THE RETURN CODE FROM BINARY TO PACKED DECIMAL   */ 01149000
*/*                                                                  */ 01150000
*/*          EDMK WILL CONVERT THE RETURN CODE FROM PACKED DECIMAL   */ 01151000
*/*          TO ZONED DECIMAL AND POINT REGISTER ONE TO THE FIRST    */ 01152000
*/*          SIGNIFICANT CHARACTER                                   */ 01153000
*/*                                                                  */ 01154000
*/*          LEFT JUSTIFY THE RETURN CODE IN THE TEXT FIELD AND      */ 01155000
*/*          UPDATE THE TEXT INSER LENGTH                            */ 01156000
*/*                                                                  */ 01157000
*/*CHANGE ACTIVITY-                                                  */ 01158000
*/*      @Z40RQKC-                                                   */ 01159000
*/*          NEW FOR RELEASE 4 RAC4 SUPPORT                          */ 01160000
*/*                                                                  */ 01161000
*/********************************************************************/ 01162000
*                                                                  0430 01163000
*CONVRC:                                                           0430 01164000
*        PROCEDURE(CONINS);                                        0430 01165000
*                                                                  0430 01166000
CONVRC   STM   @14,@12,@SA00016                                    0430 01167000
         MVC   @PC00016(4),0(@01)                                  0430 01168000
*DECLARE                                                           0431 01169000
*  1 CONINS,                         /* AN INSERT                    */ 01170000
*    2 CONHEAD,                                                    0431 01171000
*      3 CONLEN FIXED(15),                                         0431 01172000
*      3 CONOFF FIXED(15),                                         0431 01173000
*    2 CONTEXT CHAR(16);                                           0431 01174000
*                                                                  0431 01175000
*   CVD(STATUS,PACKDEC);             /* BINARY TO PACKED DECIMAL     */ 01176000
         CVD   STATUS,PACKDEC                                      0432 01177000
*   CONTEXT = EDITMASK;              /* INITIALIZE THE EDIT MASK     */ 01178000
*                                                                  0433 01179000
         L     @10,@PC00016                                        0433 01180000
         MVC   CONTEXT(16,@10),@CB00181                            0433 01181000
*   RESPECIFY (GPR01P) RSTD;                                       0434 01182000
*   GPR01P = ADDR(CONTEXT);          /* ADDR OF TEXT                 */ 01183000
         LA    GPR01P,CONTEXT(,@10)                                0435 01184000
*   GEN REFS(PACKDEC)                /* EDIT THE RETURN CODE         */ 01185000
*       (EDMK 0(16,1),PACKDEC);                                    0436 01186000
*                                                                  0436 01187000
         EDMK 0(16,1),PACKDEC                                           01188000
*   CONLEN = LENGTH(CONTEXT) -       /* ORIGINAL LENGTH              */ 01189000
*           (GPR01P -                /* FIRST NON-ZERO DIGIT         */ 01190000
*           ADDR(CONTEXT));          /* FIRST ZERO DIGIT             */ 01191000
         L     @10,@PC00016                                        0437 01192000
         LA    @15,CONTEXT(,@10)                                   0437 01193000
         LR    @00,@15                                             0437 01194000
         SLR   @00,GPR01P                                          0437 01195000
         AH    @00,@CH00124                                        0437 01196000
         STH   @00,CONLEN(,@10)                                    0437 01197000
*   CONTEXT(1:CONLEN) =              /* LEFT JUSTIFY THE RETURN CODE */ 01198000
*       CONTEXT(GPR01P-ADDR(CONTEXT)+1:LENGTH(CONTEXT));           0438 01199000
         LR    @14,@00                                             0438 01200000
         BCTR  @14,0                                               0438 01201000
         LCR   @15,@15                                             0438 01202000
         ALR   @15,GPR01P                                          0438 01203000
         ALR   @15,@10                                             0438 01204000
         MVC   @TS00001(16),CONTEXT(@15)                           0438 01205000
         EX    @14,@SM02213                                        0438 01206000
*   RESPECIFY (GPR01P) UNRSTD;                                     0439 01207000
*                                                                  0439 01208000
*   CONLEN = CONLEN +                /* RETURN CODE LENGTH           */ 01209000
*           LENGTH(CONHEAD);         /* TEXT INSERT HEADER LENGTH    */ 01210000
         AH    @00,@CH00040                                        0440 01211000
         STH   @00,CONLEN(,@10)                                    0440 01212000
*END CONVRC;                                                       0441 01213000
@EL00016 DS    0H                                                  0441 01214000
@EF00016 DS    0H                                                  0441 01215000
@ER00016 LM    @14,@12,@SA00016                                    0441 01216000
         BR    @14                                                 0441 01217000
*SYNAD:                                                            0442 01218000
*        PROC OPTIONS(DONTSAVE,NOSAVEAREA);                        0442 01219000
*                                                                  0442 01220000
*                                                                  0442 01221000
SYNAD    DS    0H                                                  0443 01222000
*        IOERR='1'B;              /* INDICATE I/O ERROR HAS OCCURED  */ 01223000
         OI    IOERR,B'00010000'                                   0443 01224000
*        END SYNAD;                                                0444 01225000
@EL00017 DS    0H                                                  0444 01226000
@EF00017 DS    0H                                                  0444 01227000
@ER00017 BR    @14                                                 0444 01228000
