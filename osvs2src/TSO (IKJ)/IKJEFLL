*/********************************************************************/ 01234000
*                                                                  0445 01235000
*                                                                  0445 01236000
*  DECLARE /*GENERAL PURPOSE REGISTERS */                          0445 01237000
*    GPR01F FIXED(31) REG(1),                                      0445 01238000
*    GPR01P PTR(31) REG(1),                                        0445 01239000
*    GPR15P PTR(31) REG(15);                                       0445 01240000
*  DCL 1 I056 DEF(ILIST) BDY(WORD) CHAR(32767),  /*LIST FOR ENQ*/  0446 01241000
*    3 I05600 PTR(31),  /*TCB ADDR IF BOTH TCB AND ECB*/           0446 01242000
*    3 I05601 PTR(31),  /*TCB OR ECB ADDRESS*/                     0446 01243000
*    3 I05602 (*) CHAR(12) BDY(WORD),  /*ARRAY OF RESOURCE ELEMENTS*/   01244000
*      5 I05603 BIT(8),  /*BIT FLAGS 1*/                           0446 01245000
*      5 I05604 PTR(8),  /*MINOR RESOURCE NAME LENGTH*/            0446 01246000
*      5 I05605 BIT(8),  /*BIT FLAGS 2*/                           0446 01247000
*      5 I05655 CHAR(1),  /*CONTROL PGM USE*/                      0446 01248000
*      5 I05606 PTR(31),  /*MAJOR RESOURCE NAME ADDR*/             0446 01249000
*      5 I05607 PTR(31);  /*MINOR RESOURCE NAME ADDR*/             0446 01250000
*  DCL 1 I048 DEF(ILIST) CHAR(32767) BDY(WORD),                    0447 01251000
*     3 I04801 PTR(31) , /* TCB ADDR */                            0447 01252000
*     3 I04802(*) CHAR(12) BDY(WORD),                              0447 01253000
*        5 I04803 PTR(31), /* UCB @ IN LAST ENTRY */               0447 01254000
*           7 I04804 BIT(8),   /* FLAGS */                         0447 01255000
*           7 I04805 PTR(8),   /*MINOR RESOURCE NAME LENGTH*/      0447 01256000
*           7 I04806 BIT(8),   /* FLAGS */                         0447 01257000
*           7 *      CHAR(1),  /* CONTROL PROGRAM USE */           0447 01258000
*        5 I04807 PTR(31),  /* MAJOR RESOURCE NAME @ */            0447 01259000
*        5 I04808 PTR(31); /* MINOR RESOURCE NAME @ */             0447 01260000
*                                                                  0447 01261000
*  DECLARE ILIST CHAR(48) BDY(DWORD); /*STANDARD LIST*/            0448 01262000
*                                                                  0448 01263000
*  DECLARE /* COMMON VARIABLES */                                  0449 01264000
