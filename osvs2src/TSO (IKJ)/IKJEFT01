         TITLE 'IKJEFT01 -- TMP INITIALIZATION ROUTINE                 *00001000
                        '                                               00002000
         ENTRY  IKJEFT0A                                                00003000
IKJEFT01 CSECT ,                                                01S0002 00004000
@MAINENT DS    0H                                               01S0002 00005000
         USING *,@15                                            01S0002 00006000
         B     @PROLOG                                          01S0002 00007000
         DC    AL1(16)                                          01S0002 00008000
         DC    C'IKJEFT01  78.177'                              01S0002 00009000
         DROP  @15                                                      00010000
@PROLOG  STM   @14,@12,12(@13)                                  01S0002 00011000
         BALR  @10,0                                            01S0002 00012000
@PSTART  LA    @11,4095(,@10)                                   01S0002 00013000
         USING @PSTART,@10                                      01S0002 00014000
         USING @PSTART+4095,@11                                 01S0002 00015000
         L     @00,@SIZDATD                                     01S0002 00016000
         GETMAIN  R,LV=(0)                                              00017000
         LR    @12,@01                                          01S0002 00018000
         USING @DATD,@12                                        01S0002 00019000
         ST    @13,@SA00001+4                                   01S0002 00020000
         LM    @00,@01,20(@13)                                  01S0002 00021000
         ST    @12,8(,@13)                                      01S0002 00022000
         LR    @13,@12                                          01S0002 00023000
         MVC   @PC00001(8),0(@01)                               01S0002 00024000
LWASWAR  EQU   4                                                        00025000
LWAMSPE  EQU   16                                                       00026000
INITDONE EQU  4                                                         00027000
NQUSERID EQU  8                                                         00028000
DQUSERID EQU  12                                                        00029000
SCHEDULE EQU  16                                                        00030000
BCDONE   EQU  20                                                        00031000
TERMINAT EQU  24                                                        00032000
NQDQ0    EQU  0                                                         00033000
NQDQ4    EQU  4                                                         00034000
NQDQ8    EQU  8                                                         00035000
STARTP2  EQU  16                                                        00036000
INITRDY  EQU  20                                                        00037000
ENDTASK  EQU  24                                                        00038000
JSCBDISP EQU 188                                                        00039000
JSCBAOS1 EQU  72                                                        00040000
JSCBAOS2 EQU 192                                                        00041000
*    /****************************************************************/ 00042000
*    /* SET POINTERS TO INPUT PARAMETERS AND MODESET TO SUPV STATE   */ 00043000
*    /****************************************************************/ 00044000
*                                                               01S0125 00045000
*    FMAINP = R1;                   /* SAVE ENTRY PARM PTR           */ 00046000
         LR    FMAINP,R1                                        01S0125 00047000
*    FSTCMDP = ADDR(PARM);          /* ESTABLISH FIRST CMD           */ 00048000
         L     @06,@PC00001                                     01S0126 00049000
         ST    @06,FSTCMDP                                      01S0126 00050000
*    RETRYWAP = ADDR(WA2);          /* VALID IF FROM IKJEFT07        */ 00051000
         L     @06,@PC00001+4                                   01S0127 00052000
         ST    @06,RETRYWAP                                     01S0127 00053000
*    /* SET SUPERVISOR STATE                                         */ 00054000
*    DO; /*MODESET  MODE(SUP)*/                                 01S0128 00055000
*      I10701 = '00000000'B;/*OR MASK*/                         01S0129 00056000
         MVI   I10701,B'00000000'                               01S0129 00057000
*      I10702 = '00000000'B;/*AND MASK*/                        01S0130 00058000
         MVI   I10702,B'00000000'                               01S0130 00059000
*      I10703 = '00000000'B;/*RESERVED*/                        01S0131 00060000
         MVI   I10703,B'00000000'                               01S0131 00061000
*      I10704 = '00001100'B;                                    01S0132 00062000
         MVI   I10704,B'00001100'                               01S0132 00063000
*      RESPECIFY GPR01F RESTRICTED;                             01S0133 00064000
*      GPR01F = I107; /*MODESET PARMLIST*/                      01S0134 00065000
         L     GPR01F,I107                                      01S0134 00066000
*      SVC (107);/*ISSUE MODESET SVC*/                          01S0135 00067000
         SVC   107                                              01S0135 00068000
*      RESPECIFY (GPR01F) UNRESTRICTED;                         01S0136 00069000
*    END; /*MODESET  MODE(SUP)*/                                01S0137 00070000
*                                                               01S0137 00071000
*                                                               01S0137 00072000
*    /****************************************************************/ 00073000
*    /* GET SPACE FOR TMP PRIVATE, PROTECTED WORK AREA, KEY 1        */ 00074000
*    /****************************************************************/ 00075000
*                                                               01S0138 00076000
*    IF FSTCMDP ^= EFFWD            /* CHK FOR RETRY ENTRY           */ 00077000
*      THEN DO;                     /* NO, ESTABLISH STAE/STAI WA    */ 00078000
         CLC   FSTCMDP(4),@CF00088                              01S0138 00079000
         BE    @RF00138                                         01S0138 00080000
*        /* GET ENTRY PSW PROTECT KEY                                */ 00081000
*        RESPECIFY (R2) RSTD;                                   01S0140 00082000
*        DO; /*MODESET  SAVEKEY(R2) WORKREG(R2)*/               01S0141 00083000
*          RESPECIFY (GPR02F) RESTRICTED;                       01S0142 00084000
*          R2 = GPR02F; /*SAVE REGISTER 2*/                     01S0143 00085000
*          IPK; /*SAVE CURRENT PSW KEY*/                        01S0144 00086000
         IPK                                                    01S0144 00087000
*          R2 = GPR02F;                                         01S0145 00088000
*          GPR02F = R2; /*RESTORE REGISTER 2*/                  01S0146 00089000
*          RESPECIFY (GPR02F) UNRESTRICTED;                     01S0147 00090000
*        END; /*MODESET  SAVEKEY(R2) WORKREG(R2)*/              01S0148 00091000
*        RESPECIFY (R0,R1,R3) RSTD;                             01S0149 00092000
*        R3 = R2;                   /* HOLD ENTRY PSW KEY            */ 00093000
         LR    R3,R2                                            01S0150 00094000
*        CGMBLKL=LENGTH(TMPWRKA2);  /* SET LENGTH FOR GETMAIN        */ 00095000
         MVC   CGMBLKL(3),@CF03531+1                            01S0151 00096000
*        CGMBLKN = SPNM230;         /* SET FOR SUBPOOL 230           */ 00097000
         MVI   CGMBLKN,X'E6'                                    01S0152 00098000
*        R0 = CGMBLKA;              /* SET GETMAIN PARAMETERS        */ 00099000
         L     R0,CGMBLKA                                       01S0153 00100000
*        /* SET PSW PROTECT KEY 1                                    */ 00101000
*        DO; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/           01S0154 00102000
*          R2 = SPROK1; /*GET KEY FORM SAVE LOCATION*/          01S0155 00103000
         L     R2,SPROK1                                        01S0155 00104000
*          SPKA (R2); /*SET PSW KEY*/                           01S0156 00105000
         SPKA  0(R2)                                            01S0156 00106000
*        END; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/          01S0157 00107000
*        GEN(GETMAIN  R,LV=(0));    /* GETMAIN PROTECTED STORAGE     */ 00108000
         GETMAIN  R,LV=(0)                                              00109000
*        WRKAPTR2 = R1;             /* SET PTR TO PROTECTED STORAGE  */ 00110000
         LR    WRKAPTR2,R1                                      01S0159 00111000
*        TWRKA2A=TWRKA2A&&TWRKA2A;  /* CLEAR TMP PROTECTED WORK      */ 00112000
         XC    TWRKA2A(168,WRKAPTR2),TWRKA2A(WRKAPTR2)          01S0160 00113000
*        TWRKA2B=TWRKA2B&&TWRKA2B;  /*   AREA                        */ 00114000
         XC    TWRKA2B(64,WRKAPTR2),TWRKA2B(WRKAPTR2)           01S0161 00115000
*      END;                                                     01S0162 00116000
*      ELSE DO;                     /* ENTRY IS RETRY ENTRY          */ 00117000
         B     @RC00138                                         01S0163 00118000
@RF00138 DS    0H                                               01S0164 00119000
*        WRKAPTR2 = RETRYWAP;       /* RESTORE PTR TO PROTECTED WA   */ 00120000
         L     WRKAPTR2,RETRYWAP                                01S0164 00121000
*        DO; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/           01S0165 00122000
*          R2 = SPROK1; /*GET KEY FORM SAVE LOCATION*/          01S0166 00123000
         L     R2,SPROK1                                        01S0166 00124000
*          SPKA (R2); /*SET PSW KEY*/                           01S0167 00125000
         SPKA  0(R2)                                            01S0167 00126000
*        END; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/          01S0168 00127000
*        R3 = TEPKEY;               /* SET FOR ENTRY PSW PROTECT KEY */ 00128000
         L     R3,TEPKEY(,WRKAPTR2)                             01S0169 00129000
*      END;                                                     01S0170 00130000
*    WRKA2PTR = WRKAPTR2;           /* RESERVE PTR TO PROTECTED STG  */ 00131000
@RC00138 ST    WRKAPTR2,WRKA2PTR(,WRKAPTR2)                     01S0171 00132000
*    TEPKEY = R3;                   /* RESERVE ENTRY PSW KEY         */ 00133000
         ST    R3,TEPKEY(,WRKAPTR2)                             01S0172 00134000
*    /* RESTORE ENTRY PSW PROTECT KEY                                */ 00135000
*    R2 = R3;                       /* SET FOR MODESET               */ 00136000
         LR    R2,R3                                            01S0173 00137000
*    DO; /*MODESET  KEYADDR(R2) WORKREG(R2)*/                   01S0174 00138000
*      R2 = R2; /*GET KEY FORM SAVE LOCATION*/                  01S0175 00139000
*      SPKA (R2); /*SET PSW KEY*/                               01S0176 00140000
         SPKA  0(R2)                                            01S0176 00141000
*    END; /*MODESET  KEYADDR(R2) WORKREG(R2)*/                  01S0177 00142000
*    RESPECIFY (R0,R1,R2,R3) UNRSTD;                            01S0178 00143000
*    /****************************************************************/ 00144000
*    /* CALCULATE  SIZE  OF TMP DYNAMIC WORKAREA  AND  GETMAIN  FROM */ 00145000
*    /*   SUBPOOL 1.   ZERO OUT THE AREA.                            */ 00146000
*    /****************************************************************/ 00147000
*                                                               01S0179 00148000
*    RESPECIFY (R0,R1) RSTD;                                    01S0179 00149000
*    TOTLEN =                       /* CALCULATED LENGTH             */ 00150000
*        LENGTH(TMPWRKA1)           /* TMPWRKA1 LENGTH               */ 00151000
*        + STAXPLEN;                /* ADD STAXPARM LENGTH           */ 00152000
         LA    TOTLEN,1708                                      01S0180 00153000
         AH    TOTLEN,STAXPLEN                                  01S0180 00154000
*    R0 = SPL1 + TOTLEN;            /* ADD SPL1 IN HIGH ORDER BYTE   */ 00155000
         LR    R0,TOTLEN                                        01S0181 00156000
         AL    R0,@CF00132                                      01S0181 00157000
*    GEN(GETMAIN R,LV=(0));         /* GETMAIN FOR DYNAMIC WORKAREA  */ 00158000
*                                                               01S0182 00159000
         GETMAIN R,LV=(0)                                               00160000
*    WRKAPTR1 = R1;                 /* RESERVE PTR TO PP WORK AREA   */ 00161000
         LR    WRKAPTR1,R1                                      01S0183 00162000
*    RESPECIFY (R0) UNRSTD;                                     01S0184 00163000
*    /* SET PROTECT KEY 1                                            */ 00164000
*    DO; /*MODESET  SAVEKEY(R2) KEYADDR(SPROK1) WORKREG(R2)*/   01S0185 00165000
*      RESPECIFY (GPR02F) RESTRICTED;                           01S0186 00166000
*      R2 = GPR02F; /*SAVE REGISTER 2*/                         01S0187 00167000
*      IPK; /*SAVE CURRENT PSW KEY*/                            01S0188 00168000
         IPK                                                    01S0188 00169000
*      R2 = GPR02F;                                             01S0189 00170000
*      GPR02F = R2; /*RESTORE REGISTER 2*/                      01S0190 00171000
*      SPKA (SPROK1); /*SET PSW KEY*/                           01S0191 00172000
         L     @06,SPROK1                                       01S0191 00173000
         SPKA  0(@06)                                           01S0191 00174000
*      RESPECIFY (GPR02F) UNRESTRICTED;                         01S0192 00175000
*    END; /*MODESET  SAVEKEY(R2) KEYADDR(SPROK1) WORKREG(R2)*/  01S0193 00176000
*    WRKA1PTR = WRKAPTR1;           /* SET PTR TO PP WORK AREA       */ 00177000
         ST    WRKAPTR1,WRKA1PTR(,WRKAPTR2)                     01S0194 00178000
*    RESPECIFY (R2) RSTD;                                       01S0195 00179000
*    /* RESTORE ENTRY PSW PROTECT KEY                                */ 00180000
*    DO; /*MODESET  KEYADDR(R2) WORKREG(R2)*/                   01S0196 00181000
*      R2 = R2; /*GET KEY FORM SAVE LOCATION*/                  01S0197 00182000
*      SPKA (R2); /*SET PSW KEY*/                               01S0198 00183000
         SPKA  0(R2)                                            01S0198 00184000
*    END; /*MODESET  KEYADDR(R2) WORKREG(R2)*/                  01S0199 00185000
*                                                               01S0199 00186000
*    CLTMPC = TOTLEN;               /* SET TO TOTAL LENGTH           */ 00187000
         LR    CLTMPC,TOTLEN                                    01S0200 00188000
*    DO WHILE CLTMPC > D255;        /* TO CLEAR BULK OF WORK AREA    */ 00189000
         B     @DE00201                                         01S0201 00190000
@DL00201 DS    0H                                               01S0202 00191000
*      TMPWRKA1((CLTMPC-D254):CLTMPC) =                         01S0202 00192000
*          TMPWRKA1((CLTMPC-D254):CLTMPC)&&                     01S0202 00193000
*          TMPWRKA1((CLTMPC-D254):CLTMPC);                      01S0202 00194000
         LR    @06,WRKAPTR1                                     01S0202 00195000
         ALR   @06,CLTMPC                                       01S0202 00196000
         AL    @06,@CF03542                                     01S0202 00197000
         XC    TMPWRKA1(255,@06),TMPWRKA1(@06)                  01S0202 00198000
*      CLTMPC = CLTMPC-D255;                                    01S0203 00199000
         SL    CLTMPC,@CF00084                                  01S0203 00200000
*    END;                                                       01S0204 00201000
@DE00201 C     CLTMPC,@CF00084                                  01S0204 00202000
         BH    @DL00201                                         01S0204 00203000
*    IF CLTMPC > ZERO                                           01S0205 00204000
*      THEN TMPWRKA1(1:CLTMPC) = TMPWRKA1(1:CLTMPC)             01S0205 00205000
         LTR   CLTMPC,CLTMPC                                    01S0205 00206000
         BNP   @RF00205                                         01S0205 00207000
*           &&TMPWRKA1(1:CLTMPC);                               01S0206 00208000
         LR    @06,CLTMPC                                       01S0206 00209000
         BCTR  @06,0                                            01S0206 00210000
         EX    @06,@SX03543                                     01S0206 00211000
*                                                               01S0207 00212000
*    /****************************************************************/ 00213000
*    /* USE EXTRACT TO OBTAIN POINTERS TO THE ADDRESS OF THE STOP/   */ 00214000
*    /* MODIFY ECB, OF THE TIME SHARING FLAGS FIELD, AND OF THE PSCB.*/ 00215000
*    /* THE POINTERS ARE PLACED  IN THREE CONSECUTIVE WORDS  IN THE  */ 00216000
*    /* TMP WORKAREA.   A POINTER TO THE STOP/MODIFY ECB POINTER IS  */ 00217000
*    /* LOCATED IN THE FIRST WORD, A POINTER TO THE TS FLAG FIELD IN */ 00218000
*    /* THE SECOND WORD,  AND  THE PSCBPTR  IS LOCATED IN THE THIRD  */ 00219000
*    /* WORD.  THE  NXTCMD AREA  IS TEMPORARILY USED  BY THE EXTRACT */ 00220000
*    /* MACRO.                                                       */ 00221000
*    /****************************************************************/ 00222000
*                                                               01S0207 00223000
*    R2 = ADDR(TPLMECB);            /* PTR TO EXTRACT ANSWER FIELD   */ 00224000
@RF00205 LA    R2,TPLMECB(,WRKAPTR1)                            01S0207 00225000
*    R1 = ADDR(XTRCLST);            /* TEMPORARILY USE COMMAND NAME     00226000
*                                      AREA FOR EXTRACT WORK AREA    */ 00227000
         LA    R1,XTRCLST(,WRKAPTR1)                            01S0208 00228000
*    /* EXTRACT PTRS TO STOP/MODIFY ECB, TSO FLAGS AND PSCB          */ 00229000
*    GEN(EXTRACT (R2),'S',FIELDS=(COMM,TSO,PSB),MF=(E,(1)));    01S0209 00230000
         EXTRACT (R2),'S',FIELDS=(COMM,TSO,PSB),MF=(E,(1))              00231000
*    RESPECIFY (R1,R2) UNRSTD;                                  01S0210 00232000
*    TPLMECB = MECBPTR;             /* ESTABLISH STOP/MODIFY ECB     */ 00233000
         L     @06,TPLMECB(,WRKAPTR1)                           01S0211 00234000
         L     @06,MECBPTR(,@06)                                01S0211 00235000
         ST    @06,TPLMECB(,WRKAPTR1)                           01S0211 00236000
*    TPLPSCB = PSCBP;               /* ESTABLISH PSCB                */ 00237000
*                                                               01S0212 00238000
         L     @06,PSCBP(,WRKAPTR1)                             01S0212 00239000
         ST    @06,TPLPSCB(,WRKAPTR1)                           01S0212 00240000
*    /****************************************************************/ 00241000
*    /* INITIALIZE POINTERS AND PARAMETER BLOCKS IN THE TMP WORKAREA */ 00242000
*    /* THIS AREA  WILL BE USED  FOR THE REMAINDER  OF THE TERMINAL  */ 00243000
*    /* SESSION  AND  IS REFERENCED BY  THE TMP ATTENTION EXIT, THE  */ 00244000
*    /* TMP STAE EXIT, AND THE TMP STAI EXIT                         */ 00245000
*    /****************************************************************/ 00246000
*                                                               01S0213 00247000
*    /* ESTABLISH POINTERS FOR PARM BLOCKS                           */ 00248000
*    SRPLPTR = ADDR(CSPLSP);        /* SERVICE ROUTINE PARM LIST     */ 00249000
         LA    @06,CSPLSP(,WRKAPTR1)                            01S0213 00250000
         ST    @06,SRPLPTR(,WRKAPTR1)                           01S0213 00251000
*    CSPLPTR = SRPLPTR;             /* SET PTR TO CMD SCAN PL        */ 00252000
         ST    @06,CSPLPTR(,WRKAPTR1)                           01S0214 00253000
*    IOPLPTR = SRPLPTR;             /* SET PTR TO I/O SERVICE PL     */ 00254000
         ST    @06,IOPLPTR(,WRKAPTR1)                           01S0215 00255000
*    ASRPLPTR=ADDR(CSPLSP2);        /* SRV RTN PARM LIST FOR ATTN    */ 00256000
         LA    @06,CSPLSP2(,WRKAPTR1)                           01S0216 00257000
         ST    @06,ASRPLPTR(,WRKAPTR1)                          01S0216 00258000
*    SCANAP = ADDR(CSOASP);         /* SCAN ANSWER AREA              */ 00259000
         LA    @06,CSOASP(,WRKAPTR1)                            01S0217 00260000
         ST    @06,SCANAP(,WRKAPTR1)                            01S0217 00261000
*    ASCANAP=ADDR(CSOASP2);         /* SCAN ANSWER AREA FOR ATTN     */ 00262000
         LA    @06,CSOASP2(,WRKAPTR1)                           01S0218 00263000
         ST    @06,ASCANAP(,WRKAPTR1)                           01S0218 00264000
*    GTPBPTR = ADDR(GTPBSP);        /* GETLINE PARAMETER BLOCK       */ 00265000
         LA    @06,GTPBSP(,WRKAPTR1)                            01S0219 00266000
         ST    @06,GTPBPTR(,WRKAPTR1)                           01S0219 00267000
*    PGPBPTR = ADDR(PGPBSP);        /* PUTGET PARAMETER BLOCK        */ 00268000
         LA    @06,PGPBSP(,WRKAPTR1)                            01S0220 00269000
         ST    @06,PGPBPTR(,WRKAPTR1)                           01S0220 00270000
*    PTPBPTR = ADDR(PTPBSP);        /* PUTLINE PARAMETER BLOCK       */ 00271000
         LA    @06,PTPBSP(,WRKAPTR1)                            01S0221 00272000
         ST    @06,PTPBPTR(,WRKAPTR1)                           01S0221 00273000
*    ATTCHPTR = ADDR(ATTCHSP);      /* ATTACH LIST                   */ 00274000
         LA    @06,ATTCHSP(,WRKAPTR1)                           01S0222 00275000
         ST    @06,ATTCHPTR(,WRKAPTR1)                          01S0222 00276000
*    STPBPTR = ADDR(STPBSP);        /* STACK PARAMETER BLOCK         */ 00277000
         LA    @06,STPBSP(,WRKAPTR1)                            01S0223 00278000
         ST    @06,STPBPTR(,WRKAPTR1)                           01S0223 00279000
*    DYNAPPTR = ADDR(MDYNASP);      /* DYNAMIC ALLOCATION PB         */ 00280000
         LA    @06,MDYNASP(,WRKAPTR2)                           01S0224 00281000
         ST    @06,DYNAPPTR(,WRKAPTR1)                          01S0224 00282000
*    PPLPTR = ADDR(PPLSP);          /* PARSE PARAMETER LIST          */ 00283000
         LA    @06,PPLSP(,WRKAPTR1)                             01S0225 00284000
         ST    @06,PPLPTR(,WRKAPTR1)                            01S0225 00285000
*    DAPLPTR = ADDR(DAPLSP);        /* DAIR PARAMETER LIST           */ 00286000
         LA    @06,DAPLSP(,WRKAPTR1)                            01S0226 00287000
         ST    @06,DAPLPTR(,WRKAPTR1)                           01S0226 00288000
*    DAPB0PTR = ADDR(DAPBSP);       /* DAIR PARAMETER BLOCK          */ 00289000
         LA    @06,DAPBSP(,WRKAPTR1)                            01S0227 00290000
         ST    @06,DAPB0PTR(,WRKAPTR1)                          01S0227 00291000
*    READYPTR=ADDR(MODEMSP);        /* READY MESSAGE                 */ 00292000
         LA    @06,MODEMSP(,WRKAPTR1)                           01S0228 00293000
         ST    @06,READYPTR(,WRKAPTR1)                          01S0228 00294000
*    MODEMLG = RDYLEN-SEVEN;        /* GET LENGTH OF TEXT            */ 00295000
         LA    @05,7                                            01S0229 00296000
         L     @04,@CA00040                                     01S0229 00297000
         LH    MODEMLG,RDYLEN-RDYMSG(,@04)                      01S0229 00298000
         SLR   MODEMLG,@05                                      01S0229 00299000
*    STAXPPTR=(READYPTR+((MODEMLG+SEVEN)&FWD2));  /* STAX PL         */ 00300000
         ALR   @05,MODEMLG                                      01S0230 00301000
         N     @05,@CF00097                                     01S0230 00302000
         ALR   @06,@05                                          01S0230 00303000
         ST    @06,STAXPPTR(,WRKAPTR1)                          01S0230 00304000
*                                                               01S0231 00305000
*                                                               01S0231 00306000
*    CPPLPTR = ADDR(CPPLSP);        /* SET PTR TO CP PARAMETER LIST  */ 00307000
         LA    @06,CPPLSP(,WRKAPTR1)                            01S0231 00308000
         ST    @06,CPPLPTR(,WRKAPTR1)                           01S0231 00309000
*    TPLECT = ADDR(ECTSP);          /* ECT TABLE                     */ 00310000
         LA    @06,ECTSP(,WRKAPTR1)                             01S0232 00311000
         ST    @06,TPLECT(,WRKAPTR1)                            01S0232 00312000
*    RESPECIFY (ECT) BASED(TPLECT);                             01S0233 00313000
*    ECT = ECT && ECT;              /* SET COMPLETE ECT TO ZERO      */ 00314000
         XC    ECT(56,@06),ECT(@06)                             01S0234 00315000
*    CTLBKSP = CTLBKSP&&CTLBKSP;    /* CLEAR PARAMETERS              */ 00316000
*                                                               01S0235 00317000
         XC    CTLBKSP(12,WRKAPTR1),CTLBKSP(WRKAPTR1)           01S0235 00318000
*    ECTBKGRD = FGRDNOW;            /* CLEAR BACKGROUND FLAG         */ 00319000
         NI    ECTBKGRD(@06),B'11111101'                        01S0236 00320000
*    /* SET PROTECT KEY 1                                            */ 00321000
*    DO; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/               01S0237 00322000
*      R2 = SPROK1; /*GET KEY FORM SAVE LOCATION*/              01S0238 00323000
         L     @05,SPROK1                                       01S0238 00324000
         LR    R2,@05                                           01S0238 00325000
*      SPKA (R2); /*SET PSW KEY*/                               01S0239 00326000
         SPKA  0(R2)                                            01S0239 00327000
*    END; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/              01S0240 00328000
*    BKGMODE = FGRDNOW;             /* CLEAR BACKGROUND FLAG         */ 00329000
         NI    BKGMODE(WRKAPTR2),B'01111111'                    01S0241 00330000
*    /* RESTORE ENTRY PSW PROTECT KEY                                */ 00331000
*    DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/               01S0242 00332000
*      R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/              01S0243 00333000
         L     @04,TEPKEY(,WRKAPTR2)                            01S0243 00334000
