*DS    */                                                       01S0825 02093000
*                                                               01S0825 02094000
*    IF STATUS = CONTINUE            /* CHK FOR SUCCESSFUL PROCESS   */ 02095000
*      THEN                          /* YES,                         */ 02096000
         SLR   @15,@15                                          01S0826 02097000
         C     @15,STATUS                                       01S0826 02098000
         BNE   @RF00826                                         01S0826 02099000
*        RETCODE = ZERO;             /*   SET NO ERROR RETURN CODE   */ 02100000
         ST    @15,RETCODE                                      01S0827 02101000
*      ELSE                          /* NO, PROCESS NOT SUCCESSFUL   */ 02102000
*        IF STATUS = ERRUSER         /* CHK FOR PROCESS FAILURE      */ 02103000
*            |ASXBUSER = ZEROES      /*   OR NO USERID SUPPLIED      */ 02104000
*          THEN                      /* YES,                         */ 02105000
