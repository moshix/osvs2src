*                                       CREATE NEW STAE REQUEST      */ 00735000
         ESTAE  (R4),CT,PARAM=(R3),XCTL=YES,MF=(E,(1))                  00736000
*    RESPECIFY (R0,R1,R2,R3,R4) UNRSTD;                         01S0423 00737000
*                                                               01S0423 00738000
*                                                               01S0423 00739000
*    /****************************************************************/ 00740000
*    /* DETERMINE WHETHER THIS IS A RETRY ENTRY TO THIS MODULE.   IF */ 00741000
*    /* IT IS THE FIRST ENTRY,  THE SWITCH TO SUPPRESS INTERTERMINAL */ 00742000
*    /* COMMUNICATION   AND  THE  LINE DELETE  AND  CHARACTER DELETE */ 00743000
*    /* CHARACTERS  ARE SET  TO THE SAVE STATUS  THAT EXISTED AT THE */ 00744000
*    /* END OF THE LAST SESSION AS INDICATED IN THE UPT.             */ 00745000
*    /****************************************************************/ 00746000
*                                                               01S0424 00747000
*    /* CHECK POINTER TO INPUT PARAMETER FOR TYPE OF ENTRY           */ 00748000
*    IF FSTCMDP ^= EFFWD            /* CHK FOR ALL ONES      @VS43613*/ 00749000
*      THEN DO;                     /* YES, THIS IS A RETRY ENTRY    */ 00750000
         CLC   FSTCMDP(4),@CF00088                              01S0424 00751000
         BE    @RF00424                                         01S0424 00752000
*        /* CHECK UPT FOR INTERTERMINAL COMMUNICATION                */ 00753000
*        IF UPTNCOM='1'B                                        01S0426 00754000
*          THEN                     /* NO INTERTERM COMMUNICATION    */ 00755000
         L     @01,UPTPTR(,WRKAPTR2)                            01S0426 00756000
         TM    UPTNCOM(@01),B'00010000'                         01S0426 00757000
         BNO   @RF00426                                         01S0426 00758000
*            GEN(STCOM NO);         /* INDICATE NO INTERCOM          */ 00759000
         STCOM NO                                                       00760000
*          ELSE                     /* INTERTERMINAL COMMUNICATION      00761000
*                                       SPECIFIED                    */ 00762000
*            GEN(STCOM YES);        /* INDICATE INTERCOM ALLOWED     */ 00763000
*                                                               01S0428 00764000
         B     @RC00426                                         01S0428 00765000
@RF00426 DS    0H                                               01S0428 00766000
         STCOM YES                                                      00767000
*        /************************************************************/ 00768000
*        /* LINE DELETE AND CHARACTER DELETE CHARACTERS  IN THE UPT  */ 00769000
*        /* ARE USED TO INITIALIZE THE USER'S PROFILE TO THAT WHICH  */ 00770000
*        /* WAS  IN EFFECT AT THE END OF THE LAST TERMINAL SESSION.  */ 00771000
*        /************************************************************/ 00772000
*                                                               01S0429 00773000
*        RESPECIFY (R2,R3)          /* LINE DELETE CHARACTER AND     */ 00774000
*            RESTRICTED;            /*  CHARACTER DELETE CHARACTER   */ 00775000
