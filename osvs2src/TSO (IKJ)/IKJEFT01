*    END; /*MODESET  KEYADDR(R2) WORKREG(R2)*/                  01S0463 00866000
*    RESPECIFY (R0,R2) UNRSTD;                                  01S0464 00867000
*    /****************************************************************/ 00868000
*    /* INVOKE THE STACK SERVICE ROUTINE  TO INITIALIZE  THE FIRST   */ 00869000
*    /* STACK ELEMENT AS A TERMINAL ELEMENT. AN 8-BYTE PLACE HOLDER  */ 00870000
*    /* IS GOTTEN IN SP 78 TO PREVENT CORE FRAGMENTATION.  IT IS     */ 00871000
*    /* THEN FREED.                                                  */ 00872000
*    /****************************************************************/ 00873000
*                                                               01S0465 00874000
*STACK:  /* INITIALIZE THE FIRST STACK ELEMENT                       */ 00875000
*                                                               01S0465 00876000
*    LOADNOW = NO;                  /* CLEAR LOAD ACTIVE FLAG        */ 00877000
STACK    NI    LOADNOW(WRKAPTR1),B'10111111'                    01S0465 00878000
*    RESPECIFY (R0,R1,R2) RSTD;                                 01S0466 00879000
