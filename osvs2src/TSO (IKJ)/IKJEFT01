*        R2 = UPTLDEL;              /* GET LINE DELETE CHAR          */ 00777000
         L     @07,UPTPTR(,WRKAPTR2)                            01S0430 00778000
         SLR   R2,R2                                            01S0430 00779000
         IC    R2,UPTLDEL(,@07)                                 01S0430 00780000
*        R3 = UPTCDEL;              /* GET CHARACTER DELETE CHAR     */ 00781000
*                                                               01S0431 00782000
         SLR   R3,R3                                            01S0431 00783000
         IC    R3,UPTCDEL(,@07)                                 01S0431 00784000
*        /* CHECK UPT FOR PREVIOUS LINE DELETE CHARACTER             */ 00785000
*        IF UPTALD='1'B             /* CHK IF ATTENTION WAS USED     */ 00786000
*          THEN DO;                 /* YES,                          */ 00787000
         TM    UPTALD(@07),B'00000100'                          01S0432 00788000
         BNO   @RF00432                                         01S0432 00789000
*            R2 = EFFCHAR;          /*   TURN OFF LINE DELETE CHAR   */ 00790000
         LA    R2,255                                           01S0434 00791000
*            GEN(STCC   ATTN,LD=(R2),CD=(R3)); /* USE ATTN CHAR      */ 00792000
         STCC   ATTN,LD=(R2),CD=(R3)                                    00793000
*          END;                                                 01S0436 00794000
*          ELSE DO;                 /* ATTN NOT SPECIFIED AS LINE DL */ 00795000
         B     @RC00432                                         01S0437 00796000
@RF00432 DS    0H                                               01S0438 00797000
*            IF UPTLDEL = CHAR0     /* LEAVE LINE DELETE AS IS       */ 00798000
*              THEN                 /* YES,                          */ 00799000
         L     @01,UPTPTR(,WRKAPTR2)                            01S0438 00800000
         CLI   UPTLDEL(@01),X'00'                               01S0438 00801000
         BNE   @RF00438                                         01S0438 00802000
*                GEN(STCC    LD=(R2),CD=(R3));                  01S0439 00803000
         STCC    LD=(R2),CD=(R3)                                        00804000
*              ELSE                 /* NO,                           */ 00805000
*                GEN(STCC NATN,LD=(R2),CD=(R3));  /* NOT ATTENTION   */ 00806000
         B     @RC00438                                         01S0440 00807000
@RF00438 DS    0H                                               01S0440 00808000
