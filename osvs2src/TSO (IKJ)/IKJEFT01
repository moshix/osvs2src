         L     @06,STPBPTR(,WRKAPTR1)                           01S0471 00891000
         ST    @06,IOPLIOPB(,@07)                               01S0471 00892000
*    R15 = ADDR(TSTACK0);           /* PTR TO STACK ROUTINE          */ 00893000
         LA    R15,TSTACK0                                      01S0472 00894000
*    GEN FLOWS(TSTACK0) (SYNCH  (15));  /* INVOKE STACK              */ 00895000
*                                                               01S0473 00896000
         SYNCH  (15)                                                    00897000
*    R0 = SPL1*SHAREDSP+D8;         /* INDICATE SHARED SUBPOOL 78    */ 00898000
         L     R0,@CF03535                                      01S0474 00899000
*    GEN(FREEMAIN R,LV=(R0),A=(R2));  /* ISSUE FREEMAIN              */ 00900000
         FREEMAIN R,LV=(R0),A=(R2)                                      00901000
*    RESPECIFY (R0,R1,R2) UNRSTD;                               01S0476 00902000
*                                                               01S0476 00903000
*    /* CHECK RETURN CODE FOR NORMAL STACK COMPLETION                */ 00904000
*    IF BKGMODE = BKGDNOW           /* CHK FOR BACKGROUND MODE       */ 00905000
*        &( RCODE = D8              /*   & INDD OPEN FAILURE         */ 00906000
*          |RCODE = D12)            /*     OR OUTDD OPEN FAILURE     */ 00907000
*       THEN DO;                    /* YES, OPEN FAILURE             */ 00908000
         TM    BKGMODE(WRKAPTR2),B'10000000'                    01S0477 00909000
         BNO   @RF00477                                         01S0477 00910000
         L     @02,RCODE(,WRKAPTR1)                             01S0477 00911000
         C     @02,@CF00049                                     01S0477 00912000
         BE    @RT00477                                         01S0477 00913000
         C     @02,@CF00079                                     01S0477 00914000
         BNE   @RF00477                                         01S0477 00915000
@RT00477 DS    0H                                               01S0478 00916000
*         ECTLOGF = YES;            /* SET TERMINATION FLAG          */ 00917000
         L     @02,ECTPTR(,WRKAPTR2)                            01S0479 00918000
         OI    ECTLOGF(@02),B'00010000'                         01S0479 00919000
*         ECTRTCD = RCODE;          /* RESERVE EXIT RETURN CODE      */ 00920000
         MVC   ECTRTCD(3,@02),RCODE+1(WRKAPTR1)                 01S0480 00921000
*         DODONE = YES;             /* SET GO-TO-EXIT FLAG           */ 00922000
         OI    DODONE(WRKAPTR1),B'00001000'                     01S0481 00923000
*       END;                                                    01S0482 00924000
*       ELSE                        /* NOT BACKGROUND OPEN FAILURE   */ 00925000
*         IF RCODE ^= STDONE        /* CHK FOR SUCCESSFUL STACK      */ 00926000
*           THEN                    /* NO,                           */ 00927000
         B     @RC00477                                         01S0483 00928000
@RF00477 L     @02,RCODE(,WRKAPTR1)                             01S0483 00929000
         LTR   @02,@02                                          01S0483 00930000
         BNZ   @RT00483                                         01S0483 00931000
*             GO TO STCKERR;        /* INTERFACE WITH STAE EXIT RTN  */ 00932000
*    /****************************************************************/ 00933000
*    /* THE FIRST COMMAND (POSSIBLY NULL) TO BE EXECUTED IS PASSED   */ 00934000
*    /* TO THE TMP FROM THE PARM FIELD OF THE USER'S LOGON PROCE-    */ 00935000
*    /* DURE.  CORE IS OBTAINED FROM SUBPOOL 1  AND  THE COMMAND IS  */ 00936000
*    /* COPIED INTO THE SCAN INPUT BUFFER.  THE TPLCBUF POINTER IS   */ 00937000
*    /* INITIALIZED TO POINT AT AN ALL ZERO WORD WHICH LOOKS LIKE A  */ 00938000
*    /* COMMAND BUFFER OF ZERO LENGTH.                               */ 00939000
*    /****************************************************************/ 00940000
*                                                               01S0485 00941000
*    RESPECIFY (R0,R1) RSTD;                                    01S0485 00942000
@RC00477 DS    0H                                               01S0486 00943000
*    R0 = SPL1                      /* INSERT SUBPOOL NUMBER         */ 00944000
*         + FCMDLEN + D4;           /* ADD LEN OF BUFFER + HEADER    */ 00945000
         L     R0,@CF03536                                      01S0486 00946000
         L     @02,FSTCMDP                                      01S0486 00947000
         AH    R0,FCMDLEN(,@02)                                 01S0486 00948000
*    GEN(GETMAIN R,LV=(0));         /* GET BUFFER AREA IN SUBPOOL 1  */ 00949000
         GETMAIN R,LV=(0)                                               00950000
*    CSPLCBUF = R1;                 /* SET UP SCAN INPUT BUFFER      */ 00951000
         L     @02,SRPLPTR(,WRKAPTR1)                           01S0488 00952000
         ST    R1,CSPLCBUF(,@02)                                01S0488 00953000
*    RESPECIFY (R0,R1) UNRSTD;                                  01S0489 00954000
*                                                               01S0489 00955000
*    CSPLCBUF->CBUFLNG =            /* CMD BUFFER LENGTH FIELD       */ 00956000
*        FCMDLEN + D4;              /* LEN OF INPUT PARM + HEADER    */ 00957000
         L     @02,SRPLPTR(,WRKAPTR1)                           01S0490 00958000
         L     @02,CSPLCBUF(,@02)                               01S0490 00959000
         L     @07,FSTCMDP                                      01S0490 00960000
         LH    @06,FCMDLEN(,@07)                                01S0490 00961000
         LA    @05,4                                            01S0490 00962000
         ALR   @05,@06                                          01S0490 00963000
         STH   @05,CBUFLNG(,@02)                                01S0490 00964000
*    CSPLCBUF->CBUFOFF = ZERO;      /* SET OFFSET FIELD TO ZERO      */ 00965000
*                                                               01S0491 00966000
         SLR   @05,@05                                          01S0491 00967000
         STH   @05,CBUFOFF(,@02)                                01S0491 00968000
*    /* CHECK LENGTH OF INPUT PARM FIELD                             */ 00969000
*    IF FCMDLEN=ZERO                                            01S0492 00970000
*      THEN                         /* THERE IS NO DATA TO MOVE      */ 00971000
         CR    @06,@05                                          01S0492 00972000
         BE    @RT00492                                         01S0492 00973000
*        GO TO FCMD001;             /* CONTINUE WITH INITIALIZATION  */ 00974000
*                                                               01S0493 00975000
*    CSPLCBUF->CBUFDATA(1:FCMDLEN) = FCMDDATA;  /* MOVE DATA         */ 00976000
*                                                               01S0494 00977000
         BCTR  @06,0                                            01S0494 00978000
         EX    @06,@SM03557                                     01S0494 00979000
*FCMD001:  /* SET TPL POINTER TO DUMMY BUFFER                        */ 00980000
*    TPLCBUF = ADDR(TMPZEROS);      /* POINT TO ZERO LEN CMD BUFFER  */ 00981000
FCMD001  LA    @02,TMPZEROS(,WRKAPTR1)                          01S0495 00982000
         ST    @02,TPLCBUF(,WRKAPTR1)                           01S0495 00983000
*    /****************************************************************/ 00984000
*    /* LOAD THE TMP STAI EXIT AND THE TMP ATTENTION EXIT  ROUTINES  */ 00985000
*    /* INTO CORE.  SAVE THE ADDRESS OF THE STAI EXIT ROUTINE IN THE */ 00986000
*    /* TMP WORKAREA FOR USE WHEN ATTACHING COMMAND PROCESSORS.      */ 00987000
*    /****************************************************************/ 00988000
*                                                               01S0496 00989000
*    GEN(LOAD  EP=IKJEFT04);        /* LOAD STAI EXIT ROUTINE        */ 00990000
         LOAD  EP=IKJEFT04                                              00991000
*    DO; /*MODESET  SAVEKEY(R2) KEYADDR(SPROK1) WORKREG(R2)*/   01S0497 00992000
*      RESPECIFY (GPR02F) RESTRICTED;                           01S0498 00993000
*      R2 = GPR02F; /*SAVE REGISTER 2*/                         01S0499 00994000
*      IPK; /*SAVE CURRENT PSW KEY*/                            01S0500 00995000
         IPK                                                    01S0500 00996000
*      R2 = GPR02F;                                             01S0501 00997000
*      GPR02F = R2; /*RESTORE REGISTER 2*/                      01S0502 00998000
*      SPKA (SPROK1); /*SET PSW KEY*/                           01S0503 00999000
         L     @01,SPROK1                                       01S0503 01000000
         SPKA  0(@01)                                           01S0503 01001000
*      RESPECIFY (GPR02F) UNRESTRICTED;                         01S0504 01002000
*    END; /*MODESET  SAVEKEY(R2) KEYADDR(SPROK1) WORKREG(R2)*/  01S0505 01003000
*    RESPECIFY (R0,R2) RSTD;                                    01S0506 01004000
*    TMPT04 = R0;                   /* SAVE ADDR OF STAI EXIT RTN    */ 01005000
         ST    R0,TMPT04(,WRKAPTR2)                             01S0507 01006000
*    /* RESTORE ENTRY PROTECT KEY                                    */ 01007000
*    DO; /*MODESET  KEYADDR(R2) WORKREG(R2)*/                   01S0508 01008000
*      R2 = R2; /*GET KEY FORM SAVE LOCATION*/                  01S0509 01009000
*      SPKA (R2); /*SET PSW KEY*/                               01S0510 01010000
         SPKA  0(R2)                                            01S0510 01011000
*    END; /*MODESET  KEYADDR(R2) WORKREG(R2)*/                  01S0511 01012000
*    GEN(LOAD  EP=IKJEFT0D);        /* GET SECOND ENTRY POINT        */ 01013000
         LOAD  EP=IKJEFT0D                                              01014000
*    DO; /*MODESET  SAVEKEY(R2) KEYADDR(SPROK1) WORKREG(R2)*/   01S0513 01015000
*      RESPECIFY (GPR02F) RESTRICTED;                           01S0514 01016000
*      R2 = GPR02F; /*SAVE REGISTER 2*/                         01S0515 01017000
*      IPK; /*SAVE CURRENT PSW KEY*/                            01S0516 01018000
         IPK                                                    01S0516 01019000
*      R2 = GPR02F;                                             01S0517 01020000
*      GPR02F = R2; /*RESTORE REGISTER 2*/                      01S0518 01021000
*      SPKA (SPROK1); /*SET PSW KEY*/                           01S0519 01022000
         L     @01,SPROK1                                       01S0519 01023000
         SPKA  0(@01)                                           01S0519 01024000
*      RESPECIFY (GPR02F) UNRESTRICTED;                         01S0520 01025000
*    END; /*MODESET  SAVEKEY(R2) KEYADDR(SPROK1) WORKREG(R2)*/  01S0521 01026000
*    RESPECIFY (R0,R2) RSTD;                                    01S0522 01027000
*    TMPT042 = R0;                  /* SAVE ADDR2 OF STAI EXIT RTN   */ 01028000
         ST    R0,TMPT042(,WRKAPTR2)                            01S0523 01029000
*    /* RESTORE ENTRY PROTECT KEY                                    */ 01030000
*    DO; /*MODESET  KEYADDR(R2) WORKREG(R2)*/                   01S0524 01031000
*      R2 = R2; /*GET KEY FORM SAVE LOCATION*/                  01S0525 01032000
*      SPKA (R2); /*SET PSW KEY*/                               01S0526 01033000
         SPKA  0(R2)                                            01S0526 01034000
*    END; /*MODESET  KEYADDR(R2) WORKREG(R2)*/                  01S0527 01035000
*    TPLSTAI = TMPT042;             /* SET STAI EXIT PTR IN TEST PL  */ 01036000
         L     @01,TMPT042(,WRKAPTR2)                           01S0528 01037000
         ST    @01,TPLSTAI(,WRKAPTR1)                           01S0528 01038000
*    TPLSPLS = WRKAPTR2;            /* SET STAI PL PTR IN TEST PL    */ 01039000
*                                                               01S0529 01040000
         ST    WRKAPTR2,TPLSPLS(,WRKAPTR1)                      01S0529 01041000
*    IF BKGMODE = FGRDNOW           /* CHK FOR FOREGROUND MODE       */ 01042000
*      THEN DO;                     /* YES, SET ATTN ENVIRONMENT     */ 01043000
         TM    BKGMODE(WRKAPTR2),B'10000000'                    01S0530 01044000
         BNZ   @RF00530                                         01S0530 01045000
*        GEN(LOAD EP=IKJEFT03);     /* LOAD ATTN EXIT ROUTINE        */ 01046000
         LOAD EP=IKJEFT03                                               01047000
*        RESPECIFY (R1) RSTD;       /* ADDR OF ATTN EXIT ROUTINE     */ 01048000
*        R2 = R0;                   /* SAVE ATTN EXIT ADDRESS        */ 01049000
*                                                               01S0534 01050000
         LR    R2,R0                                            01S0534 01051000
*        /************************************************************/ 01052000
*        /* ESTABLISH THE TMP ATTN EXIT ROUTINE BY ISSUING THE STAX  */ 01053000
*        /* MACRO.  THE STAX PARAMETER LIST  AND  TMP MODE MESSAGE   */ 01054000
*        /* MUST NOT BE FREED OR MOVED DURING THE TERMINAL SESSION.  */ 01055000
*        /************************************************************/ 01056000
*                                                               01S0535 01057000
*        R1 = STAXPPTR;             /* SET UP STAX PARAMETERS        */ 01058000
         L     @07,STAXPPTR(,WRKAPTR1)                          01S0535 01059000
         LR    R1,@07                                           01S0535 01060000
*        STAXSP(1:STAXPLEN) = STAXPBLK;                         01S0536 01061000
*                                                               01S0536 01062000
         LH    @06,STAXPLEN                                     01S0536 01063000
         BCTR  @06,0                                            01S0536 01064000
         EX    @06,@SM03559                                     01S0536 01065000
*        /* ISSUE  THE STAX MACRO  TO ESTABLISH  THE TMP             */ 01066000
*        /* ATTENTION  EXIT.                                 Z30NQKM */ 01067000
*        GENERATE;                                              01S0537 01068000
*                                                               Z30NQKM 01069000
          STAX (R2),USADDR=(WRKAPTR2),MF=(E,(1))                        01070000
*        RESPECIFY (R0,R1,R2) UNRSTD;                           01S0538 01071000
*        RCODE = R15;               /* SAVE RETURN CODE              */ 01072000
*                                                               01S0539 01073000
         ST    R15,RCODE(,WRKAPTR1)                             01S0539 01074000
*        /* CHECK RETURN CODE FOR NORMAL STAX COMPLETION             */ 01075000
*        IF RCODE^=STXDONE                                      01S0540 01076000
*          THEN                     /* A STAX ERROR HAS OCCURRED     */ 01077000
         L     @02,RCODE(,WRKAPTR1)                             01S0540 01078000
         LTR   @02,@02                                          01S0540 01079000
         BNZ   @RT00540                                         01S0540 01080000
*            GO TO STAXERR;           /* INTERFACE FOR STAE EXIT RTN */ 01081000
*      END;                                                     01S0542 01082000
*    /****************************************************************/ 01083000
*    /* SET UP TO  XCTL  TO THE TMP MAINLINE ROUTINE.  THE COMPILER  */ 01084000
*    /* WILL FREE  THE DYNAMIC AREA  WHICH IT OBTAINED,  BUT THE TMP */ 01085000
*    /* DYNAMIC WORKAREA IS PASSED  AS A PARAMETER  TO THE MAINLINE  */ 01086000
*    /* ROUTINE.                                                     */ 01087000
*    /****************************************************************/ 01088000
*                                                               01S0543 01089000
*    REG1 = WRKAPTR2;               /* SET UP PARM FOR IKJEFT02      */ 01090000
@RF00530 L     @02,SAVRBAK(,R13)                                01S0543 01091000
         ST    WRKAPTR2,REG1(,@02)                              01S0543 01092000
*    REG15 = ADDR(BLDLNAME);        /* STORE PTR TO XCTL LIST        */ 01093000
         LA    @07,BLDLNAME(,WRKAPTR1)                          01S0544 01094000
         ST    @07,REG15(,@02)                                  01S0544 01095000
*    XCTLPL = XCTLPM;               /* MOVE IN LIST                  */ 01096000
         MVC   XCTLPL(8,WRKAPTR1),XCTLPM                        01S0545 01097000
*    RETURN TO XCTL;                /* CLEAN UP BEFORE XCTL          */ 01098000
         L     @13,4(,@13)                                      01S0546 01099000
         L     @00,@SIZDATD                                     01S0546 01100000
         LR    @01,@12                                          01S0546 01101000
         FREEMAIN R,LV=(0),A=(1)                                        01102000
         LA    @14,XCTL                                         01S0546 01103000
         LM    @15,@12,16(@13)                                  01S0546 01104000
         BR    @14                                              01S0546 01105000
*XCTL: /* THIS CODE  IS ENTERED  AFTER THE PLS COMPILER EPILOGUE, TO */ 01106000
*    /* TO ACTUALLY PERFORM THE XCTL.                                */ 01107000
*                                                               01S0547 01108000
*    GENERATE;                                                  01S0547 01109000
*                                                               01S0547 01110000
*                                                               01S0547 01111000
*                                                               01S0547 01112000
*                                                               01S0547 01113000
XCTL     DS    0H                                               01S0547 01114000
          L     R14,@12(R13)        LOAD REG 14 FROM REG SAVE AREA      01115000
          XCTL  MF=(E,(1)),SF=(E,(15)) MAINLINE TMP ROUTINE             01116000
*/********************************************************************/ 01117000
*/* THESE USER ABENDS ARE THE METHOD OF INTERFACING WITH THE TMP     */ 01118000
*/*  STAE EXIT ROUTINE FOR ERROR HANDLING.                           */ 01119000
*/********************************************************************/ 01120000
*                                                               01S0548 01121000
*/*LOADERR: GEN(ABEND 101,DUMP);    ** LOAD ERROR                    */ 01122000
*                                                               01S0548 01123000
*STCKERR:   GEN(ABEND 102,DUMP);    /* STACK ERROR                   */ 01124000
*                                                               01S0548 01125000
STCKERR  ABEND 102,DUMP                                                 01126000
*STAXERR:   GEN(ABEND 103,DUMP);    /* STAX ERROR                    */ 01127000
*                                                               01S0549 01128000
STAXERR  ABEND 103,DUMP                                                 01129000
*EFLRERR:   GEN(ABEND 104,DUMP);    /* BKGD PSCB-UPT BUILD ERROR     */ 01130000
*                                                               01S0550 01131000
EFLRERR  ABEND 104,DUMP                                                 01132000
*PUTLERR:   GEN(ABEND 105,DUMP);    /* PUTLINE ERROR                 */ 01133000
PUTLERR  ABEND 105,DUMP                                                 01134000
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */ 01135000
*/*                                                                  */ 01136000
*/*                                                                  */ 01137000
*/*                                                                  */ 01138000
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */ 01139000
*                                                               01S0552 01140000
*TDUMX1:    /* EP TO DUMMY PROCEDURE TO CONTAIN PLS EXIT CODE        */ 01141000
*    PROCEDURE                                                  01S0552 01142000
*       OPTIONS(                                                01S0552 01143000
*       NOSAVE                                                  01S0552 01144000
*       NOSAVEAREA);                                            01S0552 01145000
*                                                               01S0552 01146000
*                                                               01S0552 01147000
@EL00001 L     @13,4(,@13)                                      01S0552 01148000
@EF00001 L     @00,@SIZDATD                                     01S0552 01149000
         LR    @01,@12                                          01S0552 01150000
         FREEMAIN R,LV=(0),A=(1)                                        01151000
@ER00001 LM    @14,@12,12(@13)                                  01S0552 01152000
         BR    @14                                              01S0552 01153000
TDUMX1   DS    0H                                               01S0553 01154000
*    END  TDUMX1;                   /* RETURN TO CALLER              */ 01155000
@EL00002 DS    0H                                               01S0553 01156000
@EF00002 DS    0H                                               01S0553 01157000
@ER00002 BR    @14                                              01S0553 01158000
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */ 01159000
*/*                                                                  */ 01160000
*/* SEGMENT NAME -- BKGBLDR                                          */ 01161000
*/*                                                                  */ 01162000
*/* DESCRIPTIVE NAME -- TOP SEGMENT                                  */ 01163000
*/*                                                                  */ 01164000
*/* FUNCTION -- BKGBLDR EXECUTOR ROUTINE                             */ 01165000
*/*  OPERATION --                                                    */ 01166000
*/*           - GET SPACE (IN BACKGROUND MODE) FOR UPT, PSCB, RLGB,  */ 01167000
*/*             AND LWA                                              */ 01168000
*/*           - INITIALIZE BLOCKS WITH INFORMATION FROM UADS IF IT   */ 01169000
*/*             IS AVAILABLE, OTHERWISE WITH DEFAULT VALUES          */ 01170000
*/*                                                                  */ 01171000
*/* ENTRY -- LINKED BY REGISTER 14                                   */ 01172000
*/*                                                                  */ 01173000
*/* INPUT -- REGISTER 1 POINTS TO A FIELD TO RECEIVE THE POINTER TO  */ 01174000
*/*          THE DEVELOPED PSCB                                      */ 01175000
*/*                                                                  */ 01176000
*/* OUTPUT -- INITIALIZED UPT, PSCB, RLGB AND LWA                    */ 01177000
*/*                                                                  */ 01178000
*/* EXTERNAL REFERENCES -- N/A                                       */ 01179000
*/*                                                                  */ 01180000
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */ 01181000
*/*   RETURN CODE -- NORMAL                                          */ 01182000
*/*          ZERO -- 0, ALL SUCCESSFUL, USERID FOUND                 */ 01183000
*/*          RCNOUID -- 4, NO USERID AVAILABLE, DEFAULT BLOCKS ARE   */ 01184000
*/*                     INITIALIZED                                  */ 01185000
*/*          RCNOTTSO -- 8, USERID SUPPLIED IS NOT TSO USER, DEFAULT */ 01186000
*/*                     BLOCKS ARE INITIALIZED                       */ 01187000
*/* EXIT ERROR -- RETURN BY LINK REGISTER 14                         */ 01188000
*/*   CONDITIONS -- GETMAIN FAILURE 0R I/0 ERROR WITH UADS           */ 01189000
*/*   RETURN CODE -- ERROR                                           */ 01190000
*/*          RCUADERR -- 12, ERROR IN ACCESSING UADS                 */ 01191000
*/*          RCTERM -- 16, FAILURE IN GETMAIN FOR PSCB/UPT/LWA/RLGB  */ 01192000
*/*                                                                  */ 01193000
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */ 01194000
*                                                               01S0554 01195000
*BKGBLDR:   /* EP TO PSCB/UPT/RLGB/LWA CONSTRUCTOR                   */ 01196000
*    PROCEDURE           ;                                      01S0554 01197000
*                                                               01S0554 01198000
BKGBLDR  STM   @14,@12,@SA00003                                 01S0554 01199000
*    RESPECIFY (TCB)  BASED(TCURTCB);                           01S0555 01200000
*    RESPECIFY (IEZJSCB) BASED(TCBJSCB);                        01S0556 01201000
*    RESPECIFY (ASCB) BASED(TCUASCB);                           01S0557 01202000
*    RESPECIFY (ASXB) BASED(ASCBASXB);                          01S0558 01203000
*    RESPECIFY (LWA)  BASED(ASXBLWA);                           01S0559 01204000
*    RESPECIFY (RLGB) BASED(PSCBRLGB);                          01S0560 01205000
*    RESPECIFY (PSCB) BASED(PSCBPTR);                           01S0561 01206000
*    RESPECIFY (UPT)  BASED(UPTPTR);                            01S0562 01207000
*                                                               01S0562 01208000
*IKJEFLR:   /* EP (LOGICAL) TO PSCB/UPT/RLGB/LWA CONSTRUCTOR         */ 01209000
*    ENTRY;                                                     01S0563 01210000
*                                                               01S0563 01211000
         B     @EC00563                                         01S0563 01212000
IKJEFLR  STM   @14,@12,@SA00003                                 01S0563 01213000
@EC00563 DS    0H                                               01S0564 01214000
*    CALL INIT;                      /* INITIALIZATION FOR BKGBLDR   */ 01215000
*                                                               01S0564 01216000
         BAL   @14,INIT                                         01S0564 01217000
*    IF ASXBUSER ^= ZEROES           /* CHK IF USERID SUPPLIED       */ 01218000
*      THEN DO;                      /* YES,                         */ 01219000
         L     @02,CVTPTR                                       01S0565 01220000
         L     @02,CVTTCBP(,@02)                                01S0565 01221000
         L     @02,TCUASCB(,@02)                                01S0565 01222000
         L     @02,ASCBASXB(,@02)                               01S0565 01223000
         CLC   ASXBUSER(7,@02),@CB00154                         01S0565 01224000
         BE    @RF00565                                         01S0565 01225000
*        CALL CHKUSER;               /*   TO CHK USERID VALIDITY     */ 01226000
         BAL   @14,CHKUSER                                      01S0567 01227000
*        CALL INITUP;                /* INITIALIZE UPT AND PSCB      */ 01228000
         BAL   @14,INITUP                                       01S0568 01229000
*      END;                                                     01S0569 01230000
*                                                               01S0569 01231000
*    IF ASXBUSER = ZEROES            /* CHK FOR NO USERID SUPPLIED   */ 01232000
*       | STATUS ^= CONTINUE         /*   OR QUIT FLAG               */ 01233000
*     THEN                           /* YES,                         */ 01234000
@RF00565 L     @02,CVTPTR                                       01S0570 01235000
         L     @02,CVTTCBP(,@02)                                01S0570 01236000
         L     @02,TCUASCB(,@02)                                01S0570 01237000
         L     @02,ASCBASXB(,@02)                               01S0570 01238000
         CLC   ASXBUSER(7,@02),@CB00154                         01S0570 01239000
         BE    @RT00570                                         01S0570 01240000
         L     @02,STATUS                                       01S0570 01241000
         LTR   @02,@02                                          01S0570 01242000
         BZ    @RF00570                                         01S0570 01243000
@RT00570 DS    0H                                               01S0571 01244000
*       CALL  DUMMYUP;               /*   INITIALIZE DEFAULT BLOCKS  */ 01245000
*                                                               01S0571 01246000
         BAL   @14,DUMMYUP                                      01S0571 01247000
*   CALL CHAINUP;                    /* CHAIN UPT & PSCB FOR TMP     */ 01248000
*                                                               01S0572 01249000
@RF00570 BAL   @14,CHAINUP                                      01S0572 01250000
*   CALL CLEANUP;                    /* CLEANUP BEFORE TERMINATION   */ 01251000
*                                                               01S0573 01252000
         BAL   @14,CLEANUP                                      01S0573 01253000
*   RETURN CODE(RETCODE);                                       01S0574 01254000
         L     @15,RETCODE                                      01S0574 01255000
         L     @14,@SA00003                                     01S0574 01256000
         LM    @00,@12,@SA00003+8                               01S0574 01257000
         BR    @14                                              01S0574 01258000
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */ 01259000
*/*                                                                  */ 01260000
*/* SEGMENT NAME -- INIT                                             */ 01261000
*/*                                                                  */ 01262000
*/* DESCRIPTIVE NAME -- BKGBLDR INITIALIZER - PART 1                 */ 01263000
*/*                                                                  */ 01264000
*/* FUNCTION -- TO ESTABLISH AND INITIALIZE POINTERS AND CONTROL     */ 01265000
*/*       AREAS USED BY BKGBLDR                                      */ 01266000
*/*  OPERATION --                                                    */ 01267000
*/*           - INITIALIZE MACRO LIST FORMS AND OTHER VARIABLES      */ 01268000
*/*           - OBTAIN THE NAME OF THE UADS FROM SECURITY CONTROL    */ 01269000
*/*             BLOCKS                                               */ 01270000
*/*           - CALL ROUTINE (OPENUADS) TO OPEN THE UADS             */ 01271000
*/*                                                                  */ 01272000
*/* ENTRY -- LINKED BY REGISTER 14                                   */ 01273000
*/*                                                                  */ 01274000
*/* INPUT -- N/A                                                     */ 01275000
*/*                                                                  */ 01276000
*/* OUTPUT -- INITIALIZED ENVIRONMENT FOR BKGBLDR                    */ 01277000
*/*                                                                  */ 01278000
*/* EXTERNAL REFERENCES -- N/A                                       */ 01279000
*/*                                                                  */ 01280000
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */ 01281000
*/*   RETURN CODE -- STATUS = CONTINUE, 0                            */ 01282000
*/*                                                                  */ 01283000
*/* EXIT ERROR -- RETURN BY LINK REGISTER 14                         */ 01284000
*/*   CONDITIONS -- FAILURE IN ALLOCATION OR OPEN                    */ 01285000
*/*   RETURN CODE -- STATUS = QUIT (NON-ZERO)                        */ 01286000
*/*                                                                  */ 01287000
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */ 01288000
*INIT:  /* EP TO INITIALIZE FOR BKGBLDR                              */ 01289000
*                                                               01S0575 01290000
*    PROCEDURE   ;                                              01S0575 01291000
*                                                               01S0575 01292000
*                                                               01S0575 01293000
INIT     STM   @14,@12,@SA00004                                 01S0575 01294000
*    RETCODE = ZERO;                 /* INITIALIZE RETURN CODE AREA  */ 01295000
         SLR   @02,@02                                          01S0576 01296000
         ST    @02,RETCODE                                      01S0576 01297000
*    STATUS   = CONTINUE;            /* STATUS IS CONTINUE           */ 01298000
         ST    @02,STATUS                                       01S0577 01299000
*    UADSBUFP = ZERO;                /* NO UADS BUFFER YET           */ 01300000
         ST    @02,UADSBUFP                                     01S0578 01301000
*    UADSDPTR = ZERO;                /* NO UADS DATA BLOCKS YET      */ 01302000
         ST    @02,UADSDPTR                                     01S0579 01303000
*    CURBLOCK = ZERO;                /* NO UADS BLOCK YET            */ 01304000
         ST    @02,CURBLOCK                                     01S0580 01305000
*    NEWBLOCK = ZERO;                /* NO NEW BLOCK WANTED, YET     */ 01306000
         ST    @02,NEWBLOCK                                     01S0581 01307000
*    USERIDBP = ZERO;                /* PTR TO BLANK AFTER USERID    */ 01308000
         ST    @02,USERIDBP                                     01S0582 01309000
*    USERIDL  = ZERO;                /* LENGTH OF USERID             */ 01310000
         ST    @02,USERIDL                                      01S0583 01311000
*    USERID   = BLANK8;              /* NO USERID YET                */ 01312000
         MVC   USERID(8),@CC00050                               01S0584 01313000
*    DCBPTR = ADDR(DCB);             /* SET PTR TO DCB IN @DATD      */ 01314000
*                                                               01S0585 01315000
         LA    @07,DCB                                          01S0585 01316000
         ST    @07,DCBPTR(,WRKAPTR1)                            01S0585 01317000
*    $UPT     = ''B;                 /* ZERO OUT THE MODEL UPT       */ 01318000
         XC    $UPT(24),$UPT                                    01S0586 01319000
*    $PSCB    = ''B;                 /* ZERO OUT THE MODEL PSCB      */ 01320000
         XC    $PSCB(72),$PSCB                                  01S0587 01321000
*    RESPECIFY (R3,R4) RSTD;                                    01S0588 01322000
*    R3 = ADDR($UPT);                /* GET PTR TO MODEL UPT         */ 01323000
         LA    R3,$UPT                                          01S0589 01324000
*    R4 = ADDR($PSCB);               /* GET PTR TO MODEL PSCB        */ 01325000
         LA    R4,$PSCB                                         01S0590 01326000
*    DO; /*MODESET  KEYADDR(PROK1) WORKREG(R2)*/                01S0591 01327000
*      R2 = PROK1; /*GET KEY FORM SAVE LOCATION*/               01S0592 01328000
         LA    R2,16                                            01S0592 01329000
*      SPKA (R2); /*SET PSW KEY*/                               01S0593 01330000
         SPKA  0(R2)                                            01S0593 01331000
*    END; /*MODESET  KEYADDR(PROK1) WORKREG(R2)*/               01S0594 01332000
*    UPTPTR = R3;                    /* SET PTR TO MODEL UPT         */ 01333000
         ST    R3,UPTPTR(,WRKAPTR2)                             01S0595 01334000
*    PSCBPTR = R4;                   /* SET PTR TO MODEL PSCB        */ 01335000
         ST    R4,PSCBPTR(,WRKAPTR2)                            01S0596 01336000
*    DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/               01S0597 01337000
*      R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/              01S0598 01338000
         L     R2,TEPKEY(,WRKAPTR2)                             01S0598 01339000
*      SPKA (R2); /*SET PSW KEY*/                               01S0599 01340000
