*                 DO;  /*SETRP REGS(14,12)DUMP(YES)RECORD(NO)RC(16)*/   00727000
*                   RESPECIFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;       00728000
*                   GPR01P->SDWARCDE = 16;  /*SAVE RC VALUE*/   01S0338 00729000
