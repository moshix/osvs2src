*        SAVT044(2) = R13;          /* SET NEW BACK PTR              */ 00822000
         ST    R13,SAVT044+4                                    01S0371 00823000
*        SAVRFOR = R15;             /* SET OLD FORWARD PTR           */ 00824000
         ST    R15,SAVRFOR(,R13)                                01S0372 00825000
*        R13 = R15;                 /* SET NEW SAVE AREA PTR         */ 00826000
*                                                               01S0373 00827000
         LR    R13,R15                                          01S0373 00828000
*        RESPECIFY (R14) UNRESTRICTED;                          01S0374 00829000
*        IF POSTCODE = YES          /* TEST FOR ATTENTION            */ 00830000
*          THEN                     /* YES,                          */ 00831000
