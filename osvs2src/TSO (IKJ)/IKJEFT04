*               GO TO TRETURN;      /* TO RETURN TO ABEND            */ 00702000
         B     TRETURN                                          01S0326 00703000
*             END;                                              01S0327 00704000
*                                                               01S0327 00705000
*           R1 = REG1;              /* RESTORE PTR TO SDWA           */ 00706000
*                                                               01S0328 00707000
@RF00323 L     R1,REG1(,R13)                                    01S0328 00708000
*           /* IF ENTRY WAS NULL LINE, OR IN BACKGROUND MODE,        */ 00709000
*           /* TERMINATE WITH DUMP                                   */ 00710000
*           RESPECIFY (R2) RSTD;                                01S0329 00711000
*           RESPECIFY (CSOA) BASED(R2);                         01S0330 00712000
*           R2 = ADDR(CSOASP);                                  01S0331 00713000
         LA    R2,CSOASP(,WRKAPTR1)                             01S0331 00714000
*           IF CSOANOC = YES        /* CHK FOR NULL ENTRY            */ 00715000
*               | BKGMODE = BKGDNOW  /*   OR BACKGROUND MODE         */ 00716000
*             THEN                  /* YES,                          */ 00717000
         TM    CSOANOC(R2),B'00010000'                          01S0332 00718000
         BO    @RT00332                                         01S0332 00719000
         TM    BKGMODE(WRKAPTR2),B'10000000'                    01S0332 00720000
         BNO   @RF00332                                         01S0332 00721000
@RT00332 DS    0H                                               01S0333 00722000
*               DO;                                             01S0333 00723000
*                 RESPECIFY (R2) UNRSTD;                        01S0334 00724000
*                 TMPSCTRL = NO;    /* RESET STAI-IN-CONTROL OFF     */ 00725000
