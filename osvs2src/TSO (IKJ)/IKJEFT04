*             THEN DO;              /* NONE                          */ 00696000
         CLC   REG0(4,R13),@CF00035                             01S0323 00697000
         BNE   @RF00323                                         01S0323 00698000
*               REG15=NORECOVR;     /* SET FOR NO RETRY EXIT         */ 00699000
