         L     @07,TMPWA2P(,WRKAPTR2)                           01S0159 00127000
         ST    R1,GMLOC5(,@07)                                  01S0159 00128000
*              GEN(USING T05WA,R12);                            01S0160 00129000
         USING T05WA,R12                                                00130000
*            END;                                               01S0161 00131000
*                                                               01S0161 00132000
*        R10 = ADDR(ABMSGSP);  /* SET PTR TO MESSAGE BUILD AREA      */ 00133000
*                                                               01S0162 00134000
@RF00155 LA    R10,ABMSGSP(,WRKAPTR1)                           01S0162 00135000
*        IF SUBCODE > MAXCDE   /* CHK IF RESON CODE > FFF            */ 00136000
*          THEN                /* YES,                               */ 00137000
         L     @07,REG1(,R13)                                   01S0163 00138000
         CLC   SUBCODE(4,@07),@CF00072                          01S0163 00139000
         BNH   @RF00163                                         01S0163 00140000
*            SUBCDRS = ZERO;   /*   NOT SIGNIFICANT CODE             */ 00141000
         SLR   @07,@07                                          01S0164 00142000
         ST    @07,SUBCDRS(,R10)                                01S0164 00143000
*          ELSE                /* NOT GREATER, IS POSSIBLE CODE      */ 00144000
*            SUBCDRS=SUBCODE;  /* ERROR SUBCODE INTO WORK AREA       */ 00145000
*                                                               01S0165 00146000
         B     @RC00163                                         01S0165 00147000
@RF00163 L     @07,REG1(,R13)                                   01S0165 00148000
         L     @07,SUBCODE(,@07)                                01S0165 00149000
         ST    @07,SUBCDRS(,R10)                                01S0165 00150000
*        ABNDCD = R6;          /* REGISTER 6 TO ABEND CODE           */ 00151000
@RC00163 ST    R6,ABNDCD(,R10)                                  01S0166 00152000
*        RESPECIFY (R7) RESTRICTED;                             01S0167 00153000
*        GEN(SLL    R6,4);     /* DROP HIGH ORDER                    */ 00154000
         SLL    R6,4                                                    00155000
*        GEN(SRDL   R6,16);    /* SET SYSTEM ABEND CODE IN REG 6     */ 00156000
         SRDL   R6,16                                                   00157000
*        GEN(SRL    R7,20);    /* SET USER ABEND CODE IN REG 7       */ 00158000
         SRL    R7,20                                                   00159000
*        NONSCUR = NO;         /* RESET SECURITY FAILURE FLAG        */ 00160000
         NI    NONSCUR(WRKAPTR1),B'11111110'                    01S0171 00161000
*        IF (R6 = CD913        /* CHK FOR OPEN FAILURE CODE          */ 00162000
*           &(SUBCODE = CD38   /*   WITH REASON CODE 38              */ 00163000
*            |SUBCODE = CD3C   /*   OR REASON CODE 3C                */ 00164000
*            |SUBCODE = CD40)) /*   OR REASON CODE 40                */ 00165000
*          |(R6 = CD937        /* CHK FOR EOV FAILURE CODE           */ 00166000
*           &(SUBCODE = CD28   /*   WITH REASON CODE 28              */ 00167000
*            |SUBCODE = CD2C   /*   OR REASON CODE 2C                */ 00168000
*            |SUBCODE = CD30   /*   OR REASON CODE 30                */ 00169000
*            |SUBCODE = CD34   /*   OR REASON CODE 34                */ 00170000
*            |SUBCODE = CD38   /*   OR REASON CODE 38                */ 00171000
*            |SUBCODE = CD3C)) /*   OR REASON CODE 3C                */ 00172000
*          THEN                /* YES, IS SECURITY FAILURE           */ 00173000
         C     R6,@CF00056                                      01S0172 00174000
         BNE   @GL00001                                         01S0172 00175000
         L     @03,REG1(,R13)                                   01S0172 00176000
         L     @03,SUBCODE(,@03)                                01S0172 00177000
         C     @03,@CF00050                                     01S0172 00178000
         BE    @RT00172                                         01S0172 00179000
         C     @03,@CF00052                                     01S0172 00180000
         BE    @RT00172                                         01S0172 00181000
         C     @03,@CF00054                                     01S0172 00182000
         BE    @RT00172                                         01S0172 00183000
@GL00001 C     R6,@CF00058                                      01S0172 00184000
         BNE   @RF00172                                         01S0172 00185000
         L     @03,REG1(,R13)                                   01S0172 00186000
         L     @03,SUBCODE(,@03)                                01S0172 00187000
         C     @03,@CF00042                                     01S0172 00188000
         BE    @RT00172                                         01S0172 00189000
         C     @03,@CF00044                                     01S0172 00190000
         BE    @RT00172                                         01S0172 00191000
         C     @03,@CF00046                                     01S0172 00192000
         BE    @RT00172                                         01S0172 00193000
         C     @03,@CF00048                                     01S0172 00194000
         BE    @RT00172                                         01S0172 00195000
         C     @03,@CF00050                                     01S0172 00196000
         BE    @RT00172                                         01S0172 00197000
         C     @03,@CF00052                                     01S0172 00198000
         BNE   @RF00172                                         01S0172 00199000
@RT00172 DS    0H                                               01S0173 00200000
*            NONSCUR = YES;    /*   SET SECURITY FAILURE FLAG        */ 00201000
*                                                               01S0173 00202000
         OI    NONSCUR(WRKAPTR1),B'00000001'                    01S0173 00203000
*        IF R7 = ZERO          /* CHK IF ANY USER CODE               */ 00204000
*          THEN DO;            /* NO,                                */ 00205000
@RF00172 LTR   R7,R7                                            01S0174 00206000
         BNZ   @RF00174                                         01S0174 00207000
*            HOLCHAR = SCHAR;  /* RESERVE SYSTEM FLAG                */ 00208000
