         MVC   ECTRTCD(3,@10),SDWACMPC(@07)                     01S0137 00085000
*        ECTRCDF = ECTRCDF|HIORDR;  /* ABEND FLAG FOR CLIST          */ 00086000
*                                                               01S0138 00087000
         OI    ECTRCDF(@10),X'80'                               01S0138 00088000
*        RESPECIFY (R10,R12) RESTRICTED;                        01S0139 00089000
*        RESPECIFY (ABMSGAR) BASED(R10);                        01S0140 00090000
*        R2 = GMLNG+MACSP5L;   /* SET LENGTH OF REQUEST              */ 00091000
         LH    R2,MACSP5L                                       01S0141 00092000
         AL    R2,GMLNG                                         01S0141 00093000
*        GMLNG5 = R2;          /* RESERVE GETMAIN LENGTH             */ 00094000
         L     @07,TMPWA2P(,WRKAPTR2)                           01S0142 00095000
         ST    R2,GMLNG5(,@07)                                  01S0142 00096000
*        DO; /*GETMAIN (RC) LV(R2) SP(SPNUM) RTCD(R15)*/        01S0143 00097000
*        RESPECIFY (GPR01F,GPR15F,GPR00F) RESTRICTED;           01S0144 00098000
*        GPR01F = 0; /* REG 1 MUST BE ZERO*/                    01S0145 00099000
         SLR   GPR01F,GPR01F                                    01S0145 00100000
*        GPR15F = 0; /* RC-TYPE GETMAIN */                      01S0146 00101000
