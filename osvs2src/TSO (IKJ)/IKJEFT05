*                                                               01S0297 00545000
         ST    R15,REG15(,R13)                                  01S0297 00546000
*TRETURN:   /* RESTORE ENTRY ENVIRONMENT                             */ 00547000
*                                                               01S0298 00548000
*        R13 = SAVRBAK;        /* RESTORE ENTRY SAVE AREA PTR        */ 00549000
TRETURN  L     R13,SAVRBAK(,R13)                                01S0298 00550000
*        RESPECIFY (SAVARY) BASED(R13);                         01S0299 00551000
*        IF R14 = STAIWA       /* CHK IF SDWA SUPPLIED               */ 00552000
*          THEN DO;            /* NO, SDWA NOT SUPPLIED              */ 00553000
         C     R14,@CF00033                                     01S0300 00554000
         BNE   @RF00300                                         01S0300 00555000
*            GEN(LM   R14,R12,12(R13));  /* RESTORE ENTRY REGISTERS  */ 00556000
         LM   R14,R12,12(R13)                                           00557000
