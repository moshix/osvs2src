*                DOGETC = YES; /*   SET TO CONTINUE PROCESSING       */ 00328000
         OI    DOGETC(WRKAPTR1),B'00010000'                     01S0228 00329000
*              END;                                             01S0229 00330000
*            GO TO RETRYRTN;   /* GO TO EXIT                         */ 00331000
         B     RETRYRTN                                         01S0230 00332000
*          END;                                                 01S0231 00333000
*                                                               01S0232 00334000
*        /************************************************************/ 00335000
*COREIN: /* GET POINTER TO THE STAE EXIT PARAMETER LIST              */ 00336000
*        /************************************************************/ 00337000
*                                                               01S0232 00338000
*        I=ONE;                /* INITIALIZE INDEX TO 1              */ 00339000
*                                                               01S0232 00340000
@RF00218 DS    0H                                               01S0232 00341000
COREIN   MVC   I(2),@CH00037                                    01S0232 00342000
*        /************************************************************/ 00343000
*        /* THE USER ABEND CODE IS IN REGISTER 7.  THE SYSTEM ABEND  */ 00344000
*        /* CODE OR SVC CODE IS IN REGISTER 6                        */ 00345000
*        /************************************************************/ 00346000
*        IF R7 ^= ZERO         /* IS IT NOT A USER ABEND CODE        */ 00347000
*          THEN                /* IT IS A USER CODE                  */ 00348000
         LTR   R7,R7                                            01S0233 00349000
         BZ    @RF00233                                         01S0233 00350000
*            DO;                                                01S0234 00351000
*              RESPECIFY (R3) RESTRICTED;  /* BRANCH INDEX REGISTER  */ 00352000
*                                                               01S0235 00353000
*              /******************************************************/ 00354000
*USELOOP:      /* SEARCH THE USER ABEND CODE TABLE FOR THE CODE.     */ 00355000
*              /******************************************************/ 00356000
*              IF R7=USECODE(I)/* IS THE CODE IN THE USER TABLE?     */ 00357000
*                THEN          /* YES, IT IS IN THE TABLE            */ 00358000
USELOOP  LH    @15,I                                            01S0236 00359000
         SLA   @15,2                                            01S0236 00360000
         CH    R7,USECODE-4(@15)                                01S0236 00361000
         BNE   @RF00236                                         01S0236 00362000
*                  DO;                                          01S0237 00363000
*                    R3=USEACT(I);  /*INITIALIZE BRANCH REGISTER     */ 00364000
         LH    R3,USEACT-4(@15)                                 01S0238 00365000
*                    GO TO BRNCHTAB;  /* GO TO BRANCH TABLE          */ 00366000
         B     BRNCHTAB                                         01S0239 00367000
*                  END;                                         01S0240 00368000
*              IF USECODE(I)=ZERO   /* IS THIS THE END IF TABLE      */ 00369000
