         NI    SDWARCRD(GPR01P),B'01111111'                     01S0312 00577000
*                  GPR01P->SDWAREQ = '0'B;  /*TURN OFF DUMP INDICATOR*/ 00578000
         NI    SDWAREQ(GPR01P),B'01111111'                      01S0313 00579000
*                  GEN (RETURN (14,12));                        01S0314 00580000
         RETURN (14,12)                                                 00581000
*                  RESPECIFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;      00582000
*                END;                                           01S0316 00583000
*              END;                                             01S0317 00584000
*              ELSE DO;        /* YES, RETRY                         */ 00585000
         B     @RC00307                                         01S0318 00586000
@RF00307 DS    0H                                               01S0319 00587000
*                DO;  /*SETRP REGS(14,12)DUMP(NO)RECORD(NO)RC(4)RETADDR 00588000
*RETRYEP)*/                                                     01S0319 00589000
*                  RESPECIFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;        00590000
*                  GPR01P->SDWARCDE = 4; /*STORE RC INTO SDWA*/ 01S0321 00591000
         MVI   SDWARCDE(GPR01P),X'04'                           01S0321 00592000
*                  GPR01P->SDWARTYA = ADDR(RETRYEP);  /*SAVE RETRY ADDR 00593000
*SS*/                                                           01S0322 00594000
         L     @07,REG0(,R13)                                   01S0322 00595000
         ST    @07,SDWARTYA(,GPR01P)                            01S0322 00596000
*                  GPR01P->SDWARCRD = '0'B;  /*TURN OFF RECORD INDICATO 00597000
