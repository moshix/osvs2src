*                  RESPECIFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;      00604000
*                END;                                           01S0327 00605000
*              END;                                             01S0328 00606000
*          END;                                                 01S0329 00607000
@RC00307 DS    0H                                               01S0330 00608000
*          RESPECIFY  (SAVARY) BASED(SAVRBAK);                  01S0330 00609000
*          RESPECIFY (R14,R15) UNRSTD;                          01S0331 00610000
*                                                               01S0331 00611000
*                                                               01S0331 00612000
*        /************************************************************/ 00613000
*CMDNFND:  /* INDICATE COMMAND NOT FOUND AND GO TO RETRY ROUTINE     */ 00614000
*        /************************************************************/ 00615000
*        ABND806=YES;          /* SET COMMAND-NOT-FOUND FLAG         */ 00616000
CMDNFND  OI    ABND806(WRKAPTR1),B'00000100'                    01S0332 00617000
*        GOTO  RETRYRTN;       /* GO TO INDICATE RETRY ATTEMPT       */ 00618000
         B     RETRYRTN                                         01S0333 00619000
*                                                               01S0334 00620000
*        /************************************************************/ 00621000
*INITPROC:/* PREPARE TO INITIALIZE THE USERS CORE BY:                */ 00622000
*        /************************************************************/ 00623000
*        /* 1. DETERMINING WHETHER INITIALIZATION HAS OCCURRED       */ 00624000
*        /*    FOR THIS SAME COMMAND IMMEDIATELY PRIOR TO THIS       */ 00625000
*        /*    ATTEMPT. IF NOT, A BIT IS SET TO INDICATE THE         */ 00626000
*        /*    FIRST TIME.  IF INITIALIZATION HAS OCCURRED FOR       */ 00627000
*        /*    THIS COMMAND, THERE IS NO NEED TO ATTEMPT AGAIN       */ 00628000
*        /*    SO THE ABEND CONTINUES.                               */ 00629000
*        /* 2. INFORMING THE USER OF AN ATTEMPT TO                   */ 00630000
*        /*    RE-INITIALIZE HIS CORE.                               */ 00631000
*        /* 3. CALLING THE SUBROUTINE 'SNAPPIT' WHOSE PURPOSE        */ 00632000
*        /*    IS TO DETERMINE IF A SYSUDUMP OR SYSABEND DD ENTRY    */ 00633000
*        /*    IS IN THE TIOT. IF THERE IS, A SNAP DUMP IS           */ 00634000
*        /*    TAKEN OF THE USER'S CORE.                             */ 00635000
*        /* 4. CALLING THE SUBROUTINE 'DETACH' TO DETACH ALL         */ 00636000
*        /*    SUBTASKS OF THIS TASK.                                */ 00637000
*        /* 5. CALLING THE SUBROUTINE 'FREESUPB' WHICH FREES THE     */ 00638000
*        /*    CORE USED BY THE USER                                 */ 00639000
*        /* 6. ZERO-ING OUT THE TMP WORK AREA POINTER TO BE USED     */ 00640000
*        /*    BY THE STAE RETRY ROUTINE TO DETERMINE WHETHER        */ 00641000
*        /*    COMPLETE INITIALIZATION IS TO TAKE PLACE OR           */ 00642000
*        /*    WHETHER A SIMPLE RECOVERY WILL BE ATTEMPTED.          */ 00643000
*        /* 7. BRANCHING TO 'RETRYRTN' WHICH LOADS THE STAE RETRY    */ 00644000
*        /*    ROUTINE AND INDICATE TO ABEND THAT A RETRY            */ 00645000
*        /*    IS TO BE ATTEMPTED.                                   */ 00646000
*        /************************************************************/ 00647000
*                                                               01S0334 00648000
*        IF TMPRINIT='1'B      /* IF REINITIALIZATION WAS ATTEMPTED  */ 00649000
*            | BKGMODE = YES   /*       OR IF IN BACKGROUND MODE     */ 00650000
*          THEN                /* YES,                               */ 00651000
INITPROC L     @07,TMPWA2P(,WRKAPTR2)                           01S0334 00652000
         TM    TMPRINIT(@07),B'10000000'                        01S0334 00653000
         BO    @RT00334                                         01S0334 00654000
         TM    BKGMODE(WRKAPTR2),B'10000000'                    01S0334 00655000
         BO    @RT00334                                         01S0334 00656000
*            GOTO NORETRY;     /* DONT REINITIALIZE AGAIN            */ 00657000
*        TMPRINIT = YES;       /* ELSE, INDICATE FIRST TIME          */ 00658000
*                                                               01S0336 00659000
         OI    TMPRINIT(@07),B'10000000'                        01S0336 00660000
*        R1=ADDR(MSG56601);    /* 'REINITIALIZATION' MESSAGE         */ 00661000
         L     R1,@CA02999                                      01S0337 00662000
*        R0=MSGLEN-D4;         /* GET LENGTH OF MESSAGE TEXT         */ 00663000
         LA    @07,4                                            01S0338 00664000
         LH    R0,MSGLEN(,R1)                                   01S0338 00665000
         SLR   R0,@07                                           01S0338 00666000
*        R1=R1+D4;             /* POINT TO MESSAGE TEXT              */ 00667000
         ALR   R1,@07                                           01S0339 00668000
*        GEN(TPUT  (1),(0));   /* ISSUE MESSAGE VIA TPUT             */ 00669000
         TPUT  (1),(0)                                                  00670000
*        RESPECIFY (R15) RESTRICTED;                            01S0341 00671000
*        IF R15 ^= ZERO        /* WAS TPUT SUCCESSFUL?               */ 00672000
*          THEN                /* TPUT WAS UNSUCCESSFUL              */ 00673000
         LTR   R15,R15                                          01S0342 00674000
         BNZ   @RT00342                                         01S0342 00675000
*            GO TO ABENDRTN;   /* GO TO CONTINUE ABEND               */ 00676000
*        RESPECIFY (R15) UNRESTRICTED;                          01S0344 00677000
*                                                               01S0344 00678000
*        CALL SNAPPIT;         /* GO TAKE SNAP DUMP IF SYSUDUMP 01S0345 00679000
*                                 OR SYSABEND DD ENTRY               */ 00680000
         BAL   @14,SNAPPIT                                      01S0345 00681000
*        CALL DETACH;          /* GO DETACH SUBTASKS                 */ 00682000
         BAL   @14,DETACH                                       01S0346 00683000
*        CALL FREESUBP;        /* GO TO FREE SUBPOOLS 1-127          */ 00684000
         BAL   @14,FREESUBP                                     01S0347 00685000
*        DO; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/           01S0348 00686000
*          R2 = SPROK1; /*GET KEY FORM SAVE LOCATION*/          01S0349 00687000
         L     R2,SPROK1                                        01S0349 00688000
*          SPKA (R2); /*SET PSW KEY*/                           01S0350 00689000
         SPKA  0(R2)                                            01S0350 00690000
*        END; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/          01S0351 00691000
*        WRKA1PTR = ZERO;      /* ZERO OUT TMP WORKAREA POINTER      */ 00692000
         SLR   @07,@07                                          01S0352 00693000
         ST    @07,WRKA1PTR(,WRKAPTR2)                          01S0352 00694000
*        DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/           01S0353 00695000
*          R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/          01S0354 00696000
         L     R2,TEPKEY(,WRKAPTR2)                             01S0354 00697000
*          SPKA (R2); /*SET PSW KEY*/                           01S0355 00698000
         SPKA  0(R2)                                            01S0355 00699000
*        END; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/          01S0356 00700000
*        GO TO RETRYRTN;       /* GO TO INDICATE RETRY ATTEMPT       */ 00701000
         B     RETRYRTN                                         01S0357 00702000
*RECOVER:/************************************************************/ 00703000
*        /* THE PURPOSE OF THIS BLOCK OF CODE IS TO AID  IN THE      */ 00704000
*        /* SIMPLE RECOVERY PROCESS BY:                              */ 00705000
*        /* 1. DETERMINING WHETHER A SIMPLE RECOVERY HAS OCCURRED    */ 00706000
*        /*    FOR THIS COMMAND IMMEDIATELY PRIOR TO THIS ATTEMPT.   */ 00707000
*        /* 2. IF NOT, A BIT IS SET TO INDICATE THE FIRST ATTEMPT.   */ 00708000
*        /* 3. ISSUING A MESSAGE TO THE USER TO INFORM HIM OF THE    */ 00709000
*        /*    RECOVERY ATTEMPT.                                     */ 00710000
*        /* 4. 'SNAPPIT' IS CALLED TO TAKE A SNAP DUMP IF A          */ 00711000
*        /*    'SYSUDUMP' OR SYSABEND' DD ENTRY IS PRESENT           */ 00712000
*        /* 5. 'RETRYRTN' IS CALLED TO LOAD THE RETRY ROUTINE AND    */ 00713000
*        /*    INDICATE TO ABEND THAT A RETRY WILL BE ATTEMPTED.     */ 00714000
*        /************************************************************/ 00715000
*                                                               01S0358 00716000
*        IF TMPRTRY = YES      /* IF RECOVERY WAS ATTEMPTED          */ 00717000
*            | BKGMODE = YES   /*       OR IF IN BACKGROUND MODE     */ 00718000
*          THEN                /* RECOVERY WAS NOT ATTEMPTED?        */ 00719000
RECOVER  L     @07,TMPWA2P(,WRKAPTR2)                           01S0358 00720000
         TM    TMPRTRY(@07),B'01000000'                         01S0358 00721000
         BO    @RT00358                                         01S0358 00722000
         TM    BKGMODE(WRKAPTR2),B'10000000'                    01S0358 00723000
         BO    @RT00358                                         01S0358 00724000
*            GOTO INITPROC;    /* GO TO INITIALIZATION PROCEDURE     */ 00725000
*        TMPRTRY='1'B;         /* INDICATE RECOVERY ATTEMPT          */ 00726000
*                                                               01S0360 00727000
         OI    TMPRTRY(@07),B'01000000'                         01S0360 00728000
*        R1=ADDR(MSG56602);    /*POINT TO START OF MESSAGE BUFFER    */ 00729000
         L     R1,@CA03000                                      01S0361 00730000
*        R0=MSGLEN-D4;         /* GET LENGTH OF MESSAGE TEXT         */ 00731000
         LA    @07,4                                            01S0362 00732000
         LH    R0,MSGLEN(,R1)                                   01S0362 00733000
         SLR   R0,@07                                           01S0362 00734000
*        R1=R1+D4;             /* POINT TO START OF MESSAGE TEXT     */ 00735000
         ALR   R1,@07                                           01S0363 00736000
*        GENERATE(TPUT (1),(0));/* ISSUE MESSAGE VIA TPUT MACRO      */ 00737000
         TPUT (1),(0)                                                   00738000
*        RESPECIFY (R15) RESTRICTED;                            01S0365 00739000
*        IF R15 ^= ZERO        /* WAS TPUT SUCCESSFUL?               */ 00740000
*          THEN                /* NO, IT WAS NOT                     */ 00741000
         LTR   R15,R15                                          01S0366 00742000
         BNZ   @RT00366                                         01S0366 00743000
*            GO TO ABENDRTN;   /* GO TO ABEND ROUTINE                */ 00744000
*        RESPECIFY (R15) UNRESTRICTED;                          01S0368 00745000
*                                                               01S0368 00746000
*        CALL SNAPPIT;         /* TAKE A SNAP DUMP                   */ 00747000
         BAL   @14,SNAPPIT                                      01S0369 00748000
*        CALL DETACH;          /* GO DETACH SUBTASKS                 */ 00749000
         BAL   @14,DETACH                                       01S0370 00750000
*        TPLCTCB = ZERO;       /* ZERO ATTACHED TCB PTR              */ 00751000
         SLR   @07,@07                                          01S0371 00752000
         ST    @07,TPLCTCB(,WRKAPTR1)                           01S0371 00753000
*        TMPCECB = ZERO;       /* ZERO ATTACHED CP ECB               */ 00754000
         ST    @07,TMPCECB(,WRKAPTR1)                           01S0372 00755000
*        TMPACTRL = '0'B;      /* ZERO ATTENTION CONTROL IND         */ 00756000
*        TMPSCTRL = '0'B;      /* ZERO TMP STAI CONTROL IND          */ 00757000
*                                                               01S0374 00758000
*                                                               01S0374 00759000
         NI    TMPACTRL(WRKAPTR1),B'11100111'                   01S0374 00760000
*RETRYRTN:/***********************************************************/ 00761000
*        /* THE PURPOSE OF THIS BODY OF CODE IS TO LOAD THE TMP      */ 00762000
*        /* STAE EXIT RETRY ROUTINE AND TO SET A RETURN CODE OF      */ 00763000
*        /* 4 IN REGISTER 15 TO INDICATE TO THE ABEND ROUTINE        */ 00764000
*        /* THAT A RETRY WILL BE ATTEMPTED.CONTROL IS THEN           */ 00765000
*        /* RETURNED THE ABEND ROUTINE.                              */ 00766000
*        /************************************************************/ 00767000
*                                                               01S0375 00768000
*        RESPECIFY (R14,R15) RSTD;                              01S0375 00769000
RETRYRTN DS    0H                                               01S0376 00770000
*        R14 = REG0;           /* RESERVE SDWA INDICATOR             */ 00771000
         L     @01,SAVRBAK(,R13)                                01S0376 00772000
         L     R14,REG0(,@01)                                   01S0376 00773000
*        GEN(LOAD  EP=IKJEFT07); /* LOAD STAE RETRY MODULE           */ 00774000
         LOAD  EP=IKJEFT07                                              00775000
*        REG0=R0;              /* GET RETRY ROUTINE POINTER          */ 00776000
         L     @07,SAVRBAK(,R13)                                01S0378 00777000
         ST    R0,REG0(,@07)                                    01S0378 00778000
*        R15 = D4;             /* SET RETRY INDICATOR                */ 00779000
         LA    R15,4                                            01S0379 00780000
*        REG15 = R15;          /* RESERVE RETRY-ATTEMPT INDICATOR    */ 00781000
         ST    R15,REG15(,@07)                                  01S0380 00782000
*        GO TO TRETURN;        /* RETURN TO CALLER TO RETRY          */ 00783000
         B     TRETURN                                          01S0381 00784000
*FREESUBP:                                                      01S0382 00785000
*        /************************************************************/ 00786000
*        /* THIS ROUTINE SEARCHES DOWN THE SPQE CHAIN AND FREES      */ 00787000
*        /* ALL CORE THAT IS IN SUBPOOLS 1 TO 127.                   */ 00788000
*        /************************************************************/ 00789000
*                                                               01S0382 00790000
*        PROCEDURE                                              01S0382 00791000
*          OPTIONS (                                            01S0382 00792000
*          DONTSAVE,           /* DONT SAVE CALLER'S REGISTERS       */ 00793000
*          NOSAVEAREA);        /* NO SAVE AREA IS REQUIRED           */ 00794000
FREESUBP DS    0H                                               01S0383 00795000
*        RESPECIFY (R3) RESTRICTED;  /* RETURN ADDRESS               */ 00796000
*        R3 = R14;             /* SAVE THE RETURN ADDRESS            */ 00797000
*                                                               01S0384 00798000
         LR    R3,R14                                           01S0384 00799000
*        SPQESPTR = TCBMSS;    /* GET SPQE POINTER                   */ 00800000
         L     @01,TCBPTR(,WRKAPTR2)                            01S0385 00801000
         L     @07,TCBMSS(,@01)                                 01S0385 00802000
         ST    @07,SPQESPTR                                     01S0385 00803000
*SPCHK:                                                         01S0386 00804000
*        IF SPQEID>MINSP       /* CHECK FOR MINUMUM SUBPOOL -- 1     */ 00805000
*             & SPQEID<MAXSP   /* CHECK FOR MAXIMUM SUBPOOL -- 127   */ 00806000
*          THEN                /* SUBPOOL ID IS BETWEEN 1-127        */ 00807000
SPCHK    L     @07,SPQESPTR                                     01S0386 00808000
         CLI   SPQEID(@07),0                                    01S0386 00809000
         BNH   @RF00386                                         01S0386 00810000
         CLI   SPQEID(@07),128                                  01S0386 00811000
         BNL   @RF00386                                         01S0386 00812000
*            DO;               /* PREPARE TO FREE SUBPOOL            */ 00813000
*              R0=SPQEID*SP01; /* GET SUBPOOL ID                     */ 00814000
         SLR   R0,R0                                            01S0388 00815000
         IC    R0,SPQEID(,@07)                                  01S0388 00816000
         SLA   R0,24                                            01S0388 00817000
*              GEN(FREEMAIN R,SP=(0)); /* FREE SUBPOOL               */ 00818000
         FREEMAIN R,SP=(0)                                              00819000
*            END;                                               01S0390 00820000
*        IF SPQEAD ^= ZERO     /* END OF THE SPQE CHAIN?             */ 00821000
*          THEN                /* NOT THE END OF THE CHAIN           */ 00822000
@RF00386 L     @07,SPQESPTR                                     01S0391 00823000
         L     @07,SPQEAD(,@07)                                 01S0391 00824000
         LTR   @07,@07                                          01S0391 00825000
         BZ    @RF00391                                         01S0391 00826000
*            DO;               /* PREPARE NEXT CHECK SUBPOOL ID      */ 00827000
*              SPQESPTR=SPQEAD;  /* GET NEXT SPQE POINTER            */ 00828000
         ST    @07,SPQESPTR                                     01S0393 00829000
*              GO TO SPCHK;    /* CHECK SUBPOOL ID                   */ 00830000
         B     SPCHK                                            01S0394 00831000
*            END;              /* END SUBPOOL ID CHECK               */ 00832000
*                                                               01S0395 00833000
*        R14 = R3;             /* RESTORE RETURN ADDRESS             */ 00834000
@RF00391 LR    R14,R3                                           01S0396 00835000
*        RESPECIFY (R3) UNRSTD;                                 01S0397 00836000
*                                                               01S0397 00837000
*        END FREESUBP;         /* END OF 'FREE SUBPOOL' PROCEDURE    */ 00838000
@EL00002 DS    0H                                               01S0398 00839000
@EF00002 DS    0H                                               01S0398 00840000
@ER00002 BR    @14                                              01S0398 00841000
*SNAPPIT:                                                       01S0399 00842000
*        /************************************************************/ 00843000
*        /* THIS SUBROUTINE SEARCHES THE TASK INPUT-OUTPUT           */ 00844000
*        /* TABLE (TIOT) FOR A 'SYSABEND' OR 'SYSUDUMP' DD ENTRY.    */ 00845000
*        /* IF NOT, NO SNAP DUMP IS TAKEN. IF SO, A CHECK IS MADE    */ 00846000
*        /* TO DETERMINE IF THE SYSTEM ABEND DATA SET IS OPEN.       */ 00847000
*        /* IF SO, ITS DCB IS USED IN TAKING THE SNAP DUMP.          */ 00848000
*        /* BUT IF IT IS NOT OPEN, A DCB IS USED THAT HAS BEEN       */ 00849000
*        /* SPECIFIED IN THIS ROUTINE.                               */ 00850000
*        /************************************************************/ 00851000
*                                                               01S0399 00852000
*        PROCEDURE                                              01S0399 00853000
*          OPTIONS  (                                           01S0399 00854000
*          DONTSAVE,           /* DONT SAVE CALLER'S REGISTERS       */ 00855000
*          NOSAVEAREA);        /* NO SAVE AREA IS REQUIRED           */ 00856000
SNAPPIT  DS    0H                                               01S0400 00857000
*        RESPECIFY (R1,R4,R6) RESTRICTED;  /* INTERNAL USE REGISTERS */ 00858000
*        R4=R14;               /* SAVE RETURN ADDRESS                */ 00859000
         LR    R4,R14                                           01S0401 00860000
*        R6=ADDR(DCBLIST);     /* GET DCB LIST POINTER               */ 00861000
         LA    R6,DCBLIST                                       01S0402 00862000
*        GEN(MVC   DCBLIST(DCBLNG),DUMPDCB);  /* INITIALIZE DCB LIST */ 00863000
         MVC   DCBLIST(DCBLNG),DUMPDCB                                  00864000
*        TIOTPTR=TCBTIO;       /* INITIALIZE TIOT POINTER            */ 00865000
         L     @07,TCBPTR(,WRKAPTR2)                            01S0404 00866000
         L     @07,TCBTIO(,@07)                                 01S0404 00867000
         ST    @07,TIOTPTR                                      01S0404 00868000
*NEXTENT:                                                       01S0405 00869000
*        IF TIOEDDNM='SYSUDUMP'  /* CHECK FOR 'SYSUDUMP' DD NAME     */ 00870000
*          THEN                /* DDNAME IS PRESENT                  */ 00871000
NEXTENT  L     @07,TIOTPTR                                      01S0405 00872000
         CLC   TIOEDDNM(8,@07),@CC03113                         01S0405 00873000
         BE    @RT00405                                         01S0405 00874000
*            GO TO OPEN;       /* GO OPEN SNAP DATA SET              */ 00875000
*        IF TIOEDDNM='SYSABEND'  /* CHECK FOR 'SYSABEND' DD NAME     */ 00876000
*          THEN                /* SYSABEND DD SPECIFIED              */ 00877000
         CLC   TIOEDDNM(8,@07),@CC03116                         01S0407 00878000
         BNE   @RF00407                                         01S0407 00879000
*            DO;                                                01S0408 00880000
*              DCBDDNAM='SYSABEND';  /* PLACE DD NAME IN DCB         */ 00881000
         L     @07,DCBPTR(,WRKAPTR1)                            01S0409 00882000
         MVC   DCBDDNAM+40(8,@07),@CC03116                      01S0409 00883000
*OPEN:                                                          01S0410 00884000
*              IF TCBJSTCB->TCBFDSOP^='1'B  /* DUMP DATA SET OPEN?   */ 00885000
*              THEN            /* THE DATA SET IS NOT OPEN           */ 00886000
OPEN     L     @07,TCBPTR(,WRKAPTR2)                            01S0410 00887000
         L     @07,TCBJSTCB(,@07)                               01S0410 00888000
         TM    TCBFDSOP(@07),B'00000100'                        01S0410 00889000
         BO    @RF00410                                         01S0410 00890000
*              DO;               /* GO TO OPEN DATA SET              */ 00891000
*                                                               01S0411 00892000
*              R1=ADDR(OPENLIST);  /* GET OPEN LIST POINTER          */ 00893000
         LA    R1,OPENLIST                                      01S0412 00894000
*              GENERATE;                                        01S0413 00895000
           MVC  OPENLIST(OPENLNG),OPENL    INITIALIZE OPEN LIST         00896000
           OPEN   ((6),OUTPUT),MF=(E,(1))  OPEN DUMP DATA SET DCB       00897000
*               END;                                            01S0414 00898000
*              ELSE                                             01S0415 00899000
*              IF DCBOPEN=YES  /* CHECK THE DCB OPEN FLAG            */ 00900000
*                THEN          /* DCB HAS BEEN OPENED                */ 00901000
         B     @RC00410                                         01S0415 00902000
@RF00410 L     @07,DCBPTR(,WRKAPTR1)                            01S0415 00903000
         TM    DCBOPEN+40(@07),B'00010000'                      01S0415 00904000
         BO    @RT00415                                         01S0415 00905000
*                  GO TO SNAP; /* GO TAKE SNAP DUMP                  */ 00906000
*                ELSE          /* DCB HAS NOT BEEN OPENED            */ 00907000
*                  GEN(BR    4);  /* RETURN TO MAIN PROCEDURE        */ 00908000
         BR    4                                                        00909000
*            END;                                               01S0418 00910000
@RC00410 DS    0H                                               01S0419 00911000
*        IF TIOELNGH ^= ZERO   /* IS THIS THE TIOT END               */ 00912000
*          THEN                /* NO,GO TO NEXT DD ENTRY             */ 00913000
@RF00407 L     @07,TIOTPTR                                      01S0419 00914000
         CLI   TIOELNGH(@07),0                                  01S0419 00915000
         BE    @RF00419                                         01S0419 00916000
*            DO;                                                01S0420 00917000
*              TIOTPTR=TIOTPTR+TIOELNGH;  /* GET NEXT DD POINTER     */ 00918000
         SLR   @03,@03                                          01S0421 00919000
         IC    @03,TIOELNGH(,@07)                               01S0421 00920000
         ALR   @03,@07                                          01S0421 00921000
         ST    @03,TIOTPTR                                      01S0421 00922000
*              GO TO NEXTENT;  /* GO CHECK NEXT ENTRY                */ 00923000
         B     NEXTENT                                          01S0422 00924000
*            END;                                               01S0423 00925000
*        GO TO SNAPND;         /* NO DUMP TO BE TAKEN                */ 00926000
*                                                               01S0424 00927000
*                                                               01S0424 00928000
         B     SNAPND                                           01S0424 00929000
*SNAP:                                                          01S0425 00930000
*        R1=ADDR(SNAPLIST);    /* GET POINTER TO SNAP LIST           */ 00931000
SNAP     LA    R1,SNAPLIST                                      01S0425 00932000
*        RESPECIFY (R2,R3) RESTRICTED;  /* LIST POINTERS             */ 00933000
*        R2=SNAPID;            /* SNAP DUMP IDENTIFIER IS 007        */ 00934000
         LA    R2,7                                             01S0427 00935000
*        R3=TCURTCB;           /* GET TCB POINTER                    */ 00936000
         L     @07,CVTPTR                                       01S0428 00937000
         L     @07,CVTTCBP(,@07)                                01S0428 00938000
         L     R3,TCURTCB(,@07)                                 01S0428 00939000
*        GENERATE;             /* ISSUE SNAP MACRO                   */ 00940000
         MVC   SNAPLIST(SNAPLNG),SNAPL   INITIALIZE SNAP LIST           00941000
         SNAP   TCB=(3),DCB=(6),ID=(2),SDATA=(Q,CB),                   *00942000
               PDATA=(SA,ALLPA,SPLS),MF=(E,(1))                         00943000
*        RESPECIFY (R2,R3) UNRESTRICTED;  /* RELEASE LIST POINTERS   */ 00944000
*              IF TCBJSTCB->TCBFDSOP='1'B  /* DUMP DATA SET OPEN?    */ 00945000
*                THEN          /* DATA SET WAS NOT OPENED BY    01S0431 00946000
*                                 STAE EXIT                          */ 00947000
         L     @07,TCBPTR(,WRKAPTR2)                            01S0431 00948000
         L     @07,TCBJSTCB(,@07)                               01S0431 00949000
         TM    TCBFDSOP(@07),B'00000100'                        01S0431 00950000
         BO    @RT00431                                         01S0431 00951000
*                  GO TO SNAPND;  /* RETURN TO MAIN LINE             */ 00952000
*                                                               01S0432 00953000
*                                                               01S0432 00954000
*CLOSE:                                                         01S0433 00955000
*        R1=ADDR(OPENLIST);    /* GET POINTER TO OPEN LIST           */ 00956000
CLOSE    LA    R1,OPENLIST                                      01S0433 00957000
*        GEN(CLOSE ((6),LEAVE),MF=(E,(1)));  /* CLOSE DATA SET       */ 00958000
         CLOSE ((6),LEAVE),MF=(E,(1))                                   00959000
*SNAPND:                                                        01S0435 00960000
*        R14=R4;               /* RESTORE RETURN ADDRESS             */ 00961000
*                                                               01S0435 00962000
SNAPND   LR    R14,R4                                           01S0435 00963000
*        END SNAPPIT;          /* END SNAP DUMP ROUTINE              */ 00964000
@EL00003 DS    0H                                               01S0436 00965000
@EF00003 DS    0H                                               01S0436 00966000
@ER00003 BR    @14                                              01S0436 00967000
*DETACH:                                                        01S0437 00968000
*        /************************************************************/ 00969000
*        /* THIS SUBROUTINE SEARCHES DOWN THE TASK CONTROL           */ 00970000
*        /* BLOCK CHAIN (TCB) FOR SUBTASKS OF THIS TASK              */ 00971000
*        /* AND DETACHES THEM ALL.                                   */ 00972000
*        /************************************************************/ 00973000
*                                                               01S0437 00974000
*        PROCEDURE                                              01S0437 00975000
*          OPTIONS  (                                           01S0437 00976000
*          DONTSAVE,           /* DONT SAVE CALLER'S REGISTERS       */ 00977000
*          NOSAVEAREA);        /* NO SAVE AREA REQUIRED              */ 00978000
DETACH   DS    0H                                               01S0438 00979000
*        RESPECIFY (R1,R3) RESTRICTED;  /* REGISTERS FOR USE IN 01S0438 00980000
*                                 THIS INTERNAL ROUTINE              */ 00981000
*        R3=R14;               /* SAVE THE RETURN ADDRESS            */ 00982000
         LR    R3,R14                                           01S0439 00983000
*STASKCHK:                     /* DETACH SUBTASK                     */ 00984000
*        IF TCBLTC ^= ZERO     /* DOES SUBTASK EXIST?                */ 00985000
*          THEN                /* SUBTASK DOES EXIST                 */ 00986000
STASKCHK L     @07,TCBPTR(,WRKAPTR2)                            01S0440 00987000
         L     @07,TCBLTC(,@07)                                 01S0440 00988000
         LTR   @07,@07                                          01S0440 00989000
         BZ    @RF00440                                         01S0440 00990000
*            DO;               /* PREPARE TO DETACH SUBTASK          */ 00991000
*              SUBTASKP=TCBLTC;  /* GET POINTER TO SUBTASK           */ 00992000
         ST    @07,SUBTASKP                                     01S0442 00993000
*              R1=ADDR(SUBTASKP);  /* GET PTR TO SUBTASK POINTER     */ 00994000
         LA    R1,SUBTASKP                                      01S0443 00995000
*              GEN(DETACH (1));  /* DETACH THE SUBTASK               */ 00996000
         DETACH (1)                                                     00997000
*              GOTO STASKCHK;  /* CONTINUE SEARCH FOR SUBTASKS       */ 00998000
         B     STASKCHK                                         01S0445 00999000
*            END;                                               01S0446 01000000
*        R14=R3;               /* RESTORE RETURN ADDRESS             */ 01001000
@RF00440 LR    R14,R3                                           01S0447 01002000
*        END DETACH;           /* END DETACH PROCEDURE               */ 01003000
@EL00004 DS    0H                                               01S0448 01004000
@EF00004 DS    0H                                               01S0448 01005000
@ER00004 BR    @14                                              01S0448 01006000
*/********************************************************************/ 01007000
*/* THIS SUBROUTINE  FUNCTIONS  AS AN ABEND COMPLETION CODE CONVER-  */ 01008000
*/* SION ROUTINE.  IT PREPARES THE CODE TO BE PRINTED OUT, SETS UP   */ 01009000
*/* THE MESSAGE SEGMENT LISTS  AND  ISSUES  THE PUTLINE WITH MULTI-  */ 01010000
*/* LEVEL OPTION FOR SECOND LEVEL MESSAGE.                           */ 01011000
*/********************************************************************/ 01012000
*                                                               01S0449 01013000
*CONVTCC:  /* EP TO CONVERSION AND PRINT ROUTINE                     */ 01014000
*                                                               01S0449 01015000
*    PROC  OPTIONS(DONTSAVE,NOSAVEAREA);                        01S0449 01016000
*                                                               01S0449 01017000
CONVTCC  DS    0H                                               01S0450 01018000
*    RESPECIFY  (R1,R2,R3,WRKAPTR1,R11,R12,R13) RESTRICTED;     01S0450 01019000
*                                                               01S0450 01020000
*    IF POSTCODE = NO                /* CHK FOR ATTENTION            */ 01021000
*      THEN DO;                      /* NO, CONTINUE                 */ 01022000
*                                                               01S0451 01023000
         L     @07,TPLAECB-1(,WRKAPTR1)                         01S0451 01024000
         TM    POSTCODE(@07),B'01000000'                        01S0451 01025000
         BNZ   @RF00451                                         01S0451 01026000
*        R10 = ADDR(ABMSGSP);        /* GET PTR TO WORK AREA         */ 01027000
         LA    R10,ABMSGSP(,WRKAPTR1)                           01S0453 01028000
*        TRUMSG(1:4) = VALAR6;       /* SET ABEND CODE TEXT          */ 01029000
         MVC   TRUMSG(4,R10),VALAR6(R10)                        01S0454 01030000
*        ABMLEN = ACLEN;             /* LEN OF COMPLETION CODE MSG   */ 01031000
         MVC   ABMLEN(2,R10),@CH00035                           01S0455 01032000
*        ABMOFF = ACDEOFF;           /* OFFSET IN MESSAGE            */ 01033000
*                                                               01S0456 01034000
         MVC   ABMOFF(2,R10),@CH00033                           01S0456 01035000
*        IF NONSCUR = YES            /* CHK FOR ABEND FROM SECURITY  */ 01036000
*          THEN DO;                  /* YES,                         */ 01037000
         TM    NONSCUR(WRKAPTR1),B'00000001'                    01S0457 01038000
         BNO   @RF00457                                         01S0457 01039000
*            ACTSL = TM01SL;         /*   INITIALIZE SEGMENT LIST    */ 01040000
         L     @07,@CA02996                                     01S0459 01041000
         MVC   ACTSL(112,WRKAPTR1),TM01SL-TM01SL(@07)           01S0459 01042000
*            ACTSEG(1)=ADDR(ACTSEG(5)); /* SET 2ND SEGMENT LIST PTR  */ 01043000
         LA    @07,ACTSEG+16(,WRKAPTR1)                         01S0460 01044000
         ST    @07,ACTSEG(,WRKAPTR1)                            01S0460 01045000
*            ACTSEG(9)=ADDR(ABMSGAR);  /* SET PTR TO ABEND CODE TEXT */ 01046000
         ST    R10,ACTSEG+32(,WRKAPTR1)                         01S0461 01047000
*            ACTSEG(11)=ADDR(SUBCDAR);  /* PTR TO REASON CODE TEXT   */ 01048000
         LA    @07,SUBCDAR(,R10)                                01S0462 01049000
         ST    @07,ACTSEG+40(,WRKAPTR1)                         01S0462 01050000
*            R2 = ADDR(PTPBSP);      /* SET PTR TO PUTLINE PARM LIST */ 01051000
         LA    R2,PTPBSP(,WRKAPTR1)                             01S0463 01052000
*            R3 = ADDR(ACTSL);       /* SET PTR TO MESSAGE LIST      */ 01053000
         LA    R3,ACTSL(,WRKAPTR1)                              01S0464 01054000
*            R1 = ADDR(CSPLSP);      /* PREPARE TO LINK TO PUTLINE   */ 01055000
*                                                               01S0465 01056000
         LA    R1,CSPLSP(,WRKAPTR1)                             01S0465 01057000
*            /* ISSUE THE E-FORM OF THE PUTLINE MACRO WITH THE       */ 01058000
*            /*     MULTILEVEL OPTION                                */ 01059000
*            R15 = ADDR(TPUTLM);     /* PTR TO PUTLINE ROUTINE       */ 01060000
         LA    R15,TPUTLM                                       01S0466 01061000
*            GEN FLOWS(TPUTLM) (SYNCH  (15));  /* TO PUTLINE         */ 01062000
         SYNCH  (15)                                                    01063000
*          END;                                                 01S0468 01064000
