         TITLE 'IKJEFT05 -- TMP STAE EXIT ROUTINE                      *00001000
                        '                                               00002000
IKJEFT05 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IKJEFT05  78.177'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                               01S0002 00010000
         USING   IKJEFT05,R15                                           00011000
*                                                               01S0105 00012000
*        /************************************************************/ 00013000
*        /*   CHECK THE CODE IN REGISTER 0.  IF IT IS 12, NO SYSTEM  */ 00014000
*        /*   SAVE AREA (POINTER IN REGISTER 13) HAS BEEN PROVIDED   */ 00015000
*        /*   BY THE STAE PROCESSOR.  A SIMULATED SYSTEM SAVE AREA   */ 00016000
*        /*   IS SUPPLIED AND REGISTER 13 SET AS POINTER             */ 00017000
*        /************************************************************/ 00018000
*                                                               01S0106 00019000
*        IF R0 = STAIWA             /* CHK IF STAI WA SUPPLIED       */ 00020000
*          THEN DO;                 /* NO,                           */ 00021000
         C     R0,@CF00033                                      01S0106 00022000
         BNE   @RF00106                                         01S0106 00023000
*            R0 = R2;               /* RESERVE ENTRY REG 2           */ 00024000
         LR    R0,R2                                            01S0108 00025000
*            GEN(L    R2,8(R2));    /* PTR TO ENTRY SAVE AREA        */ 00026000
         L    R2,8(R2)                                                  00027000
*            GEN(STM   R14,R12,12(R2));  /* SAVE ENTRY REGS          */ 00028000
         STM   R14,R12,12(R2)                                           00029000
*            R13 = R2;              /* SET ENTRY SAVE AREA PTR       */ 00030000
         LR    R13,R2                                           01S0111 00031000
*            RESPECIFY (SAVARY) BASED(R13);                     01S0112 00032000
*            SR13BAK = R13;         /* INDICATE END OF SAVE CHAIN    */ 00033000
         ST    R13,SR13BAK(,R13)                                01S0113 00034000
*            REG0 = STAIWA;         /* SET ENTRY NO-SDWA FLAG        */ 00035000
         MVC   REG0(4,R13),@CF00033                             01S0114 00036000
*            WRKAPTR2 = R0;         /* SET PTR TO PROTECTED TMPWA    */ 00037000
         LR    WRKAPTR2,R0                                      01S0115 00038000
*            R6 = R1;               /* RESERVE ABEND CODE FROM REG1  */ 00039000
         LR    R6,R1                                            01S0116 00040000
*            END;                                               01S0117 00041000
*          ELSE DO;                 /* YES, STAI WORK AREA SUPPLIED  */ 00042000
         B     @RC00106                                         01S0118 00043000
@RF00106 DS    0H                                               01S0119 00044000
*            GEN(STM  R14,R12,12(R13));  /* SAVE ENTRY REGS          */ 00045000
         STM  R14,R12,12(R13)                                           00046000
*            R0 = R0&&R0;           /* SET NON NO-WORK-AREA FLAG     */ 00047000
         XR    R0,R0                                            01S0120 00048000
*            WRKAPTR2 = SDWAPARM;   /* SET TMPWA PTR (8) VIA PARM    */ 00049000
         L     WRKAPTR2,SDWAPARM(,SDWAPTR)                      01S0121 00050000
*            R6 = ABCODE;           /* RESERVE ABEND CODE            */ 00051000
         L     R6,ABCODE(,PARMPTR)                              01S0122 00052000
*            RESPECIFY (SDWA) BASED(REG1);                      01S0123 00053000
*          END;                                                 01S0124 00054000
*                                                               01S0124 00055000
*        RESPECIFY (TMPWA2) BASED(TMPWA2P);                     01S0125 00056000
@RC00106 DS    0H                                               01S0126 00057000
*        WRKAPTR1 = WRKA1PTR;        /* SET PP WA PTR VIA SUP WA     */ 00058000
         L     WRKAPTR1,WRKA1PTR(,WRKAPTR2)                     01S0126 00059000
*        RESPECIFY  (R11,R15) RESTRICTED;                       01S0127 00060000
*        GEN(USING IKJEFT05,R11);                               01S0128 00061000
         USING IKJEFT05,R11                                             00062000
*        R11 = R15;            /* SET CODE ADDRESSABILITY            */ 00063000
         LR    R11,R15                                          01S0129 00064000
*        GEN(DROP    R15);                                      01S0130 00065000
*                                                               01S0130 00066000
         DROP    R15                                                    00067000
*        RESPECIFY (PARMPTR) UNRESTRICTED;                      01S0131 00068000
*        /* ESTABLISH SAVE AREA ADDRESSABILITY                       */ 00069000
*        SVT041A=SVT041A&&SVT041A;  /* CLEAR SAVE AREA               */ 00070000
         L     @12,TMPWA2P(,WRKAPTR2)                           01S0132 00071000
         XC    SVT041A(72,@12),SVT041A(@12)                     01S0132 00072000
*        SAVT041(2) = R13;     /* SET CURRENT BACKCHAIN PTR          */ 00073000
         ST    R13,SAVT041+4(,@12)                              01S0133 00074000
*        R15 = ADDR(SAVT041);  /* RESERVE CURRENT SAVE AREA PTR      */ 00075000
         LA    R15,SAVT041(,@12)                                01S0134 00076000
*        SR13FOR = R15;        /* SET OLD FORWARD CHAIN PTR          */ 00077000
