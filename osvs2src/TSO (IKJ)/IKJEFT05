*        GMLOC5 = ZERO;        /* CLEAR AREA PTR AS FLAG             */ 00113000
         SLR   @07,@07                                          01S0154 00114000
         L     @03,TMPWA2P(,WRKAPTR2)                           01S0154 00115000
         ST    @07,GMLOC5(,@03)                                 01S0154 00116000
*        IF R15 = ZERO         /* CHK FOR SUCCESSFUL GETMAIN         */ 00117000
*          THEN                /* YES,                               */ 00118000
         CR    R15,@07                                          01S0155 00119000
         BNE   @RF00155                                         01S0155 00120000
*            DO;                                                01S0156 00121000
*              GEN(USING @DATD,R12);                            01S0157 00122000
         USING @DATD,R12                                                00123000
*              R12 = R1;       /* SET REENTRANT ADDRESSABILITY       */ 00124000
