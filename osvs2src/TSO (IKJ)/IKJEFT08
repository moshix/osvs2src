*TINIT8:  /* INITIALIZATION                                          */ 00284000
*                                                               01S0122 00285000
*    PROC   OPTIONS(DONTSAVE,NOSAVEAREA);  /* NO SAVE AREA           */ 00286000
         B     @EL00001                                         01S0122 00287000
TINIT8   DS    0H                                               01S0123 00288000
*    RESPECIFY (R14) RSTD;                                      01S0123 00289000
*                                                               01S0123 00290000
*    CALLSWS = CALLSWS&&CALLSWS;    /* CLEAR FLAGS                   */ 00291000
         XC    CALLSWS(1,WRKAPTR1),CALLSWS(WRKAPTR1)            01S0124 00292000
*    CDCBPTR=ADDR(CDCBSP);          /* SET PTR TO CALL DCB           */ 00293000
         LA    @10,CDCBSP(,WRKAPTR1)                            01S0125 00294000
         ST    @10,CDCBPTR(,WRKAPTR1)                           01S0125 00295000
*    CALLDCB = CALLDCBM;            /* INITIALIZE DCB                */ 00296000
         MVC   CALLDCB(52,WRKAPTR1),CALLDCBM                    01S0126 00297000
*    GENERATE;                                                  01S0127 00298000
           MVC   OPENPL(OPENPLL,WRKAPTR1),OPENPLM                       00299000
           MVC   CLOSEPL(CLOSPLL,WRKAPTR1),CLOSPLM                      00300000
*    PPL(1:12) = IOPL;              /* PARSE PARM LIST               */ 00301000
         L     @10,PPLPTR(,WRKAPTR1)                            01S0128 00302000
         MVC   PPL(12,@10),IOPL(SRPARMLP)                       01S0128 00303000
*    DAPL(1:12) = IOPL;             /* DAIR PARM LIST                */ 00304000
         L     @06,DAPLPTR(,WRKAPTR1)                           01S0129 00305000
         MVC   DAPL(12,@06),IOPL(SRPARMLP)                      01S0129 00306000
*    PPLPCL = PCLADCN;              /* PTR TO PCL IN PARSE PARM LIST */ 00307000
         L     @02,PCLADCN                                      01S0130 00308000
         ST    @02,PPLPCL(,@10)                                 01S0130 00309000
*    PPLANS = ADDR(PDLADDR);        /* PTR TO ANSWER SPACE FOR PARSE */ 00310000
         LA    @02,PDLADDR(,WRKAPTR1)                           01S0131 00311000
         ST    @02,PPLANS(,@10)                                 01S0131 00312000
*    PPLUWA = ZERO;                 /* CLEAR                         */ 00313000
         SLR   @02,@02                                          01S0132 00314000
         ST    @02,PPLUWA(,@10)                                 01S0132 00315000
*    DAPLPSCB = PSCBPTR;            /* PTR TO PSCB IN DAIR PARM LIST */ 00316000
         L     @05,PSCBPTR(,WRKAPTR2)                           01S0133 00317000
         ST    @05,DAPLPSCB(,@06)                               01S0133 00318000
*    DAPLDAPB=ADDR(DAPBSP);         /* ADDRESSABILITY TO DAIR PB     */ 00319000
         LA    @05,DAPBSP(,WRKAPTR1)                            01S0134 00320000
         ST    @05,DAPLDAPB(,@06)                               01S0134 00321000
*    RESPECIFY (DAPB08) BASED(DAPLDAPB);                        01S0135 00322000
*    TMPCECB = ZERO;                /* INITIALIZE USER ECB           */ 00323000
         ST    @02,TMPCECB(,WRKAPTR1)                           01S0136 00324000
*    R3SAVE = ZERO;                 /* INITIALIZE IKJRLSA POINTER    */ 00325000
         ST    @02,R3SAVE(,WRKAPTR1)                            01S0137 00326000
*    CALWKA=CALWKA&&CALWKA;         /* INITIALIZE/CLEAR WORK AREA    */ 00327000
         XC    CALWKA(256,WRKAPTR1),CALWKA(WRKAPTR1)            01S0138 00328000
*    CALWKB=CALWKB&&CALWKB;         /* INITIALIZE/CLEAR WORK AREA    */ 00329000
         XC    CALWKB(4,WRKAPTR1),CALWKB(WRKAPTR1)              01S0139 00330000
*    PPLCBUF = TPLCBUF;             /* SET PTR TO CMD BUFFER         */ 00331000
         L     @06,TPLCBUF(,WRKAPTR1)                           01S0140 00332000
         ST    @06,PPLCBUF(,@10)                                01S0140 00333000
*    R1 = PPLPTR;                   /* SET CMD SCAN PARM LIST PTR    */ 00334000
*                                                               01S0141 00335000
         LR    R1,@10                                           01S0141 00336000
*    END TINIT8;                                                01S0142 00337000
@EL00002 DS    0H                                               01S0142 00338000
@EF00002 DS    0H                                               01S0142 00339000
@ER00002 BR    @14                                              01S0142 00340000
*    RESPECIFY (R14) UNRSTD;                                    01S0143 00341000
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */ 00342000
*/*                                                                  */ 00343000
*/* SEGMENT NAME -- DOSETCP                                          */ 00344000
*/*                                                                  */ 00345000
*/* DESCRIPTIVE NAME -- N/A                                          */ 00346000
*/*                                                                  */ 00347000
*/* FUNCTION -- TO ESTABLISH AND ALLOCATE DS FOR CALLED CP           */ 00348000
*/*  OPERATION --                                                    */ 00349000
*/*           - PROCESS DATA SET NAME                                */ 00350000
*/*           - IF MEMBER NAME NOT ENTERED, ISSUE TEMPNAME MESSAGE   */ 00351000
*/*             AND RELEASE PRIOR PDL IF NECESSARY                   */ 00352000
*/*           - ALLOCATE CALLED DATA SET                             */ 00353000
*/*           - IF DATA SET NOT FOUND, PROMPT FOR MEMBER NAME        */ 00354000
*/*           - SET ALLOCATION FAILURE INDICATOR IF DAIRRC NOT ZERO  */ 00355000
*/*                                                                  */ 00356000
*/* ENTRY -- LINKED BY REGISTER 14                                   */ 00357000
*/*                                                                  */ 00358000
*/* INPUT -- PTR TO PPL ANS PLACE IN REGISTER 3                      */ 00359000
*/*                                                                  */ 00360000
*/* OUTPUT -- TEMPNAME MESSAGE ISSUED IF MEMBER NAME NOT ENTERED     */ 00361000
*/*                                                                  */ 00362000
*/* EXTERNAL REFERENCES -- N/A                                       */ 00363000
*/*                                                                  */ 00364000
*/* EXIT-NORMAL --  RETURN BY LINK REGISTER 14                       */ 00365000
*/*                 MSGNO = 0                                        */ 00366000
*/*                                                                  */ 00367000
*/*   RETURN CODE -- N/A                                             */ 00368000
*/*                                                                  */ 00369000
*/* EXIT-ERROR -- RETURN BY LINK REGISTER 14                         */ 00370000
*/*                                                                  */ 00371000
*/*   CONDITIONS -- ALLOCATION UNSUCCESSFUL (MSGNO = 99)             */ 00372000
*/*              -- DATA SET TYPE ERROR (MSGNO = 11)                 */ 00373000
*/*   RETURN CODE -- N/A                                             */ 00374000
*/*                                                                  */ 00375000
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */ 00376000
*DOSETCP:   /* ESTABLISH AND ALLOCATE DS FOR CALLED CP               */ 00377000
*                                                               01S0144 00378000
*    PROC   OPTIONS(DONTSAVE,NOSAVEAREA);  /* NO SAVE AREA           */ 00379000
*                                                               01S0144 00380000
DOSETCP  DS    0H                                               01S0145 00381000
*    /* IF NO MEMBER NAME IS CURRENT, ISSUE TEMPNAME MSG             */ 00382000
*    IF GMBRNOW = YES               /* CHK IF MEMBER NAME ENTERED    */ 00383000
*      THEN DO;                     /* NO,                           */ 00384000
         TM    GMBRNOW(WRKAPTR1),B'00000100'                    01S0145 00385000
         BNO   @RF00145                                         01S0145 00386000
*        MBRSTXT=TEMPMBNM;          /* INITIALIZE TO TEMPNAME        */ 00387000
         MVC   MBRSTXT(8,WRKAPTR1),TEMPMBNM                     01S0147 00388000
*        ACTSL = CM61SL;            /* INITIALIZE SEGMENT LIST       */ 00389000
         L     @14,@CA00133                                     01S0148 00390000
         MVC   ACTSL(112,WRKAPTR1),CM61SL-CM61SL(@14)           01S0148 00391000
