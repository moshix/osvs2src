*/*                                                                  */ 00664000
*/* SEGMENT NAME -- DOPARM                                           */ 00665000
*/*                                                                  */ 00666000
*/* DESCRIPTIVE NAME -- N/A                                          */ 00667000
*/*                                                                  */ 00668000
*/* FUNCTION -- TO CHECK LENGTH OF PARAMETERS ENTERED                */ 00669000
*/*   OPERATION --                                                   */ 00670000
*/*           - LENPARM = LENGTH OF PARAMETER FIELD                  */ 00671000
*/*           - IF NO PARAMETERS ARE ENTERED, LENPARM = ZERO         */ 00672000
*/*           - IF PARMLENGTH > 100, ISSUE TRUNCATION MESSAGE        */ 00673000
*/*           - IF PUTLINE SUCCESSFUL : LENPARM = 100                */ 00674000
*/*                        UNSUCCESSFUL : SET TERMINATION FLAG       */ 00675000
*/*           - IF LENGTH ENTERED IS LEGITIMATE, LENPARM = PARMLEN   */ 00676000
*/*                                                                  */ 00677000
*/* ENTRY -- LINKED BY REGISTER 14                                   */ 00678000
*/*                                                                  */ 00679000
*/* INPUT -- LENGTH OF PARAMETER STRING (SUPPLIED BY PARSE)          */ 00680000
*/*                                                                  */ 00681000
*/* OUTPUT -- LENGTH OF PARAMETER STRING, TRUNCATED IF NECESSARY     */ 00682000
*/*        -- TRUNCATION MESSAGE ISSUED IF PARM FIELD IS TOO LONG    */ 00683000
*/*                                                                  */ 00684000
*/* EXTERNAL REFERENCES -- N/A                                       */ 00685000
*/*                                                                  */ 00686000
*/* EXIT-NORMAL --  RETURN BY LINK REGISTER 14                       */ 00687000
*/*                 MSGNO = 0                                        */ 00688000
*/*                                                                  */ 00689000
*/*   RETURN CODE -- N/A                                             */ 00690000
*/*                                                                  */ 00691000
*/* EXIT-ERROR -- RETURN BY LINK REGISTER 14                         */ 00692000
*/*                                                                  */ 00693000
*/*   CONDITIONS -- PUTLINE FAILURE (MSGNO = 88)                     */ 00694000
*/*   RETURN CODE -- N/A                                             */ 00695000
*/*                                                                  */ 00696000
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */ 00697000
*DOPARM:  /* ESTABLISH LENGTH OF OPERAND FIELD                       */ 00698000
*                                                               01S0229 00699000
*    ENTRY;                         /* EP TO ROUTINE                 */ 00700000
*                                                               01S0229 00701000
DOPARM   DS    0H                                               01S0230 00702000
*    R3 = R3SAVE;                   /* RESTORE PDL PTR        ZA28716*/ 00703000
         L     R3,R3SAVE(,WRKAPTR1)                             01S0230 00704000
*    IF MSGNO = ZERO                /* CHK PARSE, BLDL, AND PUTLINE  */ 00705000
*      THEN DO;                     /* ALL SUCCESSFUL, CONTINUE      */ 00706000
         SLR   @14,@14                                          01S0231 00707000
         C     @14,MSGNO(,WRKAPTR1)                             01S0231 00708000
         BNE   @RF00231                                         01S0231 00709000
*        IF PARMLEN = ZERO          /* CHK IF ANY PARAMETERS ENTERED */ 00710000
*          THEN                     /* NO,                           */ 00711000
         CH    @14,PARMLEN(,R3)                                 01S0233 00712000
         BNE   @RF00233                                         01S0233 00713000
*            LENPARM = ZERO;        /*   INDICATE NO PARAMETERS      */ 00714000
         STH   @14,LENPARM(,WRKAPTR1)                           01S0234 00715000
*          ELSE DO;                 /* YES, PARAMETERS SUPPLIED      */ 00716000
         B     @RC00233                                         01S0235 00717000
@RF00233 DS    0H                                               01S0236 00718000
*            /* SET UP PARAMETER FIELD LENGTH                        */ 00719000
*            IF PARMLEN > D100      /* CHK FOR EXCESSIVE LENGTH      */ 00720000
*              THEN DO;             /* YES, TRUNCATE                 */ 00721000
         LH    @14,PARMLEN(,R3)                                 01S0236 00722000
         C     @14,@CF00079                                     01S0236 00723000
         BNH   @RF00236                                         01S0236 00724000
*                ACTSL=CM31SL;      /* INITIALIZE SEGMENT LIST       */ 00725000
         L     @14,@CA00130                                     01S0238 00726000
         MVC   ACTSL(112,WRKAPTR1),CM31SL-CM31SL(@14)           01S0238 00727000
*                R4 = PTPBPTR;      /* SET PUTLINE PARM BLOCK PTR    */ 00728000
         L     R4,PTPBPTR(,WRKAPTR1)                            01S0239 00729000
*                R1 = IOPLPTR;      /* SET PUTLINE PARM LIST PTR     */ 00730000
         L     R1,IOPLPTR(,WRKAPTR1)                            01S0240 00731000
*                R15=ADDR(DOPUTL);  /* PTR TO INVOKE-PUTLINE RTN     */ 00732000
         LA    R15,DOPUTL                                       01S0241 00733000
*                GEN FLOWS(DOPUTL) (SYNCH  (R15));  /* TO PUTLINE    */ 00734000
         SYNCH  (R15)                                                   00735000
*                LENPARM=D100;      /* SET MAXIMUM LENGTH            */ 00736000
         MVC   LENPARM(2,WRKAPTR1),@CH00079                     01S0243 00737000
*              END;                                             01S0244 00738000
*              ELSE                 /* LENGTH IS LEGITIMATE          */ 00739000
*                LENPARM=PARMLEN;   /* SET UP SUPPLIED LENGTH        */ 00740000
         B     @RC00236                                         01S0245 00741000
@RF00236 LH    @14,PARMLEN(,R3)                                 01S0245 00742000
         STH   @14,LENPARM(,WRKAPTR1)                           01S0245 00743000
*            PARMS(1:LENPARM)=PARMBUF;/* MOVE IN PARMS       @ZA12689*/ 00744000
@RC00236 LH    @14,LENPARM(,WRKAPTR1)                           01S0246 00745000
         BCTR  @14,0                                            01S0246 00746000
         L     @10,PARMPTR(,R3)                                 01S0246 00747000
         EX    @14,@SM02733                                     01S0246 00748000
*          END;                                                 01S0247 00749000
*        ECTPCMD = BLDLNAME;        /* PLACE NAME IN ECT FOR TMP IN  */ 00750000
@RC00233 L     @14,ECTPTR(,WRKAPTR2)                            01S0248 00751000
         MVC   ECTPCMD(8,@14),BLDLNAME(WRKAPTR1)                01S0248 00752000
*                                   /*   CASE OF PROGRAM ABEND       */ 00753000
*      END;                                                     01S0249 00754000
*    RESPECIFY (R14) RSTD;                                      01S0250 00755000
