*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */ 01914000
*/*                                                                  */ 01915000
*/* SEGMENT NAME -- TSRCHAPF                                         */ 01916000
*/*                                                                  */ 01917000
*/* DESCRIPTIVE NAME -- N/A                                          */ 01918000
*/*                                                                  */ 01919000
*/* FUNCTION -- TO DETERMINE FROM A LIST OF ELIGIBLE PROGRAM NAMES   */ 01920000
*/*       IF THE CURRENT PROGRAM NAME SHOULD BE ATTACHED WITH APF    */ 01921000
*/*       AUTHORIZATION                                              */ 01922000
*/*   OPERATION --                                                   */ 01923000
*/*           - SET ATTACH-WITH-AUTHORIZATION FLAG TO NO             */ 01924000
*/*           - LOOP OVER NAMES (8 BYTE) IN EXTERNAL TABLE, APFPTABL */ 01925000
*/*           - IF NAME IN LIST IS THE SAME AS THE CURRENT PROGRAM   */ 01926000
*/*             NAME, THEN EXIT FROM LOOP WITH AUTHORIZATION FLAG,   */ 01927000
*/*             DRSAPF, SET TO YES                                   */ 01928000
*/*           - IF A BLANK IS FOUND IN LIST, THEN EXIT FROM LOOP     */ 01929000
*/*           - STEP SEARCH TO NEXT TABLE ENTRY                      */ 01930000
*/*                                                                  */ 01931000
*/* ENTRY -- LINKED BY REGISTER 14                                   */ 01932000
*/*                                                                  */ 01933000
*/* INPUT --  (TSRCHNAM)                                             */ 01934000
*/*                                                                  */ 01935000
*/* OUTPUT -- (DRSAPF)                                               */ 01936000
*/*                                                                  */ 01937000
*/* EXTERNAL REFERENCES -- APFPTABL                                  */ 01938000
*/*                                                                  */ 01939000
*/* EXIT-NORMAL --  RETURN BY LINK REGISTER 14                       */ 01940000
*/*                                                                  */ 01941000
*/*   RETURN CODE -- N/A                                             */ 01942000
*/*                                                                  */ 01943000
*/* EXIT-ERROR -- N/A                                                */ 01944000
*/*                                                                  */ 01945000
*/*   CONDITIONS -- N/A                                              */ 01946000
*/*   RETURN CODE -- N/A                                             */ 01947000
*/*                                                                  */ 01948000
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */ 01949000
*TSRCHAPF:  /* EP TO DETERMINE-ELIGIBILITY-FOR-APF-ATTACH ROUTINE    */ 01950000
*                                                               01S0555 01951000
*    ENTRY;                         /* EP TO ROUTINE                 */ 01952000
TSRCHAPF DS    0H                                               01S0556 01953000
*    RESPECIFY (R1,R14) RSTD;                                   01S0556 01954000
*                                                               01S0556 01955000
*    /* MODESET TO PSW PROTECT KEY 1                                 */ 01956000
*    DO; /*MODESET  KEYADDR(PROK1) WORKREG(R2)*/                01S0557 01957000
*      R2 = PROK1; /*GET KEY FORM SAVE LOCATION*/               01S0558 01958000
         LA    R2,16                                            01S0558 01959000
*      SPKA (R2); /*SET PSW KEY*/                               01S0559 01960000
         SPKA  0(R2)                                            01S0559 01961000
*    END; /*MODESET  KEYADDR(PROK1) WORKREG(R2)*/               01S0560 01962000
*    DRSAPF = NO;                   /* PRESUME NO APF ELIGIBILITY    */ 01963000
         NI    DRSAPF(WRKAPTR2),B'10111111'                     01S0561 01964000
*    /* MODESET TO ENTRY PSW PROTECT KEY                             */ 01965000
*    DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/               01S0562 01966000
*      R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/              01S0563 01967000
         L     R2,TEPKEY(,WRKAPTR2)                             01S0563 01968000
*      SPKA (R2); /*SET PSW KEY*/                               01S0564 01969000
         SPKA  0(R2)                                            01S0564 01970000
*    END; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/              01S0565 01971000
*                                                               01S0565 01972000
*    R1 = ADDR(APFPTABL);           /* LOCATE LIST OF ELIGIBLE NAMES */ 01973000
*                                                               01S0566 01974000
         L     R1,@CV00136                                      01S0566 01975000
*    /* SEARCH LIST                                                  */ 01976000
*    DO WHILE (R1->TE8NAME ^= BLANKS  /* LOOP WHILE NEITHER BLANK    */ 01977000
*        & DRSAPF = NO);            /* LIST NAME NOR MATCHING NAME   */ 01978000
         B     @DE00567                                         01S0567 01979000
@DL00567 DS    0H                                               01S0568 01980000
*      IF R1->TE8NAME = MBRSTXT     /* CHK IF NAMES MATCH            */ 01981000
*        THEN DO;                   /* YES,                          */ 01982000
         CLC   TE8NAME(8,R1),MBRSTXT(WRKAPTR1)                  01S0568 01983000
         BNE   @RF00568                                         01S0568 01984000
*          /* MODESET TO PSW PROTECT KEY 1                           */ 01985000
*          DO; /*MODESET  KEYADDR(PROK1) WORKREG(R2)*/          01S0570 01986000
*            R2 = PROK1; /*GET KEY FORM SAVE LOCATION*/         01S0571 01987000
         LA    R2,16                                            01S0571 01988000
*            SPKA (R2); /*SET PSW KEY*/                         01S0572 01989000
         SPKA  0(R2)                                            01S0572 01990000
*          END; /*MODESET  KEYADDR(PROK1) WORKREG(R2)*/         01S0573 01991000
*          DRSAPF = YES;            /* SET FOR APF ELIGIBILITY       */ 01992000
         OI    DRSAPF(WRKAPTR2),B'01000000'                     01S0574 01993000
*          /* MODESET TO ENTRY PSW PROTECT KEY                       */ 01994000
*          DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/         01S0575 01995000
*            R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/        01S0576 01996000
         L     R2,TEPKEY(,WRKAPTR2)                             01S0576 01997000
*            SPKA (R2); /*SET PSW KEY*/                         01S0577 01998000
         SPKA  0(R2)                                            01S0577 01999000
*          END; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/        01S0578 02000000
*        END;                                                   01S0579 02001000
*      R1 = R1+D8;                  /* STEP PTR TO NEXT ENTRY        */ 02002000
@RF00568 AL    R1,@CF00038                                      01S0580 02003000
*    END;                                                       01S0581 02004000
*                                                               01S0581 02005000
@DE00567 CLC   TE8NAME(8,R1),@CC00039                           01S0581 02006000
         BE    @DC00567                                         01S0581 02007000
         TM    DRSAPF(WRKAPTR2),B'01000000'                     01S0581 02008000
         BZ    @DL00567                                         01S0581 02009000
@DC00567 DS    0H                                               01S0582 02010000
*    END  DODSNAM;                  /* RETURN TO CALLER              */ 02011000
         B     @EL00004                                         01S0582 02012000
*    RESPECIFY (R1,R14) UNRSTD;                                 01S0583 02013000
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */ 02014000
*/*                                                                  */ 02015000
*/* SEGMENT NAME -- DOPARS                                           */ 02016000
*/*                                                                  */ 02017000
*/* DESCRIPTIVE NAME -- N/A                                          */ 02018000
*/*                                                                  */ 02019000
*/* FUNCTION -- TO SCAN AND SYNTAX CHECK THE COMMAND                 */ 02020000
*/*   OPERATION --                                                   */ 02021000
*/*           - SET UP POINTERS TO COMMAND BUFFER AND PARSE PL       */ 02022000
*/*           - PARSE THE COMMAND                                    */ 02023000
*/*           - TURN OFF PDLPRES BIT                                 */ 02024000
*/*           - IF PARSRC IS ZERO, TURN ON PDLPRES BIT AND PLACE     */ 02025000
*/*             PDLANSP IN REGISTER 3 (PTR TO ANS PLACE)             */ 02026000
*/*                                                                  */ 02027000
*/* ENTRY -- LINKED BY REGISTER 14                                   */ 02028000
*/*                                                                  */ 02029000
*/* INPUT --  N/A                                                    */ 02030000
*/*                                                                  */ 02031000
*/* OUTPUT -- N/A                                                    */ 02032000
*/*                                                                  */ 02033000
*/* EXTERNAL REFERENCES -- N/A                                       */ 02034000
*/*                                                                  */ 02035000
*/* EXIT-NORMAL --  RETURN VIA SVC 3                                 */ 02036000
*/*                                                                  */ 02037000
*/*   RETURN CODE -- PARSRC = 0                                      */ 02038000
*/*                                                                  */ 02039000
*/* EXIT-ERROR -- RETURN VIA SVC 3                                   */ 02040000
*/*                                                                  */ 02041000
*/*   CONDITIONS -- PARSE FAILURE                                    */ 02042000
*/*   RETURN CODE -- PARSRC > 0                                      */ 02043000
*/*                                                                  */ 02044000
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */ 02045000
*DOPARS:    /* CHK SYNTAX OF CALL COMMAND OPERANDS                   */ 02046000
*                                                               01S0584 02047000
*    PROC OPTIONS(DONTSAVE,NOSAVEAREA);  /* NO SAVE AREA             */ 02048000
DOPARS   DS    0H                                               01S0585 02049000
*    RESPECIFY (PPL) BASED(R1);                                 01S0585 02050000
*                                                               01S0585 02051000
*    IF MSGNO = ZERO                /* CHK FOR TERMINATION FLAG      */ 02052000
*      THEN DO;                     /* NO, CONTINUE TO PARSE         */ 02053000
         SLR   @14,@14                                          01S0586 02054000
         C     @14,MSGNO(,WRKAPTR1)                             01S0586 02055000
         BNE   @RF00586                                         01S0586 02056000
*        DO; /*CALLTSSR EP(IKJPARS)*/                           01S0588 02057000
*          RESPECIFY GPR01P RSTD;                               01S0589 02058000
*          IF CVTPARS = 0                                       01S0590 02059000
*            THEN                                               01S0590 02060000
         L     @10,CVTPTR                                       01S0590 02061000
         C     @14,CVTPARS(,@10)                                01S0590 02062000
         BNE   @RF00590                                         01S0590 02063000
*              GEN(LINK EP=IKJPARS);                            01S0591 02064000
         LINK EP=IKJPARS                                                02065000
*            ELSE                                               01S0592 02066000
*              CALL CVTPARS;                                    01S0592 02067000
         B     @RC00590                                         01S0592 02068000
@RF00590 L     @10,CVTPTR                                       01S0592 02069000
         L     @15,CVTPARS(,@10)                                01S0592 02070000
         BALR  @14,@15                                          01S0592 02071000
*          RESPECIFY GPR01P UNRSTD;                             01S0593 02072000
@RC00590 DS    0H                                               01S0594 02073000
*        END;     /* TO CHK DOMMAND SYNTAX         */           01S0594 02074000
*                                                               01S0594 02075000
*        PARSRC = R15;              /* RESERVE PARSE RETURN CODE     */ 02076000
*                                                               01S0595 02077000
         ST    R15,PARSRC+1236(,WRKAPTR1)                       01S0595 02078000
*        PDLPRES = NO;              /* NO PDL TO RELEASE             */ 02079000
         NI    PDLPRES(WRKAPTR1),B'01111111'                    01S0596 02080000
*        IF PARSRC = ZERO           /* CHK FOR SUCCESSFUL PARSE      */ 02081000
*          THEN DO;                 /* YES,                          */ 02082000
         L     @14,PARSRC+1236(,WRKAPTR1)                       01S0597 02083000
         LTR   @14,@14                                          01S0597 02084000
         BNZ   @RF00597                                         01S0597 02085000
*            R3 = PDLANSP;          /*   SET POINTER TO PDL SPACE    */ 02086000
         L     @14,PPLANS(,R1)                                  01S0599 02087000
         L     R3,PDLANSP(,@14)                                 01S0599 02088000
*            PDLPRES = YES;         /*   AND FLAG TO DO IKJRLSA      */ 02089000
         OI    PDLPRES(WRKAPTR1),B'10000000'                    01S0600 02090000
*          END;                                                 01S0601 02091000
*          ELSE                     /* INDICATE PARSE FAILURE        */ 02092000
*            IF MSGNO = ZERO        /* CHK FOR TERMINATION FLAG      */ 02093000
*              THEN                 /* NO,                           */ 02094000
         B     @RC00597                                         01S0602 02095000
@RF00597 L     @14,MSGNO(,WRKAPTR1)                             01S0602 02096000
         LTR   @14,@14                                          01S0602 02097000
         BNZ   @RF00602                                         01S0602 02098000
*                MSGNO = D1;        /*   SET PARSE FAILURE FLAG      */ 02099000
         MVC   MSGNO(4,WRKAPTR1),@CF00032                       01S0603 02100000
*      END;                                                     01S0604 02101000
*                                                               01S0604 02102000
@RF00602 DS    0H                                               01S0604 02103000
@RC00597 DS    0H                                               01S0605 02104000
*    GEN(SVC   3);                  /* RETURN VIA SVC 3              */ 02105000
@RF00586 DS    0H                                               01S0605 02106000
         SVC   3                                                        02107000
*    RESPECIFY (PPL) BASED(PPLPTR);                             01S0606 02108000
*    RETURN;                        /* TERMINATE ROUTINE             */ 02109000
@EL00005 DS    0H                                               01S0607 02110000
@EF00005 DS    0H                                               01S0607 02111000
@ER00005 BR    @14                                              01S0607 02112000
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */ 02113000
*/*                                                                  */ 02114000
*/* SEGMENT NAME -- DOQUAL                                           */ 02115000
*/*                                                                  */ 02116000
*/* DESCRIPTIVE NAME -- N/A                                          */ 02117000
*/*                                                                  */ 02118000
*/* FUNCTION -- TO FULLY QUALIFY THE DS NAME                         */ 02119000
*/*   OPERATION --                                                   */ 02120000
*/*           - SET DAIR CONTROL FLAG                                */ 02121000
*/*           - IF DSNAME IS NOT ENTERED, DSNAME IS 'DEFAULT.LOAD'   */ 02122000
*/*           - IF DSNAME IS ENTERED, AND LAST QUALIFIER IS NOT      */ 02123000
*/*             ALREADY '.LOAD', APPEND '.LOAD' TO DSNAME AND UPDATE */ 02124000
*/*             LENGTH                                               */ 02125000
*/*                                                                  */ 02126000
*/* ENTRY -- LINKED BY REGISTER 14                                   */ 02127000
*/*                                                                  */ 02128000
*/* INPUT --  N/A                                                    */ 02129000
*/*                                                                  */ 02130000
*/* OUTPUT -- LENGTH OF FULLY QUALIFIED DSNAME                       */ 02131000
*/*                                                                  */ 02132000
*/* EXTERNAL REFERENCES -- N/A                                       */ 02133000
*/*                                                                  */ 02134000
*/* EXIT-NORMAL --  RETURN BY LINK REGISTER 14                       */ 02135000
*/*                                                                  */ 02136000
*/*   RETURN CODE -- N/A                                             */ 02137000
*/*                                                                  */ 02138000
*/* EXIT-ERROR -- RETURN BY LINK REGISTER 14                         */ 02139000
*/*                                                                  */ 02140000
*/*   CONDITIONS -- N/A                                              */ 02141000
*/*   RETURN CODE -- N/A                                             */ 02142000
*/*                                                                  */ 02143000
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */ 02144000
*DOQUAL:    /* ESTABLISH FULL QUALIFICATION OF DSNAME                */ 02145000
*                                                               01S0608 02146000
*    ENTRY;                         /* EP TO ROUTINE                 */ 02147000
DOQUAL   DS    0H                                               01S0609 02148000
*    RESPECIFY (R14) RSTD;                                      01S0609 02149000
*                                                               01S0609 02150000
*    /* SET STANDARD QUALIFICATION                                   */ 02151000
*    DA08UID = YES;                 /* SET DAIR CONTROL FLAG         */ 02152000
         L     @01,DAPLPTR(,WRKAPTR1)                           01S0610 02153000
         L     @01,DAPLDAPB(,@01)                               01S0610 02154000
         OI    DA08UID(@01),B'00100000'                         01S0610 02155000
*    SWDOQ = YES;                   /* SET FLAG INDICATING   @ZA20551*/ 02156000
         OI    SWDOQ(WRKAPTR1),B'00100000'                      01S0611 02157000
*                                   /* DS. WAS FULLY QUALIFY @ZA20551*/ 02158000
*    IF DSNLENG = ZERO              /* CHK IF DSNAME ENTERED         */ 02159000
*      THEN DO;                     /* NO,                           */ 02160000
         LH    @10,DSNLENG(,WRKAPTR1)                           01S0612 02161000
         LTR   @10,@10                                          01S0612 02162000
         BNZ   @RF00612                                         01S0612 02163000
*        DSNBUF = CMPRE;                                        01S0614 02164000
         MVI   DSNBUF+4(WRKAPTR1),C' '                          01S0614 02165000
         MVC   DSNBUF+5(39,WRKAPTR1),DSNBUF+4(WRKAPTR1)         01S0614 02166000
         MVC   DSNBUF(4,WRKAPTR1),CMPRE                         01S0614 02167000
*        DSNLENG = D4;                                          01S0615 02168000
         MVC   DSNLENG(2,WRKAPTR1),@CH00044                     01S0615 02169000
*        DSSGLEN = D8;              /* SET DS LENGTH FOR MSG @ZA18797*/ 02170000
         MVC   DSSGLEN(2,WRKAPTR1),@CH00038                     01S0616 02171000
*        DSSGTXT = CMPRE;           /* SET DS NAME FOR MSG   @ZA18797*/ 02172000
         MVI   DSSGTXT+4(WRKAPTR1),C' '                         01S0617 02173000
         MVC   DSSGTXT+5(39,WRKAPTR1),DSSGTXT+4(WRKAPTR1)       01S0617 02174000
         MVC   DSSGTXT(4,WRKAPTR1),CMPRE                        01S0617 02175000
*      END;                                                     01S0618 02176000
*      ELSE DO;                     /* DSNAME WAS ENTERED            */ 02177000
         B     @RC00612                                         01S0619 02178000
@RF00612 DS    0H                                               01S0620 02179000
*        RESPECIFY (X) RSTD;                                    01S0620 02180000
*        X = ADDR(DSNBUF);                                      01S0621 02181000
         LA    X,DSNBUF(,WRKAPTR1)                              01S0621 02182000
*        IF DSNLENG > D3                                        01S0622 02183000
*          THEN                                                 01S0622 02184000
         LH    @10,DSNLENG(,WRKAPTR1)                           01S0622 02185000
         C     @10,@CF00058                                     01S0622 02186000
         BNH   @RF00622                                         01S0622 02187000
*            X = X+DSNLENG-D5;                                  01S0623 02188000
         ALR   @10,X                                            01S0623 02189000
         SL    @10,@CF00061                                     01S0623 02190000
         LR    X,@10                                            01S0623 02191000
*        IF DSNLENG ^> D3                                       01S0624 02192000
*            | LAST5 ^= QUALIFR                                 01S0624 02193000
*          THEN DO;                                             01S0624 02194000
@RF00622 LH    @10,DSNLENG(,WRKAPTR1)                           01S0624 02195000
         C     @10,@CF00058                                     01S0624 02196000
         BNH   @RT00624                                         01S0624 02197000
         CLC   LAST5(5,X),QUALIFR                               01S0624 02198000
         BE    @RF00624                                         01S0624 02199000
@RT00624 DS    0H                                               01S0625 02200000
*            IF DSNLENG+5 ^> D44                                01S0626 02201000
*              THEN DO;                                         01S0626 02202000
         LH    @10,DSNLENG(,WRKAPTR1)                           01S0626 02203000
         LA    @02,5                                            01S0626 02204000
         ALR   @02,@10                                          01S0626 02205000
         C     @02,@CF00073                                     01S0626 02206000
         BH    @RF00626                                         01S0626 02207000
*                X = ADDR(DSNBUF)+DSNLENG;                      01S0628 02208000
         LA    X,DSNBUF(,WRKAPTR1)                              01S0628 02209000
         ALR   X,@10                                            01S0628 02210000
