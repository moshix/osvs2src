*    /* LOAD DAIRFAIL ROUTINE                                        */ 01766000
*    DO;        /*LOAD EP('IKJEFF18') */                        01S0517 01767000
*    RESPECIFY (GPR00P,GPR01P) RESTRICTED;                      01S0518 01768000
*    GPR00P=ADDR('IKJEFF18');   /*LOAD ENTRY POINT NAME ADDRESS*/       01769000
