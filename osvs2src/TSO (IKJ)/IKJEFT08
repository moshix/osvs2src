         L     @14,DAPLDAPB(,@14)                               01S0388 01247000
         MVI   DA08CTL(@14),X'00'                               01S0388 01248000
*        /* SET UP DATA SET NAME                                     */ 01249000
*        DSNLENG = ZERO;            /* INITIALIZE TO NO ENTRY        */ 01250000
         SLR   @14,@14                                          01S0389 01251000
         STH   @14,DSNLENG(,WRKAPTR1)                           01S0389 01252000
*        IF DSNPRES = YES           /* CHK IF DSNAME ENTERED         */ 01253000
*          THEN DO;                 /* YES,                          */ 01254000
         TM    DSNPRES(R3),B'10000000'                          01S0390 01255000
