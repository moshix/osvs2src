*DOCALL2:  /* EP TO CLOSE-OUT-CALL-FUNCTION ROUTINE                  */ 00223000
*                                                               01S0117 00224000
*    ENTRY;                                                     01S0117 00225000
@EP00117 DS    0H                                               01S0118 00226000
*    RTNCODE = ZERO;                /* INITIALIZE TO SUCCESS RC      */ 00227000
         L     @14,SAVRBAK(,R13)                                01S0118 00228000
         SLR   @10,@10                                          01S0118 00229000
         ST    @10,RTNCODE(,@14)                                01S0118 00230000
*    CALL  DOMSG;                   /* TO ISSUE ANY TERMINATION MSG  */ 00231000
         BAL   @14,DOMSG                                        01S0119 00232000
*DOCAL2A:                           /* FOR PSEUDO-RETURN             */ 00233000
*    CALL  DOTERM;                  /* TO CLOSE-OUT CALL             */ 00234000
*                                                               01S0120 00235000
DOCAL2A  BAL   @14,DOTERM                                       01S0120 00236000
*/*  END;                           **  (LOGICAL) OF CALL PROCESSOR  */ 00237000
*MAINX:  /* RETURN FORCES EPILOG FUNCTION                            */ 00238000
*    RETURN  CODE(RTNCODE);         /* END OF CALL PROCESSOR         */ 00239000
MAINX    L     @10,SAVRBAK(,R13)                                01S0121 00240000
         L     @10,RTNCODE(,@10)                                01S0121 00241000
         L     @13,4(,@13)                                      01S0121 00242000
         L     @00,@SIZDATD                                     01S0121 00243000
         LR    @01,@12                                          01S0121 00244000
         FREEMAIN R,LV=(0),A=(1)                                        00245000
         LR    @15,@10                                          01S0121 00246000
         L     @14,12(,@13)                                     01S0121 00247000
         LM    @00,@12,20(@13)                                  01S0121 00248000
         BR    @14                                              01S0121 00249000
