*/*                                                                  */ 01296000
*/* SEGMENT NAME -- DODAIR                                           */ 01297000
*/*                                                                  */ 01298000
*/* DESCRIPTIVE NAME -- N/A                                          */ 01299000
*/*                                                                  */ 01300000
*/* FUNCTION -- TO ALLOCATE THE SPECIFIED DATA SET                   */ 01301000
*/*   OPERATION --                                                   */ 01302000
*/*           - SAVE ADDRESS OF DAIR PARM LIST, PSCB AND DSNAME      */ 01303000
*/*             BUFFER                                               */ 01304000
*/*           - BLANK DDN, UNIT AND SERIAL                           */ 01305000
*/*           - ZERO RETURN CODES, PARSE PARAMETERS, STATUS AND      */ 01306000
*/*             DISPOSITION                                          */ 01307000
*/*           - SET STATUS OF SHR                                    */ 01308000
*/*           - SET DISPOSITION OF KEEP                              */ 01309000
*/*           - PERFORM SEGMENT DODAIR TO ALLOCATE DATA SET          */ 01310000
*/*           - SAVE DAIR RETURN CODE                                */ 01311000
*/*           - IF DAIR RETURN CODE IS ZERO, DETERMINE IF DS IS A    */ 01312000
*/*             PARTITIONED DATA SET:                                */ 01313000
*/*                  YES - PLACE DDNAME (FROM DAIR) IN DCB           */ 01314000
*/*                  NO - SET UP DS-TYPE-ERROR MESSAGE               */ 01315000
*/*                                                                  */ 01316000
*/* ENTRY -- LINKED BY REGISTER 14                                   */ 01317000
*/*                                                                  */ 01318000
*/* INPUT -- N/A                                                     */ 01319000
*/*                                                                  */ 01320000
*/* OUTPUT -- DAIR RETURN CODE                                       */ 01321000
*/*                                                                  */ 01322000
*/* EXTERNAL REFERENCES -- N/A                                       */ 01323000
*/*                                                                  */ 01324000
*/* EXIT-NORMAL -- RETURN VIA SVC 3                                  */ 01325000
*/*                MSGNO = 0                                         */ 01326000
*/*                                                                  */ 01327000
*/*  RETURN CODE -- DAIRRC = 0                                       */ 01328000
*/*                                                                  */ 01329000
*/* EXIT-ERROR -- RETURN VIA SVC 3                                   */ 01330000
*/*                                                                  */ 01331000
*/*   CONDITIONS -- DATA-SET-TYPE ERROR (MSGNO = 11)                 */ 01332000
*/*   RETURN CODE -- DAIRRC > 0                                      */ 01333000
*/*                                                                  */ 01334000
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */ 01335000
*DODAIR:    /* DETERMINE WHETHER THE DATA SET IS CURRENTLY ALLOCATED */ 01336000
*                                                               01S0404 01337000
*    ENTRY;                                                     01S0404 01338000
DODAIR   DS    0H                                               01S0405 01339000
*    RESPECIFY (R4) RSTD;                                       01S0405 01340000
*                                                               01S0405 01341000
*    RESPECIFY (DAPL) BASED(R2);                                01S0406 01342000
*    RESPECIFY (CPPL) BASED(R4);                                01S0407 01343000
*    DA08CD = '0008'X;              /* OPERATION CODE                */ 01344000
         L     @14,DAPLDAPB(,R2)                                01S0408 01345000
         MVC   DA08CD(2,@14),@CB02660                           01S0408 01346000
*    DAPLPSCB = CPPLPSCB;           /* PTR TO PSCB                   */ 01347000
         L     @10,CPPLPSCB(,R4)                                01S0409 01348000
         ST    @10,DAPLPSCB(,R2)                                01S0409 01349000
*    DA08PDSN=ADDR(DSNBUFFR);       /* ADDRESS OF DSNAME BUFFER      */ 01350000
         LA    @10,DSNBUFFR(,WRKAPTR1)                          01S0410 01351000
         ST    @10,DA08PDSN(,@14)                               01S0410 01352000
*    BLANK1 = ' ';                  /* BLANK DDN, UNIT AND SERIAL    */ 01353000
         MVI   BLANK1+1(@14),C' '                               01S0411 01354000
         MVC   BLANK1+2(22,@14),BLANK1+1(@14)                   01S0411 01355000
         MVI   BLANK1(@14),C' '                                 01S0411 01356000
*    ZERO1A=ZERO1A&&ZERO1A;         /* ZERO RETURN CODES             */ 01357000
         XC    ZERO1A(6,@14),ZERO1A(@14)                        01S0412 01358000
*    ZERO1=ZERO1 &&ZERO1;           /* ZERO PARSE PARAMETERS         */ 01359000
         XC    ZERO1(16,@14),ZERO1(@14)                         01S0413 01360000
*    ZERO2=ZERO2 &&ZERO2;           /* ZERO STATUS, DISPOSITION      */ 01361000
         XC    ZERO2(3,@14),ZERO2(@14)                          01S0414 01362000
*    DA08SHR = YES;                 /* STATUS OF SHR                 */ 01363000
         OI    DA08SHR(@14),B'00001000'                         01S0415 01364000
*    DA08KEEP= YES;                 /* DISPOSITION OF KEEP           */ 01365000
*                                                               01S0416 01366000
         OI    DA08KEEP(@14),B'00001000'                        01S0416 01367000
*    R1 = ADDR(DAPL);               /* PTR TO SERVICE RTN PB         */ 01368000
         LR    R1,R2                                            01S0417 01369000
*    GEN(LINK EP=IKJEFD00);         /* TO DAIR FOR ALLOCATION OF DS  */ 01370000
         LINK EP=IKJEFD00                                               01371000
*    DAIRRC = R15;                  /* SAVE DAIR RETURN CODE         */ 01372000
*                                                               01S0419 01373000
         ST    R15,DAIRRC(,WRKAPTR1)                            01S0419 01374000
*    IF DAIRRC = ZERO               /* CHK FOR SUCCESSFUL ALLOCATION */ 01375000
*      THEN DO;                     /* YES                           */ 01376000
         SLR   @14,@14                                          01S0420 01377000
         L     @10,DAIRRC(,WRKAPTR1)                            01S0420 01378000
         CLR   @10,@14                                          01S0420 01379000
         BNE   @RF00420                                         01S0420 01380000
*        IF (DA08DSO & PODS) ^= PODS/* IS DATASET NOT PDS    @ZA14237*/ 01381000
*            & MSGNO = ZERO         /*     & ANY TERMINATION FLAG    */ 01382000
*          THEN                     /* NO,                           */ 01383000
         L     @10,DAPLDAPB(,R2)                                01S0422 01384000
         MVC   @TS00001(1),DA08DSO(@10)                         01S0422 01385000
         NI    @TS00001,X'02'                                   01S0422 01386000
         CLI   @TS00001,X'02'                                   01S0422 01387000
         BE    @RF00422                                         01S0422 01388000
         C     @14,MSGNO(,WRKAPTR1)                             01S0422 01389000
         BNE   @RF00422                                         01S0422 01390000
*            MSGNO = D11;           /* DATA SET TYPE ERROR           */ 01391000
         MVC   MSGNO(4,WRKAPTR1),@CF00066                       01S0423 01392000
*                                   /*      DAIR IN THE DCB          */ 01393000
*      END;                                                     01S0424 01394000
@RF00422 DS    0H                                               01S0425 01395000
*    GEN(SVC    3);                  /* RETURN TO CALLER             */ 01396000
@RF00420 DS    0H                                               01S0425 01397000
         SVC    3                                                       01398000
*    RESPECIFY (DAPL) BASED(DAPLPTR);                           01S0426 01399000
*    RESPECIFY (CPPL) BASED(CPPLPTR);                           01S0427 01400000
*    RESPECIFY (R4) UNRSTD;                                     01S0428 01401000
*    RETURN;                         /* TERMINATE ROUTINE            */ 01402000
         B     @EL00004                                         01S0429 01403000
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */ 01404000
