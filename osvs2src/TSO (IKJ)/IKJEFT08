         MVI   DA08MNM(@14),C' '                                01S0384 01228000
         MVC   DA08MNM+1(7,@14),DA08MNM(@14)                    01S0384 01229000
*        /* SET UP PASSWORD IN DAIR PB                               */ 01230000
*        DA08PSWD = '';             /* INITIALIZE TO BLANKS  @ZA20355*/ 01231000
         MVI   DA08PSWD(@14),C' '                               01S0385 01232000
         MVC   DA08PSWD+1(7,@14),DA08PSWD(@14)                  01S0385 01233000
*        IF PASSPRES = YES          /* CHK IF PASSWORD ENTERED       */ 01234000
*          THEN                     /* YES,                          */ 01235000
