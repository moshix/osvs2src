         STH   @10,MBRSLEN(,WRKAPTR1)                           01S0379 01212000
*            MBRSTXT = '' ;         /* INITIALIZE TEXT TO BLANKS     */ 01213000
         MVI   MBRSTXT(WRKAPTR1),C' '                           01S0380 01214000
         MVC   MBRSTXT+1(7,WRKAPTR1),MBRSTXT(WRKAPTR1)          01S0380 01215000
*            MBRSTXT(1:MBRLEN) =    /* INSERT MEMBER NAME AS         */ 01216000
*                MBRBUF(1:MBRLEN);  /*     ENTERED                   */ 01217000
         BCTR  @14,0                                            01S0381 01218000
         L     @10,MBRPTR(,R3)                                  01S0381 01219000
         EX    @14,@SM02737                                     01S0381 01220000
*            MBRDTXT = MBRSTXT;     /* SET MEMBER NAME FOR DAIR      */ 01221000
         MVC   MBRDTXT(8,WRKAPTR1),MBRSTXT(WRKAPTR1)            01S0382 01222000
*          END;                                                 01S0383 01223000
*        DA08MNM = '';              /* BLANK MEMBERNAME IN DAIR PB   */ 01224000
*                                                               01S0384 01225000
@RF00377 L     @14,DAPLPTR(,WRKAPTR1)                           01S0384 01226000
