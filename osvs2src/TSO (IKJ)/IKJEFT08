*                                                               01S0372 01192000
DODSNAM  DS    0H                                               01S0373 01193000
*    IF MSGNO = ZERO                /* CHK FOR TERMINATION FLAG      */ 01194000
*      THEN DO;                     /* NO, CONTINUE                  */ 01195000
         L     @14,MSGNO(,WRKAPTR1)                             01S0373 01196000
         LTR   @14,@14                                          01S0373 01197000
         BNZ   @RF00373                                         01S0373 01198000
*        /* SET UP MEMBER NAME IN MSG SEGMENT AND DAIR PB            */ 01199000
*        MBRDLEN = D8;              /* SET LENGTH FOR DAIR           */ 01200000
         MVC   MBRDLEN(2,WRKAPTR1),@CH00038                     01S0375 01201000
*        MBRSLEN = D12;             /* SET LENGTH FOR MSG            */ 01202000
         MVC   MBRSLEN(2,WRKAPTR1),@CH00068                     01S0376 01203000
*        IF BLANKB = NO             /*                               */ 01204000
*          THEN DO;                 /* YES                           */ 01205000
         TM    BLANKB(WRKAPTR1),B'00010000'                     01S0377 01206000
         BNZ   @RF00377                                         01S0377 01207000
*            MBRSLEN=MBRLEN+D4;     /* SET LENGTH FOR MSG            */ 01208000
