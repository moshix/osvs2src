         MVC   BLDLENT(2,WRKAPTR1),@CH02616                     01S0207 00605000
*      BLDLELNG = '0040'X;          /* MEMBER BLDL LENGTH            */ 00606000
         MVC   BLDLELNG(2,WRKAPTR1),@CH02617                    01S0208 00607000
*      BLDLNAME = MBRSTXT;          /* MOVE MEMBER NAME TO BLDL      */ 00608000
         MVC   BLDLNAME(8,WRKAPTR1),MBRSTXT(WRKAPTR1)           01S0209 00609000
*                                   /*   PARAMETER LIST              */ 00610000
*      RESPECIFY (R5) RSTD;         /* RESTRICT REGISTER 5           */ 00611000
*      RESPECIFY (R4) RSTD;         /* RESTRICT REGISTER 4           */ 00612000
*      R4 = ADDR (CALLDCB);         /* ADDRESS OF DCB                */ 00613000
         LA    R4,CALLDCB(,WRKAPTR1)                            01S0212 00614000
*      R5 = ADDR (BLDLLST);         /* ADDRESS OF PARM LIST FOR BLDL */ 00615000
         LA    R5,BLDLLST(,WRKAPTR1)                            01S0213 00616000
*      GENERATE (BLDL (4),(5));     /* BLDL ON LIBRARY               */ 00617000
         BLDL (4),(5)                                                   00618000
*      BLDLRC = R15;                /* SAVE BLDL RETURN CODE         */ 00619000
         ST    R15,BLDLRC(,WRKAPTR1)                            01S0215 00620000
*      RESPECIFY (R4) UNRSTD;       /* UNRESTRICT REGISTER 4         */ 00621000
*      RESPECIFY (R5) UNRSTD;       /* UNRESTRICT REGISTER 5         */ 00622000
*      IF BLDLRC = D4               /* CHK FOR MEMBER NOT FOUND      */ 00623000
*        THEN DO;                   /* YES,                          */ 00624000
         L     @14,BLDLRC(,WRKAPTR1)                            01S0218 00625000
         CL    @14,@CF00044                                     01S0218 00626000
         BNE   @RF00218                                         01S0218 00627000
*          CALL  DOGETM;            /*   PROMPT FOR MEMBER NAME      */ 00628000
         BAL   @14,DOGETM                                       01S0220 00629000
*DOBLDLA:                           /* FOR PSEUDO-RETURN             */ 00630000
*        END;                                                   01S0221 00631000
*        ELSE                       /* IS NOT MBR-NOT-FOUND CODE     */ 00632000
*          IF BLDLRC > D4           /* CHK FOR BLDL ERROR            */ 00633000
*              & MSGNO=ZERO         /*     & NO TERMINATION FLAG     */ 00634000
*            THEN                   /* YES,                          */ 00635000
         B     @RC00218                                         01S0222 00636000
