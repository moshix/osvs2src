IKJEFT40 CSECT ,                                                   0001 00003000
IKJPUTL  DS    0H                                                  0001 00004000
         ENTRY IKJPUTL                                                  00005000
@MAINENT DS    0H                                                  0001 00006000
         USING *,@15                                               0001 00007000
         B     @PROLOG                                             0001 00008000
         DC    AL2(@EP00001-@MAINENT)                                   00009000
         DC    AL1(16)                                             0001 00010000
         DC    C'IKJEFT40  74.131'                                 0001 00011000
         DROP  @15                                                      00012000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00013000
         BALR  @12,0                                               0001 00014000
@PSTART  DS    0H                                                  0001 00015000
         USING @PSTART,@12                                         0001 00016000
         L     @00,@SIZDATD                                        0001 00017000
         GETMAIN  R,LV=(0)                                              00018000
         LR    @11,@01                                             0001 00019000
         USING @DATD,@11                                           0001 00020000
         ST    @13,@SA00001+4                                      0001 00021000
         LM    @15,@01,16(@13)                                     0001 00022000
         ST    @11,8(,@13)                                         0001 00023000
         LR    @13,@11                                             0001 00024000
         AH    @15,4(,@15)                                         0001 00025000
         BR    @15                                                 0001 00026000
@EP00001 MVC   @PC00001(16),0(@01)                                 0001 00027000
*                                                                  0025 00028000
*   /*****************************************************************/ 00029000
*   /*                                                               */ 00030000
*   /* START EXECUTABLE CODE.                                        */ 00031000
*   /*                                                               */ 00032000
*   /*****************************************************************/ 00033000
*                                                                  0025 00034000
*   SAVEREG1=R1;                                                   0025 00035000
         LR    SAVEREG1,R1                                         0025 00036000
*   FRESWICH=0;                     /* INITIALIZE FREEMAIN TO OFF  0026 00037000
*                                      (DON'T FREE)                  */ 00038000
         SLR   @14,@14                                             0026 00039000
         ST    @14,FRESWICH                                        0026 00040000
*WRITERTN:                                                         0027 00041000
*   TXIFONLY=ADDR(FORMONLY);        /* FILL IN TEXTIN PARM           */ 00042000
WRITERTN LA    @14,FORMONLY                                        0027 00043000
         ST    @14,TXIFONLY                                        0027 00044000
*   IF PARMFORM='1'B THEN           /* FORMAT ONLY BIT ON            */ 00045000
         L     @14,PTPBPTR(,R1)                                    0028 00046000
         TM    PARMFORM(@14),B'00100000'                           0028 00047000
         BNO   @RF00028                                            0028 00048000
*     DO;                                                          0029 00049000
*       FORMONLY=1;                 /* INDICATE FORMAT ONLY FOR    0030 00050000
*                                      TEXTIN                        */ 00051000
         MVC   FORMONLY(4),@CF00040                                0030 00052000
*       TXIFRMSG=ADDR(PARMAFRM);    /* USER'S PTPB FIELD             */ 00053000
         LA    @10,PARMAFRM(,@14)                                  0031 00054000
         ST    @10,TXIFRMSG                                        0031 00055000
*       PARMAFRM='00000000'X;       /* TURN CHAIN FLAG OFF           */ 00056000
         SLR   @10,@10                                             0032 00057000
         ST    @10,PARMAFRM(,@14)                                  0032 00058000
*       TXISEGMT=PARMAOUT;          /* POINT TO MSG SEGS BLOCK       */ 00059000
         L     @14,PARMAOUT(,@14)                                  0033 00060000
         ST    @14,TXISEGMT                                        0033 00061000
*       R1=ADDR(TXINPARM);          /* TEXTIN'S PARMS                */ 00062000
         LA    R1,TXINPARM                                         0034 00063000
*       CALL IKJEFT54;              /* PERFORM TEXT INSERTION        */ 00064000
         L     @15,@CV00211                                        0035 00065000
         BALR  @14,@15                                             0035 00066000
*       GOTO EXIT;                                                 0036 00067000
         B     EXIT                                                0036 00068000
*     END;                                                         0037 00069000
*   TRMOPARM(1:12)=PUTLPARM;        /* MOVE IOPL TO OWN CORE         */ 00070000
@RF00028 MVC   TRMOPARM(12),PUTLPARM(R1)                           0038 00071000
*   ADRDPRMB=ADDR(DPARMBLK);        /* POINT TO DUPLICATE PTPB       */ 00072000
         LA    @14,DPARMBLK                                        0039 00073000
         ST    @14,ADRDPRMB                                        0039 00074000
*   DPARMBLK=PTPB;                  /* COPY PTPB IN MY        STORAGE*/ 00075000
         L     @14,PTPBPTR(,R1)                                    0040 00076000
         MVC   DPARMBLK(12),PTPB(@14)                              0040 00077000
*   IF PARMDTMS='1'B THEN           /* DATA-MESSAGE BIT = DATA       */ 00078000
         TM    PARMDTMS(@14),B'00100000'                           0041 00079000
         BNO   @RF00041                                            0041 00080000
*     DO;                                                          0042 00081000
*TERMOUT:                                                          0043 00082000
*       R1=ADDR(TRMOPARM);          /* POINT TO DUPLICATE IOPL       */ 00083000
TERMOUT  LA    R1,TRMOPARM                                         0043 00084000
*       CALL IKJEFT56;              /* GOTO TERM-OUT                 */ 00085000
         L     @15,@CV00215                                        0044 00086000
         BALR  @14,@15                                             0044 00087000
*       GOTO EXIT;                                                 0045 00088000
         B     EXIT                                                0045 00089000
*     END;                          /* END WRITE DATA PROCESSING     */ 00090000
*   IF PARMIFOR='1'B THEN           /* INFORMATIONAL BIT ON          */ 00091000
@RF00041 L     @14,PTPBPTR(,R1)                                    0047 00092000
         TM    PARMIFOR(@14),B'00000010'                           0047 00093000
         BNO   @RF00047                                            0047 00094000
*     DO;                                                          0048 00095000
*       IF ECTIOWA^=0&INSEXEC='1'B& /* NOMSG Y30PQJM                 */ 00096000
*           INSTERM='0'B&NOMSG='1'B THEN/* Y30PQJM                   */ 00097000
         L     @14,ECTPTR(,R1)                                     0049 00098000
         L     @14,ECTIOWA(,@14)                                   0049 00099000
         LTR   @14,@14                                             0049 00100000
         BZ    @RF00049                                            0049 00101000
         L     @14,IOSTELM(,@14)                                   0049 00102000
         TM    INSEXEC(@14),B'00001000'                            0049 00103000
         BNO   @RF00049                                            0049 00104000
         TM    INSTERM(@14),B'10000000'                            0049 00105000
         BNZ   @RF00049                                            0049 00106000
         L     @14,LSDPTR-1(,@14)                                  0049 00107000
         LA    @14,0(,@14)                                         0049 00108000
         L     @14,LSDEXEC(,@14)                                   0049 00109000
         TM    NOMSG(@14),B'00000010'                              0049 00110000
         BO    @RT00049                                            0049 00111000
*         GOTO NORMEND;             /* RC = ZERO Y30PQJM             */ 00112000
*       IF PARMAOUT=0 THEN          /* NO OUTPUT LINE                */ 00113000
@RF00049 L     @14,PTPBPTR(,R1)                                    0051 00114000
         L     @14,PARMAOUT(,@14)                                  0051 00115000
         LTR   @14,@14                                             0051 00116000
         BNZ   @RF00051                                            0051 00117000
*         DO;                       /* PRESENT.                      */ 00118000
*           R1=ADDR(TRMOPARM);      /* POINT TO DUP IOPL             */ 00119000
         LA    R1,TRMOPARM                                         0053 00120000
*           CALL IKJEFT52;          /* CHAIN OUT SUBROUTINE          */ 00121000
         L     @15,@CV00218                                        0054 00122000
         BALR  @14,@15                                             0054 00123000
*           GOTO EXIT;                                             0055 00124000
         B     EXIT                                                0055 00125000
*         END;                                                     0056 00126000
*PUTOUT:                                                           0057 00127000
*       TXIFRMSG=ADRDPRMB+4;        /* PLACE FORMATTED MSG IN      0057 00128000
*                                      TERMOUTS PARM BLOCK           */ 00129000
@RF00051 DS    0H                                                  0057 00130000
PUTOUT   LA    @14,4                                               0057 00131000
         AL    @14,ADRDPRMB                                        0057 00132000
         ST    @14,TXIFRMSG                                        0057 00133000
*       IF PARMMLEV='0'B THEN       /* IF THIS IS SINGLE LEVEL       */ 00134000
         L     @14,PTPBPTR(,R1)                                    0058 00135000
         TM    PARMMLEV(@14),B'00000100'                           0058 00136000
         BNZ   @RF00058                                            0058 00137000
*         TXISEGMT=PARMAOUT;        /* PLACE FRMTD MSG IN P BLK      */ 00138000
         L     @14,PARMAOUT(,@14)                                  0059 00139000
         ST    @14,TXISEGMT                                        0059 00140000
*       ELSE                        /* IF THIS IS MULTI LEVEL THEN 0060 00141000
*                                      BUMP ADDR BY 4                */ 00142000
*         TXISEGMT=PARMAOUT+4;                                     0060 00143000
         B     @RC00058                                            0060 00144000
@RF00058 L     @14,PTPBPTR(,R1)                                    0060 00145000
         LA    @10,4                                               0060 00146000
         AL    @10,PARMAOUT(,@14)                                  0060 00147000
         ST    @10,TXISEGMT                                        0060 00148000
*       R1=ADDR(TXINPARM);                                         0061 00149000
@RC00058 LA    R1,TXINPARM                                         0061 00150000
*       CALL IKJEFT54;              /* TEXT INSERTION                */ 00151000
         L     @15,@CV00211                                        0062 00152000
         BALR  @14,@15                                             0062 00153000
*       IF R15^=0 THEN              /* ERROR FROM SUBROUTINE?        */ 00154000
         LTR   R15,R15                                             0063 00155000
         BNZ   @RT00063                                            0063 00156000
*         GOTO EXIT;                                               0064 00157000
*       R1=ADDR(TRMOPARM);          /* POINT TO DUPLICATE IOPL       */ 00158000
         LA    R1,TRMOPARM                                         0065 00159000
*       CALL IKJEFT56;              /* TERM OUT                      */ 00160000
         L     @15,@CV00215                                        0066 00161000
         BALR  @14,@15                                             0066 00162000
*       RETURNCD=R15;               /* SAVE TERMOUT RETURN CODE      */ 00163000
         LR    RETURNCD,R15                                        0067 00164000
*       IF FRESWICH=1 THEN          /* IF CORE IS TO BE FREED        */ 00165000
         CLC   FRESWICH(4),@CF00040                                0068 00166000
         BNE   @RF00068                                            0068 00167000
*         DO;                                                      0069 00168000
*           SPOOL=1;                /* NON SHARED SUBPOOL            */ 00169000
         MVI   SPOOL,X'01'                                         0070 00170000
*           FLENGTH=OUTBUFF->VRECLENG;/* LENGTH TO BE FREED          */ 00171000
         L     @14,OUTBUFF                                         0071 00172000
         LH    @10,VRECLENG(,@14)                                  0071 00173000
         STCM  @10,7,FLENGTH                                       0071 00174000
*           R0=FREEREG0;            /* FREEMAIN PARM REG             */ 00175000
         L     R0,FREEREG0                                         0072 00176000
*           R1=OUTBUFF;                                            0073 00177000
         LR    R1,@14                                              0073 00178000
*           GEN(FREEMAIN R,LV=(0),A=(1));                          0074 00179000
         FREEMAIN R,LV=(0),A=(1)                                        00180000
*         END;                      /* END FREE SECTION              */ 00181000
*       IF RETURNCD^=0 THEN         /* ERROR FROM TERMOUT?           */ 00182000
@RF00068 SLR   @14,@14                                             0076 00183000
         CR    RETURNCD,@14                                        0076 00184000
         BNE   @RT00076                                            0076 00185000
*         GOTO THEEND;                                             0077 00186000
*       R1=SAVEREG1;                /* POINT TO USERS IOPL           */ 00187000
         LR    R1,SAVEREG1                                         0078 00188000
*       IF                          /* IF MULTI LEVEL BIT ON         */ 00189000
*           PARMMLEV='1'B THEN                                     0079 00190000
