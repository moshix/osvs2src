         LA    @14,0(,@14)                                         0088 00222000
         ALR   @10,@14                                             0088 00223000
         ST    @10,TXISEGMT                                        0088 00224000
*           TXIFRMSG=ADDR(FORMMSG);                                0089 00225000
*                                                                  0089 00226000
         LA    @14,FORMMSG                                         0089 00227000
         ST    @14,TXIFRMSG                                        0089 00228000
*           /*********************************************************/ 00229000
*           /*                                                       */ 00230000
*           /* PLACE FOR THE AD OF THE FORMTD MESAGE                 */ 00231000
*           /*                                                       */ 00232000
*           /*********************************************************/ 00233000
*                                                                  0090 00234000
*           CHAINFLG='FF'X;         /* CHAIN INDICATION              */ 00235000
         MVI   CHAINFLG,X'FF'                                      0090 00236000
