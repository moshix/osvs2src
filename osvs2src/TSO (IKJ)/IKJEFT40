         LA    @14,0(,@14)                                         0096 00251000
         LTR   @14,@14                                             0096 00252000
         BNZ   @RF00096                                            0096 00253000
*             DO;                                                  0097 00254000
*FIRSTELM:                                                         0098 00255000
*               ECTMSGF=0;          /* SET FLAG OFF                  */ 00256000
FIRSTELM L     @14,ECTPTR(,R1)                                     0098 00257000
         MVI   ECTMSGF(@14),X'00'                                  0098 00258000
*               ECTSMSG=FORMMSG;                                   0099 00259000
         MVC   ECTSMSG(3,@14),FORMMSG+1                            0099 00260000
*               GOTO NORMEND;                                      0100 00261000
         B     NORMEND                                             0100 00262000
*             END;                                                 0101 00263000
*           IF ECTMSGF>='80'X THEN  /* CHAIN IS                      */ 00264000
@RF00096 L     @14,ECTPTR(,R1)                                     0102 00265000
         CLI   ECTMSGF(@14),X'80'                                  0102 00266000
         BL    @RF00102                                            0102 00267000
*             DO;                   /* FLAGGED                       */ 00268000
*               CALL IKJEFT53;                                     0104 00269000
