         L     MLEVLPTR,ECTSMSG-1(,@14)                            0107 00277000
         LA    MLEVLPTR,0(,MLEVLPTR)                               0107 00278000
*CHANLOOP:                                                         0108 00279000
*           IF MULANEXT=0 THEN      /* EMPTY                         */ 00280000
CHANLOOP L     @14,MULANEXT(,MLEVLPTR)                             0108 00281000
         LTR   @14,@14                                             0108 00282000
         BNZ   @RF00108                                            0108 00283000
*             DO;                   /* CHAIN SLOT                    */ 00284000
*               MULANEXT=FORMMSG;                                  0110 00285000
         L     @14,FORMMSG                                         0110 00286000
         ST    @14,MULANEXT(,MLEVLPTR)                             0110 00287000
*               GOTO NORMEND;                                      0111 00288000
         B     NORMEND                                             0111 00289000
*             END;                                                 0112 00290000
*           MLEVLPTR=MULANEXT;      /* POINT TO NEXT MESSAGE LINE    */ 00291000
@RF00108 L     MLEVLPTR,MULANEXT(,MLEVLPTR)                        0113 00292000
*           GOTO CHANLOOP;                                         0114 00293000
         B     CHANLOOP                                            0114 00294000
*         END;                                                     0115 00295000
*       GOTO NORMEND;               /* GO TO NORMAL EXIT             */ 00296000
         B     NORMEND                                             0116 00297000
*     END;                                                         0117 00298000
*                                                                  0117 00299000
*   /*****************************************************************/ 00300000
*   /*                                                               */ 00301000
*   /* IF THE WRITE-TYPE CANNOT BE DETERMINED FROM THE INPUT         */ 00302000
*   /* PARAMETERS - RETURN WITH ERROR INDICATION.                    */ 00303000
