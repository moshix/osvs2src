*           IF R15^=0 THEN                                         0093 00242000
         LTR   R15,R15                                             0093 00243000
         BNZ   @RT00093                                            0093 00244000
*             GOTO EXIT;                                           0094 00245000
*           R1=SAVEREG1;                                           0095 00246000
