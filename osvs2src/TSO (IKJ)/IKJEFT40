         L     @14,IOSTELM(,@09)                                   0083 00207000
         TM    INSOTDD(@14),B'00010000'                            0083 00208000
         BNO   @RF00083                                            0083 00209000
*             DO;                                                  0084 00210000
*               PARMAOUT=PARMAOUT->MLEANEXT;/* POINT TO SEC LEV MSG  */ 00211000
         ST    @07,PARMAOUT(,@10)                                  0085 00212000
*               GO TO PUTOUT;       /* PUT OUT SEC LEVEL MSG         */ 00213000
         B     PUTOUT                                              0086 00214000
*             END;                                                 0087 00215000
*           TXISEGMT=PARMAOUT->MLEANEXT+4;/* POINT TO THE LIN TO BE     00216000
*                                      FORMATTED                     */ 00217000
@RF00083 L     @14,PTPBPTR(,R1)                                    0088 00218000
         L     @14,PARMAOUT(,@14)                                  0088 00219000
