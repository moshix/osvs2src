         LA    @07,0(,@07)                                         0081 00198000
         LTR   @07,@07                                             0081 00199000
         BZ    @RT00081                                            0081 00200000
*             GOTO NORMEND;         /* EXIT                          */ 00201000
*           IF ECTIOWA^=0&INSOTDD='1'B THEN/* IF IN BKGRND    Y30PQJM*/ 00202000
