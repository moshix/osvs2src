PARSPCL    IKJENDP                                                      00864000
*                                                                  0279 00865000
*/********************************************************************/ 00866000
*/*                                                                  */ 00867000
*/*PCL DEFINITION-                                                   */ 00868000
*/*          TERMPCL2                                                */ 00869000
*/*                                                                  */ 00870000
*/*PURPOSE-                                                          */ 00871000
*/*          FOR PARSING OF EACH CHAR PAIR BY THE VALIDITY CHECK     */ 00872000
*/*          EXITS                                                   */ 00873000
*/*                                                                  */ 00874000
*/*CHANGE ACTIVITY-                                                  */ 00875000
*/*      @Z40DQKC-                                                   */ 00876000
*/*          NEW FOR VS2 RELEASE 4 SNA SUPPORT                       */ 00877000
*/*                                                                  */ 00878000
*/********************************************************************/ 00879000
*                                                                  0279 00880000
*   DECLARE                                                        0279 00881000
*     PCL2 STATIC GEND;                                            0279 00882000
*   GENERATE DATA DEFS(PCL2);                                      0280 00883000
*PUT2MSG:                                                          0281 00884000
*   PROCEDURE                       /* INTERNAL PROCEDURE            */ 00885000
*       OPTIONS(DONTSAVE);          /* OPTION GET NO SAVE AREA       */ 00886000
*                                                                  0281 00887000
         B     @PB00002                                            0281 00888000
PUT2MSG  DS    0H                                                  0282 00889000
*   /*****************************************************************/ 00890000
*   /*                                                               */ 00891000
*   /* OPTION DONTSAVE REGISTERS ********************MESSAGE SET     */ 00892000
*   /* UP************************** THIS SUBROUTINE SETS UP THE      */ 00893000
*   /* INTERFACE WITH THE PUTLINE SERVICE ROUTINE MACRO AND ISSUES   */ 00894000
*   /* THE MACRO. THIS MARCO INVOKES THE IKJPUTL MODULE WHICH PUTS   */ 00895000
*   /* THE MESSAGE OUT TO THE TERMINAL USER.                         */ 00896000
*   /*                                                               */ 00897000
*   /*****************************************************************/ 00898000
*                                                                  0282 00899000
*   RFY                                                            0282 00900000
*    (R1,                                                          0282 00901000
*     R3,                                                          0282 00902000
*     R4) RSTD;                     /* REGISTERS USED IN THIS ROUTINE*/ 00903000
*   R4=R14;                         /* SAVE RETURN ADDRESS           */ 00904000
         LR    R4,R14                                              0283 00905000
*   NUMSEG=1;                       /* GET NUMBER OF SEGMENTS        */ 00906000
         MVC   NUMSEG(4),@CF00083                                  0284 00907000
*   IOPLPTR=ADDR(IOPLWA);           /* GET POINTER TO I/O PARAMETER     00908000
*                                      LIST                          */ 00909000
         LA    IOPLPTR,IOPLWA                                      0285 00910000
*   IOPLUPT=CPPLUPT;                /* GET UPT POINTER               */ 00911000
         L     @12,CPPLPTR                                         0286 00912000
         L     @08,CPPLUPT(,@12)                                   0286 00913000
         ST    @08,IOPLUPT(,IOPLPTR)                               0286 00914000
*   IOPLECT=CPPLECT;                /* GET ECT POINTER               */ 00915000
         L     @12,CPPLECT(,@12)                                   0287 00916000
         ST    @12,IOPLECT(,IOPLPTR)                               0287 00917000
*   PTPBPTR=ADDR(PUTPARM);                                         0288 00918000
         LA    PTPBPTR,PUTPARM                                     0288 00919000
*   IOPLECB=ADDR(CPECB);            /* GET ECB POINTER               */ 00920000
         LA    @12,CPECB                                           0289 00921000
         ST    @12,IOPLECB(,IOPLPTR)                               0289 00922000
*   PUTPARM(1:PUTLNG)=PUTLIST;      /* INITIALIZE PUTLINE PARM LIST  */ 00923000
         LH    @12,PUTLNG                                          0290 00924000
         BCTR  @12,0                                               0290 00925000
         EX    @12,@SM02285                                        0290 00926000
*   IOPLIOPB=PTPBPTR;               /* GET PTR TO PARAMETER LIST     */ 00927000
         ST    PTPBPTR,IOPLIOPB(,IOPLPTR)                          0291 00928000
*   R3=ADDR(MSGLIST);               /* GET PTR TO MSG SEGMENTS       */ 00929000
         LA    R3,MSGLIST                                          0292 00930000
*   R1=IOPLPTR;                     /* GET PTR TO PUTLINE PARM LIST  */ 00931000
*                                                                  0293 00932000
         LR    R1,IOPLPTR                                          0293 00933000
*   /*****************************************************************/ 00934000
*   /*                                                               */ 00935000
*   /* ISSUE THE PUTLINE MACRO TO INFORM THE TERMINAL USER           */ 00936000
*   /*                                                               */ 00937000
*   /*****************************************************************/ 00938000
*                                                                  0294 00939000
*   GENERATE;                                                      0294 00940000
              PUTLINE OUTPUT=((3),MULTLVL),TERMPUT=(EDIT),MF=(E,(1))    00941000
*   R14=R4;                         /* RESTORE RETURN ADDRESS        */ 00942000
         LR    R14,R4                                              0295 00943000
*   END PUT2MSG;                    /* END PUT2MSG PROCEDURE         */ 00944000
*                                                                  0296 00945000
@EL00002 DS    0H                                                  0296 00946000
@EF00002 DS    0H                                                  0296 00947000
@ER00002 BR    @14                                                 0296 00948000
*   /*****************************************************************/ 00949000
*   /*                                                               */ 00950000
*   /* ****************VALIDITY CHECK ROUTINES*********************  */ 00951000
*   /*                                                               */ 00952000
*   /*****************************************************************/ 00953000
*                                                                  0297 00954000
*LINNOCHK:                                                         0297 00955000
*                                                                  0297 00956000
*   /*****************************************************************/ 00957000
*   /*                                                               */ 00958000
*   /* THIS VALIDITY CHECKING ROUTINE IS ENTERED BY IKJPARS WHENEVER */ 00959000
*   /* THE TERMINAL USER SPECIFIES THE 'LINES' KEYWORD ON THE        */ 00960000
*   /* TERMINAL COMMAND. THE VALUE MUST BE A NUMBER BETWEEN 1 AND    */ 00961000
*   /* 255. IF NOT, A RETURN CODE OF 4 IS RETURNED TO IKJPARS        */ 00962000
*   /* SPECIFYING THAT THE INPUT IS INVALID. IKJPARS THEN ISSUES A   */ 00963000
*   /* 'PROMPT' MESSAGE AND WAITS FOR VALID INPUT. IF THE NUMBER IS A*/ 00964000
*   /* VALID ONE, A RETURN CODE OF 0 IS SENT TO IKJPARS AND          */ 00965000
*   /* PROCESSING OF OTHER OPERANDS CONTINUE.                        */ 00966000
*   /*                                                               */ 00967000
*   /*****************************************************************/ 00968000
*                                                                  0297 00969000
*   PROCEDURE                       /* REGISTER 1 POINTS TO A 2-WORD */ 00970000
*                                                                  0297 00971000
*   /*****************************************************************/ 00972000
*   /*                                                               */ 00973000
*   /* PARAMETER LIST.1. PDE PTR, 2. SAVE AREA PTR                   */ 00974000
*   /*                                                               */ 00975000
*   /*****************************************************************/ 00976000
*                                                                  0297 00977000
*       OPTIONS(DONTSAVE(15),       /* OPTION DONT SAVE REGISTER 15  */ 00978000
*       NOSAVEAREA);                /* OPTION NOSAVERAE              */ 00979000
LINNOCHK ST    @14,12(,@13)                                        0297 00980000
         STM   @00,@12,20(@13)                                     0297 00981000
*   RFY                                                            0298 00982000
*    (GPR01P,                                                      0298 00983000
*     GPR02P,                                                      0298 00984000
*     R3,                                                          0298 00985000
*     GPR12P) RSTD;                 /*                       @Z40DQKC*/ 00986000
*   LM(GPR02P,GPR12P,UWAUWA);       /* RESTORE REGISTERS /*@Z40DQKC  */ 00987000
         L     @08,CHKUWA(,GPR01P)                                 0299 00988000
         LM    GPR02P,GPR12P,UWAUWA(@08)                           0299 00989000
*   R3=CHKPDE;                      /* GET PTR TO PDE /*@Z40DQKC     */ 00990000
         L     R3,CHKPDE(,GPR01P)                                  0300 00991000
*   RFY                                                            0301 00992000
*    (GPR01P,                                                      0301 00993000
*     GPR02P,                                                      0301 00994000
*     GPR12P) UNRSTD;               /*                       @Z40DQKC*/ 00995000
*   CALL ICONVERT;                  /* COVERT EBCDIC TO BINARY       */ 00996000
         BAL   @14,ICONVERT                                        0302 00997000
*   IF R3<=TWO55                    /* MAXIMUM LINE NUMBER IS 255    */ 00998000
*       &R3>=ONE                    /* MINIMUM LINE NUMBER IS 1      */ 00999000
*     THEN                          /* NUMBER IS VALID               */ 01000000
         CH    R3,TWO55                                            0303 01001000
         BH    @RF00303                                            0303 01002000
         CH    R3,ONE                                              0303 01003000
         BL    @RF00303                                            0303 01004000
*     DO;                                                          0304 01005000
*       LINVALUE=R3;                /* STORE THE LINE VALUE          */ 01006000
         STH   R3,LINVALUE                                         0305 01007000
*       R15=0;                      /* INDICATE LINE NUMBER IS VALID */ 01008000
         SLR   R15,R15                                             0306 01009000
*       RETURN;                     /* RETURN TO IKJPARS             */ 01010000
@EL00003 DS    0H                                                  0307 01011000
@EF00003 DS    0H                                                  0307 01012000
@ER00003 L     @14,12(,@13)                                        0307 01013000
         LM    @00,@12,20(@13)                                     0307 01014000
         BR    @14                                                 0307 01015000
*     END;                                                         0308 01016000
*   R15=4;                          /* LINE NUMBER IS INVALID,PROMPT */ 01017000
@RF00303 LA    R15,4                                               0309 01018000
*   RETURN;                         /* RETURN TO IKJPARS             */ 01019000
         B     @EL00003                                            0310 01020000
*   END LINNOCHK;                   /* END LINE NUMBER PROCEDURE     */ 01021000
*SECNOCHK:                                                         0312 01022000
*                                                                  0312 01023000
*   /*****************************************************************/ 01024000
*   /*                                                               */ 01025000
*   /* THIS VALIDITY CHECKING ROUTINE IS ENTERED BY IKJPARS WHENEVER */ 01026000
*   /* THE TERMINAL USER SPECIFIES THE 'SECONDS' KEYWORD ON THE      */ 01027000
*   /* TERMINAL COMMAND. THE VALUE MUST BE A NUMBER BETWEEN 10 AND   */ 01028000
*   /* 2550.IF NOT A RETURN CODE OF 4 IS RETURNED TO IKJPARS         */ 01029000
*   /* SPECIFYING THAT THE INPUT IS INVALID. IIKJPARS THEN ISSUES A  */ 01030000
*   /* 'PROMPT' MESSAGE AND WAITS FOR VALID INPUT. IF THE NUMBER IS A*/ 01031000
*   /* VALID ONE, A RETURN CODE OF 0 IS SENT TO IKJPARS AND          */ 01032000
*   /* PROCESSING OF OTHER OPERANDS CONTINUE.                        */ 01033000
*   /*                                                               */ 01034000
*   /*****************************************************************/ 01035000
*                                                                  0312 01036000
*   PROCEDURE                       /* REGISTER 1 POINTS TO A 2-WORD */ 01037000
*                                                                  0312 01038000
*   /*****************************************************************/ 01039000
*   /*                                                               */ 01040000
*   /* PARAMETER LIST.1. PDE PTR, 2. SAVE AREA PTR                   */ 01041000
*   /*                                                               */ 01042000
*   /*****************************************************************/ 01043000
*                                                                  0312 01044000
*       OPTIONS(DONTSAVE(15),       /* DONT SAVE REGISTER 15         */ 01045000
*       NOSAVEAREA);                /* OPTION GET NO SAVEAREA        */ 01046000
SECNOCHK ST    @14,12(,@13)                                        0312 01047000
         STM   @00,@12,20(@13)                                     0312 01048000
*   RFY                                                            0313 01049000
*    (GPR01P,                                                      0313 01050000
*     GPR02P,                                                      0313 01051000
*     R3,                                                          0313 01052000
*     GPR12P) RSTD;                 /*                       @Z40DQKC*/ 01053000
*   LM(GPR02P,GPR12P,UWAUWA);       /* RESTORE REGISTERS /*@Z40DQKC  */ 01054000
         L     @08,CHKUWA(,GPR01P)                                 0314 01055000
         LM    GPR02P,GPR12P,UWAUWA(@08)                           0314 01056000
*   R3=CHKPDE;                      /* GET PTR TO PDE /*@Z40DQKC     */ 01057000
         L     R3,CHKPDE(,GPR01P)                                  0315 01058000
*   RFY                                                            0316 01059000
*    (GPR01P,                                                      0316 01060000
*     GPR02P,                                                      0316 01061000
*     GPR12P) UNRSTD;               /*                       @Z40DQKC*/ 01062000
*   CALL ICONVERT;                  /* CONVERT INPUT TO BINARY DIGITS*/ 01063000
         BAL   @14,ICONVERT                                        0317 01064000
*   IF R3<=TWO550                   /* MAXIMUM SECONDS IS 2550       */ 01065000
*       &R3>=TEN                    /* MINIMUM SECONDS IS 10         */ 01066000
*     THEN                          /* IS THE NUMBER VALID           */ 01067000
         CH    R3,TWO550                                           0318 01068000
         BH    @RF00318                                            0318 01069000
         CH    R3,TEN                                              0318 01070000
         BL    @RF00318                                            0318 01071000
*     DO;                           /* NUMBER IS VALID               */ 01072000
*       SECVALUE=R3;                /* STORE THE NUMBER              */ 01073000
         STH   R3,SECVALUE                                         0320 01074000
*       R15=0;                      /* INDICAT THAT THE NUMBER IS  0321 01075000
*                                      VALID                         */ 01076000
         SLR   R15,R15                                             0321 01077000
*       RETURN;                     /* RETURN TO IKJPARS             */ 01078000
@EL00004 DS    0H                                                  0322 01079000
@EF00004 DS    0H                                                  0322 01080000
@ER00004 L     @14,12(,@13)                                        0322 01081000
         LM    @00,@12,20(@13)                                     0322 01082000
         BR    @14                                                 0322 01083000
*     END;                          /* END TEST FOR INVALID NUMBER   */ 01084000
*   R15=4;                          /* ELSE THE NUMBER IS INVALID    */ 01085000
@RF00318 LA    R15,4                                               0324 01086000
*   RETURN;                         /* RETURN TO IKJPARS             */ 01087000
         B     @EL00004                                            0325 01088000
*   END SECNOCHK;                   /* END SECONDS CHECK ROUTINE     */ 01089000
*LINSZCHK:                                                         0327 01090000
*                                                                  0327 01091000
