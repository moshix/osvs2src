         TITLE 'IKJEFT80 -- TERMINAL COMMAND PROCESSOR                 *00001000
                        '                                               00002000
        LCLA  &T,&SPN                                                   00003000
&SPN    SETA  1                                                         00004000
IKJEFT80 CSECT ,                                                   0002 00005000
@MAINENT DS    0H                                                  0002 00006000
         USING *,@15                                               0002 00007000
         B     @PROLOG                                             0002 00008000
         DC    AL1(16)                                             0002 00009000
         DC    C'IKJEFT80  78.177'                                 0002 00010000
         DROP  @15                                                      00011000
@PROLOG  ST    @14,12(,@13)                                        0002 00012000
         STM   @00,@12,20(@13)                                     0002 00013000
         BALR  @10,0                                               0002 00014000
@PSTART  LA    @11,4095(,@10)                                      0002 00015000
         USING @PSTART,@10                                         0002 00016000
         USING @PSTART+4095,@11                                    0002 00017000
         L     @00,@SIZDATD                                        0002 00018000
         GETMAIN  R,LV=(0)                                              00019000
         LR    @09,@01                                             0002 00020000
         USING @DATD,@09                                           0002 00021000
         ST    @13,@SA00001+4                                      0002 00022000
         LM    @00,@01,20(@13)                                     0002 00023000
         ST    @09,8(,@13)                                         0002 00024000
         LR    @13,@09                                             0002 00025000
         MVC   @PC00001(4),0(@01)                                  0002 00026000
        EJECT                                                           00027000
*                                                                  0059 00028000
*/********************************************************************/ 00029000
*/*                                                                  */ 00030000
*/*MAINLINE-                                                         */ 00031000
*/*          IKJEFT80/TERMINAL                                       */ 00032000
*/*                                                                  */ 00033000
*/********************************************************************/ 00034000
*                                                                  0059 00035000
*   CPPLPTR=R1;                     /* INITIALIZE CP PARAMETER LIST     00036000
*                                      POINTER                       */ 00037000
         ST    R1,CPPLPTR                                          0059 00038000
*   ECTPTR=CPPLECT;                                                0060 00039000
         L     @12,CPPLPTR                                         0060 00040000
         L     @08,CPPLECT(,@12)                                   0060 00041000
         ST    @08,ECTPTR                                          0060 00042000
*   CPECB=0;                        /* INITIALIZE ECB TO ZERO        */ 00043000
         SLR   @15,@15                                             0061 00044000
         ST    @15,CPECB                                           0061 00045000
*   STATUS=CONTINUE;                /* STATUS IS CONTINUE    @Z40DQKC*/ 00046000
         ST    @15,STATUS                                          0062 00047000
*   RETCODE=ZERO;                   /* RETURN CODE IS ZERO   @Z40DQKC*/ 00048000
         ST    @15,RETCODE                                         0063 00049000
*   #PAIRS=ZERO;                    /* NO XLATE PAIRS YET    @Z40DQKC*/ 00050000
         ST    @15,#PAIRS                                          0064 00051000
*   RFY                                                            0065 00052000
*    (R2,                                                          0065 00053000
*     R3) RSTD;                     /* RESTRICT REGS FOR USAGE       */ 00054000
*                                                                  0065 00055000
*   /*****************************************************************/ 00056000
*   /*                                                               */ 00057000
*   /* THE ECTNOPD BIT IS SET BY THE TMP TO INDICATE THE PRESENCE OF */ 00058000
*   /* OPERANDS                                                      */ 00059000
*   /*                                                               */ 00060000
*   /*****************************************************************/ 00061000
*                                                                  0066 00062000
*   IF ECTNOPD='1'B                 /* CHECK FOR OPERANDS            */ 00063000
*     THEN                          /* NO OPERANDS WERE SPECIFIED    */ 00064000
         TM    ECTNOPD(@08),B'10000000'                            0066 00065000
         BO    @RT00066                                            0066 00066000
*     GO TO IGNOREM;                /* GO TO ISSUE THE WARNING     0067 00067000
*                                      MESSAGE                       */ 00068000
*                                                                  0067 00069000
*   /*****************************************************************/ 00070000
*   /*                                                               */ 00071000
*   /* COMPLETE THE INITIALIZATION OF POINTERS                       */ 00072000
*   /*                                                               */ 00073000
*   /*****************************************************************/ 00074000
*                                                                  0068 00075000
*   PPLPTR=ADDR(PPLWA2);            /* PPL FOR CHKCHAR       @Z40DQKC*/ 00076000
         LA    PPLPTR,PPLWA2                                       0068 00077000
*   PPLECT=CPPLECT;                 /* PTR TO ECT            @Z40DQKC*/ 00078000
         ST    @08,PPLECT(,PPLPTR)                                 0069 00079000
*   PPLUPT=CPPLUPT;                 /* PTR TO UPT            @Z40DQKC*/ 00080000
         L     @05,CPPLUPT(,@12)                                   0070 00081000
         ST    @05,PPLUPT(,PPLPTR)                                 0070 00082000
*   PPLECB=ADDR(CPECB);             /* PTR TO ECB            @Z40DQKC*/ 00083000
         LA    @15,CPECB                                           0071 00084000
         ST    @15,PPLECB(,PPLPTR)                                 0071 00085000
*   PPLCBUF=ADDR(PAIRBUF);          /* PTR TO BUFFER         @Z40DQKC*/ 00086000
         LA    @14,PAIRBUF                                         0072 00087000
         ST    @14,PPLCBUF(,PPLPTR)                                0072 00088000
*   PPLPCL=ADDR(TERMPCL2);          /* PTR TO PCL            @Z40DQKC*/ 00089000
         L     @14,@CV02097                                        0073 00090000
         ST    @14,PPLPCL(,PPLPTR)                                 0073 00091000
*   PPLANS=ADDR(PDLPTR1);           /* PTR TO ANSWER         @Z40DQKC*/ 00092000
         LA    @14,PDLPTR1                                         0074 00093000
         ST    @14,PPLANS(,PPLPTR)                                 0074 00094000
*   PPLUWA=ADDR(CHKCHARS);          /* CHKCHAR SAVE AREA     @Z40DQKC*/ 00095000
         LA    @14,CHKCHARS                                        0075 00096000
         ST    @14,PPLUWA(,PPLPTR)                                 0075 00097000
*   PPLPTR=ADDR(PPLWA);             /* INITIALIZE THE PARSE PARM LIST*/ 00098000
         LA    PPLPTR,PPLWA                                        0076 00099000
*   PPLECT=CPPLECT;                 /* INITIALIZE ECT PTR            */ 00100000
         ST    @08,PPLECT(,PPLPTR)                                 0077 00101000
*   PPLUPT=CPPLUPT;                 /* INITIALIZE UPT PTR            */ 00102000
         ST    @05,PPLUPT(,PPLPTR)                                 0078 00103000
*   PPLECB=ADDR(CPECB);             /* INITIALIZE ECB PTR            */ 00104000
         ST    @15,PPLECB(,PPLPTR)                                 0079 00105000
*   PPLCBUF=CPPLCBUF;               /* INITIALIZE COMMAND BUFFER PTR */ 00106000
         L     @12,CPPLCBUF(,@12)                                  0080 00107000
         ST    @12,PPLCBUF(,PPLPTR)                                0080 00108000
*   PPLPCL=ADDR(TERMPCL);           /* INITIALIZE IKJPARS PCL PTR    */ 00109000
         L     @12,@CV02096                                        0081 00110000
         ST    @12,PPLPCL(,PPLPTR)                                 0081 00111000
*   PPLANS=ADDR(PDEPTR);            /* INITIALIZE PTR TO ANSWER PLACE*/ 00112000
         LA    @12,PDEPTR                                          0082 00113000
         ST    @12,PPLANS(,PPLPTR)                                 0082 00114000
*   PPLUWA=R13;                     /* INITIALIZE TERMINAL SAVE AREA    00115000
*                                      PTR                           */ 00116000
         ST    R13,PPLUWA(,PPLPTR)                                 0083 00117000
*   R1=PPLPTR;                      /* GET PTR TO IKJPARS PARAMETER     00118000
*                                      LIST                          */ 00119000
         LR    R1,PPLPTR                                           0084 00120000
*   GENERATE(LINK EP=IKJPARS);      /* GO TO PARSE THE OPERANDS      */ 00121000
         LINK EP=IKJPARS                                                00122000
*   IF R15^=0                       /* TEST RETURN CODE FOR SUCCESS  */ 00123000
*     THEN                          /* RESULT IS INVALID             */ 00124000
         LTR   R15,R15                                             0086 00125000
         BZ    @RF00086                                            0086 00126000
*     DO;                                                          0087 00127000
*       SUBCODE=R15;                /* SAVE THE RETURN CODE          */ 00128000
         ST    R15,SUBCODE                                         0088 00129000
*       IF SUBCODE=4                /* DID USER SPECIFY NOPROMPT   0089 00130000
*                                      MODE?                         */ 00131000
*         THEN                                                     0089 00132000
         L     @12,SUBCODE                                         0089 00133000
         C     @12,@CF00049                                        0089 00134000
         BE    @RT00089                                            0089 00135000
*         GO TO RET12;              /* SET RETURN CODE OF 12 AND   0090 00136000
*                                      RETURN                        */ 00137000
*       CVD(SUBCODE,WORKAREA);      /* '000C'X -> '012F'X    @Z40DQKC*/ 00138000
         CVD   @12,WORKAREA                                        0091 00139000
*       UNPK(CODE,WORKAREA);        /* '012F'X -> 'F0F1F2'X  @Z40DQKC*/ 00140000
         UNPK  CODE(5),WORKAREA(8)                                 0092 00141000
*       CODE(LENGTH(CODE))='F0'X|   /* SET ZONES             @Z40DQKC*/ 00142000
*           CODE(LENGTH(CODE));                                    0093 00143000
         OI    CODE+4,X'F0'                                        0093 00144000
*       CODELEN=LENGTH(CODEMSG);    /* LENGTH OF INSERT      @Z40DQKC*/ 00145000
         MVC   CODELEN(2),@CH02086                                 0094 00146000
*       CODEOFF=27;                 /* GET LENGTH OF CODE    @Z40DQKC*/ 00147000
         MVC   CODEOFF(2),@CH01952                                 0095 00148000
*       SEGMENT=ADDR(INVPMSG);      /* GET MESSAGE POINTER           */ 00149000
         L     @12,@CV01896                                        0096 00150000
         LA    @12,INVPMSG(,@12)                                   0096 00151000
         ST    @12,SEGMENT                                         0096 00152000
*       LEVELNP=NONEXTLM;           /* INDICATE NO NEXT LEVEL MESSAGE*/ 00153000
         L     @12,NONEXTLM                                        0097 00154000
         ST    @12,LEVELNP                                         0097 00155000
*       SEGMENT2=ADDR(ERRCODE);     /* GET ERROR MESSAGE POINTER     */ 00156000
         L     @12,@CV01896                                        0098 00157000
         LA    @12,ERRCODE(,@12)                                   0098 00158000
         ST    @12,SEGMENT2                                        0098 00159000
*       SEGMENT3=ADDR(CODEMSG);     /* GET CODE MESSAGE POINTER      */ 00160000
         LA    @12,CODEMSG                                         0099 00161000
         ST    @12,SEGMENT3                                        0099 00162000
*       NUMSEG2=2;                  /* GET NUMBER OF SEGMENTS        */ 00163000
         MVC   NUMSEG2(4),@CF00174                                 0100 00164000
*       LEVEL2P=ADDR(LEVELNP);      /* GET NEXT LEVEL MESSAGE POINTER*/ 00165000
         LA    @12,LEVELNP                                         0101 00166000
         ST    @12,LEVEL2P                                         0101 00167000
*       CALL PUT2MSG;               /* GO ISSUE THE MESSAGE          */ 00168000
         BAL   @14,PUT2MSG                                         0102 00169000
*       GO TO RET12;                /* RETURN TO TMP SET RETURN CODE    00170000
*                                      TO 12                         */ 00171000
         B     RET12                                               0103 00172000
*     END;                          /* END PARSE ERROR CHECK         */ 00173000
*                                                                  0104 00174000
*   /*****************************************************************/ 00175000
*   /*                                                               */ 00176000
*   /* CHECK FOR NO OPERAND INDICATION FROM IKJPARS                  */ 00177000
*   /*                                                               */ 00178000
*   /*****************************************************************/ 00179000
*                                                                  0105 00180000
*   IF PDEPTR=0                     /* IS PDL EMPTY                  */ 00181000
*     THEN                          /* NO OPERANDS                   */ 00182000
@RF00086 L     @12,PDEPTR                                          0105 00183000
         LTR   @12,@12                                             0105 00184000
         BNZ   @RF00105                                            0105 00185000
*     DO;                                                          0106 00186000
*IGNOREM:                                                          0107 00187000
*       SEGMENT=ADDR(MSG1);         /* ISSUE MESSAGE AND RETURN TO 0107 00188000
*                                      TMP                           */ 00189000
IGNOREM  L     @12,@CV01896                                        0107 00190000
         ST    @12,SEGMENT                                         0107 00191000
*       LEVEL2P=NONEXTLM;           /* INDICATE NO NEXT LEVEL MESSAGE*/ 00192000
         L     @12,NONEXTLM                                        0108 00193000
         ST    @12,LEVEL2P                                         0108 00194000
*       CALL PUT2MSG;               /* GO TO ISSUE THE MESSAGE       */ 00195000
         BAL   @14,PUT2MSG                                         0109 00196000
*       GO TO RET;                  /* RETURN TO TMP SET RETURN CODE    00197000
*                                      OF 0                          */ 00198000
         B     RET                                                 0110 00199000
*     END;                          /* END EMPTY PDE CHECK           */ 00200000
*                                                                  0111 00201000
*   /*****************************************************************/ 00202000
*   /*                                                               */ 00203000
*   /* ********ROUTINE FOR ISSUING TERMINAL STATUS MACROS**********  */ 00204000
*   /* THE PARAMETERS THAT ARE SPECIFIED IN THESE MACROS DEPEND ON   */ 00205000
*   /* THE OUTPUT FROM IKJPARS. THIS ROUTINE DETERMINES WHICH        */ 00206000
*   /* KEYWORDS WERE SPECIFIED AND ISSUES THE CORRESPONDING FORM OF  */ 00207000
*   /* THE APPROPRIATE MACRO.                                        */ 00208000
*   /*                                                               */ 00209000
*   /*****************************************************************/ 00210000
*                                                                  0112 00211000
*   IF LNE=1                        /* WAS LINE KEYWORD SPECIFIED?   */ 00212000
*     THEN                          /* LINE WAS SPECIFIED            */ 00213000
@RF00105 L     @12,PDEPTR                                          0112 00214000
         CLC   LNE(2,@12),@CH00083                                 0112 00215000
         BNE   @RF00112                                            0112 00216000
*     DO;                                                          0113 00217000
*       R3=LINVALUE;                /* SET LINE VALUE                */ 00218000
         LH    R3,LINVALUE                                         0114 00219000
*       GO TO STLATTN;              /* ISSUE STATTN WITH LINE      0115 00220000
*                                      PARAMETER                     */ 00221000
         B     STLATTN                                             0115 00222000
*     END;                                                         0116 00223000
*   IF LNE=2                        /* NOLINE KEYWORD SPECIFIED?     */ 00224000
*     THEN                          /* NOLINE KEYWORD WAS SPECIFIED  */ 00225000
@RF00112 L     @12,PDEPTR                                          0117 00226000
         CLC   LNE(2,@12),@CH00174                                 0117 00227000
         BNE   @RF00117                                            0117 00228000
*     DO;                                                          0118 00229000
*       R3=0;                       /* GET LINE-CANCEL VALUE         */ 00230000
         SLR   R3,R3                                               0119 00231000
*STLATTN:                           /* ISSUE STATTN MACRO WITH LINE     00232000
*                                      PARAMETER SPECIFIED           */ 00233000
*       GENERATE(STATTN LINES=(3));                                0120 00234000
STLATTN  STATTN LINES=(3)                                               00235000
*       IF R15=8                    /* CHECK FOR INVALID RETRUN CODE    00236000
*                                      Z40DQ                         */ 00237000
*         THEN                                                     0121 00238000
         C     R15,@CF00063                                        0121 00239000
         BNE   @RF00121                                            0121 00240000
*         DO;                                                      0122 00241000
*           SEGMENT=ADDR(MSGLINE);  /* GET LINE IGNORED MSGZ40DQJD   */ 00242000
         L     @12,@CV01896                                        0123 00243000
         LA    @12,MSGLINE(,@12)                                   0123 00244000
         ST    @12,SEGMENT                                         0123 00245000
*           LEVEL2P=NONEXTLM;       /* NO NEXT LEVEL MSG Z40DQJD     */ 00246000
         L     @12,NONEXTLM                                        0124 00247000
         ST    @12,LEVEL2P                                         0124 00248000
*           CALL PUT2MSG;           /* GO TO ISSUE MSG Z40DQJD       */ 00249000
         BAL   @14,PUT2MSG                                         0125 00250000
*         END;                                                     0126 00251000
*     END;                                                         0127 00252000
@RF00121 DS    0H                                                  0128 00253000
*   IF SECSNO=1                     /* SECONDS KEYWORD SPECIFIED?    */ 00254000
*     THEN                          /* SECONDS WERE SPECIFIED        */ 00255000
@RF00117 L     @12,PDEPTR                                          0128 00256000
         CLC   SECSNO(2,@12),@CH00083                              0128 00257000
         BNE   @RF00128                                            0128 00258000
*     DO;                                                          0129 00259000
*       R3=SECVALUE;                /* GET SECONDS                   */ 00260000
         LH    R3,SECVALUE                                         0130 00261000
*       R3=R3+FIVE;                 /* PREPARE TO ROUND TO NEAREST 0131 00262000
*                                      FIVE                          */ 00263000
         AH    R3,FIVE                                             0131 00264000
*       R3=R3/TEN;                  /* GET NUMBER OF TEN SECONDS     */ 00265000
         LR    @01,R3                                              0132 00266000
         SLR   @00,@00                                             0132 00267000
         LH    @12,TEN                                             0132 00268000
         DR    @00,@12                                             0132 00269000
         LR    R3,@01                                              0132 00270000
*       GO TO STSATTN;                                             0133 00271000
         B     STSATTN                                             0133 00272000
*     END;                                                         0134 00273000
*   IF SECSNO=2                     /* NOSECONDS SPECIFIED?          */ 00274000
*     THEN                          /* NOSECONDS WERE SPECIFIED      */ 00275000
@RF00128 L     @12,PDEPTR                                          0135 00276000
         CLC   SECSNO(2,@12),@CH00174                              0135 00277000
         BNE   @RF00135                                            0135 00278000
*     DO;                                                          0136 00279000
*       R3=0;                       /* PREPARE TO CANCEL SECONDS     */ 00280000
         SLR   R3,R3                                               0137 00281000
*STSATTN:                           /* ISSUE STATTN MACRO WITH THE 0138 00282000
*                                      NUMBER OF TEN SECONDS         */ 00283000
*       GENERATE(STATTN TENS=(3));                                 0138 00284000
STSATTN  STATTN TENS=(3)                                                00285000
*       IF R15=8                    /* CHECK FOR BAD RETURN        0139 00286000
*                                      CODEZ40DQJD                   */ 00287000
*         THEN                                                     0139 00288000
         C     R15,@CF00063                                        0139 00289000
         BNE   @RF00139                                            0139 00290000
*         DO;                                                      0140 00291000
*           SEGMENT=ADDR(MSGSECS);  /* GET SEC IGNORED Z40DQJD       */ 00292000
         L     @12,@CV01896                                        0141 00293000
         LA    @12,MSGSECS(,@12)                                   0141 00294000
         ST    @12,SEGMENT                                         0141 00295000
*           LEVEL2P=NONEXTLM;       /* NO NEXT LEVEL MSG Z40DQJD     */ 00296000
         L     @12,NONEXTLM                                        0142 00297000
         ST    @12,LEVEL2P                                         0142 00298000
*           CALL PUT2MSG;           /* ISSUE MESSAGE Z40DQJD         */ 00299000
         BAL   @14,PUT2MSG                                         0143 00300000
*         END;                                                     0144 00301000
*     END;                                                         0145 00302000
@RF00139 DS    0H                                                  0146 00303000
*   IF IPUT=1                       /* INPUT KEYWORD SPECIFIED?      */ 00304000
*     THEN                          /* INPUT KEYWORD WAS SPECIFIED   */ 00305000
@RF00135 L     @12,PDEPTR                                          0146 00306000
         CLC   IPUT(2,@12),@CH00083                                0146 00307000
         BNE   @RF00146                                            0146 00308000
*     DO;                                                          0147 00309000
*       VALUE2=BLANK;               /* PREPARE TO GET INPUT STRING 0148 00310000
*                                      PTR                           */ 00311000
         MVI   VALUE2+1,C' '                                       0148 00312000
         MVC   VALUE2+2(2),VALUE2+1                                0148 00313000
         MVI   VALUE2,C' '                                         0148 00314000
*       VALUE2(1:INPUT2)=INPUT1->NUM;/* GET STRING                   */ 00315000
         LH    @08,INPUT2(,@12)                                    0149 00316000
         BCTR  @08,0                                               0149 00317000
         L     @12,INPUT1(,@12)                                    0149 00318000
         EX    @08,@SM02283                                        0149 00319000
*       R3=ADDR(VALUE2);            /* GET STRING POINTER            */ 00320000
         LA    R3,VALUE2                                           0150 00321000
*       GO TO STIATTN;              /* ISSUE STATTN WITH INPUT     0151 00322000
*                                      PARAMETER                     */ 00323000
         B     STIATTN                                             0151 00324000
*     END;                                                         0152 00325000
*   IF IPUT=2                       /* NOINPUT KEYWORD SPECIFIED?    */ 00326000
*     THEN                          /* NOINPUT WAS SPECIFIED         */ 00327000
@RF00146 L     @12,PDEPTR                                          0153 00328000
         CLC   IPUT(2,@12),@CH00174                                0153 00329000
         BNE   @RF00153                                            0153 00330000
*     DO;                                                          0154 00331000
*       R3=0;                       /* PREPARE TO CANCEL INPUT       */ 00332000
         SLR   R3,R3                                               0155 00333000
*STIATTN:                           /* ISSUE STATTN MACRO WITH INPUT    00334000
*                                      PARAMETER                     */ 00335000
*       GENERATE(STATTN INPUT=(3));                                0156 00336000
STIATTN  STATTN INPUT=(3)                                               00337000
*       IF R15=8                    /* INVALID RETURN CODE Z40DQJD   */ 00338000
*         THEN                                                     0157 00339000
         C     R15,@CF00063                                        0157 00340000
         BNE   @RF00157                                            0157 00341000
*         DO;                                                      0158 00342000
*           SEGMENT=ADDR(MSGINPT);  /* INPUT IGNORED MSGZ40DQJD      */ 00343000
         L     @12,@CV01896                                        0159 00344000
         LA    @12,MSGINPT(,@12)                                   0159 00345000
         ST    @12,SEGMENT                                         0159 00346000
*           LEVEL2P=NONEXTLM;       /* NO NEXT LEVEL MSG Z40DQJD     */ 00347000
         L     @12,NONEXTLM                                        0160 00348000
         ST    @12,LEVEL2P                                         0160 00349000
*           CALL PUT2MSG;           /* ISSUE MSG Z40DQJD             */ 00350000
         BAL   @14,PUT2MSG                                         0161 00351000
*         END;                                                     0162 00352000
*     END;                                                         0163 00353000
@RF00157 DS    0H                                                  0164 00354000
*STBRK:                             /* CHECK FOR BREAK OPERAND       */ 00355000
*   IF BRK=1                        /* WAS BREAK KEYWORD SPECIFIED?  */ 00356000
*     THEN                          /* BREAK WAS SPECIFIED           */ 00357000
@RF00153 DS    0H                                                  0164 00358000
STBRK    L     @12,PDEPTR                                          0164 00359000
         CLC   BRK(2,@12),@CH00083                                 0164 00360000
         BNE   @RF00164                                            0164 00361000
*     DO;                                                          0165 00362000
*       GENERATE(STBREAK YES);      /* ISSUE STBREAK MACRO -YES      */ 00363000
         STBREAK YES                                                    00364000
*       IF R15=8                    /* CHECK FOR INVALID RETURN      */ 00365000
*         THEN                      /* RETURN CODE IS INVALID        */ 00366000
         C     R15,@CF00063                                        0167 00367000
         BNE   @RF00167                                            0167 00368000
*         DO;                                                      0168 00369000
*           SEGMENT=ADDR(MSG5);     /* GET INVALID ERROR MESSAGE     */ 00370000
         L     @12,@CV01896                                        0169 00371000
         LA    @12,MSG5(,@12)                                      0169 00372000
         ST    @12,SEGMENT                                         0169 00373000
*           LEVEL2P=NONEXTLM;       /* INDICATE NO NEXT LEVEL MSG    */ 00374000
         L     @12,NONEXTLM                                        0170 00375000
         ST    @12,LEVEL2P                                         0170 00376000
*           CALL PUT2MSG;           /* GO TO ISSUE MESSAGE           */ 00377000
         BAL   @14,PUT2MSG                                         0171 00378000
*           GO TO CHKTIMEO;         /* GO TO CHECK NEXT OPERAND      */ 00379000
         B     CHKTIMEO                                            0172 00380000
*         END;                      /* END ERROR CHECK               */ 00381000
*     END;                          /* END CHECK FOR BREAK OPERAND   */ 00382000
@RF00167 DS    0H                                                  0175 00383000
*   IF BRK=2                        /* NOBREAK SPECIFIED?            */ 00384000
*     THEN                          /* ISSUE STBREAK MACRO -NO       */ 00385000
@RF00164 L     @12,PDEPTR                                          0175 00386000
         CLC   BRK(2,@12),@CH00174                                 0175 00387000
         BNE   @RF00175                                            0175 00388000
*     DO;                           /* ISSUE MACRO                   */ 00389000
*       GENERATE(STBREAK NO);       /* ISSUE MACRO WITH NO BREAK     */ 00390000
         STBREAK NO                                                     00391000
*       IF R15=8                    /* CHECK FOR INVALID RETURN CODE */ 00392000
*         THEN                      /* RETURN CODE IS INVALID        */ 00393000
         C     R15,@CF00063                                        0178 00394000
         BNE   @RF00178                                            0178 00395000
*         DO;                       /* ISSUE ERROR MESSAGE           */ 00396000
*           SEGMENT=ADDR(MSG6);     /* GET ERROR MESSAGE POINTER     */ 00397000
         L     @12,@CV01896                                        0180 00398000
         LA    @12,MSG6(,@12)                                      0180 00399000
         ST    @12,SEGMENT                                         0180 00400000
*           LEVEL2P=NONEXTLM;       /* INDICATE NO NEXT LEVEL MSG    */ 00401000
         L     @12,NONEXTLM                                        0181 00402000
         ST    @12,LEVEL2P                                         0181 00403000
*           CALL PUT2MSG;           /* GO TO ISSUE MESSAGE           */ 00404000
         BAL   @14,PUT2MSG                                         0182 00405000
*         END;                      /* END CHECK FOR ERROR CONDITION */ 00406000
*     END;                          /* END CHECK FOR NOBREAK OPERAND */ 00407000
@RF00178 DS    0H                                                  0185 00408000
*CHKTIMEO:                                                         0185 00409000
