*/*                                                                  */ 02241000
*/********************************************************************/ 02242000
*                                                                  0628 02243000
*LOAD:                                                             0628 02244000
*   PROCEDURE;                                                     0628 02245000
LOAD     STM   @14,@12,12(@13)                                     0628 02246000
*   RFY                                                            0629 02247000
*    (GPR00P,                                                      0629 02248000
*     GPR14P,                                                      0629 02249000
*     GPR15P) RSTD;                                                0629 02250000
*   STM(GPR00P,GPR15P,ESTAREGS);    /* REGS FOR ESTAE RESTORING      */ 02251000
         LA    @12,$ESTAPRM                                        0630 02252000
         STM   GPR00P,GPR15P,ESTAREGS(@12)                         0630 02253000
*   RFY                                                            0631 02254000
*    (GPR00P,                                                      0631 02255000
*     GPR14P,                                                      0631 02256000
*     GPR15P) UNRSTD;                                              0631 02257000
*   IF STATUS=CONTINUE&             /* NO ERRORS                     */ 02258000
*       ESTA806=NO                  /* NO 806 ABEND                  */ 02259000
*     THEN                                                         0632 02260000
         SLR   @08,@08                                             0632 02261000
         C     @08,STATUS                                          0632 02262000
         BNE   @RF00632                                            0632 02263000
