*/*                                                                  */ 02415000
*/*SEGMENT-                                                          */ 02416000
*/*          SETESTAE                                                */ 02417000
*/*                                                                  */ 02418000
*/*FUNCTION-                                                         */ 02419000
*/*          INITIALIZE ESTAE ENVIRONMENT TO PROTECT AGAINST 806     */ 02420000
*/*          ABENDS WHEN LOADING THE TRANSLATE TABLE                 */ 02421000
*/*                                                                  */ 02422000
*/*OPERATION-                                                        */ 02423000
*/*          INITIALIZE THE PARAMETERS THAT WILL BE PASSED TO THE    */ 02424000
*/*          ESTAE ROUTINE SHOULD AN 806 ABEND OCCUR.ISSUE THE       */ 02425000
*/*          ESTAE MACRO TO ESTABLISH THE RECOVERY ENVIRONMENT       */ 02426000
*/*                                                                  */ 02427000
*/*CHANGE ACTIVITY-                                                  */ 02428000
*/*      @Z40DQKC-                                                   */ 02429000
*/*          NEW SUPPORT FOR OS/VS2 RELEASE 4 TSO/VTAM               */ 02430000
*/*                                                                  */ 02431000
*/********************************************************************/ 02432000
*                                                                  0671 02433000
*SETESTAE:                                                         0671 02434000
*   PROCEDURE;                                                     0671 02435000
SETESTAE STM   @14,@12,@SA00017                                    0671 02436000
*   $ESTAPRM=''B;                   /* ZERO OUT ESTAE PARMS          */ 02437000
         XC    $ESTAPRM(69),$ESTAPRM                               0672 02438000
*   ESTA806=NO;                     /* NO 806 ABEND YET              */ 02439000
         LA    @12,$ESTAPRM                                        0673 02440000
         NI    ESTA806(@12),B'01111111'                            0673 02441000
*   ESTARETY=ADDR(LOAD);            /* RETRY POINT                   */ 02442000
         LA    @08,LOAD                                            0674 02443000
         ST    @08,ESTARETY(,@12)                                  0674 02444000
*   RFY                                                            0675 02445000
*    (GPR15P) RSTD;                                                0675 02446000
*   RFY                                                            0676 02447000
*    (GPR02P) RSTD;                                                0676 02448000
*   GPR02P=ADDR(ESTAPARM);          /* ADDRESS OF ESTAE PARMS        */ 02449000
         LR    GPR02P,@12                                          0677 02450000
*   GEN SETS(ESTALIST)              /* SET UP ESTAE ENVIRONMENT      */ 02451000
*       (ESTAE ESTAE,CT,PARAM=(2),MF=(E,ESTALIST));                0678 02452000
         ESTAE ESTAE,CT,PARAM=(2),MF=(E,ESTALIST)                       02453000
*   RETCODE=GPR15P;                 /* SAVE ESTAE RETURN CODE        */ 02454000
         ST    GPR15P,RETCODE                                      0679 02455000
*   RFY                                                            0680 02456000
*    (GPR15P,                                                      0680 02457000
*     GPR02P) UNRSTD;                                              0680 02458000
*   IF RETCODE^=ZERO                /* ESTAE ERROR                   */ 02459000
*     THEN                                                         0681 02460000
         L     @12,RETCODE                                         0681 02461000
         LTR   @12,@12                                             0681 02462000
         BZ    @RF00681                                            0681 02463000
*     STATUS=ESTAERR;               /* RECORD ESTAE ERROR            */ 02464000
         L     @12,ESTAERR                                         0682 02465000
         ST    @12,STATUS                                          0682 02466000
*   END SETESTAE;                                                  0683 02467000
@EL00017 DS    0H                                                  0683 02468000
@EF00017 DS    0H                                                  0683 02469000
@ER00017 LM    @14,@12,@SA00017                                    0683 02470000
         BR    @14                                                 0683 02471000
*                                                                  0684 02472000
*/********************************************************************/ 02473000
*/*                                                                  */ 02474000
*/*SEGMENT-                                                          */ 02475000
*/*          ESTAE                                                   */ 02476000
*/*                                                                  */ 02477000
*/*FUNCTION-                                                         */ 02478000
*/*          INTERCEPT 806 ABENDS DUE TO ERROR LOADING THE           */ 02479000
*/*          TRANSLATE TABLE                                         */ 02480000
*/*                                                                  */ 02481000
*/*OPERATION-                                                        */ 02482000
*/*          ESTABLISH ADDRESSABILITY AND SAVE REGISTER 14.IF THERE  */ 02483000
*/*          IS AN SDWA AND WE HAVE AN 806 ABEND THEN COPY THE       */ 02484000
*/*          REGISTERS FROM THE ESTAE PARMS TO THE SDWA SO THEY      */ 02485000
*/*          WILL BE RESTORED BEFORE THE RETRY ROUTINE GETS CONTROL. */ 02486000
*/*          ISSUE SETRP TO RETURN TO RTM AND EVENTUALLY TO THE      */ 02487000
*/*          RETRY ROUTINE LOAD                                      */ 02488000
*/*                                                                  */ 02489000
*/*          IF THERE IS NO SDWA OR THE ABEND IS NOT AN 806 THEN     */ 02490000
*/*          RETURN TO RTM WITHOUT REQUESTING A RETRY                */ 02491000
*/*                                                                  */ 02492000
*/*CHANGE ACTIVITY-                                                  */ 02493000
*/*      @Z40DQKC-                                                   */ 02494000
*/*          NEW SUPPORT FOR OS/VS2 RELEASE 4 TSO/VTAM               */ 02495000
*/*                                                                  */ 02496000
*/********************************************************************/ 02497000
*                                                                  0684 02498000
*ESTAE:                                                            0684 02499000
*   PROCEDURE OPTIONS(NOSAVEAREA,NOSAVE);                          0684 02500000
ESTAE    DS    0H                                                  0685 02501000
*   DCL                                                            0685 02502000
*     ABND806 CHAR(2) INIT('8060'X);                               0685 02503000
*   DCL                                                            0686 02504000
*     1 * CHAR(3) DEF(SDWACMPC),                                   0686 02505000
*      2 SYSCC CHAR(2);                                            0686 02506000
*   RFY                                                            0687 02507000
*    (GPR00P,                                                      0687 02508000
*     GPR01P,                                                      0687 02509000
*     GPR14P) RSTD;                                                0687 02510000
*   GEN(USING *,GPR15P);            /* INITIAL ADDRESSABILITY        */ 02511000
         USING *,GPR15P                                                 02512000
*   IF GPR00P^=12                   /* THE SDWA EXISTS               */ 02513000
*     THEN                                                         0689 02514000
         C     GPR00P,@CF02156                                     0689 02515000
         BE    @RF00689                                            0689 02516000
*     DO;                                                          0690 02517000
*       RFY                                                        0691 02518000
*         SDWA BASED(GPR01P);                                      0691 02519000
*       RFY                                                        0692 02520000
*         ESTAPARM BASED(SDWAPARM);                                0692 02521000
*       RFY                                                        0693 02522000
*        (GPR02P,                                                  0693 02523000
*         GPR12P,                                                  0693 02524000
*         GPR13P) RSTD;                                            0693 02525000
*       STM(GPR14P,GPR12P,RTMSAVE(13:72));/* SAVE RTMS REGISTER      */ 02526000
         STM   GPR14P,GPR12P,RTMSAVE+12(GPR13P)                    0694 02527000
*       LM(GPR02P,GPR12P,ESTAREGS(9:52));/* RESTORE REGS FOR ADDR    */ 02528000
         L     @15,SDWAPARM(,GPR01P)                               0695 02529000
         LM    GPR02P,GPR12P,ESTAREGS+8(@15)                       0695 02530000
*       RFY                                                        0696 02531000
*        (GPR02P,                                                  0696 02532000
*         GPR12P,                                                  0696 02533000
*         GPR13P,                                                  0696 02534000
*         GPR00P,                                                  0696 02535000
*         GPR14P) UNRSTD;                                          0696 02536000
*       GEN(DROP GPR15P);           /* REESTABLISH ADDRESSABILITY    */ 02537000
         DROP GPR15P                                                    02538000
*       IF SYSCC=ABND806            /* ABEND = 806                   */ 02539000
*         THEN                                                     0698 02540000
         CLC   SYSCC(2,GPR01P),ABND806                             0698 02541000
         BNE   @RF00698                                            0698 02542000
*         DO;                                                      0699 02543000
*           SDWASRSV=ESTAREGS;      /* REGS TO BE RESTORED BY RTM    */ 02544000
         L     @12,SDWAPARM(,GPR01P)                               0700 02545000
         MVC   SDWASRSV(64,GPR01P),ESTAREGS(@12)                   0700 02546000
*           ESTA806=YES;            /* WE HAVE AN 806 ABEND          */ 02547000
         OI    ESTA806(@12),B'10000000'                            0701 02548000
*           DO;                     /* SETRP DUMP(NO)RC(SDWARETY)RETA   02549000
*                                      DDR(ESTARRTN)FRESDWA(YES)REGS(   02550000
*                                      14)RETREGS(YES)               */ 02551000
*             RESPECIFY                                            0703 02552000
*              (GPR00P,                                            0703 02553000
*               GPR01P,                                            0703 02554000
*               GPR14P,                                            0703 02555000
*               GPR15P) RSTD;                                      0703 02556000
*             GPR01P->SDWARCDE=SDWARETY;/* STORE RC INTO SDWA        */ 02557000
         MVI   SDWARCDE(GPR01P),X'04'                              0704 02558000
*             GPR01P->SDWARTYA=ADDR(ESTARRTN);/* SAVE RETRY ADDRESS  */ 02559000
         L     @12,ESTARETY(,@12)                                  0705 02560000
         ST    @12,SDWARTYA(,GPR01P)                               0705 02561000
*             GPR01P->SDWAFREE='1'B;/* SET FLAG TO FREE SDWA         */ 02562000
         OI    SDWAFREE(GPR01P),B'00000100'                        0706 02563000
*             GPR01P->SDWAREQ='0'B; /* TURN OFF DUMP INDICATOR       */ 02564000
         NI    SDWAREQ(GPR01P),B'01111111'                         0707 02565000
*             GPR01P->SDWAUPRG='1'B;/* TURN ON RETREGS INDICATOR     */ 02566000
         OI    SDWAUPRG(GPR01P),B'00001000'                        0708 02567000
*             GEN(RETURN (14));                                    0709 02568000
         RETURN (14)                                                    02569000
*             RESPECIFY                                            0710 02570000
*              (GPR00P,                                            0710 02571000
*               GPR01P,                                            0710 02572000
*               GPR14P,                                            0710 02573000
*               GPR15P) UNRSTD;                                    0710 02574000
*           END;                                                   0711 02575000
*         END;                                                     0712 02576000
*       ELSE                                                       0713 02577000
*         DO;                       /* SETRP RC(SDWACWT)REGS(14)     */ 02578000
         B     @RC00698                                            0713 02579000
@RF00698 DS    0H                                                  0714 02580000
*           RESPECIFY                                              0714 02581000
*            (GPR00P,                                              0714 02582000
*             GPR01P,                                              0714 02583000
*             GPR14P,                                              0714 02584000
*             GPR15P) RSTD;                                        0714 02585000
*           GPR01P->SDWARCDE=SDWACWT;/* SAVE RC VALUE                */ 02586000
         MVI   SDWARCDE(GPR01P),X'00'                              0715 02587000
*           GEN(RETURN (14));                                      0716 02588000
         RETURN (14)                                                    02589000
*           RESPECIFY                                              0717 02590000
*            (GPR00P,                                              0717 02591000
*             GPR01P,                                              0717 02592000
*             GPR14P,                                              0717 02593000
*             GPR15P) UNRSTD;                                      0717 02594000
*         END;                                                     0718 02595000
*     END;                                                         0719 02596000
@RC00698 DS    0H                                                  0720 02597000
*   RFY                                                            0720 02598000
*    (GPR15P) RSTD;                                                0720 02599000
@RF00689 DS    0H                                                  0721 02600000
*   GPR15P=SDWACWT;                 /* NO RETRY                      */ 02601000
         SLR   GPR15P,GPR15P                                       0721 02602000
*   RETURN;                         /* RETURN TO RTM                 */ 02603000
@EL00018 DS    0H                                                  0722 02604000
@EF00018 DS    0H                                                  0722 02605000
@ER00018 BR    @14                                                 0722 02606000
*   RFY                                                            0723 02607000
*    (GPR15P) UNRSTD;                                              0723 02608000
*   END ESTAE;                                                     0724 02609000
*                                                                  0725 02610000
*/********************************************************************/ 02611000
*/*                                                                  */ 02612000
*/*SEGMENT-                                                          */ 02613000
*/*          HANDLERR                                                */ 02614000
*/*                                                                  */ 02615000
*/*FUNCTION-                                                         */ 02616000
*/*          ISSUE ERROR MESSAGES                                    */ 02617000
*/*                                                                  */ 02618000
*/*CHANGE ACTIVITY-                                                  */ 02619000
*/*      @Z40DQKC-                                                   */ 02620000
*/*          NEW SUPPORT FOR OS/VS2 RELEASE 4 TSO/VTAM               */ 02621000
*/*                                                                  */ 02622000
*/********************************************************************/ 02623000
*                                                                  0725 02624000
*HANDLERR:                                                         0725 02625000
*   PROCEDURE;                                                     0725 02626000
HANDLERR STM   @14,@12,@SA00019                                    0725 02627000
*   IF STATUS=IGNCHAR|              /* IGNORE_CHAR MESSAGE           */ 02628000
*       STATUS=IGNTRAN|             /* IGNORE_TRAN MESSAGE           */ 02629000
*       STATUS=BADCHAR|             /* BAD CHARACTER(S) IN TRAN TAB  */ 02630000
*       STATUS=BADSIZE|             /* TRAN TAB TOO SMALL LT 516     */ 02631000
*       STATUS=NOTRNTAB             /* DEFAULT TRAN TAB IN USE       */ 02632000
*     THEN                                                         0726 02633000
         L     @12,STATUS                                          0726 02634000
         C     @12,IGNCHAR                                         0726 02635000
         BE    @RT00726                                            0726 02636000
         C     @12,IGNTRAN                                         0726 02637000
         BE    @RT00726                                            0726 02638000
         C     @12,BADCHAR                                         0726 02639000
         BE    @RT00726                                            0726 02640000
         C     @12,BADSIZE                                         0726 02641000
         BE    @RT00726                                            0726 02642000
         C     @12,NOTRNTAB                                        0726 02643000
         BNE   @RF00726                                            0726 02644000
@RT00726 DS    0H                                                  0727 02645000
*     DO;                                                          0727 02646000
*       LEVEL2P=NONEXTLM;           /* NO SECOND LEVEL MESSAGE       */ 02647000
         L     @12,NONEXTLM                                        0728 02648000
         ST    @12,LEVEL2P                                         0728 02649000
*       NUMSEG=ONE;                 /* ONE MESSAGE, NO INSERTS       */ 02650000
         LH    @12,ONE                                             0729 02651000
         ST    @12,NUMSEG                                          0729 02652000
*       SEGMENT=STATUS;             /* STATUS CONTAINS ADDR OF MSG   */ 02653000
         L     @12,STATUS                                          0730 02654000
         ST    @12,SEGMENT                                         0730 02655000
*     END;                                                         0731 02656000
*   ELSE                                                           0732 02657000
*     DO;                           /* CMND_SYS_ERR MSG WITH INSERT  */ 02658000
         B     @RC00726                                            0732 02659000
@RF00726 DS    0H                                                  0733 02660000
*       CVD(RETCODE,WORKAREA);      /* '000C'X -> '012F'X            */ 02661000
         L     @12,RETCODE                                         0733 02662000
         CVD   @12,WORKAREA                                        0733 02663000
*       UNPK(CODE,WORKAREA);        /* '012F'X -> 'F0F1F2'X          */ 02664000
         UNPK  CODE(5),WORKAREA(8)                                 0734 02665000
*       CODE(LENGTH(CODE))='F0'X|   /* SET ZONES IN RIGHT MOST BYTE  */ 02666000
*           CODE(LENGTH(CODE));                                    0735 02667000
         OI    CODE+4,X'F0'                                        0735 02668000
*       CODELEN=LENGTH(CODEMSG);    /* INSERT LENGTH                 */ 02669000
         MVC   CODELEN(2),@CH02086                                 0736 02670000
*       SEGMENT=ADDR(INVPMSG);      /* 1ST LEVEL MESSAGE             */ 02671000
         L     @12,@CV01896                                        0737 02672000
         LA    @12,INVPMSG(,@12)                                   0737 02673000
         ST    @12,SEGMENT                                         0737 02674000
*       LEVEL2P=ADDR(LEVELNP);      /* 2ND LEVEL MSG OLD             */ 02675000
         LA    @12,LEVELNP                                         0738 02676000
         ST    @12,LEVEL2P                                         0738 02677000
*       NUMSEG2=2;                  /* # OF SEGMENTS IN 2ND LEVEL    */ 02678000
         MVC   NUMSEG2(4),@CF00174                                 0739 02679000
*       SEGMENT2=STATUS;            /* 2ND LEVEL MESSAGE             */ 02680000
         L     @12,STATUS                                          0740 02681000
         ST    @12,SEGMENT2                                        0740 02682000
*       SEGMENT3=ADDR(CODEMSG);     /* PTR TO RET CODE INSERT        */ 02683000
         LA    @08,CODEMSG                                         0741 02684000
         ST    @08,SEGMENT3                                        0741 02685000
*       LEVELNP=NONEXTLM;           /* NO NEXT LEVEL MSG             */ 02686000
         L     @08,NONEXTLM                                        0742 02687000
         ST    @08,LEVELNP                                         0742 02688000
*       IF STATUS=ADDR(ERRCODE)     /* PARSE ERROR                   */ 02689000
*         THEN                                                     0743 02690000
         L     @08,@CV01896                                        0743 02691000
         LA    @08,ERRCODE(,@08)                                   0743 02692000
         CR    @12,@08                                             0743 02693000
         BNE   @RF00743                                            0743 02694000
*         CODEOFF=27;               /* OFFSET FOR RETURN CODE        */ 02695000
         MVC   CODEOFF(2),@CH01952                                 0744 02696000
*       ELSE                                                       0745 02697000
*         IF STATUS=ESTAERR         /* ESTAE ERROR                   */ 02698000
*           THEN                                                   0745 02699000
         B     @RC00743                                            0745 02700000
@RF00743 CLC   STATUS(4),ESTAERR                                   0745 02701000
         BNE   @RF00745                                            0745 02702000
*           CODEOFF=27;             /* OFFSET FOR RETURN CODE        */ 02703000
         MVC   CODEOFF(2),@CH01952                                 0746 02704000
*         ELSE                                                     0747 02705000
*           IF STATUS=ERRSTTRN      /* STTRAN ERROR                  */ 02706000
*             THEN                                                 0747 02707000
         B     @RC00745                                            0747 02708000
@RF00745 CLC   STATUS(4),ERRSTTRN                                  0747 02709000
         BNE   @RF00747                                            0747 02710000
*             CODEOFF=28;           /* OFFSET FOR RETURN CODE        */ 02711000
         MVC   CODEOFF(2),@CH00324                                 0748 02712000
*           ELSE                                                   0749 02713000
*             LEVEL2P=NONEXTLM;     /* UNKNOWN ERR IN 'STATUS'       */ 02714000
         B     @RC00747                                            0749 02715000
@RF00747 L     @12,NONEXTLM                                        0749 02716000
         ST    @12,LEVEL2P                                         0749 02717000
*     END;                                                         0750 02718000
@RC00747 DS    0H                                                  0750 02719000
@RC00745 DS    0H                                                  0750 02720000
@RC00743 DS    0H                                                  0751 02721000
*   CALL PUT2MSG;                   /* SEND THE MSG                  */ 02722000
@RC00726 BAL   @14,PUT2MSG                                         0751 02723000
*   END HANDLERR;                                                  0752 02724000
@EL00019 DS    0H                                                  0752 02725000
@EF00019 DS    0H                                                  0752 02726000
@ER00019 LM    @14,@12,@SA00019                                    0752 02727000
         BR    @14                                                 0752 02728000
@PB00019 DS    0H                                                  0753 02729000
