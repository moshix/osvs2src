*                                                                  0647 02313000
*KEYCHAR:                                                          0647 02314000
*   PROCEDURE;                                                     0647 02315000
KEYCHAR  STM   @14,@12,@SA00015                                    0647 02316000
*   IF ECTDEFCS=YES                 /* = YES IMPLIES VTAM, NOT TCAM  */ 02317000
*     THEN                                                         0648 02318000
         L     @12,ECTPTR                                          0648 02319000
         TM    ECTDEFCS(@12),B'10000000'                           0648 02320000
         BNO   @RF00648                                            0648 02321000
*     DO I=1 TO #PAIRS              /* LOOP THRU THE PAIRS           */ 02322000
*           WHILE STATUS=CONTINUE;                                 0649 02323000
         LA    I,1                                                 0649 02324000
         B     @DE00649                                            0649 02325000
@DL00649 L     @12,STATUS                                          0649 02326000
         LTR   @12,@12                                             0649 02327000
         BNZ   @DC00649                                            0649 02328000
*       EXTERN=PAIRS(I,1);          /* EXTERNAL REPRESENTATION       */ 02329000
         LR    @12,I                                               0650 02330000
         ALR   @12,@12                                             0650 02331000
         LA    @08,PAIRS-2(@12)                                    0650 02332000
         MVC   EXTERN(1),0(@08)                                    0650 02333000
*       INTERN=PAIRS(I,2);          /* INTERNAL REPRESENTATION       */ 02334000
         LA    @08,PAIRS-1(@12)                                    0651 02335000
         MVC   INTERN(1),0(@08)                                    0651 02336000
*       RFY                                                        0652 02337000
*        (GPR15P) RSTD;                                            0652 02338000
*       GEN REFS(EXTERN,INTERN)     /* GO TO STTRAN                  */ 02339000
*           SETS(STTRANL)                                          0653 02340000
