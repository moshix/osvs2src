*   /*                                                               */ 01174000
*   /* PARAMETER LIST.1. PDE PTR, 2. SAVE AREA PTR                   */ 01175000
*   /*                                                               */ 01176000
*   /*****************************************************************/ 01177000
*                                                                  0342 01178000
*       OPTIONS(NOSAVEAREA,DONTSAVE(15));/* OPTION- DONT SAVE      0342 01179000
*                                      REGISTER 15                   */ 01180000
SCNCK1   ST    @14,12(,@13)                                        0342 01181000
         STM   @00,@12,20(@13)                                     0342 01182000
*   DCL                                                            0343 01183000
*     SCNCK1SA(18) FIXED(31);       /* LOCAL SAVEAREA                */ 01184000
*   RFY                                                            0344 01185000
*    (GPR01P,                                                      0344 01186000
*     GPR02P,                                                      0344 01187000
*     R3,                                                          0344 01188000
*     GPR12P) RSTD;                 /*                       @Z40DQKC*/ 01189000
*   LM(GPR02P,GPR12P,UWAUWA);       /* RESTORE REGISTERS     @Z40DQKC*/ 01190000
         L     @08,CHKUWA(,GPR01P)                                 0345 01191000
         LM    GPR02P,GPR12P,UWAUWA(@08)                           0345 01192000
*   SCNCK1SA(2)=GPR13P;             /* FORWARD CHAIN SAVEAREAS       */ 01193000
         ST    GPR13P,SCNCK1SA+4                                   0346 01194000
*   GPR13P=ADDR(SCNCK1SA);          /* R13 -> NEW SAVEAREA           */ 01195000
         LA    GPR13P,SCNCK1SA                                     0347 01196000
*   R3=CHKPDE;                      /* GET PTR TO PDE        @Z40DQKC*/ 01197000
         L     R3,CHKPDE(,GPR01P)                                  0348 01198000
*   RFY                                                            0349 01199000
*    (GPR01P,                                                      0349 01200000
*     GPR02P,                                                      0349 01201000
*     GPR12P) UNRSTD;               /*                       @Z40DQKC*/ 01202000
*   CALL ICONVERT;                  /* CONVERT INPUT TO BINARY DIGITS*/ 01203000
         BAL   @14,ICONVERT                                        0350 01204000
*   IF R3>=ONE                      /* MINIMUM NUMBER IS 1           */ 01205000
*       &R3<=TWO55                  /* MAXIMUM IS 255                */ 01206000
*     THEN                          /* IS THE NUMBER VALID           */ 01207000
         CH    R3,ONE                                              0351 01208000
         BL    @RF00351                                            0351 01209000
         CH    R3,TWO55                                            0351 01210000
         BH    @RF00351                                            0351 01211000
*     DO;                                                          0352 01212000
*       SIZEVAL1=R3;                /* STORE THE VALUE               */ 01213000
         STH   R3,SIZEVAL1                                         0353 01214000
*       R15=0;                      /* INDICATE THE NUMBER IS VALID  */ 01215000
         SLR   R15,R15                                             0354 01216000
*       GPR13P=SCNCK1SA(2);         /* GET PASSED SA PTR IN R13      */ 01217000
         L     GPR13P,SCNCK1SA+4                                   0355 01218000
*       RETURN;                     /* RETURN TO IKJPARS             */ 01219000
@EL00006 DS    0H                                                  0356 01220000
@EF00006 DS    0H                                                  0356 01221000
@ER00006 L     @14,12(,@13)                                        0356 01222000
         LM    @00,@12,20(@13)                                     0356 01223000
         BR    @14                                                 0356 01224000
*     END;                          /* END CHECK FOR VALID NUMBER    */ 01225000
*   SEGMENT=ADDR(MSG9);             /* GET ERROR MESSAGE POINTER     */ 01226000
@RF00351 L     @12,@CV01896                                        0358 01227000
         LA    @12,MSG9(,@12)                                      0358 01228000
         ST    @12,SEGMENT                                         0358 01229000
*   LEVEL2P=NONEXTLM;               /* INDICATE NO NEXT LEVEL MESSAGE*/ 01230000
         L     @12,NONEXTLM                                        0359 01231000
         ST    @12,LEVEL2P                                         0359 01232000
*   CALL PUT2MSG;                   /* GO TO ISSUE THE ERROR MESSAGE */ 01233000
         BAL   @14,PUT2MSG                                         0360 01234000
*   R15=8;                          /* NUMBER IS INVALID,PROMPT      */ 01235000
         LA    R15,8                                               0361 01236000
*   GPR13P=SCNCK1SA(2);             /* GET PASSED SA PTR IN R13      */ 01237000
         L     GPR13P,SCNCK1SA+4                                   0362 01238000
*   RETURN;                         /* RETURN TO IKJPARS             */ 01239000
         B     @EL00006                                            0363 01240000
*   END SCNCK1;                     /* END CHECK FOR VALID ROWS    0364 01241000
*                                      NUMBER                        */ 01242000
*SCNCK2:                                                           0365 01243000
*                                                                  0365 01244000
*   /*****************************************************************/ 01245000
*   /*                                                               */ 01246000
*   /* THIS VALIDITY CHECKING ROUTINE IS ENTERED BY IKJPARS WHENEVER */ 01247000
*   /* THE TERMINAL USER SPECIFIES THE 'SCRSIZE' KEYWORD ON THE      */ 01248000
*   /* TERMINAL COMMAND.THIS CHECKS LENGTH OF A ROW WHICH MUST BE A  */ 01249000
*   /* NUMBER BETWEEN 1 AND 255.IF NOT, A RETURN CODE OF 8 IS        */ 01250000
*   /* RETURNED TO IKJPARS SPECIFYING THAT THE INPUT IS INVALID.     */ 01251000
*   /* IKJPARS THEN ISSUES A 'PROMPT' MESSAGE AND WAITS FOR VALID    */ 01252000
*   /* INPUT. IF THE NUMBER IS A VALID ONE, A RETURN CODE OF 0 IS    */ 01253000
*   /* SENT TO IKJPARS AND PROCESSING OF OTHER OPERANDS CONTINUE.    */ 01254000
*   /*                                                               */ 01255000
*   /*****************************************************************/ 01256000
*                                                                  0365 01257000
*   PROCEDURE                       /* REGISTER 1 POINTS TO A 2-WORD */ 01258000
*                                                                  0365 01259000
*   /*****************************************************************/ 01260000
*   /*                                                               */ 01261000
*   /* PARAMETER LIST.1. PDE PTR, 2. SAVE AREA PTR                   */ 01262000
*   /*                                                               */ 01263000
*   /*****************************************************************/ 01264000
*                                                                  0365 01265000
*       OPTIONS(NOSAVEAREA,DONTSAVE(15));/* OPTION- DONT SAVE      0365 01266000
*                                      REGISTER 15                   */ 01267000
SCNCK2   ST    @14,12(,@13)                                        0365 01268000
         STM   @00,@12,20(@13)                                     0365 01269000
*   DCL                                                            0366 01270000
*     SCNCK2SA(18) FIXED(31);       /* LOCAL SAVE AREA               */ 01271000
*   RFY                                                            0367 01272000
*    (GPR01P,                                                      0367 01273000
*     GPR02P,                                                      0367 01274000
*     R3,                                                          0367 01275000
*     GPR12P) RSTD;                 /*                       @Z40DQKC*/ 01276000
*   LM(GPR02P,GPR12P,UWAUWA);       /* RESTORE REGISTERS     @Z40DQKC*/ 01277000
         L     @08,CHKUWA(,GPR01P)                                 0368 01278000
         LM    GPR02P,GPR12P,UWAUWA(@08)                           0368 01279000
*   SCNCK2SA(2)=GPR13P;             /* FORWARD CHAIN SAVE AREA       */ 01280000
         ST    GPR13P,SCNCK2SA+4                                   0369 01281000
*   GPR13P=ADDR(SCNCK2SA);          /* R13 -> NEW SAVE AREA          */ 01282000
         LA    GPR13P,SCNCK2SA                                     0370 01283000
*   R3=CHKPDE;                      /* GET PTR TO PDE        @Z40DQKC*/ 01284000
         L     R3,CHKPDE(,GPR01P)                                  0371 01285000
*   RFY                                                            0372 01286000
*    (GPR01P,                                                      0372 01287000
*     GPR02P,                                                      0372 01288000
*     GPR12P) UNRSTD;               /*                       @Z40DQKC*/ 01289000
*   CALL ICONVERT;                  /* CONVERT INPUT TO BINARY DIGITS*/ 01290000
         BAL   @14,ICONVERT                                        0373 01291000
*   IF R3>=ONE                      /* MINIMUM NUMBER IS 1           */ 01292000
*       &R3<=TWO55                  /* MAXIMUM IS 255                */ 01293000
*     THEN                          /* IS THE NUMBER VALID           */ 01294000
         CH    R3,ONE                                              0374 01295000
         BL    @RF00374                                            0374 01296000
         CH    R3,TWO55                                            0374 01297000
         BH    @RF00374                                            0374 01298000
*     DO;                                                          0375 01299000
*       SIZEVAL2=R3;                /* STORE THE VALUE               */ 01300000
         STH   R3,SIZEVAL2                                         0376 01301000
*       R15=0;                      /* INDICATE THE NUMBER IS VALID  */ 01302000
         SLR   R15,R15                                             0377 01303000
*       GPR13P=SCNCK2SA(2);         /* GET PASSED SA BACK IN R13     */ 01304000
         L     GPR13P,SCNCK2SA+4                                   0378 01305000
*       RETURN;                     /* RETURN TO IKJPARS             */ 01306000
@EL00007 DS    0H                                                  0379 01307000
@EF00007 DS    0H                                                  0379 01308000
@ER00007 L     @14,12(,@13)                                        0379 01309000
         LM    @00,@12,20(@13)                                     0379 01310000
         BR    @14                                                 0379 01311000
*     END;                          /* END CHECK FOR VALID NUMBER    */ 01312000
*   SEGMENT=ADDR(MSG0);             /* GET ERROR MESSAGE POINTER     */ 01313000
@RF00374 L     @12,@CV01896                                        0381 01314000
         LA    @12,MSG0(,@12)                                      0381 01315000
         ST    @12,SEGMENT                                         0381 01316000
*   LEVEL2P=NONEXTLM;               /* INDICATE NO NEXT LEVEL MESSAGE*/ 01317000
         L     @12,NONEXTLM                                        0382 01318000
         ST    @12,LEVEL2P                                         0382 01319000
*   CALL PUT2MSG;                   /* GO TO ISSUE THE ERROR MESSAGE */ 01320000
         BAL   @14,PUT2MSG                                         0383 01321000
*   R15=8;                          /* NUMBER IS INVALID,PROMPT      */ 01322000
         LA    R15,8                                               0384 01323000
*   GPR13P=SCNCK2SA(2);             /* RESTORE SA PASSED TO R13      */ 01324000
         L     GPR13P,SCNCK2SA+4                                   0385 01325000
*   RETURN;                         /* RETURN TO IKJPARS             */ 01326000
         B     @EL00007                                            0386 01327000
*   END SCNCK2;                     /* END CHECK FOR LENGTH OF ROW   */ 01328000
*ICONVERT:                                                         0388 01329000
*                                                                  0388 01330000
*   /*****************************************************************/ 01331000
*   /*                                                               */ 01332000
*   /* **************INPUT CONVERSION ROUTINE******************** THE*/ 01333000
*   /* PURPOSE OF THIS PROCEDURE IS TO ACCEPT THE TERMINAL USER'S    */ 01334000
*   /* INPUT WHICH IS IN EBCDIC AND CONVERT IT TO BINARY FORM. UPON  */ 01335000
*   /* EXIT FROM THIS ROUTINE, REGISTER 3 WILL CONTAIN THE CONVERTED */ 01336000
*   /* INPUT.                                                        */ 01337000
*   /*                                                               */ 01338000
*   /*****************************************************************/ 01339000
*                                                                  0388 01340000
*   PROCEDURE                       /* REGISTER 3 HAS PTR TO INPUT   */ 01341000
*       OPTIONS(DONTSAVE,           /* OPTION- DONT SAVE REGISTERS   */ 01342000
*       NOSAVEAREA);                /* GET NO SAVE AREA              */ 01343000
ICONVERT DS    0H                                                  0389 01344000
*   RFY                                                            0389 01345000
*    (R2,                                                          0389 01346000
*     R3,                                                          0389 01347000
*     R7) RSTD;                     /* REGISTERS USED IN PROCEDURE   */ 01348000
*   R7=R14;                         /* SAVE RETURN ADDRESS           */ 01349000
         LR    R7,R14                                              0390 01350000
*   PPDEPTR=R3;                     /* INITIALIZE INPUT POINTER      */ 01351000
         LR    PPDEPTR,R3                                          0391 01352000
*   VALUE3(1:ENTLEN)=INPUTPTR->NUM; /* GET EBCDIC INPUT              */ 01353000
         LH    @12,ENTLEN(,PPDEPTR)                                0392 01354000
         LR    @08,@12                                             0392 01355000
         BCTR  @08,0                                               0392 01356000
         L     @06,INPUTPTR(,PPDEPTR)                              0392 01357000
         EX    @08,@SM02287                                        0392 01358000
*   VALUE2='F0F0F0F0'X;             /* ALLIGN ON DOUBLE WORD         */ 01359000
         MVC   VALUE2(4),@CB02191                                  0393 01360000
*   R2=ADDR(VALUE);                 /* GET PTR TO WORKAREA           */ 01361000
         LA    R2,VALUE                                            0394 01362000
*   R3=ADDR(VALUE2)+ENTLEN;         /* GET PTR TO INPUT              */ 01363000
         LA    R3,VALUE2                                           0395 01364000
         ALR   R3,@12                                              0395 01365000
*   GENERATE;                                                      0396 01366000
*                             /* ISSUE PACK AND CVB INSTRUCTIONS    */  01367000
        PACK 0(8,2),0(4,3)       PACK EBCDIC DIGITS                *    01368000
        CVB  3,0(2)              CONVERT TO BINARY                 *    01369000
        BR   R7                  RETURN TO CALLER                       01370000
*   END ICONVERT;                                                  0397 01371000
@EL00008 DS    0H                                                  0397 01372000
@EF00008 DS    0H                                                  0397 01373000
@ER00008 BR    @14                                                 0397 01374000
*                                                                  0398 01375000
*/********************************************************************/ 01376000
*/*                                                                  */ 01377000
*/*SEGMENT-                                                          */ 01378000
*/*          NOCHAR                                                  */ 01379000
*/*                                                                  */ 01380000
*/*FUNCTION-                                                         */ 01381000
*/*          HANDLE THE NOCHAR KEYWORD                               */ 01382000
*/*                                                                  */ 01383000
*/*OPERATION-                                                        */ 01384000
*/*          FOR THE NOCHAR KEYWORD ISSUE THE STTRAN MACRO TO        */ 01385000
*/*          OBTAIN THE NAME OF THE TABLE THAT MAY HAVE BEEN         */ 01386000
*/*          MODIFIED BY CHARACTER PAIRS.RELOAD THE TABLE AND PASS   */ 01387000
*/*          THE FRESH TABLE BACK TO STTRAN                          */ 01388000
*/*                                                                  */ 01389000
*/*CHANGE ACTIVITY-                                                  */ 01390000
*/*      @Z40DQKC-                                                   */ 01391000
*/*          NEW SUPPORT FOR OS/VS2 RELEASE 4 TSO/VTAM               */ 01392000
*/*                                                                  */ 01393000
*/********************************************************************/ 01394000
*                                                                  0398 01395000
*NOCHAR:                                                           0398 01396000
*   PROCEDURE;                                                     0398 01397000
NOCHAR   STM   @14,@12,@SA00009                                    0398 01398000
*   IF ECTDEFCS=YES                 /* = YES IMPLIES VTAM NOT TCAM   */ 01399000
*     THEN                                                         0399 01400000
         L     @12,ECTPTR                                          0399 01401000
         TM    ECTDEFCS(@12),B'10000000'                           0399 01402000
         BNO   @RF00399                                            0399 01403000
*     DO;                                                          0400 01404000
*       RFY                                                        0401 01405000
*        (GPR15P) RSTD;                                            0401 01406000
*       GEN SETS(STTRANL,TRANAME)   /* GET NAME OF TABLE             */ 01407000
*           (STTRAN NOCHAR,NAME=TRANAME,MF=(E,STTRANL));           0402 01408000
         STTRAN NOCHAR,NAME=TRANAME,MF=(E,STTRANL)                      01409000
*       RETCODE=GPR15P;             /* SAVE RETURN CODE              */ 01410000
         ST    GPR15P,RETCODE                                      0403 01411000
*       RFY                                                        0404 01412000
*        (GPR15P) UNRSTD;                                          0404 01413000
*       IF TRANAME='00'X            /* DID STTRAN GIVE A NAME ??     */ 01414000
*         THEN                                                     0405 01415000
         CLI   TRANAME,X'00'                                       0405 01416000
         BNE   @RF00405                                            0405 01417000
*         RETCODE=32;               /* NO, SET INDICATOR             */ 01418000
         MVC   RETCODE(4),@CF00074                                 0406 01419000
*       IF RETCODE=ZERO             /* STTRAN SUCCESSFUL             */ 01420000
*         THEN                                                     0407 01421000
@RF00405 L     @12,RETCODE                                         0407 01422000
         LTR   @12,@12                                             0407 01423000
         BNZ   @RF00407                                            0407 01424000
*         DO;                                                      0408 01425000
*           CALL LOAD;              /* LOAD THE TABLE                */ 01426000
         BAL   @14,LOAD                                            0409 01427000
*           RFY                                                    0410 01428000
*            (GPR02P,                                              0410 01429000
*             GPR15P) RSTD;                                        0410 01430000
*           GPR02P=STTTABP;         /* ADDRESS OF TABLE              */ 01431000
         L     GPR02P,STTTABP                                      0411 01432000
*           GEN SETS(STTRANL)       /* PASS TAB                      */ 01433000
*               (STTRAN TAB=(2),NAME=TRANAME,MF=(E,STTRANL));      0412 01434000
         STTRAN TAB=(2),NAME=TRANAME,MF=(E,STTRANL)                     01435000
*           RETCODE=GPR15P;         /* SAVE THE RETURN CODE          */ 01436000
         ST    GPR15P,RETCODE                                      0413 01437000
*           RFY                                                    0414 01438000
*            (GPR02P,                                              0414 01439000
*             GPR15P) UNRSTD;                                      0414 01440000
*           IF RETCODE^=ZERO        /* ERROR IN STTRAN               */ 01441000
*             THEN                                                 0415 01442000
         L     @12,RETCODE                                         0415 01443000
         LTR   @12,@12                                             0415 01444000
         BZ    @RF00415                                            0415 01445000
*             STATUS=ERRSTTRN;      /* INDICATE ERROR STATUS         */ 01446000
         L     @12,ERRSTTRN                                        0416 01447000
         ST    @12,STATUS                                          0416 01448000
*           DO;                     /* DELETE EP(TRANAME)            */ 01449000
@RF00415 DS    0H                                                  0418 01450000
*             RESPECIFY                                            0418 01451000
*              (GPR00P) RESTRICTED;                                0418 01452000
*             GPR00P=ADDR(TRANAME); /* ->ENTRY NAME                  */ 01453000
         LA    GPR00P,TRANAME                                      0419 01454000
*             SVC(9);               /* DELETE SVC                    */ 01455000
         SVC   9                                                   0420 01456000
*             RESPECIFY                                            0421 01457000
*              (GPR00P) UNRESTRICTED;                              0421 01458000
*           END;                    /* DELETE EP(TRANAME)            */ 01459000
*         END;                                                     0423 01460000
*       ELSE                                                       0424 01461000
*         IF RETCODE=32 THEN        /* IS DEFAULT TRAN TAB IN EFFECT */ 01462000
         B     @RC00407                                            0424 01463000
@RF00407 CLC   RETCODE(4),@CF00074                                 0424 01464000
         BNE   @RF00424                                            0424 01465000
*           RETCODE=0;              /* ZERO RETURN CODE FIELD        */ 01466000
         SLR   @12,@12                                             0425 01467000
         ST    @12,RETCODE                                         0425 01468000
*         ELSE                                                     0426 01469000
*           STATUS=ERRSTTRN;        /* ELSE STTRAN ERROR             */ 01470000
         B     @RC00424                                            0426 01471000
@RF00424 L     @12,ERRSTTRN                                        0426 01472000
         ST    @12,STATUS                                          0426 01473000
*     END;                                                         0427 01474000
*   ELSE                                                           0428 01475000
*     STATUS=IGNCHAR;               /* IGNORE NOCHAR KYWD FOR VTAM   */ 01476000
         B     @RC00399                                            0428 01477000
@RF00399 L     @12,IGNCHAR                                         0428 01478000
         ST    @12,STATUS                                          0428 01479000
*   IF STATUS^=CONTINUE             /* EN ERROR HAS OCCURRED         */ 01480000
*     THEN                                                         0429 01481000
@RC00399 L     @12,STATUS                                          0429 01482000
         LTR   @12,@12                                             0429 01483000
         BZ    @RF00429                                            0429 01484000
*     CALL HANDLERR;                /* SEND AN ERROR MESSAGE         */ 01485000
         BAL   @14,HANDLERR                                        0430 01486000
*   END NOCHAR;                                                    0431 01487000
@EL00009 DS    0H                                                  0431 01488000
@EF00009 DS    0H                                                  0431 01489000
@ER00009 LM    @14,@12,@SA00009                                    0431 01490000
         BR    @14                                                 0431 01491000
*                                                                  0432 01492000
*/********************************************************************/ 01493000
*/*                                                                  */ 01494000
*/*SEGMENT-                                                          */ 01495000
*/*          CHKCHAR                                                 */ 01496000
*/*                                                                  */ 01497000
*/*FUNCTION-                                                         */ 01498000
*/*          VALIDITY CHECK THE CHARACTER PAIRS FOR STTRAN           */ 01499000
*/*                                                                  */ 01500000
*/*OPERATION-                                                        */ 01501000
*/*          A SAVE AREA IS NEEDED BECAUSE OF THE CALLING OF PARSE   */ 01502000
*/*                                                                  */ 01503000
*/*          BUILD A COMMAND BUFFER FROM THE PSTRING AND CALL PARSE  */ 01504000
*/*          TO PARSE THE PSTRING.CALL STTRAN WITH THE CHARACTERS    */ 01505000
*/*          THAT WERE FOUND                                         */ 01506000
*/*                                                                  */ 01507000
*/*CHANGE ACTIVITY-                                                  */ 01508000
*/*      @Z40DQKC-                                                   */ 01509000
*/*          NEW FOR VS2 RELEASE 4 SNA SUPPORT                       */ 01510000
*/*                                                                  */ 01511000
*/********************************************************************/ 01512000
*                                                                  0432 01513000
*CHKCHAR:                                                          0432 01514000
*   PROCEDURE OPTIONS(NOSAVEAREA);                                 0432 01515000
CHKCHAR  STM   @14,@12,12(@13)                                     0432 01516000
*   RFY                                                            0433 01517000
*    (GPR01P,                                                      0433 01518000
*     GPR02P,                                                      0433 01519000
*     GPR12P,                                                      0433 01520000
*     GPR13P) RSTD;                                                0433 01521000
*   LM(GPR02P,GPR12P,UWAUWA);       /* RESTORE CALLER OF PARSE REGS  */ 01522000
         L     @08,CHKUWA(,GPR01P)                                 0434 01523000
         LM    GPR02P,GPR12P,UWAUWA(@08)                           0434 01524000
*   RFY                                                            0435 01525000
*    (GPR02P,                                                      0435 01526000
*     GPR12P) UNRSTD;                                              0435 01527000
*   GPR13P->UWAF13=ADDR(CHKCHARS);  /* PARSE SA -> CHKCHAR SA        */ 01528000
         LA    @12,CHKCHARS                                        0436 01529000
         ST    @12,UWAF13(,GPR13P)                                 0436 01530000
*   ADDR(CHKCHARS)->UWAB13=GPR13P;  /* CHKCHAR SA -> PARSE SA        */ 01531000
         ST    GPR13P,UWAB13(,@12)                                 0437 01532000
*   GPR13P=ADDR(CHKCHARS);          /* POINT TO NEW SAVE AREA        */ 01533000
         LR    GPR13P,@12                                          0438 01534000
*   IF ECTDEFCS=YES                 /* = YES IMPLIES VTAM, NOT TCAM  */ 01535000
*     THEN                                                         0439 01536000
         L     @12,ECTPTR                                          0439 01537000
         TM    ECTDEFCS(@12),B'10000000'                           0439 01538000
         BNO   @RF00439                                            0439 01539000
*     DO;                                                          0440 01540000
*       PDEPTR1=CHKPDE;             /* POINTER TO PDE FOR PSTRING    */ 01541000
         L     @12,CHKPDE(,GPR01P)                                 0441 01542000
         ST    @12,PDEPTR1                                         0441 01543000
*       PDLPTR1=ZERO;               /* NO PDL YET                    */ 01544000
         SLR   @08,@08                                             0442 01545000
         ST    @08,PDLPTR1                                         0442 01546000
*       RFY                                                        0443 01547000
*        (GPR01P,                                                  0443 01548000
*         GPR13P) UNRSTD;                                          0443 01549000
*       PAIRLEN=PDELEN+4;           /* LENGTH OF COMMAND BUFFER      */ 01550000
         LH    @06,PDELEN(,@12)                                    0444 01551000
         LA    @05,4(,@06)                                         0444 01552000
         STH   @05,PAIRLEN                                         0444 01553000
*       PAIROFF=ZERO;               /* ZERO OFFSET TO TEXT           */ 01554000
         STH   @08,PAIROFF                                         0445 01555000
*       IF PDELEN<=LENGTH(PAIRTEXT) /* COPY PSTRING TO THE BUFFER    */ 01556000
*         THEN                                                     0446 01557000
         C     @06,@CF01290                                        0446 01558000
         BH    @RF00446                                            0446 01559000
*         PAIRTEXT(1:PDELEN)=PDESTRNG(1:PDELEN);                   0447 01560000
         BCTR  @06,0                                               0447 01561000
         L     @12,PDESPTR(,@12)                                   0447 01562000
         EX    @06,@SM02289                                        0447 01563000
*       ELSE                                                       0448 01564000
*         PAIRTEXT=PDESTRNG(1:LENGTH(PAIRTEXT));                   0448 01565000
         B     @RC00446                                            0448 01566000
@RF00446 L     @12,PDEPTR1                                         0448 01567000
         L     @12,PDESPTR(,@12)                                   0448 01568000
         MVC   PAIRTEXT(256),PDESTRNG(@12)                         0448 01569000
*       CALL PARSPAIR;              /* PARSE X'__',X'__' AND PASS TO    01570000
*                                      STTRAN                        */ 01571000
@RC00446 BAL   @14,PARSPAIR                                        0449 01572000
*       GEN REFS(PDLPTR1)           /* FREE PDL FOR CHKCHARS         */ 01573000
*           (IKJRLSA PDLPTR1);                                     0450 01574000
         IKJRLSA PDLPTR1                                                01575000
*     END;                                                         0451 01576000
*   ELSE                                                           0452 01577000
*     STATUS=IGNCHAR;               /* IGNORE CHAR KEYWORD FOR TCAM  */ 01578000
         B     @RC00439                                            0452 01579000
@RF00439 L     @12,IGNCHAR                                         0452 01580000
         ST    @12,STATUS                                          0452 01581000
*   IF STATUS^=CONTINUE             /* THERE ARE ERRORS              */ 01582000
*     THEN                                                         0453 01583000
@RC00439 L     @12,STATUS                                          0453 01584000
         LTR   @12,@12                                             0453 01585000
         BZ    @RF00453                                            0453 01586000
*     CALL HANDLERR;                /* HANDLE THE ERRORS             */ 01587000
         BAL   @14,HANDLERR                                        0454 01588000
*   RFY                                                            0455 01589000
*    (GPR13P) RSTD;                                                0455 01590000
@RF00453 DS    0H                                                  0456 01591000
*   GPR13P=GPR13P->UWAB13;          /* GET PTR TO PARSE SA           */ 01592000
         L     GPR13P,UWAB13(,GPR13P)                              0456 01593000
*   RFY                                                            0457 01594000
*    (GPR13P) UNRSTD;                                              0457 01595000
*   RETURN CODE(RETCODE);           /* RETURN TO PARSE               */ 01596000
         L     @15,RETCODE                                         0458 01597000
         L     @14,12(,@13)                                        0458 01598000
         LM    @00,@12,20(@13)                                     0458 01599000
         BR    @14                                                 0458 01600000
*   END CHKCHAR;                                                   0459 01601000
*                                                                  0460 01602000
*/********************************************************************/ 01603000
*/*                                                                  */ 01604000
*/*SEGMENT-                                                          */ 01605000
*/*          PARSPAIR                                                */ 01606000
*/*                                                                  */ 01607000
*/*FUNCTION-                                                         */ 01608000
*/*          PARSE THE CHARACTER PAIRS AND PASS THEM TO STTRAN       */ 01609000
*/*                                                                  */ 01610000
*/*OPERATION-                                                        */ 01611000
*/*          USING THE SECOND PPL PARSE A PARI OF CHARACTERS--X'__', */ 01612000
*/*          X'__'--AND THEN PASS THEM TO STTRAN WHO WILL ADD THEM   */ 01613000
*/*          TO THE EXISTING TRANSLATE TABLES                        */ 01614000
*/*                                                                  */ 01615000
*/*CHANGE ACTIVITY-                                                  */ 01616000
*/*      @Z40DQKC-                                                   */ 01617000
*/*          NEW SUPPORT FOR VS2 RELEASE 4 SNA                       */ 01618000
*/*                                                                  */ 01619000
*/********************************************************************/ 01620000
*                                                                  0460 01621000
*PARSPAIR:                                                         0460 01622000
*   PROCEDURE;                                                     0460 01623000
PARSPAIR STM   @14,@12,@SA00011                                    0460 01624000
*   RFY                                                            0461 01625000
*    (GPR15P) RSTD;                                                0461 01626000
*   DO;                             /* CALLTSSR                    0462 01627000
*                                      EP(IKJPARS)MF(E,PPLWA2)       */ 01628000
*     RESPECIFY                                                    0463 01629000
*       GPR01P RSTD;                                               0463 01630000
*     GPR01P=ADDR(PPLWA2);                                         0464 01631000
         LA    GPR01P,PPLWA2                                       0464 01632000
*     IF CVTPARS=0 THEN                                            0465 01633000
         L     @14,CVTPTR                                          0465 01634000
         L     @14,CVTPARS(,@14)                                   0465 01635000
         LTR   @14,@14                                             0465 01636000
         BNZ   @RF00465                                            0465 01637000
*       GEN(LINK EP=IKJPARS);                                      0466 01638000
         LINK EP=IKJPARS                                                01639000
*     ELSE                                                         0467 01640000
*       CALL CVTPARS;                                              0467 01641000
         B     @RC00465                                            0467 01642000
@RF00465 L     @12,CVTPTR                                          0467 01643000
         L     @15,CVTPARS(,@12)                                   0467 01644000
         BALR  @14,@15                                             0467 01645000
*     RESPECIFY                                                    0468 01646000
*       GPR01P UNRSTD;                                             0468 01647000
@RC00465 DS    0H                                                  0469 01648000
*   END;                                                           0469 01649000
*   RETCODE=GPR15P;                 /* SAVE PARSE RETURN CODE        */ 01650000
         ST    GPR15P,RETCODE                                      0470 01651000
*   RFY                                                            0471 01652000
*    (GPR15P) UNRSTD;                                              0471 01653000
*   IF RETCODE=ZERO                 /* SUCCESSFUL PARSE              */ 01654000
*     THEN                                                         0472 01655000
         L     @12,RETCODE                                         0472 01656000
         LTR   @12,@12                                             0472 01657000
         BNZ   @RF00472                                            0472 01658000
*     DO;                                                          0473 01659000
*       RFY                                                        0474 01660000
*        (GPR02P) RSTD;                                            0474 01661000
*       GPR02P=PDLPTR1;             /* ADDR OF PDL                   */ 01662000
         L     GPR02P,PDLPTR1                                      0475 01663000
*       GEN REFS(GPR02P)            /* ADDRESS THE PDL               */ 01664000
*           (USING IKJPDL2,GPR02P);                                0476 01665000
         USING IKJPDL2,GPR02P                                           01666000
*       #PAIRS=#PAIRS+1;            /* INCREMENT PAIRS COUNTER       */ 01667000
         LA    @12,1                                               0477 01668000
         AL    @12,#PAIRS                                          0477 01669000
         ST    @12,#PAIRS                                          0477 01670000
*       RFY                                                        0478 01671000
*         PDE BASED(ADDR($EXTERNL));                               0478 01672000
*       PAIRS(#PAIRS,1)=            /* GET EXTERNAL REPRESENTATION   */ 01673000
*           PDESTRNG(ONE);                                         0479 01674000
         ALR   @12,@12                                             0479 01675000
         LA    @08,PAIRS-2(@12)                                    0479 01676000
         LH    @06,ONE                                             0479 01677000
         LA    @05,$EXTERNL                                        0479 01678000
         L     @05,PDESPTR(,@05)                                   0479 01679000
         ALR   @05,@06                                             0479 01680000
         BCTR  @05,0                                               0479 01681000
         MVC   0(1,@08),PDESTRNG(@05)                              0479 01682000
*       RFY                                                        0480 01683000
*         PDE BASED(ADDR($INTERNL));                               0480 01684000
*       PAIRS(#PAIRS,2)=            /* GET INTERNAL REPRESENTATION   */ 01685000
*           PDESTRNG(ONE);                                         0481 01686000
         LA    @08,PAIRS-1(@12)                                    0481 01687000
         LA    @12,$INTERNL                                        0481 01688000
         L     @12,PDESPTR(,@12)                                   0481 01689000
         ALR   @12,@06                                             0481 01690000
         BCTR  @12,0                                               0481 01691000
         MVC   0(1,@08),PDESTRNG(@12)                              0481 01692000
*       RFY                                                        0482 01693000
*         PDE BASED(PDEPTR1);                                      0482 01694000
*       RFY                                                        0483 01695000
*        (GPR02P) UNRSTD;                                          0483 01696000
*     END;                                                         0484 01697000
*   ELSE                                                           0485 01698000
*     IF RETCODE>FOUR               /* UNSUCCESSFUL PARSE            */ 01699000
*       THEN                                                       0485 01700000
         B     @RC00472                                            0485 01701000
@RF00472 L     @12,RETCODE                                         0485 01702000
         C     @12,@CF00049                                        0485 01703000
         BNH   @RF00485                                            0485 01704000
*       STATUS=ADDR(ERRCODE);       /* SET PARSE ERROR MESSAGE       */ 01705000
         L     @12,@CV01896                                        0486 01706000
         LA    @12,ERRCODE(,@12)                                   0486 01707000
         ST    @12,STATUS                                          0486 01708000
*   END PARSPAIR;                                                  0487 01709000
@EL00011 DS    0H                                                  0487 01710000
@EF00011 DS    0H                                                  0487 01711000
@ER00011 LM    @14,@12,@SA00011                                    0487 01712000
         BR    @14                                                 0487 01713000
*                                                                  0488 01714000
*/********************************************************************/ 01715000
*/*                                                                  */ 01716000
*/*SEGMENT-                                                          */ 01717000
*/*          CHKACHAR                                                */ 01718000
*/*                                                                  */ 01719000
*/*FUNCTION-                                                         */ 01720000
*/*          GET THE HEX REPRESENTATION FOR THE CHAR                 */ 01721000
*/*                                                                  */ 01722000
*/*OPERATION-                                                        */ 01723000
*/*          FOR A HEX REPRESENTATION CONVERT FROM EBCDIC TO HEX.IN  */ 01724000
*/*          THE CHAR AND HEX CASE ENSURE THAT THE CHARACTER IS NOT  */ 01725000
*/*          AN UPPER CASE LETTER                                    */ 01726000
*/*                                                                  */ 01727000
*/*          TO ASSURE THAT THE USER CAN GET OUT OF TRANSLATE MODE   */ 01728000
*/*          THE USER WILL NOT BE ALLOWED TO TRANSLATE THE UPPER     */ 01729000
*/*          CASE LETTERS                                            */ 01730000
*/*                                                                  */ 01731000
*/*CHANGE ACTIVITY-                                                  */ 01732000
*/*      @Z40DQKC-                                                   */ 01733000
*/*          NEW SUPPORT FOR VS2 RELEASE 4 SNA                       */ 01734000
*/*                                                                  */ 01735000
*/********************************************************************/ 01736000
*                                                                  0488 01737000
*CHKACHAR:                                                         0488 01738000
*   PROCEDURE OPTIONS(NOSAVEAREA);                                 0488 01739000
CHKACHAR STM   @14,@12,12(@13)                                     0488 01740000
*   RFY                                                            0489 01741000
*    (GPR01P,                                                      0489 01742000
*     GPR02P,                                                      0489 01743000
*     GPR12P) RSTD;                                                0489 01744000
*   LM(GPR02P,GPR12P,UWAUWA);       /* GET CHKCHARS REGS             */ 01745000
         L     @08,CHKUWA(,GPR01P)                                 0490 01746000
         LM    GPR02P,GPR12P,UWAUWA(@08)                           0490 01747000
*   PDEPTR1=CHKPDE;                 /* GET PDE FOR X'__' OR C'_'     */ 01748000
         L     @08,CHKPDE(,GPR01P)                                 0491 01749000
         ST    @08,PDEPTR1                                         0491 01750000
*   RFY                                                            0492 01751000
*    (GPR01P,                                                      0492 01752000
*     GPR02P,                                                      0492 01753000
*     GPR12P) UNRSTD;                                              0492 01754000
*   HEXTWO=PDESTRNG(1);             /* MOVE ONE CHAR                 */ 01755000
         MVI   HEXTWO+1,C' '                                       0493 01756000
         L     @12,PDESPTR(,@08)                                   0493 01757000
         MVC   HEXTWO(1),PDESTRNG(@12)                             0493 01758000
*   IF PDETYPE=HEX&                 /* TYPE = X'__'                  */ 01759000
*       PDELEN=TWO                  /* TWO HEX DIGITS                */ 01760000
*     THEN                                                         0494 01761000
         CLI   PDETYPE(@08),C'X'                                   0494 01762000
         BNE   @RF00494                                            0494 01763000
         CLC   PDELEN(2,@08),@CH00174                              0494 01764000
         BNE   @RF00494                                            0494 01765000
*     DO;                                                          0495 01766000
*       HEXTWO=PDESTRNG(1:2)|FOLD;  /* FOLD TO UPPER CASE            */ 01767000
         MVC   HEXTWO(2),PDESTRNG(@12)                             0496 01768000
         OC    HEXTWO(2),@CC02117                                  0496 01769000
*       TR(HEXTWO,HEXTABLE(1));     /* 'C2'X->'FB'X,'F0'X->'F0'X     */ 01770000
         TR    HEXTWO(2),HEXTABLE                                  0497 01771000
*       IF HEXTWO(1)^=INVHEX&       /* BOTH CHARACTERS ARE VALID     */ 01772000
*           HEXTWO(2)^=INVHEX THEN                                 0498 01773000
         CLI   HEXTWO,X'00'                                        0498 01774000
         BE    @RF00498                                            0498 01775000
         CLI   HEXTWO+1,X'00'                                      0498 01776000
         BE    @RF00498                                            0498 01777000
*         DO;                                                      0499 01778000
*           PACK(HEXTWO,HEXTWO);    /* 'FBF0'X -> '0B0F'X            */ 01779000
         PACK  HEXTWO(2),HEXTWO(2)                                 0500 01780000
*           SRL(HEXTWO,FOUR);       /* '0B0F'X -> '00B0'X            */ 01781000
         SLR   @12,@12                                             0501 01782000
         ICM   @12,3,HEXTWO                                        0501 01783000
         SRL   @12,4                                               0501 01784000
         STCM  @12,3,HEXTWO                                        0501 01785000
*           SLL(HEXTWO,8);          /* '00B0'X -> 'B000'X ....       */ 01786000
         SLR   @12,@12                                             0502 01787000
         ICM   @12,3,HEXTWO                                        0502 01788000
         SLL   @12,8                                               0502 01789000
         STCM  @12,3,HEXTWO                                        0502 01790000
*         END;                                                     0503 01791000
*       ELSE                                                       0504 01792000
*         STATUS=INVCHAR;           /* CHAR IS INVALID               */ 01793000
         B     @RC00498                                            0504 01794000
@RF00498 MVC   STATUS(4),@CF00161                                  0504 01795000
*     END;                                                         0505 01796000
*   ELSE                                                           0506 01797000
*     IF PDETYPE^=CHARCHAR|         /* NOT A C'_'                    */ 01798000
*         PDELEN^=ONE               /* NOT OF LENGTH ONE             */ 01799000
*       THEN                                                       0506 01800000
         B     @RC00494                                            0506 01801000
@RF00494 L     @12,PDEPTR1                                         0506 01802000
         CLI   PDETYPE(@12),C'C'                                   0506 01803000
         BNE   @RT00506                                            0506 01804000
         CLC   PDELEN(2,@12),ONE                                   0506 01805000
         BE    @RF00506                                            0506 01806000
@RT00506 DS    0H                                                  0507 01807000
*       STATUS=INVCHAR;             /* INVALID CHARACTER             */ 01808000
         MVC   STATUS(4),@CF00161                                  0507 01809000
*   IF STATUS=CONTINUE              /* THINGS OK SO FAR              */ 01810000
*     THEN                                                         0508 01811000
@RF00506 DS    0H                                                  0508 01812000
@RC00494 L     @12,STATUS                                          0508 01813000
         LTR   @12,@12                                             0508 01814000
         BNZ   @RF00508                                            0508 01815000
*     DO;                                                          0509 01816000
*       RFY                                                        0510 01817000
*        (GPR01P) RSTD;                                            0510 01818000
*       GPR01P=ZERO;                /* NULL VALUE                    */ 01819000
         SLR   GPR01P,GPR01P                                       0511 01820000
*       TRT(HEXTWO(1),CHARTABL(1)); /* TEST FOR UPPER CASE           */ 01821000
         TRT   HEXTWO(1),CHARTABL                                  0512 01822000
*       STATUS=GPR01P;              /* ZERO OR UPPER CASE            */ 01823000
         ST    GPR01P,STATUS                                       0513 01824000
*       RFY                                                        0514 01825000
*        (GPR01P) UNRSTD;                                          0514 01826000
*     END;                                                         0515 01827000
*   IF STATUS=CONTINUE              /* EVERYTHING OK SO FAR          */ 01828000
*     THEN                                                         0516 01829000
@RF00508 SLR   @12,@12                                             0516 01830000
         C     @12,STATUS                                          0516 01831000
         BNE   @RF00516                                            0516 01832000
*     DO;                                                          0517 01833000
*       RETCODE=ZERO;               /* ZERO RETURN CODE              */ 01834000
         ST    @12,RETCODE                                         0518 01835000
*       PDESTRNG(1)=HEXTWO(1);      /* MOVE CONVERTED CHAR INTO PDE  */ 01836000
         L     @12,PDEPTR1                                         0519 01837000
         L     @12,PDESPTR(,@12)                                   0519 01838000
         MVC   PDESTRNG(1,@12),HEXTWO                              0519 01839000
*     END;                                                         0520 01840000
*   ELSE                                                           0521 01841000
*     DO;                           /* INVALID CHARACTER             */ 01842000
         B     @RC00516                                            0521 01843000
@RF00516 DS    0H                                                  0522 01844000
*       RETCODE=FOUR;               /* FORCE A PROMPT                */ 01845000
         MVC   RETCODE(4),@CF00049                                 0522 01846000
*       STATUS=CONTINUE;            /* RESET CONTINUE STATUS         */ 01847000
         SLR   @12,@12                                             0523 01848000
         ST    @12,STATUS                                          0523 01849000
*     END;                                                         0524 01850000
*   RETURN CODE(RETCODE);           /* RETURN TO PARSE               */ 01851000
@RC00516 L     @15,RETCODE                                         0525 01852000
         L     @14,12(,@13)                                        0525 01853000
         LM    @00,@12,20(@13)                                     0525 01854000
         BR    @14                                                 0525 01855000
*   END CHKACHAR;                                                  0526 01856000
*                                                                  0527 01857000
*/********************************************************************/ 01858000
*/*                                                                  */ 01859000
*/*SEGMENT-                                                          */ 01860000
*/*          CHKTRAN                                                 */ 01861000
*/*                                                                  */ 01862000
*/*FUNCTION-                                                         */ 01863000
*/*          VERIFY THE NAME OF THE TRANSLATE TABLE                  */ 01864000
*/*                                                                  */ 01865000
*/*OPERATION-                                                        */ 01866000
*/*          A SAVE AREA IS NEEDED BECAUSE OF THE INVOCATION OF      */ 01867000
*/*          OTHER SEGMENTS                                          */ 01868000
*/*                                                                  */ 01869000
*/*          INITIALIZE THE ESTAE ENVIRONMENT THAT WILL INTERCEPT    */ 01870000
*/*          THE 806 ABEND FOR AN UNKNOWN TABLE NAME.SAVE THE        */ 01871000
*/*          REGISTERS AND THEN LOAD THE TRANSLATE TABLE.IF THERE    */ 01872000
*/*          IS NO 806 ABEND THEN ISSUE STTRAN TO PASS THE TABLE TO  */ 01873000
*/*          VTAM,OTHERWISE REPROMPT FOR A NEW NAME.CANCEL THE       */ 01874000
*/*          ESTAE ENVIRONMENT.                                      */ 01875000
*/*                                                                  */ 01876000
*/*CHANGE ACTIVITY-                                                  */ 01877000
*/*      @Z40DQKC-                                                   */ 01878000
*/*          NEW SUPPORT FOR VS2 RELEASE 4 SNA                       */ 01879000
*/*                                                                  */ 01880000
*/********************************************************************/ 01881000
*                                                                  0527 01882000
*CHKTRAN:                                                          0527 01883000
*   PROCEDURE OPTIONS(NOSAVEAREA);                                 0527 01884000
CHKTRAN  STM   @14,@12,12(@13)                                     0527 01885000
*   RFY                                                            0528 01886000
*    (GPR01P,                                                      0528 01887000
*     GPR02P,                                                      0528 01888000
*     GPR12P,                                                      0528 01889000
*     GPR13P) RSTD;                                                0528 01890000
*   LM(GPR02P,GPR12P,UWAUWA);       /* RESTORE CALLER OF PARSE REGS  */ 01891000
         L     @08,CHKUWA(,GPR01P)                                 0529 01892000
         LM    GPR02P,GPR12P,UWAUWA(@08)                           0529 01893000
*   RFY                                                            0530 01894000
*    (GPR02P,                                                      0530 01895000
*     GPR12P) UNRSTD;                                              0530 01896000
*   GPR13P->UWAF13=ADDR(CHKTRANS);  /* PARSE SA -> CHKTRAN SA        */ 01897000
         LA    @12,CHKTRANS                                        0531 01898000
         ST    @12,UWAF13(,GPR13P)                                 0531 01899000
*   ADDR(CHKTRANS)->UWAB13=GPR13P;  /* CHKTRAN SA -> PARSE SA        */ 01900000
         ST    GPR13P,UWAB13(,@12)                                 0532 01901000
*   GPR13P=ADDR(CHKTRANS);          /* POINT TO NEW SAVE AREA        */ 01902000
         LR    GPR13P,@12                                          0533 01903000
*   IF ECTDEFCS=YES                 /* = YES IMPLIES VTAM NOT TCAM   */ 01904000
*     THEN                                                         0534 01905000
         L     @12,ECTPTR                                          0534 01906000
         TM    ECTDEFCS(@12),B'10000000'                           0534 01907000
         BNO   @RF00534                                            0534 01908000
*     DO;                                                          0535 01909000
*       PDEPTR1=CHKPDE;             /* POINTER TO PDE FOR PSTRING    */ 01910000
         L     @12,CHKPDE(,GPR01P)                                 0536 01911000
         ST    @12,PDEPTR1                                         0536 01912000
*       RFY                                                        0537 01913000
*        (GPR01P,                                                  0537 01914000
*         GPR13P) UNRSTD;                                          0537 01915000
*       TRANAME=BLANK;              /* BLANK OUT THE NAME            */ 01916000
         MVI   TRANAME+1,C' '                                      0538 01917000
         MVC   TRANAME+2(6),TRANAME+1                              0538 01918000
         MVI   TRANAME,C' '                                        0538 01919000
*       TRANAME(1:PDELEN)=PDESTRNG(1:PDELEN);/* COPY TAB NAME        */ 01920000
         LH    @08,PDELEN(,@12)                                    0539 01921000
         BCTR  @08,0                                               0539 01922000
         L     @12,PDESPTR(,@12)                                   0539 01923000
         EX    @08,@SM02291                                        0539 01924000
*       CALL SETESTAE;              /* SETUP ESTAE ENVIRONMENT       */ 01925000
         BAL   @14,SETESTAE                                        0540 01926000
*       CALL LOAD;                  /* LOAD TRANSLATE TABLE          */ 01927000
         BAL   @14,LOAD                                            0541 01928000
*       IF STATUS=CONTINUE&         /* NO ERRORS                     */ 01929000
*           ESTA806=NO              /* NO 806 ABEND ON LOAD          */ 01930000
*         THEN                                                     0542 01931000
         L     @12,STATUS                                          0542 01932000
         LTR   @12,@12                                             0542 01933000
         BNZ   @RF00542                                            0542 01934000
         LA    @12,$ESTAPRM                                        0542 01935000
         TM    ESTA806(@12),B'10000000'                            0542 01936000
         BNZ   @RF00542                                            0542 01937000
*         DO;                                                      0543 01938000
*           COOL=NO;                /* INIT FLAG                     */ 01939000
         NI    COOL,B'01111111'                                    0544 01940000
*           TSIZEERR=NO;            /* INIT FLAG                     */ 01941000
         NI    TSIZEERR,B'01111111'                                0545 01942000
*           TCHARERR=NO;            /* INIT FLAG                     */ 01943000
         NI    TCHARERR,B'01111111'                                0546 01944000
*           MATCH=NO;               /* INIT FLAG                     */ 01945000
         NI    MATCH,B'01111111'                                   0547 01946000
*           LLEPTR=TCBLLS;          /* INIT LOAD LIST ELE PTR        */ 01947000
         L     @12,CVTPTR                                          0548 01948000
         L     @12,CVTTCBP(,@12)                                   0548 01949000
         L     @12,TCBPTR(,@12)                                    0548 01950000
         L     LLEPTR,TCBLLS(,@12)                                 0548 01951000
*           DO WHILE LLEPTR^=0&MATCH=NO;                           0549 01952000
         B     @DE00549                                            0549 01953000
@DL00549 DS    0H                                                  0550 01954000
*             CDENTPTR=LLECDPT;     /* GET ADDR OF CDE               */ 01955000
         L     CDENTPTR,LLECDPT(,LLEPTR)                           0550 01956000
*             ENDCHAN=NO;                                          0551 01957000
         NI    ENDCHAN,B'01111111'                                 0551 01958000
*             DO WHILE CDNAME^=TRANAME/* FIND CORRECT DCE            */ 01959000
*                   &ENDCHAN=NO;                                   0552 01960000
         B     @DE00552                                            0552 01961000
@DL00552 DS    0H                                                  0553 01962000
*               IF CDMIN=NO         /* IF NOT A MINOR NAME           */ 01963000
*                 THEN              /* AND NO MATCH ON NAME          */ 01964000
         TM    CDMIN(CDENTPTR),B'00000100'                         0553 01965000
         BNZ   @RF00553                                            0553 01966000
*                 CDENTPTR=CDCHAIN; /* GET NEXT CDE ON CHAIN         */ 01967000
         L     CDENTPTR,CDCHAIN(,CDENTPTR)                         0554 01968000
*               ELSE                /* ELSE FIND MAJOR NAME          */ 01969000
*                 CDENTPTR=CDXLMJP; /* RESET CDE BASE                */ 01970000
         B     @RC00553                                            0555 01971000
@RF00553 L     CDENTPTR,CDXLMJP(,CDENTPTR)                         0555 01972000
*               IF CDENTPTR=0       /* REACH END OF THIS CHAIN ?     */ 01973000
*                 THEN                                             0556 01974000
@RC00553 LTR   CDENTPTR,CDENTPTR                                   0556 01975000
         BNZ   @RF00556                                            0556 01976000
*                 ENDCHAN=YES;      /* YES,GOTO NEXT LLE             */ 01977000
         OI    ENDCHAN,B'10000000'                                 0557 01978000
*             END;                  /* END SEARCH                    */ 01979000
@RF00556 DS    0H                                                  0558 01980000
@DE00552 CLC   CDNAME(8,CDENTPTR),TRANAME                          0558 01981000
         BE    @DC00552                                            0558 01982000
         TM    ENDCHAN,B'10000000'                                 0558 01983000
         BZ    @DL00552                                            0558 01984000
@DC00552 DS    0H                                                  0559 01985000
*             IF ENDCHAN=NO         /* WAS TABLE NAME FOUND ?        */ 01986000
*               THEN                                               0559 01987000
         TM    ENDCHAN,B'10000000'                                 0559 01988000
         BNZ   @RF00559                                            0559 01989000
*               DO;                 /* YES, EXTENT LIST CHECK NEXT   */ 01990000
*                 XTLPTR=CDXLMJP;   /* CDE -> EXTENT LIST            */ 01991000
         L     XTLPTR,CDXLMJP(,CDENTPTR)                           0561 01992000
*                 MATCH=YES;        /* END DO LOOP                   */ 01993000
         OI    MATCH,B'10000000'                                   0562 01994000
*                 IF XTLMSBLN<516   /* IS TABLE TO SMALL             */ 01995000
*                   THEN                                           0563 01996000
         CLC   XTLMSBLN(3,XTLPTR),@CF02224+1                       0563 01997000
         BNL   @RF00563                                            0563 01998000
*                   TSIZEERR=YES;   /* INDICATE ERRONEOUS TABLE SIZE */ 01999000
         OI    TSIZEERR,B'10000000'                                0564 02000000
*                 ELSE              /* ELSE, VALIDITY CHECK FOR UPPER*/ 02001000
*                   DO;             /* CASE LETTERS AND NUMBERS      */ 02002000
*                                                                  0565 02003000
         B     @RC00563                                            0565 02004000
@RF00563 DS    0H                                                  0566 02005000
*                     /***********************************************/ 02006000
*                     /*                                             */ 02007000
*                     /* NOTICE THAT PLS (IN HIS INFINATE WISDOM)    */ 02008000
*                     /* CAN'T FIGURE OUT WHERE 'C1'X IS IN A 256    */ 02009000
*                     /* BYTE TABLE... THEREFORE, 'C2'X IS REALLY    */ 02010000
*                     /* 'C1'X INTO THE TABLE... ECT. ????????       */ 02011000
*                     /* VS43255                                     */ 02012000
*                     /*                                             */ 02013000
*                     /***********************************************/ 02014000
*                                                                  0566 02015000
*/*    FIRST VALIDITY CHECK THE SECOND TABLE...  NO SPECIFIC REASON     02016000
*      JUST THAT THIS ONE WAS DECLARED FIRST....                     */ 02017000
*                                                                  0566 02018000
*                     IF LOADTAB('C2'X:'CA'X)=TESTTAB(1:9)&LOADTAB('D2' 02019000
*                         :'DA'X)=TESTTAB(10:18)&LOADTAB('E3'X:'EA'X)=  02020000
*                         TESTTAB(19:26)&LOADTAB('F1'X:'FA'X)=TESTTAB(  02021000
*                         27:LENGTH(TESTTAB)) THEN/* EVERYTHING IS 0566 02022000
*                                      COOL IN THE 2ND               */ 02023000
         L     @12,STTTABP                                         0566 02024000
         L     @08,COMPTABP(,@12)                                  0566 02025000
         CLC   LOADTAB+193(9,@08),TESTTAB                          0566 02026000
         BNE   @RF00566                                            0566 02027000
         CLC   LOADTAB+209(9,@08),TESTTAB+9                        0566 02028000
         BNE   @RF00566                                            0566 02029000
         CLC   LOADTAB+226(8,@08),TESTTAB+18                       0566 02030000
         BNE   @RF00566                                            0566 02031000
         CLC   LOADTAB+240(10,@08),TESTTAB+26                      0566 02032000
         BNE   @RF00566                                            0566 02033000
*                       DO;         /* TABLE                         */ 02034000
*                                                                  0567 02035000
*                         /*******************************************/ 02036000
*                         /*                                         */ 02037000
*                         /* NOW VALIDITY CHECK THE FIRST TABLE      */ 02038000
*                         /*                                         */ 02039000
*                         /*******************************************/ 02040000
*                                                                  0568 02041000
*                         IF LOADTAB2('C2'X:'CA'X)=TESTTAB(1:9)&   0568 02042000
*                             LOADTAB2('D2'X:'DA'X)=TESTTAB(10:18)&     02043000
*                             LOADTAB2('E3'X:'EA'X)=TESTTAB(19:26)&     02044000
*                             LOADTAB2('F1'X:'FA'X)=TESTTAB(27:LENGTH(  02045000
*                             TESTTAB)) THEN                       0568 02046000
         CLC   LOADTAB2+193(9,@12),TESTTAB                         0568 02047000
         BNE   @RF00568                                            0568 02048000
         CLC   LOADTAB2+209(9,@12),TESTTAB+9                       0568 02049000
         BNE   @RF00568                                            0568 02050000
         CLC   LOADTAB2+226(8,@12),TESTTAB+18                      0568 02051000
         BNE   @RF00568                                            0568 02052000
         CLC   LOADTAB2+240(10,@12),TESTTAB+26                     0568 02053000
         BNE   @RF00568                                            0568 02054000
*                           COOL=YES;/* THIS IS A GOOD TABLE VS43301 */ 02055000
         OI    COOL,B'10000000'                                    0569 02056000
*                         ELSE                                     0570 02057000
*                           TCHARERR=YES;/* ERROR FOUND IN TABLE     */ 02058000
         B     @RC00568                                            0570 02059000
@RF00568 OI    TCHARERR,B'10000000'                                0570 02060000
*                       END;                                       0571 02061000
*                     ELSE                                         0572 02062000
*                       TCHARERR=YES;/* ERROR FOUND IN TABLE         */ 02063000
         B     @RC00566                                            0572 02064000
@RF00566 OI    TCHARERR,B'10000000'                                0572 02065000
*                   END;                                           0573 02066000
*               END;                                               0574 02067000
*             ELSE                                                 0575 02068000
*               LLEPTR=LLECHN;      /* PICK UP NEXT LL ELEMENT       */ 02069000
         B     @RC00559                                            0575 02070000
@RF00559 L     LLEPTR,LLECHN(,LLEPTR)                              0575 02071000
*           END;                                                   0576 02072000
@RC00559 DS    0H                                                  0576 02073000
@DE00549 LTR   LLEPTR,LLEPTR                                       0576 02074000
         BZ    @DC00549                                            0576 02075000
         TM    MATCH,B'10000000'                                   0576 02076000
         BZ    @DL00549                                            0576 02077000
@DC00549 DS    0H                                                  0577 02078000
*           IF COOL=NO              /* BAD TABLE ?                   */ 02079000
*             THEN                                                 0577 02080000
         TM    COOL,B'10000000'                                    0577 02081000
         BNZ   @RF00577                                            0577 02082000
*             DO;                                                  0578 02083000
*               DO;                 /* DELETE EP(TRANAME)            */ 02084000
*                 RESPECIFY                                        0580 02085000
*                  (GPR00P) RESTRICTED;                            0580 02086000
*                 GPR00P=ADDR(TRANAME);/* ->ENTRY NAME               */ 02087000
         LA    GPR00P,TRANAME                                      0581 02088000
*                 SVC(9);           /* DELETE SVC                    */ 02089000
         SVC   9                                                   0582 02090000
*                 RESPECIFY                                        0583 02091000
*                  (GPR00P) UNRESTRICTED;                          0583 02092000
*               END;                /* DELETE EP(TRANAME) YES, DELETE   02093000
*                                      IT AND PUT MSG                */ 02094000
*               IF TCHARERR=YES     /* CHAR ERROR ?                  */ 02095000
*                 THEN              /* YES                           */ 02096000
         TM    TCHARERR,B'10000000'                                0585 02097000
         BNO   @RF00585                                            0585 02098000
*                 STATUS=BADCHAR;   /* BAD CHAR IN TABLE MSG         */ 02099000
         L     @12,BADCHAR                                         0586 02100000
         ST    @12,STATUS                                          0586 02101000
*               IF TSIZEERR=YES     /* SIZE INCORRECT ?              */ 02102000
*                 THEN              /* YES                           */ 02103000
@RF00585 TM    TSIZEERR,B'10000000'                                0587 02104000
         BNO   @RF00587                                            0587 02105000
*                 STATUS=BADSIZE;   /* TABLE SIZE TOO SMALL MESSAGE  */ 02106000
         L     @12,BADSIZE                                         0588 02107000
         ST    @12,STATUS                                          0588 02108000
*             END;                                                 0589 02109000
@RF00587 DS    0H                                                  0590 02110000
*         END;                                                     0590 02111000
@RF00577 DS    0H                                                  0591 02112000
*       IF STATUS=CONTINUE&         /* NO ERRORS                     */ 02113000
*           ESTA806=NO              /* NO 806 ON TABLE LOADING       */ 02114000
*         THEN                                                     0591 02115000
@RF00542 L     @12,STATUS                                          0591 02116000
         LTR   @12,@12                                             0591 02117000
         BNZ   @RF00591                                            0591 02118000
         LA    @12,$ESTAPRM                                        0591 02119000
         TM    ESTA806(@12),B'10000000'                            0591 02120000
         BNZ   @RF00591                                            0591 02121000
*         DO;                                                      0592 02122000
*           RFY                                                    0593 02123000
*            (GPR02P,                                              0593 02124000
*             GPR15P) RSTD;                                        0593 02125000
*           GPR02P=STTTABP;         /* POINTER TO TABLE              */ 02126000
         L     GPR02P,STTTABP                                      0594 02127000
*           GEN SETS(STTRANL)       /* PASS TABLE TO VTAM            */ 02128000
*               (STTRAN TAB=(2),NAME=TRANAME,MF=(E,STTRANL));      0595 02129000
         STTRAN TAB=(2),NAME=TRANAME,MF=(E,STTRANL)                     02130000
*           RETCODE=GPR15P;         /* STTRAN RETURN CODE            */ 02131000
         ST    GPR15P,RETCODE                                      0596 02132000
*           RFY                                                    0597 02133000
*            (GPR02P,                                              0597 02134000
*             GPR15P) UNRSTD;                                      0597 02135000
*           IF RETCODE^=ZERO        /* ERROR IN STTRAN               */ 02136000
*             THEN                                                 0598 02137000
         L     @12,RETCODE                                         0598 02138000
         LTR   @12,@12                                             0598 02139000
         BZ    @RF00598                                            0598 02140000
*             STATUS=ERRSTTRN;      /* INDICATE ERROR IN STTRAN      */ 02141000
         L     @12,ERRSTTRN                                        0599 02142000
         ST    @12,STATUS                                          0599 02143000
*           DO;                     /* DELETE EP(TRANAME)            */ 02144000
@RF00598 DS    0H                                                  0601 02145000
*             RESPECIFY                                            0601 02146000
*              (GPR00P) RESTRICTED;                                0601 02147000
*             GPR00P=ADDR(TRANAME); /* ->ENTRY NAME                  */ 02148000
         LA    GPR00P,TRANAME                                      0602 02149000
*             SVC(9);               /* DELETE SVC                    */ 02150000
         SVC   9                                                   0603 02151000
*             RESPECIFY                                            0604 02152000
*              (GPR00P) UNRESTRICTED;                              0604 02153000
*           END;                    /* DELETE EP(TRANAME)            */ 02154000
*         END;                                                     0606 02155000
*       ELSE                                                       0607 02156000
*         IF ESTA806=YES            /* 806 ON TABLE                  */ 02157000
*           THEN                                                   0607 02158000
         B     @RC00591                                            0607 02159000
@RF00591 LA    @12,$ESTAPRM                                        0607 02160000
         TM    ESTA806(@12),B'10000000'                            0607 02161000
         BNO   @RF00607                                            0607 02162000
*           RETCODE=FOUR;           /* REPROMPT                      */ 02163000
         MVC   RETCODE(4),@CF00049                                 0608 02164000
*       GEN(ESTAE 0);               /* CANCEL ESTAE                  */ 02165000
@RF00607 DS    0H                                                  0609 02166000
@RC00591 DS    0H                                                  0609 02167000
         ESTAE 0                                                        02168000
*     END;                                                         0610 02169000
*   ELSE                                                           0611 02170000
*     STATUS=IGNTRAN;               /* IGNORE TRAN KEYWORD FOR TCAM  */ 02171000
         B     @RC00534                                            0611 02172000
@RF00534 L     @12,IGNTRAN                                         0611 02173000
         ST    @12,STATUS                                          0611 02174000
*   IF STATUS^=CONTINUE             /* ESTAE ERROR                   */ 02175000
*     THEN                                                         0612 02176000
@RC00534 L     @12,STATUS                                          0612 02177000
         LTR   @12,@12                                             0612 02178000
         BZ    @RF00612                                            0612 02179000
*     DO;                                                          0613 02180000
*       CALL HANDLERR;              /* GO PUT OUT MSG                */ 02181000
         BAL   @14,HANDLERR                                        0614 02182000
*       IF STATUS=BADSIZE|          /* WAS TRAN TABLE TOO SMALL      */ 02183000
*           STATUS=BADCHAR          /* OR HAVE BAD CHARACTERS        */ 02184000
*         THEN                                                     0615 02185000
         L     @12,STATUS                                          0615 02186000
         C     @12,BADSIZE                                         0615 02187000
         BE    @RT00615                                            0615 02188000
         C     @12,BADCHAR                                         0615 02189000
         BNE   @RF00615                                            0615 02190000
@RT00615 DS    0H                                                  0616 02191000
*         DO;                                                      0616 02192000
*           STATUS=CONTINUE;        /* LET THE USER HAVE ANOTHER TRY */ 02193000
         SLR   @12,@12                                             0617 02194000
         ST    @12,STATUS                                          0617 02195000
*           RETCODE=FOUR;           /* TELL PARSE TO REPROMPT        */ 02196000
         MVC   RETCODE(4),@CF00049                                 0618 02197000
*         END;                                                     0619 02198000
*       ELSE                        /* CHECK FOR ANOTHER PROBLEM     */ 02199000
*         IF RETCODE^=0 THEN        /* NON ZERO FROM STTRAN          */ 02200000
         B     @RC00615                                            0620 02201000
@RF00615 SLR   @12,@12                                             0620 02202000
         C     @12,RETCODE                                         0620 02203000
         BE    @RF00620                                            0620 02204000
*           RETCODE=0;              /* ZERO TO AVOID REPROMPT        */ 02205000
         ST    @12,RETCODE                                         0621 02206000
*     END;                                                         0622 02207000
@RF00620 DS    0H                                                  0622 02208000
@RC00615 DS    0H                                                  0623 02209000
*   RFY                                                            0623 02210000
*    (GPR13P) RSTD;                                                0623 02211000
@RF00612 DS    0H                                                  0624 02212000
*   GPR13P=GPR13P->UWAB13;          /* GET PTR TO PARSE SA           */ 02213000
         L     GPR13P,UWAB13(,GPR13P)                              0624 02214000
*   RFY                                                            0625 02215000
*    (GPR13P) UNRSTD;                                              0625 02216000
*   RETURN CODE(RETCODE);           /* RETURN TO PARSE               */ 02217000
         L     @15,RETCODE                                         0626 02218000
         L     @14,12(,@13)                                        0626 02219000
         LM    @00,@12,20(@13)                                     0626 02220000
         BR    @14                                                 0626 02221000
*   END CHKTRAN;                                                   0627 02222000
*                                                                  0628 02223000
*/********************************************************************/ 02224000
*/*                                                                  */ 02225000
*/*SEGMENT-                                                          */ 02226000
*/*          LOAD                                                    */ 02227000
*/*                                                                  */ 02228000
*/*FUNCTION-                                                         */ 02229000
*/*          LOAD THE TRANSLATE TABLE                                */ 02230000
*/*                                                                  */ 02231000
*/*OPERATION-                                                        */ 02232000
*/*          THIS SEGMENT IS THE RETRY POINT IF THERE IS AN 806      */ 02233000
*/*          ABEND TRYING TO LOAD THE TRANSLATE TABLE.IF WE DO NOT   */ 02234000
*/*          HAVE AN 806 ABEND IN PROGRESS AT THIS POINT THEN WE     */ 02235000
*/*          WILL LOAD THE TABLE AND SAVE ITS ADDRESS                */ 02236000
*/*                                                                  */ 02237000
*/*CHANGE ACTIVITY-                                                  */ 02238000
*/*      @Z40DQKC-                                                   */ 02239000
