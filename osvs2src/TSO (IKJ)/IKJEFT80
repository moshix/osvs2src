         BNZ   @RF00632                                            0632 02265000
*     DO;                                                          0633 02266000
*       STTTABP=ZERO;               /* NO TABLE YET                  */ 02267000
         ST    @08,STTTABP                                         0634 02268000
*       DO;                         /* LOAD EP(TRANAME)              */ 02269000
*         RESPECIFY                                                0636 02270000
*          (GPR00P,                                                0636 02271000
*           GPR01P) RESTRICTED;                                    0636 02272000
*         GPR00P=ADDR(TRANAME);     /* LOAD ENTRY POINT NAME ADDRESS */ 02273000
         LA    GPR00P,TRANAME                                      0637 02274000
*         GPR01P=0;                 /* JOBLIB OR LINKLIB DCB         */ 02275000
         SLR   GPR01P,GPR01P                                       0638 02276000
*         SVC(8);                   /* ISSUE LOAD SVC                */ 02277000
         SVC   8                                                   0639 02278000
*         RESPECIFY                                                0640 02279000
*          (GPR00P,                                                0640 02280000
*           GPR01P) UNRESTRICTED;                                  0640 02281000
*       END;                        /* LOAD EP(TRANAME)              */ 02282000
*       RFY                                                        0642 02283000
*        (GPR00P) RSTD;                                            0642 02284000
*       STTTABP=GPR00P;             /* ADDRESS OF THE TABLE          */ 02285000
         ST    GPR00P,STTTABP                                      0643 02286000
*       RFY                                                        0644 02287000
*        (GPR00P) UNRSTD;                                          0644 02288000
*     END;                                                         0645 02289000
*   END LOAD;                                                      0646 02290000
@EL00014 DS    0H                                                  0646 02291000
@EF00014 DS    0H                                                  0646 02292000
