         STM   @00,@12,20(@13)                                     0518 01470000
*     DECLARE                                                      0519 01471000
*        DELLINE CHAR(4) BASED(CHARPTR);/* LINE DELETE CHARACTER     */ 01472000
*        RFY (R3)RSTD;         /* REGISTER 3 IS TO BE USED Z40DQJD  */  01473000
*        /* ESTABLISH ADDRESSABILITY IN THIS ROUTINE                 */ 01474000
*        GENERATE;                                                 0521 01475000
              L   3,4(1)             GET PTR TO PROFILE'S SAVE AREA     01476000
        LM   11,12,64(3)       ESTABLISH ADDRESSABILITY                 01477000
        L    3,0(1)            GET PDE POINTER                          01478000
*        PDEADDR=R3;            /* INITIALIZE PDE PTR                */ 01479000
         ST    R3,PDEADDR                                          0522 01480000
*        RFY (R3)UNRSTD;                  /*                Z40DQJD*/   01481000
*        IF CHARLEN=1           /* LENGTH OF CHARACTER = 1           */ 01482000
*          THEN                 /* CHARACTER IS VALID UNLESS IS 'NL' */ 01483000
         L     @10,PDEADDR                                         0524 01484000
         CLC   CHARLEN(2,@10),@CH00054                             0524 01485000
         BNE   @RF00524                                            0524 01486000
*            DO;                                                   0525 01487000
*              IF DELLINE(1)='15'X  /* 'NL' SPECIFIED                */ 01488000
*                THEN                                              0526 01489000
