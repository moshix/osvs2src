*        /************************************************************/ 00089000
*        GENERATE(CALLTSSR EP=IKJPARS);/* LINK TO IKJPARS            */ 00090000
         CALLTSSR EP=IKJPARS                                            00091000
*        RCODE=R15;             /* GET THE RETURN CODE FROM PARSE    */ 00092000
         ST    R15,RCODE                                           0096 00093000
*        IF R15^=0              /* CHECK FOR SUCCESS                 */ 00094000
*          THEN                 /* PARSE WAS UNSUCCESSFUL            */ 00095000
         LTR   R15,R15                                             0097 00096000
         BZ    @RF00097                                            0097 00097000
*            /********************************************************/ 00098000
*            /* SET UP MULTI-LEVEL MESSAGE LIST AND CONVERT CODE     */ 00099000
*            /********************************************************/ 00100000
*            DO;                                                   0098 00101000
*FIBMESS:                       /*                             Y01676*/ 00102000
*              IF FIBBIT = '1'B THEN/* FROM SVC 100?           Y01676*/ 00103000
FIBMESS  TM    FIBBIT,B'10000000'                                  0099 00104000
         BO    @RT00099                                            0099 00105000
*                GOTO FIBMESS2;     /* YES-ISSUE DIFFERENT MSG Y01676*/ 00106000
*              IF RCODE=4                                          0101 00107000
*                THEN                                              0101 00108000
         L     @10,RCODE                                           0101 00109000
         CH    @10,@CH00045                                        0101 00110000
         BE    @RT00101                                            0101 00111000
*                  GO TO RET12; /* RETURN-UNSUCCESSFUL RETURN CODE=12*/ 00112000
*              RFY (R5)RSTD;   /*                            Z40DQJD*/  00113000
*              R5=RCODE;        /*PREPARE FOR CVD              M05017*/ 00114000
         LR    R5,@10                                              0104 00115000
*              GEN;             /*                             M05017*/ 00116000
              CVD   R5,PACKDEC   CONVERT RETURN CODE TO DEC   M05017    00117000
              UNPK  CMESSAG(8),PACKDEC(8)  UNPACK CODE        M05017    00118000
              OI    CMESSAG+7,X'F0'  FIX SIGN FOR PRINTING    M05017    00119000
*              RFY (R5)UNRSTD;  /*                            Z40DQJD*/ 00120000
