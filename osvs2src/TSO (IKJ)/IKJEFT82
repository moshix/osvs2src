*              GO TO RET12;     /* GO TO RETURN TO TMP-RETURN CODE=12*/ 00169000
         B     RET12                                               0123 00170000
*            END;               /* END CHEK FOR UNSUCCESSFUL PARSE   */ 00171000
*        /************************************************************/ 00172000
*        /* CHECK THE PARAMETER DESCRIPTOR LIST FROM PARSE           */ 00173000
*        /* SERVICE ROUTINE. IF NO VALID OPERANDS HAVE BEEN          */ 00174000
*        /* ENTERED, ISSUE WARNING MESSAGE.                          */ 00175000
*        /************************************************************/ 00176000
*        IF PRMT=0 &           /*IF NO VALID OPERANDS WERE     Y01156*/ 00177000
*           MSGID=0 &          /*SPECIFIED ON PROFILE COMMAND  Y01156*/ 00178000
*           INTERCOM=0 &                                     /*Y01156*/ 00179000
*           PAUSE=0 &                                        /*Y01156*/ 00180000
*           CHART=0 &                                        /*Y01156*/ 00181000
