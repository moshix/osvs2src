         TITLE 'IKJEFT82 -- PROFILE COMMAND PROCESSOR                  *00001000
                        '                                               00002000
IKJEFT82 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKJEFT82  78.177'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @12,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@12                                         0001 00014000
         L     @00,@SIZDATD                                        0001 00015000
         GETMAIN  R,LV=(0)                                              00016000
         LR    @11,@01                                             0001 00017000
         USING @DATD,@11                                           0001 00018000
         ST    @13,@SA00001+4                                      0001 00019000
         LM    @00,@01,20(@13)                                     0001 00020000
         ST    @11,8(,@13)                                         0001 00021000
         LR    @13,@11                                             0001 00022000
         XC    @ZTEMPS(@ZLEN),@ZTEMPS                                   00023000
         MVC   @PC00001(4),0(@01)                                  0001 00024000
JSCBDISP EQU 188                                                        00025000
JSCBAOS1 EQU  72                                                        00026000
JSCBAOS2 EQU 192                                                        00027000
*        CPPLPTR=R1;            /*INTIALIZE COMMAND PARAMETER POINTER*/ 00028000
         ST    R1,CPPLPTR                                          0075 00029000
*        CHARLNMG='00'X;        /*INIT 'CHAR-LINE INVALID' MSG Y01156*/ 00030000
         MVI   CHARLNMG,X'00'                                      0076 00031000
*                               /* TO NO ERRORS FOUND          Y01156*/ 00032000
*        RFY (R3,R6)RSTD;      /* REGS 3 & 6 USED BY THIS ROUTINE  0077 00033000
*                                                            Z40DQJD*/  00034000
*        /************************************************************/ 00035000
*        /* INITIALIZE POINTERS TO BE USED BY SERVICE ROUTINES       */ 00036000
*        /************************************************************/ 00037000
*        ECTPTR=CPPLECT;        /* INITIALIZE PARSE ECT POINTER      */ 00038000
         L     @10,CPPLPTR                                         0078 00039000
         L     @07,CPPLECT(,@10)                                   0078 00040000
         ST    @07,ECTPTR                                          0078 00041000
*        UPTPTR=CPPLUPT;        /* INITIALIZE PARSE UPT PTR          */ 00042000
