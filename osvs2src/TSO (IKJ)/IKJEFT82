         LH    @10,MSGID(,@10)                                     0135 00244000
         LTR   @10,@10                                             0135 00245000
         BZ    @RT00135                                            0135 00246000
*            GOTO CHKINT;       /* GO TO CHECK FOR NEXT OPERAND      */ 00247000
