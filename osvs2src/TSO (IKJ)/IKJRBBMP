@9FE     LA    @F,99                                              0150  02460000
         CH    @F,STATUS                                          0150  02470000
         BC    07,@9FC                                            0150  02480000
*      DO;                                                              02490000
*                                                                       02500000
* /*  THE FOLLOWING FOUR CODE SECTIONS CONTROL THE BMP'S PROCESSING   * 02510000
* /*  OF EACH INPUT LINE.  THIS LOOP IS EXECUTED UNTIL END-OF-FILE    * 02520000
* /*  IS ENCOUNTERED OR AN ERROR CONDITION OCCURS.                    * 02530000
*                                                                       02540000
*                                                                       02550000
* GETALINE:                                                             02560000
*                                                                       02570000
* /*  THE FOLLOWING SECTION GETS A LINE FROM THE SYSIN DATA SET       * 02580000
* /*  AND ANALYZES THE INDICATORS PASSED BACK FROM THE PUTGET         * 02590000
* /*  SERVICE ROUTINE.                                                * 02600000
*                                                                       02610000
*        GEN (PUTGET PARM=PGPBA,MF=(E,IOPLA)); /* GET A COMMAND FROM    02620000
*                                       SYSIN                         * 02630000
GETALINE PUTGET PARM=PGPBA,MF=(E,IOPLA)                                 02640000
         DS    0H                                                       02650000
*        IF RTNREG=4 THEN            /* IF A COMMAND IS OBTAINED      * 02660000
         CH    @F,@D1                                             0153  02670000
         BC    07,@9FB                                            0153  02680000
*          DO;                       /* THEN                          * 02690000
*            IF SYSINEOF='1'B THEN   /* IF AN 'END' IS SIMULATED      * 02700000
         TM    BSTAT,B'00010000'                                  0155  02710000
         BC    12,@9FA                                            0155  02720000
*              DO;                   /* THEN                          * 02730000
*                IF ECTPCMD(1)='00'X THEN /* IF NO CMD WAS PROCESSED  * 02740000
         L     @8,ECTPTR                                          0157  02750000
         CLI   12(@8),X'00'                                       0157  02760000
         BC    07,@9F9                                            0157  02770000
*                  DO;               /* PREVIOUSLY THEN               * 02780000
*                    INDICATR=6;     /* SET NO INPUT TO BE PROCESSED    02790000
*                                       INDICATOR                     * 02800000
         LA    @F,6                                               0159  02810000
         STH   @F,STATUS                                          0159  02820000
         BC    15,@9F8                                            0161  02830000
*                  END;                                                 02840000
*                ELSE                                                   02850000
*                  INDICATR=11;      /* OTHERWISE SET NORMAL INDICTR  * 02860000
@9F9     LA    @F,11                                              0161  02870000
         STH   @F,STATUS                                          0161  02880000
*              END;                                                     02890000
*          END;                                                         02900000
         BC    15,@9F7                                            0164  02910000
*        ELSE                                                           02920000
*          DO;                                                          02930000
*            ERRORCDE=RTNREG;        /* SAVE PUTGET RETURN CODE       * 02940000
@9FB     STH   @F,STATUS+2                                        0165  02950000
*            INDICATR=2;             /* SET PUTGET ERROR INDICATOR    * 02960000
         LA    @F,2                                               0166  02970000
         STH   @F,STATUS                                          0166  02980000
*          END;                                                         02990000
*        IF INDICATR=99 THEN         /* IF A LINE OBTAINED SUCCESSFUL * 03000000
@9F7     LA    @F,99                                              0168  03010000
         CH    @F,STATUS                                          0168  03020000
         BC    07,@9F6                                            0168  03030000
*          DO;                       /* THEN                          * 03040000
*                                                                       03050000
* /*  THE FOLLOWING CODE SECTION RESULTS IN THE INPUT LINE BEING      * 03060000
* /*  SCANNED FOR A VALID COMMAND.  ONLY IF THE COMMAND IS            * 03070000
* /*  SYNTACTICALLY VALID AND IS EITHER 'ACCOUNT' OR 'UADSREFM'       * 03080000
* /*  WILL AN ATTACH BE DONE.                                         * 03090000
*                                                                       03100000
*            CSPLCBUF=PGPBIBUF;      /* SET COMMAND BUFFER IN CSPL    * 03110000
         L     @8,PGPBPTR                                         0170  03120000
         L     @9,CSPLPTR                                         0170  03130000
         MVC   20(4,@9),12(@8)                                    0170  03140000
*            R1=ADDR (CSPL);         /* SET LINK REGISTER             * 03150000
         LR    @1,@9                                              0171  03160000
*            GEN ( LINK EP=IKJSCAN); /* LINK TO COMMAND SCAN          * 03170000
          LINK EP=IKJSCAN                                               03180000
         DS    0H                                                       03190000
*            IF RTNREG^=0 THEN       /* IF CMD SCAN FOUND ERROR       * 03200000
         LTR   @F,@F                                              0173  03210000
         BC    08,@9F5                                            0173  03220000
*              DO;                   /* THEN                          * 03230000
*                ERRORCDE=RTNREG;    /* SAVE CMD SCAN RETURN CODE     * 03240000
         STH   @F,STATUS+2                                        0175  03250000
*                INDICATR=4;         /* SET SCAN ERROR INDICATOR      * 03260000
         LA    @F,4                                               0176  03270000
         STH   @F,STATUS                                          0176  03280000
         BC    15,@9F4                                            0178  03290000
*              END;                                                     03300000
*            ELSE                                                       03310000
*              IF CSOANOC='1'B THEN  /* IF NULL LINE                  * 03320000
@9F5     L     @8,CSOAPTR                                         0178  03330000
