@9CF     TM    BSTAT,B'00001000'                                  0309  07860000
         BC    01,@9CD                                            0309  07870000
*              DO;                   /* PUT OUT ERROR MESSAGE         * 07880000
*                INDICATR=9;         /* INDICATE IS UTILITY ABEND     * 07890000
