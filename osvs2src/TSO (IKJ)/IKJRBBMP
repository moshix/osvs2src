*                      ELSE                                             03830000
*                        INDICATR=13; /* SET CMD NAME ERROR INDICATOR * 03840000
@9ED     EQU   *                                                  0189  03850000
@9EE     LA    @F,13                                              0189  03860000
         STH   @F,STATUS                                          0189  03870000
*                  END;                                                 03880000
         BC    15,@9EB                                            0191  03890000
*                ELSE                                                   03900000
*                  INDICATR=12;      /* SET CMDNAME SYNTAX ERROR      * 03910000
@9F2     LA    @F,12                                              0191  03920000
         STH   @F,STATUS                                          0191  03930000
*            IF INDICATR=99 THEN     /* IF VALID COMMAND NAME FOUND   * 03940000
@9EB     EQU   *                                                  0192  03950000
@9F4     LA    @F,99                                              0192  03960000
         CH    @F,STATUS                                          0192  03970000
         BC    07,@9EA                                            0192  03980000
*              DO;                                                      03990000
*                                                                       04000000
* /*  THE FOLLOWING CODE SECTION RESULTS IN AN ATTACH OF THE          * 04010000
* /*  COMMAND PROCESSOR SPECIFIED ON THE INPUT LINE.  WHEN            * 04020000
* /*  THE CP'S PROCESSING IS COMPLETED, THE CP IS DETACHED.           * 04030000
*                                                                       04040000
*                CPPLCBUF=PGPBIBUF;  /* SET COMMAND BUFFER IN CPPL    * 04050000
         L     @8,PGPBPTR                                         0194  04060000
         L     @9,CPPLPTR                                         0194  04070000
         MVC   0(4,@9),12(@8)                                     0194  04080000
*                ATTECB=ATTECB&&ATTECB; /* INITIALIZE ATTACH ECB      * 04090000
         XC    ATTECB(4),ATTECB                                   0195  04100000
*                RESTRICT (R2,R3);   /* RESTRICT R2 R3 FOR ATTACH     * 04110000
*                R1=ADDR (CPPL);     /* SET UP REGISTERS FOR ATTACH   * 04120000
         LR    @1,@9                                              0197  04130000
*                R2=ADDR (ECTPCMD);                                     04140000
         L     @6,ECTPTR                                          0198  04150000
         LA    @2,12(0,@6)                                        0198  04160000
*                R3=ADDR (ATTECB);                                      04170000
         LA    @3,ATTECB                                          0199  04180000
*                R15=ADDR (ATLIST);                                     04190000
         LA    @F,ATLIST                                          0200  04200000
*                GEN                                                    04210000
* (ATTACH EPLOC=(R2),ECB=(R3),MF=(E,(1)),SF=(E,(15)));                  04220000
         ATTACH EPLOC=(R2),ECB=(R3),MF=(E,(1)),SF=(E,(15))              04230000
         DS    0H                                                       04240000
*                RELEASE (R2,R3);                                       04250000
*                ATTATCB=R1;         /* SAVE ATTACH TCB ADDRESS       * 04260000
         ST    @1,ATTATCB                                         0203  04270000
*                IF RTNREG^=0 THEN   /* IF ATTACH FAILED              * 04280000
         LTR   @F,@F                                              0204  04290000
         BC    08,@9E9                                            0204  04300000
*                  DO;               /* THEN                          * 04310000
*                    ERRORCDE=RTNREG; /* SAVE ATTACH ERROR CODE       * 04320000
         STH   @F,STATUS+2                                        0206  04330000
*                    INDICATR=3;     /* SET ATTACH ERROR INDICATOR    * 04340000
         LA    @F,3                                               0207  04350000
         STH   @F,STATUS                                          0207  04360000
         BC    15,@9E8                                            0209  04370000
*                  END;                                                 04380000
*                ELSE                /* COMMAND ATTACHED              * 04390000
*                  DO;                                                  04400000
@9E9     EQU   *                                                  0209  04410000
*                    GEN (WAIT ECB=ATTECB); /* WAIT COMMAND PROCESSOR   04420000
*                                       TO RETURN                     * 04430000
         WAIT ECB=ATTECB                                                04440000
         DS    0H                                                       04450000
*                    IF ATTCODE^=0 THEN /* IF COMMAND FOUND AN ERROR  * 04460000
         SR    @F,@F                                              0211  04470000
         CH    @F,ATTECB+2                                        0211  04480000
         BC    08,@9E7                                            0211  04490000
*                      DO;           /* THEN                          * 04500000
*                                                                       04510000
* /*  THE FOLLOWING SEGMENT SETS A SPECIFIC ERROR INDICATOR SO THAT   * 04520000
* /*  THE PROPER MESSAGE CAN BE WRITTEN AND RETURN CODE SET.          * 04530000
*                                                                       04540000
*                        IF BADINDCB='1'B THEN /* IF INPUT DCB INVALID* 04550000
         TM    BSTAT,B'01000000'                                  0213  04560000
         BC    12,@9E6                                            0213  04570000
*                          INDICATR=8; /* THEN SET INPUT DCB INDICATOR* 04580000
         LA    @F,8                                               0214  04590000
         STH   @F,STATUS                                          0214  04600000
         BC    15,@9E5                                            0215  04610000
*                        ELSE                                           04620000
*                          IF SYSINERR='1'B THEN /* IF INPUT I/O ERROR* 04630000
@9E6     TM    BSTAT,B'00100000'                                  0215  04640000
         BC    12,@9E4                                            0215  04650000
*                            INDICATR=7; /* THEN SET INPUT ERROR        04660000
*                                       INDICATR                      * 04670000
         LA    @F,7                                               0216  04680000
         STH   @F,STATUS                                          0216  04690000
         BC    15,@9E3                                            0217  04700000
*                          ELSE                                         04710000
*                            IF SYSINBYD='1'B THEN /* IF INPUT READ     04720000
*                                       BEYOND EOF                    * 04730000
@9E4     TM    BSTAT,B'00000100'                                  0217  04740000
         BC    12,@9E2                                            0217  04750000
*                              INDICATR=10; /* THEN SET INPUT EOF       04760000
*                                       INDICATOR                     * 04770000
         LA    @F,10                                              0218  04780000
         STH   @F,STATUS                                          0218  04790000
         BC    15,@9E1                                            0219  04800000
*                            ELSE                                       04810000
*                              IF SUBABEND='1'B THEN /* IF SUBTASK OF   04820000
*                                       COMMAND ABEND                 * 04830000
@9E2     TM    BSTAT,B'00000010'                                  0219  04840000
         BC    12,@9E0                                            0219  04850000
*                                INDICATR=9; /* SET SYSABEND INDICATOR* 04860000
         LA    @F,9                                               0220  04870000
         STH   @F,STATUS                                          0220  04880000
         BC    15,@9DF                                            0221  04890000
*                              ELSE                                     04900000
*                                IF SYSPTERR='1'B THEN /* IF SYSPRINT   04910000
*                                       ERROR                         * 04920000
@9E0     TM    BSTAT,B'00001000'                                  0221  04930000
         BC    12,@9DE                                            0221  04940000
*                                  INDICATR=0; /* SET SYSPRINT ERROR    04950000
*                                       INDICATOR                     * 04960000
         SR    @F,@F                                              0222  04970000
         STH   @F,STATUS                                          0222  04980000
         BC    15,@9DD                                            0223  04990000
*                                ELSE                                   05000000
*                                  IF ECTRCDF='80'X THEN /* IF ACCOUNT  05010000
*                                       ABENDED                       * 05020000
@9DE     L     @8,ECTPTR                                          0223  05030000
         CLI   0(@8),X'80'                                        0223  05040000
         BC    07,@9DC                                            0223  05050000
*                                    INDICATR=9; /* SET SYSABEND        05060000
*                                       INDICATOR                     * 05070000
         LA    @F,9                                               0224  05080000
         STH   @F,STATUS                                          0224  05090000
         BC    15,@9DB                                            0225  05100000
*                                  ELSE                                 05110000
*                                    INDICATR=5; /* OTHERWISE SET       05120000
*                                       INPUT STREAM FLUSHED INDICATOR* 05130000
@9DC     LA    @F,5                                               0225  05140000
         STH   @F,STATUS                                          0225  05150000
*                      END;                                             05160000
@9DB     EQU   *                                                  0226  05170000
@9DD     EQU   *                                                  0226  05180000
@9DF     EQU   *                                                  0226  05190000
@9E1     EQU   *                                                  0226  05200000
@9E3     EQU   *                                                  0226  05210000
@9E5     EQU   *                                                  0226  05220000
*                    R1=ADDR (ATTATCB); /* SET TCB POINTER FOR DETACH * 05230000
@9E7     LA    @1,ATTATCB                                         0227  05240000
*                    GEN (DETACH (1)); /* DETACH COMMAND PROCESSOR    * 05250000
         DETACH (1)                                                     05260000
         DS    0H                                                       05270000
*                  END;                                                 05280000
*                CBUFPTR=PGPBIBUF;   /* LOCATE COMMAND BUFFER         * 05290000
@9E8     L     @8,PGPBPTR                                         0230  05300000
         MVC   CBUFPTR(4),12(@8)                                  0230  05310000
*                R0=16777216+CBUFLNG; /* SET SUBPOOL NO & BUFFER LEN  * 05320000
         L     @9,CBUFPTR                                         0231  05330000
         LH    @F,0(0,@9)                                         0231  05340000
         A     @F,@D2                                             0231  05350000
         LR    @0,@F                                              0231  05360000
*                R1=CBUFPTR;                                            05370000
         L     @1,CBUFPTR                                         0232  05380000
*                GEN (FREEMAIN R,LV=(0),A=(1)); /* RELEASE COMMAND      05390000
*                                       BUFFER                        * 05400000
         FREEMAIN R,LV=(0),A=(1)                                        05410000
         DS    0H                                                       05420000
*                IF INDICATR=99 THEN /* IF COMMAND PROCESSED NORMALLY * 05430000
         LA    @F,99                                              0234  05440000
         CH    @F,STATUS                                          0234  05450000
         BC    07,@9DA                                            0234  05460000
*                  DO;               /* THEN                          * 05470000
*                    IF SYSINEOF^='1'B THEN /* IF EOF HAS NOT OCCURRED  05480000
*                                       ON SYSIN                      * 05490000
         TM    BSTAT,B'00010000'                                  0236  05500000
*                      GO TO GETALINE; /* THEN GO TO PROCESS NEXT CMD * 05510000
         BC    12,GETALINE                                        0237  05520000
*                    ELSE                                               05530000
*                      INDICATR=11;  /* ELSE INDICATE EOF             * 05540000
         LA    @F,11                                              0238  05550000
         STH   @F,STATUS                                          0238  05560000
*                  END;                                                 05570000
*              END;                                                     05580000
@9DA     EQU   *                                                  0240  05590000
*          END;                                                         05600000
@9EA     EQU   *                                                  0241  05610000
*      END;                                                             05620000
@9F6     EQU   *                                                  0242  05630000
*                                                                       05640000
* /*  THE FOLLOWING CODE SECTION PUTS OUT THE FINAL MESSAGE IN THE    * 05650000
* /*  BMP'S PROCESSING IF NO ERRORS HAVE OCCURRED WHILE WRITING TO    * 05660000
* /*  THE SYSPRINT DATA SET.                                          * 05670000
*                                                                       05680000
*                                                                       05690000
* EXIT:                                                                 05700000
*    IF INDICATR^=0 THEN             /* IF INDICATR INDICATES MESSAGE * 05710000
@9FC     EQU   *                                                  0243  05720000
EXIT     SR    @F,@F                                              0243  05730000
         CH    @F,STATUS                                          0243  05740000
         BC    08,@9D9                                            0243  05750000
*      DO;                           /* TO BE WRITTEN OUT THEN        * 05760000
*        MSGPTR=ADDR (MESSAGE);      /* SET MESSAGE POINTER           * 05770000
         L     @8,@V1              ADDRESS OF MESSAGE             0245  05780000
         ST    @8,MSGPTR                                          0245  05790000
*        MSGQ=MSGS (INDICATR)+ADDR(MESSAGE); /* CALCULATE MSG TXT ADDR* 05800000
