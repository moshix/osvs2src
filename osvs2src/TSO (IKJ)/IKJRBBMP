         L     @3,DCBPTR                                          0274  06640000
         TM    48(@3),B'00010000'                                 0274  06650000
         BC    12,@9D5                                            0274  06660000
*      DO;                                                              06670000
*        R1=ADDR (CLOSLIST);         /* SET ADDR OF LIST FORM         * 06680000
         LA    @1,CLOSLIST                                        0276  06690000
*        R2=ADDR (DCBG);             /* SET ADDR OF DCB               * 06700000
         LA    @2,DCBG                                            0277  06710000
*        GEN (CLOSE  ((2)),MF=(E,(1))); /* CLOSE INPUT DCB            * 06720000
         CLOSE  ((2)),MF=(E,(1))                                        06730000
         DS    0H                                                       06740000
*      END;                                                             06750000
*    RELEASE (R2);                                                      06760000
*    RESTRICT (RETREG);                                                 06770000
*    IF INDICATR=11 THEN             /* IF UTILITY NORMAL ENDED       * 06780000
@9D5     LA    @F,11                                              0282  06790000
         CH    @F,STATUS                                          0282  06800000
         BC    07,@9D4                                            0282  06810000
*      RETREG=0;                     /* THEN SET NORMAL RETURN CODE   * 06820000
         SR    @6,@6                                              0283  06830000
         BC    15,@9D3                                            0284  06840000
*    ELSE                                                               06850000
*      IF INDICATR=0 THEN            /* ELSE IF UTILITY ENDED WITH    * 06860000
@9D4     SR    @F,@F                                              0284  06870000
         CH    @F,STATUS                                          0284  06880000
         BC    07,@9D2                                            0284  06890000
*        RETREG=8;                   /* OUTPUT ERROR THEN SET MESSAGE   06900000
*                                       DATA SET ERROR RETURN CODE    * 06910000
         LA    @6,8                                               0285  06920000
         BC    15,@9D1                                            0286  06930000
*      ELSE                                                             06940000
*        RETREG=4;                   /* ELSE SET ERROR RETURN CODE    * 06950000
@9D2     LA    @6,4                                               0286  06960000
*    RETURN CODE(RETREG);            /* RETURN TO CALLER              * 06970000
@9D1     EQU   *                                                  0287  06980000
@9D3     L     @D,4(0,@D)                                         0287  06990000
         LR    @F,@6                                              0287  07000000
         ST    @F,16(0,@D)                                        0287  07010000
         LR    @1,@C                                              0287  07020000
         L     @0,@SIZ001                                         0287  07030000
         FREEMAIN R,LV=(0),A=(1)                                  0287  07040000
         LM    @E,@C,12(@D)                                       0287  07050000
         BCR   15,@E                                              0287  07060000
*    RELEASE (RETREG);                                                  07070000
*                                                                       07080000
* /*  THE FOLLOWING ARE THE LIST FORM OF MACRO INSTRUCTIONS USED      * 07090000
* /*  BY THE BMP.                                                     * 07100000
*                                                                       07110000
*    GENERATE;                                                          07120000
PTLIST   PUTLINE  OUTPUT=(0),MF=L              PUTLINE LIST             07130000
PGLIST   PUTGET   OUTPUT=(0,SINGLE,MODE),MF=L  PUTGET LIST              07140000
ATTLIST  ATTACH   SHSPV=78,SZERO=NO,SF=L       ATTACH LIST              07150000
ATTEND   EQU     *                                                      07160000
ATTLEN   DC AL2(ATTEND-ATTLIST)                                         07170000
STALIST  STAE     MF=L                         STAE LIST                07180000
STAEND   EQU   *                                                        07190000
STALEN   DC   AL2(STAEND-STALIST)                                       07200000
CLOSELST CLOSE  (,),MF=L                       CLOSE LIST               07210000
CLOSEND  EQU    *                                                       07220000
CLOSELEN DC     AL2(CLOSEND-CLOSELST)                                   07230000
         DS    0H                                                       07240000
*                                                                       07250000
* /*  THE FOLLOWING STAE ROUTINE IS GIVEN CONTROL WHEN A TASK ON      * 07260000
* /*  THE BMP-LEVEL ABENDS.  IF THE ABEND IS A 'B01' USER CODE WHICH  * 07270000
* /*  IS ISSUED BY THE I/O SERVICE ROUTINES, A RETRY IS SCHEDULED.    * 07280000
* /*  IF ANY OTHER ERROR OCCURS, THE MESSAGE 'UTILITY ABENDED'        * 07290000
* /*  IS WRITTEN AND NO RETRY IS ATTEMPTED.                           * 07300000
*                                                                       07310000
*                                                                       07320000
* IKJRBBMS:                                                             07330000
*    PROC (DUMBPARM) OPTIONS(NOSAVEAREA,DONTSAVE);                      07340000
@EL01    L     @D,4(0,@D)                                         0290  07350000
         LR    @1,@C                                              0290  07360000
         L     @0,@SIZ001                                         0290  07370000
         FREEMAIN R,LV=(0),A=(1)                                  0290  07380000
         LM    @E,@C,12(@D)                                       0290  07390000
         BCR   15,@E                                              0290  07400000
IKJRBBMS EQU   *                                                  0290  07410000
*    RESTRICT (R2,STAEWPTR,STAEPTR);                                    07420000
*    GENERATE;                                                          07430000
         DROP  @B,@C                 DROP BASE REGISTERS                07440000
         LR    @B,@F                 PUT ENTRY PT INTO REG 11           07450000
         USING IKJRBBMS,@B           SET ADDRESS FOR STAE EXIT          07460000
         DS    0H                                                       07470000
*    R2=R14;                         /* SAVE RETURN ADDRESS           * 07480000
         LR    @2,@E                                              0293  07490000
*    IF R0^=12 THEN                  /* IS STAE WORK AREA BUILT?      * 07500000
         CH    @0,@D3                                             0294  07510000
         BC    08,@9D0                                            0294  07520000
*      DO;                           /* YES, CAN CONTINUE PROCESSING  * 07530000
*        STAEWPTR=R1;                /* SET ADDR OF STAE WORK AREA    * 07540000
         LR    @4,@1                                              0296  07550000
*        STAEPTR=STAEXPRM;           /* SET ADDR OF STAE PARM LIST    * 07560000
         L     @5,0(0,@4)                                         0297  07570000
*        R11=STAECODE;               /* SET ADDRESS TO CODE           * 07580000
         L     @B,0(0,@5)                                         0298  07590000
*        R12=STAEDATA;               /* SET ADDRESS TO DATA           * 07600000
         L     @C,4(0,@5)                                         0299  07610000
*        R13=STAESAVE;               /* SET ADDRESS TO SAVE AREA      * 07620000
         L     @D,8(0,@5)                                         0300  07630000
*        GENERATE;                                                      07640000
         DROP  @B                                                       07650000
         USING @PSTART,@B            ADDRESSABILITY TO CODE             07660000
         USING @DATD,@C              ADDRESSABILITY TO DATA             07670000
         DS    0H                                                       07680000
*        IF STAEABND='0B01'X THEN    /* IF I/O SERVICE ROUT ABENDED   * 07690000
         L     @F,@X10                                            0302  07700000
         MVC   @TEMP3+1(3),5(@4)                                  0302  07710000
         C     @F,@TEMP3                                          0302  07720000
         BC    07,@9CF                                            0302  07730000
*          DO;                                                          07740000
*            R0=ADDR (IKJRBBMR);     /* SET ADDR OF STAE RETRY ROUT   * 07750000
         LA    @0,IKJRBBMR                                        0304  07760000
*            R1=STAEWPTR;            /* SET ADDR OF STAE WORK AREA    * 07770000
         LR    @1,@4                                              0305  07780000
*            R15=4;                  /* INDICATE RETRY TO BE SCHEDULED* 07790000
         LA    @F,4                                               0306  07800000
         BC    15,@9CE                                            0308  07810000
*          END;                                                         07820000
