         L     @9,MSGPTR                                          0313  08010000
         LA    @A,0(@8,@9)                                        0313  08020000
         MVC   @TEMP2+2(2),0(@A)                                  0313  08030000
         A     @F,@TEMP2                                          0313  08040000
         ST    @F,MSGQ                                            0313  08050000
*                MSGSEG=1;           /* NO TEXT INSERTION REQUIRED    * 08060000
         LA    @F,1                                               0314  08070000
         ST    @F,OLD                                             0314  08080000
*                MSGADDR=ADDR (MSGTXT); /* SET MSG TEXT ADDR IN OLD   * 08090000
         LA    @F,MSGTXT                                          0315  08100000
         ST    @F,OLD+4                                           0315  08110000
*                MSGTXT (1:MSGLL)=MSGTXTIN(1:MSGLL); /* MOVE MSGTXT     08120000
*                                       INTO BMP STORAGE              * 08130000
         L     @6,MSGQ                                            0316  08140000
         LR    @E,@6                                              0316  08150000
         LH    @7,0(0,@6)                                         0316  08160000
         BCTR  @7,0                                               0316  08170000
         LA    @A,MSGTXT                                          0316  08180000
         EX    @7,@MVC                                            0316  08190000
*                GEN (PUTLINE PARM=PTPBA,OUTPUT=OLD,MF=(E,IOPLA));      08200000
         PUTLINE PARM=PTPBA,OUTPUT=OLD,MF=(E,IOPLA)                     08210000
         DS    0H                                                       08220000
*              END;                                                     08230000
*            R15=0;                  /* CONTINUE WITH ABEND           * 08240000
@9CD     SR    @F,@F                                              0319  08250000
*          END;                                                         08260000
*      END;                                                             08270000
         BC    15,@9CC                                            0322  08280000
*    ELSE                                                               08290000
*      R15=0;                        /* NO STAE WA - CONTINUE ABEND   * 08300000
@9D0     SR    @F,@F                                              0322  08310000
*    R14=R2;                         /* RESTORE RETURN ADDRESS        * 08320000
@9CC     LR    @E,@2                                              0323  08330000
*    RETURN;                                                            08340000
*    END IKJRBBMS;                                                      08350000
@EL02    BCR   15,@E                                              0325  08360000
*                                                                       08370000
* /*  THE FOLLOWING STAE RETRY IS GIVEN CONTROL ONLY AFTER THE        * 08380000
* /*  I/O SERVICE ROUTINES HAVE ISSUED A 'B01' ABEND.  IN THIS RETRY  * 08390000
* /*  ROUTINE, AN ERROR INDICATOR IS SET SO THAT THE PROPER MESSAGE   * 08400000
* /*  IS WRITTEN AND RETURN CODE SET.                                 * 08410000
*                                                                       08420000
*                                                                       08430000
* IKJRBBMR:                                                             08440000
*    PROC (DUMYPARM) OPTIONS(NOSAVEAREA,DONTSAVE);                      08450000
IKJRBBMR EQU   *                                                  0326  08460000
*    RESTRICT (STAEWPTR,STAEPTR);                                       08470000
*    GENERATE;                                                          08480000
          DROP  @B                   DROP BASE REGISTERS                08490000
          LR    @B,@F                PUT ENTRY PT IN REG 11             08500000
          USING IKJRBBMR,@B          SET ADDRESS FOR 4 RTN              08510000
         DS    0H                                                       08520000
*    IF R0^=12 THEN                  /* IS STAE WORK AREA BUILT?      * 08530000
         CH    @0,@D3                                             0329  08540000
         BC    08,@9CB                                            0329  08550000
*      DO;                                                              08560000
*        STAEWPTR=R1;                /* SET ADDR OF STAE WORK AREA    * 08570000
         LR    @4,@1                                              0331  08580000
*        STAEPTR=STAEXPRM;           /* SET ADDR OF STAE PARM LIST    * 08590000
         L     @5,0(0,@4)                                         0332  08600000
*        R11=STAECODE;               /* SET ADDRESS TO CODE           * 08610000
         L     @B,0(0,@5)                                         0333  08620000
*        R12=STAEDATA;               /* SET ADDRESS TO DATA           * 08630000
         L     @C,4(0,@5)                                         0334  08640000
*        R13=STAESAVE;               /* SET ADDRESS TO SAVEAREA       * 08650000
         L     @D,8(0,@5)                                         0335  08660000
*        GENERATE;                                                      08670000
          DROP  @B                                                      08680000
          USING @PSTART,@B           ADDRESS TO CODE                    08690000
          USING @DATD,@C             ADDRESS TO DATA                    08700000
         DS    0H                                                       08710000
*        IF BADINDCB='1'B THEN       /* IF INPUT DCB INFO INVALID     * 08720000
         TM    BSTAT,B'01000000'                                  0337  08730000
         BC    12,@9CA                                            0337  08740000
*          INDICATR=8;               /* THEN INDICATE THIS            * 08750000
         LA    @F,8                                               0338  08760000
         STH   @F,STATUS                                          0338  08770000
         BC    15,@9C9                                            0339  08780000
*        ELSE                                                           08790000
*          IF SYSINERR='1'B THEN     /* IF I/O ERROR ON INPUT         * 08800000
@9CA     TM    BSTAT,B'00100000'                                  0339  08810000
         BC    12,@9C8                                            0339  08820000
*            INDICATR=7;             /* THEN INDICATE THIS            * 08830000
         LA    @F,7                                               0340  08840000
         STH   @F,STATUS                                          0340  08850000
         BC    15,@9C7                                            0341  08860000
*          ELSE                                                         08870000
*            IF SYSPTERR='1'B THEN   /* IF SYSPRINT ERROR             * 08880000
@9C8     TM    BSTAT,B'00001000'                                  0341  08890000
         BC    12,@9C6                                            0341  08900000
*              INDICATR=0;           /* THEN INDICATE THIS            * 08910000
         SR    @F,@F                                              0342  08920000
         STH   @F,STATUS                                          0342  08930000
         BC    15,@9C5                                            0343  08940000
*            ELSE                                                       08950000
*              IF SYSINBYD='1'B THEN /* IF ATTEMPTED TO READ BYD EOF  * 08960000
@9C6     TM    BSTAT,B'00000100'                                  0343  08970000
         BC    12,@9C4                                            0343  08980000
*                INDICATR=10;        /* THEN INDICATE THIS            * 08990000
         LA    @F,10                                              0344  09000000
         STH   @F,STATUS                                          0344  09010000
         BC    15,@9C3                                            0345  09020000
*              ELSE                                                     09030000
*                INDICATR=9;         /* INDICATE IS OTHER PROBLEM     * 09040000
@9C4     LA    @F,9                                               0345  09050000
         STH   @F,STATUS                                          0345  09060000
*        R0=104;                     /* INDICATE LEN OF AREA TO FREE  * 09070000
@9C3     EQU   *                                                  0346  09080000
@9C5     EQU   *                                                  0346  09090000
@9C7     EQU   *                                                  0346  09100000
@9C9     LA    @0,104                                             0346  09110000
*        R1=STAEWPTR;                /* SET ADDR OF AREA TO FREE      * 09120000
         LR    @1,@4                                              0347  09130000
*        GEN (FREEMAIN R,LV=(0),A=(1)); /* FREE STAE WORK AREA        * 09140000
         FREEMAIN R,LV=(0),A=(1)                                        09150000
         DS    0H                                                       09160000
*        GO TO EXIT;                 /* WRITE MSG IF POSSIBLE         * 09170000
         BC    15,EXIT                                            0349  09180000
*                                    /* THEN CLEAN UP                 * 09190000
*      END;                                                             09200000
*    ELSE                                                               09210000
*      RETURN;                       /* IF STAE WORK AREA NOT BUILT   * 09220000
*                                    /* RETURN TO ABEND               * 09230000
*    END IKJRBBMR;                                                      09240000
@9C2     EQU   *                                                  0352  09250000
@EL03    BCR   15,@E                                              0352  09260000
*    END IKJRBBMP                                                       09270000
*                                                                       09280000
* /* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     * 09290000
