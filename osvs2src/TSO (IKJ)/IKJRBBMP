*        MSGADDR=ADDR (MSGTXT);      /* SET MSG TEXT ADDR IN OLD      * 05930000
         LA    @F,MSGTXT                                          0248  05940000
         ST    @F,OLD+4                                           0248  05950000
*        IF INDICATR < 5 THEN        /* IF MSG NEED TO PROVIDE ERROR  * 05960000
         LA    @F,5                                               0249  05970000
         CH    @F,STATUS                                          0249  05980000
         BC    12,@9D8                                            0249  05990000
*          DO;                       /* CODE THEN                     * 06000000
*            RESTRICT (RETREG);      /* RESTRICT RETREG FOR CONVERT   * 06010000
*            RETREG=ERRORCDE;        /* GET THE ERROR CODE            * 06020000
         LH    @6,STATUS+2                                        0252  06030000
*            GEN (CVD  RETREG,PACKDEC); /* AND CONVERT TO EBCDIC      * 06040000
         CVD  RETREG,PACKDEC                                            06050000
         DS    0H                                                       06060000
*            GEN (UNPK ZONEDEC(UNPKLEN),PACKDEC(UNPKLEN));              06070000
         UNPK ZONEDEC(UNPKLEN),PACKDEC(UNPKLEN)                         06080000
         DS    0H                                                       06090000
*            GEN (OI   ZONEDEC+7,X'F0');                                06100000
         OI   ZONEDEC+7,X'F0'                                           06110000
         DS    0H                                                       06120000
*            MSGCODE=ZONEDEC (6:8);  /* MOVE ERROR CODE TO MESSAGE    * 06130000
         L     @7,MSGQ                                            0256  06140000
         MVC   46(3,@7),ZONEDEC+5                                 0256  06150000
*            RELEASE (RETREG);                                          06160000
*          END;                                                         06170000
*        MSGTXT (1:MSGLL)=MSGTXTIN(1:MSGLL); /* MOVE MSGTXT INTO BMP    06180000
*                                       STORAGE                       * 06190000
@9D8     L     @8,MSGQ                                            0259  06200000
         LR    @E,@8                                              0259  06210000
         LH    @9,0(0,@8)                                         0259  06220000
         BCTR  @9,0                                               0259  06230000
         LA    @A,MSGTXT                                          0259  06240000
         EX    @9,@MVC                                            0259  06250000
*        GEN (PUTLINE PARM=PTPBA,OUTPUT=OLD,MF=(E,IOPLA));              06260000
         PUTLINE PARM=PTPBA,OUTPUT=OLD,MF=(E,IOPLA)                     06270000
         DS    0H                                                       06280000
*        IF RTNREG^=0 THEN           /* IF PUTLINE NOT SUCCESSFUL     * 06290000
         LTR   @F,@F                                              0261  06300000
         BC    08,@9D7                                            0261  06310000
*          INDICATR=0;               /* THEN SET NOMSG INDICATOR      * 06320000
         SR    @F,@F                                              0262  06330000
         STH   @F,STATUS                                          0262  06340000
*      END;                                                             06350000
@9D7     EQU   *                                                  0263  06360000
*                                                                       06370000
* /*  THE FOLLOWING SEGMENT CLEANS UP RESOURCES - DELETES THE         * 06380000
* /*  BACKGROUND I/O MODULES AND CLOSES OPEN DCB'S.                   * 06390000
*                                                                       06400000
*    GENERATE;                                                          06410000
@9D9     EQU   *                                                  0264  06420000
          DELETE  EP=IKJRBBMC        /*DELETE BACKGROUND I/O MODULES*/  06430000
         DS    0H                                                       06440000
*    DCBPTR=PUTDCB;                  /* LOCATE OUTPUT DCB             * 06450000
         MVC   DCBPTR(4),BKPUT+8                                  0265  06460000
*    RESTRICT (R2);                                                     06470000
*    IF DCBOFOPN='1'B THEN           /* IF OUTPUT DCB IS OPEN         * 06480000
         L     @3,DCBPTR                                          0267  06490000
         TM    48(@3),B'00010000'                                 0267  06500000
         BC    12,@9D6                                            0267  06510000
*      DO;                                                              06520000
*        R1=ADDR (CLOSLIST);         /* SET ADDR OF LIST FORM         * 06530000
         LA    @1,CLOSLIST                                        0269  06540000
*        R2=ADDR (DCBP);             /* SET ADDR OF DCB               * 06550000
         LA    @2,DCBP                                            0270  06560000
*        GEN (CLOSE  ((2)),MF=(E,(1))); /* CLOSE OUTPUT DCB           * 06570000
         CLOSE  ((2)),MF=(E,(1))                                        06580000
         DS    0H                                                       06590000
*      END;                                                             06600000
*    DCBPTR=GETDCB;                  /* LOCATE INPUT DCB              * 06610000
@9D6     MVC   DCBPTR(4),BKGET+8                                  0273  06620000
