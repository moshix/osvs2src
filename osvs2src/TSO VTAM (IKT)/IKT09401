         TITLE '******** IKT09401:    TCLEARQ - TERMINAL CONTROL MACRO *00001000
                 *******'                                               00002000
IKT09401 CSECT ,                                                   0001 00003000
@MAINENT BALR  @15,0                                               0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKT09401  77.342'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*        RSAVE = TVWALLWA;             /*GET ADDR OF LL WORK AREA    */ 00013000
         L     @02,TSBEXTNT(,TSBPTR)                               0041 00014000
         L     @02,TVWAPTR(,@02)                                   0041 00015000
         L     RSAVE,TVWALLWA(,@02)                                0041 00016000
*        RDATA = ADDR(AUTOAREA);       /*GET AUTODATA ADDRESS        */ 00017000
         LA    RDATA,AUTOAREA(,RSAVE)                              0042 00018000
*        GEN(USING @DATD,RDATA)                                    0043 00019000
*            REFS(RDATA);              /*GET AUTODATA ADDRESSABILITY */ 00020000
         USING @DATD,RDATA                                              00021000
*        RPARMAD = ADDR(PARMS);        /*ADDR OF QUEUE MGR PARM AREA */ 00022000
         LA    RPARMAD,PARMS(,RSAVE)                               0044 00023000
*        /************************************************************/ 00024000
*        /*                                                          */ 00025000
*        /* THE INPUT PARAMETER REGISTER 1 MUST BE NEGATIVE OR ZERO. */ 00026000
*        /* IF IT IS POSITIVE THE PARAMETER ERROR IS NOTED WITH      */ 00027000
*        /* RETURN CODE 4.                                           */ 00028000
*        /*                                                          */ 00029000
*        /************************************************************/ 00030000
*                                                                  0045 00031000
*        IF RPARM1 > 0                                             0045 00032000
*        THEN                                                      0045 00033000
         LTR   RPARM1,RPARM1                                       0045 00034000
         BNP   @RF00045                                            0045 00035000
*          RETCODE = CODE04;                                       0046 00036000
         LA    RETCODE,4                                           0046 00037000
*        ELSE                                                      0047 00038000
*                                                                  0047 00039000
*          /**********************************************************/ 00040000
*          /*                                                        */ 00041000
*          /* THE WORKAREA IS ADDRESSED BY TVWALLWA A WORK AREA VALID*/ 00042000
*          /* FOR AS LONG AS THE LOCAL LOCK IS HELD. IT CONTAINS A  E*/ 00043000
*          /* SAVEAREA FOR Q-MGR CALLS AND A PARM STRUCTURE FOR Q-MGR*/ 00044000
*          /* INPUT PARAMETERS                                       */ 00045000
*          /*                                                        */ 00046000
*          /**********************************************************/ 00047000
*TCLRQ:    DO;                                                     0047 00048000
         B     @RC00045                                            0047 00049000
@RF00045 DS    0H                                                  0047 00050000
TCLRQ    DS    0H                                                  0048 00051000
*            RHOLD = RPARM1;           /*SAVE PARM VALUE             */ 00052000
         LR    RHOLD,RPARM1                                        0048 00053000
*            OLDRLINK = RLINK;         /*SAVE RETURN ADDR REG 14     */ 00054000
         LR    OLDRLINK,RLINK                                      0049 00055000
*            RFY (RPARM1,RLINK) UNRSTD;                            0050 00056000
*                                                                  0050 00057000
*                                                                  0050 00058000
*            /********************************************************/ 00059000
*            /*                                                      */ 00060000
*            /* THE INPUT PARAMETER NOW LOCATED IN RHOLD IS EXAMINED.*/ 00061000
*            /* IF NEGATIVE TCLEARQ(INPUT) PROCESSING IS DONE. IF    */ 00062000
*            /* ZERO, TCLEARQ(OUTPUT) PROCESSING IS DONE.            */ 00063000
*            /*                                                      */ 00064000
*            /********************************************************/ 00065000
*            IF RHOLD < 0                                          0051 00066000
*            THEN                                                  0051 00067000
*                                                                  0051 00068000
         LTR   RHOLD,RHOLD                                         0051 00069000
         BNM   @RF00051                                            0051 00070000
*              /******************************************************/ 00071000
*              /*                                                    */ 00072000
*              /* TCLEARQ(INPUT) - TSBIFLSH AND TSBSPIT ARE SET.     */ 00073000
*              /* THE Q-MGR IS INVOKED TO CLEAR THE INPUT QUEUE.     */ 00074000
*              /*                                                    */ 00075000
*              /******************************************************/ 00076000
*                DO;                                               0052 00077000
*                  RFY RHOLD UNRSTD;                               0053 00078000
*                  TSBIFLSH = '1'B;    /*FLUSH TYPED AHEAD INPUT     */ 00079000
         OI    TSBIFLSH(TSBPTR),B'00001000'                        0054 00080000
*                  TSBSPIT  = '1'B;    /*HALT PROMPTING              */ 00081000
         OI    TSBSPIT(TSBPTR),B'00100000'                         0055 00082000
*                  IPREQ = CLEARI;     /*REQUEST INPUT QUEUE CLEARED */ 00083000
*                                                                  0056 00084000
         MVI   IPREQ(RPARMAD),X'05'                                0056 00085000
*                  CALL IKTQMIN(PARMS);/*CALL INPUT QUEUE MANAGER    */ 00086000
*                                                                  0057 00087000
         LA    @14,PARMS(,RSAVE)                                   0057 00088000
         ST    @14,@AL00001                                        0057 00089000
         L     @01,CVTPTR                                          0057 00090000
         L     @01,TCASTPTR(,@01)                                  0057 00091000
         L     @15,TCASIQM(,@01)                                   0057 00092000
         LA    @01,@AL00001                                        0057 00093000
         BALR  @14,@15                                             0057 00094000
*                  /**************************************************/ 00095000
*                  /*                                                */ 00096000
*                  /* IF THE TERMINAL OUTPUT MANAGER IS AVAILABLE    */ 00097000
*                  /* AND NOT NOW SCHEDULED, SCHEDULE IT.            */ 00098000
*                  /*                                                */ 00099000
*                  /**************************************************/ 00100000
*                                                                  0058 00101000
*                  IF TVWATIS = '0'B  /* CHECK FOR TOM SCHED @ZA27288*/ 00102000
*                  THEN                                            0058 00103000
         L     @14,TSBEXTNT(,TSBPTR)                               0058 00104000
         L     @01,TVWAPTR(,@14)                                   0058 00105000
         TM    TVWATIS(@01),B'01000000'                            0058 00106000
         BNZ   @RF00058                                            0058 00107000
*                    DO;                                           0059 00108000
*                      RFY (REG0,RPARM1,RWORK8) RSTD;              0060 00109000
*                      RWORK8 = TSBXSRB;  /*GET ADDR OF SRB          */ 00110000
         L     RWORK8,TSBXSRB(,@14)                                0061 00111000
*                      GEN(SCHEDULE SRB=(RWORK8),SCOPE=LOCAL)      0062 00112000
*                         REFS(RWORK8,CVTPTR,CVTLSMQ,CVTMAP,SRBSECT)    00113000
*                             SETS(REG0,RPARM1,RETCODE,SRBFLNK);   0062 00114000
         SCHEDULE SRB=(RWORK8),SCOPE=LOCAL                              00115000
*                      RFY (REG0,RPARM1,RWORK8) UNRSTD;            0063 00116000
*                      TVWATIS = '1'B;                             0064 00117000
         L     @14,TSBEXTNT(,TSBPTR)                               0064 00118000
         L     @14,TVWAPTR(,@14)                                   0064 00119000
         OI    TVWATIS(@14),B'01000000'                            0064 00120000
*                    END;                                          0065 00121000
*                  ELSE;                                           0066 00122000
@RF00058 DS    0H                                                  0067 00123000
*                END;                  /*END OF TCLEARQ(INPUT)       */ 00124000
*                                                                  0067 00125000
*            ELSE                                                  0068 00126000
*                                                                  0068 00127000
*              /******************************************************/ 00128000
*              /*                                                    */ 00129000
*              /* TCLEARQ(OUTPUT) - INVOKE OUTPUT QUEUE MANAGER      */ 00130000
*              /* TO CLEAR OUTPUT QUEUE OF ALL BUT ASID MESSAGES     */ 00131000
*              /*                                                    */ 00132000
*              /******************************************************/ 00133000
*              DO;                                                 0068 00134000
         B     @RC00051                                            0068 00135000
@RF00051 DS    0H                                                  0069 00136000
*                OPREQ = CLEARO;       /*REQUEST OUTPUT QUEUE CLEARED*/ 00137000
         MVI   OPREQ(RPARMAD),X'05'                                0069 00138000
*                CALL IKTQMOUT(PARMS); /*CALL OUTPUT QUEUE MANAGER   */ 00139000
         LA    @14,PARMS(,RSAVE)                                   0070 00140000
         ST    @14,@AL00001                                        0070 00141000
         L     @01,CVTPTR                                          0070 00142000
         L     @01,TCASTPTR(,@01)                                  0070 00143000
         L     @15,TCASOQM(,@01)                                   0070 00144000
         LA    @01,@AL00001                                        0070 00145000
         BALR  @14,@15                                             0070 00146000
*              END;                                                0071 00147000
*                                                                  0071 00148000
*            /********************************************************/ 00149000
*            /*                                                      */ 00150000
*            /* RESTORE RETURN REGISTER                              */ 00151000
*            /*                                                      */ 00152000
*            /********************************************************/ 00153000
*                                                                  0072 00154000
*            RFY RLINK RSTD;                                       0072 00155000
@RC00051 DS    0H                                                  0073 00156000
*            RLINK = OLDRLINK;         /*RESTORE RETURN REGISTER     */ 00157000
         LR    RLINK,OLDRLINK                                      0073 00158000
*            RETCODE = CODE00;         /*SET GOOD RETURN CODE        */ 00159000
*                                                                  0074 00160000
         SLR   RETCODE,RETCODE                                     0074 00161000
*          END TCLRQ;                  /*END OF TCLEARQ PROCESSING   */ 00162000
*        RETURN;                                           /*@ZM20487*/ 00163000
@EL00001 DS    0H                                                  0076 00164000
@EF00001 DS    0H                                                  0076 00165000
@ER00001 BR    @14                                                 0076 00166000
*        END IKT09401                                              0077 00167000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.      * 00168000
*/*%INCLUDE SYSLIB  (IKTEQU  )                                        * 00169000
