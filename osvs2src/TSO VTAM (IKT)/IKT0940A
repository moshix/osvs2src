IKT0940A CSECT ,                                                   0001 00003000
@MAINENT BALR  @15,0                                               0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKT0940A  77.319'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*        RWORK2 = RPARM0;              /*COPY REGISTERS              */ 00013000
         LR    RWORK2,RPARM0                                       0027 00014000
*        RWORK3 = RPARM1;                                          0028 00015000
         LR    RWORK3,RPARM1                                       0028 00016000
*        SLL(RWORK2,SHIFT8);           /*SHIFT OUT ENTRY CODE        */ 00017000
         SLR   @10,@10                                             0029 00018000
         IC    @10,SHIFT8                                          0029 00019000
         SLL   RWORK2,0(@10)                                       0029 00020000
*        SRL(RWORK2,SHIFT16);          /*SHIFT OUT LINE PARM         */ 00021000
         SLR   @04,@04                                             0030 00022000
         IC    @04,SHIFT16                                         0030 00023000
         SRL   RWORK2,0(@04)                                       0030 00024000
*        SRL(RWORK3,SHIFT8);           /*SHIFT OUT SIZE PARM         */ 00025000
*                                                                  0031 00026000
         SRL   RWORK3,0(@10)                                       0031 00027000
*        IF RWORK2 ^= 0 | RWORK3 ^= 0                              0032 00028000
*        THEN                                                      0032 00029000
         SLR   @10,@10                                             0032 00030000
         CLR   RWORK2,@10                                          0032 00031000
         BNE   @RT00032                                            0032 00032000
         CLR   RWORK3,@10                                          0032 00033000
         BE    @RF00032                                            0032 00034000
@RT00032 DS    0H                                                  0033 00035000
*          RETCODE = CODE04;                                       0033 00036000
         LA    RETCODE,4                                           0033 00037000
*        ELSE                                                      0034 00038000
*                                                                  0034 00039000
*          /**********************************************************/ 00040000
*          /*                                                        */ 00041000
*          /* THE SIZE PARAMETER IN RPARM1 IS REQUIRED WHENEVER      */ 00042000
*          /* STSIZE IS INVOKED. IF RPARM1 IS ZERO, THE RETURN CODE  */ 00043000
*          /* IS SET TO 8.                                           */ 00044000
*          /*                                                        */ 00045000
*          /**********************************************************/ 00046000
*                                                                  0034 00047000
*          IF RPARM1 = 0                                           0034 00048000
*          THEN                                                    0034 00049000
         B     @RC00032                                            0034 00050000
@RF00032 LTR   RPARM1,RPARM1                                       0034 00051000
         BNZ   @RF00034                                            0034 00052000
*            RETCODE = CODE08;                                     0035 00053000
         LA    RETCODE,8                                           0035 00054000
*          ELSE                                                    0036 00055000
*                                                                  0036 00056000
*            /********************************************************/ 00057000
*            /*                                                      */ 00058000
*            /* SHIFT OUT ENTRY CODE FROM RPARM0 AND ACQUIRE THE     */ 00059000
*            /* KEY OF THE TSB CREATOR (TCAS).                       */ 00060000
*            /*                                                      */ 00061000
*            /********************************************************/ 00062000
*                                                                  0036 00063000
*STSZPROC:   DO;                                                   0036 00064000
         B     @RC00034                                            0036 00065000
@RF00034 DS    0H                                                  0036 00066000
STSZPROC DS    0H                                                  0037 00067000
*              GEN(MODESET KEYADDR=TCASKEY,WORKREG=2)              0037 00068000
*                  REFS(TCASKEY) SETS(RWORK2);/*GET TCAS KEY         */ 00069000
         MODESET KEYADDR=TCASKEY,WORKREG=2                              00070000
*              SLL(RPARM0,SHIFT8);     /*SHIFT OUT ENTRY CODE        */ 00071000
         SLR   @10,@10                                             0038 00072000
         IC    @10,SHIFT8                                          0038 00073000
         SLL   RPARM0,0(@10)                                       0038 00074000
*              SRL(RPARM0,SHIFT8);                                 0039 00075000
*                                                                  0039 00076000
         SRL   RPARM0,0(@10)                                       0039 00077000
*              /******************************************************/ 00078000
*              /*                                                    */ 00079000
*              /* IF THE TERMINAL IN USE IS A DISPLAY AND THE LINE   */ 00080000
*               /* PARAMETER WAS NOT SPECIFIED, THE RETURN CODE IS   */ 00081000
*              /* SET TO 8.                                          */ 00082000
*              /*                                                    */ 00083000
*              /******************************************************/ 00084000
*                                                                  0040 00085000
*              IF TSBDSPLY = '1'B                                  0040 00086000
*              THEN                                                0040 00087000
*                                                                  0040 00088000
         TM    TSBDSPLY(TSBPTR),B'00100000'                        0040 00089000
         BNO   @RF00040                                            0040 00090000
*                IF RPARM0 = 0                                     0041 00091000
*                THEN                                              0041 00092000
         LTR   RPARM0,RPARM0                                       0041 00093000
         BNZ   @RF00041                                            0041 00094000
*                  RETCODE = CODE08;                               0042 00095000
         LA    RETCODE,8                                           0042 00096000
*                ELSE                                              0043 00097000
*                                                                  0043 00098000
*                  /**************************************************/ 00099000
*                  /*                                                */ 00100000
*                  /* THE DISPROC ROUTINE IS INVOKED TO UPDATE THE   */ 00101000
*                  /* TSB AND VERIFY THE SCREEN DIMENSIONS AS        */ 00102000
*                  /* STANDARD.                                      */ 00103000
*                  /*                                                */ 00104000
*                  /**************************************************/ 00105000
*                                                                  0043 00106000
*                  GEN(BAL RBRANCH,DISPROC)                        0043 00107000
*                      REFS(DISPROC) SETS(RBRANCH);                0043 00108000
*                                                                  0043 00109000
         B     @RC00041                                            0043 00110000
@RF00041 DS    0H                                                  0043 00111000
         BAL RBRANCH,DISPROC                                            00112000
*              ELSE                                                0044 00113000
*                                                                  0044 00114000
*                /****************************************************/ 00115000
*                /*                                                  */ 00116000
*                /* IF THE TERMINAL IN USE IS NOT A DISPLAY AND THE  */ 00117000
*                /* LINE PARAMETER WAS SPECIFIED, THE RETURN CODE IS */ 00118000
*                /* SET TO 8.                                        */ 00119000
*                /*                                                  */ 00120000
*                /****************************************************/ 00121000
*                                                                  0044 00122000
*                IF RPARM0 ^= 0                                    0044 00123000
*                THEN                                              0044 00124000
         B     @RC00040                                            0044 00125000
@RF00040 LTR   RPARM0,RPARM0                                       0044 00126000
         BZ    @RF00044                                            0044 00127000
*                  RETCODE = CODE08;                               0045 00128000
         LA    RETCODE,8                                           0045 00129000
*                ELSE                                              0046 00130000
*                                                                  0046 00131000
*                  /**************************************************/ 00132000
*                  /*                                                */ 00133000
*                  /* SET VALUE OF SIZE IN TSB AND SET GOOD          */ 00134000
*                  /* RETURN CODE                                    */ 00135000
*                  /*                                                */ 00136000
*                  /**************************************************/ 00137000
*                                                                  0046 00138000
*                  DO;                                             0046 00139000
         B     @RC00044                                            0046 00140000
@RF00044 DS    0H                                                  0047 00141000
*                    TSBLNSZ = RPARM1;                             0047 00142000
         STC   RPARM1,TSBLNSZ(,TSBPTR)                             0047 00143000
*                    RETCODE = CODE00;                             0048 00144000
         SLR   RETCODE,RETCODE                                     0048 00145000
*                  END;                                            0049 00146000
*                                                                  0049 00147000
*            GEN(MODESET EXTKEY=SUPR); /*RETURN TO SUPR KEY          */ 00148000
*                                                                  0050 00149000
@RC00044 DS    0H                                                  0050 00150000
@RC00040 DS    0H                                                  0050 00151000
         MODESET EXTKEY=SUPR                                            00152000
*            END STSZPROC;                                         0051 00153000
*        RETURN;                                           /*@ZM20487*/ 00154000
@EL00001 DS    0H                                                  0052 00155000
@EF00001 DS    0H                                                  0052 00156000
@ER00001 BR    @14                                                 0052 00157000
*        /************************************************************/ 00158000
*        /*                                                          */ 00159000
*        /* DISPROC IS INVOKED VIA A 'BAL' FROM THE MAINLINE. THIS   */ 00160000
*        /* ROUTINE SETS THE FIELDS IN THE TSB TO INDICATE THE       */ 00161000
*        /* SPECIFIED SCREEN DIMENSIONS, AND CHECKS THE DIMENSION    */ 00162000
*        /* AGAINST KNOWN STANDARDS. NON-STANDARD DIMENSIONS ARE     */ 00163000
*        /* ALLOWED, BUT CAUSE THE RETURN CODE TO BE SET TO 12.      */ 00164000
*        /*                                                          */ 00165000
*        /************************************************************/ 00166000
*                                                                  0053 00167000
*DISPROC:                                                          0053 00168000
*                                                                  0053 00169000
*        TSBLNSZ = RPARM1;             /*MOVE IN LINE SIZE           */ 00170000
DISPROC  STC   RPARM1,TSBLNSZ(,TSBPTR)                             0053 00171000
*        TSBLNNO = RPARM0;             /*MOVE IN NUMBER OF LINES     */ 00172000
         STC   RPARM0,TSBLNNO(,TSBPTR)                             0054 00173000
*        TVWAXSCD = '1'B;              /*EXAM SCR DIMEN SW   @G58AK3A*/ 00174000
*                                                                  0055 00175000
         L     @10,TSBEXTNT(,TSBPTR)                               0055 00176000
         L     @10,TSBXTVWA(,@10)                                  0055 00177000
         OI    TVWAXSCD(@10),B'00001000'                           0055 00178000
*        /***********************************************************/  00179000
*        /*                                                         */  00180000
*        /* STANDARD SIZES FOR 3270 DISPLAYS ARE 24X80 AND 12X40.   */  00181000
*        /* IF A STANDARD SIZE WAS SPECIFIED, RETURN CODE IS 0. IF  */  00182000
*        /* NON-STANDARD SIZE WAS SPECIFIED, RETURN CODE IS 12. NO  */  00183000
*        /* OTHER DISPLAYS ARE SUPPORTED BY TSO/VTAM.               */  00184000
*        /*                                                         */  00185000
*        /***********************************************************/  00186000
*                                                                  0056 00187000
*        IF (TSBLNNO = LENGTH1 & TSBLNSZ = WIDTH1) |               0056 00188000
*           (TSBLNNO = LENGTH2 & TSBLNSZ = WIDTH2)       /*        0056 00189000
*                                      WAS STANDARD SIZE GIVEN      */  00190000
*        THEN                                                      0056 00191000
         CLI   TSBLNNO(TSBPTR),24                                  0056 00192000
         BNE   @GL00002                                            0056 00193000
         CLI   TSBLNSZ(TSBPTR),80                                  0056 00194000
         BE    @RT00056                                            0056 00195000
@GL00002 CLI   TSBLNNO(TSBPTR),12                                  0056 00196000
         BNE   @RF00056                                            0056 00197000
         CLI   TSBLNSZ(TSBPTR),40                                  0056 00198000
         BNE   @RF00056                                            0056 00199000
@RT00056 DS    0H                                                  0057 00200000
*          RETCODE = CODE00;           /*YES - STANDARD SIZE        */  00201000
         SLR   RETCODE,RETCODE                                     0057 00202000
*        ELSE                                                      0058 00203000
*          RETCODE = CODE12;           /*NO - NON STANDARD SIZE     */  00204000
*                                                                  0058 00205000
         B     @RC00056                                            0058 00206000
@RF00056 LA    RETCODE,12                                          0058 00207000
*        /***********************************************************/  00208000
*        /*                                                         */  00209000
