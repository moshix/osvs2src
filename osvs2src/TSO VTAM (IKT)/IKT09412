         TITLE '********  IKT09412:  STFSMODE - TERMINAL CONTROL MACRO *00001000
                 *******'                                               00002000
IKT09412 CSECT ,                                                   0001 00003000
@MAINENT BALR  @15,0                                               0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKT09412  77.319'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*        /************************************************************/ 00013000
*        /* SAVE REGISTER 1.  CLEAR RESHOW KEY VALUE IN      @G58AK3B*/ 00014000
*        /* RIGHT-MOST BYTE OF REGISTER 1.                   @G58AK3B*/ 00015000
*        /************************************************************/ 00016000
*        RSHWKEY = RPARM1;             /*SAVE REGISTER 1     @G58AK3B*/ 00017000
         LR    RSHWKEY,RPARM1                                      0024 00018000
*        SRL(RPARM1,SHIFT8);           /*SHIFT OUT RSHWKEY   @G58AK3B*/ 00019000
         SLR   @07,@07                                             0025 00020000
         IC    @07,SHIFT8                                          0025 00021000
         SRL   RPARM1,0(@07)                                       0025 00022000
*        SLL(RPARM1,SHIFT8);           /*REALIGN REGISTER 1  @G58AK3B*/ 00023000
*                                                                  0026 00024000
         SLL   RPARM1,0(@07)                                       0026 00025000
*        /************************************************************/ 00026000
*        /*                                                          */ 00027000
*        /* SHIFT OUT PARAMETER BIT. IF REMAINING BITS ARE NOT ZERO  */ 00028000
*        /* RETURN CODE 4 IS ISSUED.                                 */ 00029000
*        /*                                                          */ 00030000
*        /************************************************************/ 00031000
*        SLDL(RPARM0,SHIFT2);          /*SHIFT OUT PARM BITS  OZ11997*/ 00032000
         SLR   @07,@07                                             0027 00033000
         IC    @07,SHIFT2                                          0027 00034000
         SLDL  RPARM0,0(@07)                                       0027 00035000
*        IF RPARM1 ^= 0 THEN           /*INVALID - RETURN CODE 4     */ 00036000
         LTR   RPARM1,RPARM1                                       0028 00037000
         BZ    @RF00028                                            0028 00038000
*          RETCODE = CODE04;                                       0029 00039000
         LA    RETCODE,4                                           0029 00040000
*        ELSE                                                      0030 00041000
*          /**********************************************************/ 00042000
*          /*                                                        */ 00043000
*          /* IF THE TERMINAL IS NOT A DISPLAY TERMINAL, THE INVALID */ 00044000
*          /* TERMINAL IS NOTED WITH RETURN CODE 8                   */ 00045000
*          /*                                                        */ 00046000
*          /**********************************************************/ 00047000
*          IF TSBDSPLY = '0'B THEN                                 0030 00048000
         B     @RC00028                                            0030 00049000
@RF00028 TM    TSBDSPLY(TSBPTR),B'00100000'                        0030 00050000
         BNZ   @RF00030                                            0030 00051000
*            RETCODE = CODE08;                                     0031 00052000
         LA    RETCODE,8                                           0031 00053000
*          ELSE                                                    0032 00054000
*            /********************************************************/ 00055000
*            /*                                                      */ 00056000
*            /* THE PARAMETER BIT IS RESTORED. FULLSCREEN MODE WILL  */ 00057000
*            /* BE SET OFF OR ON BASED ON THE PARAMETER BIT.         */ 00058000
*            /*                                                      */ 00059000
*            /********************************************************/ 00060000
*            DO;                                                   0032 00061000
         B     @RC00030                                            0032 00062000
@RF00030 DS    0H                                                  0033 00063000
*              SRDL(RPARM0,SHIFT2);    /*RESTORE PARM BITS    OZ11997*/ 00064000
         SLR   @07,@07                                             0033 00065000
         IC    @07,SHIFT2                                          0033 00066000
         SRDL  RPARM0,0(@07)                                       0033 00067000
*              RETCODE = CODE00;       /*SET GOOD RETURN CODE        */ 00068000
         SLR   RETCODE,RETCODE                                     0034 00069000
*              IF RFIX1 >=0 THEN       /*LOOK FOR MODE OFF    OZ11997*/ 00070000
         LTR   RFIX1,RFIX1                                         0035 00071000
         BM    @RF00035                                            0035 00072000
*                /****************************************************/ 00073000
*                /*                                                  */ 00074000
*                /* TURN OFF FULL SCREEN MODE INDICATORS             */ 00075000
*                /*                                                  */ 00076000
*                /****************************************************/ 00077000
*                DO;                                               0036 00078000
*                  TVWALNCT = 1;       /*SET LINE COUNTER    @YM03603*/ 00079000
         L     @07,TSBEXTNT(,TSBPTR)                               0037 00080000
         L     @07,TSBXTVWA(,@07)                                  0037 00081000
         MVI   TVWALNCT(@07),X'01'                                 0037 00082000
*                  TVWAFSM = '0'B;     /*FULL SCREEN MODE BIT        */ 00083000
*                  TVWAWO  = '0'B;     /*FULL SCREEN WRITTEN OVER    */ 00084000
*                  TVWAFSW = '0'B;     /*FULL SCREEN WAITING         */ 00085000
         NI    TVWAFSM(@07),B'10001111'                            0040 00086000
*                  /**************************************************/ 00087000
*                  /* WHENEVER STFSMODE IS OFF, THE RESHOW   @G58AK3B*/ 00088000
*                  /* KEY DEFAULTS TO THE PA2 KEY.           @G58AK3B*/ 00089000
*                  /**************************************************/ 00090000
*                  TVWARSHW = PA2;     /*RESET RESHOW KEY    @G58AK3B*/ 00091000
         MVI   TVWARSHW(@07),X'6E'                                 0041 00092000
*                END;                                              0042 00093000
*              ELSE                                                0043 00094000
*                /****************************************************/ 00095000
*                /*                                                  */ 00096000
*                /* TURN ON FULL SCREEN MODE INDICATOR               */ 00097000
*                /*                                                  */ 00098000
*                /****************************************************/ 00099000
*                DO;                                               0043 00100000
         B     @RC00035                                            0043 00101000
@RF00035 DS    0H                                                  0044 00102000
*                  TVWAFSM = '1'B;     /*FULL SCREEN MODE BIT        */ 00103000
         L     @07,TSBEXTNT(,TSBPTR)                               0044 00104000
         L     @07,TSBXTVWA(,@07)                                  0044 00105000
         OI    TVWAFSM(@07),B'01000000'                            0044 00106000
*                  SLL(RPARM1,SHIFT1); /*SHIFT OUT MODE BIT   OZ11997*/ 00107000
         SLR   @04,@04                                             0045 00108000
         IC    @04,SHIFT1                                          0045 00109000
         SLL   RPARM1,0(@04)                                       0045 00110000
*                  IF RPARM1 ^= 0 THEN /*LOOK FOR INITIAL CALLOZ11997*/ 00111000
         LTR   RPARM1,RPARM1                                       0046 00112000
         BZ    @RF00046                                            0046 00113000
*                    TVWANFSP = '1'B;  /*SET NO FULL SCR PAGE OZ11997*/ 00114000
         OI    TVWANFSP(@07),B'00000100'                           0047 00115000
*                  ELSE                                            0048 00116000
*                    TVWANFSP = '0'B;  /*ALLOW FULL SCR PAGE  OZ11997*/ 00117000
         B     @RC00046                                            0048 00118000
@RF00046 L     @07,TSBEXTNT(,TSBPTR)                               0048 00119000
         L     @07,TSBXTVWA(,@07)                                  0048 00120000
         NI    TVWANFSP(@07),B'11111011'                           0048 00121000
*                  /**************************************************/ 00122000
*                  /* THE RSHWKEY VALUE IS FOUND IN THE      @G58AK3B*/ 00123000
*                  /* RIGHT-MOST BYTE OF THE REGISTER.       @G58AK3B*/ 00124000
*                  /**************************************************/ 00125000
*                  SLL(RSHWKEY,SHIFT24); /*ISOLATE RESHOW    @G58AK3B*/ 00126000
@RC00046 SLR   @07,@07                                             0049 00127000
         IC    @07,SHIFT24                                         0049 00128000
         SLL   RSHWKEY,0(@07)                                      0049 00129000
*                  SRL(RSHWKEY,SHIFT24); /*IN RIGHT-MOST BYTE@G58AK3B*/ 00130000
         SRL   RSHWKEY,0(@07)                                      0050 00131000
*                  /**************************************************/ 00132000
*                  /* WHEN STFSMODE IS ON AND A RSHWKEY HAS  @G58AK3B*/ 00133000
*                  /* BEEN SPECIFIED, THE SPECIFIED RSHWKEY  @G58AK3B*/ 00134000
*                  /* BECOMES THE RESHOW KEY.                @G58AK3B*/ 00135000
*                  /* WHEN STFSMODE IS ON BUT RSHWKEY HAS    @G58AK3B*/ 00136000
*                  /* NOT BEEN SPECIFIED, THE RESHOW KEY IS  @G58AK3B*/ 00137000
*                  /* NOT CHANGED.                           @G58AK3B*/ 00138000
*                  /**************************************************/ 00139000
*                  IF RSHWKEY ^= ZERO  /*RSHWKEY SPECIFIED?  @ZM20757*/ 00140000
*                  THEN                /*RSHWKEY SPECIFIED   @G58AK3B*/ 00141000
         LTR   RSHWKEY,RSHWKEY                                     0051 00142000
         BZ    @RF00051                                            0051 00143000
*                    TVWARSHW = KEYS(RSHWKEY); /* SET RSHWKEY@G58AK3B*/ 00144000
         L     @07,TSBEXTNT(,TSBPTR)                               0052 00145000
         L     @07,TSBXTVWA(,@07)                                  0052 00146000
         LA    @04,KEYS-1(RSHWKEY)                                 0052 00147000
         MVC   TVWARSHW(1,@07),0(@04)                              0052 00148000
*                  ELSE;               /*RSHWKEY NOT SPECIFD @G58AK3B*/ 00149000
@RF00051 DS    0H                                                  0054 00150000
*                END;                                              0054 00151000
*            END;                      /*END OF MODE PROCESSING      */ 00152000
*        END IKT09412                                              0056 00153000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.      * 00154000
*/*%INCLUDE SYSLIB  (IKJTSB  )                                        * 00155000
*/*%INCLUDE SYSLIB  (IKTTSBX )                                        * 00156000
*/*%INCLUDE SYSLIB  (IKTTVWA )                                        * 00157000
