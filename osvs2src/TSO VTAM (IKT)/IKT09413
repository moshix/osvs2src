         TITLE '******** IKT09413:  STLINENO - TERMINAL CONTROL MACRO  *00001000
                ********'                                               00002000
IKT09413 CSECT ,                                                   0001 00003000
@MAINENT BALR  @15,0                                               0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKT09413  77.319'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*        /************************************************************/ 00013000
*        /*                                                          */ 00014000
*        /* SHIFT OUT PARAMETERS FROM INPUT REG.  IF ANY REMAINING   */ 00015000
*        /* BITS ARE NOT ZERO, ERROR RETURN CODE 4 IS ISSUED.        */ 00016000
*        /*                                                          */ 00017000
*        /************************************************************/ 00018000
*        REG2 = RPARM1;                /*MOVE PARM REG TO WORK REG   */ 00019000
         LR    REG2,RPARM1                                         0025 00020000
*        SLL(REG2,SHIFT1);             /*SHIFT OUT MODE BIT          */ 00021000
         SLR   @10,@10                                             0026 00022000
         IC    @10,SHIFT1                                          0026 00023000
         SLL   REG2,0(@10)                                         0026 00024000
*        SRL(REG2,SHIFT9);             /*SHIFT OUT LINE NUMBER       */ 00025000
         SLR   @10,@10                                             0027 00026000
         IC    @10,SHIFT9                                          0027 00027000
         SRL   REG2,0(@10)                                         0027 00028000
*        IF REG2 ^= 0 THEN                                         0028 00029000
         LTR   REG2,REG2                                           0028 00030000
         BZ    @RF00028                                            0028 00031000
*          RETCODE = CODE04;                                       0029 00032000
         LA    RETCODE,4                                           0029 00033000
*        ELSE                                                      0030 00034000
*          /**********************************************************/ 00035000
*          /*                                                        */ 00036000
*          /* IF THE TERMINAL IN USE IS NOT A DISPLAY TERMINAL, THE  */ 00037000
*          /* RETURN CODE IS SET TO 8.                               */ 00038000
*          /*                                                        */ 00039000
*          /**********************************************************/ 00040000
*          IF TSBDSPLY = '0'B THEN                                 0030 00041000
         B     @RC00028                                            0030 00042000
@RF00028 TM    TSBDSPLY(TSBPTR),B'00100000'                        0030 00043000
         BNZ   @RF00030                                            0030 00044000
*            RETCODE = CODE08;                                     0031 00045000
         LA    RETCODE,8                                           0031 00046000
*          ELSE                                                    0032 00047000
*            /********************************************************/ 00048000
*            /*                                                      */ 00049000
*            /* THE LINE NUMBER SPECIFIED MUST BE GREATER THAN ZERO  */ 00050000
*            /* AND NO LARGER THAN THE MAX ALLOWED NUMBER OF LINES   */ 00051000
*            /* FOR THE TERMINAL IN USE. INVALID LINE NUMBER IS NOTED*/ 00052000
*            /* WITH RETURN CODE 12.                                 */ 00053000
*            /*                                                      */ 00054000
*            /********************************************************/ 00055000
*            DO;                                                   0032 00056000
         B     @RC00030                                            0032 00057000
@RF00030 DS    0H                                                  0033 00058000
*              REG2A = RPARM1;         /*SAVE PARM WITH MODE BIT   0033 00059000
*                                        INTACT.                     */ 00060000
         LR    REG2A,RPARM1                                        0033 00061000
*              SLL(RPARM1,SHIFT1);     /*SHIFT OUT MODE BIT          */ 00062000
         SLR   @10,@10                                             0034 00063000
         IC    @10,SHIFT1                                          0034 00064000
         SLL   RPARM1,0(@10)                                       0034 00065000
*              SRL(RPARM1,SHIFT1);     /*RESTORE LINE NUMBER         */ 00066000
         SRL   RPARM1,0(@10)                                       0035 00067000
*              IF RPARM1 = 0 THEN      /*ZERO LINE NUMBER IS INVALID */ 00068000
         LTR   RPARM1,RPARM1                                       0036 00069000
         BNZ   @RF00036                                            0036 00070000
*                RETCODE = CODE12;                                 0037 00071000
         LA    RETCODE,12                                          0037 00072000
*              ELSE                                                0038 00073000
*                /****************************************************/ 00074000
*                /*                                                  */ 00075000
*                /* IF LINE NUMBER IS NOT TOO LARGE, LINNOPROC IS    */ 00076000
*                /* CALLED TO SET THE LINE NUMBER AND PERFORM MODE   */ 00077000
*                /* PROCESSING.                                      */ 00078000
*                /*                                                  */ 00079000
*                /****************************************************/ 00080000
*                                                                  0038 00081000
*                IF TSBXTMBF = LGSCREEN THEN /*LOOK FOR LARGE SCREEN */ 00082000
         B     @RC00036                                            0038 00083000
@RF00036 L     @10,TSBEXTNT(,TSBPTR)                               0038 00084000
         CLC   TSBXTMBF(2,@10),@CH00057                            0038 00085000
         BNE   @RF00038                                            0038 00086000
*                  IF RPARM1 > MAXLNLG THEN /*TOO LARGE, SET RETCODE */ 00087000
         CL    RPARM1,@CF00059                                     0039 00088000
         BNH   @RF00039                                            0039 00089000
*                    RETCODE = CODE12;                             0040 00090000
         LA    RETCODE,12                                          0040 00091000
*                  ELSE                     /*OK, CALL LNNOPROC      */ 00092000
*                    GEN (BAL RBRANCH,LNNOPROC)                    0041 00093000
*                       SETS (RBRANCH) REFS (LNNOPROC);            0041 00094000
         B     @RC00039                                            0041 00095000
@RF00039 DS    0H                                                  0041 00096000
         BAL RBRANCH,LNNOPROC                                           00097000
*                ELSE                       /*MUST BE SMALL SCREEN   */ 00098000
*                  IF RPARM1 > MAXLNSM THEN /*TOO LARGE, SET RETCODE */ 00099000
         B     @RC00038                                            0042 00100000
@RF00038 LA    @10,12                                              0042 00101000
         CLR   RPARM1,@10                                          0042 00102000
         BNH   @RF00042                                            0042 00103000
*                    RETCODE = CODE12;                             0043 00104000
         LR    RETCODE,@10                                         0043 00105000
*                  ELSE                     /*OK, CALL LNNOPROC      */ 00106000
*                    GEN (BAL RBRANCH,LNNOPROC)                    0044 00107000
*                       SETS (RBRANCH) REFS (LNNOPROC);            0044 00108000
         B     @RC00042                                            0044 00109000
@RF00042 DS    0H                                                  0044 00110000
         BAL RBRANCH,LNNOPROC                                           00111000
*            END;                                                  0045 00112000
*        RETURN;                                           /*@ZM20487*/ 00113000
@EL00001 DS    0H                                                  0046 00114000
@EF00001 DS    0H                                                  0046 00115000
@ER00001 BR    @14                                                 0046 00116000
*        /************************************************************/ 00117000
*        /*                                                          */ 00118000
*        /* LNNOPROC: STORES LINE NUMBER IN TVWALNCT.  SETS FULL     */ 00119000
*        /*           SCREEN MODE ON OR OFF BASED ON THE MODE BIT    */ 00120000
*        /*           IN PARAMETER REGISTER 1.                       */ 00121000
*        /*                                                          */ 00122000
*        /************************************************************/ 00123000
*LNNOPROC:                                                         0047 00124000
*        REG3 = TSBXTVWA;              /*GET ADDRES OF TVWA          */ 00125000
LNNOPROC L     @10,TSBEXTNT(,TSBPTR)                               0047 00126000
         L     REG3,TSBXTVWA(,@10)                                 0047 00127000
*        RFY TVWA BASED(REG3);         /*REBASE TVWA                 */ 00128000
*        GEN (STCM RPARM1,1,TVWALNCT(REG3))                        0049 00129000
*           REFS (RPARM1) SETS (TVWALNCT); /*STORE LINE NUM IN TVWA  */ 00130000
         STCM RPARM1,1,TVWALNCT(REG3)                                   00131000
*        IF REG2A < 0 THEN             /*HIGH ORDER BIT ON?          */ 00132000
         LTR   REG2A,REG2A                                         0050 00133000
         BNM   @RF00050                                            0050 00134000
*          DO;                                                     0051 00135000
*            TVWAFSM = '1'B;           /*YES-SET FULL SCREEN MODE ON */ 00136000
         OI    TVWAFSM(REG3),B'01000000'                           0052 00137000
*          END;                                                    0053 00138000
*        ELSE                          /*HIGH ORDER BIT OFF          */ 00139000
*          DO;                         /*TURN OFF FULL SCR MODE INDIC*/ 00140000
         B     @RC00050                                            0054 00141000
@RF00050 DS    0H                                                  0055 00142000
*            TVWAFSM = '0'B;           /*FULL SCREEN MODE            */ 00143000
*            TVWAWO  = '0'B;           /*FULL SCREEN WRITTEN OVER    */ 00144000
*            TVWAFSW = '0'B;           /*FULL SCREEN TPUT WAITING    */ 00145000
         NI    TVWAFSM(REG3),B'10001111'                           0057 00146000
*          END;                                                    0058 00147000
*        /************************************************************/ 00148000
*        /*                                                          */ 00149000
*        /* RETURN TO MAINLINE WITH RETURN CODE 0                    */ 00150000
*        /*                                                          */ 00151000
*        /************************************************************/ 00152000
*        RETCODE = CODE00;             /*SET GOOD RETURN CODE        */ 00153000
@RC00050 SLR   RETCODE,RETCODE                                     0059 00154000
*        GEN (BR RBRANCH)                                          0060 00155000
*           REFS (RBRANCH);            /*RETURN TO MAINLINE          */ 00156000
         BR RBRANCH                                                     00157000
*        END IKT09413                                              0061 00158000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.      * 00159000
*/*%INCLUDE SYSLIB  (IKJTSB  )                                        * 00160000
*/*%INCLUDE SYSLIB  (IKTTSBX )                                        * 00161000
*/*%INCLUDE SYSLIB  (IKTTVWA )                                        * 00162000
*;                                                                 0061 00163000
         B     @EL00001                                            0061 00164000
@DATA    DS    0H                                                       00165000
