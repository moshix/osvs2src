         TITLE '*** IKT3270I: INPUT DATA HANDLING ROUTINE ***          *00001000
                        '                                               00002000
IKT3270I CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKT3270I  78.045'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @05,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@05                                         0001 00013000
*  TVWAPTR=TSBXTVWA;                   /* ADDRESSING TO TVWA @G58AK3B*/ 00014000
         L     @12,PSAAOLD                                         0111 00015000
         L     @12,ASCBTSB(,@12)                                   0111 00016000
         L     @07,TSBEXTNT(,@12)                                  0111 00017000
         L     TVWAPTR,TSBXTVWA(,@07)                              0111 00018000
*  TIMGETMN = TVWATIMW;                /* TIM WORK AREA ADDR @G58AK3B*/ 00019000
         L     TIMGETMN,TVWATIMW(,TVWAPTR)                         0112 00020000
*  TIMLNNO = MAX(2,MIN(TSBLNNO,PHYSLNNO));                 /*@ZA13627*/ 00021000
*                                                                  0113 00022000
         SLR   @08,@08                                             0113 00023000
         IC    @08,TSBLNNO(,@12)                                   0113 00024000
         L     @07,PHYSLNNO(,TIMGETMN)                             0113 00025000
         CR    @08,@07                                             0113 00026000
         BNH   *+6                                                      00027000
         LR    @08,@07                                             0113 00028000
         LA    @07,2                                               0113 00029000
         CR    @08,@07                                             0113 00030000
         BNL   *+6                                                      00031000
         LR    @08,@07                                             0113 00032000
         ST    @08,TIMLNNO(,TIMGETMN)                              0113 00033000
*  IF TSBIFLSH='1'B THEN               /* THROW DATA AWAY?           */ 00034000
         TM    TSBIFLSH(@12),B'00001000'                           0114 00035000
         BNO   @RF00114                                            0114 00036000
*   DO;                                                            0115 00037000
*     IF RPLRLEN > BUFLEN |            /* CHECK ANOTHER RECEIVE NEC. */ 00038000
*        RPLFIRST = '1'B |                                 /*@G58AK3B*/ 00039000
*        RPLMIDLE = '1'B THEN                              /*@G58AK3B*/ 00040000
         LA    @12,2348                                            0116 00041000
         LR    @08,TIMGETMN                                        0116 00042000
         ALR   @08,@12                                             0116 00043000
         CLC   RPLRLEN(4,@08),@CF02152                             0116 00044000
         BH    @RT00116                                            0116 00045000
         ALR   @12,TIMGETMN                                        0116 00046000
         TM    RPLFIRST+16(@12),B'11000000'                        0116 00047000
         BZ    @RF00116                                            0116 00048000
@RT00116 DS    0H                                                  0117 00049000
*       REC2FLG = '1'B;                /* INDICATE NEXT RECEIVE NECES*/ 00050000
         OI    REC2FLG(TIMGETMN),B'00000010'                       0117 00051000
*     ELSE                             /* OTHERWISE SCHEDULE TOM     */ 00052000
*       DO;                                                        0118 00053000
         B     @RC00116                                            0118 00054000
@RF00116 DS    0H                                                  0119 00055000
*         REC2FLG='0'B;                /* NO 2ND RECEIVE NECESSARY   */ 00056000
*         TIMSCHTM='1'B;               /* SCHEDULE TOM               */ 00057000
         OI    TIMSCHTM(TIMGETMN),B'01000000'                      0120 00058000
         NI    REC2FLG(TIMGETMN),B'11111101'                       0120 00059000
*         TVWANOFB = '0'B;             /* RESET NO FLASHBACK @G38AK3B*/ 00060000
         NI    TVWANOFB(TVWAPTR),B'11101111'                       0121 00061000
*         TVWABKMG='0'B;               /* NO FLASHBACK       @ZM20856*/ 00062000
         NI    TVWABKMG(TVWAPTR),B'11111011'                       0122 00063000
*         TVWATQL1=0;                  /* CLEAR FLASHBACK 1  @ZM20856*/ 00064000
         MVI   TVWATQL1(TVWAPTR),X'00'                             0123 00065000
*         TVWATQL2=0;                  /* CLEAR FLASHBACK 2  @ZM20856*/ 00066000
         MVI   TVWATQL2(TVWAPTR),X'00'                             0124 00067000
*       END;                                                       0125 00068000
*   END;                               /* END DO FOR TSBIFLSH='1'    */ 00069000
*  ELSE                                                            0127 00070000
*      DO;                                                         0127 00071000
         B     @RC00114                                            0127 00072000
@RF00114 DS    0H                                                  0128 00073000
*      IF TVWAPGN='0'B THEN            /* NO PAGING?                 */ 00074000
         TM    TVWAPGN(TVWAPTR),B'01000000'                        0128 00075000
         BNZ   @RF00128                                            0128 00076000
*      DO;                             /* NO PAGING                  */ 00077000
*        IF RPLCSI='1'B   THEN         /* CHECK FOR ASCII INPUT      */ 00078000
         LA    @12,2348                                            0130 00079000
         ALR   @12,TIMGETMN                                        0130 00080000
         TM    RPLCSI+16(@12),B'00001000'                          0130 00081000
         BNO   @RF00130                                            0130 00082000
*         CALL ASCITRAN;               /* CALL ROUTINE TO CONVERT INP*/ 00083000
         BAL   @14,ASCITRAN                                        0131 00084000
*        ELSE;                         /* DO NOTHING IF ALREADY EBCD */ 00085000
*                                                                  0132 00086000
@RF00130 DS    0H                                                  0133 00087000
*        /************************************************************/ 00088000
*        /*  THE FOLLOWING CODE CHECKS THE AVAILIBILITY OF A USER    */ 00089000
*        /*  EXIT. IF THE ADDRESS OF IKTIDSX2 IS NOT ZERO, THEN THE  */ 00090000
*        /*  USER IS GIVEN CONTROL WITH REG0 CONTAINING THE ADDRESS  */ 00091000
*        /*  OF THE DATA TO BE SCANNED AND REG1 CONTAINING THE       */ 00092000
*        /*  LENGTH OF THAT DATA.                                    */ 00093000
*        /************************************************************/ 00094000
*                                                                  0133 00095000
*        IF ADDR(IKTIDSX2)^=0 THEN                                 0133 00096000
         L     @12,@CV02178                                        0133 00097000
         LTR   @12,@12                                             0133 00098000
         BZ    @RF00133                                            0133 00099000
*         DO;                                                      0134 00100000
*           RFY (REG0,REG1,REG2) RSTD; /* RESTRICT WORK REGS         */ 00101000
*           REG0=RPLAREA;              /* DATA TO BE TRANSLATED      */ 00102000
         LA    @12,2348                                            0136 00103000
         LR    @08,TIMGETMN                                        0136 00104000
         ALR   @08,@12                                             0136 00105000
         L     REG0,RPLAREA(,@08)                                  0136 00106000
*           IF RPLRLEN > BUFLEN THEN                               0137 00107000
         LA    @08,1500                                            0137 00108000
         ALR   @12,TIMGETMN                                        0137 00109000
         C     @08,RPLRLEN(,@12)                                   0137 00110000
         BNL   @RF00137                                            0137 00111000
*             REG1=BUFLEN;             /* SCAN MAX BUFFER LENGTH     */ 00112000
         LR    REG1,@08                                            0138 00113000
*           ELSE                       /* OTHERWISE SCAN ALL DATA    */ 00114000
*             REG1=RPLRLEN;                                        0139 00115000
         B     @RC00137                                            0139 00116000
@RF00137 LA    @12,2348                                            0139 00117000
         ALR   @12,TIMGETMN                                        0139 00118000
         L     REG1,RPLRLEN(,@12)                                  0139 00119000
*           REG2 = REG13;              /* SAVE REGISTER 13   @G58AK3B*/ 00120000
@RC00137 LR    REG2,REG13                                          0140 00121000
*           REG13 = ADDR(SAREA2);      /* SEC SAVE AREA      @G58AK3B*/ 00122000
         LA    REG13,SAREA2(,TIMGETMN)                             0141 00123000
*           CALL IKTIDSX2;             /* CALL USER TRANSLATION RTN  */ 00124000
         L     @15,@CV02178                                        0142 00125000
         BALR  @14,@15                                             0142 00126000
*           REG13 = REG2;              /* RESTORE REG 13     @G58AK3B*/ 00127000
         LR    REG13,REG2                                          0143 00128000
*           RFY (REG0,REG1,REG2) UNRSTD;                           0144 00129000
*         END;                                                     0145 00130000
*        ELSE;                                                     0146 00131000
@RF00133 DS    0H                                                  0147 00132000
*/********************************************************************/ 00133000
*/*    AT THIS POINT THE INPUT DATA HAS BEEN TRANSLATED IF NECESSARY */ 00134000
*/*    NOW THE DATA WILL BE SCANNED FOR INPUT LINE DELIMITERS.       */ 00135000
*/*            THE DATA WILL BE BROKEN INTO LINE SEGMENTS AND        */ 00136000
*/*    PLACED ON THE INPUT QUEUE.                                    */ 00137000
*/********************************************************************/ 00138000
*                                                                  0147 00139000
*        /************************************************************/ 00140000
*        /* THIS CODE WILL HELP TOM IN THE PLACEMENT OF THE NEXT     */ 00141000
*        /* ATTRIBUTE BYTE WHEN VTIOC IS DOING THE SCREEN MANAGEMENT */ 00142000
*        /************************************************************/ 00143000
*                                                                  0147 00144000
*        IF REC2FLG='0'B & TVWASCAN='1'B THEN                      0147 00145000
         TM    REC2FLG(TIMGETMN),B'00000010'                       0147 00146000
         BNZ   @RF00147                                            0147 00147000
         TM    TVWASCAN(TVWAPTR),B'10000000'                       0147 00148000
         BNO   @RF00147                                            0147 00149000
*          TVWADARC='1'B;              /* SET DATA FOUND BIT @ZA19883*/ 00150000
         OI    TVWADARC(TVWAPTR),B'00000001'                       0148 00151000
*        ELSE;                         /* 2ND RECEIVE OR TVWASCAN OFF*/ 00152000
*                                                                  0149 00153000
@RF00147 DS    0H                                                  0150 00154000
*        SCANPTR=RPLAREA;              /* INITIALIZE SCAN AREA       */ 00155000
         LA    @12,2348                                            0150 00156000
         LR    @08,TIMGETMN                                        0150 00157000
         ALR   @08,@12                                             0150 00158000
         L     SCANPTR,RPLAREA(,@08)                               0150 00159000
*        DONECNT=0;                    /* NUMBER BYTES DATA PROCESSED*/ 00160000
         SLR   @08,@08                                             0151 00161000
         ST    @08,DONECNT(,TIMGETMN)                              0151 00162000
*        DATALINE=0;                   /* INPUT LINE LENGTH          */ 00163000
         ST    @08,DATALINE(,TIMGETMN)                             0152 00164000
*        IF RPLRLEN > BUFLEN THEN      /* IS ALL DATA IN BUFFER?     */ 00165000
         LA    @08,1500                                            0153 00166000
         ALR   @12,TIMGETMN                                        0153 00167000
         C     @08,RPLRLEN(,@12)                                   0153 00168000
         BNL   @RF00153                                            0153 00169000
*          COUNT=BUFLEN;               /* NO, SCAN BUFFER LENGTH     */ 00170000
         ST    @08,COUNT(,TIMGETMN)                                0154 00171000
*        ELSE                                                      0155 00172000
*          COUNT=RPLRLEN;              /* YES, SCAN DATA LENGTH      */ 00173000
         B     @RC00153                                            0155 00174000
@RF00153 LA    @12,2348                                            0155 00175000
         ALR   @12,TIMGETMN                                        0155 00176000
         L     @12,RPLRLEN(,@12)                                   0155 00177000
         ST    @12,COUNT(,TIMGETMN)                                0155 00178000
*        IF REC2FLG = '0'B THEN                            /*@ZA13627*/ 00179000
@RC00153 TM    REC2FLG(TIMGETMN),B'00000010'                       0156 00180000
         BNZ   @RF00156                                            0156 00181000
*          CALL HEADPROC;              /* CALL HEADER PROCESS RTN    */ 00182000
         BAL   @14,HEADPROC                                        0157 00183000
*        ELSE;                                             /*@ZA13627*/ 00184000
@RF00156 DS    0H                                                  0159 00185000
         EJECT                                                          00186000
*      /**************************************************************/ 00187000
*      /*  NOW THE DATA WILL BE SCANNED FOR THE LINE LENGTH          */ 00188000
*      /*  WHEN THE LINE LENGTH IS FOUND, THE DATA WILL BE PUT ON    */ 00189000
*      /*  THE INPUT QUEUE PRECEDED BY THE HEADER INFORMATION.       */ 00190000
*      /*  SINCE DATA CAN ONLY BE SCANNED IN 256 BYTE LENGTHS,       */ 00191000
*      /*  THIS VALUE WILL BE USED WHEN THE REMAINING DATA EXCEEDS   */ 00192000
*      /*  THE 256 BYTE MAXIMUM.                                     */ 00193000
*      /**************************************************************/ 00194000
*                                                                  0159 00195000
*        DO UNTIL DONECNT=COUNT  | TIMADDER='1'B;                  0159 00196000
@DL00159 DS    0H                                                  0160 00197000
*          RFY SCANAREA BASED(SCANPTR); /* BUFFER AREA POINTER       */ 00198000
*          RFY (REG1,REG2) RSTD;       /* USED BY TRT BUILT-IN FUNC  */ 00199000
*          DELIMFLG='0'B;              /* INIT DELIM FLAG            */ 00200000
         NI    DELIMFLG(TIMGETMN),B'01111111'                      0162 00201000
*          REG1=0;                     /* INITIALIZE REG 1 FOR SCAN  */ 00202000
         SLR   REG1,REG1                                           0163 00203000
*          BUFPTR=SCANPTR;             /* RESET BUFFER POINTER       */ 00204000
         ST    SCANPTR,BUFPTR(,TIMGETMN)                           0164 00205000
*          DO UNTIL DELIMFLG='1'B | DONECNT=COUNT;                 0165 00206000
@DL00165 DS    0H                                                  0166 00207000
*           /* SET UP VALUE FOR TRT LENGTH. LIMIT TO 256             */ 00208000
*           IF COUNT-DONECNT > 256 THEN                            0166 00209000
         LA    @12,256                                             0166 00210000
         L     @08,COUNT(,TIMGETMN)                                0166 00211000
         SL    @08,DONECNT(,TIMGETMN)                              0166 00212000
         CR    @08,@12                                             0166 00213000
         BNH   @RF00166                                            0166 00214000
*            VALUE=256;                /* SCAN VALUE AT MAX          */ 00215000
         ST    @12,VALUE(,TIMGETMN)                                0167 00216000
*           ELSE                                                   0168 00217000
*            VALUE=COUNT-DONECNT;      /* ELSE SCAN ALL DATA         */ 00218000
         B     @RC00166                                            0168 00219000
@RF00166 L     @12,COUNT(,TIMGETMN)                                0168 00220000
         SL    @12,DONECNT(,TIMGETMN)                              0168 00221000
         ST    @12,VALUE(,TIMGETMN)                                0168 00222000
*           IF VALUE >0 THEN           /* CHECK FOR NULL LINE        */ 00223000
@RC00166 L     @12,VALUE(,TIMGETMN)                                0169 00224000
         LTR   @12,@12                                             0169 00225000
         BNP   @RF00169                                            0169 00226000
*           DO;                                                    0170 00227000
*           TRT(SCANAREA(1:VALUE),NEWLINE(1)); /* TRANSLATE DATA     */ 00228000
         L     @03,VALUE(,TIMGETMN)                                0171 00229000
         BCTR  @03,0                                               0171 00230000
         EX    @03,@SB02264                                        0171 00231000
*           BC(8,NODELIM);             /* NO DELIMITER FOUND         */ 00232000
         BC    8,NODELIM                                           0172 00233000
*           DELIMFLG='1'B;             /* TURN ON DELIMITER FOUND FLG*/ 00234000
         OI    DELIMFLG(TIMGETMN),B'10000000'                      0173 00235000
*           DATALINE=(REG1-SCANPTR)+DATALINE; /* LINE SIZE           */ 00236000
         LR    @12,REG1                                            0174 00237000
         SLR   @12,SCANPTR                                         0174 00238000
         LR    @08,@12                                             0174 00239000
         AL    @08,DATALINE(,TIMGETMN)                             0174 00240000
         ST    @08,DATALINE(,TIMGETMN)                             0174 00241000
*           VALUE=REG1-SCANPTR;        /* RESET VALUE                */ 00242000
         ST    @12,VALUE(,TIMGETMN)                                0175 00243000
*           BC(15,DELIM);              /* SKIP TO DELIMITER FND CDE  */ 00244000
         BC    15,DELIM                                            0176 00245000
*           END;                       /* END VALUE GREATER THAN 0   */ 00246000
*           ELSE;                                                  0178 00247000
@RF00169 DS    0H                                                  0179 00248000
*        NODELIM:                                                  0179 00249000
*           DATALINE=VALUE+DATALINE;   /* SIZE OF DATA LINE          */ 00250000
NODELIM  L     @12,VALUE(,TIMGETMN)                                0179 00251000
         AL    @12,DATALINE(,TIMGETMN)                             0179 00252000
         ST    @12,DATALINE(,TIMGETMN)                             0179 00253000
*        DELIM:                                                    0180 00254000
*           SCANPTR=SCANPTR+VALUE;     /* UPDATE SCAN POINTER        */ 00255000
DELIM    L     @12,VALUE(,TIMGETMN)                                0180 00256000
         ALR   SCANPTR,@12                                         0180 00257000
*           DONECNT=DONECNT+VALUE;     /* UPDATE DATA PROCESSED COUNT*/ 00258000
         AL    @12,DONECNT(,TIMGETMN)                              0181 00259000
         ST    @12,DONECNT(,TIMGETMN)                              0181 00260000
*          END;                        /* END OF DO WHILE LOOP       */ 00261000
@DE00165 TM    DELIMFLG(TIMGETMN),B'10000000'                      0182 00262000
         BO    @DC00165                                            0182 00263000
         CLC   DONECNT(4,TIMGETMN),COUNT(TIMGETMN)                 0182 00264000
         BNE   @DL00165                                            0182 00265000
@DC00165 DS    0H                                                  0183 00266000
*          RFY (REG1,REG2) UNRSTD;     /* FREE REGISTERS 1 AND 2     */ 00267000
*                                                                  0183 00268000
*         /***********************************************************/ 00269000
*         /* THIS MARKS THE END OF THE DO WHILE TO SCAN FOR DELIMITER*/ 00270000
*         /* EITHER A DELIMITER WAS FOUND OR ALL THE DATA IN THE     */ 00271000
*         /* BUFFER WAS PROCESSED.                                   */ 00272000
*         /***********************************************************/ 00273000
*                                                                  0184 00274000
*         IF DELIMFLG='1'B THEN        /* CHECK FOR A DELIMITER FOUND*/ 00275000
         TM    DELIMFLG(TIMGETMN),B'10000000'                      0184 00276000
         BNO   @RF00184                                            0184 00277000
*          DO;                                                     0185 00278000
*            IF IPPRTL='1'B THEN       /* CHECK FOR PRIOR PARTIAL    */ 00279000
         TM    IPPRTL(TIMGETMN),B'00100000'                        0186 00280000
         BNO   @RF00186                                            0186 00281000
*              PRIORPRT='1'B;          /* SET FLAG ACCORDINGLY       */ 00282000
         OI    PRIORPRT(TIMGETMN),B'00000001'                      0187 00283000
*            ELSE;                                                 0188 00284000
@RF00186 DS    0H                                                  0189 00285000
*            IPAVBL='1'B;              /* MARK LINE AVAILABLE        */ 00286000
*            IPPRTL='0'B;              /* LINE NOT PARTIAL LINE      */ 00287000
         OI    IPAVBL(TIMGETMN),B'10000000'                        0190 00288000
         NI    IPPRTL(TIMGETMN),B'11011111'                        0190 00289000
*            IF PRIORPRT='0'B THEN     /* INCLUDE HEADER FOR NEW LINE*/ 00290000
         TM    PRIORPRT(TIMGETMN),B'00000001'                      0191 00291000
         BNZ   @RF00191                                            0191 00292000
*            DO;                                                   0192 00293000
*            IPBFSZ=HEADCNT+DATALINE;  /* SET LINE LENGTH FOR Q MGR  */ 00294000
         LH    @12,HEADCNT(,TIMGETMN)                              0193 00295000
         LR    @08,@12                                             0193 00296000
         AL    @08,DATALINE(,TIMGETMN)                             0193 00297000
         STH   @08,IPBFSZ(,TIMGETMN)                               0193 00298000
*            IPCCC=HEADCNT;            /* SIZE OF  HEADER            */ 00299000
         STC   @12,IPCCC(,TIMGETMN)                                0194 00300000
*            END;                                                  0195 00301000
*            ELSE                      /* NO HEADER FOR PREVIOUS PRTL*/ 00302000
*            DO;                                                   0196 00303000
         B     @RC00191                                            0196 00304000
@RF00191 DS    0H                                                  0197 00305000
*            IPBFSZ=DATALINE;          /* ONLY DATA ON QUEUE         */ 00306000
         L     @12,DATALINE(,TIMGETMN)                             0197 00307000
         STH   @12,IPBFSZ(,TIMGETMN)                               0197 00308000
*            IPCCC=0;                  /* HEADER VALUE SET TO ZERO   */ 00309000
         MVI   IPCCC(TIMGETMN),X'00'                               0198 00310000
*            END;                                                  0199 00311000
*            IF SCANAREA(1)=FIELDMRK THEN                          0200 00312000
@RC00191 CLI   SCANAREA(SCANPTR),X'1E'                             0200 00313000
         BNE   @RF00200                                            0200 00314000
*              DONECNT=DONECNT+1;      /* UPDATE DATA SCANNED NUMBER */ 00315000
         LA    @12,1                                               0201 00316000
         AL    @12,DONECNT(,TIMGETMN)                              0201 00317000
         ST    @12,DONECNT(,TIMGETMN)                              0201 00318000
*            ELSE;                                                 0202 00319000
@RF00200 DS    0H                                                  0203 00320000
*            CALL ADDELRTN;            /* ADD ELEMENT TO INPUT QUEUE */ 00321000
         BAL   @14,ADDELRTN                                        0203 00322000
*          END;                        /* END DELIMITER FOUND PROC   */ 00323000
*         ELSE                                                     0205 00324000
*             DO;                      /* NO DELIMITER FOUND         */ 00325000
         B     @RC00184                                            0205 00326000
@RF00184 DS    0H                                                  0206 00327000
*               IF IPPRTL='1'B THEN    /* CHECK FOR PREVIOUS PRTL    */ 00328000
         TM    IPPRTL(TIMGETMN),B'00100000'                        0206 00329000
         BNO   @RF00206                                            0206 00330000
*                 PRIORPRT='1'B;       /* SET INDICATOR IF YES       */ 00331000
         OI    PRIORPRT(TIMGETMN),B'00000001'                      0207 00332000
*               ELSE;                                              0208 00333000
@RF00206 DS    0H                                                  0209 00334000
*               IF PRIORPRT='0'B THEN  /* CHECK FOR PARTIAL LINE     */ 00335000
         TM    PRIORPRT(TIMGETMN),B'00000001'                      0209 00336000
         BNZ   @RF00209                                            0209 00337000
*               DO;                                                0210 00338000
*               IPBFSZ=HEADCNT+DATALINE; /* SIZE OF ELEMENT NEEDED   */ 00339000
         LH    @12,HEADCNT(,TIMGETMN)                              0211 00340000
         LR    @08,@12                                             0211 00341000
         AL    @08,DATALINE(,TIMGETMN)                             0211 00342000
         STH   @08,IPBFSZ(,TIMGETMN)                               0211 00343000
*               IPCCC=HEADCNT;         /* PART OF LINE THAT IS HEADER*/ 00344000
         STC   @12,IPCCC(,TIMGETMN)                                0212 00345000
*               END;                   /* END FULL LINE PROCESS      */ 00346000
*               ELSE                   /* PARTIAL LINE PROCESS       */ 00347000
*                 DO;                                              0214 00348000
         B     @RC00209                                            0214 00349000
@RF00209 DS    0H                                                  0215 00350000
*                   IPCCC=0;           /* DO NOT PROCESS HEADER      */ 00351000
         MVI   IPCCC(TIMGETMN),X'00'                               0215 00352000
*                   IPBFSZ=DATALINE;   /* SIZE OF Q MANAGER BUFFER   */ 00353000
         L     @12,DATALINE(,TIMGETMN)                             0216 00354000
         STH   @12,IPBFSZ(,TIMGETMN)                               0216 00355000
*                 END;                                             0217 00356000
*               IF RPLRLEN > COUNT |     /* MORE DATA?       @ZM20581*/ 00357000
*                  RPLFIRST='1'B |       /* BOC?             @ZM20581*/ 00358000
*                  RPLMIDLE='1'B THEN    /* MOC?             @ZM20581*/ 00359000
@RC00209 LA    @12,2348                                            0218 00360000
         LR    @08,TIMGETMN                                        0218 00361000
         ALR   @08,@12                                             0218 00362000
         L     @08,RPLRLEN(,@08)                                   0218 00363000
         C     @08,COUNT(,TIMGETMN)                                0218 00364000
         BH    @RT00218                                            0218 00365000
         ALR   @12,TIMGETMN                                        0218 00366000
         TM    RPLFIRST+16(@12),B'11000000'                        0218 00367000
         BZ    @RF00218                                            0218 00368000
@RT00218 DS    0H                                                  0219 00369000
*                 DO;                                              0219 00370000
*                   DONECNT=COUNT;     /* PROCESSING FINISHED        */ 00371000
         L     @12,COUNT(,TIMGETMN)                                0220 00372000
         ST    @12,DONECNT(,TIMGETMN)                              0220 00373000
*                   IPAVBL='0'B;       /* LINE NOT AVAILABLE FOR TGET*/ 00374000
*                   IPPRTL='1'B;       /* INPUT NOT A FULL LINE      */ 00375000
         OI    IPPRTL(TIMGETMN),B'00100000'                        0222 00376000
         NI    IPAVBL(TIMGETMN),B'01111111'                        0222 00377000
*                 END;                                             0223 00378000
*               ELSE                                               0224 00379000
*                 DO;                                              0224 00380000
         B     @RC00218                                            0224 00381000
@RF00218 DS    0H                                                  0225 00382000
*                   IPAVBL='1'B;       /* LINE IS AVAILABLE          */ 00383000
*                   IPPRTL='0'B;       /* INPUT IS A FULL LINE       */ 00384000
         OI    IPAVBL(TIMGETMN),B'10000000'                        0226 00385000
         NI    IPPRTL(TIMGETMN),B'11011111'                        0226 00386000
