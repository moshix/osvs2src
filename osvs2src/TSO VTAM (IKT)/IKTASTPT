*DATAEDIT:PROC OPTIONS(NOSAVEAREA,NOSAVE);                         0313 00804000
DATAEDIT DS    0H                                                  0314 00805000
*/*                                                                  */ 00806000
*/********************************************************************/ 00807000
*/*                                                                  */ 00808000
*/*      DATA EDIT IS AN INTERNAL PROCEDURE THAT SCANS THE           */ 00809000
*/*      CALLER'S DATA AND REMOVES TRAILING BLANKS ADJUSTING         */ 00810000
*/*      THE BYTE COUNT ACCORDINGLY.                                 */ 00811000
*/*    **** DATAEDIT SAVES RETURN ADDRESS INTO THE LEVEL 'A'         */ 00812000
*/*         RETURN 'SAVE WORD' - XSASAVEA.                           */ 00813000
*/*                                                                  */ 00814000
*/********************************************************************/ 00815000
*/*                                                                  */ 00816000
*DCL I REG(1);                                                     0314 00817000
*DCL BLANK CHAR(1) CONSTANT(' ');                                  0315 00818000
*RFY RTNREG RSTD;                                                  0316 00819000
*XSASAVEA = RTNREG;                                                0317 00820000
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0317 00821000
*RFY RTNREG UNRSTD;                                                0318 00822000
*IF XSAEDITO^= '00'B THEN;           /* IF NOT EDIT, NO-OP           */ 00823000
         TM    XSAEDITO(RBSECPTR),B'00000011'                      0319 00824000
         BNZ   @RT00319                                            0319 00825000
*ELSE                                                              0321 00826000
*  DO;                               /* EDIT OPTION-REMOVE TRAILING     00827000
*                                       BLANKS                       */ 00828000
*    XSADMOVE = '1'B;                /* SET DATA MOVE INDICATOR      */ 00829000
         OI    XSADMOVE(RBSECPTR),B'01000000'                      0322 00830000
*    RFY REG2 RSTD;                                                0323 00831000
*    REG2 = RBLINKB -> RBOPSWB2;     /* POINT TO CALLERS KEY         */ 00832000
         L     @14,RBLINKB-1(,RBSECPTR)                            0324 00833000
         SLR   REG2,REG2                                           0324 00834000
         IC    REG2,RBOPSWB2(,@14)                                 0324 00835000
*    GEN SETS(REG2);                                               0325 00836000
        MODESET KEYADDR=(2),WORKREG=2                                   00837000
*    RFY REG2 UNRSTD;                                              0326 00838000
*    RFY I RSTD;                                                   0327 00839000
*    DO I = XSAPRMSZ TO 2 BY -1      /* SCAN FROM END OF DATA        */ 00840000
*       WHILE TPUTDATA(I) = BLANK;   /* CONTINUE WHILE BLANK         */ 00841000
         LH    I,XSAPRMSZ(,RBSECPTR)                               0328 00842000
         B     @DE00328                                            0328 00843000
@DL00328 L     @14,XSABFRAD-1(,RBSECPTR)                           0328 00844000
         ALR   @14,I                                               0328 00845000
         BCTR  @14,0                                               0328 00846000
         CLI   TPUTDATA(@14),C' '                                  0328 00847000
         BNE   @DC00328                                            0328 00848000
*    END;                                                          0329 00849000
         BCTR  I,0                                                 0329 00850000
@DE00328 C     I,@CF00072                                          0329 00851000
         BNL   @DL00328                                            0329 00852000
@DC00328 DS    0H                                                  0330 00853000
*    RFY REG2 RSTD;                                                0330 00854000
*    GEN SETS(REG2)                                                0331 00855000
*        REFS(RBLINK,RBOPSW,RBBASIC,TCBRBP);                       0331 00856000
        MODESET EXTKEY=ZERO GET BACK INTO KEY 0                         00857000
*    RFY REG2 UNRSTD;                                              0332 00858000
*    XSADMOVE = '0'B;                /* CLEAR DATA MOVE INDICATOR    */ 00859000
         NI    XSADMOVE(RBSECPTR),B'10111111'                      0333 00860000
*    XSAPRMSZ = I;                   /* UPDATE TPUT SIZE. MIN VALUE     00861000
*                                       IS 1, EVEN IF WHOLE TPUT IS     00862000
*                                       BLANK                        */ 00863000
         STH   I,XSAPRMSZ(,RBSECPTR)                               0334 00864000
*    RFY I UNRSTD;                                                 0335 00865000
*  END;                              /* END OF EDIT DO GROUP         */ 00866000
*RFY RTNREG RSTD;                                                  0337 00867000
@RT00319 DS    0H                                                  0338 00868000
*RTNREG = XSASAVEA;                  /* RESTORE RETURN ADDRESS       */ 00869000
         L     RTNREG,XSASAVEA(,RBSECPTR)                          0338 00870000
*RETURN;                                                           0339 00871000
@EL00005 DS    0H                                                  0339 00872000
@EF00005 DS    0H                                                  0339 00873000
@ER00005 BR    @14                                                 0339 00874000
*RFY RTNREG UNRSTD;                                                0340 00875000
*END DATAEDIT;                                                     0341 00876000
*VERIFY: PROC OPTIONS(NOSAVEAREA,NOSAVE);                          0342 00877000
VERIFY   DS    0H                                                  0343 00878000
*/*                                                                  */ 00879000
*/********************************************************************/ 00880000
*/*                                                                  */ 00881000
*/*      THIS TESTS IF CALLER'S DATA ADDRESS IS IN THE CALLER'S      */ 00882000
*/*      KEY. IT DOES SO BY REFERENCING THE FIRST BYTE OF EACH       */ 00883000
*/*      PAGE REQUIRED FOR THE TPUT.                                 */ 00884000
*/*    ***VERIFY SAVES RETURN ADDRESS INTO THE LEVEL 'B' RETURN      */ 00885000
*/*       'SAVE WORD' - XSASAVEB.                                    */ 00886000
*/*                                                                  */ 00887000
*/********************************************************************/ 00888000
*/*                                                                  */ 00889000
*DCL APAGE FIXED(15) CONSTANT(4096); /* PAGE SIZE                    */ 00890000
*DCL MODULUS FIXED(8) CONSTANT(12);  /* MODULUS OF A VS2 PAGE IS   0344 00891000
*                                       2**12=4096                   */ 00892000
*RFY RTNREG RSTD;                                                  0345 00893000
*XSASAVEB = RTNREG;                  /* SAVE RETURN ADDRESS          */ 00894000
         ST    RTNREG,XSASAVEB(,RBSECPTR)                          0346 00895000
*RFY RTNREG UNRSTD;                                                0347 00896000
*XSADMOVE = '1'B;                    /* INDICATE 'DATA VERIFICATION     00897000
*                                       UNDERWAY'                    */ 00898000
*                                                                  0348 00899000
         OI    XSADMOVE(RBSECPTR),B'01000000'                      0348 00900000
*/********************************************************************/ 00901000
*/*    GET INTO CALLER'S KEY TO VALIDATE DATA                        */ 00902000
*/********************************************************************/ 00903000
*RFY REG2 RSTD;                                                    0349 00904000
*REG2 = RBLINKB -> RBOPSWB2;         /* POINT TO KEY OF CALLER       */ 00905000
         L     @14,RBLINKB-1(,RBSECPTR)                            0350 00906000
         SLR   REG2,REG2                                           0350 00907000
         IC    REG2,RBOPSWB2(,@14)                                 0350 00908000
*GEN SETS(REG2);                                                   0351 00909000
        MODESET KEYADDR=(2),WORKREG=2                                   00910000
*RFY REG2 UNRSTD;                                                  0352 00911000
*RFY WORK1REG RSTD;                                                0353 00912000
*WORK1REG = XSABFRAD;                /* GET INPUT BUFFER ADDRESS     */ 00913000
         L     WORK1REG,XSABFRAD-1(,RBSECPTR)                      0354 00914000
         LA    WORK1REG,0(,WORK1REG)                               0354 00915000
*SRL(WORK1REG,MODULUS);              /* SHIFT OFF PARTIAL PAGE       */ 00916000
         SRL   WORK1REG,12                                         0355 00917000
*SLL(WORK1REG,MODULUS);              /* SHIFT IN ZEROS               */ 00918000
*                                                                  0356 00919000
         SLL   WORK1REG,12                                         0356 00920000
*/********************************************************************/ 00921000
*/*       LOOP THRU EACH PAGE, MOVING FIRST BYTE TO ITSELF           */ 00922000
*/*       ABEND WILL CAUSE IKT93EST TO GAIN CONTROL AND CHANGE       */ 00923000
*/*       CODE TO 15D X.                                             */ 00924000
*/********************************************************************/ 00925000
*                                                                  0357 00926000
*RFY WORK2REG RSTD;                                                0357 00927000
*WORK2REG = XSABFRAD +XSAPRMSZ;      /* SET UP LOOP UPPER LIMIT      */ 00928000
         L     WORK2REG,XSABFRAD-1(,RBSECPTR)                      0358 00929000
         LA    WORK2REG,0(,WORK2REG)                               0358 00930000
         AH    WORK2REG,XSAPRMSZ(,RBSECPTR)                        0358 00931000
*DO WHILE WORK1REG<WORK2REG;                                       0359 00932000
         B     @DE00359                                            0359 00933000
@DL00359 DS    0H                                                  0360 00934000
*   GEN REFS(WORK1REG)                                             0360 00935000
*       (CLC 0(1,WORK1REG),0(WORK1REG)); /* REFERENCE PAGE           */ 00936000
         CLC 0(1,WORK1REG),0(WORK1REG)                                  00937000
*   WORK1REG = WORK1REG + APAGE;     /* INCREMENT BY PAGE SIZE       */ 00938000
         AL    WORK1REG,@CF02305                                   0361 00939000
*END;                                                              0362 00940000
@DE00359 CR    WORK1REG,WORK2REG                                   0362 00941000
         BL    @DL00359                                            0362 00942000
*RFY (WORK1REG,WORK2REG) UNRSTD;                                   0363 00943000
*                                                                  0363 00944000
*/********************************************************************/ 00945000
*/*         GET BACK INTO KEY 0                                      */ 00946000
*/********************************************************************/ 00947000
*                                                                  0364 00948000
*GEN (MODESET EXTKEY=ZERO);                                        0364 00949000
         MODESET EXTKEY=ZERO                                            00950000
*XSADMOVE = '0'B;                    /* CLEAR INDICATOR THAT DATA  0365 00951000
*                                       VERIFICATION WAS GOING ON    */ 00952000
         NI    XSADMOVE(RBSECPTR),B'10111111'                      0365 00953000
*RFY RTNREG RSTD;                                                  0366 00954000
*RTNREG = XSASAVEB;                                                0367 00955000
         L     RTNREG,XSASAVEB(,RBSECPTR)                          0367 00956000
*RETURN;                                                           0368 00957000
