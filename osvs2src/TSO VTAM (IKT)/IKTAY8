*                                                            @ZM21005*/ 02261000
         SPOST                                                          02262000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,               *02263000
               RELATED=(SPOST)      /*                       @ZM21005*/ 02264000
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=USE,RELATED=(SPOST)   02265000
*                                   /*                       @ZM21005*/ 02266000
*             TSBWTJID=0;           /* CLEAR TSBWTJID                */ 02267000
         SLR   @07,@07                                          01S0598 02268000
         STH   @07,TSBWTJID(,RWORKV)                            01S0598 02269000
*             REG1=TVCSSRBA;        /* GET SRB ADDRESS               */ 02270000
         L     @07,TSBEXTNT(,RWORKV)                            01S0599 02271000
         L     @07,TSBXCSAP(,@07)                               01S0599 02272000
         L     REG1,TVCSSRBA-1(,@07)                            01S0599 02273000
         LA    REG1,0(,REG1)                                    01S0599 02274000
*             CALL FREESRB;         /* FREE SRB                      */ 02275000
         BAL   @14,FREESRB                                      01S0600 02276000
*             REG1=RWORKV;          /* GET ADDRESS OF THIS TSB       */ 02277000
         LR    REG1,RWORKV                                      01S0601 02278000
*             CALL FREETVCS;        /* FREE TVCS                     */ 02279000
         BAL   @14,FREETVCS                                     01S0602 02280000
*             RFY                                               01S0603 02281000
*               REG1 UNRSTD;                                    01S0603 02282000
*           END;                                                01S0604 02283000
*         ELSE                      /* TARGET STILL THERE            */ 02284000
*                                                               01S0605 02285000
*           /*********************************************************/ 02286000
*           /*                                                       */ 02287000
*           /* SET THE SOURCE RELINQUISHES CONTROL BIT IN THE TVCS   */ 02288000
*           /* INDICATORS AND PERFORM THE COMPARE AND SWAP TO STORE  */ 02289000
*           /* THE INDICATORS BACK IN THE TVCS.                      */ 02290000
*           /*                                                       */ 02291000
*           /*********************************************************/ 02292000
*                                                               01S0605 02293000
*           DO;                                                 01S0605 02294000
         B     @RC00594                                         01S0605 02295000
@RF00594 DS    0H                                               01S0606 02296000
*             WRKSRCRC='1'B;        /* SET SOURCE RELINQUISHES CNTL  */ 02297000
         OI    WRKSRCRC,B'10000000'                             01S0606 02298000
*             RWORKX=WRKINDS;       /* SET UPDATE REGISTER           */ 02299000
         L     RWORKX,WRKINDS                                   01S0607 02300000
*             CS(RWORKW,RWORKX,TVCSINDS);                       01S0608 02301000
         L     @07,TSBEXTNT(,RWORKV)                            01S0608 02302000
         L     @07,TSBXCSAP(,@07)                               01S0608 02303000
         CS    RWORKW,@06,TVCSINDS(@07)                         01S0608 02304000
*             BC(NE,CSLOOP5);       /* REPEAT IF CS FAILED           */ 02305000
         BC    7,CSLOOP5                                        01S0609 02306000
*           END;                                                01S0610 02307000
*         RFY                                                   01S0611 02308000
*          (RWORKW,                                             01S0611 02309000
*           RWORKX) UNRSTD;                                     01S0611 02310000
@RC00594 DS    0H                                               01S0612 02311000
*       END;                        /* END OF **ASID TPUT FOUND**    */ 02312000
*     ELSE                                                      01S0613 02313000
*       ;                           /* TPUT NOT FOUND IN THIS TSB    */ 02314000
@RF00589 DS    0H                                               01S0614 02315000
*     RWORKV=TSBXFWD;               /* POINT TO NEXT TSB             */ 02316000
         L     @07,TSBEXTNT(,RWORKV)                            01S0614 02317000
         L     @07,TSBXFWD(,@07)                                01S0614 02318000
         LR    RWORKV,@07                                       01S0614 02319000
*   END;                            /* END OF ** LOOP THRU TSBS**    */ 02320000
@DE00588 LTR   RWORKV,RWORKV                                    01S0615 02321000
         BNZ   @DL00588                                         01S0615 02322000
*   RFY                                                         01S0616 02323000
*     TSB BASED(TSBPTR);                                        01S0616 02324000
*   RFY                                                         01S0617 02325000
*     RWORKV UNRSTD;                                            01S0617 02326000
*                                                               01S0617 02327000
*   /*****************************************************************/ 02328000
*   /*                                                               */ 02329000
*   /* RESTORE RETURN ADDRESS AND RETURN TO CALLER                   */ 02330000
*   /*                                                               */ 02331000
*   /*****************************************************************/ 02332000
*                                                               01S0618 02333000
*   RFY                                                         01S0618 02334000
*     RLINK RSTD;                                               01S0618 02335000
*   RLINK=SAVE14A;                  /* RESTORE RETURN ADDRESS        */ 02336000
         L     RLINK,SAVE14A                                    01S0619 02337000
*   RFY                                                         01S0620 02338000
*     RLINK UNRSTD;                                             01S0620 02339000
*   RETURN;                                                     01S0621 02340000
@EL00012 DS    0H                                               01S0621 02341000
@EF00012 DS    0H                                               01S0621 02342000
@ER00012 BR    @14                                              01S0621 02343000
*   END SRCHVTAM;                                               01S0622 02344000
*                                                               01S0622 02345000
@PB00012 DS    0H                                               01S0622 02346000
*   /*****************************************************************/ 02347000
*   /*                                                               */ 02348000
*   /* THE TIOCRPT DOES NOT HAVE A PLS VERSION. THE MACRO IS INVOKED */ 02349000
*   /* WITH A GEN STATEMENT.                                         */ 02350000
*   /*                                                               */ 02351000
