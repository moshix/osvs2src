         SLA   REG0,16                                          01S0556 02085000
         O     REG0,@CF01681                                    01S0556 02086000
*                     RFY                                       01S0557 02087000
*                       ASCB BASED(PSAAOLD);/*               @YM03242*/ 02088000
*                     REG1='80000000'X;/* SHOW RESET REQUEST         */ 02089000
         L     REG1,@CF01623                                    01S0558 02090000
*                     REG3=RSAVE;   /* SAVE SAVEAREA ADDR            */ 02091000
         LR    REG3,RSAVE                                       01S0559 02092000
*                     RSAVE='0400'X;/* NON-DISPATCH MASK             */ 02093000
         LA    RSAVE,1024                                       01S0560 02094000
*                     CALL STATUS;  /* DISPATCH WAITING TASKS        */ 02095000
         L     @04,CVTPTR                                       01S0561 02096000
         L     @04,CVTABEND(,@04)                               01S0561 02097000
         L     @15,SCVTSTAT(,@04)                               01S0561 02098000
         BALR  @14,@15                                          01S0561 02099000
*                     RSAVE=REG3;   /* RESTORE SAVEAREA ADD          */ 02100000
         LR    RSAVE,REG3                                       01S0562 02101000
*                     RFY                                       01S0563 02102000
*                      (REG0,                                   01S0563 02103000
*                       REG1,                                   01S0563 02104000
*                       REG3,                                   01S0563 02105000
*                       REG14,                                  01S0563 02106000
*                       REG15) UNRSTD;                          01S0563 02107000
*                   END;            /* END OF **STATUS**             */ 02108000
*                 ELSE                                          01S0565 02109000
*                   ;               /* NO NEED FOR STATUS            */ 02110000
@RF00551 DS    0H                                               01S0566 02111000
*               END;                /* END OF **USE THIS TSB**       */ 02112000
*             ELSE                                              01S0567 02113000
*               ;                   /* ASID TPUT IN PROGRESS         */ 02114000
@RF00522 DS    0H                                               01S0568 02115000
*           END;                    /* END OF **TSB MATCH FOUND**    */ 02116000
*         ELSE                                                  01S0569 02117000
*           ;                       /* TSB NOT IN USE OR NO MATCH    */ 02118000
*                                                               01S0569 02119000
@RF00519 DS    0H                                               01S0570 02120000
*         /***********************************************************/ 02121000
*         /*                                                         */ 02122000
*         /* INCREMENT POINTER BY SIZE OF TSB                        */ 02123000
*         /*                                                         */ 02124000
*         /***********************************************************/ 02125000
*                                                               01S0570 02126000
*         RWORKW=RWORKW+RWORKX;     /* INCR TO NEXT TSB              */ 02127000
         ALR   RWORKW,RWORKX                                    01S0570 02128000
*       END;                        /* END OF **CHECK TSB/ASID **       02129000
*                                      LOOP TO SEARCH TSB CHAIN      */ 02130000
         AL    I,@CF00101                                       01S0571 02131000
@DE00518 CH    I,TIOCNTSB                                       01S0571 02132000
         BNH   @DL00518                                         01S0571 02133000
*       RFY                                                     01S0572 02134000
*        (RWORKY,                                               01S0572 02135000
*         RWORKW,                                               01S0572 02136000
*         RWORKX) UNRSTD;                                       01S0572 02137000
*       RFY                                                     01S0573 02138000
*         TSB BASED(TSBPTR);        /* RESET TSB BASE POINTER        */ 02139000
*     END;                          /* END OF **TSO UP**             */ 02140000
*   ELSE                                                        01S0575 02141000
*     ;                             /* TCAM/TSO NOT UP               */ 02142000
*                                                               01S0575 02143000
@RF00510 DS    0H                                               01S0576 02144000
*   /*****************************************************************/ 02145000
*   /*                                                               */ 02146000
*   /* RESTORE RETURN ADDRESS AND RETURN TO CALLER                   */ 02147000
*   /*                                                               */ 02148000
*   /*****************************************************************/ 02149000
*                                                               01S0576 02150000
*   RFY                                                         01S0576 02151000
*     RLINK RSTD;                                               01S0576 02152000
*   RLINK=SAVE14A;                  /* RESTORE RETURN ADDRESS        */ 02153000
         L     RLINK,SAVE14A                                    01S0577 02154000
*   RFY                                                         01S0578 02155000
*     RLINK UNRSTD;                                             01S0578 02156000
*   RETURN;                         /* RETURN TO CALLER              */ 02157000
@EL00011 DS    0H                                               01S0579 02158000
@EF00011 DS    0H                                               01S0579 02159000
@ER00011 BR    @14                                              01S0579 02160000
*   END SRCHTCAM;                                               01S0580 02161000
         EJECT                                                          02162000
*                                                               01S0581 02163000
*   /*****************************************************************/ 02164000
*   /*                                                               */ 02165000
*   /* SRCHVTAM SEARCH VTAM TSB CHAIN FOR ASID TPUT FROM THE         */ 02166000
*   /* TERMINATING ADDRESS SPACE THEN A VTAM TSB IS FOUND WHICH      */ 02167000
*   /* INDICATES AN ASID TPUT FROM THE TERMINATING ADRESS SPACE, THE */ 02168000
*   /* CSA AREA (TVCS) ASSOCIATED WITH THE TPUT IS EXAMINED. IF THE  */ 02169000
*   /* TARGET HAS RELINQUISED CONTROL, AN SPOST IS ISSUED TO LET     */ 02170000
*   /* OUTSTANDING POSTS COMPLETE, AND THE SRB AND TVCS AREAS ARE    */ 02171000
*   /* FREED IF THE TARGET HAS NOT RELINQUISHED CONTROL, THE TVCS BIT*/ 02172000
*   /* IS SET WHICH INDICATES THE SOURCE RELINQUISHED CONTROL        */ 02173000
*   /* (TVCSRCRC) AND THE TARGET ADDRESS SPACE IS LEFT WITH THE      */ 02174000
*   /* RESPONSIBILITY OF FREEING THE TVCS AND RELATED SRB.           */ 02175000
*   /*                                                               */ 02176000
*   /*****************************************************************/ 02177000
*                                                               01S0581 02178000
*SRCHVTAM:                                                      01S0581 02179000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                       01S0581 02180000
SRCHVTAM DS    0H                                               01S0582 02181000
*   RFY                                                         01S0582 02182000
*     RLINK RSTD;                                               01S0582 02183000
*   SAVE14A=RLINK;                  /* SAVE RETURN ADDRESS           */ 02184000
         ST    RLINK,SAVE14A                                    01S0583 02185000
*   RFY                                                         01S0584 02186000
*     RLINK UNRSTD;                                             01S0584 02187000
*   RFY                                                         01S0585 02188000
*     RWORKV RSTD;                                              01S0585 02189000
*   RWORKV=TCASTSB;                 /* POINT TO FIRST VTAM TSB       */ 02190000
         L     @07,TCASTPTR                                     01S0586 02191000
         L     RWORKV,TCASTSB(,@07)                             01S0586 02192000
*   RFY                                                         01S0587 02193000
*     TSB BASED(RWORKV);                                        01S0587 02194000
*                                                               01S0587 02195000
*   /*****************************************************************/ 02196000
*   /*                                                               */ 02197000
*   /* EACH TSB ON THE TSO/VTAM TSB CHAIN IS EXAMINED FOR ASID TPUT  */ 02198000
*   /* FROM THE TERMINATING ADDRESS SPACE. WHEN THE ADDRESS OF THE   */ 02199000
*   /* NEXT (OR FIRST) TSB IS 0, THE SEARCH IS ENDED.                */ 02200000
*   /*                                                               */ 02201000
*   /*****************************************************************/ 02202000
*                                                               01S0588 02203000
*   DO WHILE(RWORKV^=0);            /* START **LOOP THRU TSBS**      */ 02204000
         B     @DE00588                                         01S0588 02205000
@DL00588 DS    0H                                               01S0589 02206000
*     IF TSBXCSAP^=0&               /* IS THERE A CROSS-MEM TPUT     */ 02207000
*         TSBWTJID=RMPLASID THEN    /* FROM TERMINATING A.S.         */ 02208000
*                                                               01S0589 02209000
         L     @07,TSBEXTNT(,RWORKV)                            01S0589 02210000
         L     @07,TSBXCSAP(,@07)                               01S0589 02211000
         LTR   @07,@07                                          01S0589 02212000
         BZ    @RF00589                                         01S0589 02213000
         LH    @15,RMPLASID(,RRMPL)                             01S0589 02214000
         N     @15,@CF01708                                     01S0589 02215000
         CH    @15,TSBWTJID(,RWORKV)                            01S0589 02216000
         BNE   @RF00589                                         01S0589 02217000
*       /*************************************************************/ 02218000
*       /*                                                           */ 02219000
*       /* WHEN A TSB IS FOUND WHICH INDICATES AN ASID TPUT FROM THE */ 02220000
*       /* TERMINATING ADDRESS SPACE, THE TVCS INDICATORS ARE PICKED */ 02221000
*       /* UP FOR EXAMINATION. A COMPARE AND SWAP LOOP IS SET UP TO  */ 02222000
*       /* DETERMINE IF THE TARGET HAS RELINQUISHED CONTROL, AND IF  */ 02223000
*       /* NOT, TO SET THE SOURCE AS RELINQUISHED CONTROL.           */ 02224000
*       /*                                                           */ 02225000
*       /*************************************************************/ 02226000
*                                                               01S0590 02227000
*       DO;                         /* START **ASID TPUT FOUND**     */ 02228000
*         RFY                                                   01S0591 02229000
*          (RWORKW,                                             01S0591 02230000
*           RWORKX) RSTD;                                       01S0591 02231000
*         RWORKW=TVCSINDS;          /* GET TVCS INDICATORS           */ 02232000
         L     RWORKW,TVCSINDS(,@07)                            01S0592 02233000
*CSLOOP5:                                                       01S0593 02234000
*         WRKINDS=RWORKW;           /* RWORKW IS SET WHEN CS FAILS   */ 02235000
CSLOOP5  ST    RWORKW,WRKINDS                                   01S0593 02236000
*         IF WRKTGRC='1'B THEN      /* IF TARGET GONE - FREE SRB        02237000
*                                      AND TVCS                      */ 02238000
*                                                               01S0594 02239000
         TM    WRKTGRC,B'01000000'                              01S0594 02240000
         BNO   @RF00594                                         01S0594 02241000
*           /*********************************************************/ 02242000
*           /*                                                       */ 02243000
*           /* WHEN THE TARGET HAS RELINQUISHED CONTROL, SPOST IS    */ 02244000
*           /* ISSUED TO ALLOW OUTSTANDING POSTS TO COMPLETE. THE    */ 02245000
*           /* LOCKS HAVE TO BE RELEASED SINCE THERE IS NOT A BRANCH */ 02246000
*           /* ENTRY TO SPOST. FOLLOWING THE SPOST, THE TVCS AND SRB */ 02247000
*           /* ARE FREED.                                            */ 02248000
*           /*                                                       */ 02249000
*           /*********************************************************/ 02250000
*                                                               01S0595 02251000
*           DO;                                                 01S0595 02252000
*             RFY                                               01S0596 02253000
*               REG1 RSTD;                                      01S0596 02254000
*             GEN CODE REFS(CVTPTR,CVTSPOST,CVTMAP,PSALITA,FLC,RSAVE)   02255000
*                 SETS(REG0,REG1,REG14,REG15);                  01S0597 02256000
*                                   /*                       @ZM21005*/ 02257000
         SETLOCK RELEASE,TYPE=CMS,REGS=USE,RELATED=(SPOST)   /*         02258000
*                                                            @ZM21005*/ 02259000
