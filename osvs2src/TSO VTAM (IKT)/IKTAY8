*         CALL FRTCAST;             /* FREE THE TCAS TABLE           */ 00755000
         BAL   @14,FRTCAST                                      01S0251 00756000
*       END;                                                    01S0252 00757000
*     ELSE                                                      01S0253 00758000
*       ;                           /* TCAS STILL ALIVE - LEAVE IT      00759000
*                                      ALONE                         */ 00760000
@RF00243 DS    0H                                               01S0254 00761000
*   ELSE                                                        01S0254 00762000
*     ;                             /* USERS STILL AROUND LEAVE TCAS    00763000
*                                      TABLE THERE                   */ 00764000
*                                                               01S0254 00765000
