*           /*********************************************************/ 01896000
*           /*                                                       */ 01897000
*           /* IF ASID MATCH IS FOUND, ZERO THE HIGH ORDER BYTE OF   */ 01898000
*           /* TSBECB. IF THE TPUT IS IN PROGRESS, LOOK AT THE NEXT  */ 01899000
*           /* TSB                                                   */ 01900000
*           /*                                                       */ 01901000
*           /*********************************************************/ 01902000
*                                                               01S0520 01903000
*           DO;                     /* START **TSB MATCH FOUND**     */ 01904000
*             WKTSECB1=0;           /* TURN OFF POST AND WAIT BITS   */ 01905000
         MVI   WKTSECB1(RWORKW),X'00'                           01S0521 01906000
*             IF TSBTJIP='0'B THEN  /* IF TPUT IN PROGRESS (TJIP=1)     01907000
*                                      LOOK AT NEXT TSB ELSE CONTIN  */ 01908000
*                                                               01S0522 01909000
         TM    TSBTJIP(RWORKW),B'00000010'                      01S0522 01910000
         BNZ   @RF00522                                         01S0522 01911000
*               /*****************************************************/ 01912000
*               /*                                                   */ 01913000
*               /* CLEAR X-MEMORY TPUT INDICATORS IN THE TSB         */ 01914000
*               /*                                                   */ 01915000
*               /*****************************************************/ 01916000
*                                                               01S0523 01917000
*               DO;                 /* START **USE THIS TSB**        */ 01918000
*                 TSBOWIP='0'B;     /* CLEAR CROSS MEMORY TPUT       */ 01919000
*                 TSBTJIP='0'B;     /* RELATED TSB INDICATORS        */ 01920000
*                 TSBTJBF='0'B;                                 01S0526 01921000
