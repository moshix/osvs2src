*                                      COMPARE AND SWAP LOOP USED TO    00687000
*                                      UPDATE TCASUSEC               */ 00688000
*   RWORKV=TCASUSER;                /* GET WORD CONTAINING USER COUNT*/ 00689000
         L     @01,TCASTPTR                                     01S0235 00690000
         L     RWORKV,TCASUSER(,@01)                            01S0235 00691000
*CSLOOP2:                                                       01S0236 00692000
*   WRKUSER=RWORKV;                 /* MOVE WORD TO WORK AREA        */ 00693000
