         BNZ   @RF00551                                         01S0551 02048000
*                   DO;             /* START **STATUS**              */ 02049000
*                     TSBOWAIT='0'B;/* TURN OFF OWAIT BIT - THIS        02050000
*                                      INSTR IN TCAM IS NI      01S0553 02051000
*                                      TSBFLG4,TSBOWAIT - APPARENTLY    02052000
*                                      AN ERROR THAT FOR SOME REASON    02053000
*                                      DOES NO HARM. REPORTED TO SJ     02054000
*                                      FE 9-15-75                    */ 02055000
*                                                               01S0553 02056000
         NI    TSBOWAIT(RWORKW),B'11011111'                     01S0553 02057000
*                     /***********************************************/ 02058000
*                     /*                                             */ 02059000
*                     /* A NORMAL TPUT IS WAITING FOR THIS TSB MAKE  */ 02060000
*                     /* ALL TASKS ASSOCIATED WITH THIS TSB ACTIVE.  */ 02061000
*                     /* INTERFACE TO STATUS: REG0 = ASID IN BYTES   */ 02062000
*                     /* 0-1 STATUS ENTRY CODE(8) IN BYTES 2-3 REG1 =*/ 02063000
*                     /* '80000000'X RESET REG13 = TCB NON-DISPATCH  */ 02064000
*                     /* BIT MASK '0400'X REG14 = RETURN ADDRESS     */ 02065000
*                     /* REG15 = EPA TO STATUS                       */ 02066000
*                     /*                                             */ 02067000
*                     /***********************************************/ 02068000
*                                                               01S0554 02069000
*                     RFY                                       01S0554 02070000
*                      (REG0,                                   01S0554 02071000
*                       REG1,                                   01S0554 02072000
*                       REG3,                                   01S0554 02073000
*                       REG14,                                  01S0554 02074000
*                       REG15) RSTD;                            01S0554 02075000
*                     RFY                                       01S0555 02076000
*                       ASCB BASED(TSBASCBA);/*              @YM03242   02077000
*                                      GET TARGET ASCB               */ 02078000
*                     REG0=(ASCBASID*65536)|'0008'X;/* PUT ASID IN      02079000
*                                      BYTES 0-1 AND ENTRY CODE 01S0556 02080000
*                                      '0008'X IN BYTES 2-3.         */ 02081000
