*                     CS(REG0,REG15,WKTSBF2M);                  01S0536 01959000
         CS    REG0,@15,WKTSBF2M(RWORKW)                        01S0536 01960000
*                     BC(NE,CSLOOP4);/* LOOP TIL UPDATE DONE         */ 01961000
*                                                               01S0537 01962000
         BC    7,CSLOOP4                                        01S0537 01963000
*                     /***********************************************/ 01964000
*                     /*                                             */ 01965000
*                     /* INVOKE IEDAYTPQ TO TPOST THE TSB TO IEDAYP  */ 01966000
*                     /* SO THAT TCAM WILL NOT EXPECT MORE OUTPUT.   */ 01967000
*                     /* THE REQUIRED INTERFACE IS: REG0 = RETURN    */ 01968000
*                     /* ADDRESS REG1 = 0 TO TPOST TSB TO IEDAYP REG2*/ 01969000
*                     /* = TSB ADDRESS REG13 = ADDRESS OF 6-WORD SAVE*/ 01970000
*                     /* AREA IN TIOCRPT REG15 = EPA OF IEDAYTPQ     */ 01971000
*                     /*                                             */ 01972000
*                     /***********************************************/ 01973000
*                                                               01S0538 01974000
*                     RFY                                       01S0538 01975000
*                      (REG1,                                   01S0538 01976000
*                       REG2,                                   01S0538 01977000
*                       REG3) RSTD;                             01S0538 01978000
*                                                               01S0538 01979000
*                     /***********************************************/ 01980000
*                     /*                                             */ 01981000
*                     /* THE ADDRESS OF IEDAYTPQ (TPOST) IS OBTAINED */ 01982000
*                     /* BY ISSUING THE LOAD MACRO. LOAD WILL RETURN */ 01983000
*                     /* THE ADDRESS OF THE ROUTINE IN REG 0. THE CMS*/ 01984000
*                     /* AND LOCAL LOCKS MUST BE RELEASED SINCE THERE*/ 01985000
*                     /* IS NOT A BRANCH ENTRY TO LOAD. THE LOCKS ARE*/ 01986000
*                     /* RE-OBTAINED AS SOON AS THE LOAD IS DONE.    */ 01987000
*                     /* IEDAYTPQ IS IN LPALIB                       */ 01988000
*                     /*                                             */ 01989000
*                     /***********************************************/ 01990000
*                                                               01S0539 01991000
*                     GEN CODE REFS(CVTPTR,CVTSPOST,CVTMAP,PSALITA,FLC, 01992000
*                         RSAVE,TPQBASE)/*                   @ZM21005*/ 01993000
*                         SETS(REG0,REG1,REG14,REG15,TPQBASE);  01S0539 01994000
         SETLOCK RELEASE,TYPE=CMS,REGS=USE,RELATED=(LOAD) /* @ZM21005*/ 01995000
         SETLOCK RELEASE,TYPE=LOCAL,REGS=USE,RELATED=(LOAD)  /*         01996000
*                                                            @ZM21005*/ 01997000
         LOAD  EP=IEDAYTPQ          /* GET ADDRESS OF TPOST          */ 01998000
         ST    REG0,TPQBASE         /* SAVE BASE OF TPOST    @ZM21005*/ 01999000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,               *02000000
               RELATED=(LOAD)       /*                       @ZM21005*/ 02001000
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=USE,RELATED=(LOAD) /* 02002000
*                                                            @ZM21005*/ 02003000
*                     REG15=ADDR(IEDAYTPQ);/* GET ADDRESS OF ROUTINE    02004000
*                                      FOR TPOST             @ZM21005*/ 02005000
         L     REG15,TPQBASE                                    01S0540 02006000
*                     REG1=0;       /* SET REG1 TO ZERO              */ 02007000
         SLR   REG1,REG1                                        01S0541 02008000
*                     REG2=RWORKW;  /* ADDR TSB IN RWORKW            */ 02009000
         LR    REG2,RWORKW                                      01S0542 02010000
*                     REG3=RSAVE;   /* SAVE ADDR OF SAVEAREA         */ 02011000
         LR    REG3,RSAVE                                       01S0543 02012000
*                     GEN(LA RSAVE,TIOCSAVE+44)                 01S0544 02013000
*                     SETS(RSAVE) REFS(TIOCSAVE);/* ADDR OF LAST 6      02014000
*                                      WORDS IN TIOCSAVE             */ 02015000
         LA RSAVE,TIOCSAVE+44                                           02016000
*                     GEN(BALR REG0,REG15)/* INVOKE IEDAYTPQ         */ 02017000
*                     REFS(REG15) SETS(REG0);                   01S0545 02018000
         BALR REG0,REG15                                                02019000
*                     GEN(MODESET EXTKEY=SUPR);/* CONTROL RETURNS IN    02020000
*                                      KEY 6-GET SUPR AGAIN  @YM03226*/ 02021000
         MODESET EXTKEY=SUPR                                            02022000
*                     RSAVE=REG3;   /* RESTORE SAVE AREA ADD         */ 02023000
         LR    RSAVE,REG3                                       01S0547 02024000
*                     RFY                                       01S0548 02025000
*                      (REG0,                                   01S0548 02026000
*                       REG1,                                   01S0548 02027000
*                       REG2,                                   01S0548 02028000
*                       REG3,                                   01S0548 02029000
*                       REG15) UNRSTD;                          01S0548 02030000
*                   END;            /* END OF **TPOST**              */ 02031000
*                 ELSE                                          01S0550 02032000
*                   ;               /* TPOST NOT NEEDED              */ 02033000
*                                                               01S0550 02034000
@RF00529 DS    0H                                               01S0551 02035000
*                 /***************************************************/ 02036000
*                 /*                                                 */ 02037000
*                 /* CALL STATUS IF AN OWAIT IS IN PROGRESS AND TSO  */ 02038000
*                 /* HAS NOT OUT OF CORE ABENDED                     */ 02039000
*                 /*                                                 */ 02040000
*                 /***************************************************/ 02041000
*                                                               01S0551 02042000
*                 IF TSBWOWIP='1'B& /* IF OWAIT IN PROGRESS          */ 02043000
*                     TSBOCAB='0'B THEN/* AND NOT OUT-OF-CORE ABEND  */ 02044000
         TM    TSBWOWIP(RWORKW),B'00010000'                     01S0551 02045000
         BNO   @RF00551                                         01S0551 02046000
