*    /*                                                              */ 00282000
*    /*        FOLLOWING CODE ISSUE QUIESCE LOGON                    */ 00283000
*    /*                                                              */ 00284000
*QUIESCE : PROC OPTIONS(NOSAVE,NOSAVEAREA)    ;                    0121 00285000
QUIESCE  DS    0H                                                  0122 00286000
*      SA4=R14 ;                           /* SAVE RETURN REG        */ 00287000
         LR    SA4,R14                                             0122 00288000
*      RFY (R14,R15,R0,R1) RSTD  /* RESTRICT PARA REG*/ ;          0123 00289000
*      GEN ;                                                       0124 00290000
