         TITLE 'IKTCAS21 - ACB CONTROL                                 *00001000
                        '                                               00002000
IKTCAS21 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKTCAS21  77.319'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
*      TCASTPTR=TWATCAST ;             /* ESTABLISH TCAST ADDR       */ 00018000
         L     TCASTPTR,TWATCAST(,TWAPTR)                          0026 00019000
*      RFY WESTD BASED(R10) ;          /* W.E.ADDR IS PASSED IN R10  */ 00020000
*      RFY  IFGACB BASED(ACBPTR) ;     /* ACB BASE PTR               */ 00021000
*      RFY  IFGEXLST BASED(EXLSTPTR) ;  /* EXLST BASE PTR            */ 00022000
*      ACBPTR=ADDR(ACBADDR) ;          /* ACB ADDRESSIBILITY         */ 00023000
         LA    @06,ACBADDR                                         0030 00024000
         ST    @06,ACBPTR                                          0030 00025000
*      EXLSTPTR=ADDR(EXLSTADD) ;       /* EXLST ADDRESSIBILITY       */ 00026000
         LA    @06,EXLSTADD                                        0031 00027000
         ST    @06,EXLSTPTR                                        0031 00028000
*    /*                                                              */ 00029000
*    /*      ONE OF 4 FUNCTIONS MAY BE REQUESTED:                    */ 00030000
*    /*        ACB OPEN                                              */ 00031000
*    /*        ACB CLOSE                                             */ 00032000
*    /*        START LOGON                                           */ 00033000
*    /*        QUIESCE LOGON                                         */ 00034000
*    /*                                                              */ 00035000
*    /*      THE FOLLOWING CODE DETERMINE WHICH FUNCTION HAS BEEN    */ 00036000
*    /*      REQUESTED AND BRANCH TO THE PROPER ROUTINE TO PERFORM   */ 00037000
*    /*      THE FUNCTION                                            */ 00038000
*    /*                                                              */ 00039000
*      WERC=0 ;                        /* INITIALIZE RETURN CODE     */ 00040000
         SLR   @06,@06                                             0032 00041000
         STH   @06,WERC(,R10)                                      0032 00042000
*      IF WECODE1F=FCVIAO              /* IS THIS ACB OPEN           */ 00043000
*         THEN CALL OPEN ;             /* YES, BRANCH TO IT          */ 00044000
         CLI   WECODE1F(R10),X'11'                                 0033 00045000
         BNE   @RF00033                                            0033 00046000
         BAL   @14,OPEN                                            0034 00047000
*         ELSE IF WECODE1F=FCVIAC      /* NO, IS THIS ACB CLOSE      */ 00048000
*           THEN CALL CLOSE ;          /* YES, BRANCH TO IT          */ 00049000
         B     @RC00033                                            0035 00050000
@RF00033 CLI   WECODE1F(R10),X'12'                                 0035 00051000
         BNE   @RF00035                                            0035 00052000
         BAL   @14,CLOSE                                           0036 00053000
*           ELSE IF WECODE1F=FCVIAS    /* NO, IS THIS START LOGON    */ 00054000
*             THEN CALL START ;        /* YES, BRANCH TO IT          */ 00055000
         B     @RC00035                                            0037 00056000
@RF00035 CLI   WECODE1F(R10),X'14'                                 0037 00057000
         BNE   @RF00037                                            0037 00058000
         BAL   @14,START                                           0038 00059000
*             ELSE IF WECODE1F=FCVIAQ  /* NO, IS THIS QUIESCE LOGON  */ 00060000
*               THEN CALL QUIESCE ;    /* YES, BRANCH TO IT          */ 00061000
         B     @RC00037                                            0039 00062000
@RF00037 CLI   WECODE1F(R10),X'13'                                 0039 00063000
         BNE   @RF00039                                            0039 00064000
         BAL   @14,QUIESCE                                         0040 00065000
*    /*                                                              */ 00066000
*    /*      THE FOLLOWING CODE CHECKS THE RETURN CODE SET BY        */ 00067000
*    /*      PERFORMING ROUTINE. IF IT IS NOT ZERO, THE W.E.         */ 00068000
*    /*      REPRESENTING THIS REQUEST IS DIRECTED TO THE SECOND     */ 00069000
*    /*      DESTINATION INDICATED IN WECODE2 FIELD OF W.E.          */ 00070000
*    /*                                                              */ 00071000
*      IF WERC ^=0                     /* IS RETURN CODE NOT ZERO    */ 00072000
*         THEN DO ;                    /* NO, DIRECT W.E. TO SECOND  */ 00073000
@RF00039 DS    0H                                                  0041 00074000
@RC00037 DS    0H                                                  0041 00075000
@RC00035 DS    0H                                                  0041 00076000
@RC00033 LH    @15,WERC(,R10)                                      0041 00077000
         N     @15,@CF00568                                        0041 00078000
         LTR   @15,@15                                             0041 00079000
         BZ    @RF00041                                            0041 00080000
*         WECODE1=WECODE1 && WECODE2 ; /*     DEST BY SWAP WECODE1   */ 00081000
         XC    WECODE1(2,R10),WECODE2(R10)                         0043 00082000
*         WECODE2=WECODE2 && WECODE1 ; /*     AND WECODE2. BECAUSE   */ 00083000
         XC    WECODE2(2,R10),WECODE1(R10)                         0044 00084000
*         WECODE1=WECODE1 && WECODE2 ; /*     WECODE1 SHOULD CONTAIN */ 00085000
         XC    WECODE1(2,R10),WECODE2(R10)                         0045 00086000
*         END ;                        /*     THE DESTINATION AND    */ 00087000
*                                      /*     WECODE2 THE ORIGINATOR */ 00088000
         EJECT                                                          00089000
*    /*                                                              */ 00090000
*    /*      THE FOLLOWING CODE OPENS THE ACB                        */ 00091000
*    /*                                                              */ 00092000
*OPEN : PROC OPTIONS(NOSAVE,NOSAVEAREA)    ;                       0047 00093000
         B     @PB00002                                            0047 00094000
OPEN     DS    0H                                                  0048 00095000
*      SA1=R14 ;                          /* SAVE RETURN REG      */    00096000
         LR    SA1,R14                                             0048 00097000
*      IF TCASACBP='        '          /* IS THERE PASS WORD IN TCAST*/ 00098000
*         THEN ACBPASSW=0 ;            /* NO,ZERO PASS WORD ADDR     */ 00099000
         CLC   TCASACBP(8,TCASTPTR),@CC00526                       0049 00100000
         BNE   @RF00049                                            0049 00101000
         L     @01,ACBPTR                                          0050 00102000
         SLR   @00,@00                                             0050 00103000
         ST    @00,ACBPASSW(,@01)                                  0050 00104000
*         ELSE PWORD=TCASACBP ;        /* YES,STORE PASS WORD        */ 00105000
         B     @RC00049                                            0051 00106000
@RF00049 MVC   PWORD(8),TCASACBP(TCASTPTR)                         0051 00107000
*      EXLLGNP=LOGON ;                 /* STORE LOGON EXIT ADDRESS   */ 00108000
@RC00049 L     @15,EXLSTPTR                                        0052 00109000
         MVC   EXLLGNP(4,@15),LOGON                                0052 00110000
*      EXLTPNDP=TPEND ;                /* STORE TPEND EXIT ADDRESS   */ 00111000
         MVC   EXLTPNDP(4,@15),TPEND                               0053 00112000
*      RFY (R14,R15,R0,R1) RSTD  /* RESTRICT PARA REG*/ ;          0054 00113000
*      GEN ;                                                       0055 00114000
         OPEN  ACBADDR                OPEN ACB                          00115000
*      IF R15 >=8                      /* TEST RETURN CODE           */ 00116000
*         THEN DO ;                    /* OPEN FAILED                */ 00117000
         C     R15,@CF00042                                        0056 00118000
         BL    @RF00056                                            0056 00119000
*         GEN SETS(WA) ;               /* USE SHOWCB TO GET ERROR CD */ 00120000
         SHOWCB ACB=ACBADDR,FIELDS=ERROR,AREA=WA1,LENGTH=4,AM=VTAM      00121000
*         WERC=WA1 ;                   /* STORE ERROR CODE AS RETURN */ 00122000
         L     @05,WA1                                             0059 00123000
         STH   @05,WERC(,R10)                                      0059 00124000
*         END ;                        /*   CODE IN W.E.             */ 00125000
*         ELSE TWAVFL5='1'B ;          /* NO ERROR,SET OPEN BIT ON   */ 00126000
         B     @RC00056                                            0061 00127000
@RF00056 OI    TWAVFL5(TWAPTR),B'00001000'                         0061 00128000
*      RFY (R14,R15,R0,R1) UNRSTD /*UNRESTRI PARA R */ ;           0062 00129000
@RC00056 DS    0H                                                  0063 00130000
*      R14=SA1 ;                           /* RESTORE RET REG        */ 00131000
         LR    R14,SA1                                             0063 00132000
*      END OPEN ;                                                  0064 00133000
@EL00002 DS    0H                                                  0064 00134000
@EF00002 DS    0H                                                  0064 00135000
@ER00002 BR    @14                                                 0064 00136000
         EJECT                                                          00137000
*    /*                                                              */ 00138000
*    /*        THE FOLLOWING CODE CLOSE THE ACB                      */ 00139000
*    /*                                                              */ 00140000
*CLOSE : PROC OPTIONS(NOSAVE,NOSAVEAREA)    ;                      0065 00141000
CLOSE    DS    0H                                                  0066 00142000
*        SA2=R14 ;                         /* SAVE RETURN REG        */ 00143000
         LR    SA2,R14                                             0066 00144000
*      RFY (R14,R15,R0,R1) RSTD  /* RESTRICT PARA REG*/ ;          0067 00145000
*      GEN ;                                                       0068 00146000
         CLOSE ACBADDR                ISSUE CLOSE  ACB                  00147000
*      RFY (R14,R15,R0,R1) UNRSTD /*UNRESTRI PARA R */ ;           0069 00148000
*      TWAVFL5='0'B ;                  /* SET OPEN BIT OFF           */ 00149000
         NI    TWAVFL5(TWAPTR),B'11110111'                         0070 00150000
*      R14=SA2 ;                       /* RESTORE RETURN REG         */ 00151000
         LR    R14,SA2                                             0071 00152000
*      END CLOSE ;                                                 0072 00153000
@EL00003 DS    0H                                                  0072 00154000
@EF00003 DS    0H                                                  0072 00155000
@ER00003 BR    @14                                                 0072 00156000
         EJECT                                                          00157000
*    /*                                                              */ 00158000
*    /*        FOLLOWING ROUTINE START LOGONS BY ISSUING SETLOGON    */ 00159000
*    /*        MACRO                                                 */ 00160000
*    /*                                                              */ 00161000
*START : PROC OPTIONS(NOSAVE,NOSAVEAREA)    ;                      0073 00162000
START    DS    0H                                                  0074 00163000
*      SA3=R14 ;                       /* SAVE RETURN REG           */  00164000
         ST    R14,SA3                                             0074 00165000
*/*                                                                  */ 00166000
*/*      IN ORDER TO START LOGON ACB MUST BE CLOSED AND RE-OPENED    */ 00167000
*/*      AGAIN IF ACB IS ALREADY OPENED                              */ 00168000
*/*                                                                  */ 00169000
*      WERC=0 ;                        /* INITIALIZE RETURN CODE     */ 00170000
         SLR   @06,@06                                             0075 00171000
         STH   @06,WERC(,R10)                                      0075 00172000
*      IF TWAVFL5='1'B                 /* IS OPEN BIT ON             */ 00173000
*         THEN DO ;                    /* YES                        */ 00174000
         TM    TWAVFL5(TWAPTR),B'00001000'                         0076 00175000
         BNO   @RF00076                                            0076 00176000
*         CALL CLOSE ;                 /* CLOSE THE ACB              */ 00177000
         BAL   @14,CLOSE                                           0078 00178000
*         CALL OPEN ;                  /* RE-OPEN ACB                */ 00179000
         BAL   @14,OPEN                                            0079 00180000
*         END ;                                                    0080 00181000
*         ELSE CALL OPEN ;             /* OPEN ACB                   */ 00182000
         B     @RC00076                                            0081 00183000
@RF00076 BAL   @14,OPEN                                            0081 00184000
*      IF WERC=0                       /* CHECK RTN CODE IS OPEN O.K.*/ 00185000
*         THEN DO ;                    /* YES                        */ 00186000
@RC00076 LH    @06,WERC(,R10)                                      0082 00187000
         N     @06,@CF00568                                        0082 00188000
         LTR   @06,@06                                             0082 00189000
         BNZ   @RF00082                                            0082 00190000
*         RFY (R14,R15,R0,R1) RSTD  /* RESTRICT PARA REG*/ ;       0084 00191000
*         GEN ;                        /* ISSUE SETLOGON TO START    */ 00192000
         SETLOGON RPL=RPLADDR,OPTCD=(SYN,START)                         00193000
*         IF R15=0                     /* IS SETLOGON SUCCESSFUL     */ 00194000
*            THEN DO ;                 /* YES                        */ 00195000
         LTR   R15,R15                                             0086 00196000
         BNZ   @RF00086                                            0086 00197000
*            TWAVFL6='1'B ;            /* SET START LOGON BIT ON     */ 00198000
         OI    TWAVFL6(TWAPTR),B'00000100'                         0088 00199000
*            R1=7 ;                    /* 7 IS ACCEPTING LOGON MSG # */ 00200000
         LA    R1,7                                                0089 00201000
*            CALL IKTCAS56 ;           /* TO GET MSG # 7             */ 00202000
         L     @15,TWAMSG(,TWAPTR)                                 0090 00203000
         BALR  @14,@15                                             0090 00204000
*            R1=R1+4 ;                 /* R1 PTR TO THE LENGTH AND   */ 00205000
         AL    R1,@CF00036                                         0091 00206000
*                                      /* MSG, THEN SKIP LENGTH      */ 00207000
*            GEN ;                                                 0092 00208000
         WTO   MF=(E,(1))             ISSUE ACCEPTING LOGON MSG         00209000
*            END ;                                                 0093 00210000
*            ELSE DO ;                 /* NO, GO FIND ERROR          */ 00211000
         B     @RC00086                                            0094 00212000
@RF00086 DS    0H                                                  0095 00213000
*            WA1=0 ;                   /* INITIALIZE WORK AREA       */ 00214000
         SLR   @06,@06                                             0095 00215000
         ST    @06,WA1                                             0095 00216000
*            WA2=0 ;                   /* INITIALIZE WORK AREA       */ 00217000
         ST    @06,WA2                                             0096 00218000
*            GEN SETS(WA) ;            /* ISSUE SHOWCB TO GET ERROR  */ 00219000
         SHOWCB AM=VTAM,RPL=RPLADDR,FIELDS=(RTNCD,FDBK2),             CC00220000
               AREA=WA,LENGTH=8       TO EXTRACT ERROR CD PLACE IN WA   00221000
*/*                                                                  */ 00222000
*/*      IF RTNCD IS 16 AND FDBK2 IS 13 IN RPL, MEANS THE CONNECTION */ 00223000
*/*      OF ACB JUST OPENED IS BROKEN. SO OPEN SHOULD BE RE-ISSUED   */ 00224000
*/*                                                                  */ 00225000
*        IF WA1=RCLGFAIL & WA2=13      /* CHECK ERROR CONDITION      */ 00226000
*           THEN DO ;                  /* OPEN HAS BEEN BROKEN       */ 00227000
         CLC   WA1(4),@CF00086                                     0098 00228000
         BNE   @RF00098                                            0098 00229000
         CLC   WA2(4),@CF00536                                     0098 00230000
         BNE   @RF00098                                            0098 00231000
*           CALL OPEN ;                /* RE-OPEN                    */ 00232000
         BAL   @14,OPEN                                            0100 00233000
*           IF WERC=0                  /* IS OPEN SUCCESSFUL         */ 00234000
*              THEN DO;                /* YES,RE-ISSUE START LOGON   */ 00235000
         LH    @07,WERC(,R10)                                      0101 00236000
         N     @07,@CF00568                                        0101 00237000
         LTR   @07,@07                                             0101 00238000
         BNZ   @RF00101                                            0101 00239000
*              GEN ;                                               0103 00240000
         SETLOGON RPL=RPLADDR,OPTCD=(SYN,START)  TO START LOGON         00241000
*              IF R15=0                /* IS START LOGON SUCCESSFUL  */ 00242000
*            THEN DO ;                 /* YES                        */ 00243000
         LTR   R15,R15                                             0104 00244000
         BNZ   @RF00104                                            0104 00245000
*            TWAVFL6='1'B ;            /* SET START LOGON BIT ON     */ 00246000
         OI    TWAVFL6(TWAPTR),B'00000100'                         0106 00247000
*            R1=7 ;                    /* 7 IS ACCEPTING LOGON MSG # */ 00248000
         LA    R1,7                                                0107 00249000
*            CALL IKTCAS56 ;           /* TO GET MSG # 7             */ 00250000
         L     @15,TWAMSG(,TWAPTR)                                 0108 00251000
         BALR  @14,@15                                             0108 00252000
*            R1=R1+4 ;                 /* R1 PTR TO THE LENGTH AND   */ 00253000
         AL    R1,@CF00036                                         0109 00254000
*                                      /* MSG, THEN SKIP LENGTH      */ 00255000
*            GEN ;                                                 0110 00256000
         WTO   MF=(E,(1))             ISSUE ACCEPTING LOGON MSG         00257000
*            END ;                                                 0111 00258000
*                 ELSE WERC=RCLGFAIL ; /* NO, STORE FAILURE RTN CODE */ 00259000
         B     @RC00104                                            0112 00260000
@RF00104 MVC   WERC(2,R10),@CH00086                                0112 00261000
*              RFY (R14,R15,R0,R1) UNRSTD /*UNRESTRI PARA R */ ;   0113 00262000
@RC00104 DS    0H                                                  0114 00263000
*              END ;                                               0114 00264000
*           END ;                                                  0115 00265000
*           ELSE WERC=RCLGFAIL ;       /* NOT BROKEN CONNECTION, IT  */ 00266000
         B     @RC00098                                            0116 00267000
@RF00098 MVC   WERC(2,R10),@CH00086                                0116 00268000
*                                      /*   IS SOMETHING ELSE THEN   */ 00269000
*                                      /*   STORE FAILURE RTN CODE   */ 00270000
*         END ;                                                    0117 00271000
@RC00098 DS    0H                                                  0118 00272000
*      END ;                                                       0118 00273000
@RC00086 DS    0H                                                  0119 00274000
*      R14=SA3 ;                           /* RESTORE REG           */  00275000
@RF00082 L     R14,SA3                                             0119 00276000
*      END START ;                                                 0120 00277000
@EL00004 DS    0H                                                  0120 00278000
@EF00004 DS    0H                                                  0120 00279000
@ER00004 BR    @14                                                 0120 00280000
