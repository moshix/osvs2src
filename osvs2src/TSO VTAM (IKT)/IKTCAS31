*     DO;                           /* YES                           */ 00419000
*       R8=ADDR(LBPTR);             /* FREEMAIN ADDRESS              */ 00420000
         LA    R8,LBPTR                                            0254 00421000
*       R9=LBLEN;                   /* GET LENGTH                    */ 00422000
         LH    R9,LBLEN                                            0255 00423000
