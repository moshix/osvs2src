         TITLE '*** IKTCAS31 - ADDRESS SPACE CREATION ***              *00001000
                        '                                               00002000
IKTCAS31 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKTCAS31  77.319'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
*   IF TWAUEIFC(2)=FCUICR           /* IS FOOTPRINT ALREADY SET      */ 00018000
*     THEN                                                         0128 00019000
         CLI   TWAUEIFC+8(TWAPTR),17                               0128 00020000
         BNE   @RF00128                                            0128 00021000
*     DO;                           /* YES                           */ 00022000
*       CALL FREEBLK;               /* CALL FREE GETMAIN STORAGE     */ 00023000
         BAL   @14,FREEBLK                                         0130 00024000
*       CALL RELTERM;               /* TO RELEASE TERMINAL ROUTINE   */ 00025000
         BAL   @14,RELTERM                                         0131 00026000
*       RETURN;                     /* RETURN TO ESTAI EXIT          */ 00027000
