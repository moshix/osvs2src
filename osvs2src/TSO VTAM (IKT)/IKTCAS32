         TITLE 'IKTCAS32                                               *00001000
                        '                                               00002000
IKTCAS32 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKTCAS32  77.319'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
*    /*                                                              */ 00018000
*    /*      IN ORDER TO CANCEL OR TERMINATE MEMORY OF USER          */ 00019000
*    /*      ADDRESS SPACES, ADDRESS OF ASCB OR ASID MUST BE OBTAINED*/ 00020000
*    /*      A GETMAIN STORAGE IS PROVIDED TO LIST ALL ASCB          */ 00021000
*    /*      ADDRESSES.                                              */ 00022000
*    /*                                                              */ 00023000
*ASTERM :                                                          0023 00024000
*      IF TWAUEIFC(2)=FCUITC           /* IS THIS RETRY, BECAUSE     */ 00025000
*                                      /* FOOTPRINT ALREAD SET       */ 00026000
*         THEN DO ;                    /* YES                        */ 00027000
ASTERM   CLI   TWAUEIFC+8(TWAPTR),33                               0023 00028000
         BNE   @RF00023                                            0023 00029000
*         CALL FREE ;                  /* FREE MAIN IF GETMAINED     */ 00030000
         BAL   @14,FREE                                            0025 00031000
*         END ;                                                    0026 00032000
*         ELSE DO ;                    /* NORMAL ENTRY               */ 00033000
         B     @RC00023                                            0027 00034000
@RF00023 DS    0H                                                  0028 00035000
*      TWAUEIFC(2)=FCUITC ;            /* SET FOOTPRINT IN ESTAI PARA*/ 00036000
         MVI   TWAUEIFC+8(TWAPTR),X'21'                            0028 00037000
*      TWAUERA(2)=ADDR(ASTERM) ;        /* SPECIFY RETRY ADDR      */   00038000
         LA    @15,ASTERM                                          0029 00039000
         STCM  @15,7,TWAUERA+8(TWAPTR)                             0029 00040000
*      TWAUERRS(2)=EVAL(R13+4)->P  ;   /* STORE SAVE AREA ADDRESS    */ 00041000
         LA    @15,4                                               0030 00042000
         ALR   @15,R13                                             0030 00043000
         L     @15,P(,@15)                                         0030 00044000
         ST    @15,TWAUERRS+8(,TWAPTR)                             0030 00045000
*         END ;                                                    0031 00046000
*      RFY TCAST BASED (TWATCAST) ;     /* GET TCAST ADDR            */ 00047000
@RC00023 DS    0H                                                  0033 00048000
*      RFY  WESTD  BASED(R10) ;                                    0033 00049000
*      IF TCASUSEC^=0                  /* IS USER COUNT NOT ZERO     */ 00050000
*         THEN DO ;                    /* NO, PERFORM THE FUNCTION   */ 00051000
         L     @01,TWATCAST(,TWAPTR)                               0034 00052000
         LH    @15,TCASUSEC(,@01)                                  0034 00053000
         LTR   @15,@15                                             0034 00054000
         BZ    @RF00034                                            0034 00055000
*      R8=TCASUSEC ;                    /* GET USER COUNT            */ 00056000
         LR    R8,@15                                              0036 00057000
*    /*                                                              */ 00058000
*    /*        GET STORAGE  FOR A LIST OF ASCB ADDRESS               */ 00059000
*    /*                                                              */ 00060000
*      R8=(R8+4)*4 ;                    /* CALCULATE GETMAIN LENGTH  */ 00061000
         LA    @15,4                                               0037 00062000
         ALR   @15,R8                                              0037 00063000
         SLA   @15,2                                               0037 00064000
         LR    R8,@15                                              0037 00065000
*      RFY (R14,R15,R0,R1) RSTD  /* RESTRICT PARA REG*/ ;          0038 00066000
*      GEN CODE ;                                                  0039 00067000
         GETMAIN R,LV=(8)         GETMAIN FOR LIST OF ASCB ADDR         00068000
*      AALISTP=R1 ;                     /* STORE POINTER             */ 00069000
         ST    R1,AALISTP                                          0040 00070000
*      AALISTL=R8 ;                     /* STORE LENGTH              */ 00071000
         ST    R8,AALISTL                                          0041 00072000
*      R9=AALISTP ;                     /* LOAD LIST POINTER         */ 00073000
         L     @07,AALISTP                                         0042 00074000
         LR    R9,@07                                              0042 00075000
*      R8=FCUIAA  ;                     /* INDICATE EXTRACT ASCB ADDR*/ 00076000
         LA    R8,50                                               0043 00077000
*    /*                                                              */ 00078000
*    /*      CALL IKTCAS33 TO FILL IN ASCB ADDRESSES                 */ 00079000
*    /*                                                              */ 00080000
*      GEN CODE ;                                                  0044 00081000
         MODESET KEY=ZERO,MODE=SUP  GET INTO SUPV STATE KEY ZERO        00082000
*      RFY (R14,R15,R0,R1) UNRSTD /*UNRESTRI PARA R */ ;           0045 00083000
*      CALL IKTCAS33 ;                  /* TO GET ASCB ADDR          */ 00084000
         L     @15,@CV01088                                        0046 00085000
         BALR  @14,@15                                             0046 00086000
*      RFY (R14,R15,R0,R1) RSTD  /* RESTRICT PARA REG*/ ;          0047 00087000
*    /*                                                              */ 00088000
*    /*        ISSUE MEMORY TERMINATION BY CALLING  RTM              */ 00089000
*    /*                                                              */ 00090000
*      IF WECODE1F=FCUITM               /* IS THIS MEMORY TERM       */ 00091000
*         THEN DO ;                     /* YES                       */ 00092000
         CLI   WECODE1F(R10),X'22'                                 0048 00093000
         BNE   @RF00048                                            0048 00094000
*         ASCBPTR=R9 -> P ;             /* LOAD ASCB ADDR            */ 00095000
         L     ASCBPTR,P(,R9)                                      0050 00096000
*         DO WHILE (ASCBPTR ^= 0 ) ;    /* ISSUE MEMTERM FOR ALL A.S.*/ 00097000
         B     @DE00051                                            0051 00098000
@DL00051 DS    0H                                                  0052 00099000
*         R8=ASCBASID ;                 /* GET ASID                  */ 00100000
         LH    R8,ASCBASID(,ASCBPTR)                               0052 00101000
         N     R8,@CF01107                                         0052 00102000
*         GEN CODE REFS (CVTBTERM) SETS(R2,R7) ;                   0053 00103000
         L     7,4(13)       SAVE IT- RTM DESTROY SAVEAREA BACK PTR     00104000
         CALLRTM TYPE=MEMTERM,COMPCOD=X'622',ASID=(8)  TERM MEMORY      00105000
         ST    7,4(13)       RESTORE SAVEAREA PTR                       00106000
*       R9=R9+4 ;                       /* NEXT ASCB ADDR            */ 00107000
         AL    R9,@CF00053                                         0054 00108000
*         ASCBPTR=R9 -> P ;             /* LOAD ASCB ADDR            */ 00109000
         L     ASCBPTR,P(,R9)                                      0055 00110000
*         END ;                         /* LOOP                      */ 00111000
@DE00051 LTR   ASCBPTR,ASCBPTR                                     0056 00112000
         BNZ   @DL00051                                            0056 00113000
*         END ;                         /* DONE                      */ 00114000
*    /*                                                              */ 00115000
*    /*        ISSUE SYSTEM INITIATED  CANCEL                        */ 00116000
*    /*                                                              */ 00117000
*      IF WECODE1F=FCUITC               /* IS THIS SIC               */ 00118000
*         THEN DO ;                    /* YES                        */ 00119000
@RF00048 CLI   WECODE1F(R10),X'21'                                 0058 00120000
         BNE   @RF00058                                            0058 00121000
*         R1=R9 ->P ;                   /* R1 CONTAIN ASCB ADDR      */ 00122000
         L     R1,P(,R9)                                           0060 00123000
*         RFY (R14,R15) UNRSTD ;                                   0061 00124000
*         DO WHILE (R1 ^=0);            /* SIC ALL USERS             */ 00125000
         B     @DE00062                                            0062 00126000
@DL00062 DS    0H                                                  0063 00127000
*         R0=COMPCODE ;                 /* LOAD COMPLETION CODE      */ 00128000
         ICM   R0,15,COMPCODE                                      0063 00129000
*         CALL SIC  ;                   /* BRANCH TO SIC ROUTINE     */ 00130000
         L     @14,CVTPTR                                          0064 00131000
         L     @15,CVTSIC(,@14)                                    0064 00132000
         BALR  @14,@15                                             0064 00133000
*         R9=R9+4 ;                     /* POINTS TO NEXT ADDR       */ 00134000
         AL    R9,@CF00053                                         0065 00135000
*         R1=R9 -> P ;                  /* LOAD ASCB ADDR            */ 00136000
         L     R1,P(,R9)                                           0066 00137000
*         END ;                         /* LOOP                      */ 00138000
@DE00062 LTR   R1,R1                                               0067 00139000
         BNZ   @DL00062                                            0067 00140000
*         END ;                                                    0068 00141000
*      GEN CODE ;                                                  0069 00142000
@RF00058 DS    0H                                                  0069 00143000
         MODESET KEY=NZERO,MODE=PROB  BACK TO ORIGINAL                  00144000
*         CALL FREE ;                  /* FREEMAIN STORAGE           */ 00145000
         BAL   @14,FREE                                            0070 00146000
*      END ;                                                       0071 00147000
*      TWAUEIFC(2)=0 ;                 /* RESET FOOTPRINT FOR EXIT   */ 00148000
@RF00034 MVI   TWAUEIFC+8(TWAPTR),X'00'                            0072 00149000
*    /*                                                              */ 00150000
*    /*        FREEMAIN                                              */ 00151000
*    /*                                                              */ 00152000
*FREE : PROC OPTIONS(SAVE(R14),NOSAVEAREA) ;                       0073 00153000
@EL00001 L     @13,4(,@13)                                         0073 00154000
@EF00001 DS    0H                                                  0073 00155000
@ER00001 LM    @14,@12,12(@13)                                     0073 00156000
         BR    @14                                                 0073 00157000
FREE     ST    @14,12(,@13)                                        0073 00158000
*      IF AALISTP^=0                   /*  IS THERE GETMAIN AREA     */ 00159000
*         THEN DO ;                    /* YES, BECAUSE PTR NOT ZERO  */ 00160000
         L     @15,AALISTP                                         0074 00161000
         LTR   @15,@15                                             0074 00162000
         BZ    @RF00074                                            0074 00163000
*         RFY (R14,R15,R0,R1) RSTD  /* RESTRICT PARA REG*/ ;       0076 00164000
*      R9=AALISTP ;                     /* GET POINTER OF GETMAIN    */ 00165000
         L     @07,AALISTP                                         0077 00166000
         LR    R9,@07                                              0077 00167000
*      R8=AALISTL ;                     /* GET LENGTH                */ 00168000
         L     R8,AALISTL                                          0078 00169000
*      GEN CODE ;                                                  0079 00170000
         FREEMAIN R,LV=(R8),A=(R9)        FREEMAIN FOR TEMP STORAGE     00171000
*      RFY (R14,R15,R0,R1) UNRSTD /*UNRESTRI PARA R */ ;           0080 00172000
*         AALISTP=0 ;                  /* RESET PTR                  */ 00173000
         SLR   @15,@15                                             0081 00174000
         ST    @15,AALISTP                                         0081 00175000
*         AALISTL=0 ;                  /* DONT NEED ANY MORE LENGTH  */ 00176000
         ST    @15,AALISTL                                         0082 00177000
*         END ;                                                    0083 00178000
*      END ;                                                       0084 00179000
