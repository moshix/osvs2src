*     IF I<9 THEN                                                  0703 01862000
         C     I,@CF00069                                          0703 01863000
         BNL   @RF00703                                            0703 01864000
*       WORKPRM(I)=WORKCHAR;                                       0704 01865000
         LA    @04,WORKPRM-1(I)                                    0704 01866000
         MVC   0(1,@04),WORKCHAR(SCANPTR)                          0704 01867000
*     SCANPTR=SCANPTR+1;                                           0705 01868000
@RF00703 AL    SCANPTR,@CF00057                                    0705 01869000
*   END;                                                           0706 01870000
@DE00701 CLI   WORKCHAR(SCANPTR),C'='                              0706 01871000
         BE    @DC00701                                            0706 01872000
         CLI   WORKCHAR(SCANPTR),C' '                              0706 01873000
         BE    @DC00701                                            0706 01874000
         CLI   WORKCHAR(SCANPTR),C','                              0706 01875000
         BNE   @DL00701                                            0706 01876000
@DC00701 DS    0H                                                  0707 01877000
*   IF I=0|                         /* ERROR IN                      */ 01878000
*       I>8|                        /* LENGTH OR                     */ 01879000
*       WORKCHAR=' '|               /* ENDING                        */ 01880000
*       WORKCHAR=',' THEN           /* CHARACTER                     */ 01881000
         LTR   I,I                                                 0707 01882000
         BZ    @RT00707                                            0707 01883000
