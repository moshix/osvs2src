*NBRXTRCT:                                                         0781 02118000
*   PROC OPTIONS(SAVE(14));                                        0781 02119000
NBRXTRCT ST    @14,12(,@13)                                        0781 02120000
*                                                                  0782 02121000
*/*  *****************************************************************/ 02122000
*/*                                                                  */ 02123000
*/*    THIS SUBROUTINE MOVES A PARAMETER INTO A WORKAREA, VERIFIES   */ 02124000
*/*    THAT IT IS NUMERIC, AND CHECKS ITS LENGTH.                    */ 02125000
*/*                                                                  */ 02126000
*/*  *****************************************************************/ 02127000
*                                                                  0782 02128000
*   I=0;                            /* ZERO OUT LENGTH COUNTER       */ 02129000
         SLR   I,I                                                 0782 02130000
*   DO WHILE(WORKCHAR^=','&WORKCHAR^=' ');                         0783 02131000
         B     @DE00783                                            0783 02132000
@DL00783 DS    0H                                                  0784 02133000
*     I=I+1;                        /* ADD 1 TO LENGTH               */ 02134000
         AL    I,@CF00057                                          0784 02135000
*     IF I<9 THEN                   /* LENGTH STILL GOOD             */ 02136000
         C     I,@CF00069                                          0785 02137000
         BNL   @RF00785                                            0785 02138000
*       DO;                         /* YES                           */ 02139000
*         WORKPRM(I)=WORKCHAR;      /* MOVE NEXT CHARACTER           */ 02140000
         LA    @04,WORKPRM-1(I)                                    0787 02141000
         MVC   0(1,@04),WORKCHAR(SCANPTR)                          0787 02142000
*         IF WORKCHAR<'0'|          /* IS IT                         */ 02143000
*             WORKCHAR>'9' THEN     /* NON-NUMERIC                   */ 02144000
         CLI   WORKCHAR(SCANPTR),C'0'                              0788 02145000
         BL    @RT00788                                            0788 02146000
         CLI   WORKCHAR(SCANPTR),C'9'                              0788 02147000
         BNH   @RF00788                                            0788 02148000
@RT00788 DS    0H                                                  0789 02149000
*           I=9;                    /* YES - FORCE ERROR             */ 02150000
         LA    I,9                                                 0789 02151000
*       END;                                                       0790 02152000
@RF00788 DS    0H                                                  0791 02153000
*     SCANPTR=SCANPTR+1;            /* PT AT NEXT CHARACTER          */ 02154000
@RF00785 AL    SCANPTR,@CF00057                                    0791 02155000
*   END;                                                           0792 02156000
@DE00783 CLI   WORKCHAR(SCANPTR),C','                              0792 02157000
         BE    @DC00783                                            0792 02158000
         CLI   WORKCHAR(SCANPTR),C' '                              0792 02159000
         BNE   @DL00783                                            0792 02160000
@DC00783 DS    0H                                                  0793 02161000
*   IF I=0|I>8 THEN                 /* ANY ERRORS DISCOVERED         */ 02162000
