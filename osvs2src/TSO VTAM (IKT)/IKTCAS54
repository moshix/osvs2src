*   DO WHILE(TBLNAME^=KEYWDNM);     /* FIND                          */ 00648000
         B     @DE00313                                            0313 00649000
@DL00313 DS    0H                                                  0314 00650000
*     TABLEPTR=TABLEPTR+20;         /* PROPER                        */ 00651000
         AL    TABLEPTR,@CF00144                                   0314 00652000
*   END;                            /* ENTRY                         */ 00653000
@DE00313 CLC   TBLNAME(8,TABLEPTR),KEYWDNM                         0315 00654000
         BNE   @DL00313                                            0315 00655000
*   SAVEPTR=ADDR(SAVEBUFR)+8*(TBLDISP-1);/* ADDR OF MBR SAVE AREA    */ 00656000
         LA    SAVEPTR,SAVEBUFR                                    0316 00657000
