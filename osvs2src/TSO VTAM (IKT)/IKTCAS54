*         RETURN;                   /* AND EXIT                      */ 00368000
         B     @EL00003                                            0221 00369000
*       END;                                                       0222 00370000
*     CALL PRCSPARM;                /* VERIFY KEYWORD PARAMETER      */ 00371000
@RF00218 BAL   @14,PRCSPARM                                        0223 00372000
*     IF SAVECHAR=HEXFF THEN        /* WAS THE PARM INVALID          */ 00373000
         CLI   SAVECHAR(SAVEPTR),X'FF'                             0224 00374000
         BNE   @RF00224                                            0224 00375000
*       DO;                         /* YES                           */ 00376000
*         MLPMINV='1'B;             /* SHOW IT                       */ 00377000
         OI    MLPMINV(MLPMPTR),B'01000000'                        0226 00378000
*         RETURN;                   /* AND EXIT                      */ 00379000
