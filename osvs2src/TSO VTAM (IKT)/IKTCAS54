         BZ    @RF00825                                            0825 02290000
*     DO;                           /* YES                   @ZM20856*/ 02291000
*       SAVECHAR=HEXFF;             /* FLAG ERROR            @ZM20856*/ 02292000
         MVI   SAVECHAR(SAVEPTR),X'FF'                             0827 02293000
*       RETURN;                     /*                       @ZM20856*/ 02294000
         B     @EL00017                                            0828 02295000
*     END;                          /*                       @ZM20856*/ 02296000
*   ELSE                                                           0830 02297000
