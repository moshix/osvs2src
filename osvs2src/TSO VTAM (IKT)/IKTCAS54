*       IF TBLRSTD='1'B THEN        /* RESTRICTED VALUES             */ 02054000
@RF00747 TM    TBLRSTD(TABLEPTR),B'10000000'                       0756 02055000
         BNO   @RF00756                                            0756 02056000
*         DO;                       /* YES                           */ 02057000
*           RSTDPTR=TBLPTR;         /* PTR TO RSTD TABLE             */ 02058000
         L     RSTDPTR,TBLPTR(,TABLEPTR)                           0758 02059000
*           DO WHILE(RSTDCHAR^=HEXFF&RSTDNAME^=WORKPARM);/* SEARCH   */ 02060000
         B     @DE00759                                            0759 02061000
@DL00759 DS    0H                                                  0760 02062000
*             RSTDPTR=RSTDPTR+8;    /* FOR A                         */ 02063000
         AL    RSTDPTR,@CF00034                                    0760 02064000
*           END;                    /* MATCH                         */ 02065000
@DE00759 CLI   RSTDCHAR(RSTDPTR),X'FF'                             0761 02066000
         BE    @DC00759                                            0761 02067000
         CLC   RSTDNAME(8,RSTDPTR),WORKPARM                        0761 02068000
         BNE   @DL00759                                            0761 02069000
@DC00759 DS    0H                                                  0762 02070000
*           IF RSTDCHAR=HEXFF THEN  /* FIND ONE                      */ 02071000
         CLI   RSTDCHAR(RSTDPTR),X'FF'                             0762 02072000
         BNE   @RF00762                                            0762 02073000
*             SAVECHAR=HEXFF;       /* NOPE                          */ 02074000
         MVI   SAVECHAR(SAVEPTR),X'FF'                             0763 02075000
*         END;                                                     0764 02076000
@RF00762 DS    0H                                                  0765 02077000
*       IF SAVECHAR^=HEXFF THEN     /* STILL GOOD VALUE              */ 02078000
@RF00756 CLI   SAVECHAR(SAVEPTR),X'FF'                             0765 02079000
         BE    @RF00765                                            0765 02080000
*         IF TBLBINRY='1'B THEN     /* CONVERT TO BINARY             */ 02081000
         TM    TBLBINRY(TABLEPTR),B'00100000'                      0766 02082000
         BNO   @RF00766                                            0766 02083000
*           DO;                                                    0767 02084000
*             PACK(TEMPAREA,WORKPARM);/* PACK THE NUMBER             */ 02085000
         PACK  TEMPAREA(8),WORKPARM(8)                             0768 02086000
*             WORKPARM=''B;         /* ZERO OUT RECEIVNG FIELD       */ 02087000
         XC    WORKPARM(8),WORKPARM                                0769 02088000
*             CVB(WRKPRMF2,TEMPAREA);/* CVB INTO WORKPARM            */ 02089000
         CVB   @04,TEMPAREA                                        0770 02090000
         ST    @04,WRKPRMF2                                        0770 02091000
*             IF TBLRANGE='1'B THEN /* DO A RANGE CHECK              */ 02092000
