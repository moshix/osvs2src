*   IF SAVECHAR^=HEXBC THEN         /* MEMBER= ENTERED               */ 00664000
         CLI   SAVECHAR(SAVEPTR),X'BC'                             0318 00665000
         BE    @RF00318                                            0318 00666000
*     JFCBELNM=SAVAREA;             /* YES - MOVE IN NEW NAME        */ 00667000
         MVC   JFCBELNM(8,JFCBPTR),SAVAREA(SAVEPTR)                0319 00668000
