*     TABLEPTR=TABLEPTR+20;         /* PROPER                        */ 00531000
         AL    TABLEPTR,@CF00144                                   0278 00532000
*   END;                            /* ENTRY                         */ 00533000
@DE00277 CLC   TBLNAME(8,TABLEPTR),KEYWDNM                         0279 00534000
         BNE   @DL00277                                            0279 00535000
*   SAVEPTR=ADDR(SAVEBUFR)+8*(TBLDISP-1);/* ADDR OF WORK AREA        */ 00536000
         LA    SAVEPTR,SAVEBUFR                                    0280 00537000
         LH    @04,TBLDISP(,TABLEPTR)                              0280 00538000
         BCTR  @04,0                                               0280 00539000
         SLA   @04,3                                               0280 00540000
         ALR   SAVEPTR,@04                                         0280 00541000
*   IF SAVECHAR=HEXBC THEN                                         0281 00542000
         CLI   SAVECHAR(SAVEPTR),X'BC'                             0281 00543000
         BE    @RT00281                                            0281 00544000
*     ;                             /* MEMBER= ENTERED NO - USE NAME    00545000
*                                      IN PROC                       */ 00546000
*   ELSE                                                           0283 00547000
*     DO;                           /* YES - GET MEMBER NAME         */ 00548000
*       WORKPARM=SAVAREA;           /* GET NAME ENTERED BY THE     0284 00549000
*                                      OPERATOR IF ITS 2 DIGITS, ADD    00550000
*                                      THEM TO 'MEMDEFLT', ELSE USE     00551000
*                                      THE NAME AS IS                */ 00552000
         MVC   WORKPARM(8),SAVAREA(SAVEPTR)                        0284 00553000
*       IF(WORKPRM(1)>='0'&WORKPRM(1)<='9')&(WORKPRM(2)>='0'&WORKPRM(2) 00554000
*           <='9') THEN                                            0285 00555000
         CLI   WORKPRM,C'0'                                        0285 00556000
         BL    @RF00285                                            0285 00557000
         CLI   WORKPRM,C'9'                                        0285 00558000
         BH    @RF00285                                            0285 00559000
         CLI   WORKPRM+1,C'0'                                      0285 00560000
         BL    @RF00285                                            0285 00561000
