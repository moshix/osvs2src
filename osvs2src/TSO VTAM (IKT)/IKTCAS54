@DL00728 DS    0H                                                  0729 01976000
*             SCANPTR=SCANPTR+1;    /* NEXT CHAR             @ZM20856*/ 01977000
         AL    SCANPTR,@CF00057                                    0729 01978000
*           END;                    /* PT PAST PARM          @ZM20856*/ 01979000
@DE00728 CLI   WORKPARM,C','                                       0730 01980000
         BE    @DC00728                                            0730 01981000
         CLI   WORKPARM,C' '                                       0730 01982000
         BNE   @DL00728                                            0730 01983000
@DC00728 DS    0H                                                  0731 01984000
*           RETURN;                 /* IGNORE > 16           @ZM20856*/ 01985000
@EL00014 DS    0H                                                  0731 01986000
@EF00014 DS    0H                                                  0731 01987000
@ER00014 L     @14,@SA00014                                        0731 01988000
         BR    @14                                                 0731 01989000
*         END;                      /*                       @ZM20856*/ 01990000
*       ELSE                        /* INCREMENT COUNT       @ZM20856*/ 01991000
*         CALL SPLITPRM;            /* SPLIT PARMS           @ZM20856*/ 01992000
@RF00726 BAL   @14,SPLITPRM                                        0733 01993000
*     END;                          /* END PROCESS           @ZM20856*/ 01994000
*   ELSE                                                           0735 01995000
*     DO;                           /* OTHER KEYWORD         @ZM20856*/ 01996000
         B     @RC00723                                            0735 01997000
@RF00723 DS    0H                                                  0736 01998000
*       SAVEPTR=SAVEPTR+8*(TBLDISP-1);/* ADDRESS PROPER WORK AREA  0736 01999000
*                                                            @ZM20856*/ 02000000
         LH    @04,TBLDISP(,TABLEPTR)                              0736 02001000
         BCTR  @04,0                                               0736 02002000
         SLA   @04,3                                               0736 02003000
         ALR   SAVEPTR,@04                                         0736 02004000
*       IF SAVECHAR^=HEXBC THEN     /* PARAMETER ALREADY THERE       */ 02005000
         CLI   SAVECHAR(SAVEPTR),X'BC'                             0737 02006000
         BE    @RF00737                                            0737 02007000
*         DO;                       /* YES                           */ 02008000
*           SAVECHAR=HEXFF;         /* FLAG AS AN ERROR              */ 02009000
         MVI   SAVECHAR(SAVEPTR),X'FF'                             0739 02010000
*           RETURN;                 /* EXIT                          */ 02011000
         B     @EL00014                                            0740 02012000
*         END;                                                     0741 02013000
*       IF TBLNUMRC='1'B THEN       /* VALUE MUST BE NUMERIC         */ 02014000
@RF00737 TM    TBLNUMRC(TABLEPTR),B'01000000'                      0742 02015000
         BNO   @RF00742                                            0742 02016000
*         CALL NBRXTRCT;            /* YES                           */ 02017000
         BAL   @14,NBRXTRCT                                        0743 02018000
*       ELSE                                                       0744 02019000
*         CALL REGXTRCT;            /* NO                            */ 02020000
         B     @RC00742                                            0744 02021000
@RF00742 BAL   @14,REGXTRCT                                        0744 02022000
*       IF SAVECHAR=HEXFF THEN      /* ERROR DETECTED                */ 02023000
@RC00742 CLI   SAVECHAR(SAVEPTR),X'FF'                             0745 02024000
         BE    @RT00745                                            0745 02025000
*         RETURN;                   /* YES                           */ 02026000
*       IF TBLRIGHT='1'B&I<8 THEN   /* RIGHT-JUSTIFY                 */ 02027000
         TM    TBLRIGHT(TABLEPTR),B'00010000'                      0747 02028000
         BNO   @RF00747                                            0747 02029000
