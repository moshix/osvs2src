         TITLE '*** IKTCAS54 - PARAMETER PROCESSOR SCANNER             *00001000
                        '                                               00002000
IKTCAS54 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKTCAS54  77.336'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @12,0                                               0001 00012000
@PSTART  LA    @08,4095(,@12)                                      0001 00013000
         USING @PSTART,@12                                         0001 00014000
         USING @PSTART+4095,@08                                    0001 00015000
         L     @00,@SIZDATD                                        0001 00016000
         GETMAIN  R,LV=(0)                                              00017000
         LR    @10,@01                                             0001 00018000
         USING @DATD,@10                                           0001 00019000
         ST    @13,@SA00001+4                                      0001 00020000
         LM    @00,@01,20(@13)                                     0001 00021000
         ST    @10,8(,@13)                                         0001 00022000
         LR    @13,@10                                             0001 00023000
         EJECT                                                          00024000
*                                                                  0119 00025000
*/*  *****************************************************************/ 00026000
*/*                                                                  */ 00027000
*/*    THIS PROGRAM IS ORGANIZED AS FOLLOWS:                         */ 00028000
*/*                                                                  */ 00029000
*/*        A - HOUSEKEEPING                                          */ 00030000
*/*                                                                  */ 00031000
*/*        B - MAINLINE, WHICH CONSISTS OF THE FOLLOWING:            */ 00032000
*/*            1)  IF DATA IS IN A CIB, MOVE IT INTO A WORK BUFFER   */ 00033000
*/*                AND RELEASE THE CIB WITH THE QEDIT MACRO          */ 00034000
*/*            2)  VALIDITY CHECKING OF INPUT OPTIONS.  IF ANY       */ 00035000
*/*                INCONSISTENCIES ARE FOUND, TERMINATE WITH A RETURN*/ 00036000
*/*                CODE OF 4.                                        */ 00037000
*/*            3)  SCAN PARAMETERS, FROM EITHER A DATA OR WORK       */ 00038000
*/*                BUFFER.                                           */ 00039000
*/*            4)  IF PARAMETERS FROM A PARMLIB ARE DESIRED, READ IN */ 00040000
*/*                THE DESIRED MEMBER AND PROCESS ITS PARAMETERS.    */ 00041000
*/*            5)  IF NECESSARY, OVERLAY MEMBER PARAMETERS WITH      */ 00042000
*/*                BUFFER PARAMETERS.                                */ 00043000
*/*            6)  IF REQUESTED, UPDATE TCAS TABLE                   */ 00044000
*/*            7)  IF REQUESTED, WRITE OUT PARAMETERS                */ 00045000
*/*                                                                  */ 00046000
*/*        C - CLEANUP AND RETURN                                    */ 00047000
*/*                                                                  */ 00048000
*/*  *****************************************************************/ 00049000
*                                                                  0119 00050000
*   SCANERR='0'B;                   /* INITIALIZE SW                 */ 00051000
*   GOSW1='1'B;                     /* INITIALIZE SW                 */ 00052000
         OI    GOSW1,B'00100000'                                   0120 00053000
         NI    SCANERR,B'01111111'                                 0120 00054000
*   PRNTPTR=ADDR(PRNTDAT5);         /* 1ST CHAR IN PRINT LINE        */ 00055000
         LA    PRNTPTR,PRNTDAT5                                    0121 00056000
*   PRNTEND=PRNTPTR+67;             /* LAST CHAR IN PRINT LINE       */ 00057000
         LA    @15,67                                              0122 00058000
         ALR   @15,PRNTPTR                                         0122 00059000
         ST    @15,PRNTEND                                         0122 00060000
*   SYNADSW1='N';                   /* INITIALIZE                    */ 00061000
         MVI   SYNADSW1,C'N'                                       0123 00062000
*   SYNADSW2='N';                   /* ERROR                         */ 00063000
         MVI   SYNADSW2,C'N'                                       0124 00064000
*   DCBABSW1='N';                   /* HANDLING                      */ 00065000
         MVI   DCBABSW1,C'N'                                       0125 00066000
*   DCBABSW2='N';                   /* SWITCHES                      */ 00067000
         MVI   DCBABSW2,C'N'                                       0126 00068000
*   CALL CHKOPTNS;                  /* CHECK INPUT OPTIONS, ETC      */ 00069000
         BAL   @14,CHKOPTNS                                        0127 00070000
*   IF MLPMINV='1'B THEN            /* ERRORS                        */ 00071000
         TM    MLPMINV(MLPMPTR),B'01000000'                        0128 00072000
         BNO   @RF00128                                            0128 00073000
*     RTNCODE=4;                    /* YES - NON-ZERO RTN CODE       */ 00074000
         MVC   RTNCODE(2),@CH00063                                 0129 00075000
*   ELSE                                                           0130 00076000
*     DO;                           /* NO - CONTINUE ON WITH         */ 00077000
         B     @RC00128                                            0130 00078000
@RF00128 DS    0H                                                  0131 00079000
*       RTNCODE=0;                  /* ZERO RETURN CODE              */ 00080000
         SLR   @02,@02                                             0131 00081000
         STH   @02,RTNCODE                                         0131 00082000
*       CALL SCANPARM;              /* SCAN BUFFER PARAMETERS        */ 00083000
         BAL   @14,SCANPARM                                        0132 00084000
*       IF MLPMINV='0'B THEN        /* ALL THE PARMS OK              */ 00085000
         TM    MLPMINV(MLPMPTR),B'01000000'                        0133 00086000
         BNZ   @RF00133                                            0133 00087000
*         DO;                       /* YES - CONTINUE ON             */ 00088000
*           CALL READMBR;           /* READ AND PROCESS PARMLIB      */ 00089000
         BAL   @14,READMBR                                         0135 00090000
*           CALL OVERLAY;           /* MERGE PARMS                   */ 00091000
         BAL   @14,OVERLAY                                         0136 00092000
*           CALL TCASUPDT;          /* UPDATE TCAS TABLE PARMS       */ 00093000
         BAL   @14,TCASUPDT                                        0137 00094000
*           CALL WRITEOUT;          /* PRINT TCAS TABLE PARMS        */ 00095000
         BAL   @14,WRITEOUT                                        0138 00096000
*         END;                                                     0139 00097000
*       ELSE                        /* NO - ERROR FOUND              */ 00098000
*         IF PRINTSW='1'B THEN      /* DOING A PRINTOUT              */ 00099000
         B     @RC00133                                            0140 00100000
@RF00133 TM    PRINTSW,B'00001000'                                 0140 00101000
         BNO   @RF00140                                            0140 00102000
*           DO;                     /* YES                           */ 00103000
*             GEN(CLOSE PRINTOUT)   /* CLOSE PRINTOUT                */ 00104000
*             REFS(R0,R1,R14,R15,PRINTOUT);                        0142 00105000
         CLOSE PRINTOUT                                                 00106000
*           END;                                                   0143 00107000
*     END;                                                         0144 00108000
@RF00140 DS    0H                                                  0144 00109000
@RC00133 DS    0H                                                  0145 00110000
*   RESPECIFY                                                      0145 00111000
*    (R15) RESTRICTED;                                             0145 00112000
@RC00128 DS    0H                                                  0146 00113000
*   R15=RTNCODE;                    /* INSERT RETURN CODE            */ 00114000
         LH    R15,RTNCODE                                         0146 00115000
*   RESPECIFY                                                      0147 00116000
*    (R15) UNRESTRICTED;                                           0147 00117000
*   RETURN;                         /* FINISHED                      */ 00118000
@EL00001 L     @13,4(,@13)                                         0148 00119000
@EF00001 L     @00,@SIZDATD                                        0148 00120000
         LR    @01,@10                                             0148 00121000
         FREEMAIN R,LV=(0),A=(1)                                        00122000
@ER00001 L     @14,12(,@13)                                        0148 00123000
         LM    @00,@12,20(@13)                                     0148 00124000
         BR    @14                                                 0148 00125000
         EJECT                                                          00126000
*CHKOPTNS:                                                         0149 00127000
*   PROC OPTIONS(SAVE(14));                                        0149 00128000
CHKOPTNS ST    @14,@SA00002                                        0149 00129000
*                                                                  0150 00130000
*/*  *****************************************************************/ 00131000
*/*                                                                  */ 00132000
*/*    THIS SUBROUTINE DOES THREE THINGS:                            */ 00133000
*/*                                                                  */ 00134000
*/*    -   IF PARAMETERS ARE IN A CIB BUFFER, MOVE THEM INTO A WORK  */ 00135000
*/*        BUFFER AND FREE THE CIB BUFFER                            */ 00136000
*/*    -   MAKE SURE THE INPUT OPTIONS ARE CONSISTENT.               */ 00137000
*/*    -   IF A PRINTOUT WANTED, OPEN THE PRINTOUT DCB               */ 00138000
*/*                                                                  */ 00139000
*/*  *****************************************************************/ 00140000
*                                                                  0150 00141000
*   CIBUFFER='';                    /* CLEAR WORK BUFFER             */ 00142000
         MVI   CIBUFFER,C' '                                       0150 00143000
         MVC   CIBUFFER+1(47),CIBUFFER                             0150 00144000
*   IF MLPMCIB='0'B|MLPMSCA=0 THEN                                 0151 00145000
         TM    MLPMCIB(MLPMPTR),B'00000001'                        0151 00146000
         BZ    @RT00151                                            0151 00147000
         L     @15,MLPMSCA(,MLPMPTR)                               0151 00148000
         LTR   @15,@15                                             0151 00149000
         BZ    @RT00151                                            0151 00150000
*     ;                             /* SKIP IF NO CIB BUFFER         */ 00151000
*   ELSE                            /* A CIB BUFFER                  */ 00152000
*     DO;                           /* YES                           */ 00153000
*       RESPECIFY                                                  0154 00154000
*        (CIBNAME)                  /* SET UP NEW BASE FOR CIB       */ 00155000
*             BASED(MLPMSCA);                                      0154 00156000
*       IF CIBDATLN>0 THEN          /* IF DATA, MOVE INTO            */ 00157000
         L     @15,MLPMSCA(,MLPMPTR)                               0155 00158000
         LH    @14,CIBDATLN(,@15)                                  0155 00159000
         LTR   @14,@14                                             0155 00160000
         BNP   @RF00155                                            0155 00161000
*         MVC(CIBUFFER(1:CIBDATLN),CIBDATA);/* WORK BUFFER           */ 00162000
         L     @01,MLPMSCA(,MLPMPTR)                               0156 00163000
         LH    @14,CIBDATLN(,@01)                                  0156 00164000
         BCTR  @14,0                                               0156 00165000
         EX    @14,@SM01643                                        0156 00166000
*       RESPECIFY                                                  0157 00167000
*        (CIBNAME);                 /* REVERT TO OLD BASE            */ 00168000
@RF00155 DS    0H                                                  0158 00169000
*       RESPECIFY                                                  0158 00170000
*        (R2,                                                      0158 00171000
*         R3) RESTRICTED;                                          0158 00172000
*       R2=ADDR(CHCIBP);            /* ADDR OF CIB PTR               */ 00173000
         L     R2,CSCBPTR(,TWAPTR)                                 0159 00174000
         LA    R2,CHCIBP(,R2)                                      0159 00175000
*       R3=MLPMSCA;                 /* ADDR OF CIB TO BE FREED       */ 00176000
         L     R3,MLPMSCA(,MLPMPTR)                                0160 00177000
*       GEN(QEDIT ORIGIN=(R2),BLOCK=(R3))/* FREE CIB                 */ 00178000
*       REFS(R0,R1,R2,R3,R14,R15);                                 0161 00179000
         QEDIT ORIGIN=(R2),BLOCK=(R3)                                   00180000
*       RESPECIFY                                                  0162 00181000
*        (R2,                                                      0162 00182000
*         R3) UNRESTRICTED;                                        0162 00183000
*     END;                                                         0163 00184000
*   IF MLPMFL1='00'X|               /* CHECK                         */ 00185000
*       (MLPMBUF='1'B&MLPMCIB='1'B) /* OBVIOUS                       */ 00186000
*     THEN                                                         0164 00187000
@RT00151 CLI   MLPMFL1(MLPMPTR),X'00'                              0164 00188000
         BE    @RT00164                                            0164 00189000
         TM    MLPMBUF(MLPMPTR),B'00000011'                        0164 00190000
         BNO   @RF00164                                            0164 00191000
@RT00164 DS    0H                                                  0165 00192000
*     DO;                           /* INPUT                         */ 00193000
*       MLPMINV='1'B;               /* ERRORS                        */ 00194000
         OI    MLPMINV(MLPMPTR),B'01000000'                        0166 00195000
*       RETURN;                     /* .                             */ 00196000
@EL00002 DS    0H                                                  0167 00197000
@EF00002 DS    0H                                                  0167 00198000
@ER00002 L     @14,@SA00002                                        0167 00199000
         BR    @14                                                 0167 00200000
*     END;                          /* .                             */ 00201000
*   IF MLPMWPM='1'B THEN            /* PRINTOUT WANTED               */ 00202000
@RF00164 TM    MLPMWPM(MLPMPTR),B'00010000'                        0169 00203000
         BNO   @RF00169                                            0169 00204000
*     DO;                           /* YES                           */ 00205000
*       GEN(OPEN  (PRINTOUT,OUTPUT))                               0171 00206000
*       REFS(R0,R1,R14,R15,PRINTOUT);                              0171 00207000
         OPEN  (PRINTOUT,OUTPUT)                                        00208000
*       DCBPTR=ADDR(PRINTOUT);      /* SETUP PTR                     */ 00209000
         LA    DCBPTR,PRINTOUT                                     0172 00210000
*       IF DCBABSW2='Y'|DCBOFOPN='0'B THEN/* GOOD OPEN               */ 00211000
         CLI   DCBABSW2,C'Y'                                       0173 00212000
         BE    @RT00173                                            0173 00213000
         TM    DCBOFOPN+40(DCBPTR),B'00010000'                     0173 00214000
         BNZ   @RF00173                                            0173 00215000
@RT00173 DS    0H                                                  0174 00216000
*         DO;                       /* NO                            */ 00217000
*           RESPECIFY                                              0175 00218000
*            (R1) RESTRICTED;                                      0175 00219000
*           R1=17;                  /* MESSAGE NUMBER                */ 00220000
         LA    R1,17                                               0176 00221000
*           CALL MSGBLK;            /* ADDR OF LIST FORM OF MSG      */ 00222000
         L     @15,TWAMSG(,TWAPTR)                                 0177 00223000
         BALR  @14,@15                                             0177 00224000
*           R1=ADDR(IKT017I);       /* ADDR OF LIST FORM             */ 00225000
         LA    @00,IKT017I(,R1)                                    0178 00226000
         LR    R1,@00                                              0178 00227000
*           GEN(WTO   MF=(E,(1)))   /* ISSUE WTO                     */ 00228000
*           REFS(R0,R1,R14,R15);                                   0179 00229000
         WTO   MF=(E,(1))                                               00230000
*           RESPECIFY                                              0180 00231000
*            (R1) UNRESTRICTED;                                    0180 00232000
*           PRINTSW='0'B;           /* TURN OFF PRINT SW             */ 00233000
         NI    PRINTSW,B'11110111'                                 0181 00234000
*         END;                                                     0182 00235000
*       ELSE                                                       0183 00236000
*         PRINTSW='1'B;             /* TURN ON PRINT SW              */ 00237000
         B     @RC00173                                            0183 00238000
@RF00173 OI    PRINTSW,B'00001000'                                 0183 00239000
*     END;                                                         0184 00240000
*   ELSE                                                           0185 00241000
*     PRINTSW='0'B;                 /* TURN OFF PRINT SW             */ 00242000
         B     @RC00169                                            0185 00243000
@RF00169 NI    PRINTSW,B'11110111'                                 0185 00244000
*   END CHKOPTNS;                                                  0186 00245000
         B     @EL00002                                            0186 00246000
         EJECT                                                          00247000
*SCANPARM:                                                         0187 00248000
*   PROC OPTIONS(SAVE(14));                                        0187 00249000
SCANPARM ST    @14,@SA00003                                        0187 00250000
*                                                                  0188 00251000
*/*  *****************************************************************/ 00252000
*/*                                                                  */ 00253000
*/*    THIS SUBROUTINE PROCESSES THE PARAMETERS ENTERED BY THE       */ 00254000
*/*    OPERATOR ON THE VARIOUS COMMANDS.  THE FOLLOWING THINGS ARE   */ 00255000
*/*    DONE:                                                         */ 00256000
*/*                                                                  */ 00257000
*/*    -   INITIALIZE THE BUFFER SAVE AREA                           */ 00258000
*/*    -   SET UP POINTER TO PROPER INPUT BUFFER                     */ 00259000
*/*    -   IF BUFFER EMPTY, EXIT                                     */ 00260000
*/*    -   EXCEPTION CODING                                          */ 00261000
*/*    -   PROCESS THE PARMS FOUND IN THE BUFFER.  IF ANY PARMS ARE  */ 00262000
*/*        IN ERROR, SET THE ERROR SWITCH AND EXIT.                  */ 00263000
*/*    -   IF THE OPERATOR WANTS TO USE A DIFFERENT MEMBER NAME      */ 00264000
*/*        WHEN ACCESSING PARMLIB, SET UP THE NEW NAME               */ 00265000
*/*    -   IF CERTAIN OPTIONS ARE SPECIFIED BY THE OPERATOR, SET     */ 00266000
*/*        SWITCHES TO INDICATE WHAT THEY ARE                        */ 00267000
*/*                                                                  */ 00268000
*/*  *****************************************************************/ 00269000
*                                                                  0188 00270000
*   SAVEBUFR=SAVES1;                /* INITIALIZE SAVE AREA          */ 00271000
         MVC   SAVEBUFR(24),SAVES1                                 0188 00272000
*   IF MLPMBUF='1'B THEN            /* DATA BUFFER                   */ 00273000
         TM    MLPMBUF(MLPMPTR),B'00000010'                        0189 00274000
         BNO   @RF00189                                            0189 00275000
*     SCANPTR=MLPMSCA;              /* YES                           */ 00276000
         L     SCANPTR,MLPMSCA(,MLPMPTR)                           0190 00277000
*   ELSE                                                           0191 00278000
*     SCANPTR=ADDR(CIBUFFER);       /* NO                            */ 00279000
         B     @RC00189                                            0191 00280000
@RF00189 LA    SCANPTR,CIBUFFER                                    0191 00281000
*   IF PRINTSW='1'B THEN            /* PRINTOUT WANTED               */ 00282000
@RC00189 TM    PRINTSW,B'00001000'                                 0192 00283000
         BNO   @RF00192                                            0192 00284000
*     DO;                           /* YES                           */ 00285000
*       PRNTCC='1';                 /* SKIP TO TOP OF PAGE           */ 00286000
         MVI   PRNTCC,C'1'                                         0194 00287000
*       PRNTDATA='CONSOLE VALUES ENTERED:';/* HEADER LINE            */ 00288000
         MVI   PRNTDATA+23,C' '                                    0195 00289000
         MVC   PRNTDATA+24(108),PRNTDATA+23                        0195 00290000
         MVC   PRNTDATA(23),@CC01524                               0195 00291000
*       CALL PRINTIT;               /* PRINT THE HEADER              */ 00292000
         BAL   @14,PRINTIT                                         0196 00293000
*       PRNTLINE='';                /* SKIP TO NEXT LINE             */ 00294000
         MVI   PRNTLINE,C' '                                       0197 00295000
         MVC   PRNTLINE+1(132),PRNTLINE                            0197 00296000
*       IF WORKCHAR=' ' THEN        /* DATA                          */ 00297000
         CLI   WORKCHAR(SCANPTR),C' '                              0198 00298000
         BNE   @RF00198                                            0198 00299000
*         PRNTDAT5='*NONE ENTERED*';/* FROM                          */ 00300000
         MVI   PRNTDAT5+14,C' '                                    0199 00301000
         MVC   PRNTDAT5+15(113),PRNTDAT5+14                        0199 00302000
         MVC   PRNTDAT5(14),@CC01527                               0199 00303000
*       ELSE                        /* THE                           */ 00304000
*         PRNTDAT5=DATABUF;         /* BUFFER                        */ 00305000
         B     @RC00198                                            0200 00306000
@RF00198 MVI   PRNTDAT5+48,C' '                                    0200 00307000
         MVC   PRNTDAT5+49(79),PRNTDAT5+48                         0200 00308000
         MVC   PRNTDAT5(48),DATABUF(SCANPTR)                       0200 00309000
*       CALL PRINTIT;               /* PRINT THE BUFFER              */ 00310000
@RC00198 BAL   @14,PRINTIT                                         0201 00311000
*     END;                                                         0202 00312000
*   IF WORKCHAR=' ' THEN            /* FIRST CHARACTER BLANK         */ 00313000
@RF00192 CLI   WORKCHAR(SCANPTR),C' '                              0203 00314000
         BE    @RT00203                                            0203 00315000
*     RETURN;                       /* YES - EXIT                    */ 00316000
*                                                                  0205 00317000
*/*  *****************************************************************/ 00318000
*/*                                                                  */ 00319000
*/*    THIS SECTION OF CODE HANDLES INPUT DATA THAT IS NOT IN        */ 00320000
*/*    KEYWORD NAME, EQUAL SIGN, KEYWORD PARAMETER, SEQUENCE.        */ 00321000
*/*                                                                  */ 00322000
*/*  *****************************************************************/ 00323000
*                                                                  0205 00324000
*   IF DATA2='U '|DATA7='IGNORE ' THEN/* IGNORE RESPONSE             */ 00325000
         CLC   DATA2(2,SCANPTR),@CC01529                           0205 00326000
         BE    @RT00205                                            0205 00327000
         CLC   DATA7(7,SCANPTR),@CC01530                           0205 00328000
         BNE   @RF00205                                            0205 00329000
@RT00205 DS    0H                                                  0206 00330000
*     DO;                           /* YES                           */ 00331000
*       MLPMIGN='1'B;               /* SET SWITCH                    */ 00332000
         OI    MLPMIGN(MLPMPTR),B'00000001'                        0207 00333000
*       RETURN;                     /* EXIT                          */ 00334000
@EL00003 DS    0H                                                  0208 00335000
@EF00003 DS    0H                                                  0208 00336000
@ER00003 L     @14,@SA00003                                        0208 00337000
         BR    @14                                                 0208 00338000
*     END;                                                         0209 00339000
*   IF DATA6='FSTOP ' THEN          /* 'FSTOP' BY ITSELF             */ 00340000
@RF00205 CLC   DATA6(6,SCANPTR),@CC01531                           0210 00341000
         BNE   @RF00210                                            0210 00342000
*     DATA11='USER=FSTOP ';         /* YES - ADD KEYWORD NAME        */ 00343000
         MVC   DATA11(11,SCANPTR),@CC01532                         0211 00344000
*   ELSE                                                           0212 00345000
*     IF DATA4='SIC ' THEN          /* 'SIC' BY ITSELF               */ 00346000
         B     @RC00210                                            0212 00347000
@RF00210 CLC   DATA4(4,SCANPTR),@CC01534                           0212 00348000
         BNE   @RF00212                                            0212 00349000
*       DATA9='USER=SIC ';          /* YES - ADD KEYWORD NAME        */ 00350000
         MVC   DATA9(9,SCANPTR),@CC01535                           0213 00351000
*   DO WHILE(WORKCHAR^=' ');        /* PROCESS PARMS                 */ 00352000
@RF00212 DS    0H                                                  0214 00353000
@RC00210 B     @DE00214                                            0214 00354000
@DL00214 DS    0H                                                  0215 00355000
*     TABLEPTR=ADDR(VERIBUFR);      /* INITIALIZE                    */ 00356000
         LA    TABLEPTR,VERIBUFR                                   0215 00357000
*     SAVEPTR=ADDR(SAVEBUFR);       /* POINTERS                      */ 00358000
         LA    SAVEPTR,SAVEBUFR                                    0216 00359000
*     CALL FNDNMTCH;                /* GET A KEYWORD NAME            */ 00360000
         BAL   @14,FNDNMTCH                                        0217 00361000
*     IF SCANERR='1'B THEN          /* WAS AN ERROR FOUND            */ 00362000
         TM    SCANERR,B'10000000'                                 0218 00363000
         BNO   @RF00218                                            0218 00364000
*       DO;                         /* YES                           */ 00365000
*         MLPMINV='1'B;             /* SHOW IT                       */ 00366000
