         OI    SCANERR,B'10000000'                                 0717 01920000
*   END FNDNMTCH;                                                  0718 01921000
         B     @EL00013                                            0718 01922000
         EJECT                                                          01923000
*PRCSPARM:                                                         0719 01924000
*   PROC OPTIONS(SAVE(14));                                        0719 01925000
PRCSPARM ST    @14,@SA00014                                        0719 01926000
*                                                                  0720 01927000
*/*  *****************************************************************/ 01928000
*/*                                                                  */ 01929000
*/*    THIS SUBROUTINE WILL VERIFY THAT THE PARAMETER FOLLOWING A    */ 01930000
*/*    KEYWORD NAME IS A LEGAL PARAMETER.  UPON ENTRY, 'SCANPTR'     */ 01931000
*/*    SHOULD POINT TO THE FIRST CHARACTER OF THE PARAMETER,         */ 01932000
*/*    'TABLEPTR' SHOULD POINT AT THE TABLE ENTRY WE WILL USE FOR    */ 01933000
*/*    VALIDATION, AND 'SAVEPTR' SHOULD POINT AT TABLE WHICH WILL BE */ 01934000
*/*    USED TO SAVE THE EDITED AND VERIFIED PARAMETER.               */ 01935000
*/*    UPON EXIT, 'SCANPTR' WILL POINT AT THE CHARACTER WHICH STOPPED*/ 01936000
*/*    THE SCANNING OF THE PARAMETER, 'TABLEPTR' WILL BE UNCHANGED,  */ 01937000
*/*    AND 'SAVEPTR' WILL POINT AT THE SAVE AREA WHERE THE EDITED    */ 01938000
*/*    AND VERIFIED PARAMETER IS.  NOTES: - IF THE PARAMETER IS      */ 01939000
*/*    INVALID, THE FIRST BYTE OF THE SAVE AREA WILL BE 'FF'X.       */ 01940000
*/*                                       - IF MORE THAN ONE         */ 01941000
*/*    PARAMETER IS ENTERED FOR A SINGLE KEYWORD, THEN THAT          */ 01942000
*/*    KEYWORD WILL BE FLAGGED AS AN ERROR, WITH THE EXCEPTION OF    */ 01943000
*/*    THE 'RCFBDUMP' KEYWORD.                                       */ 01944000
*/*  *****************************************************************/ 01945000
*                                                                  0720 01946000
*   IF TBLZERO='1'B THEN            /* ZERO-FILL                     */ 01947000
         TM    TBLZERO(TABLEPTR),B'00001000'                       0720 01948000
         BNO   @RF00720                                            0720 01949000
*     WORKPARM=ZERO;                /* YES                           */ 01950000
         MVC   WORKPARM(8),@CC00103                                0721 01951000
*   ELSE                                                           0722 01952000
*     WORKPARM=BLANK;               /* NO - BLANK-FILL               */ 01953000
         B     @RC00720                                            0722 01954000
@RF00720 MVC   WORKPARM(8),@CC00097                                0722 01955000
*   IF TBLNAME=RCFBDUMP THEN        /* 'RCFBDUMP             @ZM20856*/ 01956000
@RC00720 CLC   TBLNAME(8,TABLEPTR),RCFBDUMP                        0723 01957000
         BNE   @RF00723                                            0723 01958000
*     DO;                                                          0724 01959000
*       SAVEPTR=SAVEPTR+8*(TBLDISP-1+DUMPCNT);/* ADDRESS PROPER WORK    01960000
*                                      AREA                  @ZM20856*/ 01961000
         LH    @04,DUMPCNT                                         0725 01962000
         LH    @15,TBLDISP(,TABLEPTR)                              0725 01963000
         BCTR  @15,0                                               0725 01964000
         ALR   @15,@04                                             0725 01965000
         SLA   @15,3                                               0725 01966000
         ALR   SAVEPTR,@15                                         0725 01967000
*       IF DUMPCNT>16 THEN          /* MAX '16' PARMS        @ZM20856   01968000
*                                      NOTE: DUMPCNT INITIALIZED IN     01969000
*                                      PROC 'READMBR' TO 0.  @ZM20856*/ 01970000
