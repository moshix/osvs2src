*           TSO2=WORKPRM(2);                                       0289 00569000
         MVC   TSO2(1),WORKPRM+1                                   0289 00570000
*           SAVAREA=TSOKEYNN;                                      0290 00571000
         MVC   SAVAREA(8,SAVEPTR),TSOKEYNN                         0290 00572000
*         END;                                                     0291 00573000
*     END;                                                         0292 00574000
*   END GETMBRNM;                                                  0293 00575000
@EL00004 DS    0H                                                  0293 00576000
@EF00004 DS    0H                                                  0293 00577000
