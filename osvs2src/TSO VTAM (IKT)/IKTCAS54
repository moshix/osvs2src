*       END;                                                       0228 00381000
*     IF WORKCHAR^=' ' THEN         /* IF NOT THE END                */ 00382000
@RF00224 CLI   WORKCHAR(SCANPTR),C' '                              0229 00383000
         BE    @RF00229                                            0229 00384000
*       SCANPTR=SCANPTR+1;          /* PT PAST THE COMMA             */ 00385000
         AL    SCANPTR,@CF00057                                    0230 00386000
*   END;                                                           0231 00387000
@RF00229 DS    0H                                                  0231 00388000
@DE00214 CLI   WORKCHAR(SCANPTR),C' '                              0231 00389000
         BNE   @DL00214                                            0231 00390000
*   CALL GETMBRNM;                  /* SETUP PARMLIB MBR NAME        */ 00391000
         BAL   @14,GETMBRNM                                        0232 00392000
*   IF PRINTSW='1'B THEN            /* PRINTOUT WANTED               */ 00393000
         TM    PRINTSW,B'00001000'                                 0233 00394000
         BNO   @RF00233                                            0233 00395000
*     DO;                           /* YES - PRNT ACCPTD VALUES      */ 00396000
*       PRNTCC='0';                 /* SKIP AN EXTRA LINE            */ 00397000
         MVI   PRNTCC,C'0'                                         0235 00398000
*       PRNTDATA='CONSOLE VALUES ACCEPTED:';/* SUB-HEADER LINE       */ 00399000
         MVI   PRNTDATA+24,C' '                                    0236 00400000
         MVC   PRNTDATA+25(107),PRNTDATA+24                        0236 00401000
         MVC   PRNTDATA(24),@CC01541                               0236 00402000
*       CALL PRINTIT;               /* PRINT SUB-HEADER              */ 00403000
         BAL   @14,PRINTIT                                         0237 00404000
