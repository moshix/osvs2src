         BNE   @RF00707                                            0707 01889000
@RT00707 DS    0H                                                  0708 01890000
*     DO;                           /* YES                           */ 01891000
*       SCANERR='1'B;               /* SHOW ERROR                    */ 01892000
         OI    SCANERR,B'10000000'                                 0709 01893000
*       RETURN;                     /* EXIT                          */ 01894000
@EL00013 DS    0H                                                  0710 01895000
@EF00013 DS    0H                                                  0710 01896000
@ER00013 L     @14,12(,@13)                                        0710 01897000
         BR    @14                                                 0710 01898000
*     END;                                                         0711 01899000
*   ELSE                            /* NO                            */ 01900000
*     SCANPTR=SCANPTR+1;            /* POINT PAST '=' SIGN SCAN TABLE   01901000
*                                      LOOKING FOR A MATCH, OR THE 0712 01902000
*                                      END OF THE TABLE              */ 01903000
@RF00707 AL    SCANPTR,@CF00057                                    0712 01904000
*   DO WHILE(TBLNAME1^='FF'X&TBLNAME^=WORKPARM);                   0713 01905000
         B     @DE00713                                            0713 01906000
@DL00713 DS    0H                                                  0714 01907000
*     TABLEPTR=TABLEPTR+20;                                        0714 01908000
         AL    TABLEPTR,@CF00144                                   0714 01909000
*   END;                                                           0715 01910000
@DE00713 CLI   TBLNAME1(TABLEPTR),X'FF'                            0715 01911000
