         EJECT                                                          02221000
*SPLITPRM:                                                         0807 02222000
*   PROC OPTIONS(SAVE(14));                                        0807 02223000
SPLITPRM ST    @14,12(,@13)                                        0807 02224000
*                                                                  0808 02225000
*/*  *****************************************************************/ 02226000
*/*                                                                  */ 02227000
*/*    THIS SUBROUTINE SPLITS THE KEYWORD 'RCFBDUMP' INTO 3 LOGICAL  */ 02228000
*/*    VALUES AND TRANSLATES THEM TO BINARY. VALUES:                 */ 02229000
*/*            1. VTAM RETURN CODE                                   */ 02230000
*/*            2. VTAM FEEDBACK CODE                                 */ 02231000
*/*            3. DUMP OPTION - 0=LOCAL, 1=ALL                       */ 02232000
*/*  *****************************************************************/ 02233000
*                                                                  0808 02234000
*   I=0;                            /* ZERO LENGTH COUNT     @ZM20856*/ 02235000
         SLR   I,I                                                 0808 02236000
*   SAVAREAD=SAVAREAD&&SAVAREAD;    /* INITIALIZE AREA       @ZM20856*/ 02237000
         XC    SAVAREAD(8,SAVEPTR),SAVAREAD(SAVEPTR)               0809 02238000
*   DO WHILE(WORKCHAR^=','&WORKCHAR^=' ');/*                 @ZM20856*/ 02239000
         B     @DE00810                                            0810 02240000
@DL00810 DS    0H                                                  0811 02241000
*     I=I+1;                        /* 1 TO LENGTH           @ZM20856*/ 02242000
         AL    I,@CF00057                                          0811 02243000
*     IF I<6 THEN                   /* GOOD LENGTH           @ZM20856*/ 02244000
         C     I,@CF00065                                          0812 02245000
         BNL   @RF00812                                            0812 02246000
*       WORKPARM(I)=WORKCHAR;       /* MOVE NEXT CHAR        @ZM20856*/ 02247000
         LA    @01,WORKPARM-1(I)                                   0813 02248000
         MVC   0(1,@01),WORKCHAR(SCANPTR)                          0813 02249000
*     SCANPTR=SCANPTR+1;            /* PT NEXT CHAR          @ZM20856*/ 02250000
@RF00812 AL    SCANPTR,@CF00057                                    0814 02251000
*   END;                            /* END DO WHILE          @ZM20856*/ 02252000
@DE00810 CLI   WORKCHAR(SCANPTR),C','                              0815 02253000
         BE    @DC00810                                            0815 02254000
         CLI   WORKCHAR(SCANPTR),C' '                              0815 02255000
         BNE   @DL00810                                            0815 02256000
@DC00810 DS    0H                                                  0816 02257000
*   IF I^=5 THEN                    /* INVALID PARM          @ZM20856*/ 02258000
         C     I,@CF00248                                          0816 02259000
         BE    @RF00816                                            0816 02260000
*     DO;                           /* YES                   @ZM20856*/ 02261000
*       SAVECHAR=HEXFF;             /* YES                   @ZM20856*/ 02262000
         MVI   SAVECHAR(SAVEPTR),X'FF'                             0818 02263000
*       RETURN;                     /* NEXT KEYWORD          @ZM20856*/ 02264000
@EL00017 DS    0H                                                  0819 02265000
@EF00017 DS    0H                                                  0819 02266000
@ER00017 L     @14,12(,@13)                                        0819 02267000
         BR    @14                                                 0819 02268000
*     END;                          /*                       @ZM20856*/ 02269000
*   ELSE                                                           0821 02270000
*     ;                             /* VALID                 @ZM20856*/ 02271000
*                                                                  0821 02272000
@RF00816 DS    0H                                                  0822 02273000
*   /*****************************************************************/ 02274000
*   /*                                                               */ 02275000
*   /* PROCESS VTAM RETURN CODE                              @ZM20856*/ 02276000
*   /*                                                               */ 02277000
*   /*****************************************************************/ 02278000
*                                                                  0822 02279000
*   SPLIT(5:6)=WORKPARM(1:2);       /* EXTRACT RC            @ZM21059*/ 02280000
         MVC   SPLIT+4(2),WORKPARM                                 0822 02281000
*   TR(SPLIT(5:6),TABBINRY);        /* TRANS TO BINARY-ALSO TRANS  0823 02282000
