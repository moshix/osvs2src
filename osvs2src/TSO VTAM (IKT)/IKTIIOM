         BNO   @RF00161                                            0161 00366000
*              TSBNOBRK='0'B;           /* BREAKIN ALLOWED   @G58AK3A*/ 00367000
         NI    TSBNOBRK(@08),B'11111101'                           0162 00368000
*            ELSE                       /* FOR OTHERS DEFAULT@G58AK3A*/ 00369000
*              TSBNOBRK='1'B;           /* IS NOBREAK        @G58AK3A*/ 00370000
         B     @RC00161                                            0163 00371000
@RF00161 L     @12,PSAAOLD                                         0163 00372000
         L     @12,ASCBTSB(,@12)                                   0163 00373000
         OI    TSBNOBRK(@12),B'00000010'                           0163 00374000
*            IF TSBXTMTP=1 | TSBXTMTP=4 THEN /*              @G58AK3A*/ 00375000
@RC00161 L     @12,PSAAOLD                                         0164 00376000
         L     @12,ASCBTSB(,@12)                                   0164 00377000
         L     @12,TSBEXTNT(,@12)                                  0164 00378000
         CLI   TSBXTMTP(@12),1                                     0164 00379000
         BE    @RT00164                                            0164 00380000
         CLI   TSBXTMTP(@12),4                                     0164 00381000
         BNE   @RF00164                                            0164 00382000
@RT00164 DS    0H                                                  0165 00383000
*             DO;                       /* PROCESS 3270 OR NDS       */ 00384000
*               TVWAXSCD='1'B;          /* EXAMINE SCR DIMENS@G58AK3A*/ 00385000
         L     @12,TVWAPTR                                         0166 00386000
         OI    TVWAXSCD(@12),B'00001000'                           0166 00387000
*               TVWALNCT=1;             /* INITIALIZE LINE COUNT 3270*/ 00388000
         MVI   TVWALNCT(@12),X'01'                                 0167 00389000
*               TVWAFMSC='1'B;          /* FORMAT 3270 SCREEN        */ 00390000
*               TVWAKBDL='1'B;          /* LOCK KEYBOARD      OZ11996*/ 00391000
         OI    TVWAFMSC(@12),B'10100000'                           0169 00392000
*               /*****************************************************/ 00393000
*               /* WHEN LOSTERM HAS BEEN ENTERED (TVWALTE    @G58AK3B*/ 00394000
*               /* ON) AND AT SOME PREVIOUS SESSION THERE WAS@G58AK3B*/ 00395000
*               /* A CONNECTION WITH A DISPLAY TERMINAL      @G58AK3B*/ 00396000
*               /* (TVWARSHW > 0), THEN THE RESHOW KEY WILL  @G58AK3B*/ 00397000
*               /* BE THAT SPECIFIED BY TVWARSHW.            @G58AK3B*/ 00398000
*               /* WHEN LOSTERM HAS BEEN ENTERED (TVWALTE    @G58AK3B*/ 00399000
*               /* ON) AND AT NO PREVIOUS TIME WAS THERE A   @G58AK3B*/ 00400000
*               /* (TVWARSHW = 0), THEN THE PA2 KEY WILL BE  @G58AK3B*/ 00401000
*               /* IF NOT LOSTERM (TVWALTE OFF), SINCE       @G58AK3B*/ 00402000
*               /* THE RESHOW KEY.                           @G58AK3B*/ 00403000
*               /*****************************************************/ 00404000
*               IF TVWALTE = '0'B |     /* NOT LOSTERM OR    @G58AK3B*/ 00405000
*                  TVWARSHW = '00'X     /* RECNECT FROM 3767?@G58AK3B*/ 00406000
*               THEN                    /* YES               @G58AK3B*/ 00407000
         TM    TVWALTE(@12),B'00000010'                            0170 00408000
         BZ    @RT00170                                            0170 00409000
         CLI   TVWARSHW(@12),X'00'                                 0170 00410000
         BNE   @RF00170                                            0170 00411000
@RT00170 DS    0H                                                  0171 00412000
*                 TVWARSHW=PA2;         /* SET RESHOW CODE    OZ11996*/ 00413000
         L     @12,TVWAPTR                                         0171 00414000
         MVC   TVWARSHW(1,@12),PA2                                 0171 00415000
*               ELSE;                   /* LOSTERM           @G58AK3B*/ 00416000
@RF00170 DS    0H                                                  0173 00417000
*               IF TSBXTMBF=1920 THEN                              0173 00418000
         L     @12,PSAAOLD                                         0173 00419000
         L     @12,ASCBTSB(,@12)                                   0173 00420000
         L     @08,TSBEXTNT(,@12)                                  0173 00421000
         CLC   TSBXTMBF(2,@08),@CH01582                            0173 00422000
         BNE   @RF00173                                            0173 00423000
*                DO;                    /* SET SCREEN SIZE VALUES    */ 00424000
*                 TSBLNSZ='50'X;        /* 80 CHARS PER LINE MOD 1   */ 00425000
         MVI   TSBLNSZ(@12),X'50'                                  0175 00426000
*                 TSBLNNO='18'X;        /* 24  LINES PER  SCREEN     */ 00427000
         MVI   TSBLNNO(@12),X'18'                                  0176 00428000
*                END;                                              0177 00429000
*               ELSE                                               0178 00430000
*                DO;                    /* SET SCREEN  SIZE VALUES   */ 00431000
         B     @RC00173                                            0178 00432000
@RF00173 DS    0H                                                  0179 00433000
*                 TSBLNSZ='28'X;        /* 40 CHARS PER LINE MOD 2   */ 00434000
         L     @12,PSAAOLD                                         0179 00435000
         L     @12,ASCBTSB(,@12)                                   0179 00436000
         MVI   TSBLNSZ(@12),X'28'                                  0179 00437000
*                 TSBLNNO='0C'X;        /* 12 LINES PER SCREEN       */ 00438000
         MVI   TSBLNNO(@12),X'0C'                                  0180 00439000
*                END;                                              0181 00440000
*             END;                                                 0182 00441000
*            ELSE                                                  0183 00442000
*              IF TSBXTMTP='2'X THEN                               0183 00443000
         B     @RC00164                                            0183 00444000
@RF00164 L     @12,PSAAOLD                                         0183 00445000
