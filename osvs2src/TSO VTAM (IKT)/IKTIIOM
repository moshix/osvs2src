         BNE   @RF00183                                            0183 00449000
*                TSBLNSZ='84'X;         /* ROCKET LINE SIZE          */ 00450000
         MVI   TSBLNSZ(@12),X'84'                                  0184 00451000
*              ELSE;                                               0185 00452000
@RF00183 DS    0H                                                  0186 00453000
*            /* SCHEDULE THE TIM SRB, MODESET TO KEY=6               */ 00454000
*            REG2=TSBXSRBI;             /* ADDRESS OF TIM SRB        */ 00455000
@RC00164 L     @12,PSAAOLD                                         0186 00456000
         L     @12,ASCBTSB(,@12)                                   0186 00457000
         L     @12,TSBEXTNT(,@12)                                  0186 00458000
         L     REG2,TSBXSRBI(,@12)                                 0186 00459000
*            GEN REFS(REG2,CVTPTR,CVTLSMQ,CVTMAP,                  0187 00460000
*                     SRBFLNK,SRBSECT)                             0187 00461000
*                SETS(REG0,REG1,REG14,REG15);                      0187 00462000
            SCHEDULE SRB=(REG2),SCOPE=LOCAL                             00463000
*            REG10=0;                   /* INDICATE GOOD RETURN CODE */ 00464000
