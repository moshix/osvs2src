         L     @12,TSBEXTNT(,@12)                                  0069 00082000
         ST    REG1,TSBXSRBI(,@12)                                 0069 00083000
*      COUNT=COUNT-1;                   /* DECREMENT LOOP CONTROL    */ 00084000
@RC00067 BCTR  COUNT,0                                             0070 00085000
*      RFY REG1 UNRSTD;                 /* FREE REGISTER FOR CMPLER  */ 00086000
*    END;                                                          0072 00087000
*  ELSE                                 /* REG15 INDICATES GETMAIN ER*/ 00088000
*    DO;                                                           0073 00089000
         B     @RC00065                                            0073 00090000
@RF00065 DS    0H                                                  0074 00091000
*      /**************************************************************/ 00092000
*      /* FIRST DETERMINE IF FAILURE OCCURRED AFTER GETMAIN FOR TOM  */ 00093000
*      /* SRB.  IF YES, THE TOM SRB WILL BE FREED BEFORE RETURN.     */ 00094000
*      /**************************************************************/ 00095000
*      IF COUNT=1 THEN                  /* IF 1, FREE TOM SRB        */ 00096000
         C     COUNT,@CF00069                                      0074 00097000
         BNE   @RF00074                                            0074 00098000
*        DO;                                                       0075 00099000
*         RFY (REG2,REG4,REG7) RSTD;    /* REG USED IN FREEMAIN      */ 00100000
*         REG2=TSBXSRB;                 /* ADDRESS OF SRB TO FREE    */ 00101000
