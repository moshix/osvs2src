         L     @12,TSBXSRBI(,@12)                                  0144 00288000
         L     @12,SRBPARM(,@12)                                   0144 00289000
         ST    @12,TVWATIMW(,@11)                                  0144 00290000
*            /********************************************************/ 00291000
*            /*  THE FRR WORK AREAS WILL START 2600 BYTES FROM       */ 00292000
*            /*  THE BEGINNING ADDRESS OF THE TIM WORK AREA. THE     */ 00293000
*            /*  REMAINING 600 BYTES WILL BE ALLOCATED TO THE TIM    */ 00294000
*            /*  AND TOM FRR WORK AREAS. EACH WILL BE 200 BYTES.     */ 00295000
*            /********************************************************/ 00296000
*                                                                  0145 00297000
*            TVWAFRWI=REG8+MAXWRKLN;    /* INITIALIZE POINTER      0145 00298000
*                                           TO TIM FRR WORK        0145 00299000
*                                           AREA.            @ZM20856*/ 00300000
         LA    @12,2600                                            0145 00301000
         ALR   @12,REG8                                            0145 00302000
