         TITLE '*** IKTIIOM : INITIALIZATION RTN FOR TIMS AND TOMS *** *00001000
                        '                                               00002000
IKTIIOM  CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKTIIOM   77.318'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @06,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@06                                         0001 00013000
* REG4=TVWATCB;                       /* ADDR OF TCB                 */ 00014000
         L     @15,PSAAOLD                                         0051 00015000
         L     @12,ASCBTSB(,@15)                                   0051 00016000
         L     @12,TSBEXTNT(,@12)                                  0051 00017000
         L     @12,TSBXTVWA(,@12)                                  0051 00018000
         L     REG4,TVWATCB(,@12)                                  0051 00019000
* REG7=PSAAOLD;                       /* ADDR OF ASCB                */ 00020000
         LR    REG7,@15                                            0052 00021000
* GEN REFS(REG4,REG7,CVTMAP,CVTCRMN,CVTPTR)                        0053 00022000
*     SETS(REG0,REG1,REG3,REG14,REG15,REG9,REG13);                 0053 00023000
    GETMAIN RC,LV=1000,SP=229,BRANCH=YES                                00024000
    LR  REG9,@01              GET ADDR OF WORK AREA FOR DATD            00025000
    USING @DATD,REG9                                                    00026000
    ST  REG13,@SA00001+4                                                00027000
    ST  REG9,8(,REG13)                                                  00028000
    LR  REG13,REG9            NEW SAVE AREA ADDR                        00029000
* TVWAPTR=TSBXTVWA;                   /* SAVE PTR TO TVWA    @G58AKKM*/ 00030000
         L     @15,PSAAOLD                                         0054 00031000
         L     @15,ASCBTSB(,@15)                                   0054 00032000
         L     @15,TSBEXTNT(,@15)                                  0054 00033000
         L     @15,TSBXTVWA(,@15)                                  0054 00034000
         ST    @15,TVWAPTR                                         0054 00035000
* RFY TVWA BASED(TVWAPTR);                                 /*@G58AKKM*/ 00036000
*  RFY (REG1,REG4,REG7) UNRSTD;                                    0056 00037000
* /*******************************************************************/ 00038000
* /* THE FOLLOWING DO LOOP WILL GETMAIN 2 SRB'S--ONE FOR THE TERMINAL*/ 00039000
* /* INPUT MANAGER, THE OTHER FOR THE OUTPUT MANAGER.  SHOULD EITHER */ 00040000
* /* GETMAIN FAIL, CONTROL WILL BE RETURNED TO THE CALLING PROGRAM   */ 00041000
* /* WITH AN ERROR INDICATOR IN REG15.                               */ 00042000
* /*******************************************************************/ 00043000
* COUNT=2;                              /* INITIALIZE LOOP CONTROL   */ 00044000
         LA    COUNT,2                                             0057 00045000
* GETERROR='0'B;                        /* INITIALIZE ERROR FLAG     */ 00046000
         NI    GETERROR,B'01111111'                                0058 00047000
* DO UNTIL (COUNT=0 | GETERROR='1'B);   /* LOOP FOR SRB GETMAIN      */ 00048000
@DL00059 DS    0H                                                  0060 00049000
* RFY (REG1,REG15,REG4,REG7,REG10) RSTD; /* RGS USED IN GETMAIN      */ 00050000
* REG4=TVWATCB;                         /* PASS ADDRESS OF TCB       */ 00051000
         L     @12,TVWAPTR                                         0061 00052000
         L     REG4,TVWATCB(,@12)                                  0061 00053000
* REG7=PSAAOLD;                         /* PASS ADDRESS OF ASCB      */ 00054000
         L     REG7,PSAAOLD                                        0062 00055000
* REG10=SRBLEN;                         /* LENGTH TO GETMAIN         */ 00056000
         L     REG10,SRBLEN                                        0063 00057000
*      GEN  REFS(REG7,REG4,CVTMAP,CVTCRMN,CVTPTR,REG10)            0064 00058000
*      SETS(REG0,REG1,REG3,REG14,REG15);                           0064 00059000
  GETMAIN RC,LV=(REG10),SP=239,BRANCH=YES                               00060000
*  /******************************************************************/ 00061000
*  /* CHECK TO ASSURE THAT GETMAIN WAS SUCCESSFUL.  IF YES, SAVE THE */ 00062000
*  /* ADDRESS OF THE SRB IN THE APPROPRIATE TSBX POINTER.            */ 00063000
*  /******************************************************************/ 00064000
*  IF REG15=0 THEN                      /* GOOD RETURN FROM GETMAIN  */ 00065000
         LTR   REG15,REG15                                         0065 00066000
         BNZ   @RF00065                                            0065 00067000
*    DO;                                                           0066 00068000
*      IF COUNT=2 THEN                  /* FIRST GETMAIN             */ 00069000
         C     COUNT,@CF00109                                      0067 00070000
         BNE   @RF00067                                            0067 00071000
*        TSBXSRB=REG1;                  /* SAVE TOM SRB POINTER      */ 00072000
         L     @12,PSAAOLD                                         0068 00073000
         L     @12,ASCBTSB(,@12)                                   0068 00074000
         L     @12,TSBEXTNT(,@12)                                  0068 00075000
