*                SETS(REG0,REG1,REG3,REG14,REG15);                 0108 00190000
            FREEMAIN RC,LV=(REG10),SP=239,A=(REG2),BRANCH=YES           00191000
            FREEMAIN RC,LV=(REG10),SP=239,A=(REG5),BRANCH=YES         **00192000
                                                          /*@ZA16228*/  00193000
*            RFY (REG4,REG5,REG7,REG10)  UNRSTD;                   0109 00194000
*            /* CLEAR TSBX ADDRESSES FOR TIM/TOM SRBS                */ 00195000
