         BR    @14                                                 0193 00481000
LSTMRTN  STM   @14,@12,12(@13)                                     0193 00482000
*/* ALL FIELDS COMMON TO 3270 AND THE 3767/3770 INTERACTIVE TERMINALS*/ 00483000
*/* WILL BE CLEARED FIRST                                            */ 00484000
* TVWATOPQ=0;                          /* CLEAR FLASH BACK QUEUE     */ 00485000
         L     @12,TVWAPTR                                         0194 00486000
         SLR   @08,@08                                             0194 00487000
         ST    @08,TVWATOPQ(,@12)                                  0194 00488000
* TVWAATTN=0;                          /* CLEAR ATTENTION COUNT      */ 00489000
         MVI   TVWAATTN(@12),X'00'                                 0195 00490000
* TVWATQL1=0;                          /* CLEAR LENGTH FLASH BK 1    */ 00491000
         MVI   TVWATQL1(@12),X'00'                                 0196 00492000
* TVWATQL2=0;                          /* CLEAR LENGTH FLASH BK 2    */ 00493000
         MVI   TVWATQL2(@12),X'00'                                 0197 00494000
* TVWAFLG1='0'X;                       /* CLEAR TIM/TOM FLAG 1       */ 00495000
         MVI   TVWAFLG1(@12),X'00'                                 0198 00496000
* TVWAFLG2='0'X;                       /* CLEAR TIM/TOM FLAG 2       */ 00497000
         MVI   TVWAFLG2(@12),X'00'                                 0199 00498000
* TVWAFLG6='0'X;                       /* CLEAR FLAG BYTE 6  @G58AK3A*/ 00499000
