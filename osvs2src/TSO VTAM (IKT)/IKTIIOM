*             SETS(REG0,REG1,REG3,REG14,REG15);                    0081 00114000
         FREEMAIN RC,LV=(REG10),SP=239,A=(REG2),BRANCH=YES              00115000
*         TSBXSRB=0;                    /* CLEAR POINTER TO SRB      */ 00116000
         L     @12,PSAAOLD                                         0082 00117000
         L     @12,ASCBTSB(,@12)                                   0082 00118000
         L     @12,TSBEXTNT(,@12)                                  0082 00119000
         SLR   @11,@11                                             0082 00120000
         ST    @11,TSBXSRB(,@12)                                   0082 00121000
*        END;                           /* END FREE TOM SRB CODE     */ 00122000
*      ELSE;                                                       0084 00123000
@RF00074 DS    0H                                                  0085 00124000
*      GETERROR='1'B;                                              0085 00125000
         OI    GETERROR,B'10000000'                                0085 00126000
*    END;                                                          0086 00127000
* END;                                                             0087 00128000
@RC00065 DS    0H                                                  0087 00129000
@DE00059 LTR   COUNT,COUNT                                         0087 00130000
         BZ    @DC00059                                            0087 00131000
         TM    GETERROR,B'10000000'                                0087 00132000
         BNO   @DL00059                                            0087 00133000
@DC00059 DS    0H                                                  0088 00134000
*    /****************************************************************/ 00135000
*    /* CHECK REASON FOR FALLING THROUGH DO LOOP.  IF ERROR CONDITION*/ 00136000
*    /* THEN RETURN TO THE CALLING PGM WITH RETURN CODE=8 IN REG15   */ 00137000
*    /****************************************************************/ 00138000
*    IF GETERROR='1'B THEN              /* INDICATE ERROR ON GETMAIN */ 00139000
         TM    GETERROR,B'10000000'                                0088 00140000
         BNO   @RF00088                                            0088 00141000
*      DO;                              /* RETURN WITH CC = 8        */ 00142000
*        REG10=8;                                                  0090 00143000
         LA    REG10,8                                             0090 00144000
*        CALL FREESTOR;                 /* FREE WORK AREA AND RETURN */ 00145000
         BAL   @14,FREESTOR                                        0091 00146000
*      END;                             /* RETURN TO LOGON INIT      */ 00147000
*    ELSE                                                          0093 00148000
*      DO;                              /* GETMAIN FOR WORK AREA     */ 00149000
         B     @RC00088                                            0093 00150000
@RF00088 DS    0H                                                  0094 00151000
