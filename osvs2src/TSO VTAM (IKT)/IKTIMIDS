         TITLE '*** IKTIMIDS: INPUT MANAGER FOR THE 3270/SNA  TERMINALS*00001000
               S ***    '                                               00002000
IKTIMIDS CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKTIMIDS  77.343'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @05,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@05                                         0001 00012000
         LR    WRKSPC,REG1         LOAD ADDR OF GETMAINED AREA          00013000
         USING @DATD,WRKSPC        SET UP ADDRESSABILITY                00014000
         ST    @13,TIMGETMN        SET UP ADDRESSABILITY                00015000
*TVWAPTR=TSBXTVWA;                     /* ADDRESSING TO TVWA         */ 00016000
         L     @12,PSAAOLD                                         0134 00017000
         L     @12,ASCBTSB(,@12)                                   0134 00018000
         L     @12,TSBEXTNT(,@12)                                  0134 00019000
         L     @12,TSBXTVWA(,@12)                                  0134 00020000
         ST    @12,TVWAPTR                                         0134 00021000
*SRBRTN=REG14;                         /* SAVE RETURN ADDRESS        */ 00022000
         L     @12,TIMGETMN                                        0135 00023000
         ST    REG14,SRBRTN(,@12)                                  0135 00024000
*STM(REG0,REG15,SAVEREGS);             /* FREEMAIN RECOVERY POINT    */ 00025000
         STM   REG0,REG15,SAVEREGS(@12)                            0136 00026000
*RFY (REG1,REG14) UNRSTD;              /* FREE REGISTERS 1 AND 14    */ 00027000
*/********************************************************************/ 00028000
*/* THE FOLLOWING CODE WILL BE EXECUTED UPON INITIAL ENTRY TO        */ 00029000
*/* IKTIMIDS.  THE SRB WILL BE FREED AND THE RPL FOR THE VTAM        */ 00030000
*/* RECEIVE MACRO WILL BE INITIALIZED.                               */ 00031000
*/********************************************************************/ 00032000
* DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/        0138 00033000
*CKMC0002:                                                         0139 00034000
*   STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */    0139 00035000
CKMC0002 L     @14,TVWAPTR                                         0139 00036000
         L     @14,TVWAFRWI(,@14)                                  0139 00037000
         STM   REG0,REG15,FRWACKRG(@14)                            0139 00038000
*   FRWACKPT(2)=ADDR(CKMC0002); /* SAVE ADDRESS    @ZM20856 */     0140 00039000
         LA    @12,CKMC0002                                        0140 00040000
         ST    @12,FRWACKPT+4(,@14)                                0140 00041000
* END;                  /* CHECKPOINT         @ZM20856*/           0141 00042000
*REG7=ADDR(IKTIOFRR);                  /* FRR ADDRESS        @G58AK3B*/ 00043000
         L     @14,CVTPTR                                          0142 00044000
         L     @01,CVTTCASP(,@14)                                  0142 00045000
         L     REG7,TCASFRR(,@01)                                  0142 00046000
*GEN   REFS(FRRSCURR,FRRS,FRRSLAST,PSALSFCC,PSA,FRRSELEN,PSACSTK,  0143 00047000
*          FRRSFRRA,FRRSENTR,FRRSPARM,REG7)                        0143 00048000
*          SETS(REG1,REG2,REG3,FRRSCURR);                          0143 00049000
   SETFRR A,FRRAD=(REG7),WRKREGS=(REG1,REG2),PARMAD=(REG3)              00050000
*PARMPTR = REG3;                       /* ADDRESS OF PARM AREA       */ 00051000
         ST    REG3,PARMPTR                                        0144 00052000
*                                      /* DELETE OLD FRR CODE@ZM20856*/ 00053000
*RFY (REG3,REG7) UNRSTD;               /* FREE REGS FOR COMPILER     */ 00054000
*CALL INITFRR;                         /* CALL FRR INITIALIZE      0146 00055000
*                                         ROUTINE            @ZM20856*/ 00056000
         BAL   @14,INITFRR                                         0146 00057000
*/********************************************************************/ 00058000
*/* OBTAIN THE LOCAL LOCK AND FREE THE ADDRESS OF THE TIM SRB        */ 00059000
*/* IN THE TSB EXTENSION.                                            */ 00060000
*/********************************************************************/ 00061000
*REG2=REG0;                            /* SAVE ADDRESS OF SRB TO FREE*/ 00062000
         LR    REG2,REG0                                           0147 00063000
*GEN(SETLOCK OBTAIN,TYPE=LOCAL,RELATED=(SRB),MODE=UNCOND,REGS=USE) 0148 00064000
*REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);                    0148 00065000
         SETLOCK OBTAIN,TYPE=LOCAL,RELATED=(SRB),MODE=UNCOND,REGS=USE   00066000
*TSBXSRBI=0;                           /* CLEAR ADDRESS OF SRB       */ 00067000
         L     @14,PSAAOLD                                         0149 00068000
         L     @01,ASCBTSB(,@14)                                   0149 00069000
         L     @01,TSBEXTNT(,@01)                                  0149 00070000
         SLR   @14,@14                                             0149 00071000
         ST    @14,TSBXSRBI(,@01)                                  0149 00072000
*GEN(SETLOCK RELEASE,TYPE=LOCAL,RELATED=(SRB),REGS=USE)            0150 00073000
*REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);                    0150 00074000
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(SRB),REGS=USE              00075000
*/********************************************************************/ 00076000
*/* FREE SRB                                                         */ 00077000
*/********************************************************************/ 00078000
*RFY (REG7,REG8,REG4) RSTD;            /* USED TO HOLD WORK ADDRESS  */ 00079000
*REG8=REG4;                            /* REG4 DESTROYED BY FREEMAIN */ 00080000
         LR    REG8,REG4                                           0152 00081000
*REG7=SRBLEN;                          /* LENGTH OF SRB              */ 00082000
         L     REG7,SRBLEN                                         0153 00083000
*GEN REFS(REG2,REG7,CVTSPSA,CVTMAP,WSAGGMFM,WSAG,CVTGLMN,PSALITA,FLC)   00084000
*    SETS(REG0,REG1,REG14,REG15,REG4,REG3);                        0154 00085000
A1 SETLOCK  OBTAIN,TYPE=SALLOC,MODE=UNCOND,RELATED=(A2),REGS=USE        00086000
   FREEMAIN RC,LV=(REG7),A=(REG2),SP=239,BRANCH=(YES,GLOBAL)            00087000
A2 SETLOCK  RELEASE,TYPE=SALLOC,RELATED=(A1),REGS=USE                   00088000
*REG4=REG8;                            /* RESTORE WORK AREA ADDR     */ 00089000
         LR    REG4,REG8                                           0155 00090000
*STM(REG0,REG15,SAVEREGS);             /* SAVE REGS FOR RESTART      */ 00091000
         L     @14,TIMGETMN                                        0156 00092000
         STM   REG0,REG15,SAVEREGS(@14)                            0156 00093000
*RFY (REG0,REG1,REG2,REG4,REG8,REG7) UNRSTD;/* RELEASE REGISTERS     */ 00094000
*RESTARTA:                             /* CALL RTN TO SET UP RPL     */ 00095000
*GEN(SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RPL),REGS=USE) 0158 00096000
*    REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);        /*@ZM20546*/ 00097000
RESTARTA SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RPL),REGS=USE   00098000
*CALL PRIMPROC;                        /* CALL RTN TO SET UP RPL     */ 00099000
         BAL   @14,PRIMPROC                                        0159 00100000
*GEN(SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RPL),REGS=USE)            0160 00101000
*    REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);        /*@ZM20546*/ 00102000
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RPL),REGS=USE              00103000
         EJECT                                                          00104000
*/********************************************************************/ 00105000
*/* THE FOLLOWING CODE CONTAINS THE INFINITE LOOP THAT PERFORMS      */ 00106000
*/* THE BASIC FUNCTION OF IKTIMIDS. THAT FUNCTION IS TO ISSUE        */ 00107000
*/* THE VTAM RECEIVE MACRO, DETERMINE WHAT THE INPUT IS WHEN         */ 00108000
*/* THE RECEIVE IS SATISFIED, CALL THE APPROPRIATE ROUTINE           */ 00109000
*/* TO PROCESS THE INPUT AND LOOP BACK TO REISSUE THE RECEIVE.       */ 00110000
*/* EACH TIME VTAM GAINS CONTROL THROUGH THE RECEIVE MACRO           */ 00111000
*/* THE FRR THAT IKTTIMRT SETS UP IS DELETED AND MUST BE RESET.      */ 00112000
*/********************************************************************/ 00113000
*REC: RFY (REG0,REG1,REG2,REG3,REG7) RSTD;      /*RESTRICT REGISTERS */ 00114000
REC      DS    0H                                                  0162 00115000
*                                               /*           @ZM20858*/ 00116000
* DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/        0162 00117000
*CKMC0003:                                                         0163 00118000
*   STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */    0163 00119000
CKMC0003 L     @14,TVWAPTR                                         0163 00120000
         L     @14,TVWAFRWI(,@14)                                  0163 00121000
         STM   REG0,REG15,FRWACKRG(@14)                            0163 00122000
*   FRWACKPT(2)=ADDR(CKMC0003); /* SAVE ADDRESS    @ZM20856 */     0164 00123000
         LA    @12,CKMC0003                                        0164 00124000
         ST    @12,FRWACKPT+4(,@14)                                0164 00125000
* END;                  /* CHECK POINT        @ZM20856*/           0165 00126000
*RPLRRESP='1'B;                        /*                    @ZA24528*/ 00127000
*RPLDFASY='1'B;                        /*                    @ZA24528*/ 00128000
*RPLNFSYN='0'B;                        /*                    @ZA24528*/ 00129000
         L     @14,TIMGETMN                                        0168 00130000
         LA    @12,2348                                            0168 00131000
         LR    @06,@14                                             0168 00132000
         ALR   @06,@12                                             0168 00133000
         OI    RPLRRESP+16(@06),B'00001010'                        0168 00134000
         NI    RPLNFSYN+16(@06),B'11111011'                        0168 00135000
*REG2=ADDR(IFGRPL);                    /* ADDR OF RPL FOR RECEIVE    */ 00136000
         ALR   @14,@12                                             0169 00137000
         LR    REG2,@14                                            0169 00138000
*REG7=ADDR(IKTIOFRR);                  /* PTR TO FRR ENTRY   @G58AK3B*/ 00139000
         L     @14,CVTPTR                                          0170 00140000
         L     @14,CVTTCASP(,@14)                                  0170 00141000
         L     REG7,TCASFRR(,@14)                                  0170 00142000
*GEN REFS(REG1,REG2,REG3,REG7,FRRSCURR,FRRS,FRRSLAST,PSALSFCC,PSA, 0171 00143000
*         FRRSELEN,PSACSTK,FRRSFRRA,FRRSENTR,FRRSPARM,PSALITA,FLC) 0171 00144000
*    SETS(REG1,REG2,REG3,REG14,REG15,REG0); /*RTN CDS IN 15,0 ZM20858*/ 00145000
  RECEIVE  RPL=(REG2)                                                   00146000
  SETFRR A,FRRAD=(REG7),WRKREGS=(REG1,REG2),PARMAD=(REG3)               00147000
*PARMPTR = REG3;                       /* POINTER TO FRR PARAMETERS  */ 00148000
         ST    REG3,PARMPTR                                        0172 00149000
*RFY (REG1,REG2,REG3,REG7) UNRSTD;     /* FREE REGISTERS     @ZM20856*/ 00150000
*CALL INITFRR;                         /* CALL FRR INITIALIZE      0174 00151000
*                                         ROUTINE            @ZM20856*/ 00152000
         BAL   @14,INITFRR                                         0174 00153000
*/********************************************************************/ 00154000
*/* EXAMINE INPUT FROM RECEIVE                                       */ 00155000
*/********************************************************************/ 00156000
*GEN(STM REG14,REG1,12(REG13))                                     0175 00157000
*    REFS(REG13);                      /* SAVE RECEIVE RETURN CODE 0175 00158000
*                                         REGISTERS 15,0     @ZM20858*/ 00159000
         STM REG14,REG1,12(REG13)                                       00160000
*GEN(SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RECV),REGS=USE)     00161000
*    REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);        /*@ZM20546*/ 00162000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RECV),REGS=USE  00163000
*GEN(LM REG14,REG1,12(REG13))                                      0177 00164000
*    REFS(REG13);                      /* RESTORE RETURN CODE REGS 0177 00165000
*                                         15,0               @ZM20858*/ 00166000
         LM REG14,REG1,12(REG13)                                        00167000
*IF TVWATOD='1'B THEN                  /* IS IKTOMIDS DEAD?  @ZM20856*/ 00168000
         L     @14,TVWAPTR                                         0178 00169000
         TM    TVWATOD(@14),B'10000000'                            0178 00170000
         BNO   @RF00178                                            0178 00171000
*  DO;                                 /*                    @ZM20856*/ 00172000
*      RFY REG1 RSTD;                  /* RESTRICT REG 1     @ZM20856*/ 00173000
*      REG1 = ADDR(XITPARMS);          /* PTR TO PARAMETERS  @ZM20856*/ 00174000
         L     @14,TIMGETMN                                        0181 00175000
         LA    REG1,XITPARMS(,@14)                                 0181 00176000
*      WRKAREAL = TIMWAL;              /* WORK AREA LENGTH   @ZM20856*/ 00177000
         MVC   WRKAREAL(4,@14),@CF02479                            0182 00178000
*      LIOWA = TOTALL;                 /* I-O WORK LENGTH    @ZM20856*/ 00179000
         MVC   LIOWA(4,@14),@CF02481                               0183 00180000
*      CALL IKTEXIT;                   /* EXIT PERMANENTLY   @ZM20856*/ 00181000
         L     @12,CVTPTR                                          0184 00182000
         L     @12,CVTTCASP(,@12)                                  0184 00183000
         L     @15,TCASEXIT(,@12)                                  0184 00184000
         BALR  @14,@15                                             0184 00185000
*      RFY REG1 UNRSTD;                /* FREE REGISTER 1    @G58AK3B*/ 00186000
*  END;                                /*                    @ZM20856*/ 00187000
*ELSE                                  /* IKTOMIDS IS ALIVE  @ZM20856*/ 00188000
*IF REG15=0 THEN                       /* CHECK FOR DATA RECEIVED    */ 00189000
         B     @RC00178                                            0187 00190000
@RF00178 LTR   REG15,REG15                                         0187 00191000
         BNZ   @RF00187                                            0187 00192000
*  IF RPLDATA='1'B THEN                /* CHECK CONTROL IS DATA      */ 00193000
         LA    @14,2348                                            0188 00194000
         AL    @14,TIMGETMN                                        0188 00195000
         TM    RPLDATA+16(@14),B'10000000'                         0188 00196000
         BNO   @RF00188                                            0188 00197000
*     CALL DATARTN;                    /* YES, CALL DATA ROUTINE     */ 00198000
         BAL   @14,DATARTN                                         0189 00199000
*  ELSE                                /* CONTROL IS NOT DATA        */ 00200000
*      IF RECERCNT >=3 THEN            /* CHECK NUMBER OF TIMES      */ 00201000
         B     @RC00188                                            0190 00202000
@RF00188 L     @14,TIMGETMN                                        0190 00203000
         L     @14,RECERCNT(,@14)                                  0190 00204000
         C     @14,@CF00073                                        0190 00205000
         BL    @RF00190                                            0190 00206000
*        CALL EXITFRR;                 /* CALL ABEND ROUTINE @ZM20856*/ 00207000
         BAL   @14,EXITFRR                                         0191 00208000
*      ELSE                            /* LESS THAN 3, UPDATE COUNT  */ 00209000
*        RECERCNT=RECERCNT+1;          /* BY ONE                     */ 00210000
         B     @RC00190                                            0192 00211000
@RF00190 L     @14,TIMGETMN                                        0192 00212000
         LA    @12,1                                               0192 00213000
         AL    @12,RECERCNT(,@14)                                  0192 00214000
         ST    @12,RECERCNT(,@14)                                  0192 00215000
*ELSE                                  /* ERROR                      */ 00216000
*  DO;                                 /* CHECK ERROR CODE           */ 00217000
         B     @RC00187                                            0193 00218000
@RF00187 DS    0H                                                  0194 00219000
*    CALL VTAMERR;                     /* CHECK VTAM ERROR CODE      */ 00220000
         BAL   @14,VTAMERR                                         0194 00221000
*    IF REGRTN='1'B THEN               /* REFRESH RPL NEEDED?        */ 00222000
         L     @14,TIMGETMN                                        0195 00223000
         TM    REGRTN(@14),B'00001000'                             0195 00224000
         BNO   @RF00195                                            0195 00225000
*       DO;                            /* YES, GO TO REFRESH RTN     */ 00226000
*         LM(REG0,REG15,SAVEREGS);     /* RESTORE REGISTERS          */ 00227000
         L     @14,TIMGETMN                                        0197 00228000
         LM    REG0,REG15,SAVEREGS(@14)                            0197 00229000
*         CALL PRIMPROC;               /* CALL INITIALIZATION ROUTINE*/ 00230000
         BAL   @14,PRIMPROC                                        0198 00231000
*         REGRTN='0'B;                 /* RESET REFRESH INDICATOR    */ 00232000
         L     @14,TIMGETMN                                        0199 00233000
         NI    REGRTN(@14),B'11110111'                             0199 00234000
*       END;                           /* RPL REFRESHED              */ 00235000
*    ELSE;                             /* RPL NEED NOT BE REFRESHED  */ 00236000
@RF00195 DS    0H                                                  0202 00237000
*  END;                                /* END OF ERROR CHECK         */ 00238000
*GEN(SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RECV),REGS=USE)           0203 00239000
*    REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);        /*@ZM20546*/ 00240000
@RC00187 DS    0H                                                  0203 00241000
@RC00178 DS    0H                                                  0203 00242000
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RECV),REGS=USE             00243000
*RFY REG0 UNRSTD;                      /* FREE UP REG 0      @ZM20858*/ 00244000
*GO TO REC;                            /* LOOP BACK TO ISSUE RECEIVE */ 00245000
         B     REC                                                 0205 00246000
         EJECT                                                          00247000
*/********************************************************************/ 00248000
*/*     THE FOLLOWING CODE BUILDS AND INITIALIZES THE RPL FOR THE    */ 00249000
*/*     VTAM RECEIVE MACRO AND WILL BE THE PRIMARY RESTARTING POINT  */ 00250000
*/*     FOR IKTIMIDS IN CASE OF FAILURE DUE TO AN ERROR CONDITON.    */ 00251000
*/********************************************************************/ 00252000
*PRIMPROC:                                                         0206 00253000
*  PROCEDURE OPTIONS(SAVE(14));                                    0206 00254000
PRIMPROC ST    @14,@SA00002                                        0206 00255000
*  DCL DUMMYRPL CHAR(LENGTH(IFGRPL)) STATIC LOCAL GENERATED;       0207 00256000
*  GEN REFS(TIMBUFF) DATA DEFS(DUMMYRPL);                  /*@ZA13627*/ 00257000
*  RFY ISTNIB BASED(TVWANIB);          /* BASE THE NIB DSECT         */ 00258000
*  IFGRPL=DUMMYRPL;                    /* INITIALIZE RPL             */ 00259000
         L     @14,TIMGETMN                                        0210 00260000
         LA    @12,2348                                            0210 00261000
         LR    @04,@14                                             0210 00262000
         ALR   @04,@12                                             0210 00263000
         MVC   IFGRPL(112,@04),DUMMYRPL                            0210 00264000
*  RPLDACB=TVWAACB;                    /* PLACE ACB ADDR IN RPL      */ 00265000
         LR    @04,@14                                             0211 00266000
         ALR   @04,@12                                             0211 00267000
         L     @11,TVWAPTR                                         0211 00268000
         L     @10,TVWAACB(,@11)                                   0211 00269000
         ST    @10,RPLDACB(,@04)                                   0211 00270000
*  RPLARG=NIBCID;                      /* PLACE CID IN RPL           */ 00271000
         LR    @04,@14                                             0212 00272000
         ALR   @04,@12                                             0212 00273000
         L     @01,TVWANIB(,@11)                                   0212 00274000
         L     @10,NIBCID(,@01)                                    0212 00275000
         ST    @10,RPLARG(,@04)                                    0212 00276000
*  RPLAREA=ADDR(TIMBUFF);              /* INITIALIZE ADDRESS OF INPUT*/ 00277000
         ALR   @12,@14                                             0213 00278000
         LA    @04,TIMBUFF(,@14)                                   0213 00279000
         ST    @04,RPLAREA(,@12)                                   0213 00280000
*  IPTRMTYP=TSBXTMTP;                  /* INITIALIZE TERMINAL TYPE   */ 00281000
         L     @12,PSAAOLD                                         0214 00282000
         L     @12,ASCBTSB(,@12)                                   0214 00283000
         L     @12,TSBEXTNT(,@12)                                  0214 00284000
         IC    @04,TSBXTMTP(,@12)                                  0214 00285000
         STC   @04,IPTRMTYP(,@14)                                  0214 00286000
*  TVWATOPQ=ADDR(FLASHBK1);            /* INITIALIZE ADDR OF FLASHBK */ 00287000
         LA    @04,FLASHBK1(,@14)                                  0215 00288000
         ST    @04,TVWATOPQ(,@11)                                  0215 00289000
*  IF TSBXTMBF=1920 THEN                                           0216 00290000
         CLC   TSBXTMBF(2,@12),@CH02539                            0216 00291000
         BNE   @RF00216                                            0216 00292000
*    PHYSLNNO = 24;                    /* NUMBER OF LINES    @ZA13627*/ 00293000
         MVC   PHYSLNNO(4,@14),@CF00090                            0217 00294000
*  ELSE                                                            0218 00295000
*    PHYSLNNO = 12;                    /* NUMBER OF LINES    @ZA13627*/ 00296000
         B     @RC00216                                            0218 00297000
@RF00216 L     @14,TIMGETMN                                        0218 00298000
         MVC   PHYSLNNO(4,@14),@CF02046                            0218 00299000
*RETURN;                                                           0219 00300000
@EL00002 DS    0H                                                  0219 00301000
@EF00002 DS    0H                                                  0219 00302000
@ER00002 L     @14,@SA00002                                        0219 00303000
         BR    @14                                                 0219 00304000
*END PRIMPROC;                         /* END OF PRIMPROC            */ 00305000
         B     @EL00002                                            0220 00306000
         EJECT                                                          00307000
*/********************************************************************/ 00308000
*/*    THIS ROUTINE WILL CHECK TO SEE IF IKTOMIDS IS STILL           */ 00309000
*/*    AVAILABLE TO BE SCHEDULED.  THE TVWATIS FLAG IS 1 WHEN        */ 00310000
*/*    IKTOMIDS IS EITHER RUNNING OR SCHEDULED.  TVWATAS MEANS       */ 00311000
*/*    THAT ONLY IKTIMIDS CAN SCHEDULE IKTOMIDS.                     */ 00312000
*/********************************************************************/ 00313000
*SCHTOM: PROCEDURE OPTIONS(SAVE(14));                              0221 00314000
*                                                                  0221 00315000
SCHTOM   ST    @14,@SA00003                                        0221 00316000
*  TVWATAS = '0'B;                     /* TOM AVAILABLE      @ZM20546*/ 00317000
         L     @14,TVWAPTR                                         0222 00318000
         NI    TVWATAS(@14),B'11011111'                            0222 00319000
*  IF TVWATIS='0'B THEN                /* IKTOMIDS NOT SCHEDULED     */ 00320000
         TM    TVWATIS(@14),B'01000000'                            0223 00321000
         BNZ   @RF00223                                            0223 00322000
*    DO;                               /* SCHEDULE IKTOMIDS          */ 00323000
*      RFY REG2 RSTD;                  /* RESTRICT REGISTER 2        */ 00324000
*      REG2=TSBXSRB;                   /* ADDRESS OF SRB TO FREE     */ 00325000
         L     @14,PSAAOLD                                         0226 00326000
         L     @14,ASCBTSB(,@14)                                   0226 00327000
         L     @14,TSBEXTNT(,@14)                                  0226 00328000
         L     REG2,TSBXSRB(,@14)                                  0226 00329000
*      GEN(SCHEDULE SRB=(REG2),SCOPE=LOCAL)                        0227 00330000
*              REFS(CVTPTR,CVTLSMQ,CVTMAP,SRBFLNK,SRBSECT);        0227 00331000
         SCHEDULE SRB=(REG2),SCOPE=LOCAL                                00332000
*      TVWATIS='1'B;                   /* SET TOM SCHEDULED ON       */ 00333000
         L     @14,TVWAPTR                                         0228 00334000
         OI    TVWATIS(@14),B'01000000'                            0228 00335000
*      RFY REG2 UNRSTD;                /* FREE REGISTER 2            */ 00336000
*    END;                              /* IKTOMIDS IS SCHEDULED      */ 00337000
*  ELSE;                               /* IKTOMIDS HAS BEEN SCHEDULED*/ 00338000
@RF00223 DS    0H                                                  0232 00339000
*  TIMSCHTM = '0'B;                    /* RESET SCHEDULE-TOM FLAG    */ 00340000
         L     @14,TIMGETMN                                        0232 00341000
         NI    TIMSCHTM(@14),B'10111111'                           0232 00342000
*  RETURN;                             /* RETURN TO CALLER           */ 00343000
@EL00003 DS    0H                                                  0233 00344000
@EF00003 DS    0H                                                  0233 00345000
@ER00003 L     @14,@SA00003                                        0233 00346000
         BR    @14                                                 0233 00347000
*  END SCHTOM;                         /* END OF SCHTOM              */ 00348000
         EJECT                                                          00349000
*/********************************************************************/ 00350000
*/*    THIS ROUTINE WILL RECEIVE CONTROL WHEN THE RECEIVE MACRO      */ 00351000
*/*    IS SATISFIED WITH DATA.                                       */ 00352000
*/********************************************************************/ 00353000
*DATARTN:                                                          0235 00354000
