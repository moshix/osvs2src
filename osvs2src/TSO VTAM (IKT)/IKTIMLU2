         L     @01,ASCBTSB(,@12)                                   0167 00076000
         L     @01,TSBEXTNT(,@01)                                  0167 00077000
         SLR   @12,@12                                             0167 00078000
         ST    @12,TSBXSRBI(,@01)                                  0167 00079000
* GEN(SETLOCK RELEASE,TYPE=LOCAL,RELATED=(SRB),REGS=USE)           0168 00080000
* REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);                   0168 00081000
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(SRB),REGS=USE              00082000
* /*******************************************************************/ 00083000
* /* FREE SRB                                                        */ 00084000
* /*******************************************************************/ 00085000
* RFY (REG7,REG8) RSTD;                /* USED TO HOLD WORK ADDRESS  */ 00086000
* REG8=REG4;                           /* REG4 DESTROYED BY FREEMAIN */ 00087000
         LR    REG8,REG4                                           0170 00088000
* REG7=SRBLEN;                         /* SRB LENGTH                 */ 00089000
         L     REG7,SRBLEN                                         0171 00090000
* GEN    REFS(REG2,REG7,CVTSPSA,CVTMAP,WSAGGMFM,WSAG,CVTGLMN,      0172 00091000
*           PSALITA,FLC) SETS(REG0,REG1,REG14,REG15,REG4,REG3);    0172 00092000
A1 SETLOCK  OBTAIN,TYPE=SALLOC,MODE=UNCOND,RELATED=(A2),REGS=USE        00093000
   FREEMAIN RC,LV=(REG7),A=(REG2),SP=239,BRANCH=(YES,GLOBAL)            00094000
A2 SETLOCK  RELEASE,TYPE=SALLOC,RELATED=(A1),REGS=USE                   00095000
* REG4=REG8;                           /* RESTORE WORK AREA ADDR     */ 00096000
         LR    REG4,REG8                                           0173 00097000
* STM(REG0,REG15,SAVEREGS);            /* SAVE REGS FOR RESTART      */ 00098000
         L     @12,TIMGETMN                                        0174 00099000
         STM   REG0,REG15,SAVEREGS(@12)                            0174 00100000
* RESTARTA:                                                        0175 00101000
* RFY (REG0,REG2,REG8,REG7) UNRSTD;    /* FREE UP REGS       @ZM20858*/ 00102000
RESTARTA DS    0H                                                  0176 00103000
* GEN(SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RPL),REGS=USE)     00104000
*     REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);               0176 00105000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RPL),REGS=USE   00106000
* CALL PRIMPROC;                       /* CALL RTN TO SET UP RPL     */ 00107000
         BAL   @14,PRIMPROC                                        0177 00108000
* GEN(SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RPL),REGS=USE)           0178 00109000
*     REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);               0178 00110000
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RPL),REGS=USE              00111000
         EJECT                                                          00112000
*/********************************************************************/ 00113000
*/*     THE FOLLOWING CODE CONTAINS THE INFINITE LOOP THAT PERFORMS  */ 00114000
*/*     THE BASIC FUNCTION OF IKTIMLU2. THAT FUNCTION IS TO ISSUE    */ 00115000
*/*     THE VTAM RECEIVE MACRO, DETERMINE WHAT THE INPUT IS WHEN     */ 00116000
*/*     THE RECEIVE IS SATISFIED, CALL THE APPROPRIATE ROUTINE       */ 00117000
*/*     TO PROCESS THE INPUT AND LOOP BACK TO REISSUE THE RECEIVE.   */ 00118000
*/*     EACH TIME VTAM GAINS CONTROL THROUGH THE RECEIVE MACRO       */ 00119000
*/*     THE FRR THAT IKTIMLU2 SETS UP IS DELETED AND MUST BE RESET.  */ 00120000
*/********************************************************************/ 00121000
*DO UNTIL (1 ^= 1);                                                0179 00122000
@DL00179 DS    0H                                                  0180 00123000
*   DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/      0180 00124000
*CKMC0003:                                                         0181 00125000
*     STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */  0181 00126000
CKMC0003 L     @12,TVWAPTR                                         0181 00127000
         L     @07,TVWAFRWI(,@12)                                  0181 00128000
         STM   REG0,REG15,FRWACKRG(@07)                            0181 00129000
*     FRWACKPT(2)=ADDR(CKMC0003); /* SAVE ADDRESS    @ZM20856 */   0182 00130000
         LA    @15,CKMC0003                                        0182 00131000
         ST    @15,FRWACKPT+4(,@07)                                0182 00132000
*   END;                /* CHECK POINT        @ZM20856*/           0183 00133000
*  RPLCNTRL = '000000'X;               /* RESET RPL CNTRL FIELD      */ 00134000
