*  IF TVWATIS='0'B THEN                /* IKTOMLU2 NOT SCHEDULED     */ 00747000
         TM    TVWATIS(@12),B'01000000'                            0366 00748000
         BNZ   @RF00366                                            0366 00749000
*    DO;                               /* SCHEDULE IKTOMLU2          */ 00750000
*      RFY REG2 RSTD;                  /* RESTRICT REGISTER 2        */ 00751000
*      REG2=TSBXSRB;                   /* ADDRESS OF SRB TO FREE     */ 00752000
         L     @12,PSAAOLD                                         0369 00753000
         L     @12,ASCBTSB(,@12)                                   0369 00754000
