         TITLE '*** IKTIMLU2: INPUT MANAGER FOR 3270 DSE LU2 TERMINALS *00001000
                ***     '                                               00002000
IKTIMLU2 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKTIMLU2  78.016'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @05,0                                               0001 00010000
@PSTART  LA    @06,4095(,@05)                                      0001 00011000
         USING @PSTART,@05                                         0001 00012000
         USING @PSTART+4095,@06                                    0001 00013000
         LR    WRKSPC,REG1         LOAD ADDR OF GETMAINED AREA          00014000
         USING @DATD,WRKSPC        SET UP ADDRESSABILITY                00015000
         ST    @13,TIMGETMN        SET UP ADDRESSABILITY                00016000
*   RPLPTR=TIMGETMN+LENGTH(TIMWA)+CMPSZ;                           0149 00017000
         L     @15,TIMGETMN                                        0149 00018000
         LA    @12,2348                                            0149 00019000
         ALR   @12,@15                                             0149 00020000
         ST    @12,RPLPTR                                          0149 00021000
*   RFY IFGRPL BASED(RPLPTR);                                      0150 00022000
*   RFY WSAG   BASED(CVTSPSA);         /* USED IN GLOBAL FREEMAIN    */ 00023000
*   TVWAPTR=TSBXTVWA;                  /* ADDRESSING TO TVWA         */ 00024000
         L     @12,PSAAOLD                                         0152 00025000
         L     @12,ASCBTSB(,@12)                                   0152 00026000
         L     @12,TSBEXTNT(,@12)                                  0152 00027000
         L     @12,TSBXTVWA(,@12)                                  0152 00028000
         ST    @12,TVWAPTR                                         0152 00029000
*   SRBRTN=REG14;                      /* SAVE RETURN POINT ADDRESS  */ 00030000
         ST    REG14,SRBRTN(,@15)                                  0153 00031000
*   STM(REG0,REG15,SAVEREGS);          /* FREEMAIN RECOVERY POINT    */ 00032000
         STM   REG0,REG15,SAVEREGS(@15)                            0154 00033000
*   RFY (REG1,REG14) UNRSTD;           /* FREE REGISTER 1 AND 14     */ 00034000
*/********************************************************************/ 00035000
*/*     THE FOLLOWING CODE WILL BE EXECUTED UPON INITIAL ENTRY TO    */ 00036000
*/*     IKTIMLU2.  THE SRB WILL BE FREED AND THE RPL FOR THE VTAM    */ 00037000
*/*     RECEIVE MACRO WILL BE INITIALIZED.                           */ 00038000
*/********************************************************************/ 00039000
* DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/        0156 00040000
*CKMC0002:                                                         0157 00041000
*   STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */    0157 00042000
CKMC0002 L     @12,TVWAPTR                                         0157 00043000
         L     @12,TVWAFRWI(,@12)                                  0157 00044000
         STM   REG0,REG15,FRWACKRG(@12)                            0157 00045000
*   FRWACKPT(2)=ADDR(CKMC0002); /* SAVE ADDRESS    @ZM20856 */     0158 00046000
         LA    @01,CKMC0002                                        0158 00047000
         ST    @01,FRWACKPT+4(,@12)                                0158 00048000
* END;                  /* CHECKPOINT         @ZM20856*/           0159 00049000
* REG7=ADDR(IKTIOFRR);                                             0160 00050000
         L     @12,CVTPTR                                          0160 00051000
         L     @01,CVTTCASP(,@12)                                  0160 00052000
         L     REG7,TCASFRR(,@01)                                  0160 00053000
* GEN  REFS(FRRSCURR,FRRS,FRRSLAST,PSALSFCC,PSA,FRRSELEN,PSACSTK,  0161 00054000
*           FRRSFRRA,FRRSENTR,FRRSPARM,REG7)                       0161 00055000
*           SETS(REG1,REG2,REG3,FRRSCURR);                         0161 00056000
   SETFRR A,FRRAD=(REG7),WRKREGS=(REG1,REG2),PARMAD=(REG3)              00057000
* PARMPTR = REG3;                      /* ADDRESS OF PARM AREA       */ 00058000
         ST    REG3,PARMPTR                                        0162 00059000
* RFY (REG3,REG7) UNRSTD;              /* FREE REGS FOR COMPILER     */ 00060000
*                                                                  0163 00061000
* CALL INITFRR;                        /* CALL FRR INITIALIZE      0164 00062000
*                                         ROUTINE            @ZM20856*/ 00063000
         BAL   @14,INITFRR                                         0164 00064000
* /*******************************************************************/ 00065000
* /*  OBTAIN THE LOCAL LOCK AND FREE THE ADDRESS OF THE TIM SRB      */ 00066000
* /*  IN THE TSB EXTENSION.                                          */ 00067000
* /*******************************************************************/ 00068000
* REG2=REG0;                           /* SAVE ADDRESS OF SRB TO FREE*/ 00069000
         LR    REG2,REG0                                           0165 00070000
* GEN(SETLOCK OBTAIN,TYPE=LOCAL,RELATED=(SRB),MODE=UNCOND,REGS=USE)     00071000
* REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);                   0166 00072000
         SETLOCK OBTAIN,TYPE=LOCAL,RELATED=(SRB),MODE=UNCOND,REGS=USE   00073000
* TSBXSRBI=0;                          /* CLEAR ADDRESS OF SRB       */ 00074000
