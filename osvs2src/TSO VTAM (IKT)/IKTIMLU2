         MVI   TVWALNCT(@07),X'01'                                 0467 01081000
*                   TVWAFMSC='1'B;     /*  SET FORMAT SCREEN FLG     */ 01082000
         OI    TVWAFMSC(@07),B'10000000'                           0468 01083000
*                   IF TVWAKBDL='0'B THEN                          0469 01084000
         TM    TVWAKBDL(@07),B'00100000'                           0469 01085000
         BNZ   @RF00469                                            0469 01086000
*                     DO;              /* KEYBOARD-UNLOCK PROCESSING */ 01087000
*                       TVWAULK='1'B;  /* REQUEST KEYBOARD UNLOCK    */ 01088000
         OI    TVWAULK(@07),B'00000100'                            0471 01089000
*                       TIMSCHTM='1'B; /* SET TIM SCHED TOM FLAG     */ 01090000
         OI    TIMSCHTM(@12),B'01000000'                           0472 01091000
*                     END;             /* END OF KEYBOARD-UNLOCK PROC*/ 01092000
*                   ELSE;              /* END OF KEYBOARD UNLK       */ 01093000
@RF00469 DS    0H                                                  0475 01094000
*                 END;                 /* END OF CLEAR PROCESSING    */ 01095000
*               ELSE                   /* NOT CLEAR AID              */ 01096000
*                 CALL IKT3270I;       /* PROCESS INPUT IN RPL       */ 01097000
         B     @RC00465                                            0476 01098000
@RF00465 L     @12,CVTPTR                                          0476 01099000
         L     @12,CVTTCASP(,@12)                                  0476 01100000
         L     @15,TCASDATI(,@12)                                  0476 01101000
         BALR  @14,@15                                             0476 01102000
*           END;                       /* END OF PAGING FLAG OFF     */ 01103000
@RC00465 DS    0H                                                  0477 01104000
@RC00461 DS    0H                                                  0477 01105000
@RC00459 DS    0H                                                  0478 01106000
*    END;                              /* END OF FIRST RECEIVE       */ 01107000
@RC00452 DS    0H                                                  0478 01108000
@RC00447 DS    0H                                                  0478 01109000
@RC00442 DS    0H                                                  0478 01110000
@RC00405 DS    0H                                                  0479 01111000
*    IF REC2FLG = '0'B THEN            /* CHECK IF FIRST RECEIVE     */ 01112000
@RC00386 L     @12,TIMGETMN                                        0479 01113000
         TM    REC2FLG(@12),B'00000010'                            0479 01114000
         BNZ   @RF00479                                            0479 01115000
*      DO;                             /* BEGIN FIRST-RECEIVE PROCESS*/ 01116000
*        TVWAKBDL='1'B;                /* KEYBOARD NOW LOCKED        */ 01117000
         L     @07,TVWAPTR                                         0481 01118000
         OI    TVWAKBDL(@07),B'00100000'                           0481 01119000
*        TVWAIOTR='1'B;                /* INPUT RECEIVED      OZ11997*/ 01120000
         OI    TVWAIOTR(@07),B'01000000'                           0482 01121000
*        PRELINE=0;                    /* RESET PRELINE              */ 01122000
         SLR   @07,@07                                             0483 01123000
         ST    @07,PRELINE(,@12)                                   0483 01124000
*        IF TIMSCHTM='1'B THEN         /* IS TOM TO BE SCHEDULED?    */ 01125000
         TM    TIMSCHTM(@12),B'01000000'                           0484 01126000
         BNO   @RF00484                                            0484 01127000
*          DO;                         /* PROCESS HERE IF YES        */ 01128000
*            CALL SCHTOM;              /* SCHEDULE IKTOMLU2          */ 01129000
         BAL   @14,SCHTOM                                          0486 01130000
*          END;                        /* IKTOMLU2 IS SCHEDULED      */ 01131000
*        ELSE                          /* TOM NEED NOT BE SCHEDULED  */ 01132000
*          DO;                         /* CHECK FOR MORE DATA TO SEND*/ 01133000
         B     @RC00484                                            0488 01134000
@RF00484 DS    0H                                                  0489 01135000
*            IF TVWATAS='1'B THEN      /* CHECK IF TOM UNAVAILABLE   */ 01136000
         L     @12,TVWAPTR                                         0489 01137000
         TM    TVWATAS(@12),B'00100000'                            0489 01138000
         BNO   @RF00489                                            0489 01139000
*              DO;                     /* TOM IS UNAVAILABLE         */ 01140000
*                TVWATAS='0'B;         /* SET NO SCHEDULE BIT OFF    */ 01141000
         NI    TVWATAS(@12),B'11011111'                            0491 01142000
*                IF TVWADOOQ='1'B | TVWAULK='1'B THEN              0492 01143000
         TM    TVWADOOQ(@12),B'01000000'                           0492 01144000
         BO    @RT00492                                            0492 01145000
         TM    TVWAULK(@12),B'00000100'                            0492 01146000
         BNO   @RF00492                                            0492 01147000
@RT00492 DS    0H                                                  0493 01148000
*                  CALL SCHTOM;        /* CALL SCHEDULE TOM RTN      */ 01149000
         BAL   @14,SCHTOM                                          0493 01150000
*                ELSE;                                             0494 01151000
@RF00492 DS    0H                                                  0495 01152000
*              END;                    /* TOM IS NOW AVAILABLE       */ 01153000
*            ELSE;                     /* TOM IS AVAILABLE           */ 01154000
@RF00489 DS    0H                                                  0497 01155000
*          END;                        /* END OF DATA CHECK          */ 01156000
*      END;                            /* END FIRST-RECEIVE PROCESS  */ 01157000
*    ELSE;                             /* SECOND RECEIVE             */ 01158000
@RF00479 DS    0H                                                  0500 01159000
*    RPLCMD = '0'B;                    /* RESET CHANGE-DIRECTION BIT */ 01160000
@RC00479 L     @12,RPLPTR                                          0500 01161000
         NI    RPLCMD+16(@12),B'11011111'                          0500 01162000
*  END DATARTN;                        /* END OF DATARTN             */ 01163000
@EL00009 DS    0H                                                  0501 01164000
@EF00009 DS    0H                                                  0501 01165000
@ER00009 LM    @14,@12,@SA00009                                    0501 01166000
         BR    @14                                                 0501 01167000
         EJECT                                                          01168000
*DATARDBF:  PROCEDURE OPTIONS(SAVE(RTNREG));                       0502 01169000
*                                                                  0502 01170000
DATARDBF ST    @14,@SA00010                                        0502 01171000
*  /******************************************************************/ 01172000
*  /* PREPARE TO RETRIEVE THE CONTENTS OF THE TERMINAL BUFFER AS THE */ 01173000
*  /* RESULT OF TOM'S HAVING ISSUED A READ-BUFFER.  MOVE THE CONTENTS*/ 01174000
*  /* OF THE BUFFER INTO A GETMAINED AREA.  IF THE DATA IS IN ASCII, */ 01175000
*  /* TRANSLATE DATA INTO EBCDIC.                                    */ 01176000
*  /* TVWAGMPT POINTS TO THE GETMAINED AREA.  THE GETMAINED AREA HAS */ 01177000
*  /* THE FOLLOWING FORMAT:                                          */ 01178000
*  /*                                                                */ 01179000
*  /*  **********                                                    */ 01180000
*  /*  *TVWAGMPT****                                                 */ 01181000
*  /*  **********  *                                                 */ 01182000
*  /*              *                                                 */ 01183000
*  /*             ***                                                */ 01184000
*  /*              *                                                 */ 01185000
*  /*              ************************************************* */ 01186000
*  /*              *GTMNLEN*DATALEN*RSV*BUFFER     CONTENTS*RSV*RSV* */ 01187000
*  /*              ************************************************* */ 01188000
*  /*                                                                */ 01189000
*  /* GTMNLEN (4 BYTES) IS THE LENGTH OF THE GETMAINED AREA.         */ 01190000
*  /* DATALEN (4 BYTES) IS THE LENGTH OF THE BUFFER CONTENTS.        */ 01191000
*  /* RSV (2 BYTES EACH) IS RESERVED FOR USE BY IKTOMLU2.            */ 01192000
*  /******************************************************************/ 01193000
*                                                                  0503 01194000
*  DCL GTMNLEN  FIXED(31) BASED(TVWAGMPT),      /* GETMAIN LENGTH    */ 01195000
*      DATALEN  FIXED(31) BASED(TVWAGMPT + 4);  /* DATA LENGTH       */ 01196000
*                                                                  0503 01197000
*  IF REC2FLG = '0'B THEN                       /* FIRST RECEIVE?    */ 01198000
         L     @12,TIMGETMN                                        0504 01199000
         TM    REC2FLG(@12),B'00000010'                            0504 01200000
         BNZ   @RF00504                                            0504 01201000
*    CALL GETAREAR;                             /* CALL GET-AREA RTN */ 01202000
         BAL   @14,GETAREAR                                        0505 01203000
*  ELSE;                                        /* NOT FIRST RECEIVE */ 01204000
@RF00504 DS    0H                                                  0507 01205000
*  IF TVWAERMG = '1'B THEN                      /* ERROR?            */ 01206000
         L     @12,TVWAPTR                                         0507 01207000
         TM    TVWAERMG(@12),B'00001000'                           0507 01208000
         BNO   @RF00507                                            0507 01209000
*    DO;                                        /* BEGIN ERROR PROCES*/ 01210000
*      TVWABRIN = '0'B;                         /* RESET BREAK FLAG  */ 01211000
         NI    TVWABRIN(@12),B'01111111'                           0509 01212000
*      TVWARDBF = '0'B;                         /* RESET RDBF FLAG   */ 01213000
         NI    TVWARDBF(@12),B'11011111'                           0510 01214000
*      TIMADDER = '1'B;                         /* ERROR             */ 01215000
         L     @12,TIMGETMN                                        0511 01216000
         OI    TIMADDER(@12),B'00010000'                           0511 01217000
*      IF RPLRLEN <= RPLBUFL &                  /* RU <= BUFFER AND  */ 01218000
*        (RPLLAST = '1'B | RPLONLY = '1'B)      /* EOC OR OC?        */ 01219000
*      THEN                                                        0512 01220000
         L     @07,RPLPTR                                          0512 01221000
         CLC   RPLRLEN(4,@07),RPLBUFL(@07)                         0512 01222000
         BH    @RF00512                                            0512 01223000
         TM    RPLLAST+16(@07),B'00110000'                         0512 01224000
         BZ    @RF00512                                            0512 01225000
*        REC2FLG = '0'B;                                           0513 01226000
         NI    REC2FLG(@12),B'11111101'                            0513 01227000
*      ELSE                                                        0514 01228000
*        REC2FLG = '1'B;                        /* 2ND RECEIVE NEEDED*/ 01229000
         B     @RC00512                                            0514 01230000
@RF00512 L     @12,TIMGETMN                                        0514 01231000
         OI    REC2FLG(@12),B'00000010'                            0514 01232000
*    END;                                       /* END ERROR PROCESS */ 01233000
*  ELSE                                         /* NO ERROR          */ 01234000
*    DO;                                        /* PROCESS DATA      */ 01235000
         B     @RC00507                                            0516 01236000
@RF00507 DS    0H                                                  0517 01237000
*      IF REC2FLG = '0'B THEN                   /* FIRST RECEIVE?    */ 01238000
         L     @12,TIMGETMN                                        0517 01239000
         TM    REC2FLG(@12),B'00000010'                            0517 01240000
         BNZ   @RF00517                                            0517 01241000
*        DO;                                    /* DO 1ST REC PROCESS*/ 01242000
*          DATALEN = MIN(RPLBUFL,RPLRLEN);      /* DATA LENGTH       */ 01243000
         L     @07,TVWAPTR                                         0519 01244000
         L     @07,TVWAGMPT(,@07)                                  0519 01245000
         L     @15,RPLPTR                                          0519 01246000
         L     @14,RPLBUFL(,@15)                                   0519 01247000
         L     @11,RPLRLEN(,@15)                                   0519 01248000
         CR    @14,@11                                             0519 01249000
         BNH   *+6                                                      01250000
         LR    @14,@11                                             0519 01251000
         ST    @14,DATALEN+4(,@07)                                 0519 01252000
*          RFY (REG0,REG1,REG2,REG3) RSTD;      /* RESTRICT REGISTERS*/ 01253000
*          REG0 = TEMPBFPT;                     /* AREA FOR MOVING   */ 01254000
         L     REG0,TEMPBFPT(,@12)                                 0521 01255000
*          REG1 = DATALEN;                      /* SIZE OF AREA      */ 01256000
         LR    REG1,@14                                            0522 01257000
*          REG2 = RPLAREA;                      /* DATA ADDRESS      */ 01258000
         L     REG2,RPLAREA(,@15)                                  0523 01259000
*          REG3 = DATALEN;                      /* SIZE OF DATA      */ 01260000
         LR    REG3,@14                                            0524 01261000
*          MVCL(REG0,REG2);                     /* MOVE IN DATA      */ 01262000
         MVCL  REG0,REG2                                           0525 01263000
*          RFY (REG0,REG1,REG2,REG3) UNRSTD;    /* FREE REGISTERS    */ 01264000
*          TEMPBFPT = TEMPBFPT + DATALEN;       /* UPDATE POINTER    */ 01265000
         L     @12,TIMGETMN                                        0527 01266000
         L     @07,TVWAPTR                                         0527 01267000
         L     @07,TVWAGMPT(,@07)                                  0527 01268000
         L     @15,TEMPBFPT(,@12)                                  0527 01269000
         AL    @15,DATALEN+4(,@07)                                 0527 01270000
         ST    @15,TEMPBFPT(,@12)                                  0527 01271000
*        END;                                   /* END 1ST REC PROCES*/ 01272000
*      ELSE                                     /* NOT FIRST RECEIVE */ 01273000
*        DO;                                                       0529 01274000
         B     @RC00517                                            0529 01275000
@RF00517 DS    0H                                                  0530 01276000
*          DATALEN = DATALEN + RPLRLEN;         /* UP DATA LENGTH    */ 01277000
         LA    @12,4                                               0530 01278000
         L     @07,TVWAPTR                                         0530 01279000
         AL    @12,TVWAGMPT(,@07)                                  0530 01280000
         L     @07,RPLPTR                                          0530 01281000
         L     @07,RPLRLEN(,@07)                                   0530 01282000
         L     @15,DATALEN(,@12)                                   0530 01283000
         ALR   @15,@07                                             0530 01284000
         ST    @15,DATALEN(,@12)                                   0530 01285000
*          TEMPBFPT = TEMPBFPT + RPLRLEN;       /* UP DATA POINTER   */ 01286000
         L     @12,TIMGETMN                                        0531 01287000
         AL    @07,TEMPBFPT(,@12)                                  0531 01288000
         ST    @07,TEMPBFPT(,@12)                                  0531 01289000
*        END;                                                      0532 01290000
*      IF RPLRLEN <= RPLBUFL &                  /* RU <= BUFFER AND  */ 01291000
*        (RPLLAST = '1'B | RPLONLY = '1'B)      /* EOC OR OC?        */ 01292000
*      THEN                                     /* RU <= BUFFER AND  */ 01293000
@RC00517 L     @12,RPLPTR                                          0533 01294000
         CLC   RPLRLEN(4,@12),RPLBUFL(@12)                         0533 01295000
         BH    @RF00533                                            0533 01296000
         TM    RPLLAST+16(@12),B'00110000'                         0533 01297000
         BZ    @RF00533                                            0533 01298000
*        DO;                                    /* EOC OR OC         */ 01299000
*          IF TSBXASCI = '1'B THEN              /* ASCII?            */ 01300000
         L     @12,PSAAOLD                                         0535 01301000
         L     @12,ASCBTSB(,@12)                                   0535 01302000
         L     @12,TSBEXTNT(,@12)                                  0535 01303000
         TM    TSBXASCI(@12),B'10000000'                           0535 01304000
         BNO   @RF00535                                            0535 01305000
*            CALL ASCITRAN;                     /* ASCII TRANSLATION */ 01306000
         BAL   @14,ASCITRAN                                        0536 01307000
*          ELSE;                                /* NOT ASCII         */ 01308000
@RF00535 DS    0H                                                  0538 01309000
*          REC2FLG = '0'B;                      /* RESET RECEIVE FLAG*/ 01310000
         L     @12,TIMGETMN                                        0538 01311000
         NI    REC2FLG(@12),B'11111101'                            0538 01312000
*          RPLBUFL = LENGTH(TIMBUFF);           /* RPL BUFFER LENGTH */ 01313000
         L     @07,RPLPTR                                          0539 01314000
         MVC   RPLBUFL(4,@07),@CF01780                             0539 01315000
*          RPLAREA = ADDR(TIMBUFF);             /* RPL BUFFER ADDRESS*/ 01316000
         LA    @12,TIMBUFF(,@12)                                   0540 01317000
         ST    @12,RPLAREA(,@07)                                   0540 01318000
*          TVWARCDT = '1'B;                     /* DATA RECEIVED     */ 01319000
         L     @12,TVWAPTR                                         0541 01320000
         OI    TVWARCDT(@12),B'00010000'                           0541 01321000
*          TVWARDBF = '0'B;                     /* RESET RDBF FLAG   */ 01322000
         NI    TVWARDBF(@12),B'11011111'                           0542 01323000
*        END;                                                      0543 01324000
*      ELSE                                     /* RU > BUFFER       */ 01325000
*        DO;                                                       0544 01326000
         B     @RC00533                                            0544 01327000
@RF00533 DS    0H                                                  0545 01328000
*          REC2FLG = '1'B;                      /* DO ANOTHER RECEIVE*/ 01329000
         L     @12,TIMGETMN                                        0545 01330000
         OI    REC2FLG(@12),B'00000010'                            0545 01331000
*          RPLAREA = TEMPBFPT;                  /* RESET BUFFER PTR  */ 01332000
         L     @07,RPLPTR                                          0546 01333000
         L     @12,TEMPBFPT(,@12)                                  0546 01334000
         ST    @12,RPLAREA(,@07)                                   0546 01335000
*          RPLBUFL = GTMNLEN - DATALEN - 10;    /* RESET BUFFER SIZE */ 01336000
         L     @12,TVWAPTR                                         0547 01337000
         L     @12,TVWAGMPT(,@12)                                  0547 01338000
         L     @15,GTMNLEN(,@12)                                   0547 01339000
         SL    @15,DATALEN+4(,@12)                                 0547 01340000
         SL    @15,@CF02237                                        0547 01341000
         ST    @15,RPLBUFL(,@07)                                   0547 01342000
*                                               /* 10 =  4 GTMNLEN   */ 01343000
*                                               /*      +4 DATALEN   */ 01344000
*                                               /*      +2 RSV-TOM   */ 01345000
*        END;                                                      0548 01346000
*    END;                                       /* END OF NO ERROR   */ 01347000
         EJECT                                                          01348000
* ASCITRAN: PROCEDURE OPTIONS(SAVE(RTNREG));                       0550 01349000
*                                                                  0550 01350000
         B     @PB00011                                            0550 01351000
ASCITRAN ST    @14,12(,@13)                                        0550 01352000
*   /*****************************************************************/ 01353000
*   /* PERFORM ASCII TRANSLATION.                                    */ 01354000
*   /*****************************************************************/ 01355000
*                                                                  0551 01356000
*   SCANPTR = TVWAGMPT + 10;                /* SET SCAN AREA POINTER */ 01357000
         L     @12,TVWAPTR                                         0551 01358000
         L     @12,TVWAGMPT(,@12)                                  0551 01359000
         LA    @07,10                                              0551 01360000
         ALR   @07,@12                                             0551 01361000
         ST    @07,SCANPTR                                         0551 01362000
*                                           /* 10 =  4 GTMNLEN       */ 01363000
*                                           /*      +4 DATALEN       */ 01364000
*                                           /*      +2 RSV-TOM       */ 01365000
*   COUNT = DATALEN;                        /* SET TRANSLATION COUNT */ 01366000
         L     @07,TIMGETMN                                        0552 01367000
         L     @12,DATALEN+4(,@12)                                 0552 01368000
         ST    @12,COUNT(,@07)                                     0552 01369000
*   DONE = '0'B;                            /* RESET DO-WHILE FLAG   */ 01370000
         NI    DONE(@07),B'11111011'                               0553 01371000
*   DO WHILE (DONE = '0'B);                                        0554 01372000
         B     @DE00554                                            0554 01373000
@DL00554 DS    0H                                                  0555 01374000
*     IF COUNT > 256 THEN                   /* COUNT > 256?          */ 01375000
         LA    @12,256                                             0555 01376000
         L     @07,TIMGETMN                                        0555 01377000
         L     @15,COUNT(,@07)                                     0555 01378000
         CR    @15,@12                                             0555 01379000
         BNH   @RF00555                                            0555 01380000
*       DO;                                                        0556 01381000
*         VALUE = 256;                      /* AMOUNT TO TRANSLATE   */ 01382000
         ST    @12,VALUE(,@07)                                     0557 01383000
*         COUNT = COUNT - 256;              /* NO. LEFT TO TRANSLATE */ 01384000
         SLR   @15,@12                                             0558 01385000
         ST    @15,COUNT(,@07)                                     0558 01386000
*       END;                                                       0559 01387000
*     ELSE                                  /* COUNT <= 256          */ 01388000
*       DO;                                                        0560 01389000
         B     @RC00555                                            0560 01390000
@RF00555 DS    0H                                                  0561 01391000
*         VALUE = COUNT;                    /* AMOUNT TO TRANSLATE   */ 01392000
         L     @12,TIMGETMN                                        0561 01393000
         L     @07,COUNT(,@12)                                     0561 01394000
         ST    @07,VALUE(,@12)                                     0561 01395000
*         DONE = '1'B;                      /* NO DATA LEFT          */ 01396000
         OI    DONE(@12),B'00000100'                               0562 01397000
*       END;                                                       0563 01398000
*     TR(SCANAREA(1:VALUE),ASCIITAB);       /* TRANSLATE DATA        */ 01399000
@RC00555 L     @12,SCANPTR                                         0564 01400000
         L     @07,TIMGETMN                                        0564 01401000
         L     @07,VALUE(,@07)                                     0564 01402000
         BCTR  @07,0                                               0564 01403000
         L     @01,TVWAPTR                                         0564 01404000
         L     @01,TVWAATBI(,@01)                                  0564 01405000
         EX    @07,@SB02829                                        0564 01406000
*     SCANPTR = SCANPTR + VALUE;            /* UPDATE SCAN-AREA PTR  */ 01407000
         L     @07,TIMGETMN                                        0565 01408000
         AL    @12,VALUE(,@07)                                     0565 01409000
         ST    @12,SCANPTR                                         0565 01410000
*   END;                                                           0566 01411000
*                                                                  0566 01412000
@DE00554 L     @12,TIMGETMN                                        0566 01413000
         TM    DONE(@12),B'00000100'                               0566 01414000
         BZ    @DL00554                                            0566 01415000
* END ASCITRAN;                             /* END OF ASCITRAN       */ 01416000
@EL00011 DS    0H                                                  0567 01417000
@EF00011 DS    0H                                                  0567 01418000
@ER00011 L     @14,12(,@13)                                        0567 01419000
         BR    @14                                                 0567 01420000
         EJECT                                                          01421000
* GETAREAR:  PROCEDURE OPTIONS(SAVE(RTNREG));                      0568 01422000
*                                                                  0568 01423000
GETAREAR ST    @14,@SA00012                                        0568 01424000
*   /*****************************************************************/ 01425000
*   /* GETMAIN AREA FOR CONTENTS OF TERMINAL BUFFER.                 */ 01426000
*   /*****************************************************************/ 01427000
*                                                                  0569 01428000
*   RFY (REG1,REG7,REG9,REG10) RSTD;                               0569 01429000
*   IF RPLONLY = '1'B THEN            /* ONLY CHAIN?                 */ 01430000
         L     @12,RPLPTR                                          0570 01431000
         TM    RPLONLY+16(@12),B'00010000'                         0570 01432000
         BNO   @RF00570                                            0570 01433000
*     REG10 = RPLRLEN + 14;           /* GETMAIN LENGTH              */ 01434000
         LA    REG10,14                                            0571 01435000
         AL    REG10,RPLRLEN(,@12)                                 0571 01436000
*                                     /* 14 =  4 GTMNLEN             */ 01437000
*                                     /*      +4 DATALEN             */ 01438000
*                                     /*      +6 RSV FOR IKTOMLU2    */ 01439000
*   ELSE                              /* MULTIPLE-RU CHAIN           */ 01440000
*                                                                  0572 01441000
*     /***************************************************************/ 01442000
*     /* FOR EACH ATTRIBUTE IN THE BUFFER, 2 BYTES (SF + ATTRIBUTE)  */ 01443000
*     /* WILL BE PASSED IN THE RECEIVE.                              */ 01444000
*     /* A POSSIBLE READ HEADING WILL REQUIRE 3 BYTES.               */ 01445000
*     /* GTMNLEN REQUIRES 4 BYTES.                                   */ 01446000
*     /* DATALEN REQUIRES 4 BYTES.                                   */ 01447000
*     /* 6 BYTES WILL BE RESERVED FOR IKTOMLU2.                      */ 01448000
*     /* THEREFORE, THE MAXIMUM NUMBER OF BYTES REQUIRED WILL BE     */ 01449000
*     /* 2*TSBXTMBF + 17.                                            */ 01450000
*     /***************************************************************/ 01451000
*                                                                  0572 01452000
*     REG10 = 2*TSBXTMBF + 17;        /* GETMAIN LENGTH              */ 01453000
*                                                                  0572 01454000
