         NI    RPLNFSYN+16(@12),B'11111011'                        0189 00153000
*  RFY (REG0,REG1,REG2,REG3,REG7,REG15) RSTD; /*             @ZM20858*/ 00154000
*  REG2=ADDR(IFGRPL);                  /* ADDR OF RPL FOR RECEIVE    */ 00155000
         LR    REG2,@12                                            0191 00156000
*  REG7=ADDR(IKTIOFRR);                /* PTR TO FRR ENTRY           */ 00157000
         L     @14,CVTPTR                                          0192 00158000
         L     @14,CVTTCASP(,@14)                                  0192 00159000
         L     REG7,TCASFRR(,@14)                                  0192 00160000
*  GEN REFS(REG1,REG2,REG3,REG7,FRRSCURR,FRRS,FRRSLAST,PSALSFCC,PSA,    00161000
*           FRRSELEN,PSACSTK,FRRSFRRA,FRRSENTR,FRRSPARM)           0193 00162000
*           SETS(REG1,REG2,REG3,REG14,REG15,REG0);                 0193 00163000
  RECEIVE  RPL=(REG2)                                                   00164000
  SETFRR A,FRRAD=(REG7),WRKREGS=(REG1,REG2),PARMAD=(REG3)               00165000
  STM REG14,REG1,12(REG13)   /*SAVE RECEIVE RTN CD REGS 0,15@ZM20858*/  00166000
*  PARMPTR = REG3;                     /* POINTER TO FRR PARAMETERS  */ 00167000
         ST    REG3,PARMPTR                                        0194 00168000
*  RFY (REG1,REG2,REG3,REG7) UNRSTD;   /* FREE REGISTERS FOR USE     */ 00169000
*  CALL INITFRR;                       /* CALL FRR INITIALIZE      0196 00170000
*                                         ROUTINE            @ZM20856*/ 00171000
         BAL   @14,INITFRR                                         0196 00172000
*  /******************************************************************/ 00173000
*  /* EXAMINE INPUT FROM RECEIVE HERE                                */ 00174000
*  /******************************************************************/ 00175000
*                                                                  0197 00176000
*  GEN(SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RECVE),REGS=USE)  00177000
*      REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);      /*@ZM20753*/ 00178000
