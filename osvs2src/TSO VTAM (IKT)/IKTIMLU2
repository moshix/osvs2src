*      GEN(SCHEDULE SRB=(REG2),SCOPE=LOCAL)                        0370 00757000
*              REFS(CVTPTR,CVTLSMQ,CVTMAP,SRBFLNK,SRBSECT);        0370 00758000
         SCHEDULE SRB=(REG2),SCOPE=LOCAL                                00759000
*      TVWATIS='1'B;                   /* SET TOM SCHEDULED ON       */ 00760000
         L     @12,TVWAPTR                                         0371 00761000
         OI    TVWATIS(@12),B'01000000'                            0371 00762000
*      RFY REG2 UNRSTD;                /* FREE REGISTER 2            */ 00763000
*    END;                              /* IKTOMLU2 IS SCHEDULED      */ 00764000
*  ELSE;                               /* IKTOMLU2 HAS BEEN SCHEDULED*/ 00765000
@RF00366 DS    0H                                                  0375 00766000
*  TIMSCHTM = '0'B;                    /* IKTOMLU2 SCHEDULED         */ 00767000
         L     @12,TIMGETMN                                        0375 00768000
         NI    TIMSCHTM(@12),B'10111111'                           0375 00769000
*  END SCHTOM;                         /* END OF SCHTOM              */ 00770000
