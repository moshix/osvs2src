*   REG13=ADDR(LOCKWA);             /* INSERT LOCAL LOCK WORKAREA  0098 00257000
*                                                            @ZA17868*/ 00258000
         LA    REG13,LOCKWA                                        0098 00259000
*   GENERATE CODE                   /* FREE ALL LOCKS                */ 00260000
*       REFS(PSALITA,FLC,PSAMPSW);                                 0099 00261000
         SETLOCK RELEASE,TYPE=CMS,REGS=SAVE,                          **00262000
               RELATED=('TSB CHAIN SCANNED')                            00263000
         SETLOCK RELEASE,TYPE=LOCAL,REGS=SAVE,RELATED=('EXITTING')      00264000
*   REG13=REG0;                     /* RESTORE REGISTER CONTENTS     */ 00265000
         LR    REG13,REG0                                          0100 00266000
*/****************************************************************** */ 00267000
*/*       GIVE TERMINAL CONTROL TO OLD ADDRESS SPACE                 */ 00268000
*/****************************************************************** */ 00269000
*                                                                  0101 00270000
*   RFY                                                            0101 00271000
*     TSB BASED(ASCBTSB);           /* TSB IS FOR THIS ADDRESS SPACE */ 00272000
*   RFY                                                            0102 00273000
*    (RETCODE,                                                     0102 00274000
*     RPLREG) RSTD;                 /* RESTRICT RETURN CODE REGISTER */ 00275000
*   RPLREG=TVWARPL;                 /* LOAD ADDRESS OF RPL INTO A  0103 00276000
*                                      REGISTER                      */ 00277000
