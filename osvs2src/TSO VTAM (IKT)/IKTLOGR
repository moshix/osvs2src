         L     @14,12(,@13)                                        0089 00227000
         LM    @00,@12,20(@13)                                     0089 00228000
         BR    @14                                                 0089 00229000
*         END;                                                     0090 00230000
*     ELSE                          /* TSB NOT IN RECONNECT STATE OR    00231000
*                                      USERID DOES NOT MATCH         */ 00232000
*       DO;                                                        0091 00233000
         B     @RC00055                                            0091 00234000
@RF00055 DS    0H                                                  0092 00235000
*         RFY                                                      0092 00236000
*           TSB BASED(TSBBASE);     /* BASE TSB ON SEARCH REGISTER   */ 00237000
*         TSBBASE=TSBXFWD;          /* GET ADDRESS OF NEXT TSB       */ 00238000
         L     @10,TSBEXTNT(,TSBBASE)                              0093 00239000
         L     @10,TSBXFWD(,@10)                                   0093 00240000
         LR    TSBBASE,@10                                         0093 00241000
*       END;                                                       0094 00242000
*   END;                            /* END OF LOOP                   */ 00243000
@RC00055 DS    0H                                                  0095 00244000
@DE00044 LTR   TSBBASE,TSBBASE                                     0095 00245000
         BNM   @DL00044                                            0095 00246000
*   TSBBASE=-TSBBASE;               /* RESTORE TSB POINTER           */ 00247000
         LCR   TSBBASE,TSBBASE                                     0096 00248000
*/****************************************************************** */ 00249000
*/*  AFTER HAVING LOCATED THE TSB BELONGING TO THE OLD SESSION       */ 00250000
*/*  FREE ALL LOCKS                                                  */ 00251000
*/****************************************************************** */ 00252000
*                                                                  0097 00253000
*   REG0=REG13;                     /* SAVE PREVIOUS SAVEAREA POINTER   00254000
*                                                            @ZA17868*/ 00255000
