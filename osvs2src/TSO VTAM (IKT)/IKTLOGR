         L     RPLREG,TVWARPL(,@14)                                0103 00282000
*   TVWARTR=ADDR(RETRYMAX);         /* LOAD MAX RETRY VALUE          */ 00283000
         SLR   @10,@10                                             0104 00284000
         LA    @10,RETRYMAX(,@10)                                  0104 00285000
         STH   @10,TVWARTR(,@14)                                   0104 00286000
*   GENERATE CODE;                                                 0105 00287000
*                  /* PASS TERMINAL CONTROL TO OLD ADDRESS SPACE */     00288000
         CLSDST RPL=(RPLREG),                                         **00289000
               OPTCD=(SYN,PASS),AAREA=(WORKREG)                         00290000
*   RFY                                                            0106 00291000
*     WORKREG UNRSTD;               /* UNRESTRICT WORK REGSITER      */ 00292000
*   DO WHILE RETCODE^=0;            /* CHECK FOR SUCCESSFUL        0107 00293000
*                                      COMPLETION OF PASS            */ 00294000
         B     @DE00107                                            0107 00295000
@DL00107 DS    0H                                                  0108 00296000
*     IF RETCODE=4&REG0=8 THEN      /* TEST FOR RETRYABLE ERROR      */ 00297000
