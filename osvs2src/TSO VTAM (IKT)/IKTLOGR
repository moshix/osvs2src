         TITLE 'IKTLOGR: TSO/VTAM LOGON RECONNECT ROUTINE              *00001000
                        '                                               00002000
IKTLOGR  CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKTLOGR   77.319'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @07,@01                                             0001 00016000
         USING @DATD,@07                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @07,8(,@13)                                         0001 00020000
         LR    @13,@07                                             0001 00021000
*                                      SETLOCK MACRO                 */ 00022000
*/****************************************************************** */ 00023000
*/*       OBTAIN LOCAL AND CMS LOCK IN ORDER TO SEARCH TSB CHAIN     */ 00024000
*/****************************************************************** */ 00025000
*                                                                  0037 00026000
*   REG0=REG13;                     /* SAVE PREVIOUS SAVEAREA POINTER   00027000
*                                                            @ZA17868*/ 00028000
         LR    REG0,REG13                                          0037 00029000
*   REG13=ADDR(LOCKWA);             /* INSERT LOCAL LOCK WORKAREA  0038 00030000
*                                                            @ZA17868*/ 00031000
         LA    REG13,LOCKWA                                        0038 00032000
*   GENERATE CODE                   /* OBTAIN THE LOCAL LOCK         */ 00033000
*       REFS(PSALITA,FLC);                                         0039 00034000
         SETLOCK   OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=SAVE,  @ZA17868 **00035000
               RELATED=('PREREQUISITE FOR CMS LOCK')                    00036000
*   GENERATE CODE REFS(PSALITA,FLC);                               0040 00037000
*                                /* GET CMS LOCK */                     00038000
         SETLOCK   OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=SAVE,             **00039000
               RELATED=('SCAN TSB CHAIN ')                              00040000
*   REG13=REG0;                     /* RESTORE REGISTER CONTENTS   0041 00041000
*                                                            @ZA17868*/ 00042000
         LR    REG13,REG0                                          0041 00043000
*/****************************************************************** */ 00044000
*/* SEARCH TSB CHAIN BASED IN TCAS TABLE                             */ 00045000
*/****************************************************************** */ 00046000
*                                                                  0042 00047000
*   RFY                                                            0042 00048000
*    (REG14,                                                       0042 00049000
*     REG15,                                                       0042 00050000
*     REG10) UNRSTD;                /* UNRESTRICT REGISTERS          */ 00051000
*   TSBBASE=TCASTSB;                /* LOAD ADDRESS OF FIRST TSB     */ 00052000
         L     @08,CVTPTR                                          0043 00053000
         L     @08,TCASTPTR(,@08)                                  0043 00054000
         L     TSBBASE,TCASTSB(,@08)                               0043 00055000
*   DO WHILE(TSBBASE>=0);           /* START SEARCH OF CHAIN         */ 00056000
         B     @DE00044                                            0044 00057000
@DL00044 DS    0H                                                  0045 00058000
*     IF TSBBASE=0 THEN             /* TEST FOR END OF CHAIN         */ 00059000
         LTR   TSBBASE,TSBBASE                                     0045 00060000
         BNZ   @RF00045                                            0045 00061000
*       DO;                                                        0046 00062000
*         REG0=REG13;               /* SAVE PREVIOUS SAVEAREA POINTER   00063000
*                                                            @ZA17868*/ 00064000
         LR    REG0,REG13                                          0047 00065000
*         REG13=ADDR(LOCKWA);       /* INSERT LOCAL LOCK WORKAREA  0048 00066000
*                                                            @ZA17868*/ 00067000
         LA    REG13,LOCKWA                                        0048 00068000
*         GENERATE CODE             /* FREE ALL LOCKS                */ 00069000
*             REFS(PSALITA,FLC,PSAMPSW);                           0049 00070000
         SETLOCK RELEASE,TYPE=CMS,REGS=SAVE,                          **00071000
               RELATED=('TSB CHAIN SCANNED')                            00072000
         SETLOCK RELEASE,TYPE=LOCAL,REGS=SAVE,RELATED=('EXITTING')      00073000
*         REG13=REG0;               /* RESTORE REGISTER CONTENTS     */ 00074000
         LR    REG13,REG0                                          0050 00075000
*         GENERATE CODE REFS(RETCODE);                             0051 00076000
         L    15,4(13)       LOAD ADDRESS OF OLD SAVE AREA              00077000
         SLR  0,0            ZERO REGISTER 0                            00078000
         ST   0,20(,15)      STORE ZERO INTO REG0 POSITION              00079000
*         RETURN CODE(USERIDBD);    /* INDICATE UERID IS BAD TO    0052 00080000
*                                      CALLER                        */ 00081000
         L     @13,4(,@13)                                         0052 00082000
         L     @00,@SIZDATD                                        0052 00083000
         LR    @01,@07                                             0052 00084000
         FREEMAIN R,LV=(0),A=(1)                                        00085000
         LA    @15,8                                               0052 00086000
         L     @14,12(,@13)                                        0052 00087000
         LM    @00,@12,20(@13)                                     0052 00088000
         BR    @14                                                 0052 00089000
*       END;                                                       0053 00090000
*     RFY                                                          0054 00091000
*       TSB BASED(TSBBASE);         /* BASE TSB ON CURRENT BASE      */ 00092000
@RF00045 DS    0H                                                  0055 00093000
*     IF TSBXWREC='1'B&USERID=TSBXUID THEN                         0055 00094000
*                                                                  0055 00095000
         L     @10,TSBEXTNT(,TSBBASE)                              0055 00096000
         TM    TSBXWREC(@10),B'00010000'                           0055 00097000
         BNO   @RF00055                                            0055 00098000
         L     @08,PARM1PTR(,REG1)                                 0055 00099000
         CLC   USERID(8,@08),TSBXUID(@10)                          0055 00100000
         BNE   @RF00055                                            0055 00101000
*/********************************************************************/ 00102000
*/*               CHECK FOR WAITING FOR RECONNECT BIT                */ 00103000
*/*                                                                  */ 00104000
*/********************************************************************  00105000
*                                                                    */ 00106000
*                                                                  0056 00107000
*                                                                  0056 00108000
*/*  SEARCH TSB CHAIN FOR COMPARISON ON USERID AND PASSWORD          */ 00109000
*/*                                                                  */ 00110000
*                                                                  0056 00111000
*       IF PASSWORD=TSBPSWD THEN    /* CHECK FOR MATCHING PASSWORD   */ 00112000
         L     @08,PARM2PTR(,REG1)                                 0056 00113000
         CLC   PASSWORD(8,@08),TSBPSWD(TSBBASE)                    0056 00114000
         BNE   @RF00056                                            0056 00115000
*         DO;                       /* INSERT ALL NEW VALUES FROM  0057 00116000
*                                      THIS TSB                      */ 00117000
*           TSBTRMID=ASCBTSB->TSBTRMID;/* INSERT NEW TERMINAL ID   0058 00118000
*                                                            @G32TKKM*/ 00119000
         L     @08,ASCBPTR                                         0058 00120000
         L     @08,ASCBTSB(,@08)                                   0058 00121000
         MVC   TSBTRMID(8,TSBBASE),TSBTRMID(@08)                   0058 00122000
*/*                             INSERT NEW ASCII FLAG                */ 00123000
*                                                                  0059 00124000
*           IF ASCBTSB->TSBEXTNT->TSBXASCI='1'B THEN/* CHECK FOR BIT    00125000
*                                      ON                            */ 00126000
         L     @08,TSBEXTNT(,@08)                                  0059 00127000
         TM    TSBXASCI(@08),B'10000000'                           0059 00128000
         BNO   @RF00059                                            0059 00129000
*             TSBXASCI='1'B;        /* TURN ON BIT VARIABLE          */ 00130000
         OI    TSBXASCI(@10),B'10000000'                           0060 00131000
*           ELSE                                                   0061 00132000
*             TSBXASCI='0'B;        /* TURN OFF THE BIT              */ 00133000
         B     @RC00059                                            0061 00134000
@RF00059 L     @10,TSBEXTNT(,TSBBASE)                              0061 00135000
         NI    TSBXASCI(@10),B'01111111'                           0061 00136000
*/*                              INDICATE DISPLAY SCREEN             */ 00137000
*                                                                  0062 00138000
*           IF ASCBTSB->TSBDSPLY='1'B THEN/* CHECK BIT ORIENTATION   */ 00139000
@RC00059 L     @10,ASCBPTR                                         0062 00140000
         L     @10,ASCBTSB(,@10)                                   0062 00141000
         TM    TSBDSPLY(@10),B'00100000'                           0062 00142000
         BNO   @RF00062                                            0062 00143000
*             DO;                                                  0063 00144000
*               TSB3270='1'B;       /* TURN ON 3270 INDICATOR        */ 00145000
*               TSBDSPLY='1'B;      /* TURN ON THE BIT               */ 00146000
         OI    TSB3270(TSBBASE),B'00100010'                        0065 00147000
*             END;                                                 0066 00148000
*           ELSE                                                   0067 00149000
*             DO;                                                  0067 00150000
         B     @RC00062                                            0067 00151000
@RF00062 DS    0H                                                  0068 00152000
*               TSB3270='0'B;       /* TURN OFF 3270 INDICATOR       */ 00153000
*               TSBDSPLY='0'B;      /* TURN OFF THE BIT VARIABLE     */ 00154000
         NI    TSB3270(TSBBASE),B'11011101'                        0069 00155000
*             END;                                                 0070 00156000
*           TSBXTERM=ASCBTSB->TSBEXTNT->TSBXTERM;/* INSERT NEW     0071 00157000
*                                      TERMINAL CHARACTERISTICS      */ 00158000
@RC00062 L     @10,TSBEXTNT(,TSBBASE)                              0071 00159000
         L     @08,ASCBPTR                                         0071 00160000
         L     @08,ASCBTSB(,@08)                                   0071 00161000
         L     @08,TSBEXTNT(,@08)                                  0071 00162000
         MVC   TSBXTERM(4,@10),TSBXTERM(@08)                       0071 00163000
*           TSBXBIND=ASCBTSB->TSBEXTNT->TSBXBIND;/* INSERT NEW BIND     00164000
*                                      IMAGE                 @G58AKKM*/ 00165000
         MVC   TSBXBIND(36,@10),TSBXBIND(@08)                      0072 00166000
*           RFY                                                    0073 00167000
*             INDXREG RSTD;         /* RESTRICT TWO WORK REGISTERS   */ 00168000
*           INDXREG=TSBXAIND;       /* LOAD OLD ADDRESS SPACE USER 0074 00169000
*                                      INDEX                         */ 00170000
         L     @08,TSBEXTNT(,TSBBASE)                              0074 00171000
         LH    INDXREG,TSBXAIND(,@08)                              0074 00172000
*/****************************************************************** */ 00173000
*/*  CREATE THE APPLID OF THE OLD MEMORY TO FACILITATE PASSING OF    */ 00174000
*/*  CONTROL                                                         */ 00175000
*/****************************************************************** */ 00176000
*                                                                  0075 00177000
*           RFY                                                    0075 00178000
*             TSB BASED(ASCBTSB);   /* ESTABLISH ADDRESSABILITY TO 0075 00179000
*                                      TSB IN THIS ADDRESS SPACE     */ 00180000
*           WORKREG=ADDR(OLDAPPL);  /* LOAD ADDRESS OF WORK AREA FOR    00181000
*                                      CONVERSION                    */ 00182000
         LA    WORKREG,OLDAPPL                                     0076 00183000
*           GENERATE CODE           /* CONVERT FROM BINARY TO PACKED */ 00184000
*               REFS(INDXREG,WORKREG);                             0077 00185000
         CVD   INDXREG,0(WORKREG)   CONVERT FROM BINARY TO PACKED       00186000
         UNPK  0(8,WORKREG),0(8,WORKREG) UNPACK FROM PACKED TO ZONED    00187000
         OI    7(WORKREG),X'F0'     MASK SIGN ZONE                      00188000
*           OLDAPPL(1:7)=OLDAPPL(2:8);/* MOVE CHARACTERS OVER 1 BYTE */ 00189000
         MVC   @TS00001(7),OLDAPPL+1                               0078 00190000
         MVC   OLDAPPL(7),@TS00001                                 0078 00191000
*           OLDAPPL(8)=' ';         /* INSERT BLANK CHARACTER ON   0079 00192000
*                                      RIGHT                         */ 00193000
         MVI   OLDAPPL+7,C' '                                      0079 00194000
*           OLDAPPL(1:3)=TSO;       /* INSERT CHARACTER STRING 'TSO' */ 00195000
         MVC   OLDAPPL(3),TSO                                      0080 00196000
*           RFY                                                    0081 00197000
*             INDXREG UNRSTD;       /* UNRESTRICT THE INDEX REGISTER */ 00198000
*           TSBBASE=-TSBBASE;       /* TURN OF LOOP BY NEGATING BASE    00199000
*                                      REGISTER                      */ 00200000
         LCR   TSBBASE,TSBBASE                                     0082 00201000
*         END;                                                     0083 00202000
*       ELSE                        /* PASSWORD DOES NOT MATCH       */ 00203000
*         DO;                                                      0084 00204000
         B     @RC00056                                            0084 00205000
@RF00056 DS    0H                                                  0085 00206000
*           REG0=REG13;             /* SAVE PREVIOUS SAVEAREA POINTER   00207000
*                                                            @ZA17868*/ 00208000
         LR    REG0,REG13                                          0085 00209000
*           REG13=ADDR(LOCKWA);     /* INSERT LOCAL LOCK WORKAREA  0086 00210000
*                                                            @ZA17868*/ 00211000
         LA    REG13,LOCKWA                                        0086 00212000
*           GENERATE CODE           /* FREE ALL LOCKS                */ 00213000
*               REFS(PSALITA,FLC,PSAMPSW);                         0087 00214000
