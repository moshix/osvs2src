               RELATED=('TSB CHAIN SCANNED')                            00216000
         SETLOCK RELEASE,TYPE=LOCAL,REGS=SAVE,RELATED=('EXITTING')      00217000
*           REG13=REG0;             /* RESTORE REGISTER CONTENTS     */ 00218000
         LR    REG13,REG0                                          0088 00219000
*           RETURN CODE(PASWRDBD);  /* RETURN TO CALLER INDICATING 0089 00220000
*                                      CONDITION                     */ 00221000
         L     @13,4(,@13)                                         0089 00222000
         L     @00,@SIZDATD                                        0089 00223000
         LR    @01,@07                                             0089 00224000
