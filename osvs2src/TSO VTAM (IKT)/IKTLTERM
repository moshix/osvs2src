         L     @06,TVWAPTR(,@01)                                   0747 02693000
         OI    TVWAKBDL(@06),B'00100000'                           0747 02694000
*   TVWAULK='1'B;                   /* TURN ON UNLOCK KEY BOARD BIT     02695000
*                                                            @YM03213*/ 02696000
*   TVWATIS='1'B;                   /* INDICATE TOM IS SCHEDULED     */ 02697000
         OI    TVWAULK(@06),B'01000100'                            0749 02698000
*   TVWARET='1'B;                   /* INDICATE LTERM DRIVEN @ZM20856*/ 02699000
*   TVWACHSE='1'B;                  /* INDICATE CHASE        @ZM20856*/ 02700000
         OI    TVWARET(@06),B'00010010'                            0751 02701000
*   RFY                                                            0752 02702000
*    (REG1,                                                        0752 02703000
*     REG15) RSTD;                                                 0752 02704000
*   REG1=TVWARPL;                   /* SET RPL ADDRESS IN REG1     0753 02705000
*                                                            @ZM20856*/ 02706000
         L     REG1,TVWARPL(,@06)                                  0753 02707000
*   CALL IKTIST00;                  /* SCHEDULE LTERM TO CHASE     0754 02708000
*                                                            @ZM20856*/ 02709000
         L     @06,CVTPTR                                          0754 02710000
         L     @06,TCASTPTR(,@06)                                  0754 02711000
         L     @15,TCASSCHD(,@06)                                  0754 02712000
         BALR  @14,@15                                             0754 02713000
*   IF REG15^=0 THEN                /* LTERM NOT DRIVEN ?    @ZM20856*/ 02714000
         LTR   REG15,REG15                                         0755 02715000
         BZ    @RF00755                                            0755 02716000
*     DO;                           /* YES                   @ZM20856*/ 02717000
*       RFY                                                        0757 02718000
*        (REG2,                                                    0757 02719000
*         REG3) RSTD;               /*                       @ZM20856*/ 02720000
*       REG2=IKTMSGS(19);           /* GET MESSAGE           @ZM20856*/ 02721000
         L     @14,CVTPTR                                          0758 02722000
         L     @14,TCASTPTR(,@14)                                  0758 02723000
         L     @14,TCASMSGS(,@14)                                  0758 02724000
         L     REG2,IKTMSGS+72(,@14)                               0758 02725000
*       REG3=ADDR(WORKAREA);        /* GET ADDRESS OF WORK AREA    0759 02726000
*                                                            @ZM20856*/ 02727000
         LA    REG3,WORKAREA                                       0759 02728000
*       RFY                                                        0760 02729000
*         MSGOVRLY BASED(REG2);     /*                       @ZM20856*/ 02730000
*       WORKAREA(1:MSGLEN)=MSGTEXT(1:MSGLEN);/* GET TEXT     @ZM20856*/ 02731000
         LH    @14,MSGLEN(,REG2)                                   0761 02732000
         N     @14,@CF03064                                        0761 02733000
         BCTR  @14,0                                               0761 02734000
         EX    @14,@SM03065                                        0761 02735000
*       GENERATE CODE REFS(PSALITA,FLC);                           0762 02736000
*                                /*ISSUE MESSAGE            @ZM20856*/  02737000
         SETLOCK RELEASE,TYPE=LOCAL,REGS=SAVE,RELATED=('SVC ISSUE')     02738000
         WTO  ROUTCDE=(2,8),DESC=(3),MF=(E,(3))                         02739000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=SAVE,             **02740000
               RELATED=('SVC ISSUED')                                   02741000
*       RFY                                                        0763 02742000
*        (REG2,                                                    0763 02743000
*         REG3) UNRSTD;             /* UNRESTRICT WORK REGISTERS   0763 02744000
*                                                            @ZM20856*/ 02745000
*       RFY                                                        0764 02746000
*        (REG1,                                                    0764 02747000
*         REG15) RSTD;                                             0764 02748000
*       CALL TERMAS;                /* CANCEL ADDRESS SPACE  @ZM20856*/ 02749000
         BAL   @14,TERMAS                                          0765 02750000
*     END;                          /*                       @ZM20856*/ 02751000
*   RETURN CODE(0);                 /* INDICATE SUCCESSFUL         0767 02752000
*                                      REESTABLISHMENT               */ 02753000
@RF00755 SLR   @15,@15                                             0767 02754000
         L     @14,@SA00010                                        0767 02755000
         BR    @14                                                 0767 02756000
*   END RESSESS;                                                   0768 02757000
         B     @EL00010                                            0768 02758000
         EJECT                                                          02759000
*                                                                  0769 02760000
*/********************************************************************/ 02761000
*/* IKTTPEND                                                         */ 02762000
*/*          DRIVEN BY VTAM WHENEVER A HALT IS ISSUED FOR VTAM.      */ 02763000
*/*          IF DRIVEN FOR A NORMAL HALT,                            */ 02764000
*/*             IKTTPEND RETURNS TO VTAM IMMEDIATELY AND THE ADDRESS */ 02765000
*/*             SPACE CONTINUES TO EXECUTE.                          */ 02766000
*/*          IF DRIVEN FOR A HALT QUICK, OR A HALT CANCEL,           */ 02767000
*/*             IKTTPEND CALLS THE INTERNAL PROCEDURE TERMAS TO      */ 02768000
*/*             BRING DOWN THE ADDRESS SPACE (VIA SIC), THEN RETURNS */ 02769000
*/*             TO VTAM.                                             */ 02770000
*/*    INPUT                                                         */ 02771000
*/*          REG1 ==> 2 WORD PARAMETER LIST                          */ 02772000
*/*                   WORD 1) ACB ADDRESS                            */ 02773000
*/*                   WORD 2) VTAM REASON (ENTRY) CODE               */ 02774000
*/*                           0=HALT NORMAL                          */ 02775000
*/*                           4=HALT QUICK                           */ 02776000
*/*                           8=HALT CANCEL                          */ 02777000
*/*          REG14= VTAM RETURN ADDRESS                              */ 02778000
*/*     OUTPUT     NONE                                              */ 02779000
*/*     ENTRY                                                        */ 02780000
*/*           VIA BR 15 FROM VTAM                                    */ 02781000
*/*     EXIT                                                         */ 02782000
*/*           VIA BR 14 TO VTAM                                      */ 02783000
*/*                                                          @ZM21019*/ 02784000
*/********************************************************************/ 02785000
*                                                                  0769 02786000
*IKTTPEND:                                                         0769 02787000
*   ENTRY;                          /* TPEND EXIT FOR HALT COMMAND 0769 02788000
*                                                            @ZM21019*/ 02789000
@EP00769 DS    0H                                                  0769 02790000
@EC00769 DS    0H                                                  0770 02791000
*/********************************************************************/ 02792000
*/*                                                                  */ 02793000
*/* SEGMENT-                                                         */ 02794000
*/*      EPTLINK                                                     */ 02795000
*/*                                                                  */ 02796000
*/* FUNCTION-                                                        */ 02797000
*/*      ENTRY POINT LINKAGE                                         */ 02798000
*/*                                                          @ZM21019*/ 02799000
*/********************************************************************/ 02800000
*                                                                  0770 02801000
*   DO;                             /* ENTRY POINT LINKAGE   @ZM21019*/ 02802000
*     RFY                                                          0771 02803000
*      (REG1,                                                      0771 02804000
*       REG14) RSTD;                                               0771 02805000
*     GEN SETS(REG11,REG13);                                       0772 02806000
         LR    @04,@01             /* SAVE REGISTER 01      @ZM21019*/  02807000
         LR    @05,@14             /* SAVE REGISTER 14      @ZM21019*/  02808000
         L     @03,@SIZDATD                                             02809000
         GETMAIN R,LV=(3),SP=229   /* GET AUTOAREA          @ZM21019*/  02810000
         LR    REG11,@01           /* SET UP DATAREG        @ZM21019*/  02811000
         USING @DATD,REG11         /* ADDRESS AUTOAREA      @ZM21019*/  02812000
         LR    REG13,REG11         /* SET UP SAVEAREA ADDR  @ZM21019*/  02813000
         MODESET EXTKEY=ZERO       /* GET KEY 0             @ZM21019*/  02814000
         L     @10,PSATOLD         /* LOAD BASE ADDR OF TCB @ZM21019*/  02815000
         IC    @02,TCBPKF-TCB(,@10) /* SAVE OLD PROTECT KEY @ZM21019*/  02816000
         MVI   TCBPKF-TCB(@10),0   /* INSERT KEY ZERO       @ZM21019*/  02817000
         STC   @02,OLDKEY          /* SAVE OLD PROTECT KEY  @ZM21019*/  02818000
         LR    @01,@04             /* RESTORE REGISTER 01   @ZM21019*/  02819000
         LR    @14,@05             /* RESTORE REGISTER 14   @ZM21019*/  02820000
*     TSBPTR=ASCBTSB;               /* ADDRESS THE TSB       @ZM21019*/ 02821000
         L     @06,ASCBPTR                                         0773 02822000
         L     @06,ASCBTSB(,@06)                                   0773 02823000
         ST    @06,TSBPTR                                          0773 02824000
*     PARAMREG=REG1;                /* ADDRESS PARAMETER LIST      0774 02825000
*                                                            @ZM21019*/ 02826000
         LR    PARAMREG,REG1                                       0774 02827000
*     LSAVAREA=LSAVAREA&&LSAVAREA;  /* ZERO SAVE AREA        @ZM21019*/ 02828000
         XC    LSAVAREA(156),LSAVAREA                              0775 02829000
*     MAINEXIT=REG14;               /* SAVE RETURN ADDRESS   @ZM21019*/ 02830000
         ST    REG14,MAINEXIT                                      0776 02831000
*     RFY                                                          0777 02832000
*      (REG1,                                                      0777 02833000
*       REG14) UNRSTD;                                             0777 02834000
*   END;                            /* END OF SEGMENT        @ZM21019*/ 02835000
*   IF VTAMRSN=0 THEN               /* IF NORMAL HALT        @ZM21019*/ 02836000
         L     @14,VTAMRSN(,PARAMREG)                              0779 02837000
         LTR   @14,@14                                             0779 02838000
         BZ    @RT00779                                            0779 02839000
*     ;                             /* RETURN TO VTAM        @ZM21019*/ 02840000
*   ELSE                            /* IF NOT NORMAL HALT    @ZM21019*/ 02841000
*     DO;                           /* TERMINATE THE ADDRESS SPACE 0781 02842000
*                                      WITH 622 CODE         @ZM21019*/ 02843000
*       GEN REFS(PSALITA,FLC);                                     0782 02844000
*                                  /* TERMAS NEEDS LOCK     @ZM21019*/  02845000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=SAVE,             **02846000
               RELATED=('TPEND ENTERED')  /* GET THE LOCAL LOCK         02847000
*                                                           @ZM21019*/  02848000
*       CALL TERMAS;                /* TERMINATE THE ADDRESS SPACE 0783 02849000
*                                      WITH 622 CODE         @ZM21019*/ 02850000
         BAL   @14,TERMAS                                          0783 02851000
*     END;                          /* TERMINATE THE ADDRESS SPACE 0784 02852000
*                                      WITH 622 CODE         @ZM21019*/ 02853000
*/********************************************************************/ 02854000
*/*                                                                  */ 02855000
*/* SEGMENT-                                                         */ 02856000
*/*      XITLINK                                                     */ 02857000
*/* FUNCTION-                                                        */ 02858000
*/*      RETURN LINKAGE                                              */ 02859000
*/*                                                          @ZM21019*/ 02860000
*/********************************************************************/ 02861000
*                                                                  0785 02862000
*   DO;                             /* RESET KEY AND FREE AUTODATA 0785 02863000
*                                      AREA                  @ZM21019*/ 02864000
@RT00779 DS    0H                                                  0786 02865000
*     GENERATE REFS(PSALITA,FLC,OLDKEY,PSATOLD,TCBPKF)             0786 02866000
*         (SETLOCK RELEASE,TYPE=LOCAL,REGS=SAVE,RELATED=*);/* FREE 0786 02867000
*                                      THE LOCAL MEMORY LOCK @ZM21019*/ 02868000
         SETLOCK RELEASE,TYPE=LOCAL,REGS=SAVE,RELATED=*                 02869000
*     RFY                                                          0787 02870000
*       REG14 RSTD;                 /* RETURN REGISTER       @ZM21019*/ 02871000
*     REG14=MAINEXIT;               /* RESTORE RETURN REGISTER     0788 02872000
*                                                            @ZM21019*/ 02873000
         L     REG14,MAINEXIT                                      0788 02874000
*     GEN EXIT REFS(OLDKEY,PSATOLD,TCBPKF,REG11,REG14);            0789 02875000
*                                              /*           @ZM21019*/  02876000
         LR    @10,REG14           /* SAVE RETURN ADDRESS   @ZM21019*/  02877000
         IC    @02,OLDKEY          /* LOAD OLD TCB KEY      @ZM21019*/  02878000
         L     @15,PSATOLD         /* POINT TO THE TCB      @ZM21019*/  02879000
         STC   @02,TCBPKF-TCB(,@15) /* SET OLD PROTECT KEY  @ZM21019*/  02880000
         MODESET EXTKEY=VTAM       /* SET KEY 6             @ZM21019*/  02881000
         L     @03,@SIZDATD        /* GET SIZE OF AUTOAREA  @ZM21019*/  02882000
         LR    @01,REG11           /* GET ADDR OF AUTOAREA  @ZM21019*/  02883000
         FREEMAIN R,LV=(3),A=(1),SP=229 /* FREE AUTOAREA    @ZM21019*/  02884000
         LR    REG14,@10           /* SET RETURN ADDRESS    @ZM21019*/  02885000
         BR    REG14               /* AND RETURN            @ZM21019*/  02886000
*     RFY                                                          0790 02887000
*       REG14 UNRSTD;               /* RETURN REGISTER       @ZM21019*/ 02888000
*   END;                            /* END OF SEGMENT        @ZM21019*/ 02889000
*   END IKTLTERM                                                   0792 02890000
*                                                                  0792 02891000
