*CONROUT:                                                          0213 00609000
*   PROCEDURE OPTIONS(SAVE(14));                                   0213 00610000
         B     @PB00002                                            0213 00611000
CONROUT  ST    @14,@SA00002                                        0213 00612000
*   DECLARE                                                        0214 00613000
*     1 LSTRUC BDY(WORD) STATIC LOCAL,                             0214 00614000
*      2 DUMLIST(40) FIXED(15) BDY(HWORD) INIT(0,1,4,4,0,0,3,3,0,12,3,3 00615000
*            ,0,16,1,0,0,20,3,1,0,24,3,3,0,28,3,3,0,32,2,2,0,36,3,3,0,  00616000
*            40,3,3);               /* INITIALIZE THE CONVERSION   0214 00617000
*                                      TABLE: THE METHOD IS USE THE     00618000
*                                      FIRST 2 HALF WORDS AS THREASON   00619000
*                                      CODE (THIS IS OVERLAID BY THE    00620000
