         LA    @04,16                                           01S0732 02160000
         ALR   @04,@12                                          01S0732 02161000
         LA    @01,FRWAEXRG(,@12)                               01S0732 02162000
         ST    @01,INFOREGS(,@04)                               01S0732 02163000
*   INFOADDR=ADDR(IKTEXIT);         /* POINT TO EXIT ROUTINE @ZM20856*/ 02164000
