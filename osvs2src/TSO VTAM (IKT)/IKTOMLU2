         OI    TOMFMLO(@14),B'10000000'                         03S0300 00579000
*                           RPLSCHED='1'B;/* POST=SCHED              */ 00580000
*                           RPLEX='1'B;/* EXCEPTION REPONSE ONLY     */ 00581000
         OI    RPLSCHED(@14),B'10000100'                        03S0302 00582000
