         STM   REG0,REG15,FRWAEXRG(@12)                         01S0738 02181000
*   RFY                                                         01S0739 02182000
*     REG1 UNRSTD;                  /* FREE UP REG1          @ZM20856*/ 02183000
*   FRPOMNGR='1'B;                  /* INDICATE OUTPUT MANAGER TO FRR   02184000
*                                                            @ZM20856*/ 02185000
         OI    FRPOMNGR(@14),B'10000000'                        01S0740 02186000
*   FRPCONST=ADDR(CONSAREA);        /* SET CONSTANTS AREA       01S0741 02187000
*                                      ADDR@ZM20856                  */ 02188000
         LA    @12,CONSAREA                                     01S0741 02189000
         ST    @12,FRPCONST(,@14)                               01S0741 02190000
*   END INITFRR;                    /* END OF INITFRR        @ZM20856*/ 02191000
@EL00011 DS    0H                                               01S0742 02192000
@EF00011 DS    0H                                               01S0742 02193000
