         OI    TVWATOBB(TVWAPTR),B'00000100'                    03S0314 00612000
*                               TVWAINB='1'B;/* SESSION IS IN   03S0315 00613000
*                                      BRACKETS                      */ 00614000
         OI    TVWAINB(TVWAPTR),B'00000100'                     03S0315 00615000
*                             END;  /* END SESSION OUT OF BRACKETS   */ 00616000
*                           ELSE    /* SESSION IS IN BRACKETS        */ 00617000
*                             DO;                               03S0317 00618000
         B     @RC00311                                         03S0317 00619000
@RF00311 DS    0H                                               03S0318 00620000
*                               TVWATOBB='0'B;/* NOT BEGIN BRACKET      00621000
*                                                            @ZM20858*/ 00622000
         NI    TVWATOBB(TVWAPTR),B'11111011'                    03S0318 00623000
*                               RPLBB='0'B;/* TURN OFF BEGIN BRACKETS*/ 00624000
         NI    RPLBB+16(RPLPTR),B'01111111'                     03S0319 00625000
*                             END;                              03S0320 00626000
*                           IF TSBNOBRK='0'B&TOMEXBRU='0'B&TVWABRIN='0' 00627000
*                             THEN                              03S0321 00628000
