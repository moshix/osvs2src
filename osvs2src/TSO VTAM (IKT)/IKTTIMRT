         STM   REG0,REG15,SAVEREGS(@11)                            0192 00081000
* RFY (REG8,REG7) RSTD;                /* REG8 WILL HOLD R13  POINTER*/ 00082000
* REG8=REG13;                          /* ADDRESS OF TIM WORK AREA   */ 00083000
         LR    REG8,REG13                                          0194 00084000
* REG7=SRBLEN;                         /* LENGTH OF SRB TO FREE      */ 00085000
