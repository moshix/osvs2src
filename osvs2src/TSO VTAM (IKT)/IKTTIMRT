         TITLE '*** IKTTIMRT: INPUT MANAGER FOR THE 3767/3770 TERMINALS*00001000
               S ***    '                                               00002000
IKTTIMRT CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKTTIMRT  77.347'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @05,0                                               0001 00010000
@PSTART  LA    @06,4095(,@05)                                      0001 00011000
         USING @PSTART,@05                                         0001 00012000
         USING @PSTART+4095,@06                                    0001 00013000
         LR    WRKSPC,REG1         LOAD ADDR OF GETMAINED AREA          00014000
         USING @DATD,WRKSPC        SET UP ADDRESSABILITY                00015000
         A     REG1,C1000          BEGINNING PRIVATE WORK AREA          00016000
         ST    REG1,TIMGETMN       SET UP ADDRESSABILITY                00017000
*   TSBREG=ASCBTSB;                 /* SET UP TSB POINTER REG@ZA24528*/ 00018000
         L     @15,PSAAOLD                                         0170 00019000
         L     TSBREG,ASCBTSB(,@15)                                0170 00020000
*   RFY TSB BASED(TSBREG);          /* REBASE TSB ON REGISTER@ZA24528*/ 00021000
*   TVWAPTR=TSBXTVWA;               /* SET UP TVWA POINTER   @ZA24528*/ 00022000
         L     @15,TSBEXTNT(,TSBREG)                               0172 00023000
         L     TVWAPTR,TSBXTVWA(,@15)                              0172 00024000
*   RPLREG=TIMGETMN+LENGTH(TIMWORK); /* SET UP RPL POINTER   @ZA24528*/ 00025000
         L     @15,TIMGETMN                                        0173 00026000
         LA    RPLREG,632                                          0173 00027000
         ALR   RPLREG,@15                                          0173 00028000
*   RFY IFGRPL BASED(RPLREG);       /* BASE THE RPL ON REG.  @ZA24528*/ 00029000
*   RPLSAVE=RPLREG;                 /* SAVE THE RPL ADDRESS  @ZA24528*/ 00030000
         ST    RPLREG,RPLSAVE                                      0175 00031000
*   RFY WSAG   BASED(CVTSPSA);      /* USED IN GLOBAL FREEMAIN       */ 00032000
*   STM(REG0,REG15,SAVEREGS);       /* RECOVERY FOR FREEMAIN         */ 00033000
         STM   REG0,REG15,SAVEREGS(@15)                            0177 00034000
*   SRBRTN=REG14;                   /* SAVE ADDRESS OF RETURN POINT  */ 00035000
         ST    REG14,SRBRTN(,@15)                                  0178 00036000
*   RFY REG14 UNRSTD;               /* FREE REGISTER 14 FOR USE      */ 00037000
*    DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/     0180 00038000
*CKMC0002:                                                         0181 00039000
*      STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */ 0181 00040000
CKMC0002 L     @11,TVWAFRWI(,TVWAPTR)                              0181 00041000
         STM   REG0,REG15,FRWACKRG(@11)                            0181 00042000
*      FRWACKPT(2)=ADDR(CKMC0002); /* SAVE ADDRESS    @ZM20856 */  0182 00043000
         LA    @08,CKMC0002                                        0182 00044000
         ST    @08,FRWACKPT+4(,@11)                                0182 00045000
*    END;                                                          0183 00046000
*/********************************************************************/ 00047000
*/*     THE FOLLOWING CODE WILL BE EXECUTED UPON INITIAL ENTRY TO    */ 00048000
*/*     IKTTIMRT.  THE SRB WILL BE FREED AND THE RPL FOR THE VTAM    */ 00049000
*/*     RECEIVE MACRO WILL BE INITIALIZED.                           */ 00050000
*/********************************************************************/ 00051000
* REG7=ADDR(IKTIOFRR);              /* SET UP FRR FOR RECOVERY       */ 00052000
         L     @11,CVTPTR                                          0184 00053000
         L     @11,CVTTCASP(,@11)                                  0184 00054000
         L     REG7,TCASFRR(,@11)                                  0184 00055000
* GEN  REFS(FRRSCURR,FRRS,FRRSLAST,PSALSFCC,PSA,FRRSELEN,PSACSTK,  0185 00056000
*           FRRSFRRA,FRRSENTR,FRRSPARM,REG7)                       0185 00057000
*           SETS(REG1,REG2,REG3,FRRSCURR);                         0185 00058000
