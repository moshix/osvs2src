* PARMPTR=(REG3);                      /* ADDRESS OF PARM AREA       */ 00060000
         ST    REG3,PARMPTR                                        0186 00061000
* RFY (REG1,REG3,REG7) UNRSTD;         /* FREE REGS FOR COMPILER     */ 00062000
* CALL INITFRR;                        /* CALL FRR INIT      @ZM20856*/ 00063000
         BAL   @14,INITFRR                                         0188 00064000
* /*******************************************************************/ 00065000
* /* FIRST OBTAIN THE LOCAL LOCK TO DELETE THE SRB POINTER FROM THE  */ 00066000
* /* TSBX. THEN OBTAIN SALLOC LOCK AND DO FREEMAIN.                  */ 00067000
* /*******************************************************************/ 00068000
* REG2=REG0;                           /* SAVE SRB ADDRESS TO FREE  */  00069000
         LR    REG2,REG0                                           0189 00070000
* GEN(SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(SRB),REGS=USE)     00071000
*     REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);               0190 00072000
