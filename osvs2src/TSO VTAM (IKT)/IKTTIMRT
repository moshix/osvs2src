*           PSALITA,FLC) SETS(REG0,REG1,REG3,REG4,REG14,REG15);    0196 00088000
   SETLOCK  OBTAIN,TYPE=SALLOC,MODE=UNCOND,RELATED=(AL),REGS=USE        00089000
   FREEMAIN RC,LV=(REG7),A=(REG2),SP=239,BRANCH=(YES,GLOBAL)            00090000
   SETLOCK  RELEASE,TYPE=SALLOC,RELATED=(AL),REGS=USE                   00091000
* REG13=REG8;                          /* RESTORE ADDR OF TIM WORK   */ 00092000
         LR    REG13,REG8                                          0197 00093000
* LM(REG0,REG15,SAVEREGS);             /* RESTORE DESTROYED REGISTERS*/ 00094000
         L     @11,TIMGETMN                                        0198 00095000
         LM    REG0,REG15,SAVEREGS(@11)                            0198 00096000
*RESTARTA: CALL PRIMPROC;              /* CALL RTN TO SET UP RPL     */ 00097000
RESTARTA BAL   @14,PRIMPROC                                        0199 00098000
*RFY (REG0,REG1,REG2,REG8,REG7) UNRSTD; /* FREE REGISTERS            */ 00099000
         EJECT                                                          00100000
*/********************************************************************/ 00101000
*/*     THE FOLLOWING CODE CONTAINS THE INFINITE LOOP THAT PERFORMS  */ 00102000
*/*     THE BASIC FUNCTION OF IKTTIMRT. THAT FUNCTION IS TO ISSUE    */ 00103000
*/*     THE VTAM RECEIVE MACRO, DETERMINE WHAT THE INPUT IS WHEN     */ 00104000
*/*     THE RECEIVE IS SATISFIED, CALL THE APPROPPRIATE ROUTINE      */ 00105000
*/*     TO PROCESS THE INPUT AND LOOP BACK TO REISSUE THE RECEIVE.   */ 00106000
*/*     EACH TIME VTAM GAINS CONTROL THROUGH THE RECEIVE MACRO       */ 00107000
*/*     THE FRR THAT IKTTIMRT SETS UP IS DELETED AND MUST BE RESET.  */ 00108000
*/********************************************************************/ 00109000
*REC: RFY (REG0,REG1,REG2,REG3,REG7,REG15) RSTD; /*REGS FOR GEN      */ 00110000
REC      DS    0H                                                  0202 00111000
*                                     /*                     @ZM20858*/ 00112000
* DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/        0202 00113000
*CKMC0003:                                                         0203 00114000
*   STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */    0203 00115000
CKMC0003 L     @14,TVWAFRWI(,TVWAPTR)                              0203 00116000
         STM   REG0,REG15,FRWACKRG(@14)                            0203 00117000
*   FRWACKPT(2)=ADDR(CKMC0003); /* SAVE ADDRESS    @ZM20856 */     0204 00118000
         LA    @11,CKMC0003                                        0204 00119000
         ST    @11,FRWACKPT+4(,@14)                                0204 00120000
* END;                 /* CHECK POINT         @ZM20856*/           0205 00121000
*RPLSRTYP='02'X;                      /*  REC SYN AND ASYCHR DATA    */ 00122000
         LA    @14,16                                              0206 00123000
         ALR   @14,RPLREG                                          0206 00124000
         MVI   RPLSRTYP(@14),X'02'                                 0206 00125000
*IF TVWARCRS='1'B THEN                /* IF AUTO PROMPT MODE @ZA24528*/ 00126000
         TM    TVWARCRS(TVWAPTR),B'00000010'                       0207 00127000
         BNO   @RF00207                                            0207 00128000
* RPLRRESP='1'B;               /* ACCEPT EXCEPTION RESPONSES @ZA24528*/ 00129000
         OI    RPLRRESP(@14),B'00001000'                           0208 00130000
*ELSE;                                                   /*  @ZA24528*/ 00131000
@RF00207 DS    0H                                                  0210 00132000
*RPLCNTRL='000000'X;         /* TURN OFF ALL BITS IN RPLCNTRL@ZA24528*/ 00133000
         MVC   RPLCNTRL+16(3,RPLREG),@CB02532                      0210 00134000
*REG2=RPLREG;             /* ADDR OF RPL FOR RECEIVE FROM REG@ZA24528*/ 00135000
         LR    REG2,RPLREG                                         0211 00136000
*REG7=ADDR(IKTIOFRR);                  /* PTR TO FRR ENTRY           */ 00137000
         L     @14,CVTPTR                                          0212 00138000
         L     @14,CVTTCASP(,@14)                                  0212 00139000
         L     REG7,TCASFRR(,@14)                                  0212 00140000
*GEN(SETLOCK RELEASE,TYPE=LOCAL,RELATED=(SRB),REGS=USE)    /*@ZA24528*/ 00141000
*    REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);        /*@ZA24528*/ 00142000
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(SRB),REGS=USE              00143000
*GEN REFS(REG1,REG2,REG3,REG7,FRRSCURR,FRRS,FRRSLAST,PSALSFCC,PSA, 0214 00144000
*         FRRSELEN,PSACSTK,FRRSFRRA,FRRSENTR,FRRSPARM)             0214 00145000
*         SETS(REG1,REG2,REG3,REG15,REG0,REG14);                   0214 00146000
  RECEIVE  RPL=(REG2)                                                   00147000
  SETFRR A,FRRAD=(REG7),WRKREGS=(REG1,REG2),PARMAD=(REG3)               00148000
  STM REG14,REG1,12(REG13)  /*SAVE RTN CODES FROM RECEIVE   @ZM20858*/  00149000
*GEN(SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(SRB),REGS=USE) 0215 00150000
*    REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);        /*@ZA24528*/ 00151000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(SRB),REGS=USE   00152000
*GEN(LM REG14,REG1,12(REG13))                                      0216 00153000
*   REFS(REG13);                       /* RESTORE RETURN CODE REGS   */ 00154000
         LM REG14,REG1,12(REG13)                                        00155000
*                                      /* FROM THE RECEIVER  @ZM20858*/ 00156000
*PARMPTR=(REG3);                                                   0217 00157000
         ST    REG3,PARMPTR                                        0217 00158000
*RFY (REG1,REG2,REG3,REG7) UNRSTD;     /* FREE REGISTERS FOR USE     */ 00159000
*CALL INITFRR;                         /* CALL FRR INIT      @ZM20856*/ 00160000
         BAL   @14,INITFRR                                         0219 00161000
*IF TVWATOD='1'B THEN                  /* IS IKTTOMRT DEAD?  @ZM20856*/ 00162000
         TM    TVWATOD(TVWAPTR),B'10000000'                        0220 00163000
         BNO   @RF00220                                            0220 00164000
*  CALL FINPROC;                       /* EXIT PERMANENTLY   @ZM20586*/ 00165000
         BAL   @14,FINPROC                                         0221 00166000
*ELSE                                  /* IKTTOMRT IS ALIVE  @ZM20586*/ 00167000
*IF REG15 > 0 THEN                                                 0222 00168000
         B     @RC00220                                            0222 00169000
@RF00220 LTR   REG15,REG15                                         0222 00170000
         BNP   @RF00222                                            0222 00171000
*  DO;                                  /* NOT 0, ERROR PROCESS      */ 00172000
*    CALL VTAMERR;                      /* CALL ERROR PROCESS RTN    */ 00173000
         BAL   @14,VTAMERR                                         0224 00174000
*    IF REGRTN='1'B THEN                /* REFRESH RPL NEEDED ?      */ 00175000
         L     @14,TIMGETMN                                        0225 00176000
         TM    REGRTN(@14),B'00001000'                             0225 00177000
         BNO   @RF00225                                            0225 00178000
*      DO;                                                         0226 00179000
*        LM(REG0,REG15,SAVEREGS);       /* RESTORE REGISTERS         */ 00180000
         L     @14,TIMGETMN                                        0227 00181000
         LM    REG0,REG15,SAVEREGS(@14)                            0227 00182000
*        CALL PRIMPROC;                 /* REFRESH RPL               */ 00183000
         BAL   @14,PRIMPROC                                        0228 00184000
*        REGRTN='0'B;                   /* RESET FLAG                */ 00185000
         L     @14,TIMGETMN                                        0229 00186000
         NI    REGRTN(@14),B'11110111'                             0229 00187000
*      END;                             /* END REFRESH RPL DO PROC   */ 00188000
*    ELSE;                              /* NO REFRESH RPL, NO PROC   */ 00189000
@RF00225 DS    0H                                                  0232 00190000
*  END;                                 /* END REG15 > 0             */ 00191000
*ELSE                                   /* GOOD RECEIVE, PROCESS INPT*/ 00192000
*DO;                                                               0233 00193000
         B     @RC00222                                            0233 00194000
@RF00222 DS    0H                                                  0234 00195000
*RFY (REG0,REG15) UNRSTD;              /* FREE RTN CODE REGS @ZM20858*/ 00196000
*RECERCNT=0;                            /* RESET INPUT ERROR COUNT   */ 00197000
         L     @11,TIMGETMN                                        0235 00198000
         SLR   @08,@08                                             0235 00199000
         ST    @08,RECERCNT(,@11)                                  0235 00200000
*IF RPLDATA='1'B THEN                  /* CHECK FOR DATA RECEIVED    */ 00201000
         TM    RPLDATA+16(RPLREG),B'10000000'                      0236 00202000
         BNO   @RF00236                                            0236 00203000
*  CALL DATARTN;                       /* YES, PROCESS DATA          */ 00204000
         BAL   @14,DATARTN                                         0237 00205000
*ELSE                                                              0238 00206000
*  IF RPLCNCEL='1'B THEN               /* CHECK FOR CANCEL RECEIVED  */ 00207000
         B     @RC00236                                            0238 00208000
@RF00236 TM    RPLCNCEL+16(RPLREG),B'01000000'                     0238 00209000
         BNO   @RF00238                                            0238 00210000
*    CALL CANCELR;                     /* YES, PROCESS CANCEL        */ 00211000
         BAL   @14,CANCELR                                         0239 00212000
*  ELSE                                                            0240 00213000
*     IF RPLLUS='1'B THEN              /*  CHECK FOR LUSTAT RECEIVED */ 00214000
         B     @RC00238                                            0240 00215000
@RF00238 TM    RPLLUS+16(RPLREG),B'00100000'                       0240 00216000
         BNO   @RF00240                                            0240 00217000
*       CALL LUSTAT;                   /* YES, PROCESS LUSTAT        */ 00218000
         BAL   @14,LUSTAT                                          0241 00219000
*     ELSE                                                         0242 00220000
*       IF RPLSIGNL='1'B THEN          /* CHECK FOR ATTENTION SIGNAL */ 00221000
         B     @RC00240                                            0242 00222000
@RF00240 TM    RPLSIGNL+16(RPLREG),B'00010000'                     0242 00223000
         BNO   @RF00242                                            0242 00224000
*         DO;                          /* YES, ATTEN SIGNAL  @ZA26553*/ 00225000
*           RPLFMHDR='0'B;             /* RESET FORMAT HEADER@ZA26553*/ 00226000
         NI    RPLFMHDR(RPLREG),B'11111110'                        0244 00227000
*           IF TVWAOOPS='1'B THEN                                  0245 00228000
         TM    TVWAOOPS(TVWAPTR),B'00000001'                       0245 00229000
         BNO   @RF00245                                            0245 00230000
*             DO;                                                  0246 00231000
*               TVWAOOPS='0'B;         /* SET PAPER OUT SWITCH OFF   */ 00232000
         NI    TVWAOOPS(TVWAPTR),B'11111110'                       0247 00233000
*               CALL SCHTOM;           /* RESCHEDULE OUTPUT MGR      */ 00234000
         BAL   @14,SCHTOM                                          0248 00235000
*             END;                                                 0249 00236000
*           ELSE                       /* ATTN WITH NO OUT OF PAPER  */ 00237000
*             DO;                      /* CHECK FOR USER EXIT        */ 00238000
         B     @RC00245                                            0250 00239000
@RF00245 DS    0H                                                  0251 00240000
*               IF ADDR(IKTRTX3)^= 0 THEN                          0251 00241000
