@RF00251 BAL   @14,ATTN                                            0253 00251000
*             END;                     /* END OF USER EXIT PROCESS   */ 00252000
*         END;                         /*                    @ZA26553*/ 00253000
*       ELSE;                          /* END OF POSSIBLE INPUTS     */ 00254000
@RF00242 DS    0H                                                  0257 00255000
* END;                                 /* END OF GOOD RECEIVE PROCESS*/ 00256000
* GO TO REC;                           /* LOOP BACK TO ISSUE RECEIVE */ 00257000
         B     REC                                                 0258 00258000
         EJECT                                                          00259000
*/********************************************************************/ 00260000
*/*     THE FOLLOWING CODE BUILDS AND INITIALIZES THE RPL FOR THE    */ 00261000
*/*     VTAM RECEIVE MACRO AND WILL BE THE PRIMARY RESTARTING POINT  */ 00262000
*/*     FOR IKTTIMRT IN CASE OF FAILURE DUE TO AN ERROR CONDITON.    */ 00263000
*/********************************************************************/ 00264000
*PRIMPROC:                                                         0259 00265000
*  PROCEDURE;                                                      0259 00266000
PRIMPROC STM   @14,@12,12(@13)                                     0259 00267000
*  RFY ISTNIB  BASED(TVWANIB);         /* BASE THE NIB DSECT         */ 00268000
*  IFGRPL=DUMMYRPL;                    /* INITIALIZE RPL             */ 00269000
         MVC   IFGRPL(112,RPLREG),DUMMYRPL                         0261 00270000
*  RPLDACB=TVWAACB;                    /* PLACE ACB ADDR IN RPL      */ 00271000
         L     @11,TVWAACB(,TVWAPTR)                               0262 00272000
         ST    @11,RPLDACB(,RPLREG)                                0262 00273000
*  RPLARG=NIBCID;                      /* PLACE CID IN RPL           */ 00274000
         L     @11,TVWANIB(,TVWAPTR)                               0263 00275000
         L     @11,NIBCID(,@11)                                    0263 00276000
         ST    @11,RPLARG(,RPLREG)                                 0263 00277000
*  RPLAREA=ADDR(TIMBUFF);              /* ADDRESS OF INPUT BUFFER    */ 00278000
         L     @11,TIMGETMN                                        0264 00279000
         LA    @08,TIMBUFF(,@11)                                   0264 00280000
         ST    @08,RPLAREA(,RPLREG)                                0264 00281000
*  IPTRMTYP=TSBXTMTP;                  /* INIT TERMINAL TYPE         */ 00282000
         L     @08,TSBEXTNT(,TSBREG)                               0265 00283000
         IC    @07,TSBXTMTP(,@08)                                  0265 00284000
         STC   @07,IPTRMTYP(,@11)                                  0265 00285000
*RETURN;                                                           0266 00286000
@EL00002 DS    0H                                                  0266 00287000
@EF00002 DS    0H                                                  0266 00288000
@ER00002 LM    @14,@12,12(@13)                                     0266 00289000
         BR    @14                                                 0266 00290000
*END PRIMPROC;                                                     0267 00291000
         EJECT                                                          00292000
*/********************************************************************/ 00293000
*/*    THIS ROUTINE WILL CHECK TO SEE IF IKTTOMRT IS AVAILABLE TO    */ 00294000
*/*    BE SCHEDULED.  THE TVWATIS FLAG IS 1 WHEN IKTTOMRT IS EITHER  */ 00295000
*/*    RUNNINT OR SCHEDULED.  TVWATAS MEANS THAT ONLY IKTTIMRT CAN   */ 00296000
*/*    SCHEDULE IKTTOMRT.                                            */ 00297000
*/********************************************************************/ 00298000
*                                                                  0268 00299000
*SCHTOM: PROCEDURE;                                                0268 00300000
SCHTOM   STM   @14,@12,@SA00003                                    0268 00301000
*      TVWATAS='0'B;                   /* MARK TOM AVAILABLE @ZA26553*/ 00302000
         NI    TVWATAS(TVWAPTR),B'11011111'                        0269 00303000
*      IF TVWATIS='0'B THEN            /* IKTTOMRT NOT SCHEDULED     */ 00304000
         TM    TVWATIS(TVWAPTR),B'01000000'                        0270 00305000
         BNZ   @RF00270                                            0270 00306000
*        DO;                                                       0271 00307000
*          RFY REG2 RSTD;              /* FOR USE IN SCHEDULE        */ 00308000
*          REG2=TSBXSRB;               /* ADDRESS OF TOM SRB         */ 00309000
         L     @11,TSBEXTNT(,TSBREG)                               0273 00310000
         L     REG2,TSBXSRB(,@11)                                  0273 00311000
*          GEN(SCHEDULE SRB=(REG2),SCOPE=LOCAL)                    0274 00312000
*          REFS(TSBXSRB,CVTPTR,CVTLSMQ,CVTMAP,SRBFLNK,SRBSECT);    0274 00313000
         SCHEDULE SRB=(REG2),SCOPE=LOCAL                                00314000
*          RFY REG2 UNRSTD;             /* FREE REGISTER2            */ 00315000
*          TVWABFC='0'B;               /* SET BUFFER CONTENTION OFF  */ 00316000
         NI    TVWABFC(TVWAPTR),B'01111111'                        0276 00317000
*          TVWATIS='1'B;               /* SET TOM SCHEDULED ON       */ 00318000
         OI    TVWATIS(TVWAPTR),B'01000000'                        0277 00319000
*        END;                                                      0278 00320000
*      ELSE;                                                       0279 00321000
@RF00270 DS    0H                                                  0280 00322000
*  END SCHTOM;                                                     0280 00323000
@EL00003 DS    0H                                                  0280 00324000
@EF00003 DS    0H                                                  0280 00325000
@ER00003 LM    @14,@12,@SA00003                                    0280 00326000
         BR    @14                                                 0280 00327000
         EJECT                                                          00328000
*/********************************************************************/ 00329000
*/*    THIS ROUTINE WILL RECEIVE CONTROL WHEN THE RECEIVE MACRO      */ 00330000
*/*    IS SATISFIED WITH DATA. THE FIRST CHECK MADE IS AGAINST A     */ 00331000
*/*    GLOBAL ERROR FLAG THAT IS TURNED ON WHEN ERROR RECOVERY IS    */ 00332000
*/*    IN PROGRESS.                                                  */ 00333000
*/********************************************************************/ 00334000
*                                                                  0281 00335000
*DATARTN:                                                          0281 00336000
*  PROCEDURE;                                                      0281 00337000
DATARTN  STM   @14,@12,@SA00004                                    0281 00338000
*    IF TVWAGERR='0'B THEN                                         0282 00339000
         TM    TVWAGERR(TVWAPTR),B'00010000'                       0282 00340000
         BNZ   @RF00282                                            0282 00341000
*      DO;                                                         0283 00342000
*        IF RPLCMD='1'B THEN           /* CHANGE DIRECTION SPECIFIED */ 00343000
         TM    RPLCMD+16(RPLREG),B'00100000'                       0284 00344000
         BNO   @RF00284                                            0284 00345000
*          DO;                                                     0285 00346000
*            TIMSCHTM='1'B;            /* IKTTOMRT NEEDS SCHEDULING  */ 00347000
         L     @11,TIMGETMN                                        0286 00348000
         OI    TIMSCHTM(@11),B'01000000'                           0286 00349000
*            TVWABRIN='0'B;            /* BREAKIN COMPLETE           */ 00350000
         NI    TVWABRIN(TVWAPTR),B'01111111'                       0287 00351000
*            TVWASCD='1'B;             /* CD RECIEVED, SEND DATA     */ 00352000
         OI    TVWASCD(TVWAPTR),B'00100000'                        0288 00353000
*            IF RPLRLEN>0  &  TSBIFLSH='0'B THEN /* NO ADD NULL RU   */ 00354000
