         SLR   @04,REG3                                            0468 01470000
         STH   @04,XSAPRMSZ(,RBSECPTR)                             0468 01471000
*             XSABFRAD=XSABFRAD+REG3;/* UPDATE CALLER'S ADDRESS      */ 01472000
         ALR   @15,REG3                                            0469 01473000
         STCM  @15,7,XSABFRAD(RBSECPTR)                            0469 01474000
*             XSACURDS=XSACURDS+REG3;/* INCREMENT AMT OF DATA MOVED  */ 01475000
         LR    @04,REG3                                            0470 01476000
         AH    @04,XSACURDS(,RBSECPTR)                             0470 01477000
         STH   @04,XSACURDS(,RBSECPTR)                             0470 01478000
*           END;                                                   0471 01479000
*         RFY                                                      0472 01480000
*           IPSOURCE BASED(REG1);   /* REFERENCE DATA VIA REGISTER 0472 01481000
*                                                            @YM03231*/ 01482000
@RF00465 DS    0H                                                  0473 01483000
*         IF IPSOURCE(1)=SBA THEN   /* IS INVALID CHAR A START BUFFER   01484000
*                                      ADDRESS ?             @YM03231*/ 01485000
         CLI   IPSOURCE(REG1),X'11'                                0473 01486000
         BNE   @RF00473                                            0473 01487000
*           REG2=3;                 /* YES...PULL OUT SBA,@,@        */ 01488000
         LA    REG2,3                                              0474 01489000
*         ELSE                                                     0475 01490000
*           REG2=1;                 /* NO...JUST PULL OUT ONE CHAR   */ 01491000
         B     @RC00473                                            0475 01492000
@RF00473 LA    REG2,1                                              0475 01493000
*         RFY                                                      0476 01494000
*           IPSOURCE BASED(IPBUFADR);                              0476 01495000
@RC00473 DS    0H                                                  0477 01496000
*         IPBFSZ=IPBFSZ-REG3-REG2;  /* UPDATE QUEUE ELE SIZE TO AM'T    01497000
*                                      OF DATA MOVED PLUS THE INVALID   01498000
