         LR    @04,REG3                                            0486 01545000
         AH    @04,XSACURDS(,RBSECPTR)                             0486 01546000
         STH   @04,XSACURDS(,RBSECPTR)                             0486 01547000
*CONTIN3:                                                          0487 01548000
*                                                                  0487 01549000
*         /***********************************************************/ 01550000
*         /*                                                         */ 01551000
*         /* CONTINUE WITH TRT LOOP                                  */ 01552000
*         /*                                                         */ 01553000
*         /***********************************************************/ 01554000
*                                                                  0487 01555000
*         RFY                                                      0487 01556000
*           REG3 UNRSTD;                                           0487 01557000
CONTIN3  DS    0H                                                  0488 01558000
*       END;                        /* END EDIT'S DO WHILE           */ 01559000
@DE00457 SLR   @04,@04                                             0488 01560000
         CH    @04,XSAPRMSZ(,RBSECPTR)                             0488 01561000
         BE    @DC00457                                            0488 01562000
         CH    @04,IPBFSZ(,SAVEREG)                                0488 01563000
         BNE   @DL00457                                            0488 01564000
@DC00457 DS    0H                                                  0489 01565000
*                                                                  0489 01566000
*       /*************************************************************/ 01567000
*       /*                                                           */ 01568000
*       /* EITHER IPBFSZ OR XSAPRMSZ IS ZERO 1. IF XSAPRMSZ ^= 0 AND */ 01569000
*       /* THIS IS END OF INPUT(IPPRTL=0), THEN PAD REMAINING BUFFER */ 01570000
*       /* WITH BLANKS. 2. IF IPBFSZ ^= 0, THEN SCAN FOR ANOTHER     */ 01571000
*       /* VALID CHAR, ADJUSTING IPBFSZ AND IPBUFADR. THIS IS DONE SO*/ 01572000
*       /* THAT IF CALLER IS RETURNED A CODE '0C'HEX, INDICATING THAT*/ 01573000
*       /* MORE DATA FOLLOWS, THEN THERE SHOULD BE VALID DATA        */ 01574000
*       /*                                                           */ 01575000
*       /*************************************************************/ 01576000
*                                                                  0489 01577000
*       IF XSAPRMSZ^=0&IPPRTL='0'B THEN                            0489 01578000
         LH    @04,XSAPRMSZ(,RBSECPTR)                             0489 01579000
         LTR   @04,@04                                             0489 01580000
         BZ    @RF00489                                            0489 01581000
         TM    IPPRTL(SAVEREG),B'00100000'                         0489 01582000
         BNZ   @RF00489                                            0489 01583000
*         DO;                                                      0490 01584000
*                                                                  0490 01585000
*           /*********************************************************/ 01586000
*           /*                                                       */ 01587000
*           /* PAD INPUT AREA WITH BLANKS, BUT DON'T CHANGE XSAPRMSZ */ 01588000
*           /*                                                       */ 01589000
*           /*********************************************************/ 01590000
*                                                                  0491 01591000
*           RFY                                                    0491 01592000
*            (REG0,                                                0491 01593000
*             REG1,                                                0491 01594000
*             REG2,                                                0491 01595000
*             REG3) RSTD;                                          0491 01596000
*           REG0=XSABFRAD;          /* TARGET ADR FOR 'MVCL'         */ 01597000
         L     REG0,XSABFRAD-1(,RBSECPTR)                          0492 01598000
         ICM   REG0,8,@CB02616                                     0492 01599000
*           REG1=XSAPRMSZ;          /* AM'T OF AREA TO MOVE INTO REG2   01600000
*                                      NOT SET SINCE COUNT WILL BE 0493 01601000
*                                      ZERO, CAUSING PAD CHAR TO BE     01602000
*                                      USED                          */ 01603000
         LR    REG1,@04                                            0493 01604000
*           REG3=BLANK;             /* PADDING CHAR IS A BLANK       */ 01605000
         LA    REG3,64                                             0494 01606000
*           SLL(REG3,24);           /* PUT INTO PAD POSITION         */ 01607000
         SLL   REG3,24                                             0495 01608000
*           MVCL(REG0,REG2);        /* MOVE IN BLANKS & SET COUNT=0  */ 01609000
         MVCL  REG0,REG2                                           0496 01610000
*         END;                                                     0497 01611000
*       IF IPBFSZ^=0 THEN           /* MORE DATA IN QUEUE ELE ? YES,    01612000
*                                      VALIDATE IT                   */ 01613000
@RF00489 LH    @04,IPBFSZ(,SAVEREG)                                0498 01614000
         LTR   @04,@04                                             0498 01615000
         BZ    @RF00498                                            0498 01616000
*         DO UNTIL REG3^=0|IPBFSZ=0;                               0499 01617000
@DL00499 DS    0H                                                  0500 01618000
*           REG3=MIN(IPBFSZ,256);   /* 'TRT' UP TO 256 CHARS         */ 01619000
         LH    REG3,IPBFSZ(,SAVEREG)                               0500 01620000
         LA    @04,256                                             0500 01621000
         CR    REG3,@04                                            0500 01622000
         BNH   *+6                                                      01623000
         LR    REG3,@04                                            0500 01624000
*           REG1=0;                 /* CLEAR REGISTER BEFORE TRT     */ 01625000
         SLR   REG1,REG1                                           0501 01626000
*           TRT(IPSOURCE(1:REG3),TGETEDIT(1));/* SEE IF ANY VALID    */ 01627000
         L     @04,IPBUFADR(,SAVEREG)                              0502 01628000
         LR    @15,REG3                                            0502 01629000
         BCTR  @15,0                                               0502 01630000
         EX    @15,@SB02692                                        0502 01631000
*           BC(CC0,CONTIN4);        /* BRANCH IF THERE IS VALID DATA */ 01632000
         BC    8,CONTIN4                                           0503 01633000
*           REG3=REG1-IPBUFADR;     /* NO. OF VALID CHARS            */ 01634000
         L     @04,IPBUFADR(,SAVEREG)                              0504 01635000
         LR    REG3,REG1                                           0504 01636000
         SLR   REG3,@04                                            0504 01637000
*           IF REG3=0 THEN          /* ANY VALID AT ALL ?            */ 01638000
         LTR   REG3,REG3                                           0505 01639000
         BNZ   @RF00505                                            0505 01640000
*             DO;                   /* NO, KEEP SEARCHING            */ 01641000
*               IPBFSZ=IPBFSZ-1;    /* UPDATE QUEUE ELE SIZE         */ 01642000
         LH    @15,IPBFSZ(,SAVEREG)                                0507 01643000
         BCTR  @15,0                                               0507 01644000
         STH   @15,IPBFSZ(,SAVEREG)                                0507 01645000
*               IPBUFADR=IPBUFADR+1;/* UPDATE QUEUE ELE ADDRESS      */ 01646000
         AL    @04,@CF00050                                        0508 01647000
         ST    @04,IPBUFADR(,SAVEREG)                              0508 01648000
*             END;                                                 0509 01649000
*                                                                  0509 01650000
*           /*********************************************************/ 01651000
*           /*                                                       */ 01652000
*           /* FALL THRU TO CONTIN2.                                 */ 01653000
*           /*                                                       */ 01654000
*           /*********************************************************/ 01655000
*                                                                  0510 01656000
*CONTIN4:                                                          0510 01657000
*                                                                  0510 01658000
*           /*********************************************************/ 01659000
*           /*                                                       */ 01660000
*           /* REG3 IS 0 IF FIRST CHAR IN TRT WAS INVALID. OTHERWISE */ 01661000
*           /* IT IS THE NO. OF VALID CHARS IF BRANCH WASN'T TAKEN & */ 01662000
*           /* IT IS MIN(IPBFSZ,256) IF BRANCH WAS TAKEN. IF REG3 IS */ 01663000
*           /* 0 THEN THE DO UNTIL WILL CONTINUE SEARCHING FOR A     */ 01664000
*           /* VALID CHAR, UPDATING QUEUE ELE POINTERS (UNLESS IPBFSZ*/ 01665000
*           /* BECOMES 0, IN WHICH CASE THERE IS NO MORE DATA)       */ 01666000
*           /*                                                       */ 01667000
*           /*********************************************************/ 01668000
*                                                                  0510 01669000
*         END;                      /* END DO UNTIL                  */ 01670000
@RF00505 DS    0H                                                  0510 01671000
CONTIN4  DS    0H                                                  0510 01672000
@DE00499 SLR   @04,@04                                             0510 01673000
         CR    REG3,@04                                            0510 01674000
         BNE   @DC00499                                            0510 01675000
         CH    @04,IPBFSZ(,SAVEREG)                                0510 01676000
         BNE   @DL00499                                            0510 01677000
@DC00499 DS    0H                                                  0511 01678000
*       RFY                                                        0511 01679000
*        (REG0,                                                    0511 01680000
*         REG1,                                                    0511 01681000
*         REG2,                                                    0511 01682000
*         REG3) UNRSTD;                                            0511 01683000
@RF00498 DS    0H                                                  0512 01684000
*     END;                          /* END 'EDIT' OPTION PROCESSING  */ 01685000
*   RFY                                                            0513 01686000
*     RTNREG RSTD;                                                 0513 01687000
@RC00433 DS    0H                                                  0514 01688000
*   RTNREG=XSASAVEA;                /* RESTORE RETURN ADDRESS        */ 01689000
         L     RTNREG,XSASAVEA(,RBSECPTR)                          0514 01690000
*   RETURN;                                                        0515 01691000
@EL00005 DS    0H                                                  0515 01692000
@EF00005 DS    0H                                                  0515 01693000
@ER00005 BR    @14                                                 0515 01694000
*   RFY                                                            0516 01695000
*     RTNREG UNRSTD;                                               0516 01696000
