         SLR   REG3,@04                                            0464 01457000
*         IF REG3^=0 THEN           /* ANY DATA TO MOVE ?            */ 01458000
         LTR   REG3,REG3                                           0465 01459000
         BZ    @RF00465                                            0465 01460000
*           DO;                     /* YES                           */ 01461000
*             XSATARGT(1:REG3)=IPSOURCE(1:REG3);/* MOVE DATA         */ 01462000
         L     @15,XSABFRAD-1(,RBSECPTR)                           0467 01463000
         LA    @15,0(,@15)                                         0467 01464000
         LR    @14,REG3                                            0467 01465000
         BCTR  @14,0                                               0467 01466000
         EX    @14,@SM02698                                        0467 01467000
*             XSAPRMSZ=XSAPRMSZ-REG3;/* UPDATE CALLER'S SIZE         */ 01468000
