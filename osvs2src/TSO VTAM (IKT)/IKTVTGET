         AL    @04,IPBUFADR(,SAVEREG)                              0455 01404000
         ST    @04,IPBUFADR(,SAVEREG)                              0455 01405000
*       IPCCC=0;                    /* SET CONTROL CHARACTER COUNT   */ 01406000
         MVI   IPCCC(SAVEREG),X'00'                                0456 01407000
*                                                                  0457 01408000
*       /*************************************************************/ 01409000
*       /*                                                           */ 01410000
*       /* SCAN DATA VIA 'TRT' INSTRUCTION, MOVING VALID DATA AND    */ 01411000
*       /* POINTING PAST INVALID CHAR. CONTINUE UNTIL THERE IS NO    */ 01412000
*       /* MORE DATA TO MOVE OR NO ROOM TO MOVE IT TO.               */ 01413000
*       /*                                                           */ 01414000
*       /*************************************************************/ 01415000
*                                                                  0457 01416000
*       DO WHILE XSAPRMSZ^=0&IPBFSZ^=0;                            0457 01417000
         B     @DE00457                                            0457 01418000
@DL00457 DS    0H                                                  0458 01419000
*         RFY                                                      0458 01420000
*           REG3 RSTD;                                             0458 01421000
*         REG3=MIN(IPBFSZ,XSAPRMSZ,256);/* NO MORE THAN 256 CHARS CAN   01422000
*                                      BE TESTED AT ONCE.            */ 01423000
         LH    REG3,IPBFSZ(,SAVEREG)                               0459 01424000
         LH    @04,XSAPRMSZ(,RBSECPTR)                             0459 01425000
         CR    REG3,@04                                            0459 01426000
