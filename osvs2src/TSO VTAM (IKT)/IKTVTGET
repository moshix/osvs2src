         TITLE '*** IKTVTGET: TSO/VTAM TGET ROUTINE ***                *00001000
                        '                                               00002000
IKTVTGET CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKTVTGET  78.013'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
RBPRFXLN EQU 64                                                         00013000
RBPRFLNA EQU 32                                                         00014000
PRBLEN   EQU 136                                                        00015000
SIRBLEN  EQU 200                                                        00016000
TIRBLEN  EQU 136                                                        00017000
IRBLEN   EQU 128                                                        00018000
SVRBLEN  EQU 224                                                        00019000
*                                                                  0120 00020000
*/********************************************************************/ 00021000
*/*                                                                  */ 00022000
*/*  START OF VTAM TGET. ENTERED FROM IGC0009C WITH LOCAL LOCK HELD  */ 00023000
*/*                                                                  */ 00024000
*/********************************************************************/ 00025000
*                                                                  0120 00026000
*   XSATCBA=TCBPTR;                 /* SAVE TCB ADDRESS              */ 00027000
         STCM  TCBPTR,7,XSATCBA(RBSECPTR)                          0120 00028000
*   RFY                                                            0121 00029000
*     TCBPTR UNRSTD;                /* FREE-UP REGISTER              */ 00030000
*   RFY                                                            0122 00031000
*     TCB BASED(XSATCBA);                                          0122 00032000
*   TVWAPTR=TSBXTVWA;               /* SET-UP ADDRESSABILITY TO TVWA */ 00033000
         L     @04,TSBEXTNT(,TSBPTR)                               0123 00034000
         L     TVWAPTR,TSBXTVWA(,@04)                              0123 00035000
*   SAVEREG=TVWALLWA;               /* SET R13 TO LOCALLY-LOCKED SAVE   00036000
*                                      AREA                          */ 00037000
         L     SAVEREG,TVWALLWA(,TVWAPTR)                          0124 00038000
*   XSADATSZ=0;                     /* INITIALIZE DATA SIZE TO 0     */ 00039000
         SLR   @04,@04                                             0125 00040000
         STH   @04,XSADATSZ(,RBSECPTR)                             0125 00041000
*   XSADATVF='0'B;                  /* INITIALIZE TO 'DATA NOT     0126 00042000
*                                      VERIFIED'                     */ 00043000
         NI    XSADATVF(RBSECPTR),B'11110111'                      0126 00044000
*                                                                  0127 00045000
*/********************************************************************/ 00046000
*/*                                                                  */ 00047000
*/*     FOLLOWING DO WILL NORMALLY BE EXECUTED ONCE. IT WILL ITERATE */ 00048000
*/*     IF THERE IS NO DATA AVAILABLE & CALLER REQUESTED 'NOWAIT'.   */ 00049000
*/*     (AFTER BEING SUSPENDED/REDISPATCHED WHEN DATA IS AVAILABLE)  */ 00050000
*/*     IT WILL ALSO ITERATE IF THE QUEUE ELEMENT OBTAINED INDICATES */ 00051000
*/*     'PARTIAL LINE'. I.E. REMAINDER OF INPUT LINE IS IN A FOLLOW- */ 00052000
*/*     ING QUEUE ELEMENT.                                           */ 00053000
*/*                                                                  */ 00054000
*/********************************************************************/ 00055000
*                                                                  0127 00056000
*   DO UNTIL XSADOSWT='0'B;         /* BIG DO TO PREVENT GOTO'S      */ 00057000
@DL00127 DS    0H                                                  0128 00058000
*     XSADOSWT='0'B;                /* INIT. SWITCH TO OFF FOR EXIT  */ 00059000
         NI    XSADOSWT(RBSECPTR),B'11101111'                      0128 00060000
*     CALL TERMSTAT;                /* CHECK TERMINAL STATUS STATUS     00061000
*                                      RETURNED IN XSARC             */ 00062000
         BAL   @14,TERMSTAT                                        0129 00063000
*     IF XSARC^=CONTINUE THEN                                      0130 00064000
         CLC   XSARC(1,RBSECPTR),CONTINUE                          0130 00065000
         BNE   @RT00130                                            0130 00066000
*       ;                           /* IF TERMINAL SESSION CANCELLED    00067000
*                                      OR ATTENTION HIT, RETURN      */ 00068000
*     ELSE                                                         0132 00069000
*       DO;                                                        0132 00070000
*         IPREQ=OBTAIN;             /* REQUEST FOR DATA              */ 00071000
         MVI   IPREQ(SAVEREG),X'02'                                0133 00072000
*/*                                                                  */ 00073000
*/*  *****************************************************************/ 00074000
*/*                                                                  */ 00075000
*/*       CALL INPUT QUEUE MANAGER, IKTQMIN.                         */ 00076000
*/*       PARAMETER LIST INTERFACE IS BUILT DIRECTLY                 */ 00077000
*/*       SO THAT PLS WILL NOT GENERATE ANY 'TEMPS'                  */ 00078000
*/*       ITSELF. DIRECTLY BUILT PARAMETER LIST IS IN                */ 00079000
*/*       LOCALLY LOCKED WORK AREA.                                  */ 00080000
*/*                                                                  */ 00081000
*/*  *****************************************************************/ 00082000
*/*                                                                 **/ 00083000
*                                                                  0134 00084000
*         PRMLSTAD=ADDR(IPARMS);    /* PUT ADDR INTO 'TEMP'          */ 00085000
         LA    @04,IPARMS(,SAVEREG)                                0134 00086000
         ST    @04,PRMLSTAD(,SAVEREG)                              0134 00087000
*         RFY                                                      0135 00088000
*           PRMREG1 RSTD;                                          0135 00089000
*         PRMREG1=ADDR(PRMLSTAD);   /* REG 1 POINTS TO PLS PARM LST  */ 00090000
         LA    PRMREG1,PRMLSTAD(,SAVEREG)                          0136 00091000
*         CALL IKTQMIN;             /* CALL INPUT QUEUE MANAGER      */ 00092000
         L     @04,CVTPTR                                          0137 00093000
         L     @04,TCASTPTR(,@04)                                  0137 00094000
         L     @15,TCASIQM(,@04)                                   0137 00095000
         BALR  @14,@15                                             0137 00096000
*         RFY                                                      0138 00097000
*           PRMREG1 UNRSTD;                                        0138 00098000
*         IF IPRC=UNSUCCES THEN     /* ANY DATA AVAILABLE ?          */ 00099000
         CLI   IPRC(SAVEREG),3                                     0139 00100000
         BNE   @RF00139                                            0139 00101000
*           DO;                     /* NO DATA                       */ 00102000
*             IF TSBXTMTP^=T3767 THEN/* NOT A 3767 TERMINAL? @ZM20749*/ 00103000
         L     @04,TSBEXTNT(,TSBPTR)                               0141 00104000
         CLI   TSBXTMTP(@04),2                                     0141 00105000
         BE    @RF00141                                            0141 00106000
*               DO;                 /* NOT A 3767 TERMINAL   @ZM20749*/ 00107000
*                 IF TVWAPGN='1'B|  /* IS PAGING ON?         @YM03259*/ 00108000
*                     TVWAKBDL='1'B|/* KEYBOARD LOCKED?      @YM03259*/ 00109000
*                     TVWAATTN>0|   /* ATTENTION?            @ZM20749*/ 00110000
*                     TVWAAIGN='1'B /* ATTENTION IGNORED?    @ZM20749*/ 00111000
*                   THEN            /* PAGING OR KEYBOARD LOCKED   0143 00112000
*                                                            @YM03259*/ 00113000
         TM    TVWAPGN(TVWAPTR),B'01000000'                        0143 00114000
         BO    @RT00143                                            0143 00115000
         TM    TVWAKBDL(TVWAPTR),B'00100000'                       0143 00116000
         BO    @RT00143                                            0143 00117000
         CLI   TVWAATTN(TVWAPTR),0                                 0143 00118000
         BH    @RT00143                                            0143 00119000
         TM    TVWAAIGN(TVWAPTR),B'00100000'                       0143 00120000
         BNO   @RF00143                                            0143 00121000
@RT00143 DS    0H                                                  0144 00122000
*                   TVWAULK='1'B;   /* REQUEST TO UNLOCK KEYBOARD  0144 00123000
*                                                            @YM03259*/ 00124000
         OI    TVWAULK(TVWAPTR),B'00000100'                        0144 00125000
*                 ELSE                                             0145 00126000
*                   ;               /* NO PAGING-KEYBOARD          0145 00127000
*                                      UNLOCKED@YM03259              */ 00128000
@RF00143 DS    0H                                                  0146 00129000
*                 IF TVWATIS='0'B&  /* IS TOM NOT SCHEDULED? @YM03259*/ 00130000
*                     TVWATAS='0'B& /* AVAILABLE AND         @YM03259*/ 00131000
*                     (TVWAKBDL='1'B|/* IS THE KEYBOARD LOCKED?    0146 00132000
*                                                            @ZM20749*/ 00133000
*                     TVWAATTN>0|   /* ATTENTION?            @ZM20749*/ 00134000
*                     TVWAAIGN='1'B)/* ATTENTION IGNORED?    @ZM20749*/ 00135000
*                   THEN            /* YES                           */ 00136000
         TM    TVWATIS(TVWAPTR),B'01100000'                        0146 00137000
         BNZ   @RF00146                                            0146 00138000
         TM    TVWAKBDL(TVWAPTR),B'00100000'                       0146 00139000
         BO    @RT00146                                            0146 00140000
         CLI   TVWAATTN(TVWAPTR),0                                 0146 00141000
         BH    @RT00146                                            0146 00142000
         TM    TVWAAIGN(TVWAPTR),B'00100000'                       0146 00143000
         BNO   @RF00146                                            0146 00144000
@RT00146 DS    0H                                                  0147 00145000
*                   DO;             /* SET UP SCHEDULING OF TOM      */ 00146000
*                     TVWATIS='1'B; /* INDICATE TOM SCHEDULED      0148 00147000
*                                      SCHEDULE TOM                  */ 00148000
         OI    TVWATIS(TVWAPTR),B'01000000'                        0148 00149000
*                     RFY                                          0149 00150000
*                       WORK1REG RSTD;                             0149 00151000
*                     WORK1REG=TSBXSRB;/* GET ADDRESS OF SRB         */ 00152000
         L     @04,TSBEXTNT(,TSBPTR)                               0150 00153000
         L     WORK1REG,TSBXSRB(,@04)                              0150 00154000
*                     GEN(SCHEDULE SRB=(WORK1REG),SCOPE=LOCAL)     0151 00155000
*                     REFS(TSBXSRB,CVTPTR,CVTLSMQ,CVTMAP,SRBFLNK,  0151 00156000
*                         SRBSECT);                                0151 00157000
         SCHEDULE SRB=(WORK1REG),SCOPE=LOCAL                            00158000
*                     RFY                                          0152 00159000
*                       WORK1REG UNRSTD;                           0152 00160000
*                   END;            /* TOM HAS BEEN SCHEDULED        */ 00161000
*                 ELSE                                             0154 00162000
*                   ;               /* NO                            */ 00163000
@RF00146 DS    0H                                                  0155 00164000
*               END;                /*                       @ZM20749*/ 00165000
*             ELSE                                                 0156 00166000
*               ;                   /* A 3767 TERMINAL       @ZM20749   00167000
*                                      IF TGET NO WAIT, THEN RETURN     00168000
*                                      TO CALLER IMMEDIATELY         */ 00169000
@RF00141 DS    0H                                                  0157 00170000
*             IF XSANOWT='1'B THEN  /* DID CALLER SAY 'NOWAIT' ?     */ 00171000
         TM    XSANOWT(RBSECPTR),B'00010000'                       0157 00172000
         BNO   @RF00157                                            0157 00173000
*               DO;                 /* YES,                          */ 00174000
*                 XSARC=NOWAIT;     /* SET RETURN CODE TO NODATA     */ 00175000
         MVI   XSARC(RBSECPTR),X'04'                               0159 00176000
*               END;                                               0160 00177000
*                                                                  0160 00178000
*             /*******************************************************/ 00179000
*             /*                                                     */ 00180000
*             /* NO DATA AVAILABLE AND CALLER SPECIFIED TGET WAIT.   */ 00181000
*             /* SUSPEND CALLER UNTIL DATA IS AVAILABLE OR ATTENTION */ 00182000
*             /* HIT                                                 */ 00183000
*             /*                                                     */ 00184000
*             /*******************************************************/ 00185000
*                                                                  0161 00186000
*             ELSE                                                 0161 00187000
*               DO;                                                0161 00188000
         B     @RC00157                                            0161 00189000
@RF00157 DS    0H                                                  0162 00190000
*                 XSADOSWT='1'B;    /* SET SWITCH TO CONTINUE LOOP 0162 00191000
*                                      AFTER DATA BECOMES AVAILABLE  */ 00192000
         OI    XSADOSWT(RBSECPTR),B'00010000'                      0162 00193000
*                 RFY                                              0163 00194000
*                  (PRMREG1,                                       0163 00195000
*                   REG2) RSTD;                                    0163 00196000
*                                                                  0164 00197000
*                 /***************************************************/ 00198000
*                 /*                                                 */ 00199000
*                 /* SET 'TPUT' OR 'TGET' INDICATOR FOR SRM ACCORDING*/ 00200000
*                 /* TO WHETHER THE TERMINAL OUTPUT MANAGER WILL BE  */ 00201000
*                 /* ACTIVE BEFORE TERMINAL DATA IS ENTERED. THIS    */ 00202000
*                 /* AFFECTS THE SWAP OUT/SWAP IN OF THE ADDRESS     */ 00203000
*                 /* SPACE                                           */ 00204000
*                 /*                                                 */ 00205000
*                 /***************************************************/ 00206000
*                                                                  0164 00207000
*                 IF TSBXTMTP=T3767 /* 3767?                 @YM03601*/ 00208000
*                   THEN            /* 3767                  @YM03601*/ 00209000
         L     @04,TSBEXTNT(,TSBPTR)                               0164 00210000
         CLI   TSBXTMTP(@04),2                                     0164 00211000
         BNE   @RF00164                                            0164 00212000
*                   IF TSBSPIT='0'B&TSBAUTON='1'B&TSBSTAUT='1'B/*  0165 00213000
*                                      PROMPT?               @YM03601*/ 00214000
*                     THEN          /* PROMPT IN EFFECT      @YM03601*/ 00215000
         TM    TSBSPIT(TSBPTR),B'00100000'                         0165 00216000
         BNZ   @RF00165                                            0165 00217000
         TM    TSBAUTON(TSBPTR),B'01000100'                        0165 00218000
         BNO   @RF00165                                            0165 00219000
*                     TVWAPRMT='1'B;/* PROMPT IN EFFECT      @YM03601*/ 00220000
         OI    TVWAPRMT(TVWAPTR),B'00000010'                       0166 00221000
*                   ELSE            /* NO PROMPT             @YM03601*/ 00222000
*                     DO;           /* RESET SWITCHES        @YM03601*/ 00223000
         B     @RC00165                                            0167 00224000
@RF00165 DS    0H                                                  0168 00225000
*                       TVWAPRMT='0'B;/* NO PROMPT           @YM03601*/ 00226000
*                       TVWAP1ST='0'B;/* RESET 1ST-PROMPT    @YM03601*/ 00227000
         NI    TVWAPRMT(TVWAPTR),B'11111100'                       0169 00228000
*                     END;          /* END OF RESET          @YM03601*/ 00229000
*                 ELSE                                             0171 00230000
*                   ;               /* 3270                  @YM03601*/ 00231000
@RF00164 DS    0H                                                  0172 00232000
*                 PRMREG1=1;        /* NO SYSEVENT           @YM03259*/ 00233000
@RC00164 LA    PRMREG1,1                                           0172 00234000
*                 IF TVWATIS='0'B   /* IS TOM SCHEDULED?     @YM03259*/ 00235000
*                   THEN            /* TOM NOT SCHEDULED     @YM03259   00236000
*                                                            @ZA12766   00237000
*                                                            @ZA12766*/ 00238000
         TM    TVWATIS(TVWAPTR),B'01000000'                        0173 00239000
         BNZ   @RF00173                                            0173 00240000
*                   IF TVWAKBDL='0'B/* KEYBOARD UNLOCKED?    @YM03259*/ 00241000
*                     THEN          /* KEYBOARD UNLOCKED     @YM03259*/ 00242000
         TM    TVWAKBDL(TVWAPTR),B'00100000'                       0174 00243000
         BNZ   @RF00174                                            0174 00244000
*                     PRMREG1=INPTWAIT;/* INPUT WAIT         @YM03259*/ 00245000
         SLR   PRMREG1,PRMREG1                                     0175 00246000
*                   ELSE                                           0176 00247000
*                     ;             /* KEYBOARD LOCKED       @YM03259   00248000
*                                                            @ZA12766*/ 00249000
@RF00174 DS    0H                                                  0177 00250000
*                 ELSE              /* TOM SCHEDULED         @YM03259*/ 00251000
*                   IF TVWAKBDL='0'B/* KEYBOARD LOCKED?      @YM03259*/ 00252000
*                     THEN          /* KEYBOARD UNLOCKED     @YM03259*/ 00253000
         B     @RC00173                                            0177 00254000
@RF00173 TM    TVWAKBDL(TVWAPTR),B'00100000'                       0177 00255000
         BNZ   @RF00177                                            0177 00256000
*                     IF TVWATIR='1'B/* IS TOM RUNNING?      @YM03259*/ 00257000
*                       THEN        /* TOM IS RUNNING        @YM03259*/ 00258000
         TM    TVWATIR(TVWAPTR),B'00001000'                        0178 00259000
         BNO   @RF00178                                            0178 00260000
*                       IF TSBXTMTP=IDS|TSBXTMTP=TNDS/*      @G58AK3A*/ 00261000
*                         THEN      /* 3270 OR NDS           @YM03601*/ 00262000
         L     @04,TSBEXTNT(,TSBPTR)                               0179 00263000
         CLI   TSBXTMTP(@04),1                                     0179 00264000
         BE    @RT00179                                            0179 00265000
         CLI   TSBXTMTP(@04),4                                     0179 00266000
         BNE   @RF00179                                            0179 00267000
@RT00179 DS    0H                                                  0180 00268000
*                         PRMREG1=INPTWAIT;/* INPUT WAIT     @YM03601*/ 00269000
         SLR   PRMREG1,PRMREG1                                     0180 00270000
*                       ELSE        /* 3767 OR USER          @YM03601*/ 00271000
*                         IF TVWADOOQ='1'B/* QUEUE EMPTY?    @YM03601*/ 00272000
*                           THEN    /* QUEUE EMPTY           @YM03601*/ 00273000
         B     @RC00179                                            0181 00274000
@RF00179 TM    TVWADOOQ(TVWAPTR),B'01000000'                       0181 00275000
         BNO   @RF00181                                            0181 00276000
*                           PRMREG1=OTPTWAIT;/* OUTPUT WAIT  @YM03601*/ 00277000
         L     PRMREG1,OTPTWAIT                                    0182 00278000
*                         ELSE      /* DATA ON QUEUE         @YM03601*/ 00279000
*                           IF TVWAPRMT='1'B&TVWAP1ST='0'B/* @YM03601*/ 00280000
*                             THEN  /* FIRST PROMPT          @YM03601*/ 00281000
         B     @RC00181                                            0183 00282000
@RF00181 TM    TVWAPRMT(TVWAPTR),B'00000010'                       0183 00283000
         BNO   @RF00183                                            0183 00284000
         TM    TVWAP1ST(TVWAPTR),B'00000001'                       0183 00285000
         BNZ   @RF00183                                            0183 00286000
*                             DO;   /*                       @YM03601*/ 00287000
*                               TVWAP1ST='1'B;/* NOT 1ST PROMPT    0185 00288000
*                                                            @YM03601*/ 00289000
         OI    TVWAP1ST(TVWAPTR),B'00000001'                       0185 00290000
*                               PRMREG1=OTPTWAIT;/* OUTPUT WAIT    0186 00291000
*                                                            @YM03601*/ 00292000
         L     PRMREG1,OTPTWAIT                                    0186 00293000
*                             END;  /*                       @YM03601*/ 00294000
*                           ELSE    /*                       @YM03601*/ 00295000
*                             PRMREG1=INPTWAIT;/* INPUT WAIT @YM03601*/ 00296000
         B     @RC00183                                            0188 00297000
@RF00183 SLR   PRMREG1,PRMREG1                                     0188 00298000
*                     ELSE                                         0189 00299000
*                       ;           /* TOM IS NOT RUNNING    @YM03259*/ 00300000
@RF00178 DS    0H                                                  0190 00301000
*                   ELSE                                           0190 00302000
*                     ;             /* KEYBOARD IS LOCKED    @YM03259*/ 00303000
@RF00177 DS    0H                                                  0191 00304000
*                 IF PRMREG1=INPTWAIT THEN/* IWAIT REQUESTED @ZA24418*/ 00305000
@RC00177 DS    0H                                                  0191 00306000
@RC00173 LTR   PRMREG1,PRMREG1                                     0191 00307000
         BNZ   @RF00191                                            0191 00308000
*                   DO;             /*                       @ZA24418*/ 00309000
*                     ;             /* DELETED TESTING FOR TVWAIOP 0193 00310000
*                                                            @ZA28881*/ 00311000
*                     MPSTSOIW='1'B;/* TELL VTAM IWAIT WAS ISSUED  0194 00312000
*                                                            @ZA28881*/ 00313000
         L     @04,ASCBPTR                                         0194 00314000
         L     @04,ASXBPTR(,@04)                                   0194 00315000
         L     @04,ASXBMPST(,@04)                                  0194 00316000
         OI    MPSTSOIW(@04),B'10000000'                           0194 00317000
*                     TVWAISYS='1'B;/* SET IWAIT INDICATOR   @ZA28881*/ 00318000
         OI    TVWAISYS(TVWAPTR),B'10000000'                       0195 00319000
*                   END;            /*                       @ZA24418*/ 00320000
*                 ELSE                                             0197 00321000
*                   ;               /*                       @ZA24418*/ 00322000
@RF00191 DS    0H                                                  0198 00323000
*                 IF PRMREG1^=1     /* ISSUE SYSEVENT?       @YM03259*/ 00324000
*                   THEN            /* ISSUE SYSEVENT        @YM03259*/ 00325000
         C     PRMREG1,@CF00050                                    0198 00326000
         BE    @RF00198                                            0198 00327000
*                   DO;             /* SET UP SYSEVENT               */ 00328000
*                     REG2=ASCBASID;/* GET THIS ADDR. SPCS ASID      */ 00329000
         L     @04,ASCBPTR                                         0200 00330000
         LH    REG2,ASCBASID(,@04)                                 0200 00331000
         N     REG2,@CF02686                                       0200 00332000
*                     GEN REFS(ASCBASID,CVTPTR,CVTOPTE,CVT) SETS(  0201 00333000
*                         PRMREG0,RTNREG,ENTRYREG);                0201 00334000
***                                                                     00335000
***         INFORM SYSTEM RESOURCE MANAGER OF WAIT                      00336000
***                                                                     00337000
            SYSEVENT TERMWAIT,ASID=(REG2),ENTRY=BRANCH                  00338000
*                     RFY                                          0202 00339000
*                      (PRMREG1,                                   0202 00340000
*                       REG2) UNRSTD;                              0202 00341000
*                   END;            /* END OF SYSEVENT               */ 00342000
*                 ELSE                                             0204 00343000
*                   ;               /* DO NOT ISSUE SYSEVENT         */ 00344000
@RF00198 DS    0H                                                  0205 00345000
*                 TSBIWAIT='1'B;    /* INDICATE IWAIT CONDITION      */ 00346000
         OI    TSBIWAIT(TSBPTR),B'01000000'                        0205 00347000
*                                                                  0206 00348000
*                 /***************************************************/ 00349000
*                 /*                                                 */ 00350000
*                 /* PREPARE TO SUSPEND CALLER VIA BRANCH TO THE     */ 00351000
*                 /* STATUS ROUTINE. LOCAL LOCK HELD ON ENTRY TO     */ 00352000
*                 /* STATUS ROUTINE.                                 */ 00353000
*                 /*                                                 */ 00354000
*                 /***************************************************/ 00355000
*                                                                  0206 00356000
*                 STM(PRMREG0,ENTRYREG,TCBGRS);/* SAVE REGISTERS     */ 00357000
         L     @04,XSATCBA-1(,RBSECPTR)                            0206 00358000
         LA    @04,0(,@04)                                         0206 00359000
         STM   PRMREG0,ENTRYREG,TCBGRS(@04)                        0206 00360000
*                 RBOPSW2=ADDR(REDISPCH);/* SET UP RESUME PSW TELL 0207 00361000
*                                      STATUS TO RE-DISPATCH TASK AT    00362000
*                                      LABEL 'REDISPCH'              */ 00363000
         LA    @15,REDISPCH                                        0207 00364000
         ST    @15,RBOPSW2(,RBSECPTR)                              0207 00365000
*                 RFY                                              0208 00366000
*                  (PRMREG0,                                       0208 00367000
*                   PRMREG1) RSTD;                                 0208 00368000
*                 SAVEREG=NDISPBIT; /* SET NON-DISPATCHABLE FLAG     */ 00369000
         LA    SAVEREG,2048                                        0209 00370000
*                 PRMREG0=SDTCB;    /* SET ENTRY CODE                */ 00371000
         LA    PRMREG0,15                                          0210 00372000
*                 PRMREG1=XSATCBA;  /* SET TCB TO SUSPEND            */ 00373000
         LR    PRMREG1,@04                                         0211 00374000
*                 CALL STATUS;      /* CALL STATUS TO SUSPEND TASK   */ 00375000
         L     @04,CVTPTR                                          0212 00376000
         L     @04,SCVTSPTR(,@04)                                  0212 00377000
         L     @15,SCVTSTAT(,@04)                                  0212 00378000
         BALR  @14,@15                                             0212 00379000
*                 RFY                                              0213 00380000
*                  (PRMREG0,                                       0213 00381000
*                   PRMREG1) UNRSTD;                               0213 00382000
*                                                                  0213 00383000
*                 /***************************************************/ 00384000
*                 /*                                                 */ 00385000
*                 /* UPON BEING RE-DISPATCHED, LOCAL LOCK MUST BE    */ 00386000
*                 /* RE-OBTAINED                                     */ 00387000
*                 /*                                                 */ 00388000
*                 /***************************************************/ 00389000
*                                                                  0214 00390000
*REDISPCH:                                                         0214 00391000
*                 RFY                                              0214 00392000
*                  (RTNREG,                                        0214 00393000
*                   ENTRYREG,                                      0214 00394000
*                   REG0,                                          0214 00395000
*                   REG1) RSTD;                                    0214 00396000
REDISPCH DS    0H                                                  0215 00397000
*                 GEN SETS(RTNREG,ENTRYREG,REG0,REG1) REFS(PSALITA,FLC) 00398000
*                     ;                                            0215 00399000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,              --00400000
               RELATED=(TSB,IKTVTGET(RETURN))                           00401000
*                 RFY                                              0216 00402000
*                  (RTNREG,                                        0216 00403000
*                   ENTRYREG,                                      0216 00404000
*                   REG0,                                          0216 00405000
*                   REG1) UNRSTD;                                  0216 00406000
*               END;                /* END OF NO DATA AVAILABLE,   0217 00407000
*                                      SUSPEND CALLER                */ 00408000
*           END;                    /* END OF NO DATA AVAILABLE      */ 00409000
*/*                                                                  */ 00410000
*/*  *****************************************************************/ 00411000
*/*                                                                  */ 00412000
*/*            DATA IS AVAILABLE. EDIT DATA AND MOVE INTO            */ 00413000
*/*            CALLER'S BUFFER. EDIT ROUTINES UPDATE XSAPRMSZ,       */ 00414000
*/*            XSABFRAD, IPBFSZ,IPBUFADR                             */ 00415000
*/*                                                                  */ 00416000
*/*  *****************************************************************/ 00417000
*                                                                  0219 00418000
*         ELSE                                                     0219 00419000
*           DO;                                                    0219 00420000
         B     @RC00139                                            0219 00421000
@RF00139 DS    0H                                                  0220 00422000
*             IF XSADATVF='0'B THEN /* IF NOT DONE BEFORE, THEN GO 0220 00423000
