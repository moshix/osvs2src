*   RFY                                                            0305 00752000
*     RTNREG UNRSTD;                                               0305 00753000
*   RFY                                                            0306 00754000
*     PRMREG1 UNRSTD;                                              0306 00755000
*TERMSTAT:                                                         0307 00756000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0307 00757000
TERMSTAT DS    0H                                                  0308 00758000
*                                                                  0308 00759000
*/*                                                                  */ 00760000
*/********************************************************************/ 00761000
*/*            TERMSTAT IS AN INTERNAL PROCEDURE CALLED TO DETERMINE */ 00762000
*/*            THE STATUS OF THE TERMINAL.                           */ 00763000
*/*            IT SETS XSARC ON RETURN:                              */ 00764000
*/*             = 0 - NORMAL                                         */ 00765000
*/*             = 14 HEX - TERMINAL SESSION HAS BEEN CANCELLED       */ 00766000
*/*             =  8 - ATTENTION WAS HIT                             */ 00767000
*/*                                                                  */ 00768000
*/********************************************************************/ 00769000
*/*                                                                  */ 00770000
*                                                                  0308 00771000
*   RFY                                                            0308 00772000
*     RTNREG RSTD;                                                 0308 00773000
*   XSASAVEA=RTNREG;                /* SAVE RETURN ADDRESS           */ 00774000
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0309 00775000
*   RFY                                                            0310 00776000
*     RTNREG UNRSTD;                                               0310 00777000
*   IF TSBCANC='1'B THEN            /* TEST TERMINAL CANCELLED       */ 00778000
         TM    TSBCANC(TSBPTR),B'00000100'                         0311 00779000
         BNO   @RF00311                                            0311 00780000
*     XSARC=TERMCNCL;               /* IF YES, SET RC = CANCELLED    */ 00781000
         MVI   XSARC(RBSECPTR),X'14'                               0312 00782000
*   ELSE                                                           0313 00783000
*     IF TCBTIOTG='1'B THEN         /* TEST ATTN HIT                 */ 00784000
         B     @RC00311                                            0313 00785000
@RF00311 L     @04,XSATCBA-1(,RBSECPTR)                            0313 00786000
         TM    TCBTIOTG(@04),B'00010000'                           0313 00787000
         BNO   @RF00313                                            0313 00788000
*       XSARC=ATTEN;                /* IF YES, SET RC TO ATTEN       */ 00789000
         MVI   XSARC(RBSECPTR),X'08'                               0314 00790000
*     ELSE                                                         0315 00791000
*       XSARC=SUCCESS;              /* OTHERWISE, SET RC SUCCESSFUL  */ 00792000
         B     @RC00313                                            0315 00793000
@RF00313 IC    @04,SUCCESS                                         0315 00794000
         STC   @04,XSARC(,RBSECPTR)                                0315 00795000
*   RFY                                                            0316 00796000
*     RTNREG RSTD;                                                 0316 00797000
@RC00313 DS    0H                                                  0316 00798000
@RC00311 DS    0H                                                  0317 00799000
*   RTNREG=XSASAVEA;                /* RESTORE RETURN ADDRESS        */ 00800000
         L     RTNREG,XSASAVEA(,RBSECPTR)                          0317 00801000
*   RETURN;                                                        0318 00802000
@EL00002 DS    0H                                                  0318 00803000
@EF00002 DS    0H                                                  0318 00804000
@ER00002 BR    @14                                                 0318 00805000
*   RFY                                                            0319 00806000
*     RTNREG UNRSTD;                                               0319 00807000
*   END TERMSTAT;                                                  0320 00808000
*VERIFY:                                                           0321 00809000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0321 00810000
*                                                                  0321 00811000
VERIFY   DS    0H                                                  0322 00812000
*/*                                                                  */ 00813000
*/********************************************************************/ 00814000
*/*                                                                  */ 00815000
*/*      THIS TESTS IF CALLER'S DATA ADDRESS IS IN THE CALLER'S      */ 00816000
*/*      KEY. IT DOES SO BY REFERENCING THE FIRST BYTE OF EACH       */ 00817000
*/*      PAGE REQUIRED FOR THE TPUT.                                 */ 00818000
*/*                                                                  */ 00819000
*/********************************************************************/ 00820000
*/*                                                                  */ 00821000
*                                                                  0322 00822000
*   DCL                                                            0322 00823000
*     APAGE FIXED(15) CONSTANT(4096);/* PAGE SIZE                    */ 00824000
*   DCL                                                            0323 00825000
*     MODULUS FIXED(8) CONSTANT(12);/* MODULUS OF A VS2 PAGE IS    0323 00826000
*                                      2**12=4096                    */ 00827000
*   RFY                                                            0324 00828000
*     RTNREG RSTD;                                                 0324 00829000
*   XSASAVEA=RTNREG;                /* SAVE RETURN ADDRESS           */ 00830000
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0325 00831000
*   RFY                                                            0326 00832000
*     RTNREG UNRSTD;                                               0326 00833000
*   XSADATVF='1'B;                  /* SET INDICATOR TO 'VERIFIED'   */ 00834000
*   XSADMOVE='1'B;                  /* INDICATE 'DATA VERIFICATION 0328 00835000
*                                      UNDERWAY'                     */ 00836000
         OI    XSADATVF(RBSECPTR),B'01001000'                      0328 00837000
*/*    GET INTO CALLER'S KEY TO VALIDATE DATA                        */ 00838000
*                                                                  0329 00839000
*   RFY                                                            0329 00840000
*     REG2 RSTD;                                                   0329 00841000
*   REG2=RBLINKB->RBOPSWB2;         /* OBTAIN CALLERS KEY FROM RB    */ 00842000
         L     @04,RBLINKB-1(,RBSECPTR)                            0330 00843000
         SLR   REG2,REG2                                           0330 00844000
         IC    REG2,RBOPSWB2(,@04)                                 0330 00845000
*   GEN SETS(REG2);                                                0331 00846000
        MODESET KEYADDR=(2),WORKREG=2   GET IN TASK S KEY               00847000
*   RFY                                                            0332 00848000
*     REG2 UNRSTD;                                                 0332 00849000
*   RFY                                                            0333 00850000
*     WORK1REG RSTD;                                               0333 00851000
*   WORK1REG=XSABFRAD;              /* GET INPUT BUFFER ADDRESS      */ 00852000
         L     WORK1REG,XSABFRAD-1(,RBSECPTR)                      0334 00853000
         LA    WORK1REG,0(,WORK1REG)                               0334 00854000
*   SRL(WORK1REG,MODULUS);          /* SHIFT OFF PARTIAL PAGE        */ 00855000
         SRL   WORK1REG,12                                         0335 00856000
*   SLL(WORK1REG,MODULUS);          /* SHIFT IN ZEROS                */ 00857000
         SLL   WORK1REG,12                                         0336 00858000
*/*                                                                  */ 00859000
*/*       LOOP THRU EACH PAGE, MOVING FIRST BYTE TO ITSELF           */ 00860000
*/*                                                                  */ 00861000
*                                                                  0337 00862000
*   RFY                                                            0337 00863000
*     WORK2REG RSTD;                                               0337 00864000
*   WORK2REG=XSABFRAD+XSAPRMSZ;     /* DEFINE UPPER LIMIT            */ 00865000
         L     WORK2REG,XSABFRAD-1(,RBSECPTR)                      0338 00866000
         LA    WORK2REG,0(,WORK2REG)                               0338 00867000
         AH    WORK2REG,XSAPRMSZ(,RBSECPTR)                        0338 00868000
*                                                                  0339 00869000
*/*  IF PROGRAM CHECK OCCURS DURING BELOW DO WHILE, THEN IKT93EST    */ 00870000
*/*  WILL CHANGE THE ABEND CODE TO '15D'X                            */ 00871000
*                                                                  0339 00872000
*   DO WHILE WORK1REG<WORK2REG;                                    0339 00873000
         B     @DE00339                                            0339 00874000
@DL00339 DS    0H                                                  0340 00875000
*     GEN REFS(WORK1REG)(MVC 0(1,WORK1REG),0(WORK1REG));/* REFERENCE    00876000
*                                      PAGE                          */ 00877000
         MVC 0(1,WORK1REG),0(WORK1REG)                                  00878000
*     WORK1REG=WORK1REG+APAGE;      /* INCREMENT BY PAGE SIZE        */ 00879000
         AL    WORK1REG,@CF02642                                   0341 00880000
*   END;                                                           0342 00881000
@DE00339 CR    WORK1REG,WORK2REG                                   0342 00882000
         BL    @DL00339                                            0342 00883000
*   RFY                                                            0343 00884000
*    (WORK1REG,                                                    0343 00885000
*     WORK2REG) UNRSTD;                                            0343 00886000
*                                                                  0343 00887000
*/*                                                                  */ 00888000
*/*         GET BACK INTO KEY 0                                      */ 00889000
*/*                                                                  */ 00890000
*                                                                  0344 00891000
*   GEN(MODESET EXTKEY=ZERO);                                      0344 00892000
         MODESET EXTKEY=ZERO                                            00893000
*   XSADMOVE='0'B;                  /* CLEAR INDICATOR THAT DATA   0345 00894000
*                                      VERIFICATION WAS GOING ON     */ 00895000
         NI    XSADMOVE(RBSECPTR),B'10111111'                      0345 00896000
*   RFY                                                            0346 00897000
*     RTNREG RSTD;                                                 0346 00898000
*   RTNREG=XSASAVEA;                                               0347 00899000
         L     RTNREG,XSASAVEA(,RBSECPTR)                          0347 00900000
*   RETURN;                                                        0348 00901000
@EL00003 DS    0H                                                  0348 00902000
@EF00003 DS    0H                                                  0348 00903000
@ER00003 BR    @14                                                 0348 00904000
*   RFY                                                            0349 00905000
*     RTNREG UNRSTD;                                               0349 00906000
*   END VERIFY;                                                    0350 00907000
*EDIT3767:                                                         0351 00908000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0351 00909000
EDIT3767 DS    0H                                                  0352 00910000
*                                                                  0352 00911000
*/*                                                                  */ 00912000
*/********************************************************************/ 00913000
*/*                                                                  */ 00914000
*/*        EDIT3767 IS THE STANDARD TSO/VTAM EDIT ROUTINE FOR        */ 00915000
*/*        THE 3767 AND 3770 TERMINALS. IT CHECKS FOR A TGET EDIT    */ 00916000
*/*        REQUEST AND SCANS FOR INVALID DATA IF SO. IT MOVES THE    */ 00917000
*/*        DATA FROM THE INPUT QUEUE TO THE CALLER'S DATA AREA       */ 00918000
*/*        AND PADS WITH BLANKS IF EDIT OPTION.                      */ 00919000
*/*                                                                  */ 00920000
*/*        INPUT NEEDED---XSABFRAD - CALLER' DATA ADDRESS            */ 00921000
*/*                       XSAPRMSZ - CALLER'S DATA AREA SIZE         */ 00922000
*/*                       IPBUFADR - QUEUE ELEMENT ADDRESS           */ 00923000
*/*                       IPBFSZ   - QUEUE ELEMENT SIZE              */ 00924000
*/*                       XSAEDITO - OPTION IS EDIT IF='00'          */ 00925000
*/*                                                                  */ 00926000
*/*        OUTPUT-----EDIT3767 UPDATES THE DATA ADDRESSES AND        */ 00927000
*/*                   AREA SIZES LISTED ABOVE.                       */ 00928000
*/*                                                                  */ 00929000
*/********************************************************************/ 00930000
*/*                                                                  */ 00931000
*                                                                  0352 00932000
*   RFY                                                            0352 00933000
*     RTNREG RSTD;                                                 0352 00934000
*   XSASAVEA=RTNREG;                /* SAVE RETURN ADDRESS           */ 00935000
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0353 00936000
*   RFY                                                            0354 00937000
*     RTNREG UNRSTD;                                               0354 00938000
*   XSACURDS=0;                     /* CLEAR DATA MOVED COUNTER      */ 00939000
         SLR   @04,@04                                             0355 00940000
         STH   @04,XSACURDS(,RBSECPTR)                             0355 00941000
*   IF XSAEDITO^='00'B THEN         /* 00 IS EDIT OPTION             */ 00942000
         TM    XSAEDITO(RBSECPTR),B'00000011'                      0356 00943000
         BZ    @RF00356                                            0356 00944000
*/*                                                                  */ 00945000
*/*       TGET ASIS. IF EITHER COUNT IS ZERO ON ENTRY,               */ 00946000
*/*       NOTHING TO MOVE.                                           */ 00947000
*/*                                                                  */ 00948000
*                                                                  0357 00949000
*     IF XSAPRMSZ^=0&IPBFSZ^=0 THEN                                0357 00950000
         LH    @15,XSAPRMSZ(,RBSECPTR)                             0357 00951000
         CR    @15,@04                                             0357 00952000
         BE    @RF00357                                            0357 00953000
         LH    @14,IPBFSZ(,SAVEREG)                                0357 00954000
         CR    @14,@04                                             0357 00955000
         BE    @RF00357                                            0357 00956000
*       DO;                                                        0358 00957000
*         RFY                                                      0359 00958000
*           REG1 RSTD;                                             0359 00959000
*         REG1=MIN(XSAPRMSZ,IPBFSZ);/* GET MINIMUM FOR MOVE          */ 00960000
         LR    REG1,@14                                            0360 00961000
         CR    REG1,@15                                            0360 00962000
         BNH   *+6                                                      00963000
         LR    REG1,@15                                            0360 00964000
*         XSATARGT(1:REG1)=IPSOURCE(1:REG1);/* MOVE DATA TO CALLERS     00965000
*                                      DATA AREA. 3767 HAS A MAX   0361 00966000
*                                      IPBFSZ=256                    */ 00967000
         L     @04,XSABFRAD-1(,RBSECPTR)                           0361 00968000
         LA    @04,0(,@04)                                         0361 00969000
         LR    @11,REG1                                            0361 00970000
         BCTR  @11,0                                               0361 00971000
         L     @10,IPBUFADR(,SAVEREG)                              0361 00972000
         EX    @11,@SM02690                                        0361 00973000
*         XSAPRMSZ=XSAPRMSZ-REG1;   /* UPDATE CALLER'S SIZE          */ 00974000
         SLR   @15,REG1                                            0362 00975000
         STH   @15,XSAPRMSZ(,RBSECPTR)                             0362 00976000
*         XSABFRAD=XSABFRAD+REG1;   /* UPDATE CALLER'S ADDRESS       */ 00977000
         ALR   @04,REG1                                            0363 00978000
         STCM  @04,7,XSABFRAD(RBSECPTR)                            0363 00979000
*         IPBFSZ=IPBFSZ-REG1;       /* UPDATE QUEUE ELE SIZE         */ 00980000
         SLR   @14,REG1                                            0364 00981000
         STH   @14,IPBFSZ(,SAVEREG)                                0364 00982000
*         IPBUFADR=IPBUFADR+REG1;   /* UPDATE QUEUE ELE ADDRESS      */ 00983000
         ALR   @10,REG1                                            0365 00984000
         ST    @10,IPBUFADR(,SAVEREG)                              0365 00985000
*         XSACURDS=REG1;            /* SET AMOUNT OF DATA MOVED      */ 00986000
         STH   REG1,XSACURDS(,RBSECPTR)                            0366 00987000
*         RFY                                                      0367 00988000
*           REG1 UNRSTD;                                           0367 00989000
*       END;                        /* END 'TGET ASIS' MOVE          */ 00990000
*     ELSE                                                         0369 00991000
*       ;                           /* IF EITHER=0, NO DATA MOVE     */ 00992000
@RF00357 DS    0H                                                  0370 00993000
*   ELSE                            /* 'TGET EDIT' DATA MOVE         */ 00994000
*     DO;                                                          0370 00995000
*                                                                  0370 00996000
         B     @RC00356                                            0370 00997000
@RF00356 DS    0H                                                  0371 00998000
*       /*************************************************************/ 00999000
*       /*                                                           */ 01000000
*       /* SCAN DATA VIA 'TRT' INSTRUCTION, MOVING VALID DATA AND    */ 01001000
*       /* POINTING PAST INVALID CHAR. CONTINUE UNTIL THERE IS NO    */ 01002000
*       /* MORE DATA TO MOVE OR NO ROOM TO MOVE IT TO.               */ 01003000
*       /*                                                           */ 01004000
*       /*************************************************************/ 01005000
*                                                                  0371 01006000
*       DO WHILE XSAPRMSZ^=0&IPBFSZ^=0;                            0371 01007000
         B     @DE00371                                            0371 01008000
@DL00371 DS    0H                                                  0372 01009000
*         RFY                                                      0372 01010000
*           REG3 RSTD;                                             0372 01011000
*         REG3=MIN(IPBFSZ,XSAPRMSZ,256);/* NO MORE THAN 256 CHARS CAN   01012000
*                                      BE TESTED AT ONCE.            */ 01013000
         LH    REG3,IPBFSZ(,SAVEREG)                               0373 01014000
         LH    @04,XSAPRMSZ(,RBSECPTR)                             0373 01015000
         CR    REG3,@04                                            0373 01016000
         BNH   *+6                                                      01017000
         LR    REG3,@04                                            0373 01018000
         LA    @04,256                                             0373 01019000
         CR    REG3,@04                                            0373 01020000
         BNH   *+6                                                      01021000
         LR    REG3,@04                                            0373 01022000
*         RFY                                                      0374 01023000
*          (REG1,                                                  0374 01024000
*           REG2) RSTD;                                            0374 01025000
*         REG1=0;                   /* CLEAR REGISTER BEFORE TRT     */ 01026000
         SLR   REG1,REG1                                           0375 01027000
*         TRT(IPSOURCE(1:REG3),TGETEDIT(1));/* SEARCH FOR INVALID  0376 01028000
*                                      CHAR                          */ 01029000
         L     @04,IPBUFADR(,SAVEREG)                              0376 01030000
         LR    @15,REG3                                            0376 01031000
         BCTR  @15,0                                               0376 01032000
         EX    @15,@SB02692                                        0376 01033000
*         BC(CC0,DATAVALD);         /* BRCH IF ALL DATA VALID        */ 01034000
         BC    8,DATAVALD                                          0377 01035000
*                                                                  0378 01036000
*         /***********************************************************/ 01037000
*         /*                                                         */ 01038000
*         /* NOT ALL DATA IS VALID..REG1 POINTS TO FIRST INVALID CHAR*/ 01039000
*         /*                                                         */ 01040000
*         /***********************************************************/ 01041000
*                                                                  0378 01042000
*         REG3=REG1-IPBUFADR;       /* COMPUTE NO. OF VALID CHARS  0378 01043000
*                                      MOVED.                        */ 01044000
         L     @04,IPBUFADR(,SAVEREG)                              0378 01045000
         LR    REG3,REG1                                           0378 01046000
         SLR   REG3,@04                                            0378 01047000
*         RFY                                                      0379 01048000
*          (REG1,                                                  0379 01049000
*           REG2) UNRSTD;                                          0379 01050000
*         IF REG3^=0 THEN           /* ANY DATA TO MOVE ?            */ 01051000
         LTR   REG3,REG3                                           0380 01052000
         BZ    @RF00380                                            0380 01053000
*           DO;                     /* YES                           */ 01054000
*             XSATARGT(1:REG3)=IPSOURCE(1:REG3);/* MOVE DATA         */ 01055000
         L     @02,XSABFRAD-1(,RBSECPTR)                           0382 01056000
         LA    @02,0(,@02)                                         0382 01057000
         LR    @15,REG3                                            0382 01058000
         BCTR  @15,0                                               0382 01059000
         EX    @15,@SM02694                                        0382 01060000
*             XSAPRMSZ=XSAPRMSZ-REG3;/* UPDATE CALLER'S SIZE         */ 01061000
         LH    @04,XSAPRMSZ(,RBSECPTR)                             0383 01062000
         SLR   @04,REG3                                            0383 01063000
         STH   @04,XSAPRMSZ(,RBSECPTR)                             0383 01064000
*             XSABFRAD=XSABFRAD+REG3;/* UPDATE CALLER'S ADDRESS      */ 01065000
         ALR   @02,REG3                                            0384 01066000
         STCM  @02,7,XSABFRAD(RBSECPTR)                            0384 01067000
*             XSACURDS=XSACURDS+REG3;/* INCREMENT AMT OF DATA MOVED  */ 01068000
         LR    @04,REG3                                            0385 01069000
         AH    @04,XSACURDS(,RBSECPTR)                             0385 01070000
         STH   @04,XSACURDS(,RBSECPTR)                             0385 01071000
*           END;                                                   0386 01072000
*         IPBFSZ=IPBFSZ-REG3-1;     /* UPDATE QUEUE ELE SIZE TO AM'T    01073000
*                                      OF DATA MOVED PLUS THE INVALID   01074000
*                                      CHAR THAT IS DELETED.         */ 01075000
@RF00380 LH    @04,IPBFSZ(,SAVEREG)                                0387 01076000
         SLR   @04,REG3                                            0387 01077000
         BCTR  @04,0                                               0387 01078000
         STH   @04,IPBFSZ(,SAVEREG)                                0387 01079000
*         IPBUFADR=IPBUFADR+REG3+1; /* UPDATE QUEUE ELE ADDRESS TOO  */ 01080000
         LR    @04,REG3                                            0388 01081000
         AL    @04,IPBUFADR(,SAVEREG)                              0388 01082000
         AL    @04,@CF00050                                        0388 01083000
         ST    @04,IPBUFADR(,SAVEREG)                              0388 01084000
*         GO TO CONTIN1;            /* GO CONTINUE SEARCHING QUEUE 0389 01085000
*                                      ELE DATA                      */ 01086000
         B     CONTIN1                                             0389 01087000
*                                                                  0390 01088000
*         /***********************************************************/ 01089000
*         /*                                                         */ 01090000
*         /* ALL DATA SCANNED IN 'TRT' WAS VALID                     */ 01091000
*         /*                                                         */ 01092000
*         /***********************************************************/ 01093000
*                                                                  0390 01094000
*DATAVALD:                                                         0390 01095000
*         XSATARGT(1:REG3)=IPSOURCE(1:REG3);/* MOVE DATA             */ 01096000
DATAVALD L     @04,XSABFRAD-1(,RBSECPTR)                           0390 01097000
         LA    @04,0(,@04)                                         0390 01098000
         LR    @02,REG3                                            0390 01099000
         BCTR  @02,0                                               0390 01100000
         L     @15,IPBUFADR(,SAVEREG)                              0390 01101000
         EX    @02,@SM02696                                        0390 01102000
*         XSAPRMSZ=XSAPRMSZ-REG3;   /* UPDATE CALLER'S DATA SIZE     */ 01103000
         LH    @02,XSAPRMSZ(,RBSECPTR)                             0391 01104000
         SLR   @02,REG3                                            0391 01105000
         STH   @02,XSAPRMSZ(,RBSECPTR)                             0391 01106000
*         XSABFRAD=XSABFRAD+REG3;   /* UPDATE CALLER'S ADDRESS       */ 01107000
         ALR   @04,REG3                                            0392 01108000
         STCM  @04,7,XSABFRAD(RBSECPTR)                            0392 01109000
*         IPBFSZ=IPBFSZ-REG3;       /* UPDATE QUEUE ELE SIZE         */ 01110000
         LH    @04,IPBFSZ(,SAVEREG)                                0393 01111000
         SLR   @04,REG3                                            0393 01112000
         STH   @04,IPBFSZ(,SAVEREG)                                0393 01113000
*         IPBUFADR=IPBUFADR+REG3;   /* UPDATE QUEUE ELE ADDRESS      */ 01114000
         ALR   @15,REG3                                            0394 01115000
         ST    @15,IPBUFADR(,SAVEREG)                              0394 01116000
*         XSACURDS=XSACURDS+REG3;   /* INCREMENT AMT OF DATA MOVED   */ 01117000
         LR    @04,REG3                                            0395 01118000
         AH    @04,XSACURDS(,RBSECPTR)                             0395 01119000
         STH   @04,XSACURDS(,RBSECPTR)                             0395 01120000
*CONTIN1:                                                          0396 01121000
*                                                                  0396 01122000
*         /***********************************************************/ 01123000
*         /*                                                         */ 01124000
*         /* CONTINUE WITH TRT LOOP                                  */ 01125000
*         /*                                                         */ 01126000
*         /***********************************************************/ 01127000
*                                                                  0396 01128000
*         RFY                                                      0396 01129000
*           REG3 UNRSTD;                                           0396 01130000
CONTIN1  DS    0H                                                  0397 01131000
*       END;                        /* END EDIT'S DO WHILE           */ 01132000
@DE00371 SLR   @04,@04                                             0397 01133000
         CH    @04,XSAPRMSZ(,RBSECPTR)                             0397 01134000
         BE    @DC00371                                            0397 01135000
         CH    @04,IPBFSZ(,SAVEREG)                                0397 01136000
         BNE   @DL00371                                            0397 01137000
@DC00371 DS    0H                                                  0398 01138000
*                                                                  0398 01139000
*       /*************************************************************/ 01140000
*       /*                                                           */ 01141000
*       /* EITHER IPBFSZ OR XSAPRMSZ IS ZERO 1. IF XSAPRMSZ ^= 0 AND */ 01142000
*       /* THIS IS END OF INPUT(IPPRTL=0), THEN PAD REMAINING BUFFER */ 01143000
*       /* WITH BLANKS. 2. IF IPBFSZ ^= 0, THEN SCAN FOR ANOTHER     */ 01144000
*       /* VALID CHAR, ADJUSTING IPBFSZ AND IPBUFADR. THIS IS DONE SO*/ 01145000
*       /* THAT IF CALLER IS RETURNED A CODE '0C'HEX, INDICATING THAT*/ 01146000
*       /* MORE DATA FOLLOWS, THEN THERE SHOULD BE VALID DATA        */ 01147000
*       /*                                                           */ 01148000
*       /*************************************************************/ 01149000
*                                                                  0398 01150000
*       IF XSAPRMSZ^=0&IPPRTL='0'B THEN                            0398 01151000
         LH    @04,XSAPRMSZ(,RBSECPTR)                             0398 01152000
         LTR   @04,@04                                             0398 01153000
         BZ    @RF00398                                            0398 01154000
         TM    IPPRTL(SAVEREG),B'00100000'                         0398 01155000
         BNZ   @RF00398                                            0398 01156000
*         DO;                                                      0399 01157000
*                                                                  0399 01158000
*           /*********************************************************/ 01159000
*           /*                                                       */ 01160000
*           /* PAD INPUT AREA WITH BLANKS, BUT DON'T CHANGE XSAPRMSZ */ 01161000
*           /*                                                       */ 01162000
*           /*********************************************************/ 01163000
*                                                                  0400 01164000
*           RFY                                                    0400 01165000
*            (REG0,                                                0400 01166000
*             REG1,                                                0400 01167000
*             REG2,                                                0400 01168000
*             REG3) RSTD;                                          0400 01169000
*           REG0=XSABFRAD;          /* TARGET ADR FOR 'MVCL'         */ 01170000
         L     REG0,XSABFRAD-1(,RBSECPTR)                          0401 01171000
         ICM   REG0,8,@CB02616                                     0401 01172000
*           REG1=XSAPRMSZ;          /* AM'T OF AREA TO MOVE INTO REG2   01173000
*                                      NOT SET SINCE COUNT WILL BE 0402 01174000
*                                      ZERO, CAUSING PAD CHAR TO BE     01175000
*                                      USED                          */ 01176000
         LR    REG1,@04                                            0402 01177000
*           REG3=BLANK;             /* PADDING CHAR IS A BLANK       */ 01178000
         LA    REG3,64                                             0403 01179000
*           SLL(REG3,24);           /* PUT INTO PAD POSITION         */ 01180000
         SLL   REG3,24                                             0404 01181000
*           MVCL(REG0,REG2);        /* MOVE IN BLANKS & SET COUNT=0  */ 01182000
         MVCL  REG0,REG2                                           0405 01183000
*         END;                                                     0406 01184000
*       IF IPBFSZ^=0 THEN           /* MORE DATA IN QUEUE ELE ? YES,    01185000
*                                      VALIDATE IT                   */ 01186000
@RF00398 LH    @04,IPBFSZ(,SAVEREG)                                0407 01187000
         LTR   @04,@04                                             0407 01188000
         BZ    @RF00407                                            0407 01189000
*         DO UNTIL REG3^=0|IPBFSZ=0;                               0408 01190000
@DL00408 DS    0H                                                  0409 01191000
*           REG3=MIN(IPBFSZ,256);   /* 'TRT' UP TO 256 CHARS         */ 01192000
         LH    REG3,IPBFSZ(,SAVEREG)                               0409 01193000
         LA    @04,256                                             0409 01194000
         CR    REG3,@04                                            0409 01195000
         BNH   *+6                                                      01196000
         LR    REG3,@04                                            0409 01197000
*           REG1=0;                 /* CLEAR REGISTER BEFORE TRT     */ 01198000
         SLR   REG1,REG1                                           0410 01199000
*           TRT(IPSOURCE(1:REG3),TGETEDIT(1));/* SEE IF ANY VALID    */ 01200000
         L     @04,IPBUFADR(,SAVEREG)                              0411 01201000
         LR    @15,REG3                                            0411 01202000
         BCTR  @15,0                                               0411 01203000
         EX    @15,@SB02692                                        0411 01204000
*           BC(CC0,CONTIN2);        /* BRANCH IF THERE IS VALID DATA */ 01205000
         BC    8,CONTIN2                                           0412 01206000
*           REG3=REG1-IPBUFADR;     /* NO. OF VALID CHARS            */ 01207000
         L     @04,IPBUFADR(,SAVEREG)                              0413 01208000
         LR    REG3,REG1                                           0413 01209000
         SLR   REG3,@04                                            0413 01210000
*           IF REG3=0 THEN          /* ANY VALID AT ALL ?            */ 01211000
         LTR   REG3,REG3                                           0414 01212000
         BNZ   @RF00414                                            0414 01213000
*             DO;                   /* NO, KEEP SEARCHING            */ 01214000
*               IPBFSZ=IPBFSZ-1;    /* UPDATE QUEUE ELE SIZE         */ 01215000
         LH    @15,IPBFSZ(,SAVEREG)                                0416 01216000
         BCTR  @15,0                                               0416 01217000
         STH   @15,IPBFSZ(,SAVEREG)                                0416 01218000
*               IPBUFADR=IPBUFADR+1;/* UPDATE QUEUE ELE ADDRESS      */ 01219000
         AL    @04,@CF00050                                        0417 01220000
         ST    @04,IPBUFADR(,SAVEREG)                              0417 01221000
*             END;                                                 0418 01222000
*                                                                  0418 01223000
*           /*********************************************************/ 01224000
*           /*                                                       */ 01225000
*           /* FALL THRU TO CONTIN2.                                 */ 01226000
*           /*                                                       */ 01227000
*           /*********************************************************/ 01228000
*                                                                  0419 01229000
*CONTIN2:                                                          0419 01230000
*                                                                  0419 01231000
*           /*********************************************************/ 01232000
*           /*                                                       */ 01233000
*           /* REG3 IS 0 IF FIRST CHAR IN TRT WAS INVALID. OTHERWISE */ 01234000
*           /* IT IS THE NO. OF VALID CHARS IF BRANCH WASN'T TAKEN & */ 01235000
*           /* IT IS MIN(IPBFSZ,256) IF BRANCH WAS TAKEN. IF REG3 IS */ 01236000
*           /* 0 THEN THE DO UNTIL WILL CONTINUE SEARCHING FOR A     */ 01237000
*           /* VALID CHAR, UPDATING QUEUE ELE POINTERS (UNLESS IPBFSZ*/ 01238000
*           /* BECOMES 0, IN WHICH CASE THERE IS NO MORE DATA)       */ 01239000
*           /*                                                       */ 01240000
*           /*********************************************************/ 01241000
*                                                                  0419 01242000
*         END;                      /* END DO UNTIL                  */ 01243000
@RF00414 DS    0H                                                  0419 01244000
CONTIN2  DS    0H                                                  0419 01245000
@DE00408 SLR   @04,@04                                             0419 01246000
         CR    REG3,@04                                            0419 01247000
         BNE   @DC00408                                            0419 01248000
         CH    @04,IPBFSZ(,SAVEREG)                                0419 01249000
         BNE   @DL00408                                            0419 01250000
@DC00408 DS    0H                                                  0420 01251000
*       RFY                                                        0420 01252000
*        (REG0,                                                    0420 01253000
*         REG1,                                                    0420 01254000
*         REG2,                                                    0420 01255000
*         REG3) UNRSTD;                                            0420 01256000
@RF00407 DS    0H                                                  0421 01257000
*     END;                          /* END 'EDIT' OPTION PROCESSING  */ 01258000
*   RFY                                                            0422 01259000
*     RTNREG RSTD;                                                 0422 01260000
@RC00356 DS    0H                                                  0423 01261000
*   RTNREG=XSASAVEA;                /* RESTORE RETURN ADDRESS        */ 01262000
         L     RTNREG,XSASAVEA(,RBSECPTR)                          0423 01263000
*   RETURN;                                                        0424 01264000
@EL00004 DS    0H                                                  0424 01265000
@EF00004 DS    0H                                                  0424 01266000
@ER00004 BR    @14                                                 0424 01267000
*   RFY                                                            0425 01268000
*     RTNREG UNRSTD;                                               0425 01269000
*   END EDIT3767;                                                  0426 01270000
*EDIT3270:                                                         0427 01271000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0427 01272000
EDIT3270 DS    0H                                                  0428 01273000
*                                                                  0428 01274000
*/*                                                                  */ 01275000
*/********************************************************************/ 01276000
*/*                                                                  */ 01277000
*/*        EDIT3270 IS THE STANDARD TSO/VTAM EDIT ROUTINE FOR        */ 01278000
*/*        THE 3270 TERMINAL. IT CHECKS FOR A TGET EDIT              */ 01279000
*/*        REQUEST AND SCANS FOR INVALID DATA AND 3270 CONTROL CHARS */ 01280000
*/*        IF SO. IT MOVES THE                                       */ 01281000
*/*        DATA FROM THE INPUT QUEUE TO THE CALLER'S DATA AREA       */ 01282000
*/*        AND PADS WITH BLANKS IF EDIT OPTION.                      */ 01283000
*/*                                                                  */ 01284000
*/*        INPUT NEEDED---XSABFRAD - CALLER' DATA ADDRESS            */ 01285000
*/*                       XSAPRMSZ - CALLER'S DATA AREA SIZE         */ 01286000
*/*                       IPBUFADR - QUEUE ELEMENT ADDRESS           */ 01287000
*/*                       IPBFSZ   - QUEUE ELEMENT SIZE              */ 01288000
*/*                       XSAEDITO - OPTION IS EDIT IF='00'          */ 01289000
*/*                                                                  */ 01290000
*/*        OUTPUT-----EDIT3270 UPDATES THE DATA ADDRESSES AND        */ 01291000
*/*                   AREA SIZES LISTED ABOVE.                       */ 01292000
*/*                                                                  */ 01293000
*/********************************************************************/ 01294000
*/*                                                                  */ 01295000
*                                                                  0428 01296000
*   DCL                                                            0428 01297000
*     SBA BIT(8) CONSTANT('11'X);   /* 3270 START BUFFER ADDRES      */ 01298000
*   RFY                                                            0429 01299000
*     RTNREG RSTD;                                                 0429 01300000
*   XSASAVEA=RTNREG;                /* SAVE RETURN ADDRESS           */ 01301000
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0430 01302000
*   RFY                                                            0431 01303000
*     RTNREG UNRSTD;                                               0431 01304000
*   XSACURDS=0;                     /* CLEAR DATA MOVED COUNTER      */ 01305000
         SLR   @04,@04                                             0432 01306000
         STH   @04,XSACURDS(,RBSECPTR)                             0432 01307000
*   IF XSAEDITO^='00'B THEN         /* 00 IS EDIT OPTION             */ 01308000
         TM    XSAEDITO(RBSECPTR),B'00000011'                      0433 01309000
         BZ    @RF00433                                            0433 01310000
*     DO;                                                          0434 01311000
*                                                                  0434 01312000
*/*                                                                  */ 01313000
*/*     DECREMENT 'CONTROL CHARACTER COUNT' TO KEEP TRACK OF NUMBER  */ 01314000
*/*     OF CONTROL CHARACTERS THAT COULD NOT BE MOVED                */ 01315000
*/*                                                                  */ 01316000
*                                                                  0435 01317000
*       IPCCC=MAX(IPCCC-XSAPRMSZ,0);/* MOVE A MAX OF XSAPRMSZ CONTROL   01318000
*                                      CHARACTERS FROM QUEUE         */ 01319000
         LH    @15,XSAPRMSZ(,RBSECPTR)                             0435 01320000
         SLR   @14,@14                                             0435 01321000
         IC    @14,IPCCC(,SAVEREG)                                 0435 01322000
         SLR   @14,@15                                             0435 01323000
         CR    @14,@04                                             0435 01324000
         BNL   *+6                                                      01325000
         LR    @14,@04                                             0435 01326000
         STC   @14,IPCCC(,SAVEREG)                                 0435 01327000
*                                                                  0436 01328000
*/*     TGET ASIS DATA MOVE. IF EITHER COUNT IS ZERO ON ENTRY,       */ 01329000
*/*     NOTHING TO MOVE                                              */ 01330000
*/*                                                                  */ 01331000
*                                                                  0436 01332000
*       IF XSAPRMSZ^=0&IPBFSZ^=0 THEN                              0436 01333000
         CR    @15,@04                                             0436 01334000
         BE    @RF00436                                            0436 01335000
         LH    @14,IPBFSZ(,SAVEREG)                                0436 01336000
         CR    @14,@04                                             0436 01337000
         BE    @RF00436                                            0436 01338000
*         DO;                                                      0437 01339000
*           RFY                                                    0438 01340000
*            (REG0,                                                0438 01341000
*             REG1,                                                0438 01342000
*             REG2,                                                0438 01343000
*             REG3) RSTD;                                          0438 01344000
*           REG0=XSABFRAD;          /* TARGET ADDR FOR 'MVCL'        */ 01345000
         L     @04,XSABFRAD-1(,RBSECPTR)                           0439 01346000
         LA    @04,0(,@04)                                         0439 01347000
         LR    REG0,@04                                            0439 01348000
*           REG1=MIN(XSAPRMSZ,IPBFSZ);/* GET MINIMUM FOR MOVE        */ 01349000
         LR    REG1,@14                                            0440 01350000
         CR    REG1,@15                                            0440 01351000
         BNH   *+6                                                      01352000
         LR    REG1,@15                                            0440 01353000
*           REG2=IPBUFADR;          /* SOURCE ADDR FOR 'MVCL'        */ 01354000
         L     @11,IPBUFADR(,SAVEREG)                              0441 01355000
         LR    REG2,@11                                            0441 01356000
*           REG3=REG1;              /* SAME SIZE                     */ 01357000
         LR    REG3,REG1                                           0442 01358000
*           XSAPRMSZ=XSAPRMSZ-REG1; /* UPDATE CALLER'S SIZE          */ 01359000
         SLR   @15,REG1                                            0443 01360000
         STH   @15,XSAPRMSZ(,RBSECPTR)                             0443 01361000
*           XSABFRAD=XSABFRAD+REG1; /* UPDATE CALLER'S ADDRESS       */ 01362000
         ALR   @04,REG1                                            0444 01363000
         STCM  @04,7,XSABFRAD(RBSECPTR)                            0444 01364000
*           IPBFSZ=IPBFSZ-REG1;     /* UPDATE QUEUE ELE SIZE         */ 01365000
         SLR   @14,REG1                                            0445 01366000
         STH   @14,IPBFSZ(,SAVEREG)                                0445 01367000
*           IPBUFADR=IPBUFADR+REG1; /* UPDATE QUEUE ELE ADDRESS      */ 01368000
         ALR   @11,REG1                                            0446 01369000
         ST    @11,IPBUFADR(,SAVEREG)                              0446 01370000
*           XSACURDS=REG1;          /* SET AMOUNT OF DATA MOVED      */ 01371000
         STH   REG1,XSACURDS(,RBSECPTR)                            0447 01372000
*           MVCL(REG0,REG2);        /* MOVE DATA FROM INPUT BUFFER TO   01373000
*                                      CALLERS DATA AREA             */ 01374000
         MVCL  REG0,REG2                                           0448 01375000
*           RFY                                                    0449 01376000
*            (REG0,                                                0449 01377000
*             REG1,                                                0449 01378000
*             REG2,                                                0449 01379000
*             REG3) UNRSTD;                                        0449 01380000
*         END;                      /* END 'TGET ASIS' MOVE          */ 01381000
*       ELSE                                                       0451 01382000
*         ;                         /* IF EITHER=0, NO DATA MOVE     */ 01383000
@RF00436 DS    0H                                                  0452 01384000
*     END;                          /* END 'TGET ASIS'               */ 01385000
*   ELSE                            /* 'TGET EDIT' DATA MOVE         */ 01386000
*     DO;                                                          0453 01387000
*                                                                  0453 01388000
         B     @RC00433                                            0453 01389000
@RF00433 DS    0H                                                  0454 01390000
*       /*************************************************************/ 01391000
*       /*                                                           */ 01392000
*       /* POINT PAST 3270 CONTROL CHARACTERS. IPCCC IS >=0 AND <=6. */ 01393000
*       /*                                                           */ 01394000
*       /*************************************************************/ 01395000
*                                                                  0454 01396000
*       IPBFSZ=IPBFSZ-IPCCC;        /* ADJUST DATA COUNT             */ 01397000
         SLR   @04,@04                                             0454 01398000
