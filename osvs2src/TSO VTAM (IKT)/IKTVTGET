         CR    REG3,@04                                            0459 01430000
         BNH   *+6                                                      01431000
         LR    REG3,@04                                            0459 01432000
*         RFY                                                      0460 01433000
*          (REG1,                                                  0460 01434000
*           REG2) RSTD;                                            0460 01435000
*         REG1=0;                   /* CLEAR REGISTER BEFORE TRT     */ 01436000
         SLR   REG1,REG1                                           0461 01437000
*         TRT(IPSOURCE(1:REG3),TGETEDIT(1));/* SEARCH FOR INVALID  0462 01438000
