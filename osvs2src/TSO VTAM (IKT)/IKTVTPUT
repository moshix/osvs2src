         N     PRMREG0,@CF02477                                    0149 00260000
*PRMREG1 = TPUT;                     /* INDICATE TPUT                */ 00261000
         L     PRMREG1,TPUT                                        0150 00262000
*GEN REFS(SAVEREG,ASCBASID,CVTOPTE,CVT,CVTPTR)                     0151 00263000
*    SETS(RTNREG,ENTRYREG);                                        0151 00264000
***                                                                     00265000
***      INFORM SYSTEM RESOURCE MANAGER OF WAIT                         00266000
***                                                                     00267000
         SYSEVENT TERMWAIT,ASID=(PRMREG0),ENTRY=BRANCH                  00268000
*RFY (PRMREG0,PRMREG1) UNRSTD;                                     0152 00269000
*/*                                                                  */ 00270000
*/********************************************************************/ 00271000
*/*                                                                  */ 00272000
*/*       PREPARE TO SUSPEND CALLER VIA BRANCH TO THE STATUS         */ 00273000
*/*       ROUTINE. LOCAL LOCK HELD ON ENTRY TO STATUS ROUTINE        */ 00274000
*/*                                                                  */ 00275000
*/********************************************************************/ 00276000
*/*                                                                  */ 00277000
*STM(PRMREG0,ENTRYREG,TCBGRS);       /* SAVE REGISTERS               */ 00278000
