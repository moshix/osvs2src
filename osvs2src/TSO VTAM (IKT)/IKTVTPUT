         LA    PRMREG0,15                                          0157 00289000
*PRMREG1 = XSATCBA;                  /* SET TCB TO SUSPEND, AND SET     00290000
*                                       RESET CODE(BYTE 30 TO SET(=0)*/ 00291000
         LR    PRMREG1,@04                                         0158 00292000
*CALL STATUS;                        /* CALL STATUS TO SUSPEND TASK  */ 00293000
         L     @04,CVTPTR                                          0159 00294000
         L     @04,SCVTSPTR(,@04)                                  0159 00295000
         L     @15,SCVTSTAT(,@04)                                  0159 00296000
         BALR  @14,@15                                             0159 00297000
*RFY (PRMREG0,PRMREG1) UNRSTD;                                     0160 00298000
*/*                                                                  */ 00299000
*/********************************************************************/ 00300000
*/*                                                                  */ 00301000
*/*       UPON BEING RE-DISPATCHED, LOCAL LOCK MUST                  */ 00302000
*/*       BE RE-OBTAINED                                             */ 00303000
