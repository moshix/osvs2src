*/*                                                                  */ 00787000
*/*      DATA EDIT IS AN INTERNAL PROCEDURE THAT SCANS THE           */ 00788000
*/*      CALLER'S DATA AND REMOVES TRAILING BLANKS ADJUSTING         */ 00789000
*/*      THE BYTE COUNT ACCORDINGLY.                                 */ 00790000
*/*    **** DATAEDIT SAVES RETURN ADDRESS INTO THE LEVEL 'A'         */ 00791000
*/*         RETURN 'SAVE WORD' - XSASAVEA.                           */ 00792000
*/********************************************************************/ 00793000
*/*                                                                  */ 00794000
*DCL I REG(1);                                                     0302 00795000
*DCL BLANK CHAR(1) CONSTANT(' ');                                  0303 00796000
*RFY RTNREG RSTD;                                                  0304 00797000
*XSASAVEA = RTNREG;                                                0305 00798000
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0305 00799000
*RFY RTNREG UNRSTD;                                                0306 00800000
*IF XSAEDITO^= '00'B THEN;           /* IF NOT EDIT, NO-OP           */ 00801000
         TM    XSAEDITO(RBSECPTR),B'00000011'                      0307 00802000
         BNZ   @RT00307                                            0307 00803000
*ELSE                                                              0309 00804000
*  DO;                               /* EDIT OPTION-REMOVE TRAILING     00805000
*                                       BLANKS                       */ 00806000
*    XSADMOVE = '1'B;                /* SET DATA MOVE INDICATOR      */ 00807000
         OI    XSADMOVE(RBSECPTR),B'01000000'                      0310 00808000
*    RFY REG2 RSTD;                                                0311 00809000
*    REG2 = RBLINKB -> RBOPSWB2;     /* OBTAIN CALLERS KEY FROM RB   */ 00810000
         L     @04,RBLINKB-1(,RBSECPTR)                            0312 00811000
         SLR   REG2,REG2                                           0312 00812000
         IC    REG2,RBOPSWB2(,@04)                                 0312 00813000
*    GEN SETS(REG2);                                               0313 00814000
        MODESET KEYADDR=(2),WORKREG=2      GET IN TASKS KEY             00815000
*    RFY REG2 UNRSTD;                                              0314 00816000
*    RFY I RSTD;                                                   0315 00817000
*    DO I = XSAPRMSZ TO 2 BY -1      /* SCAN FROM END OF DATA        */ 00818000
*       WHILE TPUTDATA(I) = BLANK;   /* CONTINUE WHILE BLANK         */ 00819000
         LH    I,XSAPRMSZ(,RBSECPTR)                               0316 00820000
         B     @DE00316                                            0316 00821000
@DL00316 L     @04,XSABFRAD-1(,RBSECPTR)                           0316 00822000
         ALR   @04,I                                               0316 00823000
         BCTR  @04,0                                               0316 00824000
         CLI   TPUTDATA(@04),C' '                                  0316 00825000
         BNE   @DC00316                                            0316 00826000
*    END;                                                          0317 00827000
         BCTR  I,0                                                 0317 00828000
@DE00316 C     I,@CF00075                                          0317 00829000
         BNL   @DL00316                                            0317 00830000
@DC00316 DS    0H                                                  0318 00831000
*    RFY REG2 RSTD;                                                0318 00832000
*    GEN SETS(REG2)                                                0319 00833000
*        REFS(RBLINK,RBOPSW,RBBASIC,TCBRBP);                       0319 00834000
        MODESET EXTKEY=ZERO GET BACK INTO KEY 0                         00835000
*    RFY REG2 UNRSTD;                                              0320 00836000
*    XSADMOVE = '0'B;                /* CLEAR DATA MOVE INDICATOR    */ 00837000
         NI    XSADMOVE(RBSECPTR),B'10111111'                      0321 00838000
*    XSAPRMSZ = I;                   /* UPDATE TPUT SIZE. MIN VALUE     00839000
*                                       IS 1, EVEN IF WHOLE TPUT IS     00840000
*                                       BLANK                        */ 00841000
         STH   I,XSAPRMSZ(,RBSECPTR)                               0322 00842000
*    RFY I UNRSTD;                                                 0323 00843000
*  END;                              /* END OF EDIT DO GROUP         */ 00844000
*RFY RTNREG RSTD;                                                  0325 00845000
@RT00307 DS    0H                                                  0326 00846000
*RTNREG = XSASAVEA;                  /* RESTORE RETURN ADDRESS       */ 00847000
         L     RTNREG,XSASAVEA(,RBSECPTR)                          0326 00848000
*RETURN;                                                           0327 00849000
@EL00008 DS    0H                                                  0327 00850000
@EF00008 DS    0H                                                  0327 00851000
@ER00008 BR    @14                                                 0327 00852000
*RFY RTNREG UNRSTD;                                                0328 00853000
*END DATAEDIT;                                                     0329 00854000
*VERIFY: PROC OPTIONS(NOSAVEAREA,NOSAVE);                          0330 00855000
VERIFY   DS    0H                                                  0331 00856000
*/*                                                                  */ 00857000
*/********************************************************************/ 00858000
*/*                                                                  */ 00859000
*/*      THIS TESTS IF CALLER'S DATA ADDRESS IS IN THE CALLER'S      */ 00860000
*/*      KEY. IT DOES SO BY REFERENCING THE FIRST BYTE OF EACH       */ 00861000
*/*      PAGE REQUIRED FOR THE TPUT.                                 */ 00862000
*/*    ***VERIFY SAVES RETURN ADDRESS INTO THE LEVEL 'B' RETURN      */ 00863000
*/*       'SAVE WORD' - XSASAVEB.                                    */ 00864000
*/*                                                                  */ 00865000
*/********************************************************************/ 00866000
*/*                                                                  */ 00867000
*DCL APAGE FIXED(15) CONSTANT(4096); /* PAGE SIZE                    */ 00868000
*DCL MODULUS FIXED(8) CONSTANT(12);  /* MODULUS OF A VS2 PAGE IS   0332 00869000
*                                       2**12=4096                   */ 00870000
*RFY RTNREG RSTD;                                                  0333 00871000
*XSASAVEB = RTNREG;                  /* SAVE RETURN ADDRESS          */ 00872000
         ST    RTNREG,XSASAVEB(,RBSECPTR)                          0334 00873000
*RFY RTNREG UNRSTD;                                                0335 00874000
*XSADMOVE = '1'B;                    /* INDICATE 'DATA VERIFICATION     00875000
*                                       UNDERWAY'                    */ 00876000
         OI    XSADMOVE(RBSECPTR),B'01000000'                      0336 00877000
*/*    GET INTO CALLER'S KEY TO VALIDATE DATA                        */ 00878000
*RFY REG2 RSTD;                                                    0337 00879000
*REG2 = RBLINKB -> RBOPSWB2;       /* OBTAIN CALLERS KEY FROM RB     */ 00880000
         L     @04,RBLINKB-1(,RBSECPTR)                            0338 00881000
         SLR   REG2,REG2                                           0338 00882000
         IC    REG2,RBOPSWB2(,@04)                                 0338 00883000
*GEN SETS(REG2);                                                   0339 00884000
        MODESET KEYADDR=(2),WORKREG=2      GET IN TASKS KEY             00885000
*RFY REG2 UNRSTD;                                                  0340 00886000
*RFY WORK1REG RSTD;                                                0341 00887000
*WORK1REG = XSABFRAD;                /* GET INPUT BUFFER ADDRESS     */ 00888000
         L     WORK1REG,XSABFRAD-1(,RBSECPTR)                      0342 00889000
         LA    WORK1REG,0(,WORK1REG)                               0342 00890000
*SRL(WORK1REG,MODULUS);              /* SHIFT OFF PARTIAL PAGE       */ 00891000
         SRL   WORK1REG,12                                         0343 00892000
*SLL(WORK1REG,MODULUS);              /* SHIFT IN ZEROS               */ 00893000
         SLL   WORK1REG,12                                         0344 00894000
*/*                                                                  */ 00895000
*/*       LOOP THRU EACH PAGE, MOVING FIRST BYTE TO ITSELF           */ 00896000
*/*       ABEND WILL CAUSE IKT93EST TO GAIN CONTROL AND CHANGE       */ 00897000
*/*       CODE TO 15D X.                                             */ 00898000
*/*                                                                  */ 00899000
*RFY WORK2REG RSTD;                                                0345 00900000
*WORK2REG = XSABFRAD +XSAPRMSZ;      /* SET UP LOOP UPPER LIMIT      */ 00901000
         L     WORK2REG,XSABFRAD-1(,RBSECPTR)                      0346 00902000
         LA    WORK2REG,0(,WORK2REG)                               0346 00903000
         AH    WORK2REG,XSAPRMSZ(,RBSECPTR)                        0346 00904000
*DO WHILE WORK1REG<WORK2REG;                                       0347 00905000
         B     @DE00347                                            0347 00906000
@DL00347 DS    0H                                                  0348 00907000
*   GEN REFS(WORK1REG)                                             0348 00908000
*       (CLC 0(1,WORK1REG),0(WORK1REG)); /* REFERENCE PAGE           */ 00909000
         CLC 0(1,WORK1REG),0(WORK1REG)                                  00910000
*   WORK1REG = WORK1REG + APAGE;     /* INCREMENT BY PAGE SIZE       */ 00911000
         AL    WORK1REG,@CF02469                                   0349 00912000
*END;                                                              0350 00913000
@DE00347 CR    WORK1REG,WORK2REG                                   0350 00914000
         BL    @DL00347                                            0350 00915000
*RFY (WORK1REG,WORK2REG) UNRSTD;                                   0351 00916000
*/*                                                                  */ 00917000
*/*         GET BACK INTO KEY 0                                      */ 00918000
*/*                                                                  */ 00919000
*GEN (MODESET EXTKEY=ZERO);                                        0352 00920000
         MODESET EXTKEY=ZERO                                            00921000
*XSADMOVE = '0'B;                    /* CLEAR INDICATOR THAT DATA  0353 00922000
*                                       VERIFICATION WAS GOING  ON   */ 00923000
         NI    XSADMOVE(RBSECPTR),B'10111111'                      0353 00924000
*RFY RTNREG RSTD;                                                  0354 00925000
*RTNREG = XSASAVEB;                                                0355 00926000
         L     RTNREG,XSASAVEB(,RBSECPTR)                          0355 00927000
*RETURN;                                                           0356 00928000
@EL00009 DS    0H                                                  0356 00929000
@EF00009 DS    0H                                                  0356 00930000
@ER00009 BR    @14                                                 0356 00931000
*RFY RTNREG UNRSTD;                                                0357 00932000
*END VERIFY;                                                       0358 00933000
*END IKTVTPUT                                                      0359 00934000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.      * 00935000
