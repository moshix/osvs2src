         STC   @15,XSARC(,RBSECPTR)                                0174 00356000
*END TERMSTAT;                                                     0175 00357000
@EL00003 DS    0H                                                  0175 00358000
@EF00003 DS    0H                                                  0175 00359000
@ER00003 BR    @14                                                 0175 00360000
*RFY RTNREG UNRSTD;                                                0176 00361000
*WIFOWAIT: PROC OPTIONS(NOSAVEAREA,NOSAVE);                        0177 00362000
WIFOWAIT DS    0H                                                  0178 00363000
*/*                                                                  */ 00364000
*/********************************************************************/ 00365000
*/*                                                                  */ 00366000
*/*       WAIT IF OWAIT IS AN INTERNAL PROCEEDURE THAT               */ 00367000
*/*       CHECKS THE TERMINAL STATUS AND CHECKS IF ANOTHER           */ 00368000
*/*       TASK 'OWNS'THE TSB(TSBOWIP). THIS MIGHT BE IF THE          */ 00369000
*/*       OTHER TASK IS SUSPENDED FOR A TPUT HOLD OR BECAUSE         */ 00370000
*/*       OF THE HIGH BUFFER THRESHOLD BEING REACHED.                */ 00371000
*/*       IF TERMINAL STATUS IS NORMAL AND ANOTHER TASK 'OWNS'       */ 00372000
*/*       THE TSB THEN THE CALLING TASK IS SUSPENDED BY CALLING      */ 00373000
*/*       WAITCHK                                                    */ 00374000
*/*      ****WIFOWAIT SAVES RETURN ADDRESS INTO THE LEVEL 'A'        */ 00375000
*/*         RETURN 'SAVE WORD' - XSASAVEA.                           */ 00376000
*/*                                                                  */ 00377000
*/********************************************************************/ 00378000
*/*                                                                  */ 00379000
*RFY RTNREG RSTD;                                                  0178 00380000
*XSASAVEA = RTNREG;                  /* SAVE RETURN ADDRESS          */ 00381000
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0179 00382000
*RFY RTNREG UNRSTD;                                                0180 00383000
*CALL TERMSTAT;                      /* CHECK TERMINAL STATUS      0181 00384000
*                                       TERMSTAT SETS XSARC          */ 00385000
         BAL   @14,TERMSTAT                                        0181 00386000
*/*                                                                  */ 00387000
*/********************************************************************/ 00388000
*/*                                                                  */ 00389000
*/*       LEAVE TASK IN 'DO WHILE' UNTIL THE TERMINAL STATUS         */ 00390000
*/*       INDICATES SESSION CANCELLED/ATTENTION OR UNTIL NO          */ 00391000
*/*       ONE 'OWNS' THE TSB                                         */ 00392000
*/*                                                                  */ 00393000
*/********************************************************************/ 00394000
*/*                                                                  */ 00395000
*DO WHILE XSARC = CONTINUE & TSBOWIP = '1'B ;                      0182 00396000
         B     @DE00182                                            0182 00397000
@DL00182 DS    0H                                                  0183 00398000
*  TSBWOWIP = '1'B;                  /* INDICATE WAITING ON OWAIT  0183 00399000
*                                       IN PROGRESS. TSBOWIP IS CLEARED 00400000
*                                       WHEN OTHER TASK COMPLETES  0183 00401000
*                                       TPUT.                        */ 00402000
         OI    TSBWOWIP(TSBPTR),B'00010000'                        0183 00403000
*  CALL WAITCHK;                     /* GO SUSPEND CALLING TASK.   0184 00404000
*                                       WAITCHK CAN CHANGE VALUE   0184 00405000
*                                       OF XSARC                     */ 00406000
         BAL   @14,WAITCHK                                         0184 00407000
*END;                                                              0185 00408000
