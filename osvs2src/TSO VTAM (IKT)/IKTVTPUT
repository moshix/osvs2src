*RFY  TCBPTR  UNRSTD;                /* FREE-UP REGISTER             */ 00035000
*RFY TCB BASED(XSATCBA);                                           0089 00036000
*IF TSBXACTV = '0'B |                /* TEST IF INITIALIZ. COMPLETE  */ 00037000
*   XSAPRMSZ = 0 THEN                /* OR IF ZERO BYTE TPUT         */ 00038000
         TM    TSBXACTV(@15),B'01000000'                           0090 00039000
         BZ    @RT00090                                            0090 00040000
         LH    @04,XSAPRMSZ(,RBSECPTR)                             0090 00041000
         LTR   @04,@04                                             0090 00042000
         BNZ   @RF00090                                            0090 00043000
@RT00090 DS    0H                                                  0091 00044000
*     XSARC = SUCCESS;               /* IF INIT NOT COMPLETE OR ZERO    00045000
*                                       BYTE TPUT, NOOP AND RETURN   */ 00046000
         IC    @04,SUCCESS                                         0091 00047000
         STC   @04,XSARC(,RBSECPTR)                                0091 00048000
*ELSE                                                              0092 00049000
*  DO;                               /* INITIALIZATION COMPLETE DO   */ 00050000
         B     @RC00090                                            0092 00051000
@RF00090 DS    0H                                                  0093 00052000
*    CALL WIFOWAIT;                  /* CALL'WAIT IF OWAIT IN PROGRESS' 00053000
*                                       WHICH SETS XSARC, AND CAN  0093 00054000
*                                       SUSPEND THE CALLING TASK     */ 00055000
         BAL   @14,WIFOWAIT                                        0093 00056000
*    IF XSARC ^= CONTINUE THEN;      /* IF CODE IS NOT SET TO CONTINUE  00057000
*                                       (SET TO SESSION CANCELLED OR    00058000
*                                        ATTENTION HIT) THEN RETURN     00059000
*                                        XSARC TO CALLER             */ 00060000
         CLC   XSARC(1,RBSECPTR),CONTINUE                          0094 00061000
         BNE   @RT00094                                            0094 00062000
*    ELSE                                                          0096 00063000
*      DO;                                                         0096 00064000
*        TSBOWIP = '1'B;             /* INDICATE TSB 'OWNED' BY TASK */ 00065000
         OI    TSBOWIP(TSBPTR),B'00100000'                         0097 00066000
*        TSBCTCB = XSATCBA;          /* SAVE TCB ADDRESS     @ZA32676*/ 00067000
         L     @04,XSATCBA-1(,RBSECPTR)                            0098 00068000
         LA    @04,0(,@04)                                         0098 00069000
         ST    @04,TSBCTCB(,TSBPTR)                                0098 00070000
*        CALL DATAEDIT;              /* PERFORM DEVICE INDEPENDENT 0099 00071000
*                                       OUTPUT EDITING               */ 00072000
         BAL   @14,DATAEDIT                                        0099 00073000
*        IF TSBNOBRK = '1'B THEN     /* TERMINAL OPR SPECIFY NOBREAK?*/ 00074000
         TM    TSBNOBRK(TSBPTR),B'00000010'                        0100 00075000
         BNO   @RF00100                                            0100 00076000
*             XSABRK = '0'B;         /* YES, ENSURE 'BREAKIN'      0101 00077000
*                                       WON'T OCCUR                  */ 00078000
         NI    XSABRK(RBSECPTR),B'11111011'                        0101 00079000
*        CALL TRYTOQUE;              /* REQUEST THAT DATA BE PLACED     00080000
*                                       ON OUTPUT QUEUE. TASK WILL BE   00081000
*                                       SUSPENDED IF HIGH BUFFER   0102 00082000
*                                       THRESHOLD HAS BEEN REACHED.     00083000
*                                       TRYTOQUE SETS XSARC          */ 00084000
@RF00100 BAL   @14,TRYTOQUE                                        0102 00085000
*        IF XSARC ^= CONTINUE THEN;  /* IF CODE IS NOT SET TO CONTINUE  00086000
*                                       (SET TO NOWAIT,            0103 00087000
*                                                 SESSION CANCELLED,    00088000
*                                        OR ATTENTION HIT) THEN RETURN  00089000
*                                        XSARC TO CALLER             */ 00090000
         CLC   XSARC(1,RBSECPTR),CONTINUE                          0103 00091000
         BNE   @RT00103                                            0103 00092000
*        ELSE                                                      0105 00093000
*         DO;                                                      0105 00094000
*          CALL SCHEDTOM;            /* GO SCHEDULE THE TERMINAL OUTPUT 00095000
*                                       MANAGER, IF NECESSARY        */ 00096000
         BAL   @14,SCHEDTOM                                        0106 00097000
*          /**********************************************************/ 00098000
*          /*                                                        */ 00099000
*          /*      TEST IF TASK SPECIFIED TPUT HOLD.                 */ 00100000
*          /*      IF SO, SUSPEND CALLER UNTIL DATA ARRIVES          */ 00101000
*          /*      AT TERMINAL. IF NOT,TELL SRM OF SUCCESFUL         */ 00102000
*          /*      TPUT AND RETURN TO TASK                           */ 00103000
*          /*                                                        */ 00104000
*          /**********************************************************/ 00105000
*          /*                                                        */ 00106000
*          IF XSAHOLD = '1'B THEN    /* TASK SPECIFY HOLD?           */ 00107000
         TM    XSAHOLD(RBSECPTR),B'00001000'                       0107 00108000
         BNO   @RF00107                                            0107 00109000
*            DO;                     /* YES                          */ 00110000
*              TSBHOLD = '1'B;       /* INDICATE  TPUT HOLD FOOTPRINT*/ 00111000
         OI    TSBHOLD(TSBPTR),B'00001000'                         0109 00112000
*              CALL WAITCHK;         /* CALL'WAIT&CHECK' TO SUSPEND     00113000
*                                       CALLER AND THEN CHECK TERMINAL  00114000
*                                       STATUS. WAITCHK SETS XSARC.  */ 00115000
         BAL   @14,WAITCHK                                         0110 00116000
*              TSBHOLD = '0'B;       /* CLEAR TPUT HOLD FOOTPRINT    */ 00117000
         NI    TSBHOLD(TSBPTR),B'11110111'                         0111 00118000
*              IF XSARC = SUCCESS THEN /* TEST IF SUCCESSFUL         */ 00119000
         CLC   XSARC(1,RBSECPTR),SUCCESS                           0112 00120000
         BNE   @RF00112                                            0112 00121000
*               DO;                                                0113 00122000
*                /*                                                  */ 00123000
*                /****************************************************/ 00124000
*                /*                                                  */ 00125000
*                /*     TELL SRM THAT OUTPUT COMPLETED-'TGETTPUT'    */ 00126000
*                /*                                                  */ 00127000
*                /****************************************************/ 00128000
*                /*                                                  */ 00129000
*                RFY (PRMREG0,PRMREG1) RSTD;                       0114 00130000
*                PRMREG0 = ASCBASID;                               0115 00131000
         L     @04,ASCBPTR                                         0115 00132000
         LH    PRMREG0,ASCBASID(,@04)                              0115 00133000
         N     PRMREG0,@CF02477                                    0115 00134000
*                PRMREG1 = TPUT;      /* INDICATE TPUT(BIT 0 = 1)    */ 00135000
         L     PRMREG1,TPUT                                        0116 00136000
*                GEN REFS(SAVEREG,ASCBASID,CVTOPTE,CVT,CVTPTR)     0117 00137000
*                    SETS(PRMREG0, RTNREG,ENTRYREG);               0117 00138000
***                                                                     00139000
***      INFORM SRM OF TPUT COMPLETION                                  00140000
***                                                                     00141000
