*              RFY (PRMREG0,PRMREG1);                              0126 00171000
*            END;                    /* END OF TPUT NOHOLD DO GROUP  */ 00172000
*              /* XSARC = 0 FROM RETURN FROM 'TRYTOQUE'              */ 00173000
