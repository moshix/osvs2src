         STM   PRMREG0,ENTRYREG,TCBGRS(@04)                        0153 00281000
*RBOPSW2 = ADDR(REDISPCH);           /* SET UP RESUME PSW            */ 00282000
         LA    @15,REDISPCH                                        0154 00283000
         ST    @15,RBOPSW2(,RBSECPTR)                              0154 00284000
*RFY (PRMREG0,PRMREG1) RSTD;                                       0155 00285000
*SAVEREG = NDISPBIT;                 /* SET NON-DISPATCHABLE FLAG    */ 00286000
         LA    SAVEREG,1024                                        0156 00287000
