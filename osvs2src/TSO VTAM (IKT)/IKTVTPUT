         TITLE ' *** IKTVTPUT: TSO/VTAM TPUT ROUTINE ***               *00001000
                        '                                               00002000
IKTVTPUT CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IKTVTPUT  78.152'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
RBPRFXLN EQU 64                                                         00013000
RBPRFLNA EQU 32                                                         00014000
PRBLEN   EQU 136                                                        00015000
SIRBLEN  EQU 200                                                        00016000
TIRBLEN  EQU 136                                                        00017000
IRBLEN   EQU 128                                                        00018000
SVRBLEN  EQU 224                                                        00019000
