         L     @10,PSAAOLD                                         0414 01202000
         L     @10,TSBPTR(,@10)                                    0414 01203000
         L     @10,TSBEXTNT(,@10)                                  0414 01204000
         L     REG1,TVWAPTR(,@10)                                  0414 01205000
         LA    REG1,TVWAECBL(,REG1)                                0414 01206000
*   GENERATE CODE( WAIT ECBLIST=(1));/* WAIT ON ECBLIST FOR CONTROL  */ 01207000
          WAIT ECBLIST=(1)                                              01208000
*/********************************************************************/ 01209000
*/*  DETERMINE THE ACTION TO BE TAKEN BY CHECKING THE VARIOUS ECBS   */ 01210000
*/********************************************************************/ 01211000
*                                                                  0416 01212000
*   RFY                                                            0416 01213000
*     ECB BASED(ADDR(TVWAECB));     /* ESTABLISH ADDRESSABILITY TO 0416 01214000
*                                      TVWA ECB                      */ 01215000
*   IF ECBPOSTB='0'B THEN           /* CHECK FOR NO POST             */ 01216000
         L     @10,PSAAOLD                                         0417 01217000
         L     @10,TSBPTR(,@10)                                    0417 01218000
         L     @10,TSBEXTNT(,@10)                                  0417 01219000
         L     @10,TVWAPTR(,@10)                                   0417 01220000
         TM    ECBPOSTB+112(@10),B'01000000'                       0417 01221000
         BNZ   @RF00417                                            0417 01222000
*     DO;                           /* THIS MEANS THAT A CANCEL    0418 01223000
*                                      REQUEST WAS MADE BY TCAS      */ 01224000
*       RFY                                                        0419 01225000
*         REG14 RSTD;                                              0419 01226000
*       REG14=AUXSAVE;              /* RESTORE RETURN ADDRESS        */ 01227000
         L     REG14,AUXSAVE(,REG13)                               0420 01228000
*       RFY                                                        0421 01229000
*         REG14 UNRSTD;                                            0421 01230000
*       RETURN CODE(4);             /* INDICATE INITIALIZATION CANNOT   01231000
*                                      CONTINUE                      */ 01232000
         LA    @15,4                                               0422 01233000
         BR    @14                                                 0422 01234000
*     END;                                                         0423 01235000
*   ELSE                            /* TVWAECB WAS POSTED BY IKTRPLXT*/ 01236000
*     DO;                                                          0424 01237000
@RF00417 DS    0H                                                  0425 01238000
*       TSBXACTV='1'B;              /* INDICATE TERMINAL STATUS KNOWN*/ 01239000
         L     @10,PSAAOLD                                         0425 01240000
         L     @10,TSBPTR(,@10)                                    0425 01241000
         L     @10,TSBEXTNT(,@10)                                  0425 01242000
         OI    TSBXACTV(@10),B'01000000'                           0425 01243000
*       RFY                                                        0426 01244000
*         REG14 RSTD;                                              0426 01245000
*       REG14=AUXSAVE;              /* RESTORE RETURN ADDRESS        */ 01246000
         L     REG14,AUXSAVE(,REG13)                               0427 01247000
*       RFY                                                        0428 01248000
*         REG14 UNRSTD;                                            0428 01249000
*       RETURN CODE(0);             /* INDICATE SUCCESSFUL TERMINAL     01250000
*                                      REQUEST                       */ 01251000
         SLR   @15,@15                                             0429 01252000
         BR    @14                                                 0429 01253000
*     END;                                                         0430 01254000
*POSTERR:                                                          0431 01255000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                   0431 01256000
*                                                                  0431 01257000
POSTERR  DS    0H                                                  0432 01258000
*/*             THE ROUTINE SHOULD GIVE AN INDICATION OF FURTHER     */ 01259000
*/*        PROCESSING IMPOSSIBLE.  POST ISSUED TO CANCEL THE ADDRESS */ 01260000
*/*        SPACE.                                                    */ 01261000
*                                                                  0432 01262000
*   RFY                                                            0432 01263000
*     REG2 RSTD;                    /* RESTRICT WORK REGSITER        */ 01264000
*   REG2=ADDR(TSBXECB);             /* LOAD ADDRESS OF TSO CANCEL ECB*/ 01265000
         L     @10,PSAAOLD                                         0433 01266000
         L     @10,TSBPTR(,@10)                                    0433 01267000
         L     REG2,TSBEXTNT(,@10)                                 0433 01268000
         LA    REG2,TSBXECB(,REG2)                                 0433 01269000
*   GENERATE CODE;                                                 0434 01270000
*              /* ISSUE POST TO CANCEL INITIALIZATION */                01271000
          POST (REG2),X'0'                                              01272000
*   RFY                                                            0435 01273000
*     REG2 UNRSTD;                  /* UNRESTRICT REGISTER           */ 01274000
*   END POSTERR;                                                   0436 01275000
@EL00005 DS    0H                                                  0436 01276000
@EF00005 DS    0H                                                  0436 01277000
@ER00005 BR    @14                                                 0436 01278000
*   GENERATE CODE DEFS(POSTMAC,POSTLEN);                           0437 01279000
POSTMAC  POST  ,,ERRET=POSTERR,MF=L                                     01280000
*   END TERMCON;                                                   0438 01281000
         B     @EL00004                                            0438 01282000
*IKTQMINT:                                                         0439 01283000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0439 01284000
*                                                                  0439 01285000
IKTQMINT DS    0H                                                  0440 01286000
*/*  THIS ROUTINE INITIALIZES THE QUEUE MANAGER                      */ 01287000
*                                                                  0440 01288000
*   RFY                                                            0440 01289000
*     AUXSAVE BASED(REG13);         /* BASE SAVE AREA ON REGISTER 13 */ 01290000
*   RFY                                                            0441 01291000
*     REG14 RSTD;                   /* RESTRICT RETURN REGISTER      */ 01292000
*   AUXSAVE=REG14;                  /* SAVE RETURN ADDRESS           */ 01293000
         ST    REG14,AUXSAVE(,REG13)                               0442 01294000
*   RFY                                                            0443 01295000
*     REG1 RSTD;                                                   0443 01296000
*   TVWAQMWA=((TVWAEXL+LENGTH(IFGEXLST))/8)*8+8;/* INSERT ADDRESS OF    01297000
*                                      WORK AREA INTO TVWA   @YM03605*/ 01298000
         L     @15,PSAAOLD                                         0444 01299000
         L     @15,TSBPTR(,@15)                                    0444 01300000
         L     @15,TSBEXTNT(,@15)                                  0444 01301000
         L     @15,TVWAPTR(,@15)                                   0444 01302000
         LA    @10,8                                               0444 01303000
         LA    @02,65                                              0444 01304000
         AL    @02,TVWAEXL(,@15)                                   0444 01305000
         SRDA  @02,32                                              0444 01306000
         DR    @02,@10                                             0444 01307000
         SLA   @03,3                                               0444 01308000
         ALR   @03,@10                                             0444 01309000
         ST    @03,TVWAQMWA(,@15)                                  0444 01310000
*/*     CHECKS FOR CONTROL BLOCK TOTAL GROWTH NOT LARGER THAN A      */ 01311000
*/* THOUSAND BYTES AND RETURNS AN ERROR CODE IF THE TOTAL BYTE COUNT */ 01312000
*/* IS GREATER THAN ONE THOUSAND BYTES .                             */ 01313000
*                                                                  0445 01314000
*   IF ADDR(TVWA)+1000<TVWAQMWA+120 THEN/* CHECK FOR SPACE PROBLEM 0445 01315000
*                                                            @YM03258*/ 01316000
         AL    @15,@CF03000                                        0445 01317000
         AL    @03,@CF03001                                        0445 01318000
         CR    @15,@03                                             0445 01319000
         BNL   @RF00445                                            0445 01320000
*     DO;                                                          0446 01321000
*       REG14=AUXSAVE;              /* RESTORE RETURN ADDRESS      0447 01322000
*                                                            @YM03258*/ 01323000
         L     @10,AUXSAVE(,REG13)                                 0447 01324000
         LR    REG14,@10                                           0447 01325000
*       RETURN CODE(4);             /* RETURN ERROR CODE     @YM03258*/ 01326000
         LA    @15,4                                               0448 01327000
@EL00006 DS    0H                                                  0448 01328000
@EF00006 DS    0H                                                  0448 01329000
@ER00006 BR    @14                                                 0448 01330000
*     END;                                                         0449 01331000
*   ELSE                                                           0450 01332000
*     REG1=TVWAQMWA+120;            /* INSERT ADDRESS OF BUFFER AREA    01333000
*                                                            @YM03258*/ 01334000
@RF00445 L     @15,PSAAOLD                                         0450 01335000
         L     @15,TSBPTR(,@15)                                    0450 01336000
         L     @15,TSBEXTNT(,@15)                                  0450 01337000
         L     @15,TVWAPTR(,@15)                                   0450 01338000
         LA    REG1,120                                            0450 01339000
         AL    REG1,TVWAQMWA(,@15)                                 0450 01340000
*   RFY                                                            0451 01341000
*    (REG0,                                                        0451 01342000
*     REG3) RSTD;                                                  0451 01343000
*   REG3=TCASCLSZ+20;               /* GET SIZE OF CELL FOR BLDCPOOL */ 01344000
         L     @10,CVTPTR                                          0452 01345000
         L     @10,TCASTPTR(,@10)                                  0452 01346000
         LA    REG3,20                                             0452 01347000
         AH    REG3,TCASCLSZ(,@10)                                 0452 01348000
*   TVWACLSZ=REG3;                  /* PUT CELL SIZE IN TVWA         */ 01349000
         STH   REG3,TVWACLSZ(,@15)                                 0453 01350000
*   TVWAHBUF=TCASHBUF;              /* INSERT BUFFER CEILINGS        */ 01351000
         L     @02,TCASHBUF(,@10)                                  0454 01352000
         ST    @02,TVWAHBUF(,@15)                                  0454 01353000
*   TVWALBUF=TCASLBUF;              /* INSERT BUFFER THRESHOLD       */ 01354000
         L     @10,TCASLBUF(,@10)                                  0455 01355000
         ST    @10,TVWALBUF(,@15)                                  0455 01356000
*   GEN                             /* BUILD THE CELL POOL           */ 01357000
*       REFS(CVTBLDCP,CVTMAP);                                     0456 01358000
        BLDCPOOL CSIZE=(REG3),POOLSIZ=3,SP=229,CPADDR=(1),AUTODEL=YES,**01359000
               BRANCH=YES    DIVIDE CELL POOL                @YM03258   01360000
*   RFY                                                            0457 01361000
*     REG3 UNRSTD;                                                 0457 01362000
*   TVWACPID=REG0;                  /* STORE DYNAMIC CELL POOL ID IN    01363000
*                                      TVWA                          */ 01364000
*                                                                  0458 01365000
         L     @15,PSAAOLD                                         0458 01366000
         L     @15,TSBPTR(,@15)                                    0458 01367000
         L     @15,TSBEXTNT(,@15)                                  0458 01368000
         L     @15,TVWAPTR(,@15)                                   0458 01369000
         ST    REG0,TVWACPID(,@15)                                 0458 01370000
*   /*****************************************************************/ 01371000
*   /*                                                               */ 01372000
*   /* GET A CELL FOR THE LOOK ASIDE BUFFER                          */ 01373000
*   /*                                                               */ 01374000
*   /*****************************************************************/ 01375000
*                                                                  0459 01376000
*   GEN REFS(CVTGETCL,CVTMAP);                                     0459 01377000
        GETCELL CPID=(0),BRANCH=YES                                     01378000
*   TVWAQMLB=REG1;                  /* STORE @ OF LOOK ASIDE BUFFER     01379000
*                                      IN TVWA                       */ 01380000
         L     @15,PSAAOLD                                         0460 01381000
         L     @15,TSBPTR(,@15)                                    0460 01382000
         L     @15,TSBEXTNT(,@15)                                  0460 01383000
         L     @15,TVWAPTR(,@15)                                   0460 01384000
         ST    REG1,TVWAQMLB(,@15)                                 0460 01385000
*   TVWAQLBU='0'B;                  /* MARK LOOK ASIDE BUFFER AS NOT    01386000
*                                      IN USE                        */ 01387000
         NI    TVWAQLBU(@15),B'11111011'                           0461 01388000
*   RFY                                                            0462 01389000
*    (REG0,                                                        0462 01390000
