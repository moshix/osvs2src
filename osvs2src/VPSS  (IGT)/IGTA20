TA20     TITLE 'IGTA20 - VPSS CONTROL AND COMMAND PROCESSOR TABLE      *00001000
                        '                                               00002000
IGTA20   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTA20    77.092'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
         MVC   @PC00001(4),0(@01)                               01S0001 00022000
*   APNPPTR=ADDR(PARMAPNP);         /* LOAD ADDRESS OF APNP.         */ 00023000
         L     APNPPTR,@PC00001                                 01S0038 00024000
*   APNPSTAT=APNPOFF;               /* SET INITIAL GOOD STATUS.      */ 00025000
*                                                               01S0039 00026000
         NI    APNPSTAT(APNPPTR),B'11110111'                    01S0039 00027000
*   /*****************************************************************/ 00028000
*   /*                                                               */ 00029000
*   /* OBTAIN CORE FOR THE APCT                                      */ 00030000
*   /*                                                               */ 00031000
*   /*****************************************************************/ 00032000
*                                                               01S0040 00033000
*   RESPECIFY                                                   01S0040 00034000
*     APOT BASED(APOTPTR);          /* POINT TO APOT.                */ 00035000
*   RESPECIFY                                                   01S0041 00036000
*     APOTPTR RSTD;                 /* RESTRICT R9 TO APOT.          */ 00037000
*   APOTPTR=APNPAPOT;               /* LOAD ADDRESS OF APOT.         */ 00038000
         L     APOTPTR,APNPAPOT(,APNPPTR)                       01S0042 00039000
*   RESPECIFY                                                   01S0043 00040000
*     APCTPTR RSTD;                 /* RESTRICT POINTER TO THE APCT. */ 00041000
*   RESPECIFY                                                   01S0044 00042000
*     SSCT BASED(SSCTPTR);          /* POINT TO SSCT.                */ 00043000
*   RESPECIFY                                                   01S0045 00044000
*     R0P RSTD;                     /* RESTRICT R0.                  */ 00045000
*   RESPECIFY                                                   01S0046 00046000
*     R1P RSTD;                     /* RESTRICT R1.                  */ 00047000
*   APNPSPOL=APCTSP;                /* LOAD APCT SUBPOOL NUMBER      */ 00048000
         MVI   APNPSPOL(APNPPTR),X'E4'                          01S0047 00049000
*   APNPLEN=APOTCTLN;               /* LOAD APCT LENGTH              */ 00050000
         MVC   APNPLEN(3,APNPPTR),APOTCTLN+1(APOTPTR)           01S0048 00051000
*   R0P=APNPCORE;                   /* GET APCT SUBPOOL AND LENGTH   */ 00052000
         L     R0P,APNPCORE(,APNPPTR)                           01S0049 00053000
*   GENERATE;                                                   01S0050 00054000
*                                   /*                                  00055000
     GETMAIN R,LV=(0)                  OBTAIN CORE FOR THE APCT         00056000
*   APNPAPCT=R1P;                   /* SAVE THE ADDRESS OF THE APCT  */ 00057000
         ST    R1P,APNPAPCT(,APNPPTR)                           01S0051 00058000
*   APCTPTR=R1P;                    /* SAVE THE ADDRESS OF THE APCT  */ 00059000
         LR    APCTPTR,R1P                                      01S0052 00060000
*   GEN(MODESET EXTKEY=ZERO)                                    01S0053 00061000
*   NOREFS NOSETS;                  /* KEY ZERO FOR SSCVT            */ 00062000
         MODESET EXTKEY=ZERO                                            00063000
*   SSCTSUSE=APCTPTR;               /* PLACE THE ADDRESS OF THE APCT    00064000
*                                      IN THE SSCVT.                 */ 00065000
         L     @11,SSCTPTR(,APNPPTR)                            01S0054 00066000
         ST    APCTPTR,SSCTSUSE(,@11)                           01S0054 00067000
*   GEN(MODESET EXTKEY=SCHED)                                   01S0055 00068000
*   NOREFS NOSETS;                  /* SCHEDULER KEY 1               */ 00069000
         MODESET EXTKEY=SCHED                                           00070000
*   RESPECIFY                                                   01S0056 00071000
*     R0P;                          /* RELEASE R0.                   */ 00072000
*   RESPECIFY                                                   01S0057 00073000
*     R1P;                          /* RELEASE R1.                   */ 00074000
*                                                               01S0057 00075000
*   /*****************************************************************/ 00076000
*   /*                                                               */ 00077000
*   /* MOVE SKELETON APCT TO REAL APCT                               */ 00078000
*   /*                                                               */ 00079000
*   /*****************************************************************/ 00080000
*                                                               01S0058 00081000
*   RESPECIFY                                                   01S0058 00082000
*     R2P RSTD;                     /* RESTRICT R2.                  */ 00083000
*   RESPECIFY                                                   01S0059 00084000
*     R3P RSTD;                     /* RESTRICT R3.                  */ 00085000
*   RESPECIFY                                                   01S0060 00086000
*     R4P RSTD;                     /* RESTRICT R4.                  */ 00087000
*   RESPECIFY                                                   01S0061 00088000
*     R5P RSTD;                     /* RESTRICT R5.                  */ 00089000
*   R2P=APCTPTR;                    /* LOAD THE ADDRESS OF THE APCT  */ 00090000
         LR    R2P,APCTPTR                                      01S0062 00091000
*   R4P=APOTCTAD;                   /* LOAD THE ADDRESS OF THE APCT     00092000
*                                      SKELETON.                     */ 00093000
         L     R4P,APOTCTAD(,APOTPTR)                           01S0063 00094000
*   R3P=APOTCTLN;                   /* LOAD THE LENGTH OF THE APCT   */ 00095000
         L     @11,APOTCTLN(,APOTPTR)                           01S0064 00096000
         LR    R3P,@11                                          01S0064 00097000
*   R5P=APOTCTLN;                   /* LOAD LENGTH AGAIN FOR MOVE OF    00098000
*                                      SKELETON TO REAL CORE.        */ 00099000
         LR    R5P,@11                                          01S0065 00100000
*   GENERATE REFS(R2P,R3P,R4P,R5P);                             01S0066 00101000
*                                   /*                                  00102000
     MVCL  R2P,R4P                     MOVE THE SKELETON APCT TO ITS   X00103000
                                       REAL LOCATION.                */ 00104000
*   RESPECIFY                                                   01S0067 00105000
*     R2P;                          /* RELEASE R2.                   */ 00106000
*   RESPECIFY                                                   01S0068 00107000
*     R3P;                          /* RELEASE R3.                   */ 00108000
*   RESPECIFY                                                   01S0069 00109000
*     R4P;                          /* RELEASE R4.                   */ 00110000
*   RESPECIFY                                                   01S0070 00111000
*     R5P;                          /* RELEASE R5.                   */ 00112000
*                                                               01S0070 00113000
*   /*****************************************************************/ 00114000
*   /*                                                               */ 00115000
*   /* PLACE SUBSTSTEM NAME IN APCT                                  */ 00116000
*   /*                                                               */ 00117000
*   /*****************************************************************/ 00118000
*                                                               01S0071 00119000
*   RESPECIFY                                                   01S0071 00120000
*     APCT BASED(APCTPTR);          /* POINT TO APCT                 */ 00121000
*   APCTSSID=APNPSNAM;              /* PLACE SUBSYSTEM NAME IN APCT  */ 00122000
*                                                               01S0072 00123000
         MVC   APCTSSID(4,APCTPTR),APNPSNAM(APNPPTR)            01S0072 00124000
*   /*****************************************************************/ 00125000
*   /*                                                               */ 00126000
*   /* PROCESS THE RELOCATION TABLE                                  */ 00127000
*   /*                                                               */ 00128000
*   /*****************************************************************/ 00129000
*                                                               01S0073 00130000
*   RESPECIFY                                                   01S0073 00131000
*     OFFSET BASED(APCTPTR+CTIN);   /* POINT TO APCT                 */ 00132000
*   RESPECIFY                                                   01S0074 00133000
*     APOTAD BASED(APOTADAD+ADADIN);/* POINT TO RELOCALTION TABLE.   */ 00134000
*   ADADIN=APNPZERO;                /* INSURE INDEX IS ZERO          */ 00135000
         SLR   ADADIN,ADADIN                                    01S0075 00136000
*   DO I=1 TO APOTADCT;             /* FOR EACH ENTRY IN THE    01S0076 00137000
*                                      RELOCATION TABLE.             */ 00138000
         LA    I,1                                              01S0076 00139000
         B     @DE00076                                         01S0076 00140000
@DL00076 DS    0H                                               01S0077 00141000
*     CTIN=APOTADOF;                /* LOAD THE OFFSET INTO THE APCT.*/ 00142000
         LR    @11,ADADIN                                       01S0077 00143000
         AL    @11,APOTADAD(,APOTPTR)                           01S0077 00144000
         LH    CTIN,APOTADOF(,@11)                              01S0077 00145000
*     OFFADDR=OFFADDR+APCTPTR;      /* ADD BASE ADDRESS OF APCT TO      00146000
*                                      THE RELATIVE OFFSET POINTED TO   00147000
*                                      BY THE OFFSET IN THE     01S0078 00148000
*                                      RELOCATION TABLE.             */ 00149000
         LR    @11,APCTPTR                                      01S0078 00150000
         ALR   @11,CTIN                                         01S0078 00151000
         LR    @15,APCTPTR                                      01S0078 00152000
         AL    @15,OFFADDR(,@11)                                01S0078 00153000
         ST    @15,OFFADDR(,@11)                                01S0078 00154000
*     ADADIN=ADADIN+APOTADLN;       /* POINT TO THE NEXT ENTRY IN THE   00155000
*                                      RELOCATION TABLE.             */ 00156000
         AL    ADADIN,@CF00164                                  01S0079 00157000
*   END;                                                        01S0080 00158000
*                                                               01S0080 00159000
         AL    I,@CF00075                                       01S0080 00160000
@DE00076 C     I,APOTADCT(,APOTPTR)                             01S0080 00161000
         BNH   @DL00076                                         01S0080 00162000
*   /*****************************************************************/ 00163000
*   /*                                                               */ 00164000
*   /* CALL IGTA22 TO INITIALIZE APCT                                */ 00165000
*   /*                                                               */ 00166000
*   /*****************************************************************/ 00167000
*                                                               01S0081 00168000
*   CALL IGTA22(APNP);              /* GO INITIALIZE APCT.           */ 00169000
         ST    APNPPTR,@AL00001                                 01S0081 00170000
         L     @15,@CV00041                                     01S0081 00171000
         LA    @01,@AL00001                                     01S0081 00172000
         BALR  @14,@15                                          01S0081 00173000
*   RESPECIFY                                                   01S0082 00174000
*     R15P RSTD;                    /* RESTRICT R15.                 */ 00175000
*                                                               01S0082 00176000
*   /*****************************************************************/ 00177000
*   /*                                                               */ 00178000
*   /* IF IGTA22 SUCESSFUL INITIALIZE APCP                           */ 00179000
*   /*                                                               */ 00180000
*   /*****************************************************************/ 00181000
*                                                               01S0083 00182000
*   IF R15P=APNPZERO THEN           /* APCT INITIALIZATION WAS OK    */ 00183000
         LTR   R15P,R15P                                        01S0083 00184000
         BNZ   @RF00083                                         01S0083 00185000
*     DO;                                                       01S0084 00186000
*       RESPECIFY                                               01S0085 00187000
*         R15P;                     /* RELEASE R15.                  */ 00188000
*       RESPECIFY                                               01S0086 00189000
*         APCP BASED(APCPPTR);      /* POINT TO APCP.                */ 00190000
*       RESPECIFY                                               01S0087 00191000
*         APCPPTR RSTD;             /* RESTRICT TO APOT.             */ 00192000
*                                                               01S0087 00193000
*       /*************************************************************/ 00194000
*       /*                                                           */ 00195000
*       /* DETERMINE SIZE OF APCP                                    */ 00196000
*       /*                                                           */ 00197000
*       /*************************************************************/ 00198000
*                                                               01S0088 00199000
*       APNPCPSZ=APOTCPCT*APOTCPLN+APOTCPPL;/* CALCULATE SIZE.       */ 00200000
*                                                               01S0088 00201000
         L     @01,APOTCPCT(,APOTPTR)                           01S0088 00202000
         M     @00,APOTCPLN(,APOTPTR)                           01S0088 00203000
         AL    @01,APOTCPPL(,APOTPTR)                           01S0088 00204000
         ST    @01,APNPCPSZ(,APNPPTR)                           01S0088 00205000
*       /*************************************************************/ 00206000
*       /*                                                           */ 00207000
*       /* OBTAIN CORE FOR THE APCP                                  */ 00208000
*       /*                                                           */ 00209000
*       /*************************************************************/ 00210000
*                                                               01S0089 00211000
*       RESPECIFY                                               01S0089 00212000
*         R0P RSTD;                 /* RESTRICT R0.                  */ 00213000
*       RESPECIFY                                               01S0090 00214000
*         R1P RSTD;                 /* RESTRICT R1.                  */ 00215000
*       APNPSPOL=APCPSP;            /* LOAD APCP SUBPOOL NUMBER.     */ 00216000
         MVI   APNPSPOL(APNPPTR),X'F1'                          01S0091 00217000
*       APNPLEN=APNPCPSZ;           /* LOAD THE LENGTH OF APCP.      */ 00218000
         L     @11,APNPCPSZ(,APNPPTR)                           01S0092 00219000
         STCM  @11,7,APNPLEN(APNPPTR)                           01S0092 00220000
*       R0P=APNPCORE;               /* LOAD APCP LENGTH AND SUBPOOL. */ 00221000
         L     R0P,APNPCORE(,APNPPTR)                           01S0093 00222000
*       GENERATE;                                               01S0094 00223000
*                                   /*                                  00224000
         GETMAIN R,LV=(R0P)            GET CORE FOR  APCP               00225000
*       APNPAPCP=R1P;               /* SAVE THE ADDRESS OF APCP.     */ 00226000
         ST    R1P,APNPAPCP(,APNPPTR)                           01S0095 00227000
*       APCTAPCP=R1P;               /* SAVE THE ADDRESS OF APCP.     */ 00228000
         ST    R1P,APCTAPCP(,APCTPTR)                           01S0096 00229000
*       APCPPTR=R1P;                /* SAVE THE ADDRESS OF APCP.     */ 00230000
         LR    APCPPTR,R1P                                      01S0097 00231000
*       RESPECIFY                                               01S0098 00232000
*         R0P;                      /* RELEASE R0.                   */ 00233000
*       RESPECIFY                                               01S0099 00234000
*         R1P;                      /* RELEASE R1.                   */ 00235000
*                                                               01S0099 00236000
*       /*************************************************************/ 00237000
*       /*                                                           */ 00238000
*       /* MOVE SKELETON APCP TO REAL APCP                           */ 00239000
*       /*                                                           */ 00240000
*       /*************************************************************/ 00241000
*                                                               01S0100 00242000
*       RESPECIFY                                               01S0100 00243000
*         R2P RSTD;                 /* RESTRICT R2.                  */ 00244000
*       RESPECIFY                                               01S0101 00245000
*         R3P RSTD;                 /* RESTRICT R3.                  */ 00246000
*       RESPECIFY                                               01S0102 00247000
*         R4P RSTD;                 /* RESTRICT R4P.                 */ 00248000
*       RESPECIFY                                               01S0103 00249000
*         R5P RSTD;                 /* RESTRICT R5P.                 */ 00250000
*       R2P=APCPPTR;                /* LOAD ADDRESS OF REAL APCP.    */ 00251000
         LR    R2P,APCPPTR                                      01S0104 00252000
*       R4P=APOTCPAD;               /* LOAD THE SKELETON APCP ADDRESS*/ 00253000
         L     R4P,APOTCPAD(,APOTPTR)                           01S0105 00254000
*       R3P=APNPCPSZ;               /* LOAD THE LENGTH OF THE APCP.  */ 00255000
         L     @11,APNPCPSZ(,APNPPTR)                           01S0106 00256000
         LR    R3P,@11                                          01S0106 00257000
*       R5P=APNPCPSZ;               /* LOAD THE LENGTH OF THE APCP.  */ 00258000
         LR    R5P,@11                                          01S0107 00259000
*       GENERATE REFS(R2P,R3P,R4P,R5P);                         01S0108 00260000
*                                        /*                             00261000
         MVCL R2P,R4P                  MOVE SKELETON TO REAL CORE       00262000
*       RESPECIFY                                               01S0109 00263000
*         R2P;                      /* RELEASE R2.                   */ 00264000
*       RESPECIFY                                               01S0110 00265000
*         R3P;                      /* RELEASE R3.                   */ 00266000
*       RESPECIFY                                               01S0111 00267000
*         R4P;                      /* RELEASE R4.                   */ 00268000
*       RESPECIFY                                               01S0112 00269000
*         R5P;                      /* RELEASE R5.                   */ 00270000
*                                                               01S0112 00271000
*       /*************************************************************/ 00272000
*       /*                                                           */ 00273000
*       /* CALL IGTA60 TO INITIALIZE SSVT                            */ 00274000
*       /*                                                           */ 00275000
*       /*************************************************************/ 00276000
*                                                               01S0113 00277000
*       CALL IGTA60(APNP);          /* GO CREATE AND INITIALIZE SSVT.*/ 00278000
         ST    APNPPTR,@AL00001                                 01S0113 00279000
         L     @15,@CV00042                                     01S0113 00280000
         LA    @01,@AL00001                                     01S0113 00281000
         BALR  @14,@15                                          01S0113 00282000
*       RESPECIFY                                               01S0114 00283000
*         R15P RSTD;                /* RESTRICT R15.                 */ 00284000
*       IF R15P=APNPZERO THEN       /* THE SSVT WAS INITIALIZED OK.  */ 00285000
         LTR   R15P,R15P                                        01S0115 00286000
         BNZ   @RF00115                                         01S0115 00287000
*         DO;                                                   01S0116 00288000
*           APNPSTAT=APNPOFF;       /* INDICATE ALL INITIALIZATIO OK.*/ 00289000
         NI    APNPSTAT(APNPPTR),B'11110111'                    01S0117 00290000
*         END;                                                  01S0118 00291000
*                                                               01S0118 00292000
*       /*************************************************************/ 00293000
*       /*                                                           */ 00294000
*       /* FREE THE APCP                                             */ 00295000
*       /*                                                           */ 00296000
*       /*************************************************************/ 00297000
*                                                               01S0119 00298000
*       ELSE                                                    01S0119 00299000
*         DO;                                                   01S0119 00300000
         B     @RC00115                                         01S0119 00301000
@RF00115 DS    0H                                               01S0120 00302000
*           APNPSTAT=APNPON;        /* INDICATE INITIALIZATION FAILED*/ 00303000
         OI    APNPSTAT(APNPPTR),B'00001000'                    01S0120 00304000
*           RESPECIFY                                           01S0121 00305000
*             R0P RSTD;             /* RESTRICT R0.                  */ 00306000
*           RESPECIFY                                           01S0122 00307000
*             R1P RSTD;             /* RESTRICT R1.                  */ 00308000
*           APNPSPOL=APCPSP;        /* LOAD APCP SUBPOOL NUMBER.     */ 00309000
         MVI   APNPSPOL(APNPPTR),X'F1'                          01S0123 00310000
*           APNPLEN=APNPCPSZ;       /* LOAD APCP LENGTH.             */ 00311000
         MVC   APNPLEN(3,APNPPTR),APNPCPSZ+1(APNPPTR)           01S0124 00312000
*           R0P=APNPCORE;           /* LOAD APCP LENGTH AND SUBPOOL. */ 00313000
         L     R0P,APNPCORE(,APNPPTR)                           01S0125 00314000
*           R1P=APNPAPCP;           /* LOAD THE ADDRESS OF APCP.     */ 00315000
         L     R1P,APNPAPCP(,APNPPTR)                           01S0126 00316000
*           GENERATE;                                           01S0127 00317000
*                                   /*                                  00318000
             FREEMAIN R,LV=(R0P),A=(R1P)  FREE THE APCP                 00319000
*           RESPECIFY                                           01S0128 00320000
*             R0P;                  /* RELEASE R0.                   */ 00321000
*           RESPECIFY                                           01S0129 00322000
*             R1P;                  /* RELEASE R1.                   */ 00323000
*         END;                                                  01S0130 00324000
*     END;                                                      01S0131 00325000
*   ELSE                                                        01S0132 00326000
*     DO;                                                       01S0132 00327000
         B     @RC00083                                         01S0132 00328000
@RF00083 DS    0H                                               01S0133 00329000
*       APNPSTAT=APNPON;            /* INDICATE INITIALIZATION FAILED*/ 00330000
         OI    APNPSTAT(APNPPTR),B'00001000'                    01S0133 00331000
*     END;                                                      01S0134 00332000
*                                                               01S0134 00333000
*   /*****************************************************************/ 00334000
*   /*                                                               */ 00335000
*   /* CHECK TO SEE IF AN ERROR HAS OCCURRED                         */ 00336000
*   /*                                                               */ 00337000
*   /*****************************************************************/ 00338000
*                                                               01S0135 00339000
*   RESPECIFY                                                   01S0135 00340000
*     APNPRTRG RSTD;                /* RESTRICT RETURN REGISTER.     */ 00341000
@RC00083 DS    0H                                               01S0136 00342000
*   IF APNPSTAT=APNPOFF THEN        /* THE APCT IS THUS FAR OK.      */ 00343000
         TM    APNPSTAT(APNPPTR),B'00001000'                    01S0136 00344000
         BNZ   @RF00136                                         01S0136 00345000
*     DO;                                                       01S0137 00346000
*       APNPRTRG=APNPZERO;          /* SET GOOD RETURN CODE.         */ 00347000
         SLR   APNPRTRG,APNPRTRG                                01S0138 00348000
*     END;                                                      01S0139 00349000
*                                                               01S0139 00350000
*   /*****************************************************************/ 00351000
*   /*                                                               */ 00352000
*   /* FREE THE APCT                                                 */ 00353000
*   /*                                                               */ 00354000
*   /*****************************************************************/ 00355000
*                                                               01S0140 00356000
*   ELSE                                                        01S0140 00357000
*     DO;                                                       01S0140 00358000
         B     @RC00136                                         01S0140 00359000
@RF00136 DS    0H                                               01S0141 00360000
*       RESPECIFY                                               01S0141 00361000
*         R0P RSTD;                 /* RESTRICT R0.                  */ 00362000
*       RESPECIFY                                               01S0142 00363000
*         R1P RSTD;                 /* RESTRICT R1.                  */ 00364000
*       APNPSPOL=APCTSP;            /* INSERT APCT SUBPOOL NUMBER.   */ 00365000
         MVI   APNPSPOL(APNPPTR),X'E4'                          01S0143 00366000
*       APNPLEN=APOTCTLN;           /* INSERT APCT LENGTH            */ 00367000
         MVC   APNPLEN(3,APNPPTR),APOTCTLN+1(APOTPTR)           01S0144 00368000
*       R0P=APNPCORE;               /* LOAD APCT LENGTH AND SUBPOOL  */ 00369000
         L     R0P,APNPCORE(,APNPPTR)                           01S0145 00370000
*       R1P=APNPAPCT;               /* LOAD START ADDRESS OF APCT    */ 00371000
         L     R1P,APNPAPCT(,APNPPTR)                           01S0146 00372000
*       GENERATE;                                               01S0147 00373000
*                                   /*                                  00374000
         FREEMAIN R,LV=(0),A=(1)       FREE THE APCT                    00375000
*       RESPECIFY                                               01S0148 00376000
*         R0P;                      /* RELEASE R0.                   */ 00377000
*       RESPECIFY                                               01S0149 00378000
*         R1P;                      /* RELEASE R1.                   */ 00379000
*       APNPRTRG=APNPEROR;          /* SET ERROR RETURN CODE.        */ 00380000
*                                                               01S0150 00381000
         LA    APNPRTRG,8                                       01S0150 00382000
*       /*************************************************************/ 00383000
*       /*                                                           */ 00384000
*       /* DEACTIVATE SUBSYSTEM                                      */ 00385000
*       /*                                                           */ 00386000
*       /*************************************************************/ 00387000
*                                                               01S0151 00388000
*       GEN(MODESET EXTKEY=ZERO)                                01S0151 00389000
*       NOREFS NOSETS;              /* KEY ZERO FOR SSCVT            */ 00390000
         MODESET EXTKEY=ZERO                                            00391000
*       SSCTSUSE=APNPZERO;          /* ZERO POINTER TO APCT          */ 00392000
         SLR   @14,@14                                          01S0152 00393000
         L     @05,SSCTPTR(,APNPPTR)                            01S0152 00394000
         ST    @14,SSCTSUSE(,@05)                               01S0152 00395000
*       SSCTSSVT=APNPZERO;          /* ZERO POINTER TO SSVT TO  01S0153 00396000
*                                      INDICATE SUBSYSTEM NO ACTIVE. */ 00397000
         ST    @14,SSCTSSVT(,@05)                               01S0153 00398000
*       GEN(MODESET EXTKEY=SCHED)                               01S0154 00399000
*       NOREFS NOSETS;              /* SCHEDULER KEY 1               */ 00400000
         MODESET EXTKEY=SCHED                                           00401000
*     END;                                                      01S0155 00402000
*                                                               01S0155 00403000
*   /*****************************************************************/ 00404000
*   /*                                                               */ 00405000
*   /* RETURN TO CALLER                                              */ 00406000
*   /*                                                               */ 00407000
