*       GEN(MODESET EXTKEY=SCHED)                               01S0099 00261000
*       NOREFS NOSETS;              /* KEY ONE FOR SCHEDULER         */ 00262000
         MODESET EXTKEY=SCHED                                           00263000
*       RESPECIFY                                               01S0100 00264000
*         SSCT;                     /* RELEASE THE SSCVT.            */ 00265000
*     END;                                                      01S0101 00266000
*   ELSE                                                        01S0102 00267000
