*     APCTPTR RSTD;                 /* RESTRICT R12 TO APCT.         */ 00030000
*   APCTPTR=APNPAPCT;               /* LOAD THE ADDRESS OF APCT.     */ 00031000
         L     APCTPTR,APNPAPCT(,APNPPTR)                       01S0040 00032000
*   RESPECIFY                                                   01S0041 00033000
*     APOT BASED(APOTPTR);          /* POINT TO APOT DSECT           */ 00034000
*   RESPECIFY                                                   01S0042 00035000
*     APOTPTR RSTD;                 /* RESTRICT POINTER TO APOT.     */ 00036000
*   APOTPTR=APNPAPOT;               /* LOAD THE ADDRESS OF APOT.     */ 00037000
         L     APOTPTR,APNPAPOT(,APNPPTR)                       01S0043 00038000
*   APNPSTAT=APNPOFF;               /* INITIALIZE MESSAGE STATUS = 0 */ 00039000
*                                                               01S0044 00040000
         NI    APNPSTAT(APNPPTR),B'11110111'                    01S0044 00041000
*   /*****************************************************************/ 00042000
*   /*                                                               */ 00043000
*   /* OBTAIN CORE FOR THE SSVT                                      */ 00044000
*   /*                                                               */ 00045000
*   /*****************************************************************/ 00046000
*                                                               01S0045 00047000
*   APNPVTSZ=SSVTSIZE+(APOTVTCT*4); /* CALULATE SIZE OF SSVT         */ 00048000
         L     @15,APOTVTCT(,APOTPTR)                           01S0045 00049000
         SLA   @15,2                                            01S0045 00050000
         AL    @15,@CF00043                                     01S0045 00051000
         ST    @15,APNPVTSZ(,APNPPTR)                           01S0045 00052000
*   RESPECIFY                                                   01S0046 00053000
*     R0P RSTD;                     /* RESTRICT R0.                  */ 00054000
*   RESPECIFY                                                   01S0047 00055000
*     R1P RSTD;                     /* RESTRICT R1.                  */ 00056000
*   APNPSPOL=SSVTSP;                /* LOAD SSVT SUBPOOL NUMBER      */ 00057000
         MVI   APNPSPOL(APNPPTR),X'F5'                          01S0048 00058000
*   APNPLEN=APNPVTSZ;               /* LOAD SSVT LENGTH              */ 00059000
         STCM  @15,7,APNPLEN(APNPPTR)                           01S0049 00060000
*   R0P=APNPCORE;                   /* LOAD SSVT SUBPOOL AND LENGTH  */ 00061000
         L     R0P,APNPCORE(,APNPPTR)                           01S0050 00062000
*   GENERATE;                                                   01S0051 00063000
*                                   /*                                  00064000
     GETMAIN R,LV=(0)                  OBTAIN CORE FOR SSVT             00065000
*   SSVTPTR=R1P;                    /* LOAD THE ADDRESS OF THE SSVT  */ 00066000
         ST    R1P,SSVTPTR(,APNPPTR)                            01S0052 00067000
*   RESPECIFY                                                   01S0053 00068000
*     R0P;                          /* RELEASE R0.                   */ 00069000
*   RESPECIFY                                                   01S0054 00070000
*     R1P;                          /* RELEASE R1.                   */ 00071000
*   GEN(MODESET EXTKEY=ZERO)                                    01S0055 00072000
