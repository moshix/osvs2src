*                                      TABLE                         */ 00096000
*   DO I=1 TO APOTVTCT;             /* FOR THE NUMBER OF ENTRIES IN     00097000
*                                      THE SSVT INITIALIZATION TABLE */ 00098000
         LA    I,1                                              01S0061 00099000
         B     @DE00061                                         01S0061 00100000
@DL00061 DS    0H                                               01S0062 00101000
*     IF APOTVTCD^=APNPZO THEN      /* MUST BE A VALID CODE          */ 00102000
         L     @03,APNPVTAD(,APNPPTR)                           01S0062 00103000
         CLI   APOTVTCD(@03),0                                  01S0062 00104000
         BE    @RF00062                                         01S0062 00105000
*       DO;                                                     01S0063 00106000
*         GEN(MODESET EXTKEY=ZERO)                              01S0064 00107000
