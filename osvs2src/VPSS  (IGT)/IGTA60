         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
         MVC   @PC00001(4),0(@01)                               01S0001 00022000
*   APNPPTR=ADDR(PARMAPNP);         /* LOAD THE ADDRESS OF APNP.     */ 00023000
         L     APNPPTR,@PC00001                                 01S0036 00024000
*   RESPECIFY                                                   01S0037 00025000
*     APNQ BASED(APNQPTR);          /* POINT TO APNQ DSECT           */ 00026000
*   RESPECIFY                                                   01S0038 00027000
*     APCT BASED(APCTPTR);          /* POINT TO APCT DSECT           */ 00028000
