TD00     TITLE 'IGTD00 - MONITOR MAIN ROUTINE.                         *00001000
                        '                                               00002000
IGTD00   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTD00    77.092'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
         MVC   @PC00001(4),0(@01)                               01S0001 00022000
*   R2P=R1P;                        /* SAVE THE POINTER TO THE INPUT.*/ 00023000
         LR    R2P,R1P                                          01S0037 00024000
*                                                               02S0038 00025000
*/********************************************************************/ 00026000
*/*  ESTABLISH THE ERROR RECOVERY ENVIRONMENT.                       */ 00027000
*/********************************************************************/ 00028000
*                                                               02S0038 00029000
*   R15F=0;                         /* SET RETURN CODE.              */ 00030000
         SLR   R15F,R15F                                        02S0038 00031000
*   IF R15P=0 THEN                  /* TEST FOR ERROR RECOVERY  01S0039 00032000
*                                      ESTABLISHED.                  */ 00033000
         LTR   R15P,R15P                                        01S0039 00034000
         BNZ   @RF00039                                         01S0039 00035000
*     DO;                           /* IF ESTAE OR STAE OK           */ 00036000
*                                                               02S0041 00037000
*/********************************************************************/ 00038000
*/*                 VALIDATE INPUT PARAMETERS.                       */ 00039000
*/********************************************************************/ 00040000
*                                                               02S0041 00041000
*       RESPECIFY                                               02S0041 00042000
*        (R4F) RESTRICTED;                                      02S0041 00043000
*       R4F=0;                      /* INITIALIZE RETURN CODE.       */ 00044000
         SLR   R4F,R4F                                          02S0042 00045000
*/* INPDATA = ID,SUBS,COLD                                           */ 00046000
*/* ID = (00,99) INITIAL COMMAND SET ID.                             */ 00047000
*/* SUBS = 1 TO 4 CHAR SUBSYSTEM ID.                                 */ 00048000
*/* COLD = OPTIONAL FORCE COLD START INDICATOR.                      */ 00049000
*                                                               02S0043 00050000
*       IF INPLEN<4|INPLEN>12 THEN  /* TEST MIN AND MAX VALS FOR        00051000
*                                      LENGTH.                       */ 00052000
         L     @05,@PC00001                                     02S0043 00053000
         LH    @05,INPLEN(,@05)                                 02S0043 00054000
         C     @05,@CF00055                                     02S0043 00055000
         BL    @RT00043                                         02S0043 00056000
         C     @05,@CF00542                                     02S0043 00057000
         BNH   @RF00043                                         02S0043 00058000
@RT00043 DS    0H                                               02S0044 00059000
*         R4F=4;                    /* SET INVALID INPUT RETURN CODE */ 00060000
         LA    R4F,4                                            02S0044 00061000
*       ELSE                                                    02S0045 00062000
*         DO;                                                   02S0045 00063000
         B     @RC00043                                         02S0045 00064000
@RF00043 DS    0H                                               02S0046 00065000
*           MEMBID='        ';      /* INITIALIZE THE COMMAND SET       00066000
*                                      MEMBER NAME.                  */ 00067000
         MVC   MEMBID(8),@CC00543                               02S0046 00068000
*           IF INPDATA(3)^=',' THEN /* TEST FOR DELIMITER.           */ 00069000
         L     @05,@PC00001                                     02S0047 00070000
         CLI   INPDATA+2(@05),C','                              02S0047 00071000
         BE    @RF00047                                         02S0047 00072000
*             R4F=4;                /* SET INVALID INPUT RETURN CODE.*/ 00073000
         LA    R4F,4                                            02S0048 00074000
*           ELSE                                                02S0049 00075000
*             DO;                                               02S0049 00076000
         B     @RC00047                                         02S0049 00077000
@RF00047 DS    0H                                               02S0050 00078000
*               MAJORNM(1:4)='SYSZ';/* INITIALIZE THE MAJOR NAME        00079000
*                                      FIELD.                        */ 00080000
         MVC   MAJORNM(4),@CC00545                              02S0050 00081000
*               DO R3F=4 TO INPLEN BY 1/* SCAN THE SUBSYSTEM NAME       00082000
*                                      FIELD.                        */ 00083000
*                     WHILE INPDATA(R3F)^=','/* QUIT WHEN DELIMITER     00084000
*                                      IS FOUND.                     */ 00085000
*                     &R3F<8;       /* OR WHEN 4 CHARS MOVED    02S0051 00086000
*                                      (SUBSYTEM NAME IS FOUR CHARS     00087000
*                                      OR LESS).                     */ 00088000
         LA    R3F,4                                            02S0051 00089000
         B     @DE00051                                         02S0051 00090000
@DL00051 L     @05,@PC00001                                     02S0051 00091000
         LR    @01,@05                                          02S0051 00092000
         ALR   @01,R3F                                          02S0051 00093000
         CLI   INPDATA-1(@01),C','                              02S0051 00094000
         BE    @DC00051                                         02S0051 00095000
         C     R3F,@CF00059                                     02S0051 00096000
         BNL   @DC00051                                         02S0051 00097000
*                 MAJORNM(1+R3F)=INPDATA(R3F);/* MOVE DATA TO MAJOR     00098000
*                                      NAME.                         */ 00099000
         LA    @01,MAJORNM(R3F)                                 02S0052 00100000
         ALR   @05,R3F                                          02S0052 00101000
         MVC   0(1,@01),INPDATA-1(@05)                          02S0052 00102000
*               END;                                            02S0053 00103000
         AL    R3F,@CF00052                                     02S0053 00104000
@DE00051 L     @05,@PC00001                                     02S0053 00105000
         CH    R3F,INPLEN(,@05)                                 02S0053 00106000
         BNH   @DL00051                                         02S0053 00107000
@DC00051 DS    0H                                               02S0054 00108000
*               MEMBID(1:R3F-4)=MAJORNM(5:R3F);/* MOVE IN SUBSYS        00109000
*                                      NAME.                         */ 00110000
         LR    @05,R3F                                          02S0054 00111000
         SL    @05,@CF00204                                     02S0054 00112000
         EX    @05,@SM00662                                     02S0054 00113000
*               MEMBID(R3F-3)='I';  /* NEXT CHAR IS 'I'              */ 00114000
         LA    @05,MEMBID-4(R3F)                                02S0055 00115000
         MVI   0(@05),C'I'                                      02S0055 00116000
*               MEMBID(R3F-2:R3F-1)=INPDATA(1:2);/* GET UNIQUE ID       00117000
*                                      FIELD.                        */ 00118000
         LA    @05,MEMBID-3(R3F)                                02S0056 00119000
         L     @15,@PC00001                                     02S0056 00120000
         MVC   0(2,@05),INPDATA(@15)                            02S0056 00121000
*               IF R3F^=INPLEN+1 THEN/* TEST FOR OPTIONAL PARAMETER. */ 00122000
         LA    @05,1                                            02S0057 00123000
         AH    @05,INPLEN(,@15)                                 02S0057 00124000
         CR    R3F,@05                                          02S0057 00125000
         BE    @RF00057                                         02S0057 00126000
*                 IF INPDATA(R3F)^=',' THEN/* TEST FOR VALID    02S0058 00127000
*                                      DELIMITER                     */ 00128000
         ALR   @15,R3F                                          02S0058 00129000
         CLI   INPDATA-1(@15),C','                              02S0058 00130000
         BE    @RF00058                                         02S0058 00131000
*                   R4F=4;          /* SET INVALID INPUT RETURN CODE.*/ 00132000
         LA    R4F,4                                            02S0059 00133000
*                 ELSE                                          02S0060 00134000
*                   IF INPDATA(R3F+1)='C' THEN                  02S0060 00135000
         B     @RC00058                                         02S0060 00136000
@RF00058 L     @05,@PC00001                                     02S0060 00137000
         ALR   @05,R3F                                          02S0060 00138000
         CLI   INPDATA(@05),C'C'                                02S0060 00139000
         BNE   @RF00060                                         02S0060 00140000
*                     COLDSTRT='1'B;/* FORCE COLD START IF PARM 02S0061 00141000
*                                      PRESENT                       */ 00142000
         OI    COLDSTRT,B'10000000'                             02S0061 00143000
*                   ELSE                                        02S0062 00144000
*                     IF INPDATA(R3F+1)='W' THEN/* CHECK FOR WARM       00145000
*                                      START.                        */ 00146000
         B     @RC00060                                         02S0062 00147000
@RF00060 L     @05,@PC00001                                     02S0062 00148000
         ALR   @05,R3F                                          02S0062 00149000
         CLI   INPDATA(@05),C'W'                                02S0062 00150000
         BNE   @RF00062                                         02S0062 00151000
*                       COLDSTRT='0'B;/* SET FORCE COLD NOT REQUIRED.*/ 00152000
         NI    COLDSTRT,B'01111111'                             02S0063 00153000
*                     ELSE                                      02S0064 00154000
*                       R4F=4;      /* SET INVALID INPUT RETURN CODE.*/ 00155000
         B     @RC00062                                         02S0064 00156000
@RF00062 LA    R4F,4                                            02S0064 00157000
*             END;                                              02S0065 00158000
@RC00062 DS    0H                                               02S0065 00159000
@RC00060 DS    0H                                               02S0065 00160000
@RC00058 DS    0H                                               02S0065 00161000
@RF00057 DS    0H                                               02S0066 00162000
*         END;                                                  02S0066 00163000
@RC00047 DS    0H                                               02S0067 00164000
*       IF R4F=0 THEN               /* IF ALL OK, GO FIND THE APCT.  */ 00165000
@RC00043 LTR   R4F,R4F                                          02S0067 00166000
         BNZ   @RF00067                                         02S0067 00167000
*         DO;                                                   02S0068 00168000
*                                                               02S0068 00169000
*           /*********************************************************/ 00170000
*           /*                                                       */ 00171000
*           /* FIND THE ADDRESS OF THE SSCVT FOR THIS SUBSYSTEM.     */ 00172000
*           /*                                                       */ 00173000
*           /*********************************************************/ 00174000
*                                                               02S0069 00175000
*           RESPECIFY                                           02S0069 00176000
*            (R15F,                                             02S0069 00177000
*             R1P) RESTRICTED;                                  02S0069 00178000
*           MFLEXT(1:12)=''X;       /* ZERO THE EXTRACT LIST AREA.   */ 00179000
         XC    MFLEXT(12),MFLEXT                                02S0070 00180000
*           GEN SETS(CSCBPTR)                                   02S0071 00181000
*               (EXTRACT CSCBPTR,'S',FIELDS=(COMM),MF=(E,MFLEXT) );     00182000
         EXTRACT CSCBPTR,'S',FIELDS=(COMM),MF=(E,MFLEXT)                00183000
*           CSCBPTR=CSCBPTR         /* COMPUTE ADDRESS OF THE CSCB.  */ 00184000
*               -(ADDR(CHECBP)-ADDR(CHPTR));                    02S0072 00185000
         L     @14,CSCBPTR                                      02S0072 00186000
         SL    @14,@CF00659                                     02S0072 00187000
         ST    @14,CSCBPTR                                      02S0072 00188000
*           R1P=ADDR(MAJORNM)+4;    /* POINT TO THE SUBSYSTEM NAME      00189000
*                                      FOR IGTUDA, WHICH WILL USE IT    00190000
*                                      TO FIND THE SSCVT FOR THE        00191000
*                                      SUBSYSTEM.                    */ 00192000
         LA    R1P,MAJORNM                                      02S0073 00193000
         LA    R1P,4(,R1P)                                      02S0073 00194000
*           GEN REFS(MAJORNM)(         LINK EP=IGTUDA     );    02S0074 00195000
                  LINK EP=IGTUDA                                        00196000
*           RESPECIFY                                           02S0075 00197000
*             R1P UNRESTRICTED;                                 02S0075 00198000
*           IF R15F^=0              /* NON ZERO MEANS IT FOUND AN       00199000
*                                      SSCVT                         */ 00200000
*               &R15P->SSCTSUSE->APCTID='APCT'/* VERIFY THAT IT IS AN   00201000
*                                      AUTHORIZED 3838 SUBSYSTEM.    */ 00202000
*               &CHKEY(1:4)=MAJORNM(5:8)/* SUBSYSTEM MUST BE         */ 00203000
*               &CHKEY(5:8)='    '  /* THE NAME OF THE JOB STARTED.  */ 00204000
*             THEN                                              02S0076 00205000
         LTR   R15F,R15F                                        02S0076 00206000
         BZ    @RF00076                                         02S0076 00207000
         L     @14,SSCTSUSE(,R15P)                              02S0076 00208000
         CLC   APCTID(4,@14),@CC00372                           02S0076 00209000
         BNE   @RF00076                                         02S0076 00210000
         L     @05,CSCBPTR                                      02S0076 00211000
         CLC   CHKEY(4,@05),MAJORNM+4                           02S0076 00212000
         BNE   @RF00076                                         02S0076 00213000
         CLC   CHKEY+4(4,@05),@CC00553                          02S0076 00214000
         BNE   @RF00076                                         02S0076 00215000
*             DO;                   /* GOT A GOOD APCT.              */ 00216000
*               APCTPTR=R15P->SSCTSUSE;/* GET POINTER TO THE APCT    */ 00217000
         LR    APCTPTR,@14                                      02S0078 00218000
*               APCTSSVT=R15P->SSCTSSVT;/* SET PTR TO SSVT.          */ 00219000
         L     @14,SSCTSSVT(,R15P)                              02S0079 00220000
         ST    @14,APCTSSVT(,APCTPTR)                           02S0079 00221000
*             END;                  /* DONE.                         */ 00222000
*           ELSE                                                02S0081 00223000
*             DO;                                               02S0081 00224000
         B     @RC00076                                         02S0081 00225000
@RF00076 DS    0H                                               02S0082 00226000
*               CALL IGTDWTO(IGT200I,MAJORNM(5:8));/* WRITE THE ERROR   00227000
*                                      MESSAGE TO THE OPERATOR.      */ 00228000
         LA    @14,IGT200I                                      02S0082 00229000
         ST    @14,@AL00001                                     02S0082 00230000
         LA    @14,MAJORNM+4                                    02S0082 00231000
         ST    @14,@AL00001+4                                   02S0082 00232000
         MVI   @AL00001+4,X'80'                                 02S0082 00233000
         LA    @01,@AL00001                                     02S0082 00234000
         BAL   @14,IGTDWTO                                      02S0082 00235000
*               R4F=8;              /* SET INVALID SUBSYS REQUESTED     00236000
*                                      CODE.                         */ 00237000
         LA    R4F,8                                            02S0083 00238000
*             END;                                              02S0084 00239000
*         END;                                                  02S0085 00240000
*       ELSE                        /* INVALID INPUT MESSAGE    02S0086 00241000
*                                      REQUIRED.                     */ 00242000
*         CALL IGTDWTO(IGT201I);    /* WRITE THE ERROR MESSAGE TO THE   00243000
*                                      OPERATOR.                     */ 00244000
         B     @RC00067                                         02S0086 00245000
@RF00067 LA    @01,@AL00086                                     02S0086 00246000
         BAL   @14,IGTDWTO                                      02S0086 00247000
*       R15F=R4F;                   /* SET RETURN CODE FOR MAIN 02S0087 00248000
*                                      ROUTINE.                      */ 00249000
@RC00067 LR    R15F,R4F                                         02S0087 00250000
*       RESPECIFY                                               02S0088 00251000
*        (R4F,                                                  02S0088 00252000
*         R15F) UNRESTRICTED;                                   02S0088 00253000
*       IF R15P=0 THEN              /* IF OK PROCEED                 */ 00254000
         LTR   R15P,R15P                                        01S0089 00255000
         BNZ   @RF00089                                         01S0089 00256000
*         DO;                                                   01S0090 00257000
*           DYNENQ(1:4)=''X;        /* CLEAR THE AREA.               */ 00258000
         XC    DYNENQ(4),DYNENQ                                 01S0091 00259000
*           GEN REFS(MAJORNM,MONITOR) SETS(DYNENQ);             01S0092 00260000
            MVI   DYNENQ,X'80'      SET THE VL INDICATOR.               00261000
            ENQ (MAJORNM,MONITOR,E,7,SYSTEM),RET=USE,MF=(E,DYNENQ)      00262000
*           R3P=R15P;               /* SAVE ADDR OF ENQ WORKAREA     */ 00263000
         LR    R3P,R15P                                         01S0093 00264000
*           IF R15P=0 THEN          /* IF WE GOT IT, PROCEED BECAUSE */ 00265000
*                                                               01S0094 00266000
         LTR   R15P,R15P                                        01S0094 00267000
         BNZ   @RF00094                                         01S0094 00268000
*             /*******************************************************/ 00269000
*             /*                                                     */ 00270000
*             /* WE ARE THE ONLY MONITOR.                            */ 00271000
*             /*                                                     */ 00272000
*             /*******************************************************/ 00273000
*                                                               01S0095 00274000
*             DO;                                               01S0095 00275000
*               IF COLDSTRT='1'B    /* SEE IF FORCE COLD START  01S0096 00276000
*                                      APPLICABLE                    */ 00277000
*                   &APCTWARM='1'B THEN/* WHICH IS IF WARM IS   01S0096 00278000
*                                      POSSIBLE                      */ 00279000
         TM    COLDSTRT,B'10000000'                             01S0096 00280000
         BNO   @RF00096                                         01S0096 00281000
         TM    APCTWARM(APCTPTR),B'00000100'                    01S0096 00282000
         BNO   @RF00096                                         01S0096 00283000
*                 DO;               /* BUT COLD IS FORCED.           */ 00284000
*                   GEN REFS(WTORL,WTORL2,WTORL2L);             01S0098 00285000
*                                          /*                           00286000
           MVC  WTORL(L'WTORL2L),WTORL2 MOVE THE MESSAGE TO OUTPUT AREA 00287000
*                   WTORL(21:24)=APCTSSID;/* PUT THE SUBSYSTEM NAME     00288000
*                                      IN THE MSG                    */ 00289000
         MVC   WTORL+20(4),APCTSSID(APCTPTR)                    01S0099 00290000
*                   DO UNTIL WTORRPLY(1)='N'/* MAKE THE OPERATOR        00291000
*                                      VERIFY THE COLD               */ 00292000
*                         |WTORRPLY(1)='Y'/* START REQUEST OR DENY      00293000
*                                      IT.                           */ 00294000
*                         |WTORRPLY(1)='U';/* U IS EQUIVALENT TO YES.*/ 00295000
@DL00100 DS    0H                                               01S0101 00296000
*                     WTORRPLY='';  /* BLANK THE INPUT AREA.         */ 00297000
         MVI   WTORRPLY,C' '                                    01S0101 00298000
         MVC   WTORRPLY+1(3),WTORRPLY                           01S0101 00299000
*                     WTORECB=''X;  /* ZERO THE ECB.                 */ 00300000
         SLR   @15,@15                                          01S0102 00301000
         ST    @15,WTORECB                                      01S0102 00302000
*                     GEN SETS(WTORRPLY,WTORECB);               01S0103 00303000
           WTOR ,WTORRPLY,3,WTORECB,MF=(E,WTORL) ASK HIM.               00304000
           WAIT ECB=WTORECB        WAIT FOR REPLY.                      00305000
*                   END;                                        01S0104 00306000
@DE00100 CLI   WTORRPLY,C'N'                                    01S0104 00307000
         BE    @DC00100                                         01S0104 00308000
         CLI   WTORRPLY,C'Y'                                    01S0104 00309000
         BE    @DC00100                                         01S0104 00310000
         CLI   WTORRPLY,C'U'                                    01S0104 00311000
         BNE   @DL00100                                         01S0104 00312000
@DC00100 DS    0H                                               01S0105 00313000
*                   IF WTORRPLY(1)='Y'|/* IF HE SAYS YES, DO A COLD     00314000
*                                      START.                        */ 00315000
*                       WTORRPLY(1)='U' THEN/* 'U' MEANS YES ALOS.   */ 00316000
         CLI   WTORRPLY,C'Y'                                    01S0105 00317000
         BE    @RT00105                                         01S0105 00318000
         CLI   WTORRPLY,C'U'                                    01S0105 00319000
         BNE   @RF00105                                         01S0105 00320000
@RT00105 DS    0H                                               01S0106 00321000
*                     DO;                                       01S0106 00322000
*                       CALL IGTDWTO(IGT208I,APCTSSID);/* WRITE 01S0107 00323000
*                                      CANCEL MSG.                   */ 00324000
         LA    @15,IGT208I                                      01S0107 00325000
         ST    @15,@AL00001                                     01S0107 00326000
         LA    @15,APCTSSID(,APCTPTR)                           01S0107 00327000
         ST    @15,@AL00001+4                                   01S0107 00328000
         MVI   @AL00001+4,X'80'                                 01S0107 00329000
         LA    @01,@AL00001                                     01S0107 00330000
         BAL   @14,IGTDWTO                                      01S0107 00331000
*                       APCTSTRT='1'B;/* TELL ALLOCATION WE ARE 01S0108 00332000
*                                      STARTING                      */ 00333000
*                                                               01S0108 00334000
         L     @15,APCTSTRT(,APCTPTR)                           01S0108 00335000
@GL00010 LR    @14,@15                                          01S0108 00336000
         O     @14,@CF00666                                     01S0108 00337000
         CS    @15,@14,APCTSTRT(APCTPTR)                        01S0108 00338000
         BNE   @GL00010                                         01S0108 00339000
*                       /*********************************************/ 00340000
*                       /*                                           */ 00341000
*                       /* WAIT FOR ALL USERS TO QUIT, SIGNIFIED BY  */ 00342000
*                       /* GETTING ENQED                             */ 00343000
*                       /*                                           */ 00344000
*                       /*********************************************/ 00345000
*                                                               01S0109 00346000
*                       DYNENQ(1:4)='80000000'X;/* CLEAR THE AREA.   */ 00347000
         MVC   DYNENQ(4),@CB00559                               01S0109 00348000
*                       GEN SETS(DYNENQ) REFS(ALLOCATE,MAJORNM);        00349000
             MVI   DYNENQ,X'80'      SET THE VL INDICATOR.              00350000
             ENQ (MAJORNM,ALLOCATE,E,8,SYSTEM),MF=(E,DYNENQ)            00351000
*                       APCTSTAT=APCTSTAT&'00'X;/* CLEAR THE START UP   00352000
*                                      INDICATORS.                   */ 00353000
         NI    APCTSTAT(APCTPTR),X'00'                          01S0111 00354000
*                       APCTSTRT='1'B;/* KEEP START UP ON.           */ 00355000
         L     @15,APCTSTRT(,APCTPTR)                           01S0112 00356000
@GL00011 LR    @14,@15                                          01S0112 00357000
         O     @14,@CF00666                                     01S0112 00358000
         CS    @15,@14,APCTSTRT(APCTPTR)                        01S0112 00359000
         BNE   @GL00011                                         01S0112 00360000
*                       DYNENQ(1:4)='80000000'X;/* CLEAR THE AREA.   */ 00361000
         MVC   DYNENQ(4),@CB00559                               01S0113 00362000
*                       GEN SETS(DYNENQ) REFS(ALLOCATE,MAJORNM);        00363000
             DEQ (MAJORNM,ALLOCATE,8,SYSTEM),MF=(E,DYNENQ)              00364000
* HAVING FREED THE RESOURCE WHICH ONLY GUARANTEED THAT ALL USERS        00365000
* WERE GONE, NORMAL START UP MAY PROCEED.                               00366000
*                     END;                                      01S0115 00367000
*                 END;                                          01S0116 00368000
*                                                               01S0116 00369000
@RF00105 DS    0H                                               01S0117 00370000
*/*APCTPTR MUST POINT TO THE APCT AT THE CALL OF IGTD10.             */ 00371000
*                                                               01S0117 00372000
*               APCTHALT='0'B;      /* RESET MONITOR HALTING.        */ 00373000
@RF00096 DS    0H                                               01S0118 00374000
*               APCTTERM='0'B;      /* RESET MONITOR TERMINATION        00375000
*                                      REQUEST                       */ 00376000
         L     @15,APCTTERM(,APCTPTR)                           01S0118 00377000
@GL00012 LR    @14,@15                                          01S0118 00378000
         N     @14,@CF00669                                     01S0118 00379000
         CS    @15,@14,APCTTERM(APCTPTR)                        01S0118 00380000
         BNE   @GL00012                                         01S0118 00381000
*               CALL APCVZD10(MEMBID);/* CALL IGTD10.                */ 00382000
         LA    @15,MEMBID                                       01S0119 00383000
         ST    @15,@AL00001                                     01S0119 00384000
         L     @15,APCTZD10(,APCTPTR)                           01S0119 00385000
         LA    @01,@AL00001                                     01S0119 00386000
         BALR  @14,@15                                          01S0119 00387000
*               DYNENQ(1:4)='80000000'X;/* CLEAR THE AREA.           */ 00388000
         MVC   DYNENQ(4),@CB00559                               01S0120 00389000
*               GEN REFS(MONITOR,MAJORNM) SETS(DYNENQ)/* DEQ FROM THE   00390000
*                                      MONITOR                       */ 00391000
*                   ( DEQ (MAJORNM,MONITOR,7,SYSTEM),MF=(E,DYNENQ) );   00392000
          DEQ (MAJORNM,MONITOR,7,SYSTEM),MF=(E,DYNENQ)                  00393000
*             END;                                              01S0122 00394000
*           ELSE                                                01S0123 00395000
*             CALL IGTDWTO(IGT202I,MAJORNM(5:8));/* WRITE THE ERROR     00396000
*                                      MESSAGE TO THE OPERATOR.      */ 00397000
