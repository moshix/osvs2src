*           LISTEXB=ADDR(DCBABRT);  /* DCB ABEND ROUTINE ADDRESS.    */ 00841000
         LA    @08,DCBABRT                                      02S0441 00842000
         STCM  @08,7,LISTEXB                                    02S0441 00843000
*           LISTEX1='87'X;          /* INDICATE JFCB ENTRY IN EXLST. */ 00844000
