*         RFY                                                   01S0489 00991000
*          (GPR00F,                                             01S0489 00992000
*           GPR01P) UNRSTD;         /* FINISHED WITH REGISTERS       */ 00993000
@RF00483 DS    0H                                               01S0490 00994000
*       END;                        /* ECB WAIT COMPLETE WAIT FOR       00995000
*                                      IGTD20 TO END.                */ 00996000
*       GEN REFS(IGTD20T);                                      01S0491 00997000
*                                      /* DETACH THE TCB.            */ 00998000
  DETACH IGTD20T               RELEASE THE TCB.                         00999000
*                                                               02S0492 01000000
*/********************************************************************/ 01001000
*/*            PERFORM TERMINATION PROCESSING.                       */ 01002000
*/********************************************************************/ 01003000
*                                                               02S0492 01004000
*       /*************************************************************/ 01005000
*       /*                                                           */ 01006000
*       /* CLOSE THE DCB FOR THE 3838 LOAD MODULES.                  */ 01007000
*       /*                                                           */ 01008000
*       /*************************************************************/ 01009000
*                                                               02S0492 01010000
*       GEN SETS(CLOSEIT,DYNLDCB);                              02S0492 01011000
*                                                               02S0492 01012000
 MVI CLOSEIT,X'80'                    MOVE IN THE VL INDICATOR.         01013000
 CLOSE DYNLDCB,MF=(E,CLOSEIT)         CLOSE THE MOTHER.                 01014000
*/* DETERMINE IF WARM START MAY BE POSSIBLE.                         */ 01015000
*                                                               02S0493 01016000
*       IF APCTWARM='0'B&           /* IF NOT SET BY LOWER LEVEL TASK*/ 01017000
*           ADDR(APCTECIT)->ECBCCCNT=0 THEN/* AND TERM. WAS NORMAL.  */ 01018000
         TM    APCTWARM(APCTPTR),B'00000100'                    02S0493 01019000
