*           RESPECIFY                                           02S0502 01047000
*             APOTVT BASED(R11P);                               02S0502 01048000
*           SSVTPTR=APCTSSVT;       /* ADDRESS OF THE SUBSYS VECTOR     01049000
*                                      TBL                           */ 01050000
*                                                               02S0503 01051000
         L     @10,APCTSSVT(,APCTPTR)                           02S0503 01052000
         ST    @10,SSVTPTR                                      02S0503 01053000
*           /*********************************************************/ 01054000
*           /*                                                       */ 01055000
*           /* THIS LOOP DEACTIVATES SELECTED SUBSYSTEM FUNCTIONS BY */ 01056000
*           /* SETTING THE INDEX VALUES TO ZERO. THE ONES DEACTIVATED*/ 01057000
*           /* ARE THE SAME ONES ACTIVATED DURING INITIALIZATION.    */ 01058000
*           /*                                                       */ 01059000
*           /*********************************************************/ 01060000
*                                                               02S0504 01061000
*           DO I=1 TO APOTVTCT BY 1;/* SCAN THE ENTRIES.             */ 01062000
         LA    I,1                                              02S0504 01063000
         B     @DE00504                                         02S0504 01064000
@DL00504 DS    0H                                               02S0505 01065000
*             IF APOTVTMN='1'B      /* IF MONITOR ACTIVATED THE ENTRY*/ 01066000
*                 &APOTVTCD^=0 THEN /* AND IT IS NOT A DUMMY ENTRY.  */ 01067000
         TM    APOTVTMN(R11P),B'01000000'                       02S0505 01068000
