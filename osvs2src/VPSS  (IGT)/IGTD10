*           DO;                                                 02S0565 01265000
*             APCTWARM='1'B;        /* SET WARM START POSSIBLE       */ 01266000
         L     @08,APCTWARM(,APCTPTR)                           02S0566 01267000
@GL00015 LR    @11,@08                                          02S0566 01268000
         O     @11,@CF04047                                     02S0566 01269000
         CS    @08,@11,APCTWARM(APCTPTR)                        02S0566 01270000
         BNE   @GL00015                                         02S0566 01271000
*             CALL APCVZDWT(IGT206I,14,APCTSSID,4);/* OUTPUT MSG 206I*/ 01272000
         MVC   @AL00001(16),@AL00567                            02S0567 01273000
         LA    @08,APCTSSID(,APCTPTR)                           02S0567 01274000
