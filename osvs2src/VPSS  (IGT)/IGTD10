*                             MSTRAPTR->APUBAPUB=APUBPTR;/* SET PTR     00759000
*                                      TO TOP.                       */ 00760000
         ST    APUBPTR,APUBAPUB(,MSTRAPTR)                      02S0415 00761000
*                             MSTRAPTR->APUBUBAP=APUBPTR;/* SET PTR     00762000
