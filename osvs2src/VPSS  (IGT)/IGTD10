         ST    APUBPTR,APUBUBAP(,MSTRAPTR)                      02S0421 00784000
*                             RESPECIFY                         02S0422 00785000
*                               APUBPTR UNRESTRICTED;/* RELEASE REG. */ 00786000
*                           END;                                02S0423 00787000
*                       END;                                    02S0424 00788000
@RC00413 DS    0H                                               02S0425 00789000
*                   END;                                        02S0425 00790000
@RC00402 DS    0H                                               02S0426 00791000
*               END;                                            02S0426 00792000
@RC00382 DS    0H                                               02S0427 00793000
*           END;                                                02S0427 00794000
@RF00363 AL    LUTPTR,@CF00057                                  02S0427 00795000
@DE00362 CLC   UCBPTR(2,LUTPTR),@CH03905                        02S0427 00796000
         BNE   @DL00362                                         02S0427 00797000
*         END;                                                  01S0428 00798000
*       IGTD20PM(2)=ADDR(APCTZD20); /* ADDRESS OF ROUTINE TO BE 01S0429 00799000
*                                      ATTACHED.                     */ 00800000
