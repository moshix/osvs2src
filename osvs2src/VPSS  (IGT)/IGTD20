*         APCQINT='1'B;             /* INDICATE INTERNAL FORMAT.     */ 00636000
         OI    APCQINT(APCQPTR),B'10000000'                     01S0300 00637000
*         CALL APCVZDX2;            /* QUEUE THE APCQ.               */ 00638000
         L     @15,APCTZDX2(,APCTPTR)                           01S0301 00639000
         BALR  @14,@15                                          01S0301 00640000
*         RESPECIFY                                             01S0302 00641000
*           APCQ;                                               01S0302 00642000
*         RESPECIFY                                             01S0303 00643000
*           APCQPTR UNRESTRICTED;                               01S0303 00644000
*       END;                                                    01S0304 00645000
