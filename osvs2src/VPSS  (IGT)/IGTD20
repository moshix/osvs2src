TD20     TITLE 'IGTD20 - COMMAND INTERFACE ROUTINE.                    *00001000
                        '                                               00002000
IGTD20   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTD20    77.231'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
         EJECT                                                          00022000
*   DO;                             /* ESTABLISH THE ERROR RECOVERY  */ 00023000
*     RFY                                                       01S0134 00024000
*      (R2P,                                                    01S0134 00025000
*       R15P) RSTD;                 /* NED THESE REGS.               */ 00026000
*     D20APCT=APCTPTR;              /* POINTER TO THE APCT.          */ 00027000
         ST    APCTPTR,D20APCT                                  01S0135 00028000
*     D20CODE=20;                   /* IDENTIFY THE ROUTINE.         */ 00029000
         MVC   D20CODE(2),@CH00390                              01S0136 00030000
*     D20FLAGS=''X;                 /* CLEAR THE FLAGS.              */ 00031000
         XC    D20FLAGS(2),D20FLAGS                             01S0137 00032000
*     R2P=APCTZD4E;                 /* ADDRESS OF IGTD4E.            */ 00033000
         L     R2P,APCTZD4E(,APCTPTR)                           01S0138 00034000
*     GEN REFS(DSTAE,SSTAE,R2P);                                01S0139 00035000
*                                 /* ISSUE THE STAE OR ESTAE.        */ 00036000
     MVC   DSTAE,SSTAE               MOVE IN THE SKELETON               00037000
     IGTYSTAE (R2P),MF=(E,DSTAE),PARAM=D20STAE ISSUE THE STAE.          00038000
*     RFY                                                       01S0140 00039000
*      (R2P,                                                    01S0140 00040000
*       R15P) UNRSTD;               /* RELEASE THE REGS.             */ 00041000
*   END;                            /* END OF STAE PROCESSING.       */ 00042000
*   IF R15P=0 THEN                  /* TEST FOR SUCCESSFUL      01S0142 00043000
*                                      STAE/ESTAE.                   */ 00044000
         LTR   R15P,R15P                                        01S0142 00045000
         BNZ   @RF00142                                         01S0142 00046000
*     DO;                                                       01S0143 00047000
*                                                               01S0143 00048000
*       /*************************************************************/ 00049000
*       /*                                                           */ 00050000
*       /* ATTACH THE COMMAND PROCESSING ROUTINE.                    */ 00051000
*       /*                                                           */ 00052000
*       /*************************************************************/ 00053000
*                                                               01S0144 00054000
*       IGTD40P(1)=APCTPTR;         /* ADDRESS OF THE APCT.          */ 00055000
         ST    APCTPTR,IGTD40P                                  01S0144 00056000
*       IGTD40P(2)=ADDR(APCTZD40);  /* ADDRESS OF IGTD40.            */ 00057000
         LA    @15,APCTZD40(,APCTPTR)                           01S0145 00058000
         ST    @15,IGTD40P+4                                    01S0145 00059000
*       RESPECIFY                                               01S0146 00060000
*        (R1P,                                                  01S0146 00061000
*         R2P,                                                  01S0146 00062000
*         R3P) RESTRICTED;                                      01S0146 00063000
*       R1P=ADDR(IGTD40P);          /* POINT R1 AT THE PARM LIST.    */ 00064000
         LA    R1P,IGTD40P                                      01S0147 00065000
*       R3P=ADDR(APCTECPT);         /* ADDR OF IGTD40 TERMINATION       00066000
*                                      ECB.                          */ 00067000
         LA    R3P,APCTECPT(,APCTPTR)                           01S0148 00068000
*       R2P=CVTLINK;                /* ADDR OF LINK LIB DCB.         */ 00069000
         L     @15,CVTPTR                                       01S0149 00070000
         L     R2P,CVTLINK(,@15)                                01S0149 00071000
*       GEN( XC MFLATCH(MFLAL),MFLATCH  CLEAR THE PARM LIST. )  01S0150 00072000
*       REFS(MFLATCH);                                          01S0150 00073000
          XC MFLATCH(MFLAL),MFLATCH  CLEAR THE PARM LIST.               00074000
*       GEN REFS(R1P,R2P,R3P) SETS(MFLATCH)/* ATTACH IGTUEA - IGTD40.*/ 00075000
*           ( ATTACH  ECB=(R3P),DCB=(R2P),EP=IGTUEA,SF=(E,MFLATCH) );   00076000
          ATTACH  ECB=(R3P),DCB=(R2P),EP=IGTUEA,SF=(E,MFLATCH)          00077000
*       D40TCB=R1P;                 /* SAVE TCB ADDRESS.             */ 00078000
         ST    R1P,D40TCB                                       01S0152 00079000
         EJECT                                                          00080000
*                                                               01S0153 00081000
*       /*************************************************************/ 00082000
*       /*                                                           */ 00083000
*       /* FIND THE OPERATOR INPUT AREA.                             */ 00084000
*       /*                                                           */ 00085000
*       /*************************************************************/ 00086000
*                                                               01S0153 00087000
*       MFLEXT(1:12)=''X;           /* ZERO THE PARM LIST.           */ 00088000
         XC    MFLEXT(12),MFLEXT                                01S0153 00089000
*       GEN SETS(COMMPTR,MFLEXT)    /* LOCATE THE COMMUNICATION AREA.*/ 00090000
*           ( EXTRACT COMMPTR,'S',FIELDS=(COMM),MF=(E,MFLEXT) );        00091000
          EXTRACT COMMPTR,'S',FIELDS=(COMM),MF=(E,MFLEXT)               00092000
*       IF COMMCIB^=0 THEN          /* IS A CIB ALREADY ON THE QUEUE?*/ 00093000
         L     @15,COMMPTR                                      01S0155 00094000
         L     @00,COMMCIB(,@15)                                01S0155 00095000
         LTR   @00,@00                                          01S0155 00096000
         BZ    @RF00155                                         01S0155 00097000
*         CALL GETCIB;              /* GO GET A CIB.                 */ 00098000
         BAL   @14,GETCIB                                       01S0156 00099000
*       RESPECIFY                                               01S0157 00100000
*         R2P RESTRICTED;                                       01S0157 00101000
@RF00155 DS    0H                                               01S0158 00102000
*       R2P=ADDR(COMMCIB);                                      01S0158 00103000
         L     R2P,COMMPTR                                      01S0158 00104000
         LA    R2P,COMMCIB(,R2P)                                01S0158 00105000
*       GEN REFS(R2P)               /* ALLOW MODIFY CIBS TO BE  01S0159 00106000
*                                      CHAINED.                      */ 00107000
*           ( QEDIT ORIGIN=(R2P),CIBCTR=10  );                  01S0159 00108000
          QEDIT ORIGIN=(R2P),CIBCTR=10                                  00109000
*       RESPECIFY                                               01S0160 00110000
*         R2P UNRESTRICTED;                                     01S0160 00111000
         EJECT                                                          00112000
*                                                               01S0161 00113000
*       /*************************************************************/ 00114000
*       /*                                                           */ 00115000
*       /* BUILD THE ECB LIST.                                       */ 00116000
*       /*                                                           */ 00117000
*       /*************************************************************/ 00118000
*                                                               01S0161 00119000
*       APCTECBL=ADDR(ECBLIST);     /* SET POINTER TO ECB LIST.      */ 00120000
         LA    @02,ECBLIST                                      01S0161 00121000
         ST    @02,APCTECBL(,APCTPTR)                           01S0161 00122000
*       ECBLIST(1)=ADDR(COMMECB);   /* POINTER TO COMMUNICATION ECB. */ 00123000
         L     @02,COMMPTR                                      01S0162 00124000
         L     @02,COMMECBP(,@02)                               01S0162 00125000
         ST    @02,ECBLIST                                      01S0162 00126000
*       ECBLIST(2)=ADDR(APCTEALC);  /* ALLOCATE ENQ COMPLETES ECB.   */ 00127000
         LA    @02,APCTEALC(,APCTPTR)                           01S0163 00128000
         ST    @02,ECBLIST+4                                    01S0163 00129000
*       ECBLIST(3)=ADDR(APCTECPT);  /* IGTD40 TERMINATION ECB.       */ 00130000
         LA    @02,APCTECPT(,APCTPTR)                           01S0164 00131000
         ST    @02,ECBLIST+8                                    01S0164 00132000
*       ECBLIST(4)=ADDR(APCTEULD);  /* DISABLE/UNLOAD SCAN ECB.      */ 00133000
         LA    @02,APCTEULD(,APCTPTR)                           01S0165 00134000
         ST    @02,ECBLIST+12                                   01S0165 00135000
*       ECBLIST(5)=ADDR(APCTEIPL)   /* RE-IPL OR FORCE DUMP REQ'ED      00136000
*                                      ECB.                          */ 00137000
*           |'80000000'X;           /* SET VLIST INDICATOR.          */ 00138000
         LA    @02,APCTEIPL(,APCTPTR)                           01S0166 00139000
         O     @02,@CF02102                                     01S0166 00140000
         ST    @02,ECBLIST+16                                   01S0166 00141000
*       RESPECIFY                                               01S0167 00142000
*        (R1P,                                                  01S0167 00143000
*         R2P,                                                  01S0167 00144000
*         R3P) UNRESTRICTED;                                    01S0167 00145000
*       DO UNTIL TERMECB(2)='1'B;   /* WHEN IGTD40 TERMINATES, QUIT. */ 00146000
@DL00168 DS    0H                                               01S0169 00147000
*         DO;                       /* IGTYWAIT                 01S0169 00148000
*                                      (1)ECBLIST(ECBLIST)LONG(YES)  */ 00149000
*           RFY                                                 01S0170 00150000
*            (GPR00F,                                           01S0170 00151000
*             GPR01P) RSTD;         /* REGISTERS USED                */ 00152000
*           GPR01P=-ADDR(ECBLIST);  /* POINT TO ECB LIST             */ 00153000
         LA    GPR01P,ECBLIST                                   01S0171 00154000
         LCR   GPR01P,GPR01P                                    01S0171 00155000
*           GPR00F=1|'80000000'X;   /* WAIT COUNT                    */ 00156000
         L     GPR00F,@CF02227                                  01S0172 00157000
*           SVC(1);                 /* WAIT FOR ECB POSTING          */ 00158000
         SVC   1                                                01S0173 00159000
*           RFY                                                 01S0174 00160000
*            (GPR00F,                                           01S0174 00161000
*             GPR01P) UNRSTD;       /* FINISHED WITH REGISTERS       */ 00162000
*         END;                      /* ECB WAIT COMPLETE WAIT FOR ?  */ 00163000
*         IF SMECB(2)='1'B THEN     /* STOP/MODIFY ECB?              */ 00164000
         L     @01,COMMPTR                                      01S0176 00165000
         L     @01,COMMECBP(,@01)                               01S0176 00166000
         TM    SMECB(@01),B'01000000'                           01S0176 00167000
         BNO   @RF00176                                         01S0176 00168000
*           CALL GETCIB;            /* GO GET THE CIB AND MAKE AN       00169000
*                                      APCQ.                         */ 00170000
         BAL   @14,GETCIB                                       01S0177 00171000
*         IF ALLCECB(2)='1'B THEN   /* ALLOCATE ENQ COMPLETE?        */ 00172000
@RF00176 TM    ALLCECB(APCTPTR),B'01000000'                     01S0178 00173000
         BNO   @RF00178                                         01S0178 00174000
*           CALL GENHALT;           /* GO GENERATE A HALT COMMAND.   */ 00175000
         BAL   @14,GENHALT                                      01S0179 00176000
*         IF SCANECB(2)='1'B THEN   /* DISABLE/UNLOAD SCAN ECB?      */ 00177000
@RF00178 TM    SCANECB(APCTPTR),B'01000000'                     01S0180 00178000
         BNO   @RF00180                                         01S0180 00179000
*           CALL GENSCAN;           /* GO GENERATE A SCAN COMMAND.   */ 00180000
         BAL   @14,GENSCAN                                      01S0181 00181000
*         IF RIPLECB(2)='1'B THEN   /* RE-IPL REQUIRED ECB?          */ 00182000
@RF00180 TM    RIPLECB(APCTPTR),B'01000000'                     01S0182 00183000
         BNO   @RF00182                                         01S0182 00184000
*           CALL GENRIPL;           /* GO GENERATE A RE-IPL COMMAND. */ 00185000
         BAL   @14,GENRIPL                                      01S0183 00186000
*       END;                                                    01S0184 00187000
@RF00182 DS    0H                                               01S0184 00188000
@DE00168 TM    TERMECB(APCTPTR),B'01000000'                     01S0184 00189000
         BNO   @DL00168                                         01S0184 00190000
         EJECT                                                          00191000
*       GEN REFS(D40TCB)                                        01S0185 00192000
*           ( DETACH D40TCB           ELIMINATE IGTD40 TCB. );  01S0185 00193000
          DETACH D40TCB           ELIMINATE IGTD40 TCB.                 00194000
*       APCTACT='0'B;               /* RESET MONITOR ACTIVE     01S0186 00195000
*                                      INDICATOR.                    */ 00196000
         L     @15,APCTACT(,APCTPTR)                            01S0186 00197000
@GL00001 LR    @14,@15                                          01S0186 00198000
         N     @14,@CF02230                                     01S0186 00199000
         CS    @15,@14,APCTACT(APCTPTR)                         01S0186 00200000
         BNE   @GL00001                                         01S0186 00201000
*       MFENQ(1:4)=STATDEQ;         /* MOVE IN ENQ FLAGS.            */ 00202000
         MVC   MFENQ(4),STATDEQ                                 01S0187 00203000
*       GEN REFS(APCTENQM,ALLOCATE) /* DEQUEUE FROM THE ALLOCATE     */ 00204000
*           SETS(MFENQ)             /* RESOURCE SO ALLOCATION WILL      00205000
*                                      KNOW THAT THE MONITOR IS NOT     00206000
*                                      ACTIVE.                       */ 00207000
*           ;                                                   01S0188 00208000
       DEQ   (APCTENQM-APCT(,APCTPTR),ALLOCATE,8,SYSTEM), DEQ FROM     X00209000
               RET=HAVE,    PREVENT ABEND ON EXCEPTIONAL CASES @ZA25359*00210000
               MF=(E,MFENQ)       /* THE RESOURCE.                   */ 00211000
*       APCTTERM='0'B;              /* RESET THE TERMINATION    01S0189 00212000
*                                      INDICATOR.                    */ 00213000
         L     @15,APCTTERM(,APCTPTR)                           01S0189 00214000
@GL00002 LR    @14,@15                                          01S0189 00215000
         N     @14,@CF02233                                     01S0189 00216000
         CS    @15,@14,APCTTERM(APCTPTR)                        01S0189 00217000
         BNE   @GL00002                                         01S0189 00218000
*       GEN(IGTYSTAE 0);            /* CANCEL THE ESTAE.             */ 00219000
         IGTYSTAE 0                                                     00220000
*     END;                                                      01S0191 00221000
*   RETURN CODE(D40CODE);           /* RETURN TO CALLER BUT LET HIM     00222000
*                                      KNOW HOW IGTD40 TERMINATED.   */ 00223000
@RF00142 L     @11,D40CODE-1(,APCTPTR)                          01S0192 00224000
         LA    @11,0(,@11)                                      01S0192 00225000
         L     @13,4(,@13)                                      01S0192 00226000
         L     @00,@SIZDATD                                     01S0192 00227000
         LR    @01,@06                                          01S0192 00228000
         IGTYFSV  R,LV=(0),A=(1)                                        00229000
         LR    @15,@11                                          01S0192 00230000
         L     @14,12(,@13)                                     01S0192 00231000
         LM    @00,@12,20(@13)                                  01S0192 00232000
         BR    @14                                              01S0192 00233000
         EJECT                                                          00234000
*                                                               01S0193 00235000
*/********************************************************************/ 00236000
*/*                                                                  */ 00237000
*/*      THIS ROUTINE GENERATES A HALT COMMAND                       */ 00238000
*/*                                                                  */ 00239000
*/********************************************************************/ 00240000
*                                                               01S0193 00241000
*GENHALT:                                                       01S0193 00242000
*   PROC;                                                       01S0193 00243000
GENHALT  STM   @14,@12,@SA00002                                 01S0193 00244000
*   ALLCECB(2)='0'B;                /* RESET ALLOCATE ENQ ECB.       */ 00245000
         NI    ALLCECB(APCTPTR),B'10111111'                     01S0194 00246000
*   CALL CLEANUP;                   /* INSURE 3838 UNLOAD    @ZA25359   00247000
*                                      WORKS                 @ZA25359*/ 00248000
         BAL   @14,CLEANUP                                      01S0195 00249000
*   CALL APCVZDX1;                  /* GET AN APCQ.                  */ 00250000
         L     @15,APCTZDX1(,APCTPTR)                           01S0196 00251000
         BALR  @14,@15                                          01S0196 00252000
*   RESPECIFY                                                   01S0197 00253000
*     APCQPTR RESTRICTED;                                       01S0197 00254000
*   RESPECIFY                                                   01S0198 00255000
*     APCQ BASED(APCQPTR);                                      01S0198 00256000
*   APCQIINF=APCQHALT;              /* INDICATE A HALT COMMAND.      */ 00257000
         MVC   APCQIINF(4,APCQPTR),@CF00045                     01S0199 00258000
*   APCQINT='1'B;                   /* INDICATE INTERNALLY GENERATED */ 00259000
         OI    APCQINT(APCQPTR),B'10000000'                     01S0200 00260000
*   CALL APCVZDX2;                  /* QUEUE THE APCQ.               */ 00261000
         L     @15,APCTZDX2(,APCTPTR)                           01S0201 00262000
         BALR  @14,@15                                          01S0201 00263000
*   RESPECIFY                                                   01S0202 00264000
*     APCQPTR UNRESTRICTED;                                     01S0202 00265000
*   RESPECIFY                                                   01S0203 00266000
*     APCQ;                                                     01S0203 00267000
*   RETURN;                                                     01S0204 00268000
@EL00002 DS    0H                                               01S0204 00269000
@EF00002 DS    0H                                               01S0204 00270000
@ER00002 LM    @14,@12,@SA00002                                 01S0204 00271000
         BR    @14                                              01S0204 00272000
*   END;                                                        01S0205 00273000
         EJECT                                                          00274000
*CLEANUP:                                                       01S0206 00275000
*   PROC;                           /*                       @ZA25359*/ 00276000
*                                                               01S0206 00277000
CLEANUP  STM   @14,@12,@SA00003                                 01S0206 00278000
*   /*****************************************************************/ 00279000
*   /*                                                               */ 00280000
*   /* THIS ROUTINE INSURES THAT NO USERS APPEAR TO BE       @ZA25359*/ 00281000
*   /* ALLOCATED 3838 RESOURCES AND THEREBY PREVENT          @ZA25359*/ 00282000
*   /* UNLOADING OF THE 3838.                                @ZA25359*/ 00283000
*   /*                                                               */ 00284000
*   /*****************************************************************/ 00285000
*                                                               01S0207 00286000
*   CALL APCVZDEQ;                  /* SERIALIZE ACCESS TO   @ZA25359   00287000
*                                      APCT,APUBS, ETC.      @ZA25359*/ 00288000
         L     @15,APCTZDEQ(,APCTPTR)                           01S0207 00289000
         BALR  @14,@15                                          01S0207 00290000
*                                                               01S0208 00291000
*/* SET APUBOWN TO INDICATE NO APUB EXCLUSIVELY ALLOCATED.   @ZA25359*/ 00292000
*                                                               01S0208 00293000
*   MSTRAPUB=APCTAPUB;              /* MSTRAPUB POINTS TO FIRST MASTR   00294000
*                                                            @ZA25359*/ 00295000
         L     MSTRAPUB,APCTAPUB(,APCTPTR)                      01S0208 00296000
*   RFY                                                         01S0209 00297000
*     APUBPTR RSTD;                 /* NEED REGISTER         @ZA25359*/ 00298000
*   RFY                                                         01S0210 00299000
*     APUB BASED(APUBPTR);          /* PROVIDE BASING EXPRESSION        00300000
*                                                            @ZA25359*/ 00301000
*   DO WHILE MSTRAPUB^=0;           /* DO FOR EACH MASTER APUB  01S0211 00302000
*                                                            @ZA25359*/ 00303000
         B     @DE00211                                         01S0211 00304000
@DL00211 DS    0H                                               01S0212 00305000
*     APUBPTR=MSTRAPUB->APUBAPUB;   /* FIRST NON-CONTROL PORT APUB      00306000
*                                                            @ZA25359*/ 00307000
         L     APUBPTR,APUBAPUB(,MSTRAPUB)                      01S0212 00308000
*     DO UNTIL APUBPTR=0;           /* DO ONCE FOR EACH NON-CNTL PORT   00309000
*                                                            @ZA25359*/ 00310000
@DL00213 DS    0H                                               01S0214 00311000
*       APUBOWN=SHR;                /* INDICATE PORT NOT EXC ALLOC8D    00312000
*                                                            @ZA25359*/ 00313000
         L     @11,APUBOWN(,APUBPTR)                            01S0214 00314000
@GL00003 LR    @03,@11                                          01S0214 00315000
         N     @03,@CF02238                                     01S0214 00316000
         CS    @11,@03,APUBOWN(APUBPTR)                         01S0214 00317000
         BNE   @GL00003                                         01S0214 00318000
*       APUBPTR=APUBAPUB;           /* ON TO NEXT APUB OR DONE W/3838   00319000
*                                                            @ZA25359*/ 00320000
         L     APUBPTR,APUBAPUB(,APUBPTR)                       01S0215 00321000
*     END;                          /* DO UNTIL APUBPTR = 0  @ZA25359*/ 00322000
@DE00213 LTR   APUBPTR,APUBPTR                                  01S0216 00323000
         BNZ   @DL00213                                         01S0216 00324000
*     MSTRAPUB=MSTRAPUB->APUBMSTR;  /* POINT TO NEXT MASTER OR DONE     00325000
*                                                            @ZA25359*/ 00326000
         L     MSTRAPUB,APUBMSTR(,MSTRAPUB)                     01S0217 00327000
*   END;                            /* DO WHILE MSTRAPUB ^= 0@ZA25359*/ 00328000
@DE00211 LTR   MSTRAPUB,MSTRAPUB                                01S0218 00329000
         BNZ   @DL00211                                         01S0218 00330000
*   RFY                                                         01S0219 00331000
*     APUBPTR UNRSTD;               /* NO LONGER NEED APUBPTR@ZA25359*/ 00332000
*   RFY                                                         01S0220 00333000
*     APUB;                         /* REVERT                @ZA25359*/ 00334000
*   APCTAPDS='';                    /* NO USERS OF VPSS RESOURCES       00335000
*                                                            @ZA25359*/ 00336000
         SLR   @15,@15                                          01S0221 00337000
         ST    @15,APCTAPDS(,APCTPTR)                           01S0221 00338000
*   APCTDSAP='';                    /* DITTO                 @ZA25359*/ 00339000
         ST    @15,APCTDSAP(,APCTPTR)                           01S0222 00340000
*   CALL APCVZDDQ;                  /* END SERIALIZATION ON APCT, ETC   00341000
*                                                            @ZA25359*/ 00342000
         L     @15,APCTZDDQ(,APCTPTR)                           01S0223 00343000
         BALR  @14,@15                                          01S0223 00344000
*   END CLEANUP;                    /* END OF INTERNAL SUBROUTINE       00345000
*                                                            @ZA25359*/ 00346000
@EL00003 DS    0H                                               01S0224 00347000
@EF00003 DS    0H                                               01S0224 00348000
@ER00003 LM    @14,@12,@SA00003                                 01S0224 00349000
         BR    @14                                              01S0224 00350000
         EJECT                                                          00351000
*                                                               01S0225 00352000
*/********************************************************************  00353000
*                                                                    */ 00354000
*/*                                                                  */ 00355000
*/*       THIS ROUTINE GENERATES A DISABLE/UNLOAD SCAN COMMAND.      */ 00356000
*/*                                                                  */ 00357000
*/********************************************************************  00358000
*                                                                    */ 00359000
*                                                               01S0225 00360000
*GENSCAN:                                                       01S0225 00361000
*   PROC;                                                       01S0225 00362000
GENSCAN  STM   @14,@12,@SA00004                                 01S0225 00363000
*   SCANECB(2)='0'B;                /* RESET THE ECB.                */ 00364000
         NI    SCANECB(APCTPTR),B'10111111'                     01S0226 00365000
*   CALL APCVZDX1;                  /* GET AN APCQ.                  */ 00366000
         L     @15,APCTZDX1(,APCTPTR)                           01S0227 00367000
         BALR  @14,@15                                          01S0227 00368000
*   RESPECIFY                                                   01S0228 00369000
*     APCQPTR RESTRICTED;                                       01S0228 00370000
*   RESPECIFY                                                   01S0229 00371000
*     APCQ BASED(APCQPTR);                                      01S0229 00372000
*   APCQIINF=APCQDULS;              /* INDICATE DISABLE/UNLOAD SCAN. */ 00373000
         MVC   APCQIINF(4,APCQPTR),@CF00503                     01S0230 00374000
*   APCQINT='1'B;                   /* INDICATE INTERNAL COMMAND.    */ 00375000
         OI    APCQINT(APCQPTR),B'10000000'                     01S0231 00376000
*   CALL APCVZDX2;                  /* QUEUE THE APCQ.               */ 00377000
         L     @15,APCTZDX2(,APCTPTR)                           01S0232 00378000
         BALR  @14,@15                                          01S0232 00379000
*   RESPECIFY                                                   01S0233 00380000
*     APCQPTR UNRESTRICTED;                                     01S0233 00381000
*   RESPECIFY                                                   01S0234 00382000
*     APCQ;                                                     01S0234 00383000
*   RETURN;                                                     01S0235 00384000
@EL00004 DS    0H                                               01S0235 00385000
@EF00004 DS    0H                                               01S0235 00386000
@ER00004 LM    @14,@12,@SA00004                                 01S0235 00387000
         BR    @14                                              01S0235 00388000
*   END;                                                        01S0236 00389000
         EJECT                                                          00390000
*                                                               01S0237 00391000
*/********************************************************************/ 00392000
*/*                                                                  */ 00393000
*/*    THIS ROUTINE PROCESSES COMMUNICATION INTERFACE BLOCKS THAT    */ 00394000
*/*  HAVE BEEN QUEUED FOR IT.  THERE ARE THREE TYPES OF CIBS:        */ 00395000
*/*       1. START COMMAND, WHICH IS DISCARDED.                      */ 00396000
*/*       2. MODIFY COMMANDS, WHICH ARE CONVERTED INTO APCQS FOR THE */ 00397000
*/*          COMMAND PROCESSOR TASK.                                 */ 00398000
*/*       3. STOP COMMANDS, WHICH WILL CAUSE THE MONITOR TO TERMINATE*/ 00399000
*/*          WHEN ALL USERS HAVE TERMINATED.                         */ 00400000
*/*                                                                  */ 00401000
*/********************************************************************/ 00402000
*                                                               01S0237 00403000
*GETCIB:                                                        01S0237 00404000
*   PROC;                                                       01S0237 00405000
GETCIB   STM   @14,@12,@SA00005                                 01S0237 00406000
*   DCL                                                         01S0238 00407000
*     CIBPTR PTR(31) DEF(COMMCIB);  /* PTR TO THE CIB.               */ 00408000
*/*   * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *  */ 00409000
*/*                                                                  */ 00410000
*/*      COMMAND INPUT BUFFER MAPPING MACRO                          */ 00411000
*/*                                                                  */ 00412000
*/*  OS/VS2 RELEASE 2, OS/VS1 RELEASE 3, 09/21/72, LEVEL=1           */ 00413000
*/*                                                                  */ 00414000
*/*  METHOD OF ACCESS                                                */ 00415000
*/*      BAL  - A DSECT CARD SHOULD PRECEDE MACRO CALL.  USING ON    */ 00416000
*/*             CIBNEXT GIVES ADDRESSABILITY FOR ALL SYMBOLS.        */ 00417000
*/*      PL/S - DCL CIBPTR PTR                                       */ 00418000
*/*   * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *  */ 00419000
*                                                               01S0239 00420000
*   DECLARE                                                     01S0239 00421000
*     1 CIBNAME BASED(CIBPTR) BDY(WORD),                        01S0239 00422000
*      2 CIBNEXT PTR(31),           /* ADDRESS OF NEXT CIB IN QUEUE     00423000
*                                      (ZERO FOR LAST)               */ 00424000
*      2 CIBVERB CHAR(1),           /* COMMAND VERB CODE             */ 00425000
*      2 CIBLEN PTR(8),             /* LENGTH IN DOUBLEWORDS OF CIB     00426000
*                                      INCLUDING CIBDATA             */ 00427000
*      2 * CHAR(4) BDY(WORD,3),     /* RESERVED FOR CSCB        01S0239 00428000
*                                      COMPATIBILITY                 */ 00429000
*      2 CIBASID FIXED(15),         /* ADDRESS SPACE ID (OS/VS2)        00430000
*                                      MDC001                        */ 00431000
*       3 CIBTJID CHAR(2),          /* TSO TERMINAL JOB IDENTIFIER      00432000
*                                      (OS/VS1) MDC001               */ 00433000
*      2 CIBCONID CHAR(1),          /* IDENTIFIER OF CONSOLE ISSUING    00434000
*                                      COMMAND                       */ 00435000
*      2 * CHAR(1),                 /* RESERVED                      */ 00436000
*      2 CIBDATLN FIXED(15),        /* LENGTH IN BYTES OF DATA IN       00437000
*                                      CIBDATA                       */ 00438000
*      2 CIBDATA CHAR(8) BDY(WORD); /* DATA FROM COMMAND OPERAND     */ 00439000
*/*      (LENGTH OF CIBDATA IS A MULTIPLE OF EIGHT BYTES             */ 00440000
*/*      DEPENDING ON THE VALUE CONTAINED IN CIBLEN)                 */ 00441000
*/*            START -  FOURTH POSITIONAL PARAMETER (PARMVALUE)      */ 00442000
*/*            MODIFY - RESIDUAL OPERAND IMAGE FOLLOWING COMMA       */ 00443000
*/*                     TERMINATING FIRST POSITIONAL PARAMETER       */ 00444000
*/*            STOP -   NONE (CIB GENERATED ONLY TO GIVE CONSOLE ID) */ 00445000
*/*   * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *  */ 00446000
*                                                               01S0240 00447000
*   DO UNTIL CIBPTR=0;              /* PROCESS ALL CIBS ON THE QUEUE.*/ 00448000
@DL00240 DS    0H                                               01S0241 00449000
*     IF CIBVERB='40'X THEN         /* IS THIS A STOP COMMAND?       */ 00450000
         L     @01,COMMPTR                                      01S0241 00451000
         L     @01,CIBPTR(,@01)                                 01S0241 00452000
         CLI   CIBVERB(@01),X'40'                               01S0241 00453000
         BNE   @RF00241                                         01S0241 00454000
*       DO;                         /* PROCESS STOP COMMAND.         */ 00455000
*         CALL APCVZDWT(IGT255I,14,APCTSSID,4);/* ACKNOWLEDGE STOP.  */ 00456000
         MVC   @AL00001(16),@AL00243                            01S0243 00457000
         LA    @15,APCTSSID(,APCTPTR)                           01S0243 00458000
         ST    @15,@AL00001+8                                   01S0243 00459000
         L     @15,APCTZDWT(,APCTPTR)                           01S0243 00460000
         LA    @01,@AL00001                                     01S0243 00461000
         BALR  @14,@15                                          01S0243 00462000
*         RESPECIFY                                             01S0244 00463000
*           R2P RESTRICTED;                                     01S0244 00464000
*         R2P=ADDR(COMMCIB);                                    01S0245 00465000
         L     R2P,COMMPTR                                      01S0245 00466000
         LA    R2P,COMMCIB(,R2P)                                01S0245 00467000
*         GEN REFS(R2P)             /* ALLOW MODIFY CIBS TO BE  01S0246 00468000
*                                      CHAINED.                      */ 00469000
*             ( QEDIT ORIGIN=(R2P),CIBCTR=10  );                01S0246 00470000
          QEDIT ORIGIN=(R2P),CIBCTR=10                                  00471000
*         RESPECIFY                                             01S0247 00472000
*           R2P UNRESTRICTED;                                   01S0247 00473000
*         IF APCTTERM='0'B THEN     /* SEE IF WE ARE ALREADY    01S0248 00474000
*                                      TERMINATING, IF SO WE SHOULD     00475000
*                                      BYPASS THE ENQ.               */ 00476000
         TM    APCTTERM(APCTPTR),B'00001000'                    01S0248 00477000
         BNZ   @RF00248                                         01S0248 00478000
*           DO;                     /* DO ONLY ONCE.                 */ 00479000
*             APCTTERM='1'B;        /* INDICATE TERMINATION     01S0250 00480000
*                                      NECESSARY.                    */ 00481000
         L     @15,APCTTERM(,APCTPTR)                           01S0250 00482000
@GL00004 LR    @14,@15                                          01S0250 00483000
         O     @14,@CF02241                                     01S0250 00484000
         CS    @15,@14,APCTTERM(APCTPTR)                        01S0250 00485000
         BNE   @GL00004                                         01S0250 00486000
*             MFENQ(1:4)=STATENQ;   /* MOVE IN ENQ FLAGS.            */ 00487000
         MVC   MFENQ(4),STATENQ                                 01S0251 00488000
*             GEN SETS(MFENQ,APCTEALC)/* GET THE ALLOCATE RESOURCE SO*/ 00489000
*                 REFS(APCTENQM,ALLOCATE);                      01S0252 00490000
*                                 /* ALLOCATION WILL STOP ALLOCATING.*/ 00491000
     ENQ   (APCTENQM-APCT(,APCTPTR),ALLOCATE,E,8,SYSTEM), /*ENQ ON   */X00492000
               ECB=APCTEALC-APCT(,APCTPTR),MF=(E,MFENQ) /*  ALLOCATE */ 00493000
*             IF R15P=0|            /* IF GOT IT RIGHT AWAY, OR      */ 00494000
*                 ENQRC='08'X THEN  /* ALREADY HAD IT, MUST DUMMY       00495000
*                                      POST                          */ 00496000
         LTR   R15P,R15P                                        01S0253 00497000
         BZ    @RT00253                                         01S0253 00498000
         CLI   ENQRC(R15P),8                                    01S0253 00499000
         BNE   @RF00253                                         01S0253 00500000
@RT00253 DS    0H                                               01S0254 00501000
*               ALLCECB(1:8)='01000000'B;/* FAKE POST.               */ 00502000
         MVI   ALLCECB(APCTPTR),B'01000000'                     01S0254 00503000
*           END;                    /* END OF ONE TIME PROCESSING.   */ 00504000
*       END;                                                    01S0256 00505000
*     ELSE                                                      01S0257 00506000
*       IF CIBVERB='44'X THEN       /* IS THIS A MODIFY COMMAND?     */ 00507000
         B     @RC00241                                         01S0257 00508000
@RF00241 L     @01,COMMPTR                                      01S0257 00509000
         L     @01,CIBPTR(,@01)                                 01S0257 00510000
         CLI   CIBVERB(@01),X'44'                               01S0257 00511000
         BNE   @RF00257                                         01S0257 00512000
*         DO;                                                   01S0258 00513000
*           CALL APCVZDX1;          /* GET AN APCQ.                  */ 00514000
         L     @15,APCTZDX1(,APCTPTR)                           01S0259 00515000
         BALR  @14,@15                                          01S0259 00516000
*           RESPECIFY                                           01S0260 00517000
*             APCQPTR RESTRICTED;                               01S0260 00518000
*           RESPECIFY                                           01S0261 00519000
*             APCQ BASED(APCQPTR);                              01S0261 00520000
*           APCQCMND(1:CIBDATLN)=CIBDATA(1:CIBDATLN);/* MOVE IN 01S0262 00521000
*                                      COMMAND.                      */ 00522000
         L     @08,COMMPTR                                      01S0262 00523000
         L     @08,CIBPTR(,@08)                                 01S0262 00524000
         LH    @15,CIBDATLN(,@08)                               01S0262 00525000
         LR    @14,@15                                          01S0262 00526000
         BCTR  @14,0                                            01S0262 00527000
         EX    @14,@SM02244                                     01S0262 00528000
*           APCQTXLN=CIBDATLN;      /* LENGTH OF COMMAND.            */ 00529000
         STH   @15,APCQTXLN(,APCQPTR)                           01S0263 00530000
*           APCQSCID=CIBCONID;      /* INSERT SOURCE CONSOLE ID.     */ 00531000
         IC    @15,CIBCONID(,@08)                               01S0264 00532000
         STC   @15,APCQSCID(,APCQPTR)                           01S0264 00533000
*           CALL APCVZDX2;          /* QUEUE THE APCQ.               */ 00534000
         L     @15,APCTZDX2(,APCTPTR)                           01S0265 00535000
         BALR  @14,@15                                          01S0265 00536000
*           RESPECIFY                                           01S0266 00537000
*             APCQPTR UNRESTRICTED;                             01S0266 00538000
*           RESPECIFY                                           01S0267 00539000
*             APCQ;                                             01S0267 00540000
*         END;                                                  01S0268 00541000
*     RESPECIFY                                                 01S0269 00542000
*      (R3P,                                                    01S0269 00543000
*       R2P) RESTRICTED;                                        01S0269 00544000
@RF00257 DS    0H                                               01S0269 00545000
@RC00241 DS    0H                                               01S0270 00546000
*     R3P=CIBPTR;                   /* POINTER TO THE CIB.           */ 00547000
         L     @15,COMMPTR                                      01S0270 00548000
         L     R3P,CIBPTR(,@15)                                 01S0270 00549000
*     R2P=ADDR(CIBPTR);             /* POINTER TO QUEUE HEAD.        */ 00550000
         LA    R2P,CIBPTR(,@15)                                 01S0271 00551000
*     GEN REFS(R2P,R3P)             /* FREE THE CIB                  */ 00552000
*         ( QEDIT ORIGIN=(R2P),BLOCK=(R3P)   );                 01S0272 00553000
          QEDIT ORIGIN=(R2P),BLOCK=(R3P)                                00554000
*     RESPECIFY                                                 01S0273 00555000
*      (R2P,                                                    01S0273 00556000
*       R3P) UNRESTRICTED;                                      01S0273 00557000
*   END;                                                        01S0274 00558000
@DE00240 L     @01,COMMPTR                                      01S0274 00559000
         L     @15,CIBPTR(,@01)                                 01S0274 00560000
         LTR   @15,@15                                          01S0274 00561000
         BNZ   @DL00240                                         01S0274 00562000
*   RETURN;                                                     01S0275 00563000
@EL00005 DS    0H                                               01S0275 00564000
@EF00005 DS    0H                                               01S0275 00565000
@ER00005 LM    @14,@12,@SA00005                                 01S0275 00566000
         BR    @14                                              01S0275 00567000
*   END;                                                        01S0276 00568000
         EJECT                                                          00569000
*                                                               01S0277 00570000
*/********************************************************************/ 00571000
*/*                                                                  */ 00572000
*/*      THIS ROUTINE GENERATES A FORCE DUMP COMMAND.                */ 00573000
*/*                                                                  */ 00574000
*/********************************************************************/ 00575000
*                                                               01S0277 00576000
*GENRIPL:                                                       01S0277 00577000
*   PROC;                                                       01S0277 00578000
GENRIPL  STM   @14,@12,@SA00006                                 01S0277 00579000
*   RIPLECB(2)='0'B;                /* RESET REIPL ECB.              */ 00580000
         NI    RIPLECB(APCTPTR),B'10111111'                     01S0278 00581000
*   RESPECIFY                                                   01S0279 00582000
*     APUB BASED(APUBPTR);                                      01S0279 00583000
*   RESPECIFY                                                   01S0280 00584000
*     APUBPTR RESTRICTED;                                       01S0280 00585000
*   APUBPTR=APCTAPUB;               /* FIRST APUB.                   */ 00586000
         L     APUBPTR,APCTAPUB(,APCTPTR)                       01S0281 00587000
*   DO WHILE APUBPTR^=0;            /* SCAN APUBS FOR DUMP REQUESTS. */ 00588000
         B     @DE00282                                         01S0282 00589000
@DL00282 DS    0H                                               01S0283 00590000
*     IF APUBFDMP='1'B              /* IF EITHER TYPE OF DUMP.       */ 00591000
*         |APUBPDMP='1'B THEN       /* GENERATE A DUMP APCQ.         */ 00592000
         TM    APUBFDMP(APUBPTR),B'01100000'                    01S0283 00593000
         BZ    @RF00283                                         01S0283 00594000
*       DO;                                                     01S0284 00595000
*         CALL APCVZDX1;            /* GET AN APCQ.                  */ 00596000
         L     @15,APCTZDX1(,APCTPTR)                           01S0285 00597000
         BALR  @14,@15                                          01S0285 00598000
*         RESPECIFY                                             01S0286 00599000
