TD40     TITLE 'IGTD40 - COMMAND PROCESSING ROUTINE.                   *00001000
                        '                                               00002000
IGTD40   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTD40    77.174'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
*                                                               01S0082 00022000
*/*                                                                  */ 00023000
*/*                                                                  */ 00024000
*/*                                                                  */ 00025000
*/*                                                                  */ 00026000
*/*                                                                  */ 00027000
*/*                                                                  */ 00028000
*/*                                                                  */ 00029000
*/*                                                                  */ 00030000
*/*                                                                  */ 00031000
*/*                                                                  */ 00032000
*/* END OF APMM                                                      */ 00033000
*/*                                                                  */ 00034000
*                                                               01S0083 00035000
         EJECT                                                          00036000
*                                                               01S0083 00037000
*   /*****************************************************************/ 00038000
*   /*                                                               */ 00039000
*   /* CREATE THE STAE ENVIRONMENT.                                  */ 00040000
*   /*                                                               */ 00041000
*   /*****************************************************************/ 00042000
*                                                               01S0083 00043000
*   DO;                             /* PROCESS THE STAE.             */ 00044000
*     RFY                                                       01S0084 00045000
*      (R2P,                                                    01S0084 00046000
*       R15P) RSTD;                 /* NEED THESE REGS.              */ 00047000
*     D40APCT=APCTPTR;              /* PTR TO THE APCT.              */ 00048000
         ST    APCTPTR,D40APCT                                  01S0085 00049000
*     D40CODE=40;                   /* INDICATE THE ROUTINE.         */ 00050000
         MVC   D40CODE(2),@CH00573                              01S0086 00051000
*     D40FLAGS=''X;                 /* CLEAR FLAGS.                  */ 00052000
         XC    D40FLAGS(2),D40FLAGS                             01S0087 00053000
*     D40RTRY=ADDR(STAERTRN);       /* ADDRESS OF THE RETRY ROUTINE. */ 00054000
         LA    @14,STAERTRN                                     01S0088 00055000
         ST    @14,D40RTRY                                      01S0088 00056000
*     R2P=APCTZD4E;                 /* ADDRESS OF THE EXIT ROUTINE.  */ 00057000
         L     R2P,APCTZD4E(,APCTPTR)                           01S0089 00058000
*     GEN REFS(DSTAE,SSTAE,R2P)     /* ISSUE THE STAE.               */ 00059000
*         FLOWS(STAERTRN) SEQFLOW NOEXIT;                       01S0090 00060000
         MVC   DSTAE,SSTAE          MOV IN THE SKELETON.                00061000
         IGTYSTAE (R2P),MF=(E,DSTAE),PARAM=D40STAE ISSUE THE STAE.      00062000
*     RFY                                                       01S0091 00063000
*      (R2P,                                                    01S0091 00064000
*       R15P) UNRSTD;                                           01S0091 00065000
*   END;                            /* PROCESS THE STAE.             */ 00066000
*   INUSER='0'B;                    /* WE HAVE CONTROL.              */ 00067000
         NI    INUSER,B'01111111'                               01S0093 00068000
*   DO UNTIL APCTHALT='1'B;         /* PROCESS COMMANDS UNTIL THE       00069000
*                                      SYSTEM CARRIES OUT AN OPERATOR   00070000
*                                      HALT COMMAND. THIS CAN OCCUR     00071000
*                                      ONLY WHEN ALL USERS HAVE 01S0094 00072000
*                                      TERMINATED.                   */ 00073000
@DL00094 DS    0H                                               01S0095 00074000
*     DO;                           /* IGTYWAIT                 01S0095 00075000
*                                      ECB(APCTECPW)LONG(YES)        */ 00076000
*       RFY                                                     01S0096 00077000
*        (GPR00F,                                               01S0096 00078000
*         GPR01P) RSTD;             /* REGISTERS USED                */ 00079000
*       DCL                                                     01S0097 00080000
*         BIT0007X BIT(8) BASED(ADDR(APCTECPW));/* OVERLAY BYTE-0 OF    00081000
*                                      ECB                           */ 00082000
*       IF BIT0007X(2)='0'B THEN    /* IF ECB NOT ALREADY POSTED     */ 00083000
         TM    BIT0007X+976(APCTPTR),B'01000000'                01S0098 00084000
         BNZ   @RF00098                                         01S0098 00085000
*         DO;                       /* THEN ISSUE WAIT SVC           */ 00086000
*           GPR01P=ADDR(APCTECPW);  /* POINT TO ECB                  */ 00087000
         LA    GPR01P,APCTECPW(,APCTPTR)                        01S0100 00088000
*           GPR00F=1|'80000000'X;   /* WAIT COUNT                    */ 00089000
         L     GPR00F,@CF00871                                  01S0101 00090000
*           SVC(1);                 /* WAIT FOR ECB POSTING          */ 00091000
         SVC   1                                                01S0102 00092000
*         END;                      /* ECB WAIT COMPLETE             */ 00093000
*       RFY                                                     01S0104 00094000
*        (GPR00F,                                               01S0104 00095000
*         GPR01P) UNRSTD;           /* FINISHED WITH REGISTERS       */ 00096000
@RF00098 DS    0H                                               01S0105 00097000
*     END;                          /* ECB WAIT COMPLETE WAIT FOR A     00098000
*                                      COMMAND TO PROC.              */ 00099000
*     RESPECIFY                                                 01S0106 00100000
*       APCQ BASED(APCQPTR);        /* BASE POINTER TO THE APCQ.     */ 00101000
*     RESPECIFY                                                 01S0107 00102000
*       APCQPTR RESTRICTED;         /* POINTER TO THE APCQ.          */ 00103000
*     CALL APCVZDX3;                /* RESET THE ECB, AND REMOVE THE    00104000
*                                      OLDEST APCQ FROM THE QUEUE.      00105000
*                                      THE APCQ ADDRESS WILL BE 01S0108 00106000
*                                      PLACED IN APCQPTR.            */ 00107000
         L     @15,APCTZDX3(,APCTPTR)                           01S0108 00108000
         BALR  @14,@15                                          01S0108 00109000
*     DO WHILE APCQPTR^=0;          /* PROCESS ALL COMMANDS HAVE BEEN   00110000
*                                      REMOVED FROM THE QUEUE,  01S0109 00111000
*                                      SIGNIFIED BY THE APCQPTR BEING   00112000
*                                      ZERO ON RETURN FROM APCVZDX3     00113000
*                                      AT THE END OF THE LOOP.       */ 00114000
         B     @DE00109                                         01S0109 00115000
@DL00109 DS    0H                                               01S0110 00116000
*       IF APCQINT='0'B THEN        /* IF THE COMMAND WAS NOT   01S0110 00117000
*                                      INTERNALLY GENERATED, IT MUST    00118000
*                                      BE PARSED.                    */ 00119000
         TM    APCQINT(APCQPTR),B'10000000'                     01S0110 00120000
         BNZ   @RF00110                                         01S0110 00121000
*         DO;                                                   01S0111 00122000
*           RECURSE='0'B;           /* RESET RECURSIVE ABEND FLAG.   */ 00123000
         NI    RECURSE,B'10111111'                              01S0112 00124000
*           STM(R0P,R15P,D40REGS);  /* SAVE RETRY REGS.              */ 00125000
         STM   R0P,R15P,D40REGS                                 01S0113 00126000
*           INUSER='1'B;            /* WE ARE NOW IN THE USER.       */ 00127000
*                                                               01S0114 00128000
         OI    INUSER,B'10000000'                               01S0114 00129000
*           /*********************************************************/ 00130000
*           /*                                                       */ 00131000
*           /* WE NOW CALL AN INSTALLATION ROUTINE TO ALLOW THEM TO  */ 00132000
*           /* APPROVE OR DISAPPROVE THE COMMAND. THEY MAY ALSO      */ 00133000
*           /* EXECUTE IT IF THEY SO CHOOSE. TO TELL US WHICH OPTION */ 00134000
*           /* WAS SELECTED, R15 IS EXAMINED. RC = 0 MEANS THAT THE  */ 00135000
*           /* MONITOR IS TO PROCESS IT. THE ROUTINE MAY HAVE ALTERED*/ 00136000
*           /* THE ORIGINAL CONTENTS IF HE WANTED TO. RC = 4 MEANS   */ 00137000
*           /* THAT THE EXIT ROUTINE PROCESSED THE COMMAND. THIS     */ 00138000
*           /* FEATURE ALLOWS INSTALLATION DEFINED COMMANDS THAT THE */ 00139000
*           /* MONITOR NEED NOT BE CONCERNED WITH. ANY OTHER RETURN  */ 00140000
*           /* CODE MEANS THAT THE INSTALLATION REJECTED THE COMMAND.*/ 00141000
*           /* --------------------------------- ONLY OPERATOR       */ 00142000
*           /* GENERATED COMMANDS ARE PASSED TO THE EXIT ROUTINE.    */ 00143000
*           /* COMMANDS INTERNALLY GENERATED BY THE MONITOR ARE NOT  */ 00144000
*           /* PASSED TO THE EXIT. --------------------------- THE   */ 00145000
*           /* FORMAT OF THE INPUT IS A STANDARD PARMATER LIST       */ 00146000
*           /* POINTING TO A HALF WORD CONTAINING THE LENGTH OF THE  */ 00147000
*           /* COMMAND FOLLOWED BY THE COMMAND (SAME AS A VL RECORD).*/ 00148000
*           /* THE LENGTH OF THE COMMAND DATA MAY BE CHANGED AS LONG */ 00149000
*           /* AS THE DATA DOES NOT EXCEED 140 CHARACTERS IN LENGTH  */ 00150000
*           /* AND THE HALF WORD CONTAINING THE LENGTH IS UPDATED.   */ 00151000
*           /*                                                       */ 00152000
*           /*********************************************************/ 00153000
*                                                               01S0115 00154000
*           CALL APCV1CMD(APCQTXLN);/* LET USER VERIFY COMMAND.      */ 00155000
         LA    @08,APCQTXLN(,APCQPTR)                           01S0115 00156000
         ST    @08,@AL00001                                     01S0115 00157000
         L     @15,APCT1CMD(,APCTPTR)                           01S0115 00158000
         LA    @01,@AL00001                                     01S0115 00159000
         BALR  @14,@15                                          01S0115 00160000
*           RESPECIFY                                           01S0116 00161000
*             R15P RESTRICTED;      /* SAVE USER RETURN CODE.        */ 00162000
*           INUSER='0'B;            /* WE ARE NOW OUT OF USER.       */ 00163000
         NI    INUSER,B'01111111'                               01S0117 00164000
*           IF R15P=0 THEN          /* MEANS HE OKED IT.             */ 00165000
         LTR   R15P,R15P                                        01S0118 00166000
         BNZ   @RF00118                                         01S0118 00167000
*             DO;                                               01S0119 00168000
*               RESPECIFY                                       01S0120 00169000
*                 R15P UNRESTRICTED;/* NO LONGER NEED RC.            */ 00170000
*               IF APCQTXLN>40 THEN /* SET PROPER LENGTH.            */ 00171000
         LA    @08,40                                           01S0121 00172000
         CH    @08,APCQTXLN(,APCQPTR)                           01S0121 00173000
         BNL   @RF00121                                         01S0121 00174000
*                 TXLEN=40;                                     01S0122 00175000
         ST    @08,TXLEN                                        01S0122 00176000
*               ELSE                                            01S0123 00177000
*                 TXLEN=APCQTXLN;   /* USE WHAT WE WERE GIVEN.       */ 00178000
         B     @RC00121                                         01S0123 00179000
@RF00121 LH    @08,APCQTXLN(,APCQPTR)                           01S0123 00180000
         ST    @08,TXLEN                                        01S0123 00181000
*               CALL APCVZDWT(IGT212I,/* LOG THE COMMAND.            */ 00182000
*                   14,APCQCMND,TXLEN);                         01S0124 00183000
@RC00121 LA    @08,@CF00579                                     01S0124 00184000
         ST    @08,@AL00001                                     01S0124 00185000
         LA    @08,@CF00393                                     01S0124 00186000
         ST    @08,@AL00001+4                                   01S0124 00187000
         LA    @08,APCQCMND(,APCQPTR)                           01S0124 00188000
         ST    @08,@AL00001+8                                   01S0124 00189000
         LA    @08,TXLEN                                        01S0124 00190000
         ST    @08,@AL00001+12                                  01S0124 00191000
         MVI   @AL00001+12,X'80'                                01S0124 00192000
         L     @15,APCTZDWT(,APCTPTR)                           01S0124 00193000
         LA    @01,@AL00001                                     01S0124 00194000
         BALR  @14,@15                                          01S0124 00195000
*               CALL PARSE;         /* PARSE THE COMMAND.            */ 00196000
         BAL   @14,PARSE                                        01S0125 00197000
*               RESPECIFY                                       01S0126 00198000
*                 R15P RESTRICTED;  /* USER RETURN CODE.             */ 00199000
*             END;                                              01S0127 00200000
*           ELSE                                                01S0128 00201000
*             IF R15P^=4 THEN       /* 4 MEANS THAT USER PROCESSED      00202000
*                                      IT.                           */ 00203000
         B     @RC00118                                         01S0128 00204000
@RF00118 C     R15P,@CF00062                                    01S0128 00205000
         BE    @RF00128                                         01S0128 00206000
*               DO;                                             01S0129 00207000
*                 APCQIINF=APCQERR; /* SER ERROR CODE.               */ 00208000
         MVC   APCQIINF(4,APCQPTR),@CF00038                     01S0130 00209000
*                 CODE='12';                                    01S0131 00210000
         MVC   CODE(2),@CC00658                                 01S0131 00211000
*               END;                                            01S0132 00212000
*             ELSE                  /* USER DID ALL THAT WAS    01S0133 00213000
*                                      NECESSARY                     */ 00214000
*               APCQIINF=APCQNOP;   /* SO WE DONE HAVE ANYTHING TO      00215000
*                                      DO.                           */ 00216000
         B     @RC00128                                         01S0133 00217000
@RF00128 MVC   APCQIINF(4,APCQPTR),@CF00047                     01S0133 00218000
*           RESPECIFY                                           01S0134 00219000
*             R15P UNRESTRICTED;    /* FREE R15.                     */ 00220000
@RC00128 DS    0H                                               01S0134 00221000
@RC00118 DS    0H                                               01S0135 00222000
*         END;                                                  01S0135 00223000
*                                                               01S0135 00224000
*       /*************************************************************/ 00225000
*       /*                                                           */ 00226000
*       /* SELECT THE PROPER COMMAND PROCESSING ROUTINE.             */ 00227000
*       /*                                                           */ 00228000
*       /*************************************************************/ 00229000
*                                                               01S0136 00230000
*       DO;                         /* CASE(APCQIINF)NAMES(DISP,DUMP,   00231000
*                                      ENAB,DSAB,IPL,UNLD,IC,HALT,DUL   00232000
*                                      S,MDMP,PTCH,FDMP,UDMP,RIPL,ERR   00233000
*                                      ,NOP,DWI)                     */ 00234000
@RF00110 DS    0H                                               01S0137 00235000
*         DCL                                                   01S0137 00236000
*           CL0001@ LABEL BASED(CP0001@(APCQIINF)) VALUERANGE(DISP,DUMP 00237000
*               ,ENAB,DSAB,IPL,UNLD,IC,HALT,DULS,MDMP,PTCH,FDMP,UDMP,   00238000
*               RIPL,ERR,NOP,DWI);  /* BRANCH TARGET                 */ 00239000
*         DCL                                                   01S0138 00240000
*           CP0001@(17) PTR(31) INIT(ADDR(DISP),ADDR(DUMP),ADDR(ENAB),  00241000
*               ADDR(DSAB),ADDR(IPL),ADDR(UNLD),ADDR(IC),ADDR(HALT),    00242000
*               ADDR(DULS),ADDR(MDMP),ADDR(PTCH),ADDR(FDMP),ADDR(UDMP), 00243000
*               ADDR(RIPL),ADDR(ERR),ADDR(NOP),ADDR(DWI));      01S0138 00244000
*         GOTO CL0001@;             /* BRANCH TO SELECTED CASE  01S0139 00245000
*                                      ROUTINES MATCH UP WITH APCQ      00246000
*                                      COMMAND OP CODES.             */ 00247000
         L     @08,APCQIINF(,APCQPTR)                           01S0139 00248000
         SLA   @08,2                                            01S0139 00249000
         L     @08,CP0001@-4(@08)                               01S0139 00250000
         BR    @08                                              01S0139 00251000
*DISP:                                                          01S0140 00252000
*         DO;                       /* SUBCASE                       */ 00253000
DISP     DS    0H                                               01S0141 00254000
*           CALL APCVZD41;          /* PROCESS DISPLAY COMMAND.      */ 00255000
         L     @15,APCTZD41(,APCTPTR)                           01S0141 00256000
         BALR  @14,@15                                          01S0141 00257000
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */ 00258000
         B     CE0001@                                          01S0142 00259000
*         END;                      /* END OF SUBCASE                */ 00260000
*DUMP:                                                          01S0144 00261000
*FDMP:                                                          01S0144 00262000
*UDMP:                                                          01S0144 00263000
*         DO;                       /* SUBCASE                       */ 00264000
DUMP     DS    0H                                               01S0144 00265000
FDMP     DS    0H                                               01S0144 00266000
UDMP     DS    0H                                               01S0145 00267000
*           CALL APCVZD42;          /* PROCESS THE DUMP AND FORCE       00268000
*                                      DUMP AND USER DUMP COMMANDS.  */ 00269000
         L     @15,APCTZD42(,APCTPTR)                           01S0145 00270000
         BALR  @14,@15                                          01S0145 00271000
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */ 00272000
         B     CE0001@                                          01S0146 00273000
*         END;                      /* END OF SUBCASE                */ 00274000
*ENAB:                                                          01S0148 00275000
*         DO;                       /* SUBCASE                       */ 00276000
ENAB     DS    0H                                               01S0149 00277000
*           CALL APCVZD43;          /* PROCESS THE ENABLE COMMAND.   */ 00278000
         L     @15,APCTZD43(,APCTPTR)                           01S0149 00279000
         BALR  @14,@15                                          01S0149 00280000
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */ 00281000
         B     CE0001@                                          01S0150 00282000
*         END;                      /* END OF SUBCASE                */ 00283000
*DSAB:                                                          01S0152 00284000
*         DO;                       /* SUBCASE                       */ 00285000
DSAB     DS    0H                                               01S0153 00286000
*           CALL APCVZD44;          /* PROCESS THE DISABLE COMMAND.  */ 00287000
         L     @15,APCTZD44(,APCTPTR)                           01S0153 00288000
         BALR  @14,@15                                          01S0153 00289000
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */ 00290000
         B     CE0001@                                          01S0154 00291000
*         END;                      /* END OF SUBCASE                */ 00292000
*IPL:                                                           01S0156 00293000
*         DO;                       /* SUBCASE                       */ 00294000
IPL      DS    0H                                               01S0157 00295000
*           CALL APCVZD45;          /* PROCESS THE IPL COMMAND.      */ 00296000
         L     @15,APCTZD45(,APCTPTR)                           01S0157 00297000
         BALR  @14,@15                                          01S0157 00298000
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */ 00299000
         B     CE0001@                                          01S0158 00300000
*         END;                      /* END OF SUBCASE                */ 00301000
*UNLD:                                                          01S0160 00302000
*         DO;                       /* SUBCASE                       */ 00303000
UNLD     DS    0H                                               01S0161 00304000
*           CALL APCVZD46;          /* PROCESS THE UNLOAD COMMAND.   */ 00305000
         L     @15,APCTZD46(,APCTPTR)                           01S0161 00306000
         BALR  @14,@15                                          01S0161 00307000
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */ 00308000
         B     CE0001@                                          01S0162 00309000
*         END;                      /* END OF SUBCASE                */ 00310000
*IC:                                                            01S0164 00311000
*         DO;                       /* SUBCASE                       */ 00312000
IC       DS    0H                                               01S0165 00313000
*           CALL APCVZD47;          /* PROCESS AN INITIALIZATION        00314000
*                                      COMPLETE COMMAND.             */ 00315000
         L     @15,APCTZD47(,APCTPTR)                           01S0165 00316000
         BALR  @14,@15                                          01S0165 00317000
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */ 00318000
         B     CE0001@                                          01S0166 00319000
*         END;                      /* END OF SUBCASE                */ 00320000
*HALT:                                                          01S0168 00321000
*         DO;                       /* SUBCASE                       */ 00322000
HALT     DS    0H                                               01S0169 00323000
*           CALL APCVZD48;          /* PROCESS THE HALT COMMAND.     */ 00324000
         L     @15,APCTZD48(,APCTPTR)                           01S0169 00325000
         BALR  @14,@15                                          01S0169 00326000
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */ 00327000
         B     CE0001@                                          01S0170 00328000
*         END;                      /* END OF SUBCASE                */ 00329000
*DULS:                                                          01S0172 00330000
*         DO;                       /* SUBCASE                       */ 00331000
DULS     DS    0H                                               01S0173 00332000
*           CALL APCVZD49;          /* PROCESS THE DISABLE UNLOAD       00333000
*                                      SCAN                          */ 00334000
         L     @15,APCTZD49(,APCTPTR)                           01S0173 00335000
         BALR  @14,@15                                          01S0173 00336000
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */ 00337000
         B     CE0001@                                          01S0174 00338000
*         END;                      /* END OF SUBCASE                */ 00339000
*MDMP:                                                          01S0176 00340000
*         DO;                       /* SUBCASE                       */ 00341000
MDMP     DS    0H                                               01S0177 00342000
*           CALL APCVZDMD;          /* PROCESS THE MONITOR DUMP 01S0177 00343000
*                                      COMMAND                       */ 00344000
         L     @15,APCTZDMD(,APCTPTR)                           01S0177 00345000
         BALR  @14,@15                                          01S0177 00346000
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */ 00347000
         B     CE0001@                                          01S0178 00348000
*         END;                      /* END OF SUBCASE                */ 00349000
*RIPL:                                                          01S0180 00350000
*         DO;                       /* SUBCASE                       */ 00351000
RIPL     DS    0H                                               01S0181 00352000
*           CALL APCVZDRI;          /* PROCESS THE RE-IPL REQUEST.   */ 00353000
         L     @15,APCTZDRI(,APCTPTR)                           01S0181 00354000
         BALR  @14,@15                                          01S0181 00355000
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */ 00356000
         B     CE0001@                                          01S0182 00357000
*         END;                      /* END OF SUBCASE                */ 00358000
*ERR:                                                           01S0184 00359000
*PTCH:                                                          01S0184 00360000
*         DO;                       /* SUBCASE                       */ 00361000
*                                                               01S0184 00362000
ERR      DS    0H                                               01S0184 00363000
PTCH     DS    0H                                               01S0185 00364000
*           /*********************************************************/ 00365000
*           /*                                                       */ 00366000
*           /* PROCESS AN INVALID COMMAND VERB.                      */ 00367000
*           /*                                                       */ 00368000
*           /*********************************************************/ 00369000
*                                                               01S0185 00370000
*           IF APCQTXLN>40 THEN     /* IF MORE THAN 40 CHAR OF INPUT */ 00371000
         LA    @08,40                                           01S0185 00372000
         CH    @08,APCQTXLN(,APCQPTR)                           01S0185 00373000
         BNL   @RF00185                                         01S0185 00374000
*             TXLEN=40;             /* THEN ONLY OUTPUT 40.          */ 00375000
         ST    @08,TXLEN                                        01S0186 00376000
*           ELSE                                                01S0187 00377000
*             TXLEN=APCQTXLN;       /* USE WHAT WE ARE GIVEN.        */ 00378000
         B     @RC00185                                         01S0187 00379000
@RF00185 LH    @08,APCQTXLN(,APCQPTR)                           01S0187 00380000
         ST    @08,TXLEN                                        01S0187 00381000
*           QUOTE=TXLEN+15;         /* OFFSET TO CLOSING QUOTE.      */ 00382000
@RC00185 L     @08,TXLEN                                        01S0188 00383000
         LA    @08,15(,@08)                                     01S0188 00384000
         STH   @08,QUOTE                                        01S0188 00385000
*           CALL APCVZDWT           /* CALL THE ERROR MESSAGE WRITER.*/ 00386000
*               (IGT213I,14,APCTSSID,4,/* MESSAGE NUMBER AND SUBSYS. */ 00387000
*               41,CODE,2,          /* ERROR CODE.                   */ 00388000
*               66,APCQCMND,TXLEN,  /* THE COMMAND IN ERROR.         */ 00389000
*               52,QUOTE,2);        /* AND THE LENGTH OF THE COMMAND.*/ 00390000
         MVC   @AL00001(52),@AL00189                            01S0189 00391000
         LA    @08,APCTSSID(,APCTPTR)                           01S0189 00392000
         ST    @08,@AL00001+8                                   01S0189 00393000
         LA    @08,CODE                                         01S0189 00394000
         ST    @08,@AL00001+20                                  01S0189 00395000
         LA    @08,APCQCMND(,APCQPTR)                           01S0189 00396000
