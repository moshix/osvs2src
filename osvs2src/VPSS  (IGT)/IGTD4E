TD4E     TITLE 'IGTD4E - STAE EXIT ROUTINE FOR VPSS MONITOR.           *00001000
                        '                                               00002000
IGTD4E   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTD4E    77.092'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                               01S0002 00010000
         USING IGTD4E,15                                                00011000
*   IF R0P=12 THEN                  /* SEE IF THE SDWA WAS OBTAINED. */ 00012000
         C     R0P,@CF01173                                     01S0100 00013000
         BNE   @RF00100                                         01S0100 00014000
*     DO;                           /* IF R0 = 12, IT WAS NOT.       */ 00015000
*       APCTPTR=PARMAPCT;           /* LOCATE THE APCT.              */ 00016000
         L     APCTPTR,PARMAPCT(,R2P)                           01S0102 00017000
*       APCTACT='0'B;               /* RESET THE ACTIVE INDICATOR.   */ 00018000
*       APCTWARM='1'B;              /* SET WARM START POSSIBLE.      */ 00019000
         L     @11,APCTWARM(,APCTPTR)                           01S0104 00020000
@GL00001 LR    @10,@11                                          01S0104 00021000
         N     @10,@CB01812                                     01S0104 00022000
         O     @10,@CB01812+4                                   01S0104 00023000
         CS    @11,@10,APCTWARM(APCTPTR)                        01S0104 00024000
         BNE   @GL00001                                         01S0104 00025000
*       R15P=0;                     /* LET ABEND CONTINUE.           */ 00026000
         SLR   R15P,R15P                                        01S0105 00027000
*       GEN REFS(R14P)(BR    R14P); /* EXIT TO CALLER.               */ 00028000
         BR    R14P                                                     00029000
*     END;                                                      01S0107 00030000
*   ELSE                            /* SDWA IS PRESENT.              */ 00031000
*     DO;                           /* SO DO NORMAL PROCESSING.      */ 00032000
         B     @RC00100                                         01S0108 00033000
@RF00100 DS    0H                                               01S0109 00034000
*       STM(R14P,R12P,SAVESXXX);    /* SAVE THE REGS..               */ 00035000
         STM   R14P,R12P,SAVESXXX(R13P)                         01S0109 00036000
*       RFY                                                     01S0110 00037000
*         APCODE RSTD;              /* RESTRICT THE BASE REG.        */ 00038000
*       GEN(LR    APCODE,R15P)      /* LOAD THE BASE REG.            */ 00039000
*       SETS(APCODE) REFS(R15P);                                01S0111 00040000
         LR    APCODE,R15P                                              00041000
*       GEN(USING IGTD4E,APCODE);   /* SUPPLY THE USING STATEMENT.   */ 00042000
         USING IGTD4E,APCODE                                            00043000
*       GEN(DROP  R15P);            /* DON'T USE R15 ANY MORE.       */ 00044000
         DROP  R15P                                                     00045000
*       SDWAPTR=R1P;                /* POINTER TO THE STAE WORK AREA.*/ 00046000
         LR    SDWAPTR,R1P                                      01S0114 00047000
*       R2P=SDWAPARM;               /* POINTER TO THE PARMLIST.      */ 00048000
         L     R2P,SDWAPARM(,SDWAPTR)                           01S0115 00049000
*       APCTPTR=PARMAPCT;           /* POINTER TO THE APCT.          */ 00050000
         L     APCTPTR,PARMAPCT(,R2P)                           01S0116 00051000
*       R0P=APSVLN;                 /* SIZE OF REQUIRED SAVE AREA.   */ 00052000
         LA    R0P,80                                           01S0117 00053000
*       GEN(A     R0P,@SIZDATD)     /* SIZE OF THE DYNAMIC AREA.     */ 00054000
*       SETS(R0P);                                              01S0118 00055000
         A     R0P,@SIZDATD                                             00056000
*       GEN(IGTYGSV R,LV=(0),INITIAL=YES);/* GET DYNAMIC AREA.       */ 00057000
         IGTYGSV R,LV=(0),INITIAL=YES                                   00058000
*       RFY                                                     01S0120 00059000
*         APDATA RSTD;              /* RESTRICT THE BASE REG         */ 00060000
*       APDATA=R1P+APSVLN;          /* DATA = ADDRESS, BUT SKIP 01S0121 00061000
*                                                             SAVEARE*/ 00062000
         LA    APDATA,80(,R1P)                                  01S0121 00063000
*       GEN(USING @DATD,APDATA)     /* DEFINE THE BASE REG.          */ 00064000
*       REFS(APDATA);               /*                               */ 00065000
         USING @DATD,APDATA                                             00066000
*       IF PARMRTN=CODED10 THEN     /* SEE IF THIS IS D10 ABENDING.  */ 00067000
         CLC   PARMRTN(2,R2P),@CH00063                          01S0123 00068000
         BNE   @RF00123                                         01S0123 00069000
*         DO;                                                   02S0124 00070000
*                                                               02S0124 00071000
*           /*********************************************************/ 00072000
*           /*                                                       */ 00073000
*           /* PROCESS AN ABEND BY IGTD10.                           */ 00074000
*           /*                                                       */ 00075000
*           /*********************************************************/ 00076000
*                                                               02S0125 00077000
*           CALL HALTD40;           /* CAUSE IGTD40 TO QUIT, WHICH      00078000
*                                      WILL BE SIGNIFIED ALSO BY        00079000
*                                      IGTD20 TERMINATING.           */ 00080000
         BAL   @14,HALTD40                                      02S0125 00081000
*           APCTACT='0'B;           /* INDICATE THAT THE MONITOR IS     00082000
*                                      NO LONGER ACTIVE.             */ 00083000
*           APCTWARM='1'B;          /* THE NEXT TIME UP, A WARM START   00084000
*                                      SHOULD BE ATTEMPTED.          */ 00085000
         L     @11,APCTWARM(,APCTPTR)                           02S0127 00086000
@GL00002 LR    @10,@11                                          02S0127 00087000
         N     @10,@CB01812                                     02S0127 00088000
         O     @10,@CB01812+4                                   02S0127 00089000
         CS    @11,@10,APCTWARM(APCTPTR)                        02S0127 00090000
         BNE   @GL00002                                         02S0127 00091000
*           DO;                     /* SETRP RC(0)WKAREA(SDWAPTR)DUMP   00092000
*                                      (YES)RECORD(YES)              */ 00093000
*             RESPECIFY                                         02S0129 00094000
*              (GPR00P,                                         02S0129 00095000
*               GPR01P,                                         02S0129 00096000
*               GPR14P,                                         02S0129 00097000
*               GPR15P) RSTD;                                   02S0129 00098000
*             GPR01P=SDWAPTR;                                   02S0130 00099000
         LR    GPR01P,SDWAPTR                                   02S0130 00100000
*             GPR01P->SDWARCDE=0;   /* SAVE RC VALUE                 */ 00101000
         MVI   SDWARCDE(GPR01P),X'00'                           02S0131 00102000
*             GPR01P->SDWARCRD='1'B;/* TURN ON RECORD INDICATOR      */ 00103000
         OI    SDWARCRD(GPR01P),B'10000000'                     02S0132 00104000
*             GPR01P->SDWAREQ='1'B; /* TURN ON DUMP INDICATOR        */ 00105000
         OI    SDWAREQ(GPR01P),B'10000000'                      02S0133 00106000
*             RESPECIFY                                         02S0134 00107000
*              (GPR00P,                                         02S0134 00108000
*               GPR01P,                                         02S0134 00109000
*               GPR14P,                                         02S0134 00110000
*               GPR15P) UNRSTD;                                 02S0134 00111000
*           END;                    /* SET RETURN PARMS.             */ 00112000
*         END;                                                  02S0136 00113000
*       ELSE                                                    01S0137 00114000
*         IF PARMRTN=CODED20 THEN   /* SEE IF THIS IS D20 ABENDING.  */ 00115000
         B     @RC00123                                         01S0137 00116000
@RF00123 CLC   PARMRTN(2,R2P),@CH00065                          01S0137 00117000
         BNE   @RF00137                                         01S0137 00118000
*           DO;                                                 02S0138 00119000
*                                                               02S0138 00120000
*             /*******************************************************/ 00121000
*             /*                                                     */ 00122000
*             /* PROCESS AN ABEND BY IGTD20.                         */ 00123000
*             /*                                                     */ 00124000
*             /*******************************************************/ 00125000
*                                                               02S0139 00126000
*             APCTEULD=0;           /* CLEAR THE CROSS MEMORY ECB.   */ 00127000
         SLR   @11,@11                                          02S0139 00128000
         ST    @11,APCTEULD(,APCTPTR)                           02S0139 00129000
*             GEN REFS(CVT,CVTSPOST)(SPOST);/* STOP ANY PENDING CROSS   00130000
*                                      MEMORY POSTS FROM HAPPENING.  */ 00131000
         SPOST                                                          00132000
*             CALL HALTD40;         /* CAUSE IGTD40 TO TERMINATE.    */ 00133000
         BAL   @14,HALTD40                                      02S0141 00134000
*             DO;                   /* SETRP RC(0)WKAREA(SDWAPTR)DUMP   00135000
*                                      (YES)RECORD(YES)              */ 00136000
*               RESPECIFY                                       02S0143 00137000
*                (GPR00P,                                       02S0143 00138000
*                 GPR01P,                                       02S0143 00139000
*                 GPR14P,                                       02S0143 00140000
*                 GPR15P) RSTD;                                 02S0143 00141000
*               GPR01P=SDWAPTR;                                 02S0144 00142000
         LR    GPR01P,SDWAPTR                                   02S0144 00143000
*               GPR01P->SDWARCDE=0; /* SAVE RC VALUE                 */ 00144000
         MVI   SDWARCDE(GPR01P),X'00'                           02S0145 00145000
*               GPR01P->SDWARCRD='1'B;/* TURN ON RECORD INDICATOR    */ 00146000
         OI    SDWARCRD(GPR01P),B'10000000'                     02S0146 00147000
*               GPR01P->SDWAREQ='1'B;/* TURN ON DUMP INDICATOR       */ 00148000
         OI    SDWAREQ(GPR01P),B'10000000'                      02S0147 00149000
*               RESPECIFY                                       02S0148 00150000
*                (GPR00P,                                       02S0148 00151000
*                 GPR01P,                                       02S0148 00152000
*                 GPR14P,                                       02S0148 00153000
*                 GPR15P) UNRSTD;                               02S0148 00154000
*             END;                  /* SET RETURN PARMS.             */ 00155000
*           END;                                                02S0150 00156000
*         ELSE                      /* IT MUST BE D40.               */ 00157000
*           DO;                                                 02S0151 00158000
*                                                               02S0151 00159000
         B     @RC00137                                         02S0151 00160000
@RF00137 DS    0H                                               02S0152 00161000
*             /*******************************************************/ 00162000
*             /*                                                     */ 00163000
*             /* PROCESS AN ABEND OF IGTD40.                         */ 00164000
*             /*                                                     */ 00165000
*             /*******************************************************/ 00166000
*                                                               02S0152 00167000
*             IF INUSER=ON&         /* DETERMINE IF THE ABEND   02S0152 00168000
*                                      OCCURRED IN THE USER EXITS,      00169000
*                                      EITHER IN THE COMMAND EXIT OR    00170000
*                                      THE REIPL EXIT . IF THIS IS      00171000
*                                      WHERE THE ABEND OCCURRED, THE    00172000
*                                      PHILOSOPHY IS TO GIVE THE GUY    00173000
*                                      A DUMP, BUT NOT TAKE THE 02S0152 00174000
*                                      MONITOR DOWN.                 */ 00175000
*                 RECURSE=OFF THEN  /* IF RECURSIVE ABEND THEN QUIT. */ 00176000
         TM    INUSER(R2P),B'10000000'                          02S0152 00177000
         BNO   @RF00152                                         02S0152 00178000
         TM    RECURSE(R2P),B'01000000'                         02S0152 00179000
         BNZ   @RF00152                                         02S0152 00180000
*               DO;                                             02S0153 00181000
*                 INUSER=OFF;       /* TURN OFF THE USER INDICATOR.  */ 00182000
*                 RECURSE=ON;       /* INDICATE THAT THE NEXT ONE IS    00183000
*                                      A BAD ONE.                    */ 00184000
         OI    RECURSE(R2P),B'01000000'                         02S0155 00185000
         NI    INUSER(R2P),B'01111111'                          02S0155 00186000
*                 SDWASRSV=PARMREGS;/* SET UP RETRY REGS.            */ 00187000
         MVC   SDWASRSV(64,SDWAPTR),PARMREGS(R2P)               02S0156 00188000
*                 R2P=PARMRTRY;     /* ADDRESS OF RETRY RTN.         */ 00189000
         L     R2P,PARMRTRY(,R2P)                               02S0157 00190000
*                 DO;               /* SETRP RC(4)DUMP(YES)FRESDWA(YE   00191000
*                                      S)RETADDR(R2P->LABEL)WKAREA(SD   00192000
*                                      WAPTR)                        */ 00193000
*                   RESPECIFY                                   02S0159 00194000
*                    (GPR00P,                                   02S0159 00195000
*                     GPR01P,                                   02S0159 00196000
*                     GPR14P,                                   02S0159 00197000
*                     GPR15P) RSTD;                             02S0159 00198000
*                   GPR01P=SDWAPTR;                             02S0160 00199000
         LR    GPR01P,SDWAPTR                                   02S0160 00200000
*                   GPR01P->SDWARCDE=4;/* STORE RC INTO SDWA         */ 00201000
         MVI   SDWARCDE(GPR01P),X'04'                           02S0161 00202000
*                   GPR01P->SDWARTYA=ADDR(R2P->LABEL);/* SAVE RETRY     00203000
*                                      ADDRESS                       */ 00204000
         ST    R2P,SDWARTYA(,GPR01P)                            02S0162 00205000
*                   GPR01P->SDWAFREE='1'B;/* SET FLAG TO FREE SDWA   */ 00206000
         OI    SDWAFREE(GPR01P),B'00000100'                     02S0163 00207000
*                   GPR01P->SDWAREQ='1'B;/* TURN ON DUMP INDICATOR   */ 00208000
         OI    SDWAREQ(GPR01P),B'10000000'                      02S0164 00209000
*                   RESPECIFY                                   02S0165 00210000
*                    (GPR00P,                                   02S0165 00211000
*                     GPR01P,                                   02S0165 00212000
*                     GPR14P,                                   02S0165 00213000
*                     GPR15P) UNRSTD;                           02S0165 00214000
*                 END;              /* TRY TO RETRY.                 */ 00215000
*               END;                                            02S0167 00216000
*             ELSE                                              02S0168 00217000
*               DO;                 /* SETRP RC(0)WKAREA(SDWAPTR)DUMP   00218000
*                                      (YES)RECORD(YES)              */ 00219000
         B     @RC00152                                         02S0168 00220000
@RF00152 DS    0H                                               02S0169 00221000
*                 RESPECIFY                                     02S0169 00222000
*                  (GPR00P,                                     02S0169 00223000
*                   GPR01P,                                     02S0169 00224000
*                   GPR14P,                                     02S0169 00225000
*                   GPR15P) RSTD;                               02S0169 00226000
*                 GPR01P=SDWAPTR;                               02S0170 00227000
         LR    GPR01P,SDWAPTR                                   02S0170 00228000
*                 GPR01P->SDWARCDE=0;/* SAVE RC VALUE                */ 00229000
         MVI   SDWARCDE(GPR01P),X'00'                           02S0171 00230000
*                 GPR01P->SDWARCRD='1'B;/* TURN ON RECORD INDICATOR  */ 00231000
         OI    SDWARCRD(GPR01P),B'10000000'                     02S0172 00232000
*                 GPR01P->SDWAREQ='1'B;/* TURN ON DUMP INDICATOR     */ 00233000
         OI    SDWAREQ(GPR01P),B'10000000'                      02S0173 00234000
*                 RESPECIFY                                     02S0174 00235000
*                  (GPR00P,                                     02S0174 00236000
*                   GPR01P,                                     02S0174 00237000
*                   GPR14P,                                     02S0174 00238000
*                   GPR15P) UNRSTD;                             02S0174 00239000
*               END;                /* SET RETURN PARMS.             */ 00240000
*           END;                    /* END OF D40ABEND.              */ 00241000
@RC00152 DS    0H                                               02S0177 00242000
*       R0P=APSVLN;                 /* SIZE OF REQUIRED SAVE AREA.   */ 00243000
@RC00137 DS    0H                                               01S0177 00244000
@RC00123 LA    R0P,80                                           01S0177 00245000
*       R1P=APDATA-R0P;             /* REAL ADDR OF GOTTEN STORAGE.  */ 00246000
         LR    R1P,APDATA                                       01S0178 00247000
         SLR   R1P,R0P                                          01S0178 00248000
*       GEN REFS(R13P,APDATA) SETS(R0P,R1P,APDATA);             01S0179 00249000
*                                                   /* EPILOG.       */ 00250000
       A     R0P,@SIZDATD         /* SIZE OF THE DYNAMIC AREA.       */ 00251000
       IGTYFSV R,LV=(0),A=(1)     /* GIVE BACK THE DYNAMIC AREA.     */ 00252000
*       LM(R14P,R12P,SAVESXXX);     /* RESTORE THE REGS.             */ 00253000
         LM    R14P,R12P,SAVESXXX(R13P)                         01S0180 00254000
*       R15P=0;                     /* SET THE RETURN CODE.          */ 00255000
         SLR   R15P,R15P                                        01S0181 00256000
*       RETURN;                     /* GO BACK.                      */ 00257000
@EL00001 DS    0H                                               01S0182 00258000
@EF00001 DS    0H                                               01S0182 00259000
@ER00001 BR    @14                                              01S0182 00260000
*     END;                                                      01S0183 00261000
*   RETURN;                         /* RETURN TO CALLER.             */ 00262000
         B     @EL00001                                         01S0184 00263000
*                                                               01S0185 00264000
*   /*****************************************************************/ 00265000
*   /*                                                               */ 00266000
*   /* THIS ROUTINE CAUSES TERMINATION OF IGTD40 BY SETTING THE      */ 00267000
*   /* MONITOR HALT INDICATOR. AS SOON AS THE EXISTING COMMANDS ARE  */ 00268000
*   /* PROCESSED, IGTD40 WILL TERMINATE, THEREBY CAUSING IGTD20 TO   */ 00269000
*   /* TERMINATE (ASSUMING THAT IGTD20 IS NOT THE ABENDING TASK). THE*/ 00270000
*   /* ROUTINE THEN WAITS ON THE ECB GIVEN AS INPUT, OR ONE MINUTE,  */ 00271000
*   /* WHICH EVER COMES FIRST. THEN IT RETURNS TO THE CALLER.        */ 00272000
*   /*                                                               */ 00273000
*   /*****************************************************************/ 00274000
*                                                               01S0185 00275000
*HALTD40:                                                       01S0185 00276000
*   PROC;                                                       01S0185 00277000
HALTD40  STM   @14,@12,@SA00002                                 01S0185 00278000
*   APCTHALT='1'B;                  /* TELL IGTD40 TO QUIT.          */ 00279000
         L     @11,APCTHALT(,APCTPTR)                           01S0186 00280000
@GL00003 LR    @10,@11                                          01S0186 00281000
         O     @10,@CF01819                                     01S0186 00282000
         CS    @11,@10,APCTHALT(APCTPTR)                        01S0186 00283000
         BNE   @GL00003                                         01S0186 00284000
*   DO;                             /* IGTYPOST ECB(APCTECPW)        */ 00285000
*     RFY                                                       01S0188 00286000
*      (GPR00P,                                                 01S0188 00287000
*       GPR01P,                                                 01S0188 00288000
*       GPR14F,                                                 01S0188 00289000
*       GPR15F) RSTD;               /* REGISTERS USED                */ 00290000
*     GPR15F=APCTECPW;              /* CURRENT ECB CONTENTS          */ 00291000
         L     GPR15F,APCTECPW(,APCTPTR)                        01S0189 00292000
*LTR0017X:                                                      01S0190 00293000
*     IF GPR15F<0 THEN              /* IF ECB ALREADY                */ 00294000
LTR0017X LTR   GPR15F,GPR15F                                    01S0190 00295000
         BNM   @RF00190                                         01S0190 00296000
*       DO;                         /* WAITED UPON, MUST ISSUE POST  */ 00297000
*         GPR01P=ADDR(APCTECPW);    /* ADDRESS OF ECB TO POST        */ 00298000
         LA    GPR01P,APCTECPW(,APCTPTR)                        01S0192 00299000
*         GPR00P=0;                 /* POST CODE                     */ 00300000
         SLR   GPR00P,GPR00P                                    01S0193 00301000
*         SVC(2);                   /* ISSUE POST                    */ 00302000
         SVC   2                                                01S0194 00303000
*       END;                        /* ECB POSTED                    */ 00304000
*     ELSE                          /* ECB NOT YET POSTED            */ 00305000
*       DO;                         /* SO TRY 'COMPARE-AND-SWAP' POST*/ 00306000
         B     @RC00190                                         01S0196 00307000
@RF00190 DS    0H                                               01S0197 00308000
*         GPR14F='40000000'X;       /* POST CODE                     */ 00309000
         L     GPR14F,@CF01729                                  01S0197 00310000
*         CS(GPR15F,GPR14F,APCTECPW);/* POST THE ECB                 */ 00311000
         CS    GPR15F,@14,APCTECPW(APCTPTR)                     01S0198 00312000
*         BC(7,LTR0017X);           /* RETRY IF 'CS' FAILED          */ 00313000
         BC    7,LTR0017X                                       01S0199 00314000
*       END;                        /* ECB POSTED VIA 'CS'           */ 00315000
*     RFY                                                       01S0201 00316000
*      (GPR00P,                                                 01S0201 00317000
*       GPR01P,                                                 01S0201 00318000
*       GPR14F,                                                 01S0201 00319000
*       GPR15F) UNRSTD;             /* DONE WITH REGS                */ 00320000
@RC00190 DS    0H                                               01S0202 00321000
*   END;                            /* POST LOGIC COMPLETE WAKE UP      00322000
*                                      IGTD40 IF ASLEEP.             */ 00323000
*   GEN REFS(ONEMIN);                                           01S0203 00324000
*  THE REASON FOR THE STIMER IS THAT WHILE WE WANT IGTD40 TO HAVE A     00325000
*  CHANCE TO END GRACEFULLY, WE ARE NOT GOING TO GIVE HIM FOREVER.      00326000
*                                                                       00327000
   STIMER  WAIT,BINTVL=ONEMIN        TIME TO WAIT.                      00328000
*   RETURN;                                                     01S0204 00329000
@EL00002 DS    0H                                               01S0204 00330000
@EF00002 DS    0H                                               01S0204 00331000
@ER00002 LM    @14,@12,@SA00002                                 01S0204 00332000
         BR    @14                                              01S0204 00333000
*   END;                                                        01S0205 00334000
*                                   /* IGTXEPLG                      */ 00335000
*                                                               01S0206 00336000
*   /*****************************************************************/ 00337000
*   /*                                                               */ 00338000
*   /* SYSTEM DECLARES                                               */ 00339000
*   /*                                                               */ 00340000
*   /*****************************************************************/ 00341000
*                                                               01S0206 00342000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 00343000
*     GPR14F FIXED(31) REG(14),                                 01S0206 00344000
*     GPR15F FIXED(31) REG(15),                                 01S0206 00345000
*     GPR00P PTR(31) REG(0),                                    01S0206 00346000
*     GPR01P PTR(31) REG(1),                                    01S0206 00347000
*     GPR14P PTR(31) REG(14),                                   01S0206 00348000
*     GPR15P PTR(31) REG(15);                                   01S0206 00349000
*   DECLARE                         /* COMMON VARIABLES              */ 00350000
*     I256C CHAR(256) BASED,                                    01S0207 00351000
*     I031F FIXED(31) BASED,                                    01S0207 00352000
*     I031P PTR(31) BASED,                                      01S0207 00353000
*     I015F FIXED(15) BASED,                                    01S0207 00354000
*     I015P PTR(15) BASED,                                      01S0207 00355000
*     I008P PTR(8) BASED,                                       01S0207 00356000
*     I001C CHAR(1) BASED;                                      01S0207 00357000
*                                                               01S0208 00358000
*   /*****************************************************************/ 00359000
*   /*                                                               */ 00360000
*   /* MODULE PATCH AREA                                             */ 00361000
*   /*                                                               */ 00362000
*   /*****************************************************************/ 00363000
*                                                               01S0208 00364000
*   DCL                                                         01S0208 00365000
*     APPATCH CHAR(*) INT STATIC LOCAL GEND;                    01S0208 00366000
*   GENERATE DATA DEFS(APPATCH);                                01S0209 00367000
*                                                               01S0210 00368000
*   /*****************************************************************/ 00369000
*   /*                                                               */ 00370000
*   /* GENERAL REGISTER DECLARES                                     */ 00371000
*   /*                                                               */ 00372000
*   /*****************************************************************/ 00373000
*                                                               01S0210 00374000
*   DCL                                                         01S0210 00375000
*     R0F FIXED(31) REG(0),                                     01S0210 00376000
*     R0P PTR(24) REG(0),                                       01S0210 00377000
*     R0RSTD PTR(31) REG(0),                                    01S0210 00378000
*     R1F FIXED(31) REG(1),                                     01S0210 00379000
*     R1P PTR(24) REG(1),                                       01S0210 00380000
