         AH    @04,DVACOUNT                                     03S0273 00412000
         STH   @04,DVACOUNT                                     03S0273 00413000
*              LOWADDR(DVACOUNT) = APCQCMND(CMNDPSTR+PARMLEN:   03S0274 00414000
*                  CMNDPSTR+PARMLEN+2); /* SECURE FIRST ADDR         */ 00415000
         LA    @02,3                                            03S0274 00416000
         LR    @01,@04                                          03S0274 00417000
         MR    @00,@02                                          03S0274 00418000
         LA    @04,LOWADDR-3(@01)                               03S0274 00419000
         LH    @15,PARMLEN                                      03S0274 00420000
         LR    @14,CMNDPSTR                                     03S0274 00421000
         ALR   @14,@15                                          03S0274 00422000
         ALR   @14,APCQPTR                                      03S0274 00423000
         MVC   0(3,@04),APCQCMND-1(@14)                         03S0274 00424000
*              IF DELIMITR(DLMINDEX)=',' THEN  /* IF NOT A RANGE     */ 00425000
         SLR   @04,@04                                          03S0275 00426000
         IC    @04,DLMINDEX                                     03S0275 00427000
