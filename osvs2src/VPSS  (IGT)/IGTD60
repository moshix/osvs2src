*          END;                                                 03S0615 01424000
*        ELSE;                      /* NOT A SHR USER                */ 01425000
@RF00610 DS    0H                                               03S0617 01426000
*        APDSPTR = APDSAPDS;        /* ADVANCE TO NEXT APDS          */ 01427000
         L     APDSPTR,APDSAPDS(,APDSPTR)                       03S0617 01428000
