TD60     TITLE 'IGTD60 --- DISPLAY COMMAND PROCESSING                  *00001000
                        '                                               00002000
IGTD60   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTD60    77.174'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  LA    @08,4095(,@07)                                   01S0001 00012000
         LA    @05,4095(,@08)                                   01S0001 00013000
         USING @PSTART,@07                                      01S0001 00014000
         USING @PSTART+4095,@08                                 01S0001 00015000
         USING @PSTART+8190,@05                                 01S0001 00016000
         L     @00,@SIZDATD                                     01S0001 00017000
         IGTYGSV  R,LV=(0)                                              00018000
         LR    @06,@01                                          01S0001 00019000
         USING @DATD,@06                                        01S0001 00020000
         ST    @13,@SA00001+4                                   01S0001 00021000
         LM    @00,@01,20(@13)                                  01S0001 00022000
         ST    @06,8(,@13)                                      01S0001 00023000
         LR    @13,@06                                          01S0001 00024000
*  DO;                                                          01S0132 00025000
*  APES(1:APESLN-LENGTH(APESSDWA)) = ''X; /* CLEAR OUT ALL OF APES   */ 00026000
         XC    APES(136),APES                                   02S0133 00027000
*  APESID = APESIDC;                /* INDICATE STAE EXIT CASE       */ 00028000
         MVC   APESID(4),@CC00967                               02S0134 00029000
*  APESADDR = ADDR(APES);           /* SET ADDR OF APES IN POSITION  */ 00030000
         LA    @04,APES                                         02S0135 00031000
         ST    @04,APESADDR                                     02S0135 00032000
*  APESAPMV = ADDR(STAEMSGS);       /* LOCATE THE ARRAY OF MESSAGES  */ 00033000
         LA    @04,STAEMSGS                                     02S0136 00034000
         ST    @04,APESAPMV                                     02S0136 00035000
*  APESFLG0 = '60'X;                /* SET BITS                      */ 00036000
         MVI   APESFLG0,X'60'                                   02S0137 00037000
*  APESCODE = CERRN99;              /* MESSAGE CODE                  */ 00038000
         MVI   APESCODE,X'01'                                   02S0138 00039000
*  APESMXCD = CERRN99;              /* MSG ARRAY ENTRY TO BE USED    */ 00040000
         MVI   APESMXCD,X'01'                                   02S0139 00041000
*  APESREGP = ADDR(APESREGZ);       /* SET IT UP                     */ 00042000
         LA    @04,APESREGZ                                     02S0140 00043000
         ST    @04,APESREGP                                     02S0140 00044000
*  APESMOD = 'IGTD60  ';            /* ROUTINE TO BE SERVICED        */ 00045000
         MVC   APESMOD(8),@CC02604                              02S0141 00046000
*  APESPROC = 'IGTD60  ';           /* PROCEDURE NAME OF SAME        */ 00047000
         MVC   APESPROC(8),@CC02604                             02S0142 00048000
*  RFY (R14P,R15P) RSTD;            /* SECURE PARAMETER REGISTERS    */ 00049000
*  R14P = ADDR(RETURN41);           /* POINT TO ADDR OF NORMAL RTRN  */ 00050000
         LA    R14P,RETURN41                                    02S0144 00051000
*  R15P = ADDR(STAERTRY);           /* POINT TO ADDR OF ENTRYPOINT   */ 00052000
         LA    R15P,STAERTRY                                    02S0145 00053000
*  STM(R0P,R15P,APESREGZ);          /* SAVE EVERY ONE OF THEM        */ 00054000
         STM   R0P,R15P,APESREGZ                                02S0146 00055000
*  RFY (R14P,R15P) UNRSTD;          /* GIVE BACK THE ALTERED REGS    */ 00056000
*  RFY (R3P,R4P) RSTD;              /* SECURE REGS FOR STAE MACRO    */ 00057000
*  R3P = APCTZUKA;                  /* EXIT ROUTINE IS IGTUKA        */ 00058000
         L     R3P,APCTZUKA(,APCTPTR)                           02S0149 00059000
*  R4P = ADDR(APESADDR);            /* PASS ON ADDR OF APES          */ 00060000
         LA    R4P,APESADDR                                     02S0150 00061000
*  GENERATE  CODE                   /* ESTABLISH ERROR RECOVERY MODE */ 00062000
*      REFS(D60STAEE,D60STAES,R3P,R4P)                          02S0151 00063000
*      SETS(D60STAEA)                                           02S0151 00064000
*      FLOWS(RETURN41);             /*                          02S0151 00065000
     MVC    D60STAEA(D60STAEE),D60STAES                                 00066000
     IGTYSTAE   (3),CT,PARAM=(4),PURGE=NONE,MF=(E,D60STAEA)             00067000
*  RFY (R3P,R4P) UNRSTD;            /* GIVE BACK THE TWO TURKEYS     */ 00068000
*        /* SET CONTROL BLOCK POINTERS AND INITIALIZE VARIABLES      */ 00069000
*  AIDERROR = FALSE;                /* NO ERROR YET ON AREA ID       */ 00070000
         NI    AIDERROR,B'01111111'                             02S0153 00071000
*  AIDGIVEN = FALSE;                /* AREA ID NOT YET INPUT         */ 00072000
         NI    AIDGIVEN,B'01111111'                             02S0154 00073000
*  CIDERROR = FALSE;                /* NO ERROR YET ON CONSOLE ID    */ 00074000
         NI    CIDERROR,B'01111111'                             02S0155 00075000
*  CIDGIVEN = FALSE;                /* CONSOLE ID NOT YET INPUT      */ 00076000
         NI    CIDGIVEN,B'01111111'                             02S0156 00077000
*  CMDERROR = FALSE;                /* NO ERROR YET ON DISPLAY CMND  */ 00078000
         NI    CMDERROR,B'01111111'                             02S0157 00079000
*  DISPACTV = FALSE;                /* NO REQUEST FOR DISPLAY ACTIVE */ 00080000
         NI    DISPACTV,B'01111111'                             02S0158 00081000
*  DISPUNIT = FALSE;                /* NO REQUEST FOR DISPLAY UNITS  */ 00082000
         NI    DISPUNIT,B'01111111'                             02S0159 00083000
*  DSPBEGUN = FALSE;                /* NO WTO'S YET INITIATED        */ 00084000
         NI    DSPBEGUN,B'01111111'                             02S0160 00085000
*  DSPENDED = FALSE;                /* THEREFORE, NO FINISH TO THEM  */ 00086000
         NI    DSPENDED,B'01111111'                             02S0161 00087000
*  DVACOUNT = ZERO;                 /* NO UNIT ADDR RANGES INPUT     */ 00088000
         SLR   @04,@04                                          02S0162 00089000
         STH   @04,DVACOUNT                                     02S0162 00090000
*  DVAERROR = FALSE;                /* NO ERROR ON AN INPUT DEV ADDR */ 00091000
         NI    DVAERROR,B'01111111'                             02S0163 00092000
*  ENQDAPUB = FALSE;                /* NOT ENQUEUED ON APUB CHAIN    */ 00093000
         NI    ENQDAPUB,B'01111111'                             02S0164 00094000
*  OFFSET#1 = 14;                   /* FIRST OFFSET FOR IGT27X MSGS  */ 00095000
         MVC   OFFSET#1(4),@CF00311                             02S0165 00096000
*  STOPDSPL = FALSE;                /* NO WTO ERROR TO END DISPLAY   */ 00097000
         NI    STOPDSPL,B'01111111'                             02S0166 00098000
*  ITLOADED = FALSE;                /* IEE7603D NOT YET LOADED       */ 00099000
         NI    ITLOADED,B'01111111'                             02S0167 00100000
*  DO INDEX =1 TO LENGTH(XSAMAP) BY 1; /* INITIALIZE XSA STRUCTURE   */ 00101000
         LA    INDEX,1                                          02S0168 00102000
@DL00168 DS    0H                                               02S0169 00103000
*    INITXSA(INDEX:INDEX) = '00'X;  /* MAKE ALL CHAR'S HEX 0         */ 00104000
