*        END;                                                   02S0363 00689000
*      ELSE;                                                    02S0364 00690000
@RF00355 DS    0H                                               02S0365 00691000
*    END;                                                       02S0365 00692000
*  ELSE;                            /* ERROR ALREADY DISCOVERED      */ 00693000
@RF00346 DS    0H                                               02S0367 00694000
*        /* IF AN ERROR WAS FOUND, WTO AN APPROPRIATE MESSAGE        */ 00695000
*    IF CMDERROR=TRUE THEN          /* IF AN ERROR DETECTED IN CMND  */ 00696000
         TM    CMDERROR,B'10000000'                             01S0367 00697000
         BNO   @RF00367                                         01S0367 00698000
*      DO;                          /* IF COMMAND OR AN ID WAS ERROR */ 00699000
*  IF CMDERROR=TRUE THEN                                        02S0369 00700000
