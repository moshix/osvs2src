         BNZ   @RF00346                                         02S0346 00648000
*    DO;                                                        02S0347 00649000
*  ITLOADED = FALSE;                /* INITIALIZE SUCCESS INDICATOR  */ 00650000
         NI    ITLOADED,B'01111111'                             03S0348 00651000
*  GENERATE  CODE                                               03S0349 00652000
*      REFS(LOADFAIL)                                           03S0349 00653000
*      SETS(R0F,R1P)                                            03S0349 00654000
*      FLOWS(LOADFAIL);             /*                          03S0349 00655000
     LOAD  EP=IEE7603D,ERRET=LOADFAIL                                   00656000
*  RFY R0P RSTD;                    /* SECURE RETURN ADDRESS REG     */ 00657000
*  ADDR7603 = R0P;                  /* SAVE ADDRESS OF IEE7603D      */ 00658000
         ST    R0P,ADDR7603                                     03S0351 00659000
*  RFY R0P UNRSTD;                  /* NOW GIVE BACK THE RETURN REG  */ 00660000
*  ITLOADED = TRUE;                 /* INDICATE SUCCESSFUL LOAD      */ 00661000
         OI    ITLOADED,B'10000000'                             03S0353 00662000
*LOADFAIL: ;                        /* NO SETTING IF LOAD FAILED     */ 00663000
