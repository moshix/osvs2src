         BNH   @RF00255                                         02S0255 00372000
*            DO;                                                02S0256 00373000
*  PARMLEN = 8;                     /* COUNT FOR ,DEVICE= KEYWORD    */ 00374000
         MVC   PARMLEN(2),@CH00307                              03S0257 00375000
*  DLMCOUNT = 3;                    /* SCAN FOR , OR BLANK OR -      */ 00376000
         MVI   DLMCOUNT,X'03'                                   03S0258 00377000
*  DVSCANNG = TRUE;                 /* CONTROL FOR LOOP ON ADDR SCAN */ 00378000
         OI    DVSCANNG,B'10000000'                             03S0259 00379000
*  DO WHILE (DVSCANNG=TRUE & PLENLEFT-PARMLEN>0); /*SCAN FOR DEV ADDR*/ 00380000
         B     @DE00260                                         03S0260 00381000
@DL00260 DS    0H                                               03S0261 00382000
*    CALL DELMSCAN;                 /* SCAN FOR NEXT DELIMITER       */ 00383000
         BAL   @14,DELMSCAN                                     03S0261 00384000
*    IF PARMLEN=8 & CHRCOUNT^=3 THEN    /*           IF ,DEVICE= NOT */ 00385000
         CLC   PARMLEN(2),@CH00307                              03S0262 00386000
         BNE   @RF00262                                         03S0262 00387000
         C     CHRCOUNT,@CF00047                                03S0262 00388000
         BE    @RF00262                                         03S0262 00389000
*      DO;                          /* FOLLOWED BY AT LEAST ONE ADDR */ 00390000
*        CMDERROR = TRUE;           /* OR ADDR RANGE, THEN AN ERROR  */ 00391000
         OI    CMDERROR,B'10000000'                             03S0264 00392000
*        DVAERROR = TRUE;           /* INDICATE INVALID DEVICE ADDR  */ 00393000
         OI    DVAERROR,B'10000000'                             03S0265 00394000
*        DVSCANNG = FALSE;          /* SHUT DOWN THE ADDR SCAN LOOP  */ 00395000
         NI    DVSCANNG,B'01111111'                             03S0266 00396000
*      END;                                                     03S0267 00397000
*    ELSE                                                       03S0268 00398000
*      IF CHRCOUNT=3 THEN           /* 3 CHAR FIELD MUST BE AN ADDR  */ 00399000
         B     @RC00262                                         03S0268 00400000
@RF00262 C     CHRCOUNT,@CF00047                                03S0268 00401000
         BNE   @RF00268                                         03S0268 00402000
*        DO;                                                    03S0269 00403000
*          CALL HEXCHECK;           /* VERIFY HEX DIGITS IN ADDR     */ 00404000
         BAL   @14,HEXCHECK                                     03S0270 00405000
*          IF CMDERROR=FALSE THEN   /* IF NO DIGITS WERE NON-HEXADEC */ 00406000
         TM    CMDERROR,B'10000000'                             03S0271 00407000
         BNZ   @RF00271                                         03S0271 00408000
*            DO;                                                03S0272 00409000
*              DVACOUNT = DVACOUNT + 1; /* INCR COUNT OF ADDR PARMS  */ 00410000
