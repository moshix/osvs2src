@RT00279 DS    0H                                               03S0280 00451000
*                    DVSCANNG = FALSE; /* END OF THE ADDRESS SCANNING*/ 00452000
         NI    DVSCANNG,B'01111111'                             03S0280 00453000
*                  ELSE             /* ASSUME ANOTHER ADDRESS COMING */ 00454000
*                    PARMLEN = PARMLEN + 1; /* OTHERWISE SKIP COMMA  */ 00455000
         B     @RC00279                                         03S0281 00456000
@RF00279 LH    @04,PARMLEN                                      03S0281 00457000
         LA    @04,1(,@04)                                      03S0281 00458000
         STH   @04,PARMLEN                                      03S0281 00459000
*                END;                                           03S0282 00460000
*              ELSE                                             03S0283 00461000
*               IF DELIMITR(DLMINDEX)='-' THEN /* IF AN ADDR RANGE   */ 00462000
         B     @RC00275                                         03S0283 00463000
@RF00275 SLR   @04,@04                                          03S0283 00464000
