         CH    @04,PLENLEFT                                     02S0208 00218000
         BNL   @RF00208                                         02S0208 00219000
*          DO;                                                  02S0209 00220000
*  PARMLEN = 3;                     /* COUNT FOR ,L= KEYWORD         */ 00221000
         LA    @02,3                                            03S0210 00222000
         STH   @02,PARMLEN                                      03S0210 00223000
*  DLMCOUNT = 2;                    /* SCAN ONLY FOR , OR BLANK      */ 00224000
         MVI   DLMCOUNT,X'02'                                   03S0211 00225000
*  CALL DELMSCAN;                   /* SCAN TO NEXT DELIMITER        */ 00226000
         BAL   @14,DELMSCAN                                     03S0212 00227000
*  IF CHRCOUNT=2 | CHRCOUNT=3 THEN  /* CONSOLE ID MUST BE INCLUDED   */ 00228000
         C     CHRCOUNT,@CF00298                                03S0213 00229000
         BE    @RT00213                                         03S0213 00230000
         C     CHRCOUNT,@CF00047                                03S0213 00231000
         BNE   @RF00213                                         03S0213 00232000
@RT00213 DS    0H                                               03S0214 00233000
*    DO;                                                        03S0214 00234000
*      CIDGIVEN = TRUE;             /* INDICATE THAT CONSOLE ID GIVEN*/ 00235000
         OI    CIDGIVEN,B'10000000'                             03S0215 00236000
*      CIDCHAR = APCQCMND(CMNDPSTR+PARMLEN:CMNDPSTR+PARMLEN+1); 03S0216 00237000
