         CLI   0(@14),C','                                      03S0275 00429000
         BNE   @RF00275                                         03S0275 00430000
*                DO;                                            03S0276 00431000
*                  HIGHADDR(DVACOUNT) = LOWADDR(DVACOUNT); /*ONE ADDR*/ 00432000
         LA    @04,HIGHADDR-3(@01)                              03S0277 00433000
         LA    @14,LOWADDR-3(@01)                               03S0277 00434000
