*          RFY R2F UNRSTD;          /* UNRESTRICT REG 2              */ 00290000
*        END;                                                   03S0233 00291000
*      ELSE;                        /* OTHERWISE NO NEED TO BOTHER   */ 00292000
@RF00225 DS    0H                                               03S0235 00293000
*    END;                                                       03S0235 00294000
*  ELSE;                                                        03S0236 00295000
@RF00213 DS    0H                                               03S0237 00296000
*  IF CHRCOUNT=1 | CHRCOUNT=3 THEN  /* AREA ID MUST BE INCLUDED      */ 00297000
         C     CHRCOUNT,@CF00296                                03S0237 00298000
         BE    @RT00237                                         03S0237 00299000
