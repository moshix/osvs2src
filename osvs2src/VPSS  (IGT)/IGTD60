*                WPLMCSF = '0040'X; /* INDICATES AN MLWTO            */ 02418000
         MVC   WPLMCSF(2,@12),@CB02647                          03S0951 02419000
*                                   /* NO TEXT FOLLOWS FOR A NULL LIN*/ 02420000
*                WPLPTR = WPLPTR + WPLLGH; /* MOVE ON TO WPLLS01     */ 02421000
         ALR   @12,@02                                          03S0952 02422000
