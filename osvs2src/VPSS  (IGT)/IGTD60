         BNE   @RF00237                                         03S0237 00301000
@RT00237 DS    0H                                               03S0238 00302000
*    DO;                                                        03S0238 00303000
*      AIDGIVEN = TRUE;             /* INDICATE THAT AREA ID GIVEN   */ 00304000
         OI    AIDGIVEN,B'10000000'                             03S0239 00305000
*      XASDID = APCQCMND(CMNDPSTR+PARMLEN+CHRCOUNT-1:CMNDPSTR+PARMLEN   00306000
