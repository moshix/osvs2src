@RF00194 DS    0H                                               02S0200 00187000
*            CMDERROR = TRUE;       /* SIGNAL ERROR IN COMMAND       */ 00188000
         OI    CMDERROR,B'10000000'                             02S0200 00189000
*          END;                                                 02S0201 00190000
*  IF CMDERROR=FALSE THEN           /* IF STILL IN BUSINESS          */ 00191000
@RC00194 DS    0H                                               02S0202 00192000
@RC00189 DS    0H                                               02S0202 00193000
@RC00184 DS    0H                                               02S0202 00194000
@RC00179 TM    CMDERROR,B'10000000'                             02S0202 00195000
         BNZ   @RF00202                                         02S0202 00196000
*    DO;                                                        02S0203 00197000
*      PLENLEFT = PLENLEFT - PARMLEN; /* DECREMENT LENGTH LEFT TO GO */ 00198000
         LH    @04,PARMLEN                                      02S0204 00199000
