*    DO;                                                        02S0370 00703000
*      ERSNCODE = '16';             /* SET IGT213I ERROR REASON CODE */ 00704000
         MVC   ERSNCODE(2),@CC02642                             02S0371 00705000
*      OFFSET#2 = 41;               /* POINT TO CODE POSITION IN MSG */ 00706000
         MVC   OFFSET#2(4),@CF00181                             02S0372 00707000
*      OFFSET#3 = 66;               /* POINT TO TEXT POSITION IN MSG */ 00708000
         MVC   OFFSET#3(4),@CF00231                             02S0373 00709000
*      CALL APCVZDWT(IGT213I,OFFSET#1,APCTSSID,LENGTH(APCTSSID),        00710000
