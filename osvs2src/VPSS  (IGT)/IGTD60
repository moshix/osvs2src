*                /* SCAN TO END OF PARAMETER LIST OR UNTIL ERROR     */ 00208000
*      DO WHILE(PLENLEFT>ZERO & CMDERROR=FALSE); /* CONTINUE SCAN    */ 00209000
         B     @DE00207                                         02S0207 00210000
@DL00207 DS    0H                                               02S0208 00211000
*        IF APCQCMND(CMNDPSTR:CMNDPSTR+2)=',L=' & PLENLEFT>3 THEN /* */ 00212000
         LR    @04,APCQPTR                                      02S0208 00213000
         ALR   @04,CMNDPSTR                                     02S0208 00214000
