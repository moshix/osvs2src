@RF00447 C     BLKCOUNT,@CF00041                                03S0471 01053000
         BNE   @RF00471                                         03S0471 01054000
*                DO;                /* WPLPTR IS ALREADY SET TO WPLML*/ 01055000
*                  WPLML0 = ZERO;   /* THIS DUDE ALWAYS SET TO ZERO  */ 01056000
         L     @02,WPLPTR                                       03S0473 01057000
         MVI   WPLML0(@02),X'00'                                03S0473 01058000
*                  WPLMLLEN = 4 + LINELENA; /* LENGTH OF THIS WPLML  */ 01059000
         MVI   WPLMLLEN(@02),X'4A'                              03S0474 01060000
*                  WPLMLLTF = '2000'X; /* INDICATE DATA LINE         */ 01061000
         MVC   WPLMLLTF(2,@02),@CB02648                         03S0475 01062000
*                  WPLMLTXT(1:LINELENA) = '';   /* CLEAR LEFT COLUMNS*/ 01063000
         MVI   WPLMLTXT(@02),C' '                               03S0476 01064000
         MVC   WPLMLTXT+1(69,@02),WPLMLTXT(@02)                 03S0476 01065000
*                  WPLMLTXT(ABLK1CL1:ABLK1CL1+ABLKLEN-1) = ACTIVTXT;    01066000
         LA    @15,DSPLLINE                                     03S0477 01067000
         MVC   WPLMLTXT+2(17,@02),ACTIVTXT(@15)                 03S0477 01068000
*                  BLKCOUNT = 1;    /* 1ST BLOCK NOW IN PLACE        */ 01069000
         LA    BLKCOUNT,1                                       03S0478 01070000
*                END;                                           03S0479 01071000
*              ELSE                 /* 2ND OR 3RD BLK OF NON-1ST LINE*/ 01072000
*                IF BLKCOUNT=1 THEN /* 2ND BLOCK COMING IN           */ 01073000
         B     @RC00471                                         03S0480 01074000
@RF00471 C     BLKCOUNT,@CF00296                                03S0480 01075000
         BNE   @RF00480                                         03S0480 01076000
*                  DO;                                          03S0481 01077000
*                    WPLMLTXT(ABLK2CL1:ABLK2CL1+ABLKLEN-1) = ACTIVTXT;  01078000
         L     @02,WPLPTR                                       03S0482 01079000
         LA    @15,DSPLLINE                                     03S0482 01080000
         MVC   WPLMLTXT+27(17,@02),ACTIVTXT(@15)                03S0482 01081000
*                    BLKCOUNT = 2;  /* INCREMENT BLOCK COUNT         */ 01082000
         LA    BLKCOUNT,2                                       03S0483 01083000
*                  END;                                         03S0484 01084000
*                ELSE               /* 3RD BLOCK COMING IN           */ 01085000
*                  DO;              /* FINISH OFF THIS NON-1ST LINE  */ 01086000
         B     @RC00480                                         03S0485 01087000
@RF00480 DS    0H                                               03S0486 01088000
*                    WPLMLTXT(ABLK3CL1:ABLK3CL1+ABLKLEN-1) = ACTIVTXT;  01089000
         L     @02,WPLPTR                                       03S0486 01090000
         LA    @15,DSPLLINE                                     03S0486 01091000
         MVC   WPLMLTXT+52(17,@02),ACTIVTXT(@15)                03S0486 01092000
*                    WPLPTR = WPLPTR + WPLMLLEN;/* ADV TO NEXT LINE  */ 01093000
         SLR   @15,@15                                          03S0487 01094000
         IC    @15,WPLMLLEN(,@02)                               03S0487 01095000
         ALR   @15,@02                                          03S0487 01096000
         ST    @15,WPLPTR                                       03S0487 01097000
*                    BLKCOUNT = ZERO; /* RESET TO BEGIN NEXT LINE    */ 01098000
         SLR   BLKCOUNT,BLKCOUNT                                03S0488 01099000
*                    LINCOUNT = LINCOUNT + 1;/* INCREMENT LINE COUNT */ 01100000
         LA    LINCOUNT,1(,LINCOUNT)                            03S0489 01101000
*                  END;                                         03S0490 01102000
*          END;                     /* END OF ACTVQLFY=TRUE          */ 01103000
*        ELSE;                      /* OTHERWISE APDS ISN'T INTRSTNG */ 01104000
@RF00445 DS    0H                                               03S0493 01105000
*        IF LINCOUNT=10 | APDSAPDS=ZERO THEN /* WTO ALL OF THE LINES */ 01106000
@RC00445 C     LINCOUNT,@CF00099                                03S0493 01107000
         BE    @RT00493                                         03S0493 01108000
         L     @02,APDSAPDS(,APDSPTR)                           03S0493 01109000
         LTR   @02,@02                                          03S0493 01110000
         BNZ   @RF00493                                         03S0493 01111000
@RT00493 DS    0H                                               03S0494 01112000
*          DO;                                                  03S0494 01113000
*            IF APDSAPDS=ZERO THEN /* THIS WILL BE FINAL WTO         */ 01114000
         SLR   @02,@02                                          03S0495 01115000
         C     @02,APDSAPDS(,APDSPTR)                           03S0495 01116000
         BNE   @RF00495                                         03S0495 01117000
*              DO;                                              03S0496 01118000
*                IF LINCOUNT=ZERO THEN /* IF NO LINES COMPLETED      */ 01119000
         CR    LINCOUNT,@02                                     03S0497 01120000
         BNE   @RF00497                                         03S0497 01121000
*                  IF BLKCOUNT=ZERO THEN /* AND 1ST LINE  NOT BEGUN  */ 01122000
         CR    BLKCOUNT,@02                                     03S0498 01123000
         BNE   @RF00498                                         03S0498 01124000
*                    IF LINESOUT=ZERO THEN /* AND NONE OUT AT ALL    */ 01125000
         CH    @02,LINESOUT                                     03S0499 01126000
         BNE   @RF00499                                         03S0499 01127000
*                      DO;          /* OUTPUT NOTICE OF NO LINES     */ 01128000
*                        CALL NULLACTV; /* OUTPUT MSG OF NO JOBS     */ 01129000
         BAL   @14,NULLACTV                                     03S0501 01130000
*                      END;         /* END OF NO LINES CASE          */ 01131000
*                    ELSE           /* >=10 LINES PREVIOUSLY WTO'ED  */ 01132000
*                      DO;                                      03S0503 01133000
         B     @RC00499                                         03S0503 01134000
@RF00499 DS    0H                                               03S0504 01135000
*                        WPLPTR = ADDR(ACTVAREA);/* PT TO MSGAREA    */ 01136000
         LA    @02,DATAAREA                                     03S0504 01137000
         ST    @02,WPLPTR                                       03S0504 01138000
*                        WPLLGH = 4 + ZERO; /* THIS WILL BE NULL LINE*/ 01139000
         LA    @15,4                                            03S0505 01140000
         STH   @15,WPLLGH(,@02)                                 03S0505 01141000
*                        WPLMCSF = '0040'X; /* DENOTES MLWTO         */ 01142000
         MVC   WPLMCSF(2,@02),@CB02647                          03S0506 01143000
*                                   /* NO TEXT ON THE NULL END LINE  */ 01144000
*                        WPLPTR = WPLPTR + WPLLGH;/* ADV TO WPLLS01  */ 01145000
         ALR   @02,@15                                          03S0507 01146000
         ST    @02,WPLPTR                                       03S0507 01147000
*                        WPLLTF = '1000'X; /* MERELY AN END LINE     */ 01148000
         MVC   WPLLTF(2,@02),@CB02650                           03S0508 01149000
*                        WPLAREA = XASDID; /* MOVE IN AREA ID        */ 01150000
         L     @01,XSAPTR                                       03S0509 01151000
         MVC   WPLAREA(1,@02),XASDID+32(@01)                    03S0509 01152000
*                        WPLLINES = 1; /* ACTUAL NO. OF LINES        */ 01153000
         MVI   WPLLINES(@02),X'01'                              03S0510 01154000
*                      END;                                     03S0511 01155000
*                  ELSE             /*BLKCOUNT>ZERO - FINISH 1ST LINE*/ 01156000
*                    DO;            /* WHETHER LINESOUT =ZERO OR ^=0 */ 01157000
         B     @RC00498                                         03S0512 01158000
@RF00498 DS    0H                                               03S0513 01159000
*                      WPLPTR = WPLPTR + WPLLGH; /* ADV TO WPLLS01   */ 01160000
         L     @02,WPLPTR                                       03S0513 01161000
         LH    @15,WPLLGH(,@02)                                 03S0513 01162000
         ALR   @15,@02                                          03S0513 01163000
         ST    @15,WPLPTR                                       03S0513 01164000
*                      WPLLTF = '3000'X; /* DATA LINE & ALSO END LINE*/ 01165000
         MVC   WPLLTF(2,@15),@CB02651                           03S0514 01166000
*                      WPLAREA = XASDID; /* MOVE IN AREA ID          */ 01167000
         L     @02,XSAPTR                                       03S0515 01168000
         MVC   WPLAREA(1,@15),XASDID+32(@02)                    03S0515 01169000
*                      WPLLINES = 1; /* ACTUAL NO. OF LINES          */ 01170000
         MVI   WPLLINES(@15),X'01'                              03S0516 01171000
*                    END;                                       03S0517 01172000
*                ELSE               /* LINCOUNT>0                    */ 01173000
*                  IF LINCOUNT=1 & BLKCOUNT=ZERO THEN /*IF 2ND UNBEGN*/ 01174000
         B     @RC00497                                         03S0518 01175000
@RF00497 C     LINCOUNT,@CF00296                                03S0518 01176000
         BNE   @RF00518                                         03S0518 01177000
         C     BLKCOUNT,@CF00041                                03S0518 01178000
         BNE   @RF00518                                         03S0518 01179000
*                    DO;                                        03S0519 01180000
*                      WPLPTR = WPLPTR - LENGTH(WPLLS01);/* BACK UP  */ 01181000
         L     @02,WPLPTR                                       03S0520 01182000
         SL    @02,@CF00301                                     03S0520 01183000
         ST    @02,WPLPTR                                       03S0520 01184000
*                      WPLLTF = '3000'X; /* DATA LINE & ALSO END LINE*/ 01185000
         MVC   WPLLTF(2,@02),@CB02651                           03S0521 01186000
*                      WPLLINES = 1; /* ACTUAL NO. OF LINES          */ 01187000
         MVI   WPLLINES(@02),X'01'                              03S0522 01188000
*                    END;                                       03S0523 01189000
*                  ELSE             /* SOMEWHERE BEYOND 1ST LINE     */ 01190000
*                    DO;                                        03S0524 01191000
         B     @RC00518                                         03S0524 01192000
@RF00518 DS    0H                                               03S0525 01193000
*                      IF BLKCOUNT=ZERO THEN /* IF NEW LINE NOT BEGUN*/ 01194000
         C     BLKCOUNT,@CF00041                                03S0525 01195000
         BNE   @RF00525                                         03S0525 01196000
*                        DO;                                    03S0526 01197000
*                          WPLPTR = WPLPTR - (4+LINELENA); /*BACK UP1*/ 01198000
         L     @02,WPLPTR                                       03S0527 01199000
         SL    @02,@CF00254                                     03S0527 01200000
         ST    @02,WPLPTR                                       03S0527 01201000
*                        END;                                   03S0528 01202000
*                      ELSE         /* OTHERWISE, IN A PARTIAL LINE  */ 01203000
*                        DO;                                    03S0529 01204000
         B     @RC00525                                         03S0529 01205000
@RF00525 DS    0H                                               03S0530 01206000
*                          LINCOUNT = LINCOUNT + 1;/*ADD FOR PARTIAL */ 01207000
         LA    LINCOUNT,1(,LINCOUNT)                            03S0530 01208000
*                        END;                                   03S0531 01209000
*                      WPLMLLTF = '3000'X; /* LAST DATA LINE IS END  */ 01210000
@RC00525 L     @02,WPLPTR                                       03S0532 01211000
         MVC   WPLMLLTF(2,@02),@CB02651                         03S0532 01212000
*                      WPLPTR = ADDR(ACTVAREA) + ADDR(ACTVAREA)->       01213000
*                          WPLLGH;  /* POINT TO WPLLS01              */ 01214000
         LA    @02,DATAAREA                                     03S0533 01215000
         LH    @15,WPLLGH(,@02)                                 03S0533 01216000
         ALR   @15,@02                                          03S0533 01217000
         ST    @15,WPLPTR                                       03S0533 01218000
*                      WPLLINES = LINCOUNT; /* INSERT NO. OF LINES   */ 01219000
         STC   LINCOUNT,WPLLINES(,@15)                          03S0534 01220000
*                    END;                                       03S0535 01221000
*                DSPENDED = TRUE;   /* INDICATE THAT WTO'S ALL OVER  */ 01222000
@RC00518 DS    0H                                               03S0536 01223000
@RC00497 OI    DSPENDED,B'10000000'                             03S0536 01224000
*                LOOPING = FALSE;   /* LAST WTO - THE SHOW IS OVER   */ 01225000
         NI    LOOPING,B'01111111'                              03S0537 01226000
*              END;                 /* END OF PREP FOR FINAL WTO     */ 01227000
*            ELSE                   /* APDSAPDS^=0, HENCE LINCOUNT=10*/ 01228000
*              DO;                  /* & NOT KNOWN TO BE END OF WTO'S*/ 01229000
         B     @RC00495                                         03S0539 01230000
@RF00495 DS    0H                                               03S0540 01231000
*                WPLPTR = ADDR(ACTVAREA) + ADDR(ACTVAREA)->WPLLGH;/* */ 01232000
         LA    @02,DATAAREA                                     03S0540 01233000
         LH    @15,WPLLGH(,@02)                                 03S0540 01234000
         ALR   @15,@02                                          03S0540 01235000
         ST    @15,WPLPTR                                       03S0540 01236000
*                WPLLINES = 10;     /* INSERT ACTUAL NO. OF LINES    */ 01237000
         MVI   WPLLINES(@15),X'0A'                              03S0541 01238000
*              END;                                             03S0542 01239000
*            CALL DOTHEWTO;         /* NOW ACTUALLY DO THE MLWTO     */ 01240000
@RC00495 BAL   @14,DOTHEWTO                                     03S0543 01241000
*            IF LOOPING=TRUE THEN   /* IF WTO'S STILL IN PROGRESS    */ 01242000
         TM    LOOPING,B'10000000'                              03S0544 01243000
         BNO   @RF00544                                         03S0544 01244000
*              IF STOPDSPL=TRUE THEN /* AND DISPLAY IS TERMINATING   */ 01245000
         TM    STOPDSPL,B'10000000'                             03S0545 01246000
         BNO   @RF00545                                         03S0545 01247000
*                DO;                                            03S0546 01248000
*                  LOOPING = FALSE; /* CEASE LOOPING - SHOW IS OVER  */ 01249000
         NI    LOOPING,B'01111111'                              03S0547 01250000
*                  DSPENDED = TRUE; /* NO MORE WTO'S TO BE ISSUED    */ 01251000
         OI    DSPENDED,B'10000000'                             03S0548 01252000
*                END;                                           03S0549 01253000
*              ELSE                 /* OTHERWISE, RESET COUNTERS     */ 01254000
*                DO;                /* AND CONTINUE                  */ 01255000
         B     @RC00545                                         03S0550 01256000
@RF00545 DS    0H                                               03S0551 01257000
*                  LINESOUT = LINESOUT + LINCOUNT; /*ADD ON NEW LINES*/ 01258000
         LR    @02,LINCOUNT                                     03S0551 01259000
         AH    @02,LINESOUT                                     03S0551 01260000
         STH   @02,LINESOUT                                     03S0551 01261000
*                  LINCOUNT = ZERO; /* RESET FOR NEXT 10 LINES       */ 01262000
         SLR   LINCOUNT,LINCOUNT                                03S0552 01263000
*                END;                                           03S0553 01264000
*            ELSE;                  /* OTHERWISE, FINAL LINES NOW OUT*/ 01265000
@RF00544 DS    0H                                               03S0555 01266000
*          END;                     /* END LINCOUNT=10 | APDSPADS=0  */ 01267000
*        ELSE                       /* ACTVQLFY=FALSE - NO MSG ENTRY */ 01268000
*          APDSPTR = APDSAPDS;      /* THEN MOVE ON TO NEXT APDS     */ 01269000
         B     @RC00493                                         03S0556 01270000
@RF00493 L     APDSPTR,APDSAPDS(,APDSPTR)                       03S0556 01271000
*      END;                         /* END DO WHILE(LOOPING=TRUE)    */ 01272000
@RC00493 DS    0H                                               03S0557 01273000
@DE00410 TM    LOOPING,B'10000000'                              03S0557 01274000
         BO    @DL00410                                         03S0557 01275000
*    END;                           /* END FOR APCTAPDS^=ZERO        */ 01276000
*  ELSE                             /* APCTAPDS=ZERO - NO JOBS AT ALL*/ 01277000
*    DO;                                                        03S0559 01278000
         B     @RC00406                                         03S0559 01279000
@RF00406 DS    0H                                               03S0560 01280000
*      CALL NULLACTV;               /* SETUP MSG OF NO JOBS          */ 01281000
         BAL   @14,NULLACTV                                     03S0560 01282000
*      DSPENDED = TRUE;             /* INDICATE THAT WTO'S ALL OVER  */ 01283000
         OI    DSPENDED,B'10000000'                             03S0561 01284000
*      CALL DOTHEWTO;               /* OUTPUT THE NULL MESSAGE       */ 01285000
         BAL   @14,DOTHEWTO                                     03S0562 01286000
*    END;                                                       03S0563 01287000
*    END;                                                       02S0564 01288000
*  ELSE;                                                        02S0565 01289000
@RF00401 DS    0H                                               02S0566 01290000
*  IF STOPDSPL=TRUE THEN            /* IF DISPLAY WAS CUT SHORT      */ 01291000
@RC00401 TM    STOPDSPL,B'10000000'                             02S0566 01292000
         BNO   @RF00566                                         02S0566 01293000
*    DO;                            /* NOTIFY THE OPERATOR           */ 01294000
*      OFFSET#2 = 20;               /* 2ND VARIABLE IN IGT274I       */ 01295000
         MVC   OFFSET#2(4),@CF00276                             02S0568 01296000
*      SIXCHARS = OPERACTV;         /* OPERATION IS DISPLAY ACTIVE   */ 01297000
         MVC   SIXCHARS(6),@CC02589                             02S0569 01298000
*      CALL APCVZDWT(IGT274I,OFFSET#1,APCTSSID,LENGTH(APCTSSID),        01299000
*          OFFSET#2,SIXCHARS,LENGTH(SIXCHARS)); /* CONSOLATION MSG   */ 01300000
         LA    @04,@CF01063                                     02S0570 01301000
         ST    @04,@AL00001                                     02S0570 01302000
         LA    @04,OFFSET#1                                     02S0570 01303000
         ST    @04,@AL00001+4                                   02S0570 01304000
         LA    @04,APCTSSID(,APCTPTR)                           02S0570 01305000
         ST    @04,@AL00001+8                                   02S0570 01306000
         LA    @04,@CF00301                                     02S0570 01307000
         ST    @04,@AL00001+12                                  02S0570 01308000
         LA    @04,OFFSET#2                                     02S0570 01309000
         ST    @04,@AL00001+16                                  02S0570 01310000
         LA    @04,SIXCHARS                                     02S0570 01311000
         ST    @04,@AL00001+20                                  02S0570 01312000
         LA    @04,@CF00292                                     02S0570 01313000
         ST    @04,@AL00001+24                                  02S0570 01314000
         MVI   @AL00001+24,X'80'                                02S0570 01315000
         L     @15,APCTZDWT(,APCTPTR)                           02S0570 01316000
         LA    @01,@AL00001                                     02S0570 01317000
         BALR  @14,@15                                          02S0570 01318000
*    END;                                                       02S0571 01319000
*  ELSE;                                                        02S0572 01320000
@RF00566 DS    0H                                               02S0573 01321000
*          END;                                                 01S0573 01322000
*            /* OTHERWISE DISPLAY UNITS, AS WAS REQUESTED            */ 01323000
*        ELSE                                                   01S0574 01324000
*          DO;                                                  01S0574 01325000
         B     @RC00397                                         01S0574 01326000
@RF00397 DS    0H                                               01S0575 01327000
*  GENERATE  CODE                                               02S0575 01328000
*      REFS(WTO2701A,WTO271S)                                   02S0575 01329000
*      SETS(WTO2701A);              /* PREPARE FOR THE 1ST WTO  02S0575 01330000
     MVC  WTO2701A(WTO271E),WTO271S                                     01331000
*  CALL WTOCNTRL;                   /* WTO THE CONTROL & LABEL LINES */ 01332000
         BAL   @14,WTOCNTRL                                     02S0576 01333000
*  IF STOPDSPL=FALSE THEN           /* IF DISPLAY NOT TO BE ENDED    */ 01334000
         TM    STOPDSPL,B'10000000'                             02S0577 01335000
         BNZ   @RF00577                                         02S0577 01336000
*    DO;                                                        02S0578 01337000
*  USRCOUNT = ZERO;                 /* INITIALLY, NO SHR USERS FOUND */ 01338000
         SLR   USRCOUNT,USRCOUNT                                03S0579 01339000
*  IF APCTAPDS^=ZERO THEN           /* IF THERE IS AT LEAST 1 USER   */ 01340000
         L     @04,APCTAPDS(,APCTPTR)                           03S0580 01341000
         LTR   @04,@04                                          03S0580 01342000
         BZ    @RF00580                                         03S0580 01343000
*    DO;                            /* COUNT SHR USERS IN APDS CHAIN */ 01344000
*      APDSPTR = APCTAPDS;          /* POINT TO 1ST APDS             */ 01345000
         LR    APDSPTR,@04                                      03S0582 01346000
*      DO WHILE(APDSPTR^=ZERO);     /* CONTINUE THROUGH ENTIRE TABLE */ 01347000
         B     @DE00583                                         03S0583 01348000
@DL00583 DS    0H                                               03S0584 01349000
*        IF APDSOWN=FALSE THEN      /* IF REQUEST IS FOR SHR PORT    */ 01350000
         TM    APDSOWN(APDSPTR),B'10000000'                     03S0584 01351000
         BNZ   @RF00584                                         03S0584 01352000
*          USRCOUNT = USRCOUNT + 1; /* CHALK UP ONE MORE USER        */ 01353000
         AL    USRCOUNT,@CF00296                                03S0585 01354000
*        ELSE;                                                  03S0586 01355000
@RF00584 DS    0H                                               03S0587 01356000
*        APDSPTR = APDSAPDS;        /* ADV TO NEXT APDS IF THERE IS 1*/ 01357000
         L     APDSPTR,APDSAPDS(,APDSPTR)                       03S0587 01358000
*      END;                                                     03S0588 01359000
@DE00583 SLR   @04,@04                                          03S0588 01360000
         CR    APDSPTR,@04                                      03S0588 01361000
         BNE   @DL00583                                         03S0588 01362000
*      IF USRCOUNT>ZERO THEN        /* IF AT LEAST 1 USER WAS FOUND  */ 01363000
         CR    USRCOUNT,@04                                     03S0589 01364000
         BNH   @RF00589                                         03S0589 01365000
*        DO;                        /* THEN GET ENOUGH SHRTABLE MEMRY*/ 01366000
*          RFY (R0F,R1P) RSTD;      /* SECURE PARAMETER REGISTERS    */ 01367000
*          R0F = USRCOUNT * 12;     /* 12 BYTES IN TABLE FOR EACH USR*/ 01368000
*                                                               03S0592 01369000
*                                                               03S0592 01370000
         LR    R0F,USRCOUNT                                     03S0592 01371000
         MH    R0F,@CH00105                                     03S0592 01372000
*            DO; /*IGTYGSTK LV(R0P)*/                           03S0593 01373000
*            RFY GPR00F RSTD; /* NEEDED REGISTER */             03S0594 01374000
*            GPR00F = R0P; /* SET REG 0 TO REQUIRED LENGTH */   03S0595 01375000
*            CALL APCVZUAD; /* OBTAIN A DYNAMIC AREA */         03S0596 01376000
         L     @15,APCTZUAD(,APCTPTR)                           03S0596 01377000
         BALR  @14,@15                                          03S0596 01378000
*            RFY (GPR00F) UNRSTD; /* FINISHED WITH REGISTERS */ 03S0597 01379000
*            END; /* REG_1 -> ACQUIRED DYNAMIC AREA */      /* GO GET T 01380000
*AT MEMORY            */                                        03S0598 01381000
*          SHRTABLP = R1P;          /* SECURE ADDR OF THE GOTTEN STG */ 01382000
         ST    R1P,SHRTABLP                                     03S0599 01383000
*          RFY (R0F,R1P) UNRSTD;    /* GIVE BACK THE PARAMETER REG'RS*/ 01384000
*        END;                                                   03S0601 01385000
*      ELSE;                                                    03S0602 01386000
@RF00589 DS    0H                                               03S0603 01387000
*    END;                                                       03S0603 01388000
*  ELSE;                            /* NO APDS'S AT ALL              */ 01389000
@RF00580 DS    0H                                               03S0605 01390000
*  TABLSIZE = ZERO;                 /* INITIALLY, NO ENTRIES IN TABLE*/ 01391000
         SLR   @04,@04                                          03S0605 01392000
         STH   @04,TABLSIZE                                     03S0605 01393000
*  IF APCTAPDS^=ZERO THEN           /* IF THERE IS AT LEAST 1 APDS   */ 01394000
         L     @15,APCTAPDS(,APCTPTR)                           03S0606 01395000
         CR    @15,@04                                          03S0606 01396000
         BE    @RF00606                                         03S0606 01397000
*    DO;                            /* BUILD A TABLE                 */ 01398000
*      APDSPTR = APCTAPDS;          /* POINT TO FIRST APDS           */ 01399000
         LR    APDSPTR,@15                                      03S0608 01400000
*      DO WHILE(APDSPTR^=ZERO);     /* SEARCH TO END OF APDS CHAIN   */ 01401000
