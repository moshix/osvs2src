         BNE   @RF00943                                         03S0943 02400000
*            IF LINESOUT=ZERO THEN  /* AND NO LINES PUT OUT AT ALL   */ 02401000
         CH    @12,LINESOUT                                     03S0944 02402000
         BNE   @RF00944                                         03S0944 02403000
*              DO;                                              03S0945 02404000
*                CALL NULLUNIT;     /* PREPARE MESSAGE OF NO PORTS   */ 02405000
         BAL   @14,NULLUNIT                                     03S0946 02406000
*              END;                 /* END OF NO LINES CASE          */ 02407000
*            ELSE                   /* >=10 LINES PREVIOUSLY WTO'ED  */ 02408000
