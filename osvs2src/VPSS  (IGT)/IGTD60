         AH    @02,PARMLEN                                      03S0216 00239000
         ALR   @02,APCQPTR                                      03S0216 00240000
         MVC   CIDCHAR(2),APCQCMND-1(@02)                       03S0216 00241000
*      DO INDEX=0 TO 1 BY 1 WHILE(CMDERROR=FALSE);/* INSPECT DIGITS  */ 00242000
         SLR   INDEX,INDEX                                      03S0217 00243000
@DL00217 TM    CMDERROR,B'10000000'                             03S0217 00244000
         BNZ   @DC00217                                         03S0217 00245000
*        IF APCQCMND(CMNDPSTR+PARMLEN+INDEX:CMNDPSTR+PARMLEN+INDEX)     00246000
*            <'0' | APCQCMND(CMNDPSTR+PARMLEN+INDEX:CMNDPSTR+PARMLEN    00247000
*            +INDEX)>'9' THEN       /* IF ID DIGITS NOT VALID DECIMAL*/ 00248000
