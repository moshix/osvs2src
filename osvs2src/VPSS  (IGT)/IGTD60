@RT00218 DS    0H                                               03S0219 00260000
*          DO;                                                  03S0219 00261000
*            CIDERROR = TRUE;       /* CONSOLE ID IS INVALID         */ 00262000
         OI    CIDERROR,B'10000000'                             03S0220 00263000
*            CMDERROR = TRUE;       /* THEREFORE COMMAND IS INVALID  */ 00264000
         OI    CMDERROR,B'10000000'                             03S0221 00265000
*          END;                                                 03S0222 00266000
*        ELSE;                                                  03S0223 00267000
@RF00218 DS    0H                                               03S0224 00268000
*      END;                                                     03S0224 00269000
         AL    INDEX,@CF00296                                   03S0224 00270000
         C     INDEX,@CF00296                                   03S0224 00271000
         BNH   @DL00217                                         03S0224 00272000
@DC00217 DS    0H                                               03S0225 00273000
*      IF CMDERROR=FALSE THEN       /* IF STILL IN BUSINESS          */ 00274000
         TM    CMDERROR,B'10000000'                             03S0225 00275000
         BNZ   @RF00225                                         03S0225 00276000
*        DO;                        /* CONVERT & STORE TARGET CID    */ 00277000
*          PACK(HALFWORD,CIDCHAR);  /* CONVERT TO PACKED DECIMAL     */ 00278000
         PACK  HALFWORD(2),CIDCHAR(2)                           03S0227 00279000
*          HALFWORD = HALFWORD | '000F'X; /* ENSURE POSITIVE SIGN    */ 00280000
         OC    HALFWORD(2),@CB02615                             03S0228 00281000
*          RFY R2F RSTD;            /* SECURE REG 2 FOR CONVERSION   */ 00282000
*          CVB(R2F,HALFWORD);       /* CONVERT PACKED DECIMAL TO BIN */ 00283000
         XC    @TS00001(8),@TS00001                             03S0230 00284000
         MVC   @TS00001+6(2),HALFWORD                           03S0230 00285000
         CVB   R2F,@TS00001                                     03S0230 00286000
*          XASCID = R2F;            /* SAVE CONVERTED CID FOR CALL   */ 00287000
         L     @01,XSAPTR                                       03S0231 00288000
