*                WPLLTF = '1000'X;  /* MERELY AN END LINE            */ 02424000
         MVC   WPLLTF(2,@12),@CB02650                           03S0953 02425000
*                WPLAREA = XASDID;  /* MOVE IN AREA ID               */ 02426000
