         STH   @02,PARMLEN                                      03S0285 00473000
*                  DLMCOUNT = 2;    /* - NOT A VALID NEXT DELIMITER  */ 00474000
         MVI   DLMCOUNT,X'02'                                   03S0286 00475000
*                  CALL DELMSCAN;   /* SCAN FOR NEXT VALID DELIMITER */ 00476000
         BAL   @14,DELMSCAN                                     03S0287 00477000
*                  IF CHRCOUNT^=3 THEN /* NOT A CORRECT RANGE ADDR   */ 00478000
         C     CHRCOUNT,@CF00047                                03S0288 00479000
