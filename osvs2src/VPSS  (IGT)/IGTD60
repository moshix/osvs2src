         BNH   @DL00168                                         02S0170 00110000
*  XSAPTR = ADDR(INITXSA);          /* LOCATEE XSA AT INITXSA        */ 00111000
         LA    @04,INITXSA                                      02S0171 00112000
         ST    @04,XSAPTR                                       02S0171 00113000
*  CVD(APCQSCID,HALFWORD);          /* CONVERT FROM BINARY TO PACKDEC*/ 00114000
         SLR   @02,@02                                          02S0172 00115000
         IC    @02,APCQSCID(,APCQPTR)                           02S0172 00116000
         CVD   @02,@TS00001                                     02S0172 00117000
         MVC   HALFWORD(2),@TS00001+6                           02S0172 00118000
*  HALFWORD = HALFWORD | '000F'X;   /* ENSURE PROPER SIGN IN ZONE    */ 00119000
         OC    HALFWORD(2),@CB02615                             02S0173 00120000
*  UNPK(CIDCHAR,HALFWORD);          /* UNPACK INTO PRINTABLE FROM    */ 00121000
         UNPK  CIDCHAR(2),HALFWORD(2)                           02S0174 00122000
*  XASCID = APCQSCID;               /* PROVIDE DEFAULT FOR CONSOLE ID*/ 00123000
         AL    @04,@CF00124                                     02S0175 00124000
         STC   @02,XASCID(,@04)                                 02S0175 00125000
*  XASDID = APCTDAID;               /* PROVIDE DEFAULT FOR AREA ID   */ 00126000
         MVC   XASDID(1,@04),APCTDAID(APCTPTR)                  02S0176 00127000
*        /* EXAMINE INPUT COMMAND FOR SYNTAX OR PARAMETER ERROR      */ 00128000
*  PLENLEFT = APCQPLEN;             /* COUNT DOWN ON LENGTH OF PARMS */ 00129000
         LH    @04,APCQPLEN(,APCQPTR)                           02S0177 00130000
         STH   @04,PLENLEFT                                     02S0177 00131000
*  CMNDPSTR = APCQPSTR;             /* SET INDEX TO CHECK NEXT PARM  */ 00132000
         LH    CMNDPSTR,APCQPSTR(,APCQPTR)                      02S0178 00133000
*  IF APCQCMND(CMNDPSTR:CMNDPSTR+5)='ACTIVE' THEN /* IF DISP AACTIVE */ 00134000
         LR    @04,APCQPTR                                      02S0179 00135000
         ALR   @04,CMNDPSTR                                     02S0179 00136000
         CLC   APCQCMND-1(6,@04),@CC02589                       02S0179 00137000
         BNE   @RF00179                                         02S0179 00138000
*    DO;                                                        02S0180 00139000
*      DISPACTV = TRUE;             /* SET INDICATOR                 */ 00140000
         OI    DISPACTV,B'10000000'                             02S0181 00141000
*      PARMLEN = 6;                 /* RECORD LENGTH                 */ 00142000
         MVC   PARMLEN(2),@CH00292                              02S0182 00143000
*    END;                                                       02S0183 00144000
*  ELSE                                                         02S0184 00145000
*    IF APCQCMND(CMNDPSTR:CMNDPSTR)='A' THEN /* IF DISPLAY A         */ 00146000
         B     @RC00179                                         02S0184 00147000
@RF00179 LR    @04,APCQPTR                                      02S0184 00148000
         ALR   @04,CMNDPSTR                                     02S0184 00149000
         CLI   APCQCMND-1(@04),C'A'                             02S0184 00150000
         BNE   @RF00184                                         02S0184 00151000
*      DO;                                                      02S0185 00152000
*        DISPACTV = TRUE;           /* SET INDICATOR                 */ 00153000
         OI    DISPACTV,B'10000000'                             02S0186 00154000
*        PARMLEN = 1;               /* RECORD LENGTH                 */ 00155000
         MVC   PARMLEN(2),@CH00296                              02S0187 00156000
*      END;                                                     02S0188 00157000
*    ELSE                                                       02S0189 00158000
*      IF APCQCMND(CMNDPSTR:CMNDPSTR+4)='UNITS' THEN /* IF D UNITS   */ 00159000
         B     @RC00184                                         02S0189 00160000
@RF00184 LR    @04,APCQPTR                                      02S0189 00161000
