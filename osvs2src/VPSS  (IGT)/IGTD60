         C     CHRCOUNT,@CF00047                                03S0249 00348000
         BNH   @RF00249                                         03S0249 00349000
@RT00249 DS    0H                                               03S0250 00350000
*    DO;                                                        03S0250 00351000
*      CMDERROR = TRUE;             /* INDICATE INVALID COMMAND      */ 00352000
         OI    CMDERROR,B'10000000'                             03S0251 00353000
*    END;                                                       03S0252 00354000
*  ELSE                                                         03S0253 00355000
*    PARMLEN = PARMLEN + CHRCOUNT;  /* INCREMENT FOR INPUT ID(S)     */ 00356000
         B     @RC00249                                         03S0253 00357000
@RF00249 LR    @02,CHRCOUNT                                     03S0253 00358000
         AH    @02,PARMLEN                                      03S0253 00359000
         STH   @02,PARMLEN                                      03S0253 00360000
*          END;                                                 02S0254 00361000
*        ELSE                                                   02S0255 00362000
*          IF APCQCMND(CMNDPSTR:CMNDPSTR+7)=',DEVICE=' & PLENLEFT>10    00363000
*              THEN                 /* 3 DIGIT ADDRESS REQUIRED TOO  */ 00364000
         B     @RC00208                                         02S0255 00365000
@RF00208 LR    @04,APCQPTR                                      02S0255 00366000
