*      IF (XASDID>='A' & XASDID<='I' |  /* IF AREA ID IS A VALID     */ 00315000
*          XASDID>='J' & XASDID<='R' |  /* ALPHA CHAR., THEN FINE    */ 00316000
*          XASDID>='S' & XASDID<='Z' ) THEN; /* INPUT AREA ID IS O.K.*/ 00317000
