TDAP     TITLE 'IGTDAP - UNIT PARAMETER ANALYZER.                      *00001000
                        '                                               00002000
IGTDAP   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTDAP    77.092'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@09,12(@13)                                  01S0001 00010000
         STM   @11,@12,64(@13)                                  01S0001 00011000
         BALR  @07,0                                            01S0001 00012000
@PSTART  DS    0H                                               01S0001 00013000
         USING @PSTART,@07                                      01S0001 00014000
         L     @00,@SIZDATD                                     01S0001 00015000
         IGTYGSV  R,LV=(0)                                              00016000
         LR    @06,@01                                          01S0001 00017000
         USING @DATD,@06                                        01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         LM    @00,@01,20(@13)                                  01S0001 00020000
         ST    @06,8(,@13)                                      01S0001 00021000
         LR    @13,@06                                          01S0001 00022000
         MVC   @PC00001(12),0(@01)                              01S0001 00023000
*   APUBPTR=APCTAPUB;               /* ADDRESS OF FIRST MASTER.      */ 00024000
         L     APUBPTR,APCTAPUB(,APCTPTR)                       01S0039 00025000
*   IF UNITSLEN>=3 THEN             /* SEE IF UNIT NUMBER CAN BE        00026000
*                                      VALID                         */ 00027000
         L     @11,@PC00001+4                                   01S0040 00028000
         LH    @11,UNITSLEN(,@11)                               01S0040 00029000
         C     @11,@CF00149                                     01S0040 00030000
         BL    @RF00040                                         01S0040 00031000
*                                                               02S0041 00032000
*     /***************************************************************/ 00033000
*     /*                                                             */ 00034000
*     /* VALIDATE THE UNIT AND FIND THE APUB PTR.                    */ 00035000
*     /*                                                             */ 00036000
*     /***************************************************************/ 00037000
*                                                               02S0041 00038000
*     DO;                                                       02S0041 00039000
*       RETCODE=0;                  /* INIT RETURN CODE.             */ 00040000
         SLR   RETCODE,RETCODE                                  03S0042 00041000
*       IF UNITSLEN>=7 THEN         /* IF RANGE MIGHT EXIST,         */ 00042000
         C     @11,@CF00222                                     03S0043 00043000
         BL    @RF00043                                         03S0043 00044000
*         DO;                       /* VALIDATE NEXT GROUP, WHICH IS    00045000
*                                      EITHER CUX-CUY WITH Y>=X OR      00046000
*                                      CUX.                          */ 00047000
*           IF UNITS(4)='-' THEN    /* TEST FOR RANGE FIRST          */ 00048000
         L     @11,@PC00001                                     03S0045 00049000
         L     @11,UNITSA(,@11)                                 03S0045 00050000
         CLI   UNITS+3(@11),C'-'                                03S0045 00051000
         BNE   @RF00045                                         03S0045 00052000
*             DO;                                               03S0046 00053000
*               IF UNITS(1:2)^=UNITS(5:6)/* THEY MUST BE ON SAME 3838*/ 00054000
*                   |UNITS(7)<UNITS(3) THEN/* AND SECOND > FIRST.    */ 00055000
         CLC   UNITS(2,@11),UNITS+4(@11)                        03S0047 00056000
         BNE   @RT00047                                         03S0047 00057000
         CLC   UNITS+6(1,@11),UNITS+2(@11)                      03S0047 00058000
         BNL   @RF00047                                         03S0047 00059000
@RT00047 DS    0H                                               03S0048 00060000
*                 RETCODE=8;        /* FAIL THEM                     */ 00061000
         LA    RETCODE,8                                        03S0048 00062000
*               ELSE                                            03S0049 00063000
*                 IF UNITSLEN>7 THEN/* IF MORE TO COME               */ 00064000
         B     @RC00047                                         03S0049 00065000
@RF00047 L     @11,@PC00001+4                                   03S0049 00066000
         LH    @11,UNITSLEN(,@11)                               03S0049 00067000
         C     @11,@CF00222                                     03S0049 00068000
         BNH   @RF00049                                         03S0049 00069000
*                   IF UNITS(8)^=',' THEN/* MUST BE PROPERLY    03S0050 00070000
*                                      DELIMITED.                    */ 00071000
         L     @11,@PC00001                                     03S0050 00072000
         L     @11,UNITSA(,@11)                                 03S0050 00073000
         CLI   UNITS+7(@11),C','                                03S0050 00074000
         BE    @RF00050                                         03S0050 00075000
*                     RETCODE=8;    /* ELSE ERROR.                   */ 00076000
         LA    RETCODE,8                                        03S0051 00077000
*             END;                                              03S0052 00078000
*           ELSE                                                03S0053 00079000
*             IF UNITS(4)^=',' THEN                             03S0053 00080000
         B     @RC00045                                         03S0053 00081000
@RF00045 L     @11,@PC00001                                     03S0053 00082000
         L     @11,UNITSA(,@11)                                 03S0053 00083000
         CLI   UNITS+3(@11),C','                                03S0053 00084000
         BE    @RF00053                                         03S0053 00085000
*               RETCODE=8;                                      03S0054 00086000
         LA    RETCODE,8                                        03S0054 00087000
*         END;                                                  03S0055 00088000
*       ELSE                                                    03S0056 00089000
*         IF UNITSLEN>3 THEN        /* IF FIRST UNIT OK              */ 00090000
         B     @RC00043                                         03S0056 00091000
@RF00043 L     @11,@PC00001+4                                   03S0056 00092000
         LH    @11,UNITSLEN(,@11)                               03S0056 00093000
         C     @11,@CF00149                                     03S0056 00094000
         BNH   @RF00056                                         03S0056 00095000
*           IF UNITS(4)^=',' THEN   /* VALIDATE DELIMITER.           */ 00096000
         L     @11,@PC00001                                     03S0057 00097000
         L     @11,UNITSA(,@11)                                 03S0057 00098000
         CLI   UNITS+3(@11),C','                                03S0057 00099000
         BE    @RF00057                                         03S0057 00100000
*             RETCODE=8;            /* IF BAD, SET ERROR.            */ 00101000
         LA    RETCODE,8                                        03S0058 00102000
*       IF RETCODE=0 THEN           /* IF RANGE OK, THEN FIND APUB.  */ 00103000
@RF00057 DS    0H                                               02S0059 00104000
@RF00056 DS    0H                                               02S0059 00105000
@RC00043 LTR   RETCODE,RETCODE                                  02S0059 00106000
         BNZ   @RF00059                                         02S0059 00107000
*         DO;                                                   02S0060 00108000
*           DO;                     /* FIND THE APUB.                */ 00109000
*             DO WHILE APUBPTR^=0   /* TRY TO FIND THE RIGHT MASTER. */ 00110000
*                   &UCBNAME(1:2)^=WORKAREA(2:3);/* BUT QUIT WHEN NO    00111000
*                                      MORE.                         */ 00112000
         B     @DE00062                                         03S0062 00113000
@DL00062 DS    0H                                               03S0063 00114000
*               APUBPTR=APUBMSTR;   /* BUMP TO NEXT MASTER IF NO        00115000
*                                      MATCH.                        */ 00116000
         L     APUBPTR,APUBMSTR(,APUBPTR)                       03S0063 00117000
*             END;                                              03S0064 00118000
@DE00062 LTR   APUBPTR,APUBPTR                                  03S0064 00119000
         BZ    @DC00062                                         03S0064 00120000
         L     @11,UCBPTR(,APUBPTR)                             03S0064 00121000
         L     @03,@PC00001+8                                   03S0064 00122000
         CLC   UCBNAME(2,@11),WORKAREA+1(@03)                   03S0064 00123000
         BNE   @DL00062                                         03S0064 00124000
@DC00062 DS    0H                                               03S0065 00125000
*             IF APUBPTR^=0 THEN    /* IF MASTER FOUND, NOW FIND        00126000
*                                      USER.                         */ 00127000
         LTR   APUBPTR,APUBPTR                                  03S0065 00128000
         BZ    @RF00065                                         03S0065 00129000
*               DO WHILE APUBPTR^=0 /* BUT QUIT IF NO MATCH.         */ 00130000
*                     &UCBNAME(3)^= /* LOOK MATCH IN UNIT POSITION   */ 00131000
*                     WORKAREA(4);                              03S0066 00132000
         B     @DE00066                                         03S0066 00133000
@DL00066 DS    0H                                               03S0067 00134000
*                 APUBPTR=APUBAPUB; /* BUMP TO NEXT USER IF NO MATCH.*/ 00135000
         L     APUBPTR,APUBAPUB(,APUBPTR)                       03S0067 00136000
*               END;                                            03S0068 00137000
@DE00066 LTR   APUBPTR,APUBPTR                                  03S0068 00138000
         BZ    @DC00066                                         03S0068 00139000
         L     @11,UCBPTR(,APUBPTR)                             03S0068 00140000
         L     @03,@PC00001+8                                   03S0068 00141000
         CLC   UCBNAME+2(1,@11),WORKAREA+3(@03)                 03S0068 00142000
         BNE   @DL00066                                         03S0068 00143000
@DC00066 DS    0H                                               03S0069 00144000
*           END;                                                03S0069 00145000
@RF00065 DS    0H                                               03S0070 00146000
*           IF APUBPTR=0 THEN       /* IF COULDN'T FIND IT,          */ 00147000
         LTR   APUBPTR,APUBPTR                                  02S0070 00148000
         BNZ   @RF00070                                         02S0070 00149000
*             RETCODE=8;            /* THEN CALLER HAS AN ERROR.     */ 00150000
         LA    RETCODE,8                                        02S0071 00151000
*           ELSE                    /* OTHERWISE SETUP FOR NEXT CALL */ 00152000
*             DO;                                               02S0072 00153000
         B     @RC00070                                         02S0072 00154000
@RF00070 DS    0H                                               02S0073 00155000
*               DO;                 /* SET WORKAREA FOR NEXT ENTRY.  */ 00156000
*                 IF UNITSLEN>=7 THEN/* IF ADDITIONAL UNITS     03S0074 00157000
*                                      SPECIFIED,                    */ 00158000
         LA    @11,7                                            03S0074 00159000
         L     @03,@PC00001+4                                   03S0074 00160000
         LH    @15,UNITSLEN(,@03)                               03S0074 00161000
         CR    @15,@11                                          03S0074 00162000
         BL    @RF00074                                         03S0074 00163000
*                   IF UNITS(4)='-' THEN/* SEE IF RAGE IS GIVEN.     */ 00164000
         L     @14,@PC00001                                     03S0075 00165000
         L     @09,UNITSA(,@14)                                 03S0075 00166000
         CLI   UNITS+3(@09),C'-'                                03S0075 00167000
         BNE   @RF00075                                         03S0075 00168000
*                     DO;           /* IF SO, THEN.                  */ 00169000
*                       PORT=PORT+1;/* STEP TO NEXT PORT NUMBER.     */ 00170000
         L     @08,@PC00001+8                                   03S0077 00171000
         LA    @05,1                                            03S0077 00172000
         AL    @05,PORT(,@08)                                   03S0077 00173000
         ST    @05,PORT(,@08)                                   03S0077 00174000
*                       ENDUNIT(1)=''X;/* THEN SEE IF THIS RANGE HAS    00175000
*                                      BEEN                          */ 00176000
         MVI   ENDUNIT,X'00'                                    03S0078 00177000
*                       ENDUNIT(2:4)=UNITS(5:7);/* EXHAUSTED.        */ 00178000
         MVC   ENDUNIT+1(3),UNITS+4(@09)                        03S0079 00179000
*                       IF PORT>ENDUNIT THEN/* IF TRUE THE END OF       00180000
*                                      RANGE                         */ 00181000
         CL    @05,ENDUNIT                                      03S0080 00182000
         BNH   @RF00080                                         03S0080 00183000
*                         IF UNITSLEN>7 THEN/* SO SEE IF MORE UNITS. */ 00184000
         CR    @15,@11                                          03S0081 00185000
         BNH   @RF00081                                         03S0081 00186000
*                           DO;     /* SET UP FOR NEXT CALL.         */ 00187000
*                             UNITSLEN=UNITSLEN-8;/* NEW GROUP OF       00188000
*                                      PORTS,                        */ 00189000
         LA    @11,8                                            03S0083 00190000
         SLR   @15,@11                                          03S0083 00191000
         STH   @15,UNITSLEN(,@03)                               03S0083 00192000
*                             UNITSA=UNITSA+8;/* SO CHAGE ADDR AND      00193000
*                                      LEN.                          */ 00194000
         ALR   @09,@11                                          03S0084 00195000
         ST    @09,UNITSA(,@14)                                 03S0084 00196000
*                             IF UNITSLEN>=3 THEN/* MOVE IN NEW PORT */ 00197000
         C     @15,@CF00149                                     03S0085 00198000
         BL    @RF00085                                         03S0085 00199000
*                               WORKAREA(2:4)=UNITS(1:3);/* IF  03S0086 00200000
*                                      PRESENT                       */ 00201000
         MVC   WORKAREA+1(3,@08),UNITS(@09)                     03S0086 00202000
*                             RETCODE=0;/* TELL USER TO COME BACK       00203000
*                                      AGAIN                         */ 00204000
@RF00085 SLR   RETCODE,RETCODE                                  03S0087 00205000
*                           END;                                03S0088 00206000
*                         ELSE                                  03S0089 00207000
*                           RETCODE=4;/* NO MORE UNITS SO DON'T 03S0089 00208000
*                                      RETURN.                       */ 00209000
         B     @RC00081                                         03S0089 00210000
@RF00081 LA    RETCODE,4                                        03S0089 00211000
*                     END;                                      03S0090 00212000
*                   ELSE                                        03S0091 00213000
*                     DO;                                       03S0091 00214000
         B     @RC00075                                         03S0091 00215000
@RF00075 DS    0H                                               03S0092 00216000
*                       WORKAREA(2:4)=UNITS(5:7);/* NEXT UNIT TO WORK   00217000
*                                      ON.                           */ 00218000
         L     @11,@PC00001+8                                   03S0092 00219000
         L     @03,@PC00001                                     03S0092 00220000
         L     @15,UNITSA(,@03)                                 03S0092 00221000
         MVC   WORKAREA+1(3,@11),UNITS+4(@15)                   03S0092 00222000
*                       UNITSA=UNITSA+4;/* ADJUST ADDRESS            */ 00223000
         LA    @11,4                                            03S0093 00224000
         ALR   @15,@11                                          03S0093 00225000
         ST    @15,UNITSA(,@03)                                 03S0093 00226000
*                       UNITSLEN=UNITSLEN-4;/* AND LENGTH.           */ 00227000
         L     @03,@PC00001+4                                   03S0094 00228000
         LCR   @11,@11                                          03S0094 00229000
         AH    @11,UNITSLEN(,@03)                               03S0094 00230000
         STH   @11,UNITSLEN(,@03)                               03S0094 00231000
*                     END;                                      03S0095 00232000
*                 ELSE                                          03S0096 00233000
*                   IF UNITSLEN=3 THEN/* SINCE NO MORE UNITS         */ 00234000
         B     @RC00074                                         03S0096 00235000
@RF00074 L     @11,@PC00001+4                                   03S0096 00236000
         CLC   UNITSLEN(2,@11),@CH00149                         03S0096 00237000
         BNE   @RF00096                                         03S0096 00238000
*                     RETCODE=4;    /* TELL CALLER - NOMORE.         */ 00239000
         LA    RETCODE,4                                        03S0097 00240000
*                   ELSE                                        03S0098 00241000
*                     DO;                                       03S0098 00242000
         B     @RC00096                                         03S0098 00243000
@RF00096 DS    0H                                               03S0099 00244000
*                       UNITSA=UNITSA+4;/* ELSE SET TO CATCH ERROR      00245000
*                                      NEXT                          */ 00246000
         L     @11,@PC00001                                     03S0099 00247000
         LA    @03,4                                            03S0099 00248000
         L     @15,UNITSA(,@11)                                 03S0099 00249000
         ALR   @15,@03                                          03S0099 00250000
         ST    @15,UNITSA(,@11)                                 03S0099 00251000
*                       UNITSLEN=UNITSLEN-4;/* TIME.                 */ 00252000
         L     @11,@PC00001+4                                   03S0100 00253000
         LCR   @03,@03                                          03S0100 00254000
         AH    @03,UNITSLEN(,@11)                               03S0100 00255000
         STH   @03,UNITSLEN(,@11)                               03S0100 00256000
*                     END;                                      03S0101 00257000
*               END;                                            03S0102 00258000
@RC00096 DS    0H                                               03S0102 00259000
@RC00074 DS    0H                                               03S0103 00260000
*             END;                                              02S0103 00261000
*         END;                      /* END OF GOOD RANGE.            */ 00262000
*     END;                          /* RETCODE MAY BE SET BY    02S0105 00263000
*                                      VALRANGE.                     */ 00264000
*   ELSE                                                        01S0106 00265000
*     DO;                                                       01S0106 00266000
         B     @RC00040                                         01S0106 00267000
@RF00040 DS    0H                                               01S0107 00268000
*       RETCODE=8;                  /* INDICATE INVALID UNIT NUMBER. */ 00269000
         LA    RETCODE,8                                        01S0107 00270000
*       IF UNITSLEN<1 THEN          /* IF NO PARM EXISTS             */ 00271000
         L     @11,@PC00001+4                                   01S0108 00272000
         LH    @11,UNITSLEN(,@11)                               01S0108 00273000
         LTR   @11,@11                                          01S0108 00274000
         BP    @RF00108                                         01S0108 00275000
*         WORKAREA(2:4)='   ';      /* BLANK WORKAREA.               */ 00276000
         L     @11,@PC00001+8                                   01S0109 00277000
         MVC   WORKAREA+1(3,@11),@CC01003                       01S0109 00278000
*       ELSE                                                    01S0110 00279000
*         DO;                                                   01S0110 00280000
         B     @RC00108                                         01S0110 00281000
@RF00108 DS    0H                                               01S0111 00282000
*           WORKAREA(2:4)='   ';    /* BLANK THE WORKAREA.           */ 00283000
         L     @11,@PC00001+8                                   01S0111 00284000
         MVC   WORKAREA+1(3,@11),@CC01003                       01S0111 00285000
*           WORKAREA(2:UNITSLEN+1)  /* INSERT THE PARAMETER.         */ 00286000
*               =UNITS(1:UNITSLEN);                             01S0112 00287000
         L     @03,@PC00001+4                                   01S0112 00288000
         LH    @03,UNITSLEN(,@03)                               01S0112 00289000
         BCTR  @03,0                                            01S0112 00290000
         L     @15,@PC00001                                     01S0112 00291000
         L     @01,UNITSA(,@15)                                 01S0112 00292000
         EX    @03,@SM01088                                     01S0112 00293000
*         END;                                                  01S0113 00294000
*     END;                                                      01S0114 00295000
@RC00108 DS    0H                                               01S0115 00296000
*   RETURN CODE(RETCODE);           /* GIVE CALLER THE RESULT.  01S0115 00297000
*                                      IGTXEPLG                      */ 00298000
@RC00040 L     @13,4(,@13)                                      01S0115 00299000
         L     @00,@SIZDATD                                     01S0115 00300000
         LR    @01,@06                                          01S0115 00301000
         IGTYFSV  R,LV=(0),A=(1)                                        00302000
         LR    @15,@02                                          01S0115 00303000
         L     @14,12(,@13)                                     01S0115 00304000
         LM    @00,@09,20(@13)                                  01S0115 00305000
         LM    @11,@12,64(@13)                                  01S0115 00306000
         BR    @14                                              01S0115 00307000
