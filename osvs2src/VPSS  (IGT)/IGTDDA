TDDA     TITLE 'IGTDDA - COMMON DISABLE PROCESSING.                    *00001000
                        '                                               00002000
IGTDDA   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTDDA    77.092'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
*   RETCODE=0;                      /* INITIALIZE RETURN CODE/  01S0105 00022000
*                                      DISABLE POSSIBLE INDICATOR. 0=   00023000
*                                      NOT YET.                      */ 00024000
         SLR   RETCODE,RETCODE                                  01S0105 00025000
*   RFY                                                         01S0106 00026000
*     APUB BASED(APUBPTR);          /* BASE ADDRESS OF THE APUB.     */ 00027000
*   IF APUBOWN='0'B THEN            /* IF IT IS STILL EXCLUSIVELY       00028000
*                                      OWNED, THEN THERE IS NO POINT    00029000
*                                      IN RUNNING ANY CHECKS SINCE IT   00030000
*                                      CAN NOT BE DISABLED.          */ 00031000
         TM    APUBOWN(APUBPTR),B'01000000'                     01S0107 00032000
         BNZ   @RF00107                                         01S0107 00033000
*     DO;                           /* IT IS SHARED SO TRY TO   01S0108 00034000
*                                      DISABLE.                      */ 00035000
*                                                               01S0108 00036000
*       /*************************************************************/ 00037000
*       /*                                                           */ 00038000
*       /* SEE IF ANOTHER PORT IS BETTER THAN THIS ONE.              */ 00039000
*       /*                                                           */ 00040000
*       /*************************************************************/ 00041000
*                                                               01S0109 00042000
*                                                               02S0109 00043000
*       /*************************************************************/ 00044000
*       /*                                                           */ 00045000
*       /* THIS CODE LOOKS FOR A REGION AT LEAST AS BIG AS THE ONE   */ 00046000
*       /* BEING DISABLED.                                           */ 00047000
*       /*                                                           */ 00048000
*       /*************************************************************/ 00049000
*                                                               02S0109 00050000
*       MSTRPTR=APCTAPUB;           /* FIRST MASTER APUB.            */ 00051000
         L     MSTRPTR,APCTAPUB(,APCTPTR)                       02S0109 00052000
*       NBIGPTR=0;                  /* INIT PTR TO LARGEST APUB.     */ 00053000
         SLR   NBIGPTR,NBIGPTR                                  02S0110 00054000
*       DO WHILE MSTRPTR^=0&        /* PROCESS ALL MASTER APUBS,     */ 00055000
*             RETCODE=0;            /* OR UNTIL SEARCH IS SATISFIED. */ 00056000
         B     @DE00111                                         02S0111 00057000
@DL00111 DS    0H                                               02S0112 00058000
*         USERPTR=MSTRPTR->APUBAPUB;/* FIRST USER PORT ON THIS  02S0112 00059000
*                                      MASTER.                       */ 00060000
         L     USERPTR,APUBAPUB(,MSTRPTR)                       02S0112 00061000
*         DO WHILE USERPTR^=0       /* DO ALL USER PORTS,            */ 00062000
*               &RETCODE=0;         /* OR UNTIL SEARCH IS SATISFIED. */ 00063000
         B     @DE00113                                         02S0113 00064000
@DL00113 DS    0H                                               02S0114 00065000
*           RESPECIFY                                           02S0114 00066000
*             APUB BASED(USERPTR);  /* ADDRESS OF APUB BEING  SEARCHD*/ 00067000
*           IF USERPTR^=APUBPTR     /* SKIP THE ONE BEING DISABLED.  */ 00068000
*               &APUBIDAB='0'B      /* SKIP IT IF ITS DISABLED.      */ 00069000
*               &APUBOWN='0'B THEN  /* SKIP IT IF ITS EXCLUSIVELY       00070000
*                                      OWNED                         */ 00071000
         CR    USERPTR,APUBPTR                                  02S0115 00072000
         BE    @RF00115                                         02S0115 00073000
         TM    APUBIDAB(USERPTR),B'01000000'                    02S0115 00074000
         BNZ   @RF00115                                         02S0115 00075000
         TM    APUBOWN(USERPTR),B'01000000'                     02S0115 00076000
         BNZ   @RF00115                                         02S0115 00077000
*             IF APUBRGN>=          /* IF THE REGION IS BIG ENOUGH,  */ 00078000
*                 APUBPTR->APUBRGN THEN                         02S0116 00079000
         LH    @11,APUBRGN(,USERPTR)                            02S0116 00080000
         CH    @11,APUBRGN(,APUBPTR)                            02S0116 00081000
         BL    @RF00116                                         02S0116 00082000
*               RETCODE=4;          /* THEN THE SEARCH CAN END NOW.  */ 00083000
         LA    RETCODE,4                                        02S0117 00084000
*             ELSE                                              02S0118 00085000
*               DO;                                             02S0118 00086000
         B     @RC00116                                         02S0118 00087000
@RF00116 DS    0H                                               02S0119 00088000
*                                                               03S0119 00089000
*                 /***************************************************/ 00090000
*                 /*                                                 */ 00091000
*                 /* THIS CODE LOOKS FOR THE LARGEST APUB REGION,    */ 00092000
*                 /* OTHER THAN THE ONE BEING DISABLED. THE SEARCH   */ 00093000
*                 /* WILL NOT BE COMPLETED IF OTHER CRITERIA ARE MET */ 00094000
*                 /* FIRST, BECAUSE THE OUTPUT OF THIS SEARCH IS USED*/ 00095000
*                 /* IN CHECK2.                                      */ 00096000
*                 /*                                                 */ 00097000
*                 /***************************************************/ 00098000
*                                                               03S0119 00099000
*                 IF NBIGPTR=0 THEN /* IF THIS IS FIRST VALID APUB   */ 00100000
         LTR   NBIGPTR,NBIGPTR                                  03S0119 00101000
         BNZ   @RF00119                                         03S0119 00102000
*                   NBIGPTR=USERPTR;/* THEN IT IS THE BIGGEST.       */ 00103000
         LR    NBIGPTR,USERPTR                                  03S0120 00104000
*                 ELSE                                          03S0121 00105000
*                   IF APUBRGN>     /* IF THE NEW ONE IS BIGGER THAN */ 00106000
*                       NBIGPTR->APUBRGN THEN/* THE OLD ONE,         */ 00107000
         B     @RC00119                                         03S0121 00108000
@RF00119 LH    @11,APUBRGN(,USERPTR)                            03S0121 00109000
         CH    @11,APUBRGN(,NBIGPTR)                            03S0121 00110000
         BNH   @RF00121                                         03S0121 00111000
*                     NBIGPTR=USERPTR;/* GET NEW ADDRESS.            */ 00112000
         LR    NBIGPTR,USERPTR                                  03S0122 00113000
*               END;                                            02S0123 00114000
@RF00121 DS    0H                                               02S0123 00115000
@RC00119 DS    0H                                               02S0124 00116000
*           USERPTR=APUBAPUB;       /* GO TO NEXT APUB.              */ 00117000
@RC00116 DS    0H                                               02S0124 00118000
@RF00115 L     USERPTR,APUBAPUB(,USERPTR)                       02S0124 00119000
*           RESPECIFY                                           02S0125 00120000
*             APUB BASED(APUBPTR);  /* RETURN TO OLD BASE.           */ 00121000
*         END;                                                  02S0126 00122000
@DE00113 SLR   @11,@11                                          02S0126 00123000
         CR    USERPTR,@11                                      02S0126 00124000
         BE    @DC00113                                         02S0126 00125000
         CR    RETCODE,@11                                      02S0126 00126000
         BE    @DL00113                                         02S0126 00127000
@DC00113 DS    0H                                               02S0127 00128000
*         MSTRPTR=MSTRPTR->APUBMSTR;/* GO TO NEXT MASTER.            */ 00129000
         L     MSTRPTR,APUBMSTR(,MSTRPTR)                       02S0127 00130000
*       END;                        /* END OF FIRST CHECK.           */ 00131000
@DE00111 SLR   @11,@11                                          02S0128 00132000
         CR    MSTRPTR,@11                                      02S0128 00133000
         BE    @DC00111                                         02S0128 00134000
         CR    RETCODE,@11                                      02S0128 00135000
         BE    @DL00111                                         02S0128 00136000
@DC00111 DS    0H                                               02S0129 00137000
*       IF RETCODE=0 THEN           /* IF ABOVE TEST FAILED TO  01S0129 00138000
*                                      DISABLE THE PORT, SEE IF ANY     00139000
*                                      BODY REALLY NEEDS THIS ONE.   */ 00140000
         LTR   RETCODE,RETCODE                                  01S0129 00141000
         BNZ   @RF00129                                         01S0129 00142000
*         DO;                                                   01S0130 00143000
*                                                               01S0130 00144000
*           /*********************************************************/ 00145000
*           /*                                                       */ 00146000
*           /* FIND OUT IF THIS PORT NEEDED.                         */ 00147000
*           /*                                                       */ 00148000
*           /*********************************************************/ 00149000
*                                                               01S0131 00150000
*                                                               02S0131 00151000
*           /*********************************************************/ 00152000
*           /*                                                       */ 00153000
*           /* THIS CODE FINDS THE LARGEST REQUIRED REGION.          */ 00154000
*           /*                                                       */ 00155000
*           /*********************************************************/ 00156000
*                                                               02S0131 00157000
*           RESPECIFY                                           02S0131 00158000
*             APUB BASED(NBIGPTR);  /* POINTER TO NEXT LARGEST  02S0131 00159000
*                                      AVAILABLE REGION.             */ 00160000
*           RESPECIFY                                           02S0132 00161000
*             APDS BASED(DSPTR);                                02S0132 00162000
*           DSPTR=APCTAPDS;         /* ADDRESS OF FIRST APDS.        */ 00163000
         L     DSPTR,APCTAPDS(,APCTPTR)                         02S0133 00164000
*           RETCODE=4;              /* UNLESS RESET IN LOOP, THE APUB   00165000
*                                      CAN BE DISABLED.              */ 00166000
         LA    RETCODE,4                                        02S0134 00167000
*           DO WHILE DSPTR^=0&      /* SCAN ALL APDS'S,              */ 00168000
*                 RETCODE=4;        /* AS LONG AS NONE EXCEEDS THE      00169000
*                                      NEXT LARGEST APUB.            */ 00170000
         B     @DE00135                                         02S0135 00171000
@DL00135 DS    0H                                               02S0136 00172000
*             IF APDSARS='1'B THEN  /* IF THE APDS HAS COMLETETD        00173000
*                                      ALLOC.                        */ 00174000
         TM    APDSARS(DSPTR),B'00001000'                       02S0136 00175000
         BNO   @RF00136                                         02S0136 00176000
*               IF APDSOWN='0'B THEN/* IF THE APDS IS SHARED USER,      00177000
*                                      AND                           */ 00178000
         TM    APDSOWN(DSPTR),B'10000000'                       02S0137 00179000
         BNZ   @RF00137                                         02S0137 00180000
*                 IF NBIGPTR=0 THEN /* IF NO OTHER SHARED PORT, MUST */ 00181000
         LTR   NBIGPTR,NBIGPTR                                  02S0138 00182000
         BNZ   @RF00138                                         02S0138 00183000
*                   RETCODE=0;      /* * QUIT SINCE HAVE SHARED USER.*/ 00184000
         SLR   RETCODE,RETCODE                                  02S0139 00185000
*                 ELSE              /* IF ANOTHER SHARED PORT, SET IF*/ 00186000
*                   IF APDSREG>APUBRGN THEN/* THIS USER WILL FIT IN     00187000
*                                      IT.                           */ 00188000
