@RF00138 L     @11,APDSREG(,DSPTR)                              02S0140 00190000
         CH    @11,APUBRGN(,NBIGPTR)                            02S0140 00191000
         BNH   @RF00140                                         02S0140 00192000
*                     RETCODE=0;    /* IF NOT, MUST DISABLE LATER.   */ 00193000
         SLR   RETCODE,RETCODE                                  02S0141 00194000
*             DSPTR=APDSAPDS;       /* BUMP TO NEXT APDS.            */ 00195000
@RF00140 DS    0H                                               02S0142 00196000
@RC00138 DS    0H                                               02S0142 00197000
@RF00137 DS    0H                                               02S0142 00198000
@RF00136 L     DSPTR,APDSAPDS(,DSPTR)                           02S0142 00199000
*           END;                                                02S0143 00200000
@DE00135 LTR   DSPTR,DSPTR                                      02S0143 00201000
         BZ    @DC00135                                         02S0143 00202000
         C     RETCODE,@CF00048                                 02S0143 00203000
         BE    @DL00135                                         02S0143 00204000
@DC00135 DS    0H                                               02S0144 00205000
*           RESPECIFY                                           02S0144 00206000
*             APDS;                 /* REMOVE ADDRESSIBILITY         */ 00207000
*           RESPECIFY                                           02S0145 00208000
*             APUB BASED(APUBPTR);  /* RESTORE ADDRESSIBILITY.       */ 00209000
*         END;                                                  01S0146 00210000
*       IF RETCODE^=0 THEN          /* CAN WE DISABLE IT?            */ 00211000
@RF00129 LTR   RETCODE,RETCODE                                  01S0147 00212000
         BZ    @RF00147                                         01S0147 00213000
*         DO;                       /* IF NOT 0, CAN DO.             */ 00214000
*                                                               01S0148 00215000
*           /*********************************************************/ 00216000
*           /*                                                       */ 00217000
*           /* GENERATE A CHASER TO INSURE THAT ALL I/O IS COMPLETE. */ 00218000
*           /*                                                       */ 00219000
*           /*********************************************************/ 00220000
*                                                               01S0149 00221000
*                                                               02S0149 00222000
*           /*********************************************************/ 00223000
*           /*                                                       */ 00224000
*           /* THIS SECTION OF CODE INSURES THAT ALL I/O FOR THE     */ 00225000
*           /* SPECIFIED APUB HAS COMPLETED. IT ALSO DISABLES THE    */ 00226000
*           /* PORT.                                                 */ 00227000
*           /*                                                       */ 00228000
*           /*********************************************************/ 00229000
*                                                               02S0149 00230000
*           APUBIDAB='1'B;          /* MARK IT DISABLED FOR I/O.     */ 00231000
