*                     DO;                                       05S0212 00270000
*                       TOTALSZ=TOTALSZ-REGSIZE(I);/* REDUCE OVERRUN */ 00271000
         SLR   TOTALSZ,@14                                      05S0213 00272000
*                       REGSIZE(I)=0;/* BY THE AMOUNT POSSIBLE.      */ 00273000
         AL    @09,@CF01878                                     05S0214 00274000
         ST    @11,REGSIZE(@15,@09)                             05S0214 00275000
*                     END;                                      05S0215 00276000
