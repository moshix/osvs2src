TDIO     TITLE 'IGTDIO - PERFORM IPL I/O.                              *00001000
                        '                                               00002000
IGTDIO   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTDIO    77.174'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
         MVC   @PC00001(4),0(@01)                               01S0001 00022000
*   IF APUBDCB^=0 THEN              /* SEE IF THE DCB HAS BEEN OPENED*/ 00023000
         L     @11,APUBDCB(,APUBPTR)                            01S0081 00024000
         LTR   @11,@11                                          01S0081 00025000
         BZ    @RF00081                                         01S0081 00026000
*     DO;                           /* TRY TO IPL THE 3838 IF SO.    */ 00027000
*       IF LOADNAME(1:4)^='WAIT' THEN/* NO LOAD MODULE FOR WAIT.     */ 00028000
         L     @11,@PC00001                                     01S0083 00029000
         CLC   LOADNAME(4,@11),@CC01905                         01S0083 00030000
         BE    @RF00083                                         01S0083 00031000
*         DO;                                                   02S0084 00032000
*           IMPLNAME='IMPLCUXX';    /* INIT IMPL LOAD MODULE NAME.   */ 00033000
         MVC   IMPLNAME(8),@CC01906                             02S0085 00034000
*           IMPLID=APUBIPL;         /* BUILD MEMBER NAME FOR IMPL       00035000
*                                      LOAD MODULE                   */ 00036000
         MVC   IMPLID(2),APUBIPL(APUBPTR)                       02S0086 00037000
*           IMPLCU=UCBNAME(1:2);    /* CHANNEL AND SUBCHANNEL NUMBER.*/ 00038000
         L     @01,UCBPTR(,APUBPTR)                             02S0087 00039000
         MVC   IMPLCU(2),UCBNAME(@01)                           02S0087 00040000
*           ICTLNAME=IMPLNAME;      /* BUILD IMPL CONTROL RECORD NAME*/ 00041000
         MVC   ICTLNAME(8),IMPLNAME                             02S0088 00042000
*           ICTLNAME(2:4)='CTL';    /* SUBSTITUTE CTL FOR MPL.       */ 00043000
         MVC   ICTLNAME+1(3),@CC01907                           02S0089 00044000
*           RESPECIFY                                           02S0090 00045000
*            (R0P,                                              02S0090 00046000
*             R1P,                                              02S0090 00047000
*             R2P,                                              02S0090 00048000
*             R3P) RESTRICTED;                                  02S0090 00049000
*           R2P=ADDR(LOADNAME);     /* NAME OF IPL LOAD MODULE.      */ 00050000
         LR    R2P,@11                                          02S0091 00051000
*           R3P=APCTDCLD;           /* ADDRESS OF THE SYS1.LOADSSSS     00052000
*                                      DCB                           */ 00053000
         L     R3P,APCTDCLD(,APCTPTR)                           02S0092 00054000
*           GEN REFS(R2P,R3P,IMPLNAME,ICTLNAME) FLOWS(NOLOAD1,NOLOAD2)  00055000
*               SETS(R0P,R1P,IMPLADDR,IPLADDR,ICTLADDR) SEQFLOW;        00056000
*  LOAD THE IMPL CONTROL RECORD.                                        00057000
         LOAD  EPLOC=ICTLNAME,DCB=(R3P),ERRET=NOLOAD1                   00058000
         ST    R0P,ICTLADDR                                             00059000
*  LOAD THE IMPL MODULE.                                                00060000
         LOAD  EPLOC=IMPLNAME,DCB=(R3P),ERRET=NOLOAD1                   00061000
         ST    R0P,IMPLADDR       SAVE THE ADDRESS OF THE IMPL MODULE.  00062000
*  LOAD THE IPL MODULE.                                                 00063000
         LOAD  EPLOC=(R2P),DCB=(R3P),ERRET=NOLOAD2                      00064000
         ST    R0P,IPLADDR        SAVE THE ADDRESS OF THE IPL MODULE.   00065000
*           RC=0;                   /* INDICATE OK RETURN.           */ 00066000
         SLR   @11,@11                                          02S0094 00067000
         STH   @11,RC                                           02S0094 00068000
*           GOTO ENDLOAD;                                       02S0095 00069000
         B     ENDLOAD                                          02S0095 00070000
*NOLOAD1:                                                       02S0096 00071000
*           RC=4;                   /* IMPL MODULE NOT FOUND.        */ 00072000
NOLOAD1  MVC   RC(2),@CH00054                                   02S0096 00073000
*           GOTO ENDLOAD;                                       02S0097 00074000
         B     ENDLOAD                                          02S0097 00075000
*NOLOAD2:                                                       02S0098 00076000
*           RC=8;                   /* IPL MODULE NOT FOUND.         */ 00077000
NOLOAD2  MVC   RC(2),@CH00045                                   02S0098 00078000
*ENDLOAD:                                                       02S0099 00079000
*           ;                                                   02S0099 00080000
ENDLOAD  DS    0H                                               02S0100 00081000
*         END;                                                  02S0100 00082000
*       ELSE                                                    01S0101 00083000
*         RC=0;                     /* FOR WAIT FAKE OUT RETURN CODE.*/ 00084000
         B     @RC00083                                         01S0101 00085000
@RF00083 SLR   @11,@11                                          01S0101 00086000
         STH   @11,RC                                           01S0101 00087000
*       IF RC=0 THEN                /* IF LOAD WENT OK, THEN    01S0102 00088000
*                                      CONTINUE.                     */ 00089000
@RC00083 LH    @11,RC                                           01S0102 00090000
         LTR   @11,@11                                          01S0102 00091000
         BNZ   @RF00102                                         01S0102 00092000
*         DO;                                                   01S0103 00093000
*           RESPECIFY                                           02S0104 00094000
*             IMPLCNTL BASED(ICTLADDR);                         02S0104 00095000
*           RESPECIFY                                           02S0105 00096000
*             IPLCNTL BASED(IPLADDR);                           02S0105 00097000
*           IOBSTART=ADDR(CCWS);    /* ADDRESS WHERE CCWS ARE BUILT. */ 00098000
         L     @11,DCBPTR(,APUBPTR)                             02S0106 00099000
         L     @11,IOBSTPTR+16(,@11)                            02S0106 00100000
         LA    @05,CCWS                                         02S0106 00101000
         ST    @05,IOBSTART(,@11)                               02S0106 00102000
*           CCWPTR=ADDR(CCWS);      /* SET CCW INDEX.                */ 00103000
         LR    CCWPTR,@05                                       02S0107 00104000
*/*     START OF IMPL CCW BUILD.                                     */ 00105000
*                                                               02S0108 00106000
*           CCW=CNTLCCW;            /* IMPL/IPL CONTROL CCW.         */ 00107000
         MVC   CCW(8,CCWPTR),CNTLCCW                            02S0108 00108000
*           CCWPTR=CCWPTR+8;        /* BUMP TO NEXT CCW.             */ 00109000
         LA    @11,8                                            02S0109 00110000
         ALR   CCWPTR,@11                                       02S0109 00111000
*           IF LOADNAME(1:4)='WAIT' THEN/* FOR WAIT, ONLY THE CONTROL   00112000
*                                      CCW                           */ 00113000
         L     @15,@PC00001                                     02S0110 00114000
         CLC   LOADNAME(4,@15),@CC01905                         02S0110 00115000
         BNE   @RF00110                                         02S0110 00116000
*             DO;                   /* AND A SENSE CCW ARE REQUIRED. */ 00117000
*               CCW=SENSE;          /* PUT IN THE SENSE CCW WHICH       00118000
*                                      WILL RESET THE 3838.          */ 00119000
         MVC   CCW(8,CCWPTR),SENSE                              02S0112 00120000
*               CCWADDR=ADDR(CCWS); /* DUMMY ADDRESS TO PREVENT THE     00121000
*                                      REFERENCING OF STORAGE WITH      00122000
*                                      SOME OTHER KEY. ---- DATA IS     00123000
*                                      NOT TRANSFERED SO NOTHING IS     00124000
*                                      DESTROYED.--------            */ 00125000
         STCM  @05,7,CCWADDR(CCWPTR)                            02S0113 00126000
*               CCWPTR=CCWPTR+8;    /* BUMP TO THE NEXT CCW.         */ 00127000
         ALR   CCWPTR,@11                                       02S0114 00128000
*             END;                  /* END OF THE UNLOAD BUILD.      */ 00129000
*           ELSE                    /* PREPARE NORMAL IMPL/IPL CCWS. */ 00130000
*             DO;                   /* BUILD NORMAL CCW STRING.      */ 00131000
         B     @RC00110                                         02S0116 00132000
@RF00110 DS    0H                                               02S0117 00133000
*               IF IMCHNMD='10'X THEN/* IF TWO BYTE CHANNEL THEN        00134000
*                                      CANNOT DATA CHAIN BELOW A        00135000
*                                      MINIMUM BYTE COUNT, ELSE WILL    00136000
*                                      GET INCORRECT LENGTH GENERATED   00137000
*                                      BY THE CHANNEL                */ 00138000
         L     @11,ICTLADDR                                     02S0117 00139000
         CLI   IMCHNMD(@11),X'10'                               02S0117 00140000
         BNE   @RF00117                                         02S0117 00141000
*                 APCTDCNR=04;      /* SET MINIMUM BYTE COUNT.       */ 00142000
         MVC   APCTDCNR(2,APCTPTR),@CH00054                     02S0118 00143000
*               CCW=WRTIPLC;        /* CCW TO WRITE THE CONTROL 02S0119 00144000
*                                      RECORD.                       */ 00145000
@RF00117 MVC   CCW(8,CCWPTR),WRTIPLC                            02S0119 00146000
*               CCWADDR=ICTLADDR;   /* ADDRESS OF THE CONTROL RECORD.*/ 00147000
         L     @11,ICTLADDR                                     02S0120 00148000
         STCM  @11,7,CCWADDR(CCWPTR)                            02S0120 00149000
*               CCWPTR=CCWPTR+8;    /* BUMP TO THE NEXT CCW.         */ 00150000
         LA    @05,8                                            02S0121 00151000
         ALR   CCWPTR,@05                                       02S0121 00152000
*               CCW=WRTIPLC;        /* WRITE FIRST OF THE LOAD  02S0122 00153000
*                                      MODULE.                       */ 00154000
         MVC   CCW(8,CCWPTR),WRTIPLC                            02S0122 00155000
*               CCWADDR=IMPLADDR;   /* PUT IN THE OFFSET TO THE PART    00156000
*                                      * WE ARE WRITING.             */ 00157000
         L     @15,IMPLADDR                                     02S0123 00158000
         STCM  @15,7,CCWADDR(CCWPTR)                            02S0123 00159000
*               CCWCNT=IMCNT1*4;    /* NUMBER OF BYTES TO WRITE.     */ 00160000
         LH    @14,IMCNT1(,@11)                                 02S0124 00161000
         N     @14,@CF02012                                     02S0124 00162000
         SLA   @14,2                                            02S0124 00163000
         STH   @14,CCWCNT(,CCWPTR)                              02S0124 00164000
*               CCWPTR=CCWPTR+8;    /* BUMP TO THE NEXT CCW.         */ 00165000
         ALR   CCWPTR,@05                                       02S0125 00166000
*               IF IMCNT2^=0 THEN   /* SEE IF THERE IS A SECOND PART.*/ 00167000
         LH    @11,IMCNT2(,@11)                                 02S0126 00168000
         N     @11,@CF02012                                     02S0126 00169000
         LTR   @11,@11                                          02S0126 00170000
         BZ    @RF00126                                         02S0126 00171000
*                 DO;               /* TO WRITE .                    */ 00172000
*                   CCW=WRTIPLC;    /* CCW FOR SECOND PART OF LOAD.  */ 00173000
         MVC   CCW(8,CCWPTR),WRTIPLC                            02S0128 00174000
*                   CCWADDR=IMPLADDR+/* OFFSET TO THE SECOND PART OF    00175000
*                                      THE                           */ 00176000
*                       IMCNT1*4;   /* THE LOAD.                     */ 00177000
         ALR   @15,@14                                          02S0129 00178000
         STCM  @15,7,CCWADDR(CCWPTR)                            02S0129 00179000
*                   CCWCNT=IMCNT2*4;/* NUMBER OF BYTES TO WRITE.     */ 00180000
         SLA   @11,2                                            02S0130 00181000
         STH   @11,CCWCNT(,CCWPTR)                              02S0130 00182000
*                   CCWPTR=CCWPTR+8;/* BUMP TO THE NEXT CCW.         */ 00183000
         ALR   CCWPTR,@05                                       02S0131 00184000
*                 END;                                          02S0132 00185000
*                                                               02S0132 00186000
*/*     END OF IMPL CCW BUILD.                                       */ 00187000
*/*     START OF IPL CCW BUILD.                                      */ 00188000
*                                                               02S0133 00189000
*               CCW=WRTIPLC;        /* CCW TO WRITE THE CONTROL 02S0133 00190000
*                                      RECORD.                       */ 00191000
@RF00126 MVC   CCW(8,CCWPTR),WRTIPLC                            02S0133 00192000
*               CCWADDR=IPLADDR;    /* ADDRESS OF THE CONTROL RECORD.*/ 00193000
         L     @11,IPLADDR                                      02S0134 00194000
         STCM  @11,7,CCWADDR(CCWPTR)                            02S0134 00195000
*               CCWPTR=CCWPTR+8;    /* BUMP TO THE NEXT CCW.         */ 00196000
         AL    CCWPTR,@CF00045                                  02S0135 00197000
*               CCW=WRTREC1;        /* WRITE FIRST OF THE LOAD  02S0136 00198000
*                                      MODULE.                       */ 00199000
         MVC   CCW(8,CCWPTR),WRTREC1                            02S0136 00200000
*               CCWADDR=IPLADDR+    /* PUT IN THE OFFSET TO THE PART */ 00201000
*                   (IADDR1*2);     /* * WE ARE WRITING.             */ 00202000
         LH    @15,IADDR1(,@11)                                 02S0137 00203000
         N     @15,@CF02012                                     02S0137 00204000
         ALR   @15,@15                                          02S0137 00205000
         ALR   @15,@11                                          02S0137 00206000
         STCM  @15,7,CCWADDR(CCWPTR)                            02S0137 00207000
*               ICNT1=ICNT1/2;      /* MUST HALF COUNT BECAUSE IT IS    00208000
*                                      IN HALF WORDS IN THE INPUT,      00209000
*                                      BUT 3838 EXPECTS IT IN WORDS. */ 00210000
         LH    @15,ICNT1(,@11)                                  02S0138 00211000
         N     @15,@CF02012                                     02S0138 00212000
         SRL   @15,1                                            02S0138 00213000
         STH   @15,ICNT1(,@11)                                  02S0138 00214000
*               BYTECNT=ICNT1*4;    /* NUMBER OF BYTES TO WRITE.     */ 00215000
         SLA   @15,2                                            02S0139 00216000
         LR    BYTECNT,@15                                      02S0139 00217000
*               DO UNTIL BYTECNT<=0;/* GENERATE A DATA CHAIN OF CCWS    00218000
*                                      IF REQUIRED                   */ 00219000
@DL00140 DS    0H                                               02S0141 00220000
*                 IF BYTECNT>32760 THEN/* IF TOO BIG FOR ONE CCW     */ 00221000
         L     @11,@CF01921                                     02S0141 00222000
         CR    BYTECNT,@11                                      02S0141 00223000
         BNH   @RF00141                                         02S0141 00224000
*                   DO;             /* GENERATE DATA CHAINED CCWS.   */ 00225000
*                     CCWCNT=32760; /* PUT BYTE COUNT IN CCW.        */ 00226000
         STH   @11,CCWCNT(,CCWPTR)                              02S0143 00227000
*                     CCWCD='1'B;   /* SET DATA CHAIN.               */ 00228000
         OI    CCWCD(CCWPTR),B'10000000'                        02S0144 00229000
*                     IOBDATCH='1'B;/* INDICATE DATA CHAINING USED.  */ 00230000
         L     @15,DCBPTR(,APUBPTR)                             02S0145 00231000
         L     @15,IOBSTPTR+16(,@15)                            02S0145 00232000
         OI    IOBDATCH(@15),B'10000000'                        02S0145 00233000
*                     EVAL(CCWPTR+8)->CCW=WRTREC1;/* CCW FOR NEXT       00234000
*                                                             SEGMENT*/ 00235000
         LA    @15,8                                            02S0146 00236000
         LR    @14,CCWPTR                                       02S0146 00237000
         ALR   @14,@15                                          02S0146 00238000
         MVC   CCW(8,@14),WRTREC1                               02S0146 00239000
*                     EVAL(CCWPTR+8)->CCWADDR/* SET ADDRESS OF NEXT     00240000
*                                                             SEGMENT*/ 00241000
*                         =CCWADDR+32760;                       02S0147 00242000
         ALR   @15,CCWPTR                                       02S0147 00243000
         L     @14,CCWADDR-1(,CCWPTR)                           02S0147 00244000
         LA    @14,0(,@14)                                      02S0147 00245000
         ALR   @11,@14                                          02S0147 00246000
         STCM  @11,7,CCWADDR(@15)                               02S0147 00247000
*                   END;                                        02S0148 00248000
*                 ELSE                                          02S0149 00249000
*                   CCWCNT=BYTECNT; /* STORE COUNT TO BE WRITTEN.    */ 00250000
         B     @RC00141                                         02S0149 00251000
@RF00141 STH   BYTECNT,CCWCNT(,CCWPTR)                          02S0149 00252000
*                 CCWPTR=CCWPTR+8;  /* INCREMENT CCW PTR.            */ 00253000
@RC00141 AL    CCWPTR,@CF00045                                  02S0150 00254000
*                 BYTECNT=BYTECNT-32760;/* DECREMENT COUNT TO BE        00255000
*                                      WRITTEN.                      */ 00256000
         SL    BYTECNT,@CF01921                                 02S0151 00257000
*               END;                                            02S0152 00258000
@DE00140 LTR   BYTECNT,BYTECNT                                  02S0152 00259000
         BP    @DL00140                                         02S0152 00260000
*               IF LOADNAME(1:4)='DUMP' THEN/* DUMP REQUIRES BULK       00261000
*                                      STORAGE SIZE                  */ 00262000
         L     @11,@PC00001                                     02S0153 00263000
         CLC   LOADNAME(4,@11),@CC01924                         02S0153 00264000
         BNE   @RF00153                                         02S0153 00265000
*                 IBSIZE=APUBRGN*256;/* IN LAST WORD OF CONTROL 02S0154 00266000
*                                      RECORD.                       */ 00267000
         LH    @11,APUBRGN(,APUBPTR)                            02S0154 00268000
         SLA   @11,8                                            02S0154 00269000
         L     @05,IPLADDR                                      02S0154 00270000
         STCM  @11,7,IBSIZE(@05)                                02S0154 00271000
*               ELSE                                            02S0155 00272000
*                 DO;                                           02S0155 00273000
         B     @RC00153                                         02S0155 00274000
@RF00153 DS    0H                                               02S0156 00275000
*                   IF ICNT2^=0 THEN/* SEE IF THERE IS A SECOND PART.*/ 00276000
         L     @11,IPLADDR                                      02S0156 00277000
         LH    @05,ICNT2(,@11)                                  02S0156 00278000
         N     @05,@CF02012                                     02S0156 00279000
         LTR   @05,@05                                          02S0156 00280000
         BZ    @RF00156                                         02S0156 00281000
*                     DO;           /* TO WRITE .                    */ 00282000
*                       CCW=WRTREC1;/* CCW FOR SECOND PART OF LOAD.  */ 00283000
         MVC   CCW(8,CCWPTR),WRTREC1                            02S0158 00284000
*                       CCWADDR=IPLADDR+/* OFFSET TO THE SECOND PART    00285000
*                                      OF THE                        */ 00286000
*                           (IADDR2*2);/* THE LOAD.                  */ 00287000
         LH    @15,IADDR2(,@11)                                 02S0159 00288000
         N     @15,@CF02012                                     02S0159 00289000
         ALR   @15,@15                                          02S0159 00290000
         ALR   @15,@11                                          02S0159 00291000
         STCM  @15,7,CCWADDR(CCWPTR)                            02S0159 00292000
*                       ICNT2=ICNT2/2;/* MUST HALF COUNT BECAUSE IT     00293000
*                                      IS IN HALF WORDS IN THE INPUT,   00294000
*                                      BUT 3838 EXPECTS IT IN WORDS. */ 00295000
         L     @15,IPLADDR                                      02S0160 00296000
         LH    @15,ICNT2(,@15)                                  02S0160 00297000
         N     @15,@CF02012                                     02S0160 00298000
         SRL   @15,1                                            02S0160 00299000
         STH   @15,ICNT2(,@11)                                  02S0160 00300000
*                       BYTECNT=ICNT2*4;/* NUMBER OF BYTES TO WRITE. */ 00301000
         SLA   @15,2                                            02S0161 00302000
         LR    BYTECNT,@15                                      02S0161 00303000
*                       DO UNTIL BYTECNT<=0;/* GENERATE A DATA CHAIN    00304000
*                                      OF CCWS IF REQUIRED           */ 00305000
@DL00162 DS    0H                                               02S0163 00306000
*                         IF BYTECNT>32760 THEN/* IF TOO BIG FOR ONE    00307000
*                                      CCW                           */ 00308000
         L     @11,@CF01921                                     02S0163 00309000
         CR    BYTECNT,@11                                      02S0163 00310000
         BNH   @RF00163                                         02S0163 00311000
*                           DO;     /* GENERATE DATA CHAINED CCWS.   */ 00312000
*                             CCWCNT=32760;/* PUT BYTE COUNT IN CCW. */ 00313000
         STH   @11,CCWCNT(,CCWPTR)                              02S0165 00314000
*                             CCWCD='1'B;/* SET DATA CHAIN.          */ 00315000
         OI    CCWCD(CCWPTR),B'10000000'                        02S0166 00316000
*                             IOBDATCH='1'B;/* INDICATE DATA CHAINING   00317000
*                                      USED.                         */ 00318000
         L     @15,DCBPTR(,APUBPTR)                             02S0167 00319000
         L     @15,IOBSTPTR+16(,@15)                            02S0167 00320000
         OI    IOBDATCH(@15),B'10000000'                        02S0167 00321000
*                             EVAL(CCWPTR+8)->CCW=WRTREC1;/* CCW FOR    00322000
*                                      NEXT                   SEGMENT*/ 00323000
         LA    @15,8                                            02S0168 00324000
         LR    @14,CCWPTR                                       02S0168 00325000
         ALR   @14,@15                                          02S0168 00326000
         MVC   CCW(8,@14),WRTREC1                               02S0168 00327000
*                             EVAL(CCWPTR+8)->CCWADDR/* SET ADDRESS     00328000
*                                      OF NEXT                SEGMENT*/ 00329000
*                                 =CCWADDR+32760;               02S0169 00330000
         ALR   @15,CCWPTR                                       02S0169 00331000
         L     @14,CCWADDR-1(,CCWPTR)                           02S0169 00332000
         LA    @14,0(,@14)                                      02S0169 00333000
         ALR   @11,@14                                          02S0169 00334000
         STCM  @11,7,CCWADDR(@15)                               02S0169 00335000
*                           END;                                02S0170 00336000
*                         ELSE                                  02S0171 00337000
*                           CCWCNT=BYTECNT;/* STORE COUNT TO BE 02S0171 00338000
*                                      WRITTEN.                      */ 00339000
         B     @RC00163                                         02S0171 00340000
@RF00163 STH   BYTECNT,CCWCNT(,CCWPTR)                          02S0171 00341000
*                         CCWPTR=CCWPTR+8;/* INCREMENT CCW PTR.      */ 00342000
@RC00163 AL    CCWPTR,@CF00045                                  02S0172 00343000
*                         BYTECNT=BYTECNT-32760;/* DECREMENT COUNT TO   00344000
*                                      BE WRITTEN.                   */ 00345000
         SL    BYTECNT,@CF01921                                 02S0173 00346000
*                       END;                                    02S0174 00347000
@DE00162 LTR   BYTECNT,BYTECNT                                  02S0174 00348000
         BP    @DL00162                                         02S0174 00349000
*                     END;                                      02S0175 00350000
*                 END;                                          02S0176 00351000
@RF00156 DS    0H                                               02S0177 00352000
*             END;                                              02S0177 00353000
@RC00153 DS    0H                                               02S0178 00354000
*           CCW=NOPCCW;             /* ADD NOP TO THE END OF THE        00355000
*                                      CHANNEL PROGRAM SO THAT  02S0178 00356000
*                                      CHANNEL END AND DEVICE END MAY   00357000
*                                      OCCUR SIMULTANEOULY, THIS IS     00358000
*                                      NOT STRICTLY NECESSARY, BUT IT   00359000
*                                      AIDS IN DEBUGGING AND GIVES      00360000
*                                      THE MIH A BETTER CHANCE TO       00361000
*                                      WORK.                         */ 00362000
@RC00110 MVC   CCW(8,CCWPTR),NOPCCW                             02S0178 00363000
*/*     END OF IPL CCW BUILD.                                        */ 00364000
*                                                               02S0179 00365000
*                                                               02S0179 00366000
*           /*********************************************************/ 00367000
*           /*                                                       */ 00368000
*           /* ISSUE THE CLEAR I/O TO ELIMINATE THE POSSIBILITY THAT */ 00369000
*           /* THE 3838 OWES THE CHANNEL A DEVICE END INTERRUPT.     */ 00370000
*           /*                                                       */ 00371000
*           /*********************************************************/ 00372000
