*         DO;                       /* IF SO UPDATE THE APUBS.       */ 00083000
*           SAVEAPUB=APUBPTR;       /* SAVE THE ORIGINAL APUB   02S0132 00084000
*                                      ADDRESS.                      */ 00085000
         LR    SAVEAPUB,APUBPTR                                 02S0132 00086000
*           DO UNTIL APUBPTR=0;                                 02S0133 00087000
