         BNE   @RF00115                                         01S0115 00034000
*     DO;                           /* THEN AN INDICATOR MUST BE SET    00035000
*                                      ON TO SIGNIFY THAT BULK STORE    00036000
*                                      IS BAD.                       */ 00037000
*       SAVEAPUB=APUBPTR;           /* SAVE THE ADDRESS OF THE  01S0117 00038000
*                                      MASTER.                       */ 00039000
         LR    SAVEAPUB,APUBPTR                                 01S0117 00040000
*       DO UNTIL APUBPTR=0;         /* LOOP THRU THE APUBS ON 3838.  */ 00041000
@DL00118 DS    0H                                               01S0119 00042000
*         APUBIRIP='1'B;            /* INDICATE THAT REIPL TAKES        00043000
*                                      PLACE.                        */ 00044000
         L     @11,APUBIRIP-3(,APUBPTR)                         01S0119 00045000
@GL00001 LR    @08,@11                                          01S0119 00046000
