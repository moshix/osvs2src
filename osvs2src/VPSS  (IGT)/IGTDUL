*             END;                                              02S0156 00152000
@DE00154 LTR   XPTR,XPTR                                        02S0156 00153000
         BZ    @DC00154                                         02S0156 00154000
         TM    APUBIDWN(XPTR),B'10000000'                       02S0156 00155000
         BO    @DL00154                                         02S0156 00156000
@DC00154 DS    0H                                               02S0157 00157000
*             IF XPTR=0 THEN        /* IF ALL WERE UNLOADED,         */ 00158000
         LTR   XPTR,XPTR                                        02S0157 00159000
         BNZ   @RF00157                                         02S0157 00160000
*               DO;                                             02S0158 00161000
*                 APCTHALT='1'B;    /* SET ON HALT INDICATOR.        */ 00162000
         L     @11,APCTHALT(,APCTPTR)                           02S0159 00163000
@GL00005 LR    @15,@11                                          02S0159 00164000
         O     @15,@CF01697                                     02S0159 00165000
         CS    @11,@15,APCTHALT(APCTPTR)                        02S0159 00166000
         BNE   @GL00005                                         02S0159 00167000
*               END;                                            02S0160 00168000
*             RESPECIFY                                         02S0161 00169000
*               APUB BASED(APUBPTR);                            02S0161 00170000
@RF00157 DS    0H                                               02S0162 00171000
*           END;                                                02S0162 00172000
*       END;                                                    02S0163 00173000
@RF00150 DS    0H                                               02S0164 00174000
*       DO;                         /* RECOUNT THE IPLED 3838S.      */ 00175000
*                                                               02S0164 00176000
*         /***********************************************************/ 00177000
*         /*                                                         */ 00178000
*         /* RECOUNT THE NUMBER OF IPLED 3838S. THE COUNT IS USED BY */ 00179000
*         /* THE VPSS IO DRIVER FOR LOAD BALANCING.                  */ 00180000
*         /*                                                         */ 00181000
*         /***********************************************************/ 00182000
*                                                               02S0165 00183000
*         APUBPTR=APCTAPUB;         /* ADDRESS OF FIRST MASTER.      */ 00184000
         L     APUBPTR,APCTAPUB(,APCTPTR)                       02S0165 00185000
*         COUNT=0;                  /* INITIALIZE THE COUNT OF 3838S.*/ 00186000
         SLR   COUNT,COUNT                                      02S0166 00187000
*         DO WHILE APUBPTR^=0;      /* PROCESS ALL 3838S.            */ 00188000
         B     @DE00167                                         02S0167 00189000
@DL00167 DS    0H                                               02S0168 00190000
*           IF APUBIDWN='0'B THEN   /* IF BOX IS IPLED,              */ 00191000
         TM    APUBIDWN(APUBPTR),B'10000000'                    02S0168 00192000
         BNZ   @RF00168                                         02S0168 00193000
*             COUNT=COUNT+1;        /* INCREMENT COUNT.              */ 00194000
         AL    COUNT,@CF00120                                   02S0169 00195000
*           APUBPTR=APUBMSTR;       /* NEXT MASTER.                  */ 00196000
@RF00168 L     APUBPTR,APUBMSTR(,APUBPTR)                       02S0170 00197000
*         END;                      /* FINISH COUNT.                 */ 00198000
@DE00167 LTR   APUBPTR,APUBPTR                                  02S0171 00199000
         BNZ   @DL00167                                         02S0171 00200000
*         APCTCTUB=COUNT;           /* SET COUNT FOR IO DRIVER.      */ 00201000
         STC   COUNT,APCTCTUB(,APCTPTR)                         02S0172 00202000
*       END;                        /* END OF RECOUNT.               */ 00203000
*     END;                                                      01S0174 00204000
*   RETURN;                         /* RETURN TO CALLER. IGTXEPLG    */ 00205000
