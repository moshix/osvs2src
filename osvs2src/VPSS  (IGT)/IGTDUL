TDUL     TITLE 'IGTDUL - COMMON UNLOAD PROCESSING.                     *00001000
                        '                                               00002000
IGTDUL   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTDUL    77.092'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
*   XPTR=APUBAPUB;                  /* PTR TO FIRST USER APUB.       */ 00022000
         L     XPTR,APUBAPUB(,APUBPTR)                          01S0112 00023000
*   RESPECIFY                                                   01S0113 00024000
*     APUB BASED(XPTR);             /* TEMP BASE REG.                */ 00025000
*   DO WHILE XPTR^=0                /* TEST ALL APUBS FOR BEING 01S0114 00026000
*                                      DISABLD                       */ 00027000
*         &APUBIDAB='1'B;           /* QUIT AT END OR IF ONE IS 01S0114 00028000
*                                      ENABLD.                       */ 00029000
         B     @DE00114                                         01S0114 00030000
@DL00114 DS    0H                                               01S0115 00031000
*     XPTR=APUBAPUB;                /* GO TO NEXT APUB.              */ 00032000
         L     XPTR,APUBAPUB(,XPTR)                             01S0115 00033000
*   END;                                                        01S0116 00034000
@DE00114 LTR   XPTR,XPTR                                        01S0116 00035000
         BZ    @DC00114                                         01S0116 00036000
         TM    APUBIDAB(XPTR),B'01000000'                       01S0116 00037000
         BO    @DL00114                                         01S0116 00038000
@DC00114 DS    0H                                               01S0117 00039000
*   RESPECIFY                                                   01S0117 00040000
*     APUB BASED(APUBPTR);                                      01S0117 00041000
*   IF XPTR=0 THEN                  /* IF 0, THEN ALL ARE DISABLED.  */ 00042000
         LTR   XPTR,XPTR                                        01S0118 00043000
         BNZ   @RF00118                                         01S0118 00044000
*     DO;                                                       01S0119 00045000
*       IF APUBIDWN='0'B THEN       /* ENSURE THAT IS IS NOT ALREADY */ 00046000
         TM    APUBIDWN(APUBPTR),B'10000000'                    01S0120 00047000
         BNZ   @RF00120                                         01S0120 00048000
*         DO;                       /* UNLOADED.                     */ 00049000
*           WAITRT='WAIT';          /* CONSTRUCT NAME OF WAIT   01S0122 00050000
*                                      ROUTINE.                      */ 00051000
         MVI   WAITRT+4,C' '                                    01S0122 00052000
         MVC   WAITRT+5(3),WAITRT+4                             01S0122 00053000
         MVC   WAITRT(4),@CC01601                               01S0122 00054000
*           WAITCU=UCBNAME(1:2);    /* CHANNEL TO BE UNLOADED.       */ 00055000
         L     @11,UCBPTR(,APUBPTR)                             01S0123 00056000
         MVC   WAITCU(2),UCBNAME(@11)                           01S0123 00057000
*           WAITID=APUBIPL;         /* CONSTRUCT NAME OF WAIT   01S0124 00058000
*                                      ROUTINE.                      */ 00059000
         MVC   WAITID(2),APUBIPL(APUBPTR)                       01S0124 00060000
*           CALL APCVZDIO(WAITRT);  /* IPL TO DISABLED WAIT STATE.   */ 00061000
         LA    @11,WAITRT                                       01S0125 00062000
         ST    @11,@AL00001                                     01S0125 00063000
         MVI   @AL00001,X'80'                                   01S0125 00064000
         L     @15,APCTZDIO(,APCTPTR)                           01S0125 00065000
         LA    @01,@AL00001                                     01S0125 00066000
         BALR  @14,@15                                          01S0125 00067000
*         END;                                                  01S0126 00068000
*       DO;                                                     02S0127 00069000
@RF00120 DS    0H                                               02S0128 00070000
*         IF APUBIDWN='0'B THEN     /* SEE IF ITS ALREADY UNLOADED.  */ 00071000
         TM    APUBIDWN(APUBPTR),B'10000000'                    02S0128 00072000
         BNZ   @RF00128                                         02S0128 00073000
*           DO;                     /* IF NOT, THEN FREE THE DCB ETC.*/ 00074000
*             RESPECIFY                                         02S0130 00075000
*               R3P RESTRICTED;                                 02S0130 00076000
*             RESPECIFY                                         02S0131 00077000
*               R2P RESTRICTED;                                 02S0131 00078000
*             R2P=APUBDCB;          /* GET ADDRESS OF DCB.           */ 00079000
         L     R2P,APUBDCB(,APUBPTR)                            02S0132 00080000
*             GEN REFS(STATCLS) SETS(DYNCLS);                   02S0133 00081000
   MVC   DYNCLS,STATCLS             MOVE IN THE PARM LIST.              00082000
   CLOSE  ((R2P)),MF=(E,DYNCLS)     CLOSE THE DCB.                      00083000
*             R3P=FREELN;           /* LENGTH OF GOTTEN MEMORY.      */ 00084000
         L     @11,APUBDCB(,APUBPTR)                            02S0134 00085000
         L     R3P,FREELN(,@11)                                 02S0134 00086000
*             GEN;                                              02S0135 00087000
   FREEMAIN R,A=(R2P),LV=(R3P)       FREE THE IUB, DCB, AND ECB.     */ 00088000
*             CALL APCVZDUA(8);     /* UNALLOCATE THE PORTS.         */ 00089000
         L     @15,APCTZDUA(,APCTPTR)                           02S0136 00090000
         LA    @01,@AL00136                                     02S0136 00091000
         BALR  @14,@15                                          02S0136 00092000
*           END;                                                02S0137 00093000
*         RESPECIFY                                             02S0138 00094000
*           APUB BASED(XPTR);                                   02S0138 00095000
@RF00128 DS    0H                                               02S0139 00096000
*         XPTR=APUBPTR;             /* PTR TO MSTR BEING UNLOADED.   */ 00097000
         LR    XPTR,APUBPTR                                     02S0139 00098000
*         DO UNTIL XPTR=0;          /* MARK THE MASTER AND ALL USER  */ 00099000
@DL00140 DS    0H                                               02S0141 00100000
*           APUBIDWN='1'B;          /* PORTS UNLOADED FOR I/O,       */ 00101000
         L     @11,APUBIDWN(,XPTR)                              02S0141 00102000
@GL00002 LR    @05,@11                                          02S0141 00103000
         O     @05,@CF01691                                     02S0141 00104000
         CS    @11,@05,APUBIDWN(XPTR)                           02S0141 00105000
         BNE   @GL00002                                         02S0141 00106000
*           APUBUNLP='0'B;          /* AND RESET UNLOAD PENDING.     */ 00107000
         L     @11,APUBUNLP-1(,XPTR)                            02S0142 00108000
@GL00003 LR    @05,@11                                          02S0142 00109000
         N     @05,@CF01694                                     02S0142 00110000
         CS    @11,@05,APUBUNLP-1(XPTR)                         02S0142 00111000
         BNE   @GL00003                                         02S0142 00112000
*           XPTR=APUBAPUB;          /* BUMP TO NEXT APUB.            */ 00113000
         L     XPTR,APUBAPUB(,XPTR)                             02S0143 00114000
*         END;                                                  02S0144 00115000
@DE00140 SLR   @11,@11                                          02S0144 00116000
         CR    XPTR,@11                                         02S0144 00117000
         BNE   @DL00140                                         02S0144 00118000
*         RESPECIFY                                             02S0145 00119000
*           APUB BASED(APUBPTR);                                02S0145 00120000
*         APUBDCB=0;                /* ZERO THE DCB POINTER.         */ 00121000
         ST    @11,APUBDCB(,APUBPTR)                            02S0146 00122000
*         CALL APCVZDWT(IGT243I,    /* TELL OPERATOR IT IS UNLOADED. */ 00123000
*             14,APCTSSID,4,20,UCBNAME,3);                      02S0147 00124000
         MVC   @AL00001(28),@AL00147                            02S0147 00125000
         LA    @11,APCTSSID(,APCTPTR)                           02S0147 00126000
         ST    @11,@AL00001+8                                   02S0147 00127000
         L     @11,UCBPTR(,APUBPTR)                             02S0147 00128000
         LA    @11,UCBNAME(,@11)                                02S0147 00129000
         ST    @11,@AL00001+20                                  02S0147 00130000
         L     @15,APCTZDWT(,APCTPTR)                           02S0147 00131000
         LA    @01,@AL00001                                     02S0147 00132000
         BALR  @14,@15                                          02S0147 00133000
*       END;                                                    02S0148 00134000
*       DO;                                                     02S0149 00135000
*         IF APCTTERM='1'B THEN     /* IF WE ARE TRYING TO TERMINATE,*/ 00136000
         TM    APCTTERM(APCTPTR),B'00001000'                    02S0150 00137000
         BNO   @RF00150                                         02S0150 00138000
*           DO;                     /* SEE IF THIS WAS THE LAST 02S0151 00139000
*                                      UNLOAD.                       */ 00140000
*             RESPECIFY                                         02S0152 00141000
*               APUB BASED(XPTR);                               02S0152 00142000
*             XPTR=APCTAPUB;        /* FIRST MASTER APUB.            */ 00143000
         L     XPTR,APCTAPUB(,APCTPTR)                          02S0153 00144000
*             DO WHILE XPTR^=0      /* SEE IF ALL MASTERS ARE   02S0154 00145000
*                                      UNLOADED                      */ 00146000
*                   &APUBIDWN='1'B; /* QUIT AT END OR WHEN ONE ISN'T.*/ 00147000
         B     @DE00154                                         02S0154 00148000
@DL00154 DS    0H                                               02S0155 00149000
*               XPTR=APUBMSTR;      /* PTR TO NEXT APUB.             */ 00150000
