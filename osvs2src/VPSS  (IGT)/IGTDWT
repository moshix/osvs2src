TDWT     TITLE 'IGTDWT - MESSAGE WRITER.                               *00001000
                        '                                               00002000
IGTDWT   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTDWT    77.092'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
*   WPLPTR=MSGADDR;                 /* ADDR OF THE FIRST PART OF WTO.*/ 00022000
         L     @15,APCTZDMM(,APCTPTR)                           01S0103 00023000
         L     @14,MSGPTR(,R1P)                                 01S0103 00024000
         L     @14,MSG(,@14)                                    01S0103 00025000
         LR    @11,@15                                          01S0103 00026000
         ALR   @11,@14                                          01S0103 00027000
         L     WPLPTR,MSGADDR(,@11)                             01S0103 00028000
*   IF MSG>=MAXMSG THEN             /* SEE IF INVALID MESSAGE IS        00029000
*                                      REQSTD                        */ 00030000
         C     @14,MAXMSG(,@15)                                 01S0104 00031000
         BL    @RF00104                                         01S0104 00032000
*     DO;                           /* IF SO, SO NOTE.               */ 00033000
*       OUTBUF(1:WPLLGH+4)=         /* PUT ERROR MESSAGE IN BUFFER.  */ 00034000
*           (ADDR(APCVZDMM)+IGT220I)->MSGADDR->WPL(1:WPLLGH+4); 01S0106 00035000
         LA    @04,3                                            01S0106 00036000
         AH    @04,WPLLGH(,WPLPTR)                              01S0106 00037000
         L     @15,MSGADDR+72(,@15)                             01S0106 00038000
         EX    @04,@SM01921                                     01S0106 00039000
*       OUTBUF(14:17)=APCTSSID;     /* IDENTIFY SUBSYSTEM.           */ 00040000
         MVC   OUTBUF+13(4),APCTSSID(APCTPTR)                   01S0107 00041000
*       OUTBUF(35:36)='04';         /* IDENTIFY ERROR.               */ 00042000
         MVC   OUTBUF+34(2),@CC01816                            01S0108 00043000
*     END;                                                      01S0109 00044000
*   ELSE                                                        01S0110 00045000
*     DO;                           /* PROCESS A VALID MESSAGE.      */ 00046000
         B     @RC00104                                         01S0110 00047000
@RF00104 DS    0H                                               01S0111 00048000
*       IF WPLMCSFJ='0'B THEN       /* TEST FOR A MULTI LINE MESSAGE.*/ 00049000
         TM    WPLMCSFJ(WPLPTR),B'01000000'                     01S0111 00050000
         BNZ   @RF00111                                         01S0111 00051000
*         DO;                       /* PROCESS A SINGLE LINE REQUEST.*/ 00052000
*           OUTBUF(1:WPLLGH+4)=     /* MOVE MF=L WTO INTO OUTPUT        00053000
*                                      BUFFER                        */ 00054000
*               WPL(1:WPLLGH+4);                                01S0113 00055000
         LA    @04,3                                            01S0113 00056000
         AH    @04,WPLLGH(,WPLPTR)                              01S0113 00057000
         EX    @04,@SM01923                                     01S0113 00058000
*         END;                                                  01S0114 00059000
*       ELSE                                                    01S0115 00060000
*         DO;                       /* PROCESS A MULTILINE MSG.      */ 00061000
         B     @RC00111                                         01S0115 00062000
@RF00111 DS    0H                                               01S0116 00063000
*           OUTBUF(1:WPLLGH+8)=     /* MOVE FIRST LINE + ROUTCDE +      00064000
*                                      DESC                          */ 00065000
*               WPL(1:WPLLGH+8);    /* +LINETYPE + # LINES TO BUFFER.*/ 00066000
         LH    @10,WPLLGH(,WPLPTR)                              01S0116 00067000
         LA    @04,7                                            01S0116 00068000
         ALR   @04,@10                                          01S0116 00069000
         EX    @04,@SM01923                                     01S0116 00070000
*           NLPTR=ADDR(OUTBUF)+WPLLGH+8;/* SET PTR TO NEXT LINE.     */ 00071000
         LA    NLPTR,OUTBUF                                     01S0117 00072000
         ALR   NLPTR,@10                                        01S0117 00073000
         AL    NLPTR,@CF00073                                   01S0117 00074000
*           WPLPTR=WPLPTR+WPLLGH+4; /* BUMP PTR TO MLWTO INFO.       */ 00075000
         LA    @04,4                                            01S0118 00076000
         ALR   @10,WPLPTR                                       01S0118 00077000
         ALR   @10,@04                                          01S0118 00078000
         LR    WPLPTR,@10                                       01S0118 00079000
*           I=WPLLINES-1;           /* GET LINE COUNT.               */ 00080000
         SLR   I,I                                              01S0119 00081000
         IC    I,WPLLINES(,WPLPTR)                              01S0119 00082000
         BCTR  I,0                                              01S0119 00083000
*           WPLPTR=WPLPTR+4;        /* GET TO START OF NEXT LINE.    */ 00084000
         ALR   WPLPTR,@04                                       01S0120 00085000
*           DO I BY-1 TO 1;         /* MOVE EACH LINES.              */ 00086000
         B     @DE00121                                         01S0121 00087000
@DL00121 DS    0H                                               01S0122 00088000
*             NEXTLINE(1:WPLMLLEN)= /* MOVE NEXT LINE INTO OUTPUT       00089000
*                                      AREA.                         */ 00090000
*                 WPLML(1:WPLMLLEN);                            01S0122 00091000
         SLR   @10,@10                                          01S0122 00092000
         IC    @10,WPLMLLEN(,WPLPTR)                            01S0122 00093000
         LR    @04,@10                                          01S0122 00094000
         BCTR  @04,0                                            01S0122 00095000
         EX    @04,@SM01925                                     01S0122 00096000
*             NLPTR=NLPTR+WPLMLLEN; /* BUMP PAST THE LINE.           */ 00097000
         ALR   NLPTR,@10                                        01S0123 00098000
*             WPLPTR=WPLPTR+WPLMLLEN;/* THIS ONE, ALSO.              */ 00099000
         ALR   WPLPTR,@10                                       01S0124 00100000
*           END;                                                01S0125 00101000
         BCTR  I,0                                              01S0125 00102000
@DE00121 LTR   I,I                                              01S0125 00103000
         BP    @DL00121                                         01S0125 00104000
*         END;                      /* END OF MULTI LINE PROCESSING. */ 00105000
*       WPLPTR=ADDR(OUTBUF);        /* OUTPUT MESSAGE ADDRESS.       */ 00106000
@RC00111 LA    WPLPTR,OUTBUF                                    01S0127 00107000
*       IF VLB1='0'B THEN           /* SEE IF DATA REQUIRED IN  01S0128 00108000
*                                      MESSAGE.                      */ 00109000
         TM    VLB1(R1P),B'10000000'                            01S0128 00110000
         BNZ   @RF00128                                         01S0128 00111000
*         DO UNTIL VLB1='1'B;       /* QUIT AT END OF PARM LIST, NOTE   00112000
*                                      THAT R1 BUMPED BEFORE TEST SO    00113000
*                                      THAT REG POINTS +12 WHEN TEST    00114000
*                                      MADE                          */ 00115000
@DL00129 DS    0H                                               01S0130 00116000
*           WPL(MSGOFF:MSGOFF+VDATAL-1)=/* MOVE DATA INTO TEXT.      */ 00117000
*               VDATA(1:VDATAL);                                01S0130 00118000
         L     @04,MSGOFFP(,R1P)                                01S0130 00119000
         L     @04,MSGOFF(,@04)                                 01S0130 00120000
         ALR   @04,WPLPTR                                       01S0130 00121000
         BCTR  @04,0                                            01S0130 00122000
         L     @15,VDATALP(,R1P)                                01S0130 00123000
         L     @15,VDATAL(,@15)                                 01S0130 00124000
         BCTR  @15,0                                            01S0130 00125000
         L     @14,VDATAP(,R1P)                                 01S0130 00126000
         EX    @15,@SM01927                                     01S0130 00127000
*           R1P=12+R1P;             /* BUMP TO NEXT ENTRY.           */ 00128000
         LA    R1P,12(,R1P)                                     01S0131 00129000
*         END;                                                  01S0132 00130000
@DE00129 TM    VLB1(R1P),B'10000000'                            01S0132 00131000
         BNO   @DL00129                                         01S0132 00132000
*     END;                          /* END OF VALID MESSAGE.         */ 00133000
@RF00128 DS    0H                                               01S0134 00134000
*                                                               02S0134 00135000
*   /*****************************************************************/ 00136000
*   /*                                                               */ 00137000
*   /* WRITE EACH LINE TO THE MESSAGE LOG.                           */ 00138000
*   /*                                                               */ 00139000
*   /*****************************************************************/ 00140000
*                                                               02S0134 00141000
*   RFY                                                         02S0134 00142000
*    (R2P,                                                      02S0134 00143000
*     R3P) RSTD;                                                02S0134 00144000
@RC00104 DS    0H                                               02S0135 00145000
*   R2P=APCTLOGD;                   /* ADDRESS OF THE MESSAGE LOG       00146000
*                                      DCB.                          */ 00147000
         L     R2P,APCTLOGD(,APCTPTR)                           02S0135 00148000
*   IF R2P^=0 THEN                  /* SEE IF LOG IS OPENED.         */ 00149000
         LTR   R2P,R2P                                          02S0136 00150000
         BZ    @RF00136                                         02S0136 00151000
*     DO;                           /* IF SO THEN FORMAT AND PRINT.  */ 00152000
*                                                               03S0138 00153000
*       /*************************************************************/ 00154000
*       /*                                                           */ 00155000
*       /* THIS CODE SETS THE LOG LOCK TO THE ADDRESS OF THE TCB OF  */ 00156000
*       /* THE CALLING TASK, USING COMPARE AND SWAP. THIS PREVENTS   */ 00157000
*       /* MULTIPLE SIMULTANEOUS CALLERS FROM BOTHERING EACH OTHER.  */ 00158000
*       /* THE LOCK IS RESET TO ZERO WHEN THE LINE HAS BEEN PRINTED. */ 00159000
*       /*                                                           */ 00160000
*       /*************************************************************/ 00161000
*                                                               03S0138 00162000
*       DO;                         /* GET THE LOG LOCK.             */ 00163000
*TRYAGAIN:                                                      03S0139 00164000
*         ;                         /* RETURN TO THIS LABEL IF  03S0139 00165000
*                                      ATTEMPT * TO OBTAIN LOCK 03S0139 00166000
*                                      FAILS.                        */ 00167000
TRYAGAIN DS    0H                                               03S0140 00168000
*         LOCKSW=0;                 /* INIT THE SWITCH REQUIRED BY      00169000
*                                      CS.                           */ 00170000
         SLR   LOCKSW,LOCKSW                                    03S0140 00171000
*         CS(LOCKSW,CURTCB,APCTLOGL);/* TRY TO OBTAIN CONTROL OF        00172000
*                                      LOCK.                         */ 00173000
         L     @15,CVTPTR                                       03S0141 00174000
         L     @15,CVTTCBP(,@15)                                03S0141 00175000
         L     @00,CURTCB(,@15)                                 03S0141 00176000
         CS    LOCKSW,@00,APCTLOGL(APCTPTR)                     03S0141 00177000
*         BC(8,GOTIT);              /* GO PRINT IF WE GOT IT.        */ 00178000
         BC    8,GOTIT                                          03S0142 00179000
*         GEN REFS(KILLTIME)        /* DID NOT GET IT, SO WAIT  03S0143 00180000
*                                      AWHILE.                       */ 00181000
*             (STIMER WAIT,BINTVL=KILLTIME);/* WAIT.                 */ 00182000
         STIMER WAIT,BINTVL=KILLTIME                                    00183000
*         GOTO TRYAGAIN;            /* GO TRY TO GET IT AGAIN.       */ 00184000
         B     TRYAGAIN                                         03S0144 00185000
*GOTIT:                                                         03S0145 00186000
*         ;                         /* GOT THE LOCK SO EXIT STAGE       00187000
*                                      LEFT.                         */ 00188000
GOTIT    DS    0H                                               03S0146 00189000
*       END;                        /* END OF GETTING THE LOG LOCK.  */ 00190000
*       DO;                         /* CREATE THE RECOVERY      03S0147 00191000
*                                      ENVIRONMENT.                  */ 00192000
*         APES(1:APESLN-LENGTH(APESSDWA))=''X;/* CLEAR THE WORKAREA  */ 00193000
         XC    APES(136),APES                                   03S0148 00194000
*         APESID=APESIDC;           /* SET ID FIELD.                 */ 00195000
         MVC   APESID(4),@CC01582                               03S0149 00196000
*         APESADDR=ADDR(APES);      /* ADDRESS OF THE APES.          */ 00197000
         LA    @04,APES                                         03S0150 00198000
         ST    @04,APESADDR                                     03S0150 00199000
*         APESAPMV=ADDR(DWTITBL);   /* ADDRESS OF MSG INDEX TABLE.   */ 00200000
         LA    @04,DWTITBL                                      03S0151 00201000
         ST    @04,APESAPMV                                     03S0151 00202000
*         APESCODE=1;               /* INDEX TO FIRST AND ONLY ENTRY.*/ 00203000
         MVI   APESCODE,X'01'                                   03S0152 00204000
*         APESMXCD=1;               /* MAX INDEX VALUE.              */ 00205000
         MVI   APESMXCD,X'01'                                   03S0153 00206000
*         APESFREC='1'B;            /* INDICATE THAT ABEND IS TO GO     00207000
*                                      ON LOGREC.                    */ 00208000
         OI    APESFREC,B'01000000'                             03S0154 00209000
*         APESREGP=ADDR(APESREGZ);  /* ADDRESS OF THE RETRY REGS.    */ 00210000
         LA    @04,APESREGZ                                     03S0155 00211000
         ST    @04,APESREGP                                     03S0155 00212000
*         STM(R0P,R14P,APESREGZ);   /* STORE REGS.                   */ 00213000
         STM   R0P,R14P,APESREGZ                                03S0156 00214000
*         APESREGF=ADDR(DWTRETRY);  /* ADDRESS OF THE RETRY ROUTINE. */ 00215000
         LA    @00,DWTRETRY                                     03S0157 00216000
         ST    @00,APESREGF(,@04)                               03S0157 00217000
*         RFY                                                   03S0158 00218000
*          (R4P,                                                03S0158 00219000
*           R5P) RSTD;              /* NEED THESE REGS.              */ 00220000
*         R4P=ADDR(APESADDR);       /* ADDRESS OF PARM FOR IGTUKA.   */ 00221000
         LA    R4P,APESADDR                                     03S0159 00222000
*         R5P=APCTZUKA;             /* IGTUKA IS THE EXIT ROUTINE.   */ 00223000
         L     R5P,APCTZUKA(,APCTPTR)                           03S0160 00224000
*         GEN REFS(SSTAE,R4P,R5P)   /* ISSUE THE STAE.               */ 00225000
*             SETS(DSTAE,R1P,R0P,R15P) NOFLOWS SEQFLOW NOEXIT;  03S0161 00226000
      MVC  DSTAE,SSTAE               MOVE IN THE SKELETON.              00227000
      IGTYSTAE (R5P),PARAM=(R4P),MF=(E,DSTAE) ISSUE STAE/ESTAE.         00228000
*       END;                        /* END CREATE OF RECOVERY   03S0162 00229000
*                                      ENVIRONS                      */ 00230000
*       R3P=ADDR(PLINE);            /* ADDRESS OF THE PRINT LINE.    */ 00231000
         LA    R3P,PLINE                                        02S0163 00232000
*       WPLPTR=ADDR(OUTBUF);        /* POINTER TO FIRST (ONLY) LINE. */ 00233000
         LA    WPLPTR,OUTBUF                                    02S0164 00234000
*                                                               03S0165 00235000
*       /*************************************************************/ 00236000
*       /*                                                           */ 00237000
*       /* THIS SECTION OF CODE DETERMINES IF A NEW PAGE IS REQUIRED */ 00238000
*       /* FOR THE MESSAGE LOG.                                      */ 00239000
*       /*                                                           */ 00240000
*       /*************************************************************/ 00241000
*                                                               03S0165 00242000
*       DO;                         /* PROCESS THE NEW PAGE.         */ 00243000
*         APCTLGLN=APCTLGLN+1;      /* INCREMENT THE COUNTER.        */ 00244000
         LA    @15,1                                            03S0166 00245000
         LH    @00,APCTLGLN(,APCTPTR)                           03S0166 00246000
         ALR   @00,@15                                          03S0166 00247000
         STH   @00,APCTLGLN(,APCTPTR)                           03S0166 00248000
*         IF APCTLGLN>=APCTLINE THEN/* SEE IF THE PAGE IS FULL.      */ 00249000
         CH    @00,APCTLINE(,APCTPTR)                           03S0167 00250000
         BL    @RF00167                                         03S0167 00251000
*           DO;                                                 03S0168 00252000
*             PLINE=TITLE;          /* PUT IN THE TITLE LINE.        */ 00253000
         MVC   PLINE(125),TITLE                                 03S0169 00254000
*             APCTLGPG=APCTLGPG+1;  /* INCREMENT THE PAGE NO.        */ 00255000
         LH    @00,APCTLGPG(,APCTPTR)                           03S0170 00256000
         ALR   @00,@15                                          03S0170 00257000
         STH   @00,APCTLGPG(,APCTPTR)                           03S0170 00258000
*             CVD(APCTLGPG,PACKED); /* CONVERT PAGE NUMBER TO PACKED.*/ 00259000
         CVD   @00,@TS00001                                     03S0171 00260000
         MVC   PACKED(4),@TS00001+4                             03S0171 00261000
*             UNPK(PAGENO,PACKED);  /* UNPACK THE PAGE NO.           */ 00262000
         LA    @14,PLINE                                        03S0172 00263000
         AL    @14,@CF00116                                     03S0172 00264000
         UNPK  PAGENO(5,@14),PACKED(4)                          03S0172 00265000
*             PAGENO(5)=PAGENO(5)|'F0'X;/* REMOVE THE SIGN FIELD.    */ 00266000
         OI    PAGENO+4(@14),X'F0'                              03S0173 00267000
*             DO J=1 TO 4 WHILE PAGENO(J)='0';/* REMOVE LEADING 0S.  */ 00268000
         ST    @15,J                                            03S0174 00269000
@DL00174 LA    @14,PLINE                                        03S0174 00270000
         AL    @14,@CF00116                                     03S0174 00271000
         ALR   @15,@14                                          03S0174 00272000
         BCTR  @15,0                                            03S0174 00273000
         CLI   PAGENO(@15),C'0'                                 03S0174 00274000
         BNE   @DC00174                                         03S0174 00275000
*               PAGENO(J)=' ';                                  03S0175 00276000
         L     @15,J                                            03S0175 00277000
         ALR   @14,@15                                          03S0175 00278000
         BCTR  @14,0                                            03S0175 00279000
         MVI   PAGENO(@14),C' '                                 03S0175 00280000
*             END;                                              03S0176 00281000
         AL    @15,@CF00042                                     03S0176 00282000
         ST    @15,J                                            03S0176 00283000
         C     @15,@CF00066                                     03S0176 00284000
         BNH   @DL00174                                         03S0176 00285000
@DC00174 DS    0H                                               03S0177 00286000
*             APCTLGLN=3;           /* BEGIN LINE COUNT AFRESH, 03S0177 00287000
*                                      ALLOWOING FOR THE TITLE AND      00288000
*                                      THE TWO SKIPPED LINES.        */ 00289000
         MVC   APCTLGLN(2,APCTPTR),@CH00079                     03S0177 00290000
*             GEN REFS(R2P,R3P)     /* WRITE THE TITLE LINE.         */ 00291000
*                 (PUT  (R2P),(R3P));                           03S0178 00292000
         PUT  (R2P),(R3P)                                               00293000
*             CC=' ';               /* BLANK OUT THE CONTROL CHAR.   */ 00294000
         MVI   CC,C' '                                          03S0179 00295000
*             GEN REFS(R2P,SPACE2)  /* SPACE TWO LINES.              */ 00296000
*                 (PUT  (R2P),SPACE2);                          03S0180 00297000
         PUT  (R2P),SPACE2                                              00298000
*           END;                                                03S0181 00299000
*       END;                                                    03S0182 00300000
@RF00167 DS    0H                                               03S0183 00301000
*       DO;                         /* BUILD THE HEADER AND THE FIRST   00302000
*                                      LINE OF THE WTO.              */ 00303000
*         YEARDATE='';              /* BLANK THE DATE.               */ 00304000
         MVI   YEARDATE,C' '                                    03S0184 00305000
         MVC   YEARDATE+1(4),YEARDATE                           03S0184 00306000
*         HRMINSEC='';              /* BLANK THE TIME.               */ 00307000
         MVI   HRMINSEC,C' '                                    03S0185 00308000
         MVC   HRMINSEC+1(6),HRMINSEC                           03S0185 00309000
*         RFY                                                   03S0186 00310000
*          (R0P,                                                03S0186 00311000
*           R1P,                                                03S0186 00312000
*           R15P) RSTD;                                         03S0186 00313000
*         GEN SETS(R0P,R1P,R15P)(TIME DEC,ERRET=TIMERR)         03S0187 00314000
*         FLOWS(TIMERR);                                        03S0187 00315000
         TIME DEC,ERRET=TIMERR                                          00316000
*         FULLWORD=R1P;             /* STORE THE DATE.               */ 00317000
         ST    R1P,FULLWORD                                     03S0188 00318000
*         UNPK(YEARDATE,FULLWORD);  /* CONVERT TO EBCDIC.            */ 00319000
         UNPK  YEARDATE(5),FULLWORD(4)                          03S0189 00320000
*         FULLWORD=R0P|'0000000F'X; /* REMOVE THE SIGN.              */ 00321000
         LR    @14,R0P                                          03S0190 00322000
         O     @14,@CF01841                                     03S0190 00323000
         ST    @14,FULLWORD                                     03S0190 00324000
*         UNPK(HRMINSEC,FULLWORD);  /* CONVER TO EBCDIC.             */ 00325000
         UNPK  HRMINSEC(7),FULLWORD(4)                          03S0191 00326000
*TIMERR:                                                        03S0192 00327000
*         ;                                                     03S0192 00328000
TIMERR   DS    0H                                               03S0193 00329000
*         LENETC=WPL(1:4);          /* MOVE IN THE LENGTH ETC.       */ 00330000
         MVC   LENETC(4),WPL(WPLPTR)                            03S0193 00331000
*         STUFF(1:PLEN)=WPLTXT(1:PLEN);/* MOVE IN THE MESSAGE TEXT.  */ 00332000
         LH    @14,PLEN                                         03S0194 00333000
         LR    @11,@14                                          03S0194 00334000
         BCTR  @11,0                                            03S0194 00335000
         EX    @11,@SM01930                                     03S0194 00336000
*         PLEN=PLEN+LENGTH(HDR);    /* ALLOW FOR THE HEADER.         */ 00337000
         LA    @14,18(,@14)                                     03S0195 00338000
         STH   @14,PLEN                                         03S0195 00339000
*         HDR='';                   /* CLEAR THE HEADER.             */ 00340000
         MVI   HDR,C' '                                         03S0196 00341000
         MVC   HDR+1(17),HDR                                    03S0196 00342000
*         DOT1='.';                 /* PUT IN THE DELIMITERS.        */ 00343000
         MVI   DOT1,C'.'                                        03S0197 00344000
*         DOT2='.';                                             03S0198 00345000
         MVI   DOT2,C'.'                                        03S0198 00346000
*         DOT3='.';                                             03S0199 00347000
*                                                               03S0199 00348000
         MVI   DOT3,C'.'                                        03S0199 00349000
*         /***********************************************************/ 00350000
*         /*                                                         */ 00351000
*         /* BUILD THE DATE AND TIME.                                */ 00352000
*         /*                                                         */ 00353000
*         /***********************************************************/ 00354000
*                                                               03S0200 00355000
*         YEAR=YEARDATE(1:2);                                   03S0200 00356000
         MVC   YEAR(2),YEARDATE                                 03S0200 00357000
*         DAY=YEARDATE(3:5);                                    03S0201 00358000
         MVC   DAY(3),YEARDATE+2                                03S0201 00359000
*         HH=HRMINSEC(1:2);                                     03S0202 00360000
         MVC   HH(2),HRMINSEC                                   03S0202 00361000
*         MM=HRMINSEC(3:4);                                     03S0203 00362000
         MVC   MM(2),HRMINSEC+2                                 03S0203 00363000
*         SS=HRMINSEC(5:6);                                     03S0204 00364000
         MVC   SS(2),HRMINSEC+4                                 03S0204 00365000
*       END;                                                    03S0205 00366000
*       GEN REFS(R2P,R3P)           /* WRITE FIRST LINE TO THE LOG.  */ 00367000
*           (PUT   (R2P),(R3P) );                               02S0206 00368000
         PUT   (R2P),(R3P)                                              00369000
*       IF WPLMCSFJ='1'B THEN       /* IF THIS IS A MULTI-LINE  02S0207 00370000
*                                      MESSAGE, THE REMAINDER OF THE    00371000
*                                      LINES MUST BE WRITTEN.        */ 00372000
         TM    WPLMCSFJ(WPLPTR),B'01000000'                     02S0207 00373000
         BNO   @RF00207                                         02S0207 00374000
*         DO;                                                   02S0208 00375000
*           WPLPTR=WPLPTR+WPLLGH+4; /* GET TO NUMBER OF LINES FIELD. */ 00376000
