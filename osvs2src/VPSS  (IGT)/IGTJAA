TJAA     TITLE 'IGTJAA -- VPSS SUBSYSTEM OPEN                          *00001000
                        '                                               00002000
IGTJAA   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTJAA    77.093'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
*   APCTPTR=SSCTPTR->SSCTSUSE;      /* POINT TO APCT                 */ 00014000
         LR    @06,SSCTPTR                                      01S0090 00015000
         L     APCTPTR,SSCTSUSE(,@06)                           01S0090 00016000
*   RSSOBPTR=R1P;                   /* SAVE SSOB ADDRESS             */ 00017000
         LR    RSSOBPTR,R1P                                     01S0091 00018000
*   RFY                                                         01S0092 00019000
*    (SSCTPTR,                                                  01S0092 00020000
*     R1P) UNRSTD;                  /* FINISHED WITH INPUT REGS      */ 00021000
*   RFY                                                         01S0093 00022000
*     SSOB BASED(RSSOBPTR);         /* TELL COMPILER OF REGISTER PTR */ 00023000
*   RFY                                                         01S0094 00024000
*     INFMJFCB BASED(SSDAJFCB);     /* TELL COMPILER OF REGISTER PTR */ 00025000
*   RFY                                                         01S0095 00026000
*     DEBBASIC BASED(SSDADEBP);     /* DATA EXTENT BLOCK             */ 00027000
*   RFY                                                         01S0096 00028000
*     DEB BASED(ADDR(DEBBASIC)-LENGTH(DEB));/* DEB PREFIX            */ 00029000
*   RFY                                                         01S0097 00030000
*     FORCORE BASED(RCORE);         /* JFCB IS CONTAINED IN O/C/EOV     00031000
*                                      WORKAREA. RCORE IS DUMMY PTR  */ 00032000
*   RFY                                                         01S0098 00033000
*     IFGACB BASED((ADDR(INFMJFCB)+ /* SAVE POINTER TO               */ 00034000
*         (ADDR(DXPDCBAD)-ADDR(DXJBF)))->/* COPIED (FAKE)            */ 00035000
*         I031P);                   /* ACB.                          */ 00036000
*   RFY                                                         01S0099 00037000
*     APCX BASED(ADDR(IFGACB)+      /* VPSS ACB CALL EXTENSION IS    */ 00038000
*         (LENGTH(IFGACB)+7)/8*8);  /* COPIED BEHIND ACB.            */ 00039000
*   RFY                                                         01S0100 00040000
*     APDS BASED(SSDASSCM);         /* APDS CREATED BY SS ALLOCATION */ 00041000
         EJECT                                                          00042000
*                                                               01S0101 00043000
*   /*****************************************************************/ 00044000
*   /*                                                               */ 00045000
*   /* OBTAIN A SAVEAREA/WORKAREA. CHAIN THE NEW SAVEAREA INTO THE   */ 00046000
*   /* SAVEAREA CHAIN. INITIALIZE THE DYNAMIC AREA POINTER.          */ 00047000
*   /*                                                               */ 00048000
*   /*****************************************************************/ 00049000
*                                                               01S0101 00050000
*   DO;                             /* START OF CODE                 */ 00051000
*                                                               02S0101 00052000
*     /***************************************************************/ 00053000
*     /*                                                             */ 00054000
*     /* BUILD AND INITIALIZE A SAVEAREA/WORKAREA. ISSUE A           */ 00055000
*     /* CONDITIONAL GETMAIN FOR THE REQUESTED SA/WA SIZE. IF THE    */ 00056000
*     /* STORAGE IS UNAVAILABLE, FLAG THE ACB AS UNABLE TO OPEN, AND */ 00057000
*     /* ISSUE AN UNCONDITIONAL GETMAIN FOR A SA/WA TO BE USED IN    */ 00058000
*     /* TERMINATION PROCESSING (FROM COMMON SERVICE AREA).          */ 00059000
*     /* INITIALIZE THE SAVEAREA/WORKAREA (VIA IGTUAF) AND CARVE-OUT */ 00060000
*     /* A NEW SAVEAREA AND DYNAMIC AREA. SET REGISTER 'APDATA' TO   */ 00061000
*     /* THE ADDRESS OF THE DYNAMIC AREA.                            */ 00062000
*     /*                                                             */ 00063000
*     /***************************************************************/ 00064000
*                                                               02S0102 00065000
*     RFY                           /* REQUIRED REGISTERS            */ 00066000
*      (RSP,                        /* DYNAMIC AREA SUBPOOL          */ 00067000
*       RLV,                        /* DYNAMIC AREA LENGTH           */ 00068000
*       RADDR,                      /* DYNAMIC AREA ADDRESS          */ 00069000
*       R1P,                        /* GETMAIN RETURNED AREA ADDRESS */ 00070000
*       R15F) RSTD;                 /* GETMAIN RETURN CODE           */ 00071000
*     RSP=CNORMSP;                  /* NORMAL SUBPOOL                */ 00072000
         LA    RSP,230                                          02S0103 00073000
*     RLV=CNORMLV;                  /* NORMAL LENGTH REQUIREMENT     */ 00074000
         L     RLV,@CF03362                                     02S0104 00075000
*     GEN REFS(RSP,RLV);                                        02S0105 00076000
*                                   /*                                  00077000
         GETMAIN RC,               ISSUE CONDITIONAL GETMAIN           *00078000
               LV=(RLV),           * REQUIRED LENGTH                   *00079000
               SP=(RSP),           * REQUIRED SUBPOOL                  *00080000
               BNDRY=PAGE          * START ON PAGE BOUNDARY            *00081000
                                   * R1 HAS ADDR, R15 HAS RETURN CODE   00082000
*     IF R15F^=0 THEN               /* IF STORAGE NOT OBTAINED       */ 00083000
         LTR   R15F,R15F                                        02S0106 00084000
         BZ    @RF00106                                         02S0106 00085000
*       DO;                         /* IS ERROR. OPEN TERMINATED.    */ 00086000
*         ACBERFLG=APCCS008;        /* SET ERROR INDICATOR IN ACB    */ 00087000
         L     @14,SSOBINDV(,RSSOBPTR)                          02S0108 00088000
         L     @14,SSDAJFCB(,@14)                               02S0108 00089000
         L     @14,I031P+472(,@14)                              02S0108 00090000
         MVI   ACBERFLG(@14),X'09'                              02S0108 00091000
*         RSP=CEMERSP;              /* GET STORAGE FROM CSA          */ 00092000
         LA    RSP,241                                          02S0109 00093000
*         RLV=CEMERLV;              /* FOR TERMINATION PROCESSING.   */ 00094000
         LA    RLV,2048                                         02S0110 00095000
*         GEN REFS(RSP,RLV);                                    02S0111 00096000
*                                   /*                                  00097000
         GETMAIN RU,               ISSUE UNCONDITIONAL GETMAIN         *00098000
               LV=(RLV),           * REQUIRED LENGTH                   *00099000
               SP=(RSP),           * REQUIRED SUBPOOL                  *00100000
               BNDRY=DBLWD         * NO SPECIAL ALIGNMENT REQUIRED     *00101000
                                   * R1 HAS ADDR, NO RETURN CODE        00102000
*       END;                        /* EMERGENCY STORAGE OBTAINED.   */ 00103000
*     RADDR=R1P;                    /* SAVE ADDR OF OBTAINED AREA.   */ 00104000
@RF00106 LR    RADDR,R1P                                        02S0113 00105000
*     RFY                                                       02S0114 00106000
*      (R1P,                                                    02S0114 00107000
*       R15F) UNRSTD;               /* FINISHED WITH TEMPORARY PTRS  */ 00108000
*                                                               02S0114 00109000
*     /***************************************************************/ 00110000
*     /*                                                             */ 00111000
*     /* STORAGE NOW OBTAINED. 'RADDR' CONTAINS ITS ADDRESS, 'RSP'   */ 00112000
*     /* ITS SUBPOOL, AND 'RLV' ITS LENGTH. CONSTRUCT A              */ 00113000
*     /* SAVEAREA/WORKAREA USING IGTUAF. ON INPUT TO THIS PROGRAM,   */ 00114000
*     /* REGISTER 1 MUST CONTAIN THE DYNAMIC STORAGE ADDRESS.        */ 00115000
*     /* REGISTER 0 MUST HAVE THE SUBPOOL NUMBER IN BYTE 0, AND THE  */ 00116000
*     /* LENGTH OF THE AREA IN BYTES 1-3. ON RETURN, REGISTER 1      */ 00117000
*     /* CONTAINS THE ADDRESS OF THE APSC FOR THE NEW SAVEAREA. CHAIN*/ 00118000
*     /* THE SAVEAREAS TOGETHER.                                     */ 00119000
*     /*                                                             */ 00120000
*     /***************************************************************/ 00121000
*                                                               02S0115 00122000
*     RFY                                                       02S0115 00123000
*      (R0F,                                                    02S0115 00124000
*       R1P) RSTD;                  /* NEEDED REGS                   */ 00125000
*     R0F=RSP;                      /* SUBPOOL                       */ 00126000
         LR    R0F,RSP                                          02S0116 00127000
*     SLL(R0F,24);                  /* MOVE SUBPOOL TO BYTE 0        */ 00128000
         SLL   R0F,24                                           02S0117 00129000
*     R0F=R0F|RLV;                  /* LENGTH IN BYTES 1-3.          */ 00130000
         OR    R0F,RLV                                          02S0118 00131000
*     R1P=RADDR;                    /* ADDRESS OF DYNAMIC STORAGE    */ 00132000
         LR    R1P,RADDR                                        02S0119 00133000
*     CALL APCVZUAF;                /* INITIALIZE SA-WA. ON RETURN,     00134000
*                                      R1P -> APSC.                  */ 00135000
         L     @15,APCTZUAF(,APCTPTR)                           02S0120 00136000
         BALR  @14,@15                                          02S0120 00137000
*     R0F=(APSVLN+APSCLN+@SIZDATD+7)/* ALLOW FOR APSV, APSC, AND     */ 00138000
*         &'0000FFF8'X;             /* DYNAMIC AREA, PLUS ROUND.     */ 00139000
         LA    R0F,95                                           02S0121 00140000
         AL    R0F,@SIZDATD                                     02S0121 00141000
         N     R0F,@CF03390                                     02S0121 00142000
*     R1P->APSCCRSZ=R0F;            /* SET USED SIZE IN APSC         */ 00143000
         STH   R0F,APSCCRSZ(,R1P)                               02S0122 00144000
*     R1P=R1P+APSCLN;               /* SKIP APSC. POINT TO APSV.     */ 00145000
         LA    R1P,8(,R1P)                                      02S0123 00146000
*     RFY                                                       02S0124 00147000
*       R0F UNRSTD;                 /* FINISHED WITH REG 0           */ 00148000
*                                                               02S0124 00149000
*     /***************************************************************/ 00150000
*     /*                                                             */ 00151000
*     /* BUILD SAVEAREA. POINT TO DYNAMIC AREA.                      */ 00152000
*     /*                                                             */ 00153000
*     /***************************************************************/ 00154000
*                                                               02S0125 00155000
*     APSVNEXT=R1P;                 /* FORWARD CHAIN                 */ 00156000
         ST    R1P,APSVNEXT(,APSVPTR)                           02S0125 00157000
*     R1P->APSVPREV=APSVPTR;        /* BACKWARD CHAIN                */ 00158000
         ST    APSVPTR,APSVPREV(,R1P)                           02S0126 00159000
*     APSVPTR=R1P;                  /* POINT TO NEW SAVEAREA         */ 00160000
         LR    APSVPTR,R1P                                      02S0127 00161000
*     RFY                                                       02S0128 00162000
*      (R1P) UNRSTD;                /* FINISHED WITH REG 1           */ 00163000
*     RFY                                                       02S0129 00164000
*      (APDATA) RSTD;               /* DYNAMIC AREA POINTER          */ 00165000
*     APDATA=ADDR(APSV)+APSVLN;     /* POINT TO DYNAMIC AREA (AFTER     00166000
*                                      SAVEAREA)                     */ 00167000
         LA    APDATA,80(,APSVPTR)                              02S0130 00168000
*     GEN(USING @DATD,APDATA)                                   02S0131 00169000
*     NOSETS REFS(APDATA);          /* TELL ASSEMBLER                */ 00170000
         USING @DATD,APDATA                                             00171000
*   END;                            /* SAVEAREA-WORKAREA BUILT       */ 00172000
         EJECT                                                          00173000
*                                                               01S0133 00174000
*   /*****************************************************************/ 00175000
*   /*                                                               */ 00176000
*   /* INITIALIZE OPEN/CLOSE WORKAREA (APOC)                         */ 00177000
*   /*                                                               */ 00178000
*   /*****************************************************************/ 00179000
*                                                               01S0133 00180000
*   DO;                             /* START OF CODE                 */ 00181000
*                                                               02S0133 00182000
*     /***************************************************************/ 00183000
*     /*                                                             */ 00184000
*     /* BUILD THE COMMON VPSS OPEN/CLOSE WORKAREA (APOC). THIS AREA */ 00185000
*     /* CONTAINS THE POINTERS & DATA USED BY THE SUBSYSTEM          */ 00186000
*     /* PROCESSING ROUTINES. REGISTER 'APOCPTR' IS RESERVED TO POINT*/ 00187000
*     /* TO THIS CONTROL BLOCK.                                      */ 00188000
*     /*                                                             */ 00189000
*     /***************************************************************/ 00190000
*                                                               02S0134 00191000
*     RFY                                                       02S0134 00192000
*      (APOCPTR) RSTD;              /* USE DYNAMIC STORAGE           */ 00193000
*     APOCPTR=ADDR(DAPOC);          /* FOR THE APOC.                 */ 00194000
         LA    APOCPTR,DAPOC                                    02S0135 00195000
*     RFY                                                       02S0136 00196000
*       APOC BASED(APOCPTR);        /* TELL THE COMPILER             */ 00197000
*                                                               02S0136 00198000
*     /***************************************************************/ 00199000
*     /*                                                             */ 00200000
*     /* CLEAR THE APOC                                              */ 00201000
*     /*                                                             */ 00202000
*     /***************************************************************/ 00203000
*                                                               02S0137 00204000
*     RFY                                                       02S0137 00205000
*      (R0P,                                                    02S0137 00206000
*       R1F,                                                    02S0137 00207000
*       R15F) RSTD;                 /* INITIALIZE THE                */ 00208000
*     R0P=ADDR(APOC);               /* ENTIRE APOC TO                */ 00209000
         LR    R0P,APOCPTR                                      02S0138 00210000
*     R1F=LENGTH(APOC);             /* ZEROS. ALLOW FOR              */ 00211000
         LA    R1F,72                                           02S0139 00212000
*     R15F=0;                       /* APOC EXPANSION PAST           */ 00213000
         SLR   R15F,R15F                                        02S0140 00214000
*     MVCL(R0P,R14P);               /* 256-BYTES.                    */ 00215000
         MVCL  R0P,R14P                                         02S0141 00216000
*     RFY                                                       02S0142 00217000
*      (R0P,                                                    02S0142 00218000
*       R1F,                                                    02S0142 00219000
*       R15F) UNRSTD;               /* MOVE COMPLETE                 */ 00220000
*                                                               02S0142 00221000
*     /***************************************************************/ 00222000
*     /*                                                             */ 00223000
*     /* INITIALIZE POINTERS                                         */ 00224000
*     /*                                                             */ 00225000
*     /***************************************************************/ 00226000
*                                                               02S0143 00227000
*     APOCID=APOCIDC;               /* IN-CORE ID                    */ 00228000
         MVC   APOCID(4,APOCPTR),@CC03211                       02S0143 00229000
*     APOCLNI=RLV;                  /* LENGTH OF SA-WA               */ 00230000
         ST    RLV,APOCLNI(,APOCPTR)                            02S0144 00231000
*     APOCLNIS=RSP;                 /* SUBPOOL OF SA-WA. NOTE:  02S0145 00232000
*                                      OVERLAYS FIRST BYTE OF   02S0145 00233000
*                                      APOCLNI.                      */ 00234000
         STC   RSP,APOCLNIS(,APOCPTR)                           02S0145 00235000
*     APOCADI=RADDR;                /* ADDRESS OF SA-WA STORAGE.     */ 00236000
         ST    RADDR,APOCADI(,APOCPTR)                          02S0146 00237000
*     DEBIRBB=ADDR(APOC)|           /* SAVE VPSS OPEN/CLOSE WORKAREA */ 00238000
*         '00000001'X;              /* ADDRESS FOR RECOVERY. SET BIT    00239000
*                                      31 ON TO INDICATE NOT-APDX       00240000
*                                      PTR.                          */ 00241000
         L     @11,SSOBINDV(,RSSOBPTR)                          02S0147 00242000
         L     @11,SSDADEBP(,@11)                               02S0147 00243000
         LR    @15,APOCPTR                                      02S0147 00244000
         O     @15,@CF03398                                     02S0147 00245000
         STCM  @15,7,DEBIRBB(@11)                               02S0147 00246000
*     IF APOCLNIS=CEMERSP THEN      /* IF NOT NORMAL SA-WA GETMAIN   */ 00247000
         CLI   APOCLNIS(APOCPTR),241                            02S0148 00248000
         BNE   @RF00148                                         02S0148 00249000
*       APOCSEMG=BON;               /* SHOW 'EMERGENCY' PROCESSING.  */ 00250000
         OI    APOCSEMG(APOCPTR),B'00100000'                    02S0149 00251000
*     APOCOCEW=ADDR(INFMJFCB)-      /* ADDRESS OF SYSTEM OPEN/CLOSE  */ 00252000
*         (ADDR(DXJBF)-ADDR(FORCORE));/* WORKAREA. NOTE: JFCB IN THE    00253000
*                                      WORKAREA.                     */ 00254000
@RF00148 L     @11,SSOBINDV(,RSSOBPTR)                          02S0150 00255000
         L     @01,SSDAJFCB(,@11)                               02S0150 00256000
         LR    @15,@01                                          02S0150 00257000
         SL    @15,@CF02513                                     02S0150 00258000
         ST    @15,APOCOCEW(,APOCPTR)                           02S0150 00259000
*     RFY                                                       02S0151 00260000
*      (RLV,                                                    02S0151 00261000
*       RSP,                                                    02S0151 00262000
*       RADDR) UNRSTD;              /* FINISHED WITH REGISTERS       */ 00263000
*     RFY                                                       02S0152 00264000
*       FORCORE BASED(APOCOCEW);    /* NEW O/C/EOV WORKAREA PTR      */ 00265000
*     APOCUACB=DXUDCBAD;            /* ADDRESS OF USER ACB (NOTE: ACB   00266000
*                                      POINTED TO BY DEB IS PROTECTED   00267000
*                                      COPY).                        */ 00268000
         L     @03,DXUDCBAD(,@15)                               02S0153 00269000
         ST    @03,APOCUACB(,APOCPTR)                           02S0153 00270000
*     APOCSSOB=ADDR(SSOB);          /* SAVE SSOB ADDRESS             */ 00271000
         ST    RSSOBPTR,APOCSSOB(,APOCPTR)                      02S0154 00272000
*     APOCJFCB=ADDR(INFMJFCB);      /* JFCB ADDRESS                  */ 00273000
         ST    @01,APOCJFCB(,APOCPTR)                           02S0155 00274000
*     APOCDEB=ADDR(DEBBASIC);       /* DEB ADDRESS                   */ 00275000
         L     @03,SSDADEBP(,@11)                               02S0156 00276000
         ST    @03,APOCDEB(,APOCPTR)                            02S0156 00277000
*     APOCAPDS=ADDR(APDS);          /* APDS ADDRESS                  */ 00278000
         L     @11,SSDASSCM(,@11)                               02S0157 00279000
         ST    @11,APOCAPDS(,APOCPTR)                           02S0157 00280000
*     RFY                                                       02S0158 00281000
*      (RSSOBPTR) UNRSTD;           /* FINISHED WITH SSOB REG PTR    */ 00282000
*     RFY                                                       02S0159 00283000
*       SSOB BASED(APOCSSOB);       /* TELL PL/S OF POINTERS         */ 00284000
*     RFY                                                       02S0160 00285000
*       INFMJFCB BASED(APOCJFCB);   /* CONTAINED IN APOC.            */ 00286000
*     RFY                                                       02S0161 00287000
*       DEBBASIC BASED(APOCDEB);    /* ALLOWS REG OPTIMIZATION       */ 00288000
*     RFY                                                       02S0162 00289000
*       DEB BASED(ADDR(DEBBASIC)-LENGTH(DEB));/* DIFFERENT AVT USED  */ 00290000
*     APOCACB=DXPACBAD;             /* POINT TO ACB                  */ 00291000
         L     @11,DXPACBAD-1(,@15)                             02S0163 00292000
         LA    @11,0(,@11)                                      02S0163 00293000
         ST    @11,APOCACB(,APOCPTR)                            02S0163 00294000
*     RFY                                                       02S0164 00295000
*       IFGACB BASED(APOCACB);      /* NOTE - IS ACB COPY, NOT REAL  */ 00296000
*     RFY                                                       02S0165 00297000
*       APCX BASED(ADDR(IFGACB)+    /* VPSS ACB CALL EXTENSION MUST  */ 00298000
*           (LENGTH(IFGACB)+7)/8*8);/* FOLLOW ACB                    */ 00299000
*     RFY                                                       02S0166 00300000
*       APDS BASED(APOCAPDS);       /* APDS CREATED BY ALLOCATION    */ 00301000
*     APOCDACB=DEBDCBB;             /* ADDR ACB POINTED TO BY DEB    */ 00302000
         L     @02,DEBDCBB-1(,@03)                              02S0167 00303000
         LA    @02,0(,@02)                                      02S0167 00304000
         ST    @02,APOCDACB(,APOCPTR)                           02S0167 00305000
*     APOCUKEY=DEBPROTG/16;         /* USER PROTECT KEY (BITS 4-7)   */ 00306000
         SLR   @02,@02                                          02S0168 00307000
         IC    @02,DEBPROTG(,@03)                               02S0168 00308000
         SRL   @02,4                                            02S0168 00309000
         STC   @02,APOCUKEY(,APOCPTR)                           02S0168 00310000
*     MVO(APOCUKEY,APOCUKEY);       /* ALSO SET BITS 0-3 TO USER KEY */ 00311000
         MVO   APOCUKEY(1,APOCPTR),APOCUKEY(1,APOCPTR)          02S0169 00312000
*     APOCSOPN=BON;                 /* SHOW OPEN PROCESSING          */ 00313000
         OI    APOCSOPN(APOCPTR),B'10000000'                    02S0170 00314000
*   END;                            /* APOC INITIALIZED              */ 00315000
*   IF ACBERFLG^=0 THEN             /* IF ERROR ALREADY DETECTED,    */ 00316000
         CLI   ACBERFLG(@11),0                                  01S0172 00317000
         BNE   @RT00172                                         01S0172 00318000
*     GOTO CLEANUP;                 /* TERMINATE THE OPEN            */ 00319000
*                                                               01S0173 00320000
*   /*****************************************************************/ 00321000
*   /*                                                               */ 00322000
*   /* INSURE ACB/APCX WERE COPIED TO WORKAREA                       */ 00323000
*   /*                                                               */ 00324000
*   /*****************************************************************/ 00325000
*                                                               01S0174 00326000
*   DO;                             /* START OF CODE                 */ 00327000
*                                                               02S0174 00328000
*     /***************************************************************/ 00329000
*     /*                                                             */ 00330000
*     /* INSURE A COMPLETE ACB AND APCX EXIST. IF NOT, THIS IS AN    */ 00331000
*     /* ERROR. THE FAKE VERSION OF THE ACB IS AT LEAST LONG ENOUGH  */ 00332000
*     /* TO PREVENT OVERLAYING SYSTEM FIELDS (96 BYTES). HOWEVER THE */ 00333000
*     /* LENGTH OF THE USER ACB AND APCX IS CONTAINED IN THE FIELD   */ 00334000
*     /* 'ACBLENG'.                                                  */ 00335000
*     /*                                                             */ 00336000
*     /***************************************************************/ 00337000
*                                                               02S0175 00338000
*     IF ACBLENG<(LENGTH(IFGACB)+7)/8*8/* INSURE ACB IS LONG ENOUGH  */ 00339000
*         +APCXLN THEN              /* TO CONTAIN ACB AND APCX.      */ 00340000
         LH    @15,ACBLENG(,@11)                                02S0175 00341000
         C     @15,@CF02441                                     02S0175 00342000
         BNL   @RF00175                                         02S0175 00343000
*       DO;                         /* IF NOT, IS ERROR.             */ 00344000
*         ACBERFLG=APCCS009;        /* ERROR INDICATOR               */ 00345000
         MVI   ACBERFLG(@11),X'0A'                              02S0177 00346000
*         GOTO CLEANUP;             /* TERMINATE OPEN PROCESSING     */ 00347000
         B     CLEANUP                                          02S0178 00348000
*       END;                        /* SHORT ACB PROCESSED           */ 00349000
*   END;                            /* ACB LENGTH VALIDATED          */ 00350000
@RF00175 DS    0H                                               02S0181 00351000
         EJECT                                                          00352000
*                                                               01S0181 00353000
*   /*****************************************************************/ 00354000
*   /*                                                               */ 00355000
*   /* INSURE THAT ONLY 1 OPEN ACB EXISTS PER DD STATEMENT           */ 00356000
*   /*                                                               */ 00357000
*   /*****************************************************************/ 00358000
*                                                               01S0181 00359000
*   DO;                             /* START OF CODE                 */ 00360000
*                                                               02S0181 00361000
*     /***************************************************************/ 00362000
*     /*                                                             */ 00363000
*     /* INSURE ONLY 1 OPEN ACB PER APDS. USE COMPARE-DOUBLE-AND-SWAP*/ 00364000
*     /* LOGIC AGAINST TCB/DEB ADDRESS IN APDS. IF THE APDS IS       */ 00365000
*     /* ALREADY IN USE, SET AN ERROR INDICATOR IN THE ACB AND       */ 00366000
*     /* TERMINATE THE OPEN PROCESS.                                 */ 00367000
*     /*                                                             */ 00368000
*     /***************************************************************/ 00369000
*                                                               02S0182 00370000
*     RFY                                                       02S0182 00371000
*      (R0P,                                                    02S0182 00372000
*       R1P,                                                    02S0182 00373000
*       R14P,                                                   02S0182 00374000
*       R15P) RSTD;                 /* NEED REGS                     */ 00375000
*     LM(R0P,R1P,APDSTD);           /* CURRENT TCB/DEB SAVED VALUES  */ 00376000
         L     @11,APOCAPDS(,APOCPTR)                           02S0183 00377000
         LM    R0P,R1P,APDSTD(@11)                              02S0183 00378000
*     R14P=ADDR(TCB);               /* TCB DOING OPEN                */ 00379000
