TJAD     TITLE 'IGTJAD -- PROCESS ACB-OPEN DEFAULTS                    *00001000
                        '                                               00002000
IGTJAD   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTJAD    77.092'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
*                                                               01S0075 00022000
*   /*****************************************************************/ 00023000
*   /*                                                               */ 00024000
*   /* INITIALIZE MODIFICATION MASK                                  */ 00025000
*   /*                                                               */ 00026000
*   /*****************************************************************/ 00027000
*                                                               01S0075 00028000
*   APOCMSK=APOCMSK&&APOCMSK;       /* SET MASK TO ZEROS             */ 00029000
*                                                               01S0075 00030000
         XC    APOCMSK(8,APOCPTR),APOCMSK(APOCPTR)              01S0075 00031000
*   /*****************************************************************/ 00032000
*   /*                                                               */ 00033000
*   /* PROCESS OWN= INDICATOR                                        */ 00034000
*   /*                                                               */ 00035000
*   /*****************************************************************/ 00036000
*                                                               01S0076 00037000
*   IF APCXOWN=BON&                 /* IF USER REQUESTS EXCLUSIVE    */ 00038000
*       APDSOWN=BOFF THEN           /* ALLOCATION, BUT SHARED GIVEN  */ 00039000
         L     @11,APOCACB(,APOCPTR)                            01S0076 00040000
         TM    APCXOWN+80(@11),B'10000000'                      01S0076 00041000
         BNO   @RF00076                                         01S0076 00042000
         L     @01,APOCAPDS(,APOCPTR)                           01S0076 00043000
         TM    APDSOWN(@01),B'10000000'                         01S0076 00044000
         BNZ   @RF00076                                         01S0076 00045000
*     DO;                           /* IS ERROR. FAIL THE REQUEST    */ 00046000
*       ACBERFLG=APCCS001;          /* GIVE ERROR NUMBER             */ 00047000
         MVI   ACBERFLG(@11),X'01'                              01S0078 00048000
*       GOTO EXIT;                  /* RETURN NON-ZERO CODE          */ 00049000
         B     EXIT                                             01S0079 00050000
*     END;                          /* PROCESSED DOWNGRADE REQUEST   */ 00051000
*   IF APCXOWN=BOFF&                /* IF SHARED ALLOCATION REQUESTED*/ 00052000
*       APDSOWN=BON THEN            /* BUT EXCLUSIVE GIVEN, IS OK.   */ 00053000
@RF00076 LA    @11,80                                           01S0081 00054000
         AL    @11,APOCACB(,APOCPTR)                            01S0081 00055000
         TM    APCXOWN(@11),B'10000000'                         01S0081 00056000
         BNZ   @RF00081                                         01S0081 00057000
         L     @01,APOCAPDS(,APOCPTR)                           01S0081 00058000
         TM    APDSOWN(@01),B'10000000'                         01S0081 00059000
         BNO   @RF00081                                         01S0081 00060000
*     DO;                           /* MODIFY ACB TO SHOW UPGRADE    */ 00061000
*       APCXOWN=BON;                /* SHOW EXCLUSIVE ALLOCATION     */ 00062000
         OI    APCXOWN(@11),B'10000000'                         01S0083 00063000
*       APOCMSKO=BON;               /* SHOW OWN= FIELD MODIFIED      */ 00064000
         OI    APOCMSKO(APOCPTR),B'10000000'                    01S0084 00065000
*       APOCMOWN=BOFF;              /* SAVE ORIGINAL VALUE           */ 00066000
         NI    APOCMOWN(APOCPTR),B'01111111'                    01S0085 00067000
*     END;                          /* UPGRADE PROCESSED             */ 00068000
*                                                               01S0086 00069000
*   /*****************************************************************/ 00070000
*   /*                                                               */ 00071000
*   /* PROCESS REGION= VALUE                                         */ 00072000
*   /*                                                               */ 00073000
*   /*****************************************************************/ 00074000
*                                                               01S0087 00075000
*   IF APDSREG<APCXREG THEN         /* IF ALLOCATED LESS THAN        */ 00076000
@RF00081 L     @11,APOCAPDS(,APOCPTR)                           01S0087 00077000
         L     @15,APOCACB(,APOCPTR)                            01S0087 00078000
         L     @11,APDSREG(,@11)                                01S0087 00079000
         C     @11,APCXREG+80(,@15)                             01S0087 00080000
         BNL   @RF00087                                         01S0087 00081000
*     DO;                           /* REQUIRED, IS ERROR            */ 00082000
*       ACBERFLG=APCCS002;          /* FLAG CONDITION & EXIT         */ 00083000
         MVI   ACBERFLG(@15),X'02'                              01S0089 00084000
*       GOTO EXIT;                  /* OPEN WILL BE TERMINATED       */ 00085000
         B     EXIT                                             01S0090 00086000
*     END;                          /* DOWNGRADE PROCESSED           */ 00087000
*   IF APDSREG>APCXREG THEN         /* IF MORE THAN REQUESTED        */ 00088000
@RF00087 L     @11,APOCAPDS(,APOCPTR)                           01S0092 00089000
         L     @11,APDSREG(,@11)                                01S0092 00090000
         LA    @15,80                                           01S0092 00091000
         AL    @15,APOCACB(,APOCPTR)                            01S0092 00092000
         L     @14,APCXREG(,@15)                                01S0092 00093000
         CR    @11,@14                                          01S0092 00094000
         BNH   @RF00092                                         01S0092 00095000
*     DO;                           /* ALLOCATED, IS OK.             */ 00096000
*       APOCMSKR=BON;               /* SHOW APCX MODIFIED            */ 00097000
         OI    APOCMSKR(APOCPTR),B'01000000'                    01S0094 00098000
*       APOCMREG=APCXREG;           /* SAVE ORIGINAL APCXREG VALUE   */ 00099000
         ST    @14,APOCMREG(,APOCPTR)                           01S0095 00100000
*       APCXREG=APDSREG;            /* SHOW ACTUAL REGION ALLOCATED  */ 00101000
         ST    @11,APCXREG(,@15)                                01S0096 00102000
*     END;                          /* UPGRADE PROCESSED             */ 00103000
*                                                               01S0097 00104000
*   /*****************************************************************/ 00105000
*   /*                                                               */ 00106000
*   /* PROCESS ACBSTRNO VALUE                                        */ 00107000
*   /*                                                               */ 00108000
*   /*****************************************************************/ 00109000
*                                                               01S0098 00110000
*   IF ACBSTRNO=0 THEN              /* IF NO STRINGS REQUESTED       */ 00111000
@RF00092 L     @11,APOCACB(,APOCPTR)                            01S0098 00112000
         CLI   ACBSTRNO(@11),0                                  01S0098 00113000
         BNE   @RF00098                                         01S0098 00114000
*     DO;                           /* USE THE APCT DEFAULT          */ 00115000
*       APOCMSKS=BON;               /* SHOW ACB FIELD MODIFIED       */ 00116000
         OI    APOCMSKS(APOCPTR),B'00100000'                    01S0100 00117000
*       ACBSTRNO=APCTSTDF;          /* SET ACB TO DEFAULT VALUE      */ 00118000
         L     @15,APCTSTDF(,APCTPTR)                           01S0101 00119000
         STC   @15,ACBSTRNO(,@11)                               01S0101 00120000
*     END;                          /* UPGRADE PROCESSED             */ 00121000
*   ELSE                            /* IF STRINGS REQUESTED,         */ 00122000
*     IF ACBSTRNO>APCTSTMX THEN     /* INSURE TOO MANY NOT REQUESTED */ 00123000
         B     @RC00098                                         01S0103 00124000
@RF00098 L     @11,APOCACB(,APOCPTR)                            01S0103 00125000
         SLR   @15,@15                                          01S0103 00126000
         IC    @15,ACBSTRNO(,@11)                               01S0103 00127000
         C     @15,APCTSTMX(,APCTPTR)                           01S0103 00128000
         BNH   @RF00103                                         01S0103 00129000
*       DO;                         /* IF EXCESSIVE NUMBER,          */ 00130000
*         ACBERFLG=APCCS003;        /* ERROR FLAG IN ACB             */ 00131000
         MVI   ACBERFLG(@11),X'03'                              01S0105 00132000
*         GOTO EXIT;                /* RETURN ERROR CODE             */ 00133000
         B     EXIT                                             01S0106 00134000
*       END;                        /* DOWN GRADE PROCESSED          */ 00135000
         EJECT                                                          00136000
*                                                               01S0108 00137000
*   /*****************************************************************/ 00138000
*   /*                                                               */ 00139000
*   /* LOAD REQUESTED LERR AND/OR PERR MODULES REQUESTED IN THE APDS */ 00140000
*   /* (IF ANY).                                                     */ 00141000
*   /*                                                               */ 00142000
*   /*****************************************************************/ 00143000
*                                                               01S0108 00144000
*   DO;                                                         02S0108 00145000
*                                                               02S0108 00146000
@RF00103 DS    0H                                               02S0108 00147000
@RC00098 DS    0H                                               02S0109 00148000
*     /***************************************************************/ 00149000
*     /*                                                             */ 00150000
*     /* IF THE APDS SPECIFIES A LOGICAL (LERR) AND/OR PHYSICAL      */ 00151000
*     /* (PERR) ERROR EXIT, LOAD THEM. THE LOAD PROCESS REQUIRES TWO */ 00152000
*     /* STEPS: (1) DETERMINE IF A EXIT ROUTINE IS REQUIRED & UPDATE */ 00153000
*     /* THE ACB TO INDICATE ANY ERROR CONDITIONS, (2) THE ACTUAL    */ 00154000
*     /* LOADING OF THE MODULE AND THE UPDATING OF THE EXLST OCCURS  */ 00155000
*     /* UNDER A PRB.                                                */ 00156000
*     /*                                                             */ 00157000
*     /***************************************************************/ 00158000
*                                                               02S0109 00159000
*     IF(APDSVLER=BON|              /* PERFORM LOADING ONLY IF       */ 00160000
*         APDSVPER=BON)&            /* EXIT RTN AND ACB              */ 00161000
*         ACBEXLST>1 THEN           /* EXLST EXIST.                  */ 00162000
         L     @11,APOCAPDS(,APOCPTR)                           02S0109 00163000
         TM    APDSVLER(@11),B'00110000'                        02S0109 00164000
         BZ    @RF00109                                         02S0109 00165000
         L     @01,APOCACB(,APOCPTR)                            02S0109 00166000
         CLC   ACBEXLST(4,@01),@CF00056                         02S0109 00167000
         BNH   @RF00109                                         02S0109 00168000
*       DO;                         /* IF NOT, IGNORE THE LOADING.   */ 00169000
*                                                               02S0110 00170000
*         /***********************************************************/ 00171000
*         /*                                                         */ 00172000
*         /* SINCE EXECUTING IN DATA MANAGEMENT KEY, CANNOT REFERENCE*/ 00173000
*         /* OR UPDATE THE EXLST. HOWEVER THE ACB COPY IS IN         */ 00174000
*         /* PROTECTED STORAGE. PASS THE SYNCH ROUTINE THE NAME OF   */ 00175000
*         /* THE MODULE TO LOAD IN REGISTER 1, AND THE OFFSET INTO   */ 00176000
*         /* THE EXLST OF THE REQUIRED ENTRY IN REGISTER 0. ONLY     */ 00177000
*         /* REGISTERS APDATA, APCODE, AND APSVPTR ARE PROTECTED     */ 00178000
*         /* ACROSS THE SYNCH. ON EXIT REGISTER 15 CONTAINS A        */ 00179000
*         /* INDICATOR WHETHER THE MODULE WAS LOADED: 0--MODULE      */ 00180000
*         /* LOADED, 4--EXLST TOO SHORT, 8--UNABLE TO LOAD THE       */ 00181000
*         /* MODULE. IF THE ACB WAS BUILD IN PROTECTED STORAGE (KEY  */ 00182000
*         /* 0-7), CALL THE SYNCH ROUTINE SINCE THE CALLER MUST BE   */ 00183000
*         /* AUTHORIZED.                                             */ 00184000
*         /*                                                         */ 00185000
*         /***********************************************************/ 00186000
*                                                               02S0111 00187000
*         STM(R2F,R11F,SYNCHREG);   /* SAVE REGS ACROSS SYNCH        */ 00188000
         STM   R2F,R11F,SYNCHREG                                02S0111 00189000
*         RFY                                                   02S0112 00190000
*          (R0P,                                                02S0112 00191000
*           R1P,                                                02S0112 00192000
*           R14F,                                               02S0112 00193000
*           R15F) RSTD;             /* USED REGISTERS                */ 00194000
*         IF APDSVLER=BON THEN      /* IF LOGICAL ERROR EXIT         */ 00195000
         TM    APDSVLER(@11),B'00100000'                        02S0113 00196000
         BNO   @RF00113                                         02S0113 00197000
*           DO;                     /* ROUTINE WANTED --             */ 00198000
*             R1P=ADDR(EXLLERF)-ADDR(IFGEXLST);/* EXLST OFFSET       */ 00199000
         LA    R1P,15                                           02S0115 00200000
*             R0P=ADDR(APDSLERR);   /* @ MODULE NAME TO LOAD         */ 00201000
         LA    R0P,APDSLERR(,@11)                               02S0116 00202000
*             IF APOCUKEY>='80'X THEN/* IF USER PROTECT KEY (8-15),  */ 00203000
         CLI   APOCUKEY(APOCPTR),X'80'                          02S0117 00204000
         BL    @RF00117                                         02S0117 00205000
*               GEN(SYNCH SYNCHRTN)                             02S0118 00206000
*               REFS(SYNCHRTN);     /* LOAD UNDER SYNCH PRB          */ 00207000
         SYNCH SYNCHRTN                                                 00208000
*             ELSE                  /* ELSE, IF AUTHORIZED KEY (0-7),*/ 00209000
*               DO;                 /* THEN LOAD WHILE IN SUPV STATE */ 00210000
         B     @RC00117                                         02S0119 00211000
@RF00117 DS    0H                                               02S0120 00212000
*                 SPKA(APOCUKEY);   /* SWITCH TO ACB/EXLST KEY       */ 00213000
         SLR   @11,@11                                          02S0120 00214000
         IC    @11,APOCUKEY(,APOCPTR)                           02S0120 00215000
         SPKA  0(@11)                                           02S0120 00216000
*                 CALL SYNCHRTN;    /* LOAD THE MODULE               */ 00217000
         BAL   @14,SYNCHRTN                                     02S0121 00218000
*                 GEN(MODESET EXTKEY=DATAMGT)                   02S0122 00219000
*                 NOREFS NOSETS;    /* RESUME DATA MANAGEMENT KEY    */ 00220000
         MODESET EXTKEY=DATAMGT                                         00221000
*               END;                /* MODULE NOW LOADED             */ 00222000
*             LM(R2F,R11F,SYNCHREG);/* RELOAD DESTROYED REGISTERS    */ 00223000
@RC00117 LM    R2F,R11F,SYNCHREG                                02S0124 00224000
*             IF R15F=0 THEN        /* IF MODULE LOADED,             */ 00225000
         LTR   R15F,R15F                                        02S0125 00226000
         BNZ   @RF00125                                         02S0125 00227000
*               APOCMLER=BON;       /* UPDATE CLOSE/RECOVERY MASK    */ 00228000
         OI    APOCMLER(APOCPTR),B'00000001'                    02S0126 00229000
*             ELSE                  /* IF MODULE NOT LOADED          */ 00230000
*               IF R15F=8 THEN      /* BECAUSE MODULE MISSING, THEN  */ 00231000
         B     @RC00125                                         02S0127 00232000
@RF00125 C     R15F,@CF00040                                    02S0127 00233000
         BNE   @RF00127                                         02S0127 00234000
*                 DO;               /* FLAG ACB & TERMINATE OPEN.    */ 00235000
*                   ACBERFLG=APCCS004;/* IF NO EXLST ENTRY, IS OK,   */ 00236000
         L     @11,APOCACB(,APOCPTR)                            02S0129 00237000
         MVI   ACBERFLG(@11),X'05'                              02S0129 00238000
*                   GOTO EXIT;      /* SO IGNORE REQUEST             */ 00239000
         B     EXIT                                             02S0130 00240000
*                 END;                                          02S0131 00241000
*           END;                    /* LERR ROUTINE PROCESSED        */ 00242000
@RF00127 DS    0H                                               02S0132 00243000
@RC00125 DS    0H                                               02S0133 00244000
*         IF APDSVPER=BON THEN      /* IF PHYSICAL ERROR EXIT        */ 00245000
@RF00113 L     @11,APOCAPDS(,APOCPTR)                           02S0133 00246000
         TM    APDSVPER(@11),B'00010000'                        02S0133 00247000
         BNO   @RF00133                                         02S0133 00248000
*           DO;                     /* ROUTINE WANTED --             */ 00249000
*             R1P=ADDR(EXLSYNF)-ADDR(IFGEXLST);/* EXLST OFFSET       */ 00250000
         LA    R1P,10                                           02S0135 00251000
*             R0P=ADDR(APDSPERR);   /* ADDR MODULE NAME TO LOAD      */ 00252000
         LA    R0P,APDSPERR(,@11)                               02S0136 00253000
*             IF APOCUKEY>='80'X THEN/* IF USER PROTECT KEY (8-15),  */ 00254000
         CLI   APOCUKEY(APOCPTR),X'80'                          02S0137 00255000
         BL    @RF00137                                         02S0137 00256000
*               GEN(SYNCH SYNCHRTN)                             02S0138 00257000
*               REFS(SYNCHRTN);     /* LOAD UNDER SYNCH PRB          */ 00258000
         SYNCH SYNCHRTN                                                 00259000
*             ELSE                  /* ELSE, IF AUTHORIZED KEY (0-7),*/ 00260000
*               DO;                 /* THEN LOAD WHILE IN SUPV STATE */ 00261000
         B     @RC00137                                         02S0139 00262000
@RF00137 DS    0H                                               02S0140 00263000
*                 SPKA(APOCUKEY);   /* SWITCH TO ACB/EXLST KEY       */ 00264000
         SLR   @11,@11                                          02S0140 00265000
         IC    @11,APOCUKEY(,APOCPTR)                           02S0140 00266000
         SPKA  0(@11)                                           02S0140 00267000
*                 CALL SYNCHRTN;    /* LOAD THE MODULE               */ 00268000
         BAL   @14,SYNCHRTN                                     02S0141 00269000
*                 GEN(MODESET EXTKEY=DATAMGT)                   02S0142 00270000
*                 NOREFS NOSETS;    /* RESUME DATA MANAGEMENT KEY    */ 00271000
         MODESET EXTKEY=DATAMGT                                         00272000
*               END;                /* MODULE NOW LOADED             */ 00273000
*             LM(R2F,R11F,SYNCHREG);/* RELOAD DESTROYED REGISTERS    */ 00274000
@RC00137 LM    R2F,R11F,SYNCHREG                                02S0144 00275000
*             IF R15F=0 THEN        /* IF MODULE LOADED              */ 00276000
         LTR   R15F,R15F                                        02S0145 00277000
         BNZ   @RF00145                                         02S0145 00278000
*               APOCMPER=BON;       /* UPDATE CLOSE/RECOVERY MASK    */ 00279000
         OI    APOCMPER(APOCPTR),B'00000010'                    02S0146 00280000
*             ELSE                  /* IF MODULE NOT LOADED          */ 00281000
*               IF R15F=8 THEN      /* BECAUSE MODULE MISSING, THEN  */ 00282000
         B     @RC00145                                         02S0147 00283000
@RF00145 C     R15F,@CF00040                                    02S0147 00284000
         BNE   @RF00147                                         02S0147 00285000
*                 DO;               /* FLAG ACB & TERMINATE OPEN.    */ 00286000
*                   ACBERFLG=APCCS005;/* IF NO EXLST ENTRY, IS OK,   */ 00287000
         L     @11,APOCACB(,APOCPTR)                            02S0149 00288000
         MVI   ACBERFLG(@11),X'06'                              02S0149 00289000
*                   GOTO EXIT;      /* SO IGNORE REQUEST.            */ 00290000
         B     EXIT                                             02S0150 00291000
*                 END;                                          02S0151 00292000
*           END;                    /* PERR ROUTINE PROCESSED        */ 00293000
@RF00147 DS    0H                                               02S0152 00294000
@RC00145 DS    0H                                               02S0153 00295000
*         RFY                                                   02S0153 00296000
*          (R0P,                                                02S0153 00297000
*           R1P,                                                02S0153 00298000
*           R14F,                                               02S0153 00299000
*           R15F) UNRSTD;           /* FINISHED WITH REGS            */ 00300000
@RF00133 DS    0H                                               02S0154 00301000
*       END;                        /* LOADING COMPLETE              */ 00302000
*   END;                            /* END OF CODE                   */ 00303000
         EJECT                                                          00304000
*SYNCHRTN:                          /* NOW UNDER PRB                 */ 00305000
*   PROC OPTIONS(NOSAVE,            /* R13->PROTECTED STORAGE        */ 00306000
*       NOSAVEAREA);                /* PROTECTED REGS ARE 6,7,12,13  */ 00307000
*                                                               02S0156 00308000
         B     @PB00002                                         02S0156 00309000
SYNCHRTN DS    0H                                               02S0157 00310000
*   /*****************************************************************/ 00311000
*   /*                                                               */ 00312000
*   /* DO THE MODULE LOADING UNDER A PRB. THIS SOLVES SEVERAL        */ 00313000
*   /* INTEGRITY PROBLEMS: (1) REFERENCES/STORES OF EXLST (USER      */ 00314000
*   /* PROTECT KEY) OCCUR WITH THE PSW HAVING THE USER PROTECT KEY.  */ 00315000
*   /* (2) CONTENTS SUPERVISION PERFORMS AUTHORIZATION CHECKS ONLY IF*/ 00316000
*   /* THE LOAD SVC WAS ISSUED IN PROBLEM PROGRAM STATE. SINCE       */ 00317000
*   /* EXECUTING IN THE USER'S KEY, CANNOT UPDATE THE ACB (FAKE      */ 00318000
*   /* COPY), THE APOC, OR THE APDS, AND CANNOT SAVE REGISTERS IN THE*/ 00319000
*   /* R13 SAVEAREA. THEREFORE REGISTERS ARE SAVED PRIOR TO THE SYNCH*/ 00320000
*   /* AND RESTORED ON RETURN. SINCE ACBERFLG CANNOT BE UPDATED,     */ 00321000
*   /* REGISTER 15 IS SET TO INDICATE THE STATUS OF THE LOAD:        */ 00322000
*   /* 0--LOADED SUCCESSFULLY, 4--EXLST TOO SHORT, 8--LOAD SVC       */ 00323000
*   /* FAILED. ON ENTRY, REGISTER 1 CONTAINS THE OFFSET INTO THE     */ 00324000
*   /* EXLST OF THE DESIRED EXIT, AND REGISTER 0 CONTAINS THE ADDRESS*/ 00325000
*   /* OF THE MODULE NAME TO LOAD.                                   */ 00326000
*   /*                                                               */ 00327000
*   /*****************************************************************/ 00328000
*                                                               02S0157 00329000
*                                                               02S0157 00330000
*   /*****************************************************************/ 00331000
*   /*                                                               */ 00332000
*   /* SYNCHRTN DECLARES AND REGISTER USAGE                          */ 00333000
*   /*                                                               */ 00334000
*   /*****************************************************************/ 00335000
*                                                               02S0157 00336000
*   DCL                                                         02S0157 00337000
*     RXRETURN PTR(31) REG(03),     /* SAVE RETURN ADDRESS           */ 00338000
*     RXEXLST PTR(31) REG(02);      /* EXLST POINTER                 */ 00339000
*   RFY                             /* REQUIRED REGISTERS --         */ 00340000
*    (R0P,                          /* ADDR MODULE NAME TO LOAD      */ 00341000
*     R1P,                          /* OFFSET INTO EXLST OF ENTRY    */ 00342000
*     R15F,                         /* RETURN CODE FROM SYNCH        */ 00343000
*     R14P,                         /* INPUT RETURN ADDRESS          */ 00344000
*     RXRETURN,                     /* HOLDS RETURN ADDRESS          */ 00345000
*     RXEXLST) RSTD;                /* EXLST POINTER                 */ 00346000
*   RXRETURN=R14P;                  /* SAVE RETURN ADDRESS           */ 00347000
         LR    RXRETURN,R14P                                    02S0159 00348000
*   RFY                                                         02S0160 00349000
*     R14P UNRSTD;                  /* RETURN REGISTER               */ 00350000
*   RXEXLST=ACBEXLST;               /* POINT TO EXLST                */ 00351000
         L     @14,APOCACB(,APOCPTR)                            02S0161 00352000
         L     RXEXLST,ACBEXLST(,@14)                           02S0161 00353000
*   RFY                                                         02S0162 00354000
*     IFGEXLST BASED(RXEXLST);      /* TELL COMPILER                 */ 00355000
*   IF R1P>=EXLLEN THEN             /* IF EXIT NOT INCLUDED IN       */ 00356000
         CH    R1P,EXLLEN(,RXEXLST)                             02S0163 00357000
         BL    @RF00163                                         02S0163 00358000
*     R15F=4;                       /* EXLST, SET RETURN CODE        */ 00359000
         LA    R15F,4                                           02S0164 00360000
*   ELSE                            /* IF EXLST ENTRY EXISTS,        */ 00361000
*     DO;                           /* THEN LOAD THE MODULE          */ 00362000
         B     @RC00163                                         02S0165 00363000
@RF00163 DS    0H                                               02S0166 00364000
*       RXEXLST=ADDR(IFGEXLST)+R1P; /* POINT TO EXLST ENTRY          */ 00365000
         ALR   RXEXLST,R1P                                      02S0166 00366000
*       RFY                                                     02S0167 00367000
*         IFGEXLST;                 /* NOT POINTING AT EXLST HEADER  */ 00368000
*       RFY                                                     02S0168 00369000
*         IFGEXLEF BASED(RXEXLST);  /* EXLST ENTRY OVERLAY           */ 00370000
*       GEN FLOWS(SYNCHERR) SEQFLOW NOEXIT REFS(R0P);           02S0169 00371000
*                                   /*                                  00372000
