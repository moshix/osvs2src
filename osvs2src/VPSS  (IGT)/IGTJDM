*                                      THE SUBROUTINES               */ 00786000
         USING @DATD,APDATA                                             00787000
*   RETURN CODE(0);                 /* RETURN TO THE CALLER          */ 00788000
         SLR   @15,@15                                          01S0260 00789000
         L     @14,12(,@13)                                     01S0260 00790000
         LM    @00,@12,20(@13)                                  01S0260 00791000
         BR    @14                                              01S0260 00792000
         EJECT                                                          00793000
*FIXAPIO:                           /* REPAIR THE APIO CHAIN         */ 00794000
*   PROC;                           /* ENTERED IN KEY-0              */ 00795000
*                                                               01S0261 00796000
FIXAPIO  STM   @14,@12,@SA00002                                 01S0261 00797000
*   /*****************************************************************/ 00798000
*   /*                                                               */ 00799000
*   /* ENTERED WHEN AN ERROR OCCURS SCANNING OR UPDATING THE APIO    */ 00800000
*   /* CHAIN. THIS ROUTINE WILL ATTEMPT TO REPAIR THE CHAIN. IF      */ 00801000
*   /* REPAIR IS IMPOSSIBLE, THE QUEUE HEADERS AND RESET TO ZERO,    */ 00802000
*   /* THUS ELIMINATING THE ENTIRE APIO QUEUE. CONTROL THEN RETURNS  */ 00803000
*   /* TO CALL QUEUE USER (RETURN ADDRESS CONTAINED IN REGISTER 14). */ 00804000
*   /* CONTROL MUST BE RETURNED IN KEY-0, WITH THE RECURSION FLAG    */ 00805000
*   /* (APESPREV) SET ON. THIS PREVENTS RECURSION LOOPING IF THE     */ 00806000
*   /* CALLER HAS A PROBLEM (RATHER THAN AN INVALID APIO QUEUE). ON  */ 00807000
*   /* ENTRY, THE LOCAL AND CMS LOCKS MUST BE OWNED (THIS IS A       */ 00808000
*   /* REQUIREMENT FOR TASKS UPDATING THE APIO QUEUE).               */ 00809000
*   /*                                                               */ 00810000
*   /*****************************************************************/ 00811000
*                                                               01S0262 00812000
*                                                               01S0262 00813000
*   /*****************************************************************/ 00814000
*   /*                                                               */ 00815000
*   /* SAVE CURRENT RECOVERY ENVIRONMENT. SET RETRY FOR 'FIXAPIO1',  */ 00816000
*   /* WHICH ELIMINATES THE APIO CHAIN.                              */ 00817000
*   /*                                                               */ 00818000
*   /*****************************************************************/ 00819000
*                                                               01S0262 00820000
*   DO;                             /* ENTRY FROM ESTAE/FRR RETRY    */ 00821000
*                                                               01S0262 00822000
*     /***************************************************************/ 00823000
*     /*                                                             */ 00824000
*     /* ENTRY FROM ESTAE/FRR RETRY (IGTUKA). RETRY DOES NOT INSURE  */ 00825000
*     /* REGISTERS 0-5, 8-11, 14-15 VALID SINCE NOT SAVED IN         */ 00826000
*     /* APESREGS. ISSUE DUMMY RESTRICT TO WIPE PL/S OPTIMIZATION    */ 00827000
*     /* MEMORY OF CURRENT REGISTER VALUES                           */ 00828000
*     /*                                                             */ 00829000
*     /***************************************************************/ 00830000
*                                                               01S0263 00831000
*     RFY                                                       01S0263 00832000
*      (R0RSTD,                                                 01S0263 00833000
*       R1RSTD,                                                 01S0263 00834000
*       R2RSTD,                                                 01S0263 00835000
*       R3RSTD,                                                 01S0263 00836000
*       R4RSTD,                                                 01S0263 00837000
*       R5RSTD,                     /* 6-7 BASES                     */ 00838000
*       R8RSTD,                                                 01S0263 00839000
*       R9RSTD,                                                 01S0263 00840000
*       R10RSTD,                                                01S0263 00841000
*       R11RSTD,                    /* 12-13 APCT/APSV PTRS          */ 00842000
*       R14RSTD,                                                01S0263 00843000
*       R15RSTD) RSTD;              /* KEEP PL/S FROM CARRYING DATA  */ 00844000
*     GEN(SPACE ,);                 /* NULL CODE                     */ 00845000
         SPACE ,                                                        00846000
*     RFY                                                       01S0265 00847000
*      (R0RSTD,                                                 01S0265 00848000
*       R1RSTD,                                                 01S0265 00849000
*       R2RSTD,                                                 01S0265 00850000
*       R3RSTD,                                                 01S0265 00851000
*       R4RSTD,                                                 01S0265 00852000
*       R5RSTD,                     /* 6-7 BASES                     */ 00853000
*       R8RSTD,                                                 01S0265 00854000
*       R9RSTD,                                                 01S0265 00855000
*       R10RSTD,                                                01S0265 00856000
*       R11RSTD,                    /* 12-13 APCT/APSV PTRS          */ 00857000
*       R14RSTD,                                                01S0265 00858000
*       R15RSTD) UNRSTD;            /* ALLOW PL/S TO USE REGS AGAIN  */ 00859000
*   END;                            /* RETRY ENVIRONMENT SET         */ 00860000
*   DRETRY(03)=APESRTRY;            /* SAVE CURRENT RETRY POINT      */ 00861000
         L     @08,APESREGP                                     01S0267 00862000
         L     @15,APESRTRY(,@08)                               01S0267 00863000
         ST    @15,DRETRY+8                                     01S0267 00864000
*   APESRTRY=ADDR(FIXAPIO1);        /* NEW RETRY -- ELIMINATE APIO Q */ 00865000
         LA    @15,FIXAPIO1                                     01S0268 00866000
         ST    @15,APESRTRY(,@08)                               01S0268 00867000
*   GEN NOREFS NOSETS SEQFLOW FLOWS(FIXAPIO1);                  01S0269 00868000
*                                   /* NO CODE -- SHOW FIXAPIO1 USED    00869000
*   DO;                             /* ERROR RECOVERY COMPLETE       */ 00870000
*     APESFPRV=BOFF;                /* RESET RECURSION FLAG.         */ 00871000
         NI    APESFPRV,B'10111111'                             01S0271 00872000
*     APESCODE=CERRN99;             /* RESET ERROR CODE              */ 00873000
         MVI   APESCODE,X'03'                                   01S0272 00874000
*     APESFREC=BON;                 /* RESET DUMP/LOG FLAGS          */ 00875000
*     APESFSDP=BON;                 /* TO THEIR NORMAL VALUES        */ 00876000
         OI    APESFREC,B'01100000'                             01S0274 00877000
*   END;                            /* ALL INDICATORS RESET          */ 00878000
*                                                               01S0275 00879000
*   /*****************************************************************/ 00880000
*   /*                                                               */ 00881000
*   /* BUILD Q-VERIFICATION BLOCK                                    */ 00882000
*   /*                                                               */ 00883000
*   /*****************************************************************/ 00884000
*                                                               01S0276 00885000
*   RFY                                                         01S0276 00886000
*     APIO BASED(R0P);              /* R0P NOT VALID. ONLY APIO 01S0276 00887000
*                                      OFFSETS REFERENCED (PL/S REQ) */ 00888000
*   QVPL=QVPL&&QVPL;                /* RESET ALL FIELDS              */ 00889000
         XC    QVPL(40),QVPL                                    01S0277 00890000
*   QVPLEVR=ADDR(FIXAPIOV);         /* APIO VERIFICATION ROUTINE     */ 00891000
         LA    @08,FIXAPIOV                                     01S0278 00892000
         ST    @08,QVPLEVR                                      01S0278 00893000
*   QVPLWKA=ADDR(DFXAPIOW);         /* Q-VERIFICATION WORKAREA       */ 00894000
         LA    @08,DFXAPIOW                                     01S0279 00895000
         ST    @08,QVPLWKA                                      01S0279 00896000
*   QVPLHDR=ADDR(APCTAPIO);         /* Q-HEADER (FORWARD)            */ 00897000
         LA    @08,APCTAPIO(,APCTPTR)                           01S0280 00898000
         ST    @08,QVPLHDR                                      01S0280 00899000
*   QVPLFPTR=ADDR(APIOAPIO)-ADDR(APIO);/* FORWARD POINTER OFFSET     */ 00900000
         MVC   QVPLFPTR(2),@CH03760                             01S0281 00901000
*   QVPLTRLR=ADDR(APCTIOAP);        /* Q-HEADER (BACKWARD)           */ 00902000
         LA    @08,APCTIOAP(,APCTPTR)                           01S0282 00903000
         ST    @08,QVPLTRLR                                     01S0282 00904000
*   QVPLBPTR=ADDR(APIOIOAP)-ADDR(APIO);/* BACK CHAIN OFFSET          */ 00905000
         MVC   QVPLBPTR(2),@CH03761                             01S0283 00906000
*   RFY                                                         01S0284 00907000
*     APIO;                         /* NO APIO POINTER               */ 00908000
*                                                               01S0284 00909000
*   /*****************************************************************/ 00910000
*   /*                                                               */ 00911000
*   /* CALL IEAVEQV3 -- DOUBLE THREADED CHAIN VERIFICATION. APCT     */ 00912000
*   /* (KEY-1) AND APIO (KEY-5) UPDATE REQUIRES IEAVEQV3 TO EXECUTE  */ 00913000
*   /* IN KEY-0.                                                     */ 00914000
*   /*                                                               */ 00915000
*   /*****************************************************************/ 00916000
*                                                               01S0285 00917000
*   RFY                                                         01S0285 00918000
*     R1P RSTD;                     /* INPUT REGISTER                */ 00919000
*   R1P=ADDR(QVPL);                 /* IEAVEQV3 PARM AREA            */ 00920000
         LA    R1P,QVPL                                         01S0286 00921000
*   CALL IEAVEQV3;                  /* VERIFY QUEUE                  */ 00922000
         L     @08,CVTPTR                                       01S0287 00923000
         L     @15,CVTQV3(,@08)                                 01S0287 00924000
         BALR  @14,@15                                          01S0287 00925000
*   GEN NOREFS NOSETS FLOWS(FIXAPIOV) SEQFLOW;                  01S0288 00926000
*                                   /* NO CODE - SHOW FIXAPIOV USED     00927000
*   RFY                                                         01S0289 00928000
*     R1P UNRSTD;                   /* FINISHED WITH PARMS           */ 00929000
*   APESFPRV=BON;                   /* RESET ERROR STATUS            */ 00930000
         OI    APESFPRV,B'01000000'                             01S0290 00931000
*   APESRTRY=DRETRY(03);            /* RESET RECOVERY ADDRESS        */ 00932000
         L     @08,DRETRY+8                                     01S0291 00933000
         L     @01,APESREGP                                     01S0291 00934000
         ST    @08,APESRTRY(,@01)                               01S0291 00935000
*   RETURN;                         /* RETURN TO RESCAN APIO QUEUE   */ 00936000
@EL00002 DS    0H                                               01S0292 00937000
@EF00002 DS    0H                                               01S0292 00938000
@ER00002 LM    @14,@12,@SA00002                                 01S0292 00939000
         BR    @14                                              01S0292 00940000
*FIXAPIOV:                          /* APIO VERIFICATION EXIT        */ 00941000
*   PROC OPTIONS(NOSAVE,            /* R13 SAVEAREA NOT AVAILABLE    */ 00942000
*       NOSAVEAREA);                /* NO DYNAMIC AREA POINTER       */ 00943000
*                                                               01S0293 00944000
FIXAPIOV DS    0H                                               01S0294 00945000
*   /*****************************************************************/ 00946000
*   /*                                                               */ 00947000
*   /* FOLLOWING INTERNAL PROCEDURE GIVEN CONTROL BY IEAVEQV3 TO     */ 00948000
*   /* VERIFY THAT THE CONTROL BLOCK POINTED TO BY REGISTER 0 IS AN  */ 00949000
*   /* APIO. THIS ROUTINE MUST INSURE THAT A PROGRAM CHECK DOES NOT  */ 00950000
*   /* OCCUR. ON EXIT REGISTER 15 INDICATE IF THE BLOCK IS VALID     */ 00951000
*   /* (0=YES, 8=NO). REGISTERS 8-14 MUST BE RETURNED UNCHANGED.     */ 00952000
*   /* OTHERS ARE VOLITILE.                                          */ 00953000
*   /*                                                               */ 00954000
*   /*****************************************************************/ 00955000
*                                                               01S0294 00956000
*   RFY                                                         01S0294 00957000
*    (R0P,                          /* APIO ADDRESS                  */ 00958000
*     R8P,                                                      01S0294 00959000
*     R9P,                                                      01S0294 00960000
*     R10P,                                                     01S0294 00961000
*     R11P,                                                     01S0294 00962000
*     R12P,                                                     01S0294 00963000
*     R13P,                         /* MUST REMAIN UNCHANGED         */ 00964000
*     R14P,                         /* RETURN ADDRESS                */ 00965000
*     R15P) RSTD;                   /* INPUT ADDRESS (APIOFIXV)      */ 00966000
*   GEN REFS(R15P,APCODE,FIXAPIOV) SETS(APCODE);                01S0295 00967000
*                                   /* USING ENVIRONMENT                00968000
         PUSH  USING               SAVE CURRENT USING STATUS            00969000
         DROP  ,                   DROP ALL BASE REGISTERS              00970000
         LR    APCODE,R15P         PUT ENTRY POINT IN BASE REGISTER     00971000
         USING FIXAPIOV,APCODE     TELL ASSEMBLER                       00972000
*   RFY                                                         01S0296 00973000
*     R15P UNRSTD;                  /* DONE WITH TEMPORARY BASE      */ 00974000
*   RFY                                                         01S0297 00975000
*    (R1P,                                                      01S0297 00976000
*     R2P,                                                      01S0297 00977000
*     R15F) RSTD;                   /* REQUIRED REGISTERS            */ 00978000
*   R1P=R0P&'FFFFFFF8'X;            /* APIO ADDRESS IN USEABLE REG      00979000
*                                      (INSURE DOUBLEWORD ALIGNED)   */ 00980000
         LR    R1P,R0P                                          01S0298 00981000
         N     R1P,@CF03695                                     01S0298 00982000
*   RFY                                                         01S0299 00983000
*     APIO BASED(R1P);              /* TELL PL/S                     */ 00984000
*                                                               01S0299 00985000
*   /*****************************************************************/ 00986000
*   /*                                                               */ 00987000
*   /* FOLLOWING CHECKS USED TO VERIFY APIO -- (1) APIO IN REAL      */ 00988000
*   /* STORAGE, (2) APIO IN KEY-5, NON-FETCH PROTECTED STORAGE, AND  */ 00989000
*   /* (3) APIOID HAS 'APIO'. CARE IS TAKEN TO INSURE THAT A PROGRAM */ 00990000
*   /* CHECK DOES NOT OCCUR VERIFYING THE APIO.                      */ 00991000
*   /*                                                               */ 00992000
*   /*****************************************************************/ 00993000
*                                                               01S0300 00994000
*   R15F=8;                         /* ERROR RETURN CODE             */ 00995000
         LA    R15F,8                                           01S0300 00996000
*   IF R1P=R0P THEN                 /* INSURE APIO WAS DOUBLE WORD   */ 00997000
         CR    R1P,R0P                                          01S0301 00998000
         BNE   @RF00301                                         01S0301 00999000
*     DO;                           /* ALIGNED. IF NOT IS ERROR.     */ 01000000
*       GEN(LRA   R0P,0(,R1P))                                  01S0303 01001000
*       REFS(R1P) SETS(R0P);        /* SEE IF IN STORAGE             */ 01002000
         LRA   R0P,0(,R1P)                                              01003000
*       BC(7,FIXAPIOZ);             /* NO--NOT APIO                  */ 01004000
         BC    7,FIXAPIOZ                                       01S0304 01005000
*       DO;                         /* (TREAT ABOVE AS IF)           */ 01006000
*         R0P=R0P&'FFFFF800'X;      /* HARDWARE PAGE BDY (2K)        */ 01007000
         N     R0P,@CF03699                                     01S0306 01008000
*         GEN(ISK   R0P,R0P)                                    01S0307 01009000
*         REFS(R0P) SETS(R0P);      /* STORAGE KEY                   */ 01010000
         ISK   R0P,R0P                                                  01011000
*         R0P=R0P&'000000F8'X;      /* LEAVE ACCESS CODE, FETCH AUTH */ 01012000
         N     R0P,@CF03700                                     01S0308 01013000
*         IF R0P=16*APIOKEY THEN    /* MUST BE KEY OF APIO AND       */ 01014000
         C     R0P,@CF02411                                     01S0309 01015000
         BNE   @RF00309                                         01S0309 01016000
*           DO;                     /* NOT FETCH PROTECTED.          */ 01017000
*             R2P=ADDR(APIOSIZE);   /* INSURE 'APIOSIZE' FIELD       */ 01018000
         LA    R2P,APIOSIZE(,R1P)                               01S0311 01019000
*             GEN(LRA   R0P,0(,R2P))                            01S0312 01020000
*             REFS(R2P) SETS(R0P);  /* IS REFERENCABLE               */ 01021000
         LRA   R0P,0(,R2P)                                              01022000
*             BC(7,FIXAPIOZ);       /* (KEY CHECK NOT REQUIRED)      */ 01023000
         BC    7,FIXAPIOZ                                       01S0313 01024000
*             DO;                   /* TREAT ABOVE AS 'IF'           */ 01025000
*               R2P=ADDR(APIO)+APIOSIZE/* SET R2P = ADDR             */ 01026000
*                   -1;             /* OF LAST BYTE IN APIO          */ 01027000
         LR    R2P,R1P                                          01S0315 01028000
         AH    R2P,APIOSIZE(,R1P)                               01S0315 01029000
         BCTR  R2P,0                                            01S0315 01030000
*               GEN(LRA   R0P,0(,R1P))                          01S0316 01031000
*               REFS(R1P) SETS(R0P);/* REPEAT ABOVE CHECKS           */ 01032000
         LRA   R0P,0(,R1P)                                              01033000
*               BC(7,FIXAPIOZ);     /* TO INSURE THAT THE ENTIRE     */ 01034000
         BC    7,FIXAPIOZ                                       01S0317 01035000
*               DO;                 /* APIO (LESS THAN 2K) EXISTS    */ 01036000
*                 R0P=R0P&'FFFFF800'X;/* IN VALID STORAGE.           */ 01037000
         N     R0P,@CF03699                                     01S0319 01038000
*                 GEN(ISK   R0P,R0P)                            01S0320 01039000
*                 REFS(R0P) SETS(R0P);/* IF NOT,                     */ 01040000
         ISK   R0P,R0P                                                  01041000
*                 R0P=R0P&'000000F8'X;/* IS ERROR.                   */ 01042000
         N     R0P,@CF03700                                     01S0321 01043000
*                 IF R0P=16*APIOKEY THEN/* IF OK SO FAR,             */ 01044000
         C     R0P,@CF02411                                     01S0322 01045000
         BNE   @RF00322                                         01S0322 01046000
*                   IF APIOID=APIOIDC THEN/* THEN CAN                */ 01047000
         CLC   APIOID(4,R1P),@CC03378                           01S0323 01048000
         BNE   @RF00323                                         01S0323 01049000
*                     R15F=0;       /* REFERENCE THE APIO.           */ 01050000
         SLR   R15F,R15F                                        01S0324 01051000
*               END;                /* INSURE THAT THE APIO          */ 01052000
@RF00323 DS    0H                                               01S0325 01053000
@RF00322 DS    0H                                               01S0326 01054000
*             END;                  /* HEADER ID IS ALSO VALID       */ 01055000
*           END;                    /* (THIS CHECKS FOR OVERLAID     */ 01056000
*       END;                        /* APIO).                        */ 01057000
@RF00309 DS    0H                                               01S0329 01058000
*     END;                          /* R15F INDICATES IF BAD APIO.   */ 01059000
*   RFY                                                         01S0330 01060000
*     APIO;                         /* FINISHED WITH APIO            */ 01061000
@RF00301 DS    0H                                               01S0331 01062000
*FIXAPIOZ:                                                      01S0331 01063000
*   ;                               /* APIO VERIFIED                 */ 01064000
*                                                               01S0331 01065000
FIXAPIOZ DS    0H                                               01S0332 01066000
*   /*****************************************************************/ 01067000
*   /*                                                               */ 01068000
*   /* REGISTER 15 SET (0/8). RETURN TO Q-VERIFICATION.              */ 01069000
*   /*                                                               */ 01070000
*   /*****************************************************************/ 01071000
*                                                               01S0332 01072000
*   GEN(POP   USING)                                            01S0332 01073000
*   NOREFS NOSETS;                  /* RESTORE USING STATUS          */ 01074000
         POP   USING                                                    01075000
*   RETURN;                         /* RETURN TO Q-VERIFICATION      */ 01076000
@EL00003 DS    0H                                               01S0333 01077000
@EF00003 DS    0H                                               01S0333 01078000
@ER00003 BR    @14                                              01S0333 01079000
*   RFY                                                         01S0334 01080000
*     APIO;                         /* DONE WITH APIO                */ 01081000
*   RFY                                                         01S0335 01082000
*    (R0P,                                                      01S0335 01083000
*     R8P,                                                      01S0335 01084000
*     R9P,                                                      01S0335 01085000
*     R10P,                                                     01S0335 01086000
*     R11P,                                                     01S0335 01087000
*     R12P,                                                     01S0335 01088000
*     R13P,                                                     01S0335 01089000
*     R14P,                                                     01S0335 01090000
*     R1P,                                                      01S0335 01091000
*     R2P,                                                      01S0335 01092000
*     R15F) UNRSTD;                                             01S0335 01093000
*   END FIXAPIOV;                   /* END OF APIO VERIFICATION EXIT */ 01094000
*FIXAPIO1:                          /* QUEUE VERIFICATION FAILURE    */ 01095000
*                                                               01S0337 01096000
*   /*****************************************************************/ 01097000
*   /*                                                               */ 01098000
*   /* AN ABEND OCCURRED DURING QUEUE VERIFICATION. ELIMINATE THE    */ 01099000
*   /* ENTIRE APIO QUEUE BY RESETTING THE QUEUE HEADERS TO ZERO.     */ 01100000
*   /*                                                               */ 01101000
*   /*****************************************************************/ 01102000
*                                                               01S0337 01103000
*   DO;                             /* ENTRY FROM ESTAE/FRR RETRY    */ 01104000
*                                                               01S0337 01105000
FIXAPIO1 DS    0H                                               01S0338 01106000
*     /***************************************************************/ 01107000
*     /*                                                             */ 01108000
*     /* ENTRY FROM ESTAE/FRR RETRY (IGTUKA). RETRY DOES NOT INSURE  */ 01109000
*     /* REGISTERS 0-5, 8-11, 14-15 VALID SINCE NOT SAVED IN         */ 01110000
*     /* APESREGS. ISSUE DUMMY RESTRICT TO WIPE PL/S OPTIMIZATION    */ 01111000
*     /* MEMORY OF CURRENT REGISTER VALUES                           */ 01112000
*     /*                                                             */ 01113000
*     /***************************************************************/ 01114000
*                                                               01S0338 01115000
*     RFY                                                       01S0338 01116000
*      (R0RSTD,                                                 01S0338 01117000
*       R1RSTD,                                                 01S0338 01118000
*       R2RSTD,                                                 01S0338 01119000
*       R3RSTD,                                                 01S0338 01120000
*       R4RSTD,                                                 01S0338 01121000
*       R5RSTD,                     /* 6-7 BASES                     */ 01122000
*       R8RSTD,                                                 01S0338 01123000
*       R9RSTD,                                                 01S0338 01124000
*       R10RSTD,                                                01S0338 01125000
*       R11RSTD,                    /* 12-13 APCT/APSV PTRS          */ 01126000
*       R14RSTD,                                                01S0338 01127000
*       R15RSTD) RSTD;              /* KEEP PL/S FROM CARRYING DATA  */ 01128000
*     GEN(SPACE ,);                 /* NULL CODE                     */ 01129000
         SPACE ,                                                        01130000
*     RFY                                                       01S0340 01131000
*      (R0RSTD,                                                 01S0340 01132000
*       R1RSTD,                                                 01S0340 01133000
*       R2RSTD,                                                 01S0340 01134000
*       R3RSTD,                                                 01S0340 01135000
*       R4RSTD,                                                 01S0340 01136000
*       R5RSTD,                     /* 6-7 BASES                     */ 01137000
*       R8RSTD,                                                 01S0340 01138000
*       R9RSTD,                                                 01S0340 01139000
*       R10RSTD,                                                01S0340 01140000
*       R11RSTD,                    /* 12-13 APCT/APSV PTRS          */ 01141000
*       R14RSTD,                                                01S0340 01142000
*       R15RSTD) UNRSTD;            /* ALLOW PL/S TO USE REGS AGAIN  */ 01143000
*   END;                            /* RETRY ENVIRONMENT SET         */ 01144000
*   APESRTRY=DRETRY(03);            /* RESTORE ORIGINAL RECOVERY ADDR*/ 01145000
         L     @08,DRETRY+8                                     01S0342 01146000
         L     @01,APESREGP                                     01S0342 01147000
         ST    @08,APESRTRY(,@01)                               01S0342 01148000
*   GEN(MODESET EXTKEY=SCHED)                                   01S0343 01149000
*   NOREFS NOSETS;                  /* SWITCH TO APCT KEY            */ 01150000
         MODESET EXTKEY=SCHED                                           01151000
*   APCTAPIO=0;                     /* CLEAR THE APIO CHAIN          */ 01152000
         SLR   @08,@08                                          01S0344 01153000
         ST    @08,APCTAPIO(,APCTPTR)                           01S0344 01154000
*   APCTIOAP=0;                     /* HEADERS (FORWARD AND BACKWARD)*/ 01155000
         ST    @08,APCTIOAP(,APCTPTR)                           01S0345 01156000
*   GEN(MODESET EXTKEY=ZERO)                                    01S0346 01157000
*   NOREFS NOSETS;                  /* RESUME STANDARD KEY           */ 01158000
         MODESET EXTKEY=ZERO                                            01159000
*   RETURN;                         /* RETURN TO CALLER.             */ 01160000
         B     @EL00002                                         01S0347 01161000
