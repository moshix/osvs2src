TJDM     TITLE 'IGTJDM -- EOT/EOAS APIO CLEANUP                        *00001000
                        '                                               00002000
IGTJDM   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTJDM    77.093'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
*   APCTPTR=R0P->SSCTSUSE;          /* ESTABLISH APCT ADDRESS        */ 00014000
         LR    @08,R0P                                          01S0094 00015000
         L     APCTPTR,SSCTSUSE(,@08)                           01S0094 00016000
*   RSSOBPTR=R1P;                   /* SSOB ADDRESS IN SAFE REGISTER */ 00017000
         LR    RSSOBPTR,R1P                                     01S0095 00018000
*   RFY                                                         01S0096 00019000
*     SSOB BASED(RSSOBPTR);         /* TELL PL/S OF THE SSOB BASE    */ 00020000
*   RFY                                                         01S0097 00021000
*    (R0P,                                                      01S0097 00022000
*     R1P) UNRSTD;                  /* FINISHED WITH INPUT REGISTERS */ 00023000
         EJECT                                                          00024000
*                                                               01S0098 00025000
*   /*****************************************************************/ 00026000
*   /*                                                               */ 00027000
*   /* GET THE LOCAL AND CMS LOCKS. REQUIRED TO USE APVUWORK (PL/S   */ 00028000
*   /* DYNAMIC AREA) AND TO SCAN/UPDATE THE APIO QUEUE.              */ 00029000
*   /*                                                               */ 00030000
*   /*****************************************************************/ 00031000
*                                                               01S0098 00032000
*   DO;                             /* START OF CODE SEGMENT         */ 00033000
*                                                               02S0098 00034000
*     /***************************************************************/ 00035000
*     /*                                                             */ 00036000
*     /* OBTAIN THE LOCAL & CMS LOCKS. THE CMS LOCK IS REQUIRED TO   */ 00037000
*     /* SCAN/UPDATE THE APIO CHAIN AND TO USE APVUWORK.             */ 00038000
*     /*                                                             */ 00039000
*     /***************************************************************/ 00040000
*                                                               02S0099 00041000
*     GEN REFS(PSALITA) SETS(R0P,R1P,R11P,R12P,R13P,R14P);      02S0099 00042000
*                                                        /*             00043000
         LR    R0P,R12P            SAVE INPUT REGS 12 & 13              00044000
         LR    R1P,R13P            * (APCT, SAVEAREA)                   00045000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND, GET THE LOCAL LOCK     *00046000
               RELATED=(APVUWORK,APIO-CHAIN)                            00047000
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND, GET THE CMS LOCK         *00048000
               RELATED=(APVUWORK,APIO-CHAIN)                            00049000
         LR    R12P,R0P            RESTORE DESTROYED REGISTERS          00050000
         LR    R13P,R1P            * R11 & R14 STILL BAD                00051000
*   END;                            /* LOCAL & CMS LOCKS OWNED       */ 00052000
         EJECT                                                          00053000
*                                                               01S0101 00054000
*   /*****************************************************************/ 00055000
*   /*                                                               */ 00056000
*   /* BUILD AND INITIALIZE THE PL/S SAVEAREA AND WORKAREA. THE CMS  */ 00057000
*   /* LOCK MUST BE OWNED FOR THIS PROCESS.                          */ 00058000
*   /*                                                               */ 00059000
*   /*****************************************************************/ 00060000
*                                                               01S0101 00061000
*   DO;                             /* START OF CODE SEGMENT         */ 00062000
*                                                               02S0101 00063000
*     /***************************************************************/ 00064000
*     /*                                                             */ 00065000
*     /* BUILD AND INITIALIZE A SAVEAREA/WORKAREA. USE 'APVUWORK' FOR*/ 00066000
*     /* THE DYNAMIC AREA. NOTE THAT THE CMS LOCK MUST BE OWNED TO   */ 00067000
*     /* USE THIS AREA (FOR SERIALIZATION). CARE MUST BE USED TO     */ 00068000
*     /* INSURE MORE DYNAMIC STORAGE IS NOT USED THAN EXISTS IN      */ 00069000
*     /* APMVWORK (2000 BYTES). THE ADDRESS OF 'APVU' IS CONTAINED IN*/ 00070000
*     /* THE APCT (APCTAPVU).                                        */ 00071000
*     /*                                                             */ 00072000
*     /***************************************************************/ 00073000
*                                                               02S0102 00074000
*     RFY                                                       02S0102 00075000
*       R1P RSTD;                   /* CHAIN REGISTER                */ 00076000
*     R1P=ADDR(APVUWORK);           /* POINT TO WORKAREA             */ 00077000
*                                                               02S0103 00078000
         L     R1P,APCTAPVU(,APCTPTR)                           02S0103 00079000
         LA    R1P,APVUWORK(,R1P)                               02S0103 00080000
*     /***************************************************************/ 00081000
*     /*                                                             */ 00082000
*     /* BUILD AND CHAIN OS SAVEAREAS TOGETHER. DO NOT USE THE VPSS  */ 00083000
*     /* SAVEAREA FORMAT. THEREFORE STANDARD VPSS ROUTINES CANNOT BE */ 00084000
*     /* CALLED (SINCE THE LOCAL AND CMS LOCKS ARE OWNED, GETMAINS   */ 00085000
*     /* FOR ADDITIONAL AREAS CANNOT BE TOLERATED).                  */ 00086000
*     /*                                                             */ 00087000
*     /***************************************************************/ 00088000
*                                                               02S0104 00089000
*     APSVPTR->APSVNEXT=R1P;        /* FORWARD POINTER               */ 00090000
         ST    R1P,APSVNEXT(,APSVPTR)                           02S0104 00091000
*     R1P->APSVPREV=APSVPTR;        /* BACKWARD CHAIN POINTER        */ 00092000
         ST    APSVPTR,APSVPREV(,R1P)                           02S0105 00093000
*     APSVPTR=R1P;                  /* POINT TO NEW SAVEAREA         */ 00094000
         LR    APSVPTR,R1P                                      02S0106 00095000
*     RFY                                                       02S0107 00096000
*       R1P UNRSTD;                 /* FINISHED WITH REGISTER 1      */ 00097000
*     RFY                                                       02S0108 00098000
*       APDATA RSTD;                /* SET APDATA TO ADDRESS         */ 00099000
*     APDATA=ADDR(APSV)+APSVOSLN;   /* OF PL/S DYNAMIC AREA          */ 00100000
         LA    APDATA,72(,APSVPTR)                              02S0109 00101000
*     GEN(USING @DATD,APDATA)                                   02S0110 00102000
*     NOSETS REFS(APDATA);          /* (AFTER SAVEAREA).             */ 00103000
         USING @DATD,APDATA                                             00104000
*   END;                            /* END OF CODE SEGMENT           */ 00105000
         EJECT                                                          00106000
*                                                               01S0112 00107000
*   /*****************************************************************/ 00108000
*   /*                                                               */ 00109000
*   /* INITIALIZE THE RECOVERY AREA AND ISSUE A SETFRR TO COVER THIS */ 00110000
*   /* PROGRAM'S PROCESSING.                                         */ 00111000
*   /*                                                               */ 00112000
*   /*****************************************************************/ 00113000
*                                                               01S0112 00114000
*   DO;                             /* START OF CODE SEGMENT         */ 00115000
*                                                               02S0112 00116000
*     /***************************************************************/ 00117000
*     /*                                                             */ 00118000
*     /* ESTABLISH RECOVERY ENVIRONMENT. THIS EXIT WILL GAIN CONTROL */ 00119000
*     /* IF AN ABEND OCCURS. A DUMP IS TAKEN, THE APPROPRIATE ERROR  */ 00120000
*     /* FLAGS SET, AND CONTROL PASSED TO THE CLEANUP ROUTINE TO     */ 00121000
*     /* RELEASE RESOURCES BEFORE RETURNING TO THE CALLER.           */ 00122000
*     /*                                                             */ 00123000
*     /***************************************************************/ 00124000
*                                                               02S0113 00125000
*                                                               02S0113 00126000
*     /***************************************************************/ 00127000
*     /*                                                             */ 00128000
*     /* BUILD ESTAE/FRR EXIT PARAMETER LIST (APES)                  */ 00129000
*     /*                                                             */ 00130000
*     /***************************************************************/ 00131000
*                                                               02S0113 00132000
*     MVC(APES(1:LENGTH(CAPES)),CAPES);/* INITIALIZE APES CONSTANTS  */ 00133000
         MVC   APES(136),CAPES                                  02S0113 00134000
*     APESADDR=ADDR(APES);          /* ESTAE/SETFRR PARM VALUE       */ 00135000
         LA    @08,APES                                         02S0114 00136000
         ST    @08,APESADDR                                     02S0114 00137000
*     APESREGP=ADDR(APESREGZ);      /* DEFAULT REGISTER SAVEAREA     */ 00138000
         LA    @08,APESREGZ                                     02S0115 00139000
         ST    @08,APESREGP                                     02S0115 00140000
*     RFY                                                       02S0116 00141000
*      (R15P) RSTD;                 /* SET REGISTER 15 TO            */ 00142000
*     R15P=ADDR(EXIT);              /* BACKOUT ROUTINE               */ 00143000
         LA    R15P,EXIT                                        02S0117 00144000
*     STM(R0F,R15F,APESREGZ);       /* SAVE REGISTERS FOR RETRY      */ 00145000
         STM   R0F,R15F,APESREGZ                                02S0118 00146000
*     RFY                                                       02S0119 00147000
*       R15P UNRSTD;                /* FINISHED WITH 15              */ 00148000
*                                                               02S0119 00149000
*     /***************************************************************/ 00150000
*     /*                                                             */ 00151000
*     /* ISSUE SETFRR TO COVER THE PROCESSING. PARAMETER LIST IS     */ 00152000
*     /* 'APES' BLOCK. THE EXIT ROUTINE CAN PROCESS BOTH (E)STAE AND */ 00153000
*     /* FRR ENTRIES.                                                */ 00154000
*     /*                                                             */ 00155000
*     /***************************************************************/ 00156000
*                                                               02S0120 00157000
*     APESCODE=CERRN01;             /* SETFRR ERROR CODE             */ 00158000
         MVI   APESCODE,X'01'                                   02S0120 00159000
*     RFY                                                       02S0121 00160000
*       R15P RSTD;                  /* SETFRR SETS TO PARM LIST      */ 00161000
*     GEN REFS(PSA,PSACSTK,PSALSFCC,APCTZUKA,APCT,APCTPTR,FRRSFRRA,     00162000
*         FRRSENTR,FRRSPARM,FRRSCURR,FRRSLAST,FRRS) SETS(R14F,R15F,R15P 00163000
*         );                                                    02S0122 00164000
*                                   /*                                  00165000
         SETFRR A,FRRAD=APCTZUKA-APCT(,APCTPTR), IGTUKA IS EXIT RTN    *00166000
               PARMAD=(R15P),      * SAVE @ 6-WORD PARM FIELD HERE     *00167000
               CLEAR=NO,           * DO NOT CLEAR PARM FIELD           *00168000
               WRKREGS=(R14F,R15F), * WORK REGISTERS                   *00169000
               RELATED=(FREELLCK,APES) * APES IS INPUT                  00170000
*     APESCODE=CERRN99;             /* RESET ERROR CODE              */ 00171000
         MVI   APESCODE,X'03'                                   02S0123 00172000
*     R15P->I031P=ADDR(APES);       /* 1ST WORD OF PARM LIST IS 02S0124 00173000
*                                      ADDRESS OF 'APES'             */ 00174000
         LA    @14,APES                                         02S0124 00175000
         ST    @14,I031P(,R15P)                                 02S0124 00176000
*     RFY                                                       02S0125 00177000
*       R15P UNRSTD;                /* SETFRR PARM FIELD SET         */ 00178000
*     APESFFRR=BON;                 /* SHOW SETFRR ISSUED            */ 00179000
         OI    APESFFRR,B'10000000'                             02S0126 00180000
*   END;                            /* RECOVERY ENVIRONMENT SET      */ 00181000
         EJECT                                                          00182000
*                                                               01S0128 00183000
*   /*****************************************************************/ 00184000
*   /*                                                               */ 00185000
*   /* SCAN THE APIO QUEUE AND RELEASE ANY APIO'S STILL OWNED BY THE */ 00186000
*   /* TERMINATING TASK OR ADDRESS SPACE                             */ 00187000
*   /*                                                               */ 00188000
*   /*****************************************************************/ 00189000
*                                                               01S0128 00190000
*   DO;                                                         02S0128 00191000
*                                                               02S0128 00192000
*     /***************************************************************/ 00193000
*     /*                                                             */ 00194000
*     /* SCAN THE APIO'S TO FIND ENTRIES BELONGING TO THE TERMINATED */ 00195000
*     /* TASK OR ADDRESS SPACE. IF THIS IS A JOBSTEP TERMINATION,    */ 00196000
*     /* TREAT LIKE AN ADDRESS SPACE TERMINATION (THE INITIATOR DOES */ 00197000
*     /* NOT USE VPSS).                                              */ 00198000
*     /*                                                             */ 00199000
*     /***************************************************************/ 00200000
*                                                               02S0129 00201000
*     DRETRY(01)=APESREGF;          /* SAVE CURRENT APESREGS         */ 00202000
         L     @08,APESREGP                                     02S0129 00203000
         L     @15,APESREGF(,@08)                               02S0129 00204000
         ST    @15,DRETRY                                       02S0129 00205000
*     DRETRY(02)=APESREGE;          /* FOR REGISTERS 14 & 15         */ 00206000
         L     @15,APESREGE(,@08)                               02S0130 00207000
         ST    @15,DRETRY+4                                     02S0130 00208000
*     APESREGE=ADDR(SCANIO1);       /* RETRY APIO SCAN POINT         */ 00209000
         LA    @15,SCANIO1                                      02S0131 00210000
         ST    @15,APESREGE(,@08)                               02S0131 00211000
*     APESREGF=ADDR(FIXAPIO);       /* FIX APIO CHAIN ROUTINE        */ 00212000
         LA    @15,FIXAPIO                                      02S0132 00213000
         ST    @15,APESREGF(,@08)                               02S0132 00214000
*     GOTO SCANIO2;                 /* SKIP RETRY SCAN CODE          */ 00215000
         B     SCANIO2                                          02S0133 00216000
*SCANIO1:                           /* RETRY APIO SCAN               */ 00217000
*     DO;                           /* ENTRY FROM ESTAE/FRR RETRY    */ 00218000
*                                                               02S0134 00219000
SCANIO1  DS    0H                                               02S0135 00220000
*       /*************************************************************/ 00221000
*       /*                                                           */ 00222000
*       /* ENTRY FROM ESTAE/FRR RETRY (IGTUKA). RETRY DOES NOT INSURE*/ 00223000
*       /* REGISTERS 0-5, 8-11, 14-15 VALID SINCE NOT SAVED IN       */ 00224000
*       /* APESREGS. ISSUE DUMMY RESTRICT TO WIPE PL/S OPTIMIZATION  */ 00225000
*       /* MEMORY OF CURRENT REGISTER VALUES                         */ 00226000
*       /*                                                           */ 00227000
*       /*************************************************************/ 00228000
*                                                               02S0135 00229000
*       RFY                                                     02S0135 00230000
*        (R0RSTD,                                               02S0135 00231000
*         R1RSTD,                                               02S0135 00232000
*         R2RSTD,                                               02S0135 00233000
*         R3RSTD,                                               02S0135 00234000
*         R4RSTD,                                               02S0135 00235000
*         R5RSTD,                   /* 6-7 BASES                     */ 00236000
*         R8RSTD,                                               02S0135 00237000
*         R9RSTD,                                               02S0135 00238000
*         R10RSTD,                                              02S0135 00239000
*         R11RSTD,                  /* 12-13 APCT/APSV PTRS          */ 00240000
*         R14RSTD,                                              02S0135 00241000
*         R15RSTD) RSTD;            /* KEEP PL/S FROM CARRYING DATA  */ 00242000
*       GEN(SPACE ,);               /* NULL CODE                     */ 00243000
         SPACE ,                                                        00244000
*       RFY                                                     02S0137 00245000
*        (R0RSTD,                                               02S0137 00246000
*         R1RSTD,                                               02S0137 00247000
*         R2RSTD,                                               02S0137 00248000
*         R3RSTD,                                               02S0137 00249000
*         R4RSTD,                                               02S0137 00250000
*         R5RSTD,                   /* 6-7 BASES                     */ 00251000
*         R8RSTD,                                               02S0137 00252000
*         R9RSTD,                                               02S0137 00253000
*         R10RSTD,                                              02S0137 00254000
*         R11RSTD,                  /* 12-13 APCT/APSV PTRS          */ 00255000
*         R14RSTD,                                              02S0137 00256000
*         R15RSTD) UNRSTD;          /* ALLOW PL/S TO USE REGS AGAIN  */ 00257000
*     END;                          /* RETRY ENVIRONMENT SET         */ 00258000
*     APESREGF=DRETRY(01);          /* RESTORE ORIGINAL              */ 00259000
         L     @08,APESREGP                                     02S0139 00260000
         L     @15,DRETRY                                       02S0139 00261000
         ST    @15,APESREGF(,@08)                               02S0139 00262000
*     APESREGE=DRETRY(02);          /* REGISTERS 14 & 15             */ 00263000
         L     @15,DRETRY+4                                     02S0140 00264000
         ST    @15,APESREGE(,@08)                               02S0140 00265000
*     DO;                           /* ERROR RECOVERY COMPLETE       */ 00266000
*       APESFPRV=BOFF;              /* RESET RECURSION FLAG.         */ 00267000
         NI    APESFPRV,B'10111111'                             02S0142 00268000
*       APESCODE=CERRN99;           /* RESET ERROR CODE              */ 00269000
         MVI   APESCODE,X'03'                                   02S0143 00270000
*       APESFREC=BON;               /* RESET DUMP/LOG FLAGS          */ 00271000
*       APESFSDP=BON;               /* TO THEIR NORMAL VALUES        */ 00272000
         OI    APESFREC,B'01100000'                             02S0145 00273000
*     END;                          /* ALL INDICATORS RESET 2ND ABEND   00274000
*                                      TERMINATES REQUEST            */ 00275000
*     APESCOD1=0;                   /* CLEAR ERROR INDICATOR         */ 00276000
         MVI   APESCOD1,X'00'                                   02S0147 00277000
*SCANIO2:                           /* FIRST PASS START              */ 00278000
*                                                               02S0148 00279000
*     /***************************************************************/ 00280000
*     /*                                                             */ 00281000
*     /* SET RASCBPTR = ADDRESS OF ASCB BEING PROCESSED AND RTCCBPTR */ 00282000
*     /* = ADDRESS OF THE TCB BEING PROCESSED (=0 IF ADDRESS-SPACE OR*/ 00283000
*     /* JOBSTEP TERMINATION).                                       */ 00284000
*     /*                                                             */ 00285000
*     /***************************************************************/ 00286000
*                                                               02S0148 00287000
*     RFY                                                       02S0148 00288000
*      (RZERO,                      /* WORK REGISTER (=0)            */ 00289000
*       RASCBPTR,                   /* ADDRESS SPACE                 */ 00290000
*       RTCBPTR) RSTD;              /* TCB (OR 0)                    */ 00291000
SCANIO2  DS    0H                                               02S0149 00292000
*     RZERO=0;                      /* ZERO VALUE                    */ 00293000
         SLR   RZERO,RZERO                                      02S0149 00294000
*     RASCBPTR=SSETASCB;            /* ASCB (MAY NOT BE CURRENT)     */ 00295000
         L     @05,SSOBINDV(,RSSOBPTR)                          02S0150 00296000
         L     RASCBPTR,SSETASCB(,@05)                          02S0150 00297000
*     RTCBPTR=SSETCBA;              /* TCB IF EOT                    */ 00298000
         L     RTCBPTR,SSETCBA(,@05)                            02S0151 00299000
*     IF((SSOBFUNC=SSOBEOM)|        /* IF JOBSTEP OR ADDRESS SPACE   */ 00300000
*         (RTCBPTR->TCBJSTCA=RTCBPTR))/* TERMINATION, SET            */ 00301000
*       THEN                        /* RTCBPTR=0 (TO SHOW PURGE OF   */ 00302000
         CLC   SSOBFUNC(2,RSSOBPTR),@CH00037                    02S0152 00303000
         BE    @RT00152                                         02S0152 00304000
         L     @05,TCBJSTCA-1(,RTCBPTR)                         02S0152 00305000
         LA    @05,0(,@05)                                      02S0152 00306000
         CR    RTCBPTR,@05                                      02S0152 00307000
         BNE   @RF00152                                         02S0152 00308000
@RT00152 DS    0H                                               02S0153 00309000
*       RTCBPTR=RZERO;              /* ALL APIO'S FOR THIS ASID).    */ 00310000
*                                                               02S0153 00311000
         LR    RTCBPTR,RZERO                                    02S0153 00312000
*     /***************************************************************/ 00313000
*     /*                                                             */ 00314000
*     /* SCAN ALL APIOS. NOTE -- LOCAL & CMS LOCKS MUST BE HELD.     */ 00315000
*     /*                                                             */ 00316000
*     /***************************************************************/ 00317000
*                                                               02S0154 00318000
*     RFY                                                       02S0154 00319000
*       RAPIOPTR RSTD;              /* APIO SCANNER                  */ 00320000
@RF00152 DS    0H                                               02S0155 00321000
*     RAPIOPTR=APCTAPIO;            /* 1ST APIO                      */ 00322000
         L     RAPIOPTR,APCTAPIO(,APCTPTR)                      02S0155 00323000
*     RFY                                                       02S0156 00324000
*       APIO BASED(RAPIOPTR);       /* TELL PL/S                     */ 00325000
*     APESCODE=CERRN02;             /* APIO CHAIN ERROR              */ 00326000
         MVI   APESCODE,X'02'                                   02S0157 00327000
*     GEN(MODESET EXTKEY=DATAMGT)                               02S0158 00328000
*     NOREFS NOSETS;                /* APIO KEY. MAY NOT UPDATE PL/S    00329000
*                                      DYNAMIC AREA.                 */ 00330000
         MODESET EXTKEY=DATAMGT                                         00331000
*     DO WHILE(RAPIOPTR^=RZERO);    /* SCAN ALL APIO ENTRIES         */ 00332000
         B     @DE00159                                         02S0159 00333000
@DL00159 DS    0H                                               02S0160 00334000
*       GEN NOREFS NOSETS FLOWS(SCANIO1,FIXAPIO) SEQFLOW;       02S0160 00335000
*                                          /* SHOW RTNS USED (NO CODE)  00336000
*       IF APIOID^=APIOIDC THEN     /* IF BAD APIO, FIX-UP CHAIN     */ 00337000
         CLC   APIOID(4,RAPIOPTR),@CC03378                      02S0161 00338000
         BE    @RF00161                                         02S0161 00339000
*         DO;                       /* FORCE ABEND TO LOG ERROR &       00340000
*                                      PASS CONTROL TO THE (E)STAE OR   00341000
*                                      FRR EXIT ROUTINE.             */ 00342000
*           DO;                     /* ABEND(APCTABND)               */ 00343000
*             RESPECIFY                                         02S0164 00344000
*              (GPR01F) RESTRICTED;                             02S0164 00345000
*             GPR01F=APCTABND;      /* COMP CODE IN REG 1            */ 00346000
         L     GPR01F,APCTABND(,APCTPTR)                        02S0165 00347000
*             SVC(13);              /* ISSUE ABEND SVC               */ 00348000
         SVC   13                                               02S0166 00349000
*             RESPECIFY                                         02S0167 00350000
*              (GPR01F) UNRESTRICTED;                           02S0167 00351000
*           END;                    /* ABEND(APCTABND) FORCE ABEND   */ 00352000
*           GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);        02S0169 00353000
*                                                  /* NO CODE           00354000
*         END;                      /* ABEND ISSUED                  */ 00355000
*       IF(RASCBPTR=APIOASCB)&      /* IF CORRECT ADDRESS SPACE &    */ 00356000
*           ((RTCBPTR=RZERO)|       /* EITHER MEMORY-TERM/JOBSTEP OR */ 00357000
*           (APIODEB->DEBTCBB=RTCBPTR)) THEN/* CORRECT SUBTASK       */ 00358000
@RF00161 C     RASCBPTR,APIOASCB(,RAPIOPTR)                     02S0171 00359000
         BNE   @RF00171                                         02S0171 00360000
         CR    RTCBPTR,RZERO                                    02S0171 00361000
         BE    @RT00171                                         02S0171 00362000
         L     @02,APIODEB(,RAPIOPTR)                           02S0171 00363000
         L     @02,DEBTCBB-1(,@02)                              02S0171 00364000
         LA    @02,0(,@02)                                      02S0171 00365000
         CR    RTCBPTR,@02                                      02S0171 00366000
         BNE   @RF00171                                         02S0171 00367000
@RT00171 DS    0H                                               02S0172 00368000
*         DO;                       /* THEN MUST RELEASE.            */ 00369000
*                                                               02S0172 00370000
*           /*********************************************************/ 00371000
*           /*                                                       */ 00372000
*           /* APIO FOUND WHICH MUST BE RELEASED (RAPIOPTR->APIO)    */ 00373000
*           /*                                                       */ 00374000
*           /*********************************************************/ 00375000
*                                                               02S0173 00376000
*           IF APIOACTV^=0 THEN     /* IF APIO HAS OUTSTANDING I/O   */ 00377000
         CLI   APIOACTV(RAPIOPTR),0                             02S0173 00378000
         BE    @RF00173                                         02S0173 00379000
*             DO;                   /* THEN MUST CLEAN-UP THE APIO.  */ 00380000
*               IF SSOBFUNC=SSOBEOT THEN/* IF END-OF-TASK            */ 00381000
         CLC   SSOBFUNC(2,RSSOBPTR),@CH00045                    02S0175 00382000
         BNE   @RF00175                                         02S0175 00383000
*                 DO;               /* START OF SEGMENT              */ 00384000
*                                                               03S0176 00385000
*                   /*************************************************/ 00386000
*                   /*                                               */ 00387000
*                   /* APIO SHOWS ACTIVE. FREE THE ASSOCIATED I/O    */ 00388000
*                   /* RESOURCES BY CALLING IGTKCA (TERMINATION      */ 00389000
*                   /* ROUTINE). NOTE THAT THE I/O MUST HAVE BEEN    */ 00390000
