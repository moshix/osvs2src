TJH0     TITLE 'IGTJH0 - SUBSYSTEM ALLOCATION INTERFACE ROUTINE.       *00001000
                        '                                               00002000
*                                 DEFINE INITIAL GETMAIN MACRO          00003000
         MACRO                                                          00004000
&LABEL   IGTDGSV &R,&LV=,&SP=,&INITIAL=YES                              00005000
         L       APCTPTR,20(,@13)  SSCVT POINTER TO APCTREG FROM SAVE   00006000
         L       APCTPTR,SSCTSUSE-SSCT(,APCTPTR) POINTER TO APCT INITED 00007000
&LABEL   IGTYGSV &R,INITIAL=&INITIAL,SP=&SP,LV=&LV                      00008000
         MEND                                                           00009000
IGTJH0   CSECT ,                                                01S0002 00010000
@MAINENT DS    0H                                               01S0002 00011000
         USING *,@15                                            01S0002 00012000
         B     @PROLOG                                          01S0002 00013000
         DC    AL1(16)                                          01S0002 00014000
         DC    C'IGTJH0    77.230'                              01S0002 00015000
         DROP  @15                                                      00016000
@PROLOG  STM   @14,@12,12(@13)                                  01S0002 00017000
         BALR  @07,0                                            01S0002 00018000
@PSTART  LA    @08,4095(,@07)                                   01S0002 00019000
         USING @PSTART,@07                                      01S0002 00020000
         USING @PSTART+4095,@08                                 01S0002 00021000
         L     @00,@SIZDATD                                     01S0002 00022000
         IGTDGSV  R,LV=(0)                                              00023000
         LR    @06,@01                                          01S0002 00024000
         USING @DATD,@06                                        01S0002 00025000
         ST    @13,@SA00001+4                                   01S0002 00026000
         LM    @00,@01,20(@13)                                  01S0002 00027000
         ST    @06,8(,@13)                                      01S0002 00028000
         LR    @13,@06                                          01S0002 00029000
*                                 /* REFERENCED IN MACRO GENED PROLOG*/ 00030000
RBPRFXLN EQU 64                                                         00031000
RBPRFLNA EQU 32                                                         00032000
PRBLEN   EQU 136                                                        00033000
SIRBLEN  EQU 200                                                        00034000
TIRBLEN  EQU 136                                                        00035000
IRBLEN   EQU 128                                                        00036000
SVRBLEN  EQU 224                                                        00037000
         EJECT                                                          00038000
*   ALLCWAPT=ADDR(APAW);            /* ESTABLISH POINTER TO WORK AREA*/ 00039000
         LA    ALLCWAPT,APAW                                    01S0145 00040000
*/*  ESTABLISH SUBSYSTEM INTERFACE CONTROL BLOCK POINTERS            */ 00041000
*                                                               01S0146 00042000
*   RFY                                                         01S0146 00043000
*    (R0P,                                                      01S0146 00044000
*     R1P) RSTD;                    /* REGISTER USED AS PARMREGS     */ 00045000
*   SSOBPTR=R1P;                    /* POINTER TO THE BASIC SSOB     */ 00046000
         ST    R1P,SSOBPTR                                      01S0147 00047000
*   SSCTPTR=R0P;                    /* POINTER TO SSCVT FOR SUBSYS   */ 00048000
         LR    SSCTPTR,R0P                                      01S0148 00049000
*   RFY                                                         01S0149 00050000
*    (R0P,                                                      01S0149 00051000
*     R1P) UNRSTD;                  /* REGISTER USED AS PARMREGS     */ 00052000
*   FIRSTARB=SSAGARBP;              /* INITIALIZE ARB POINTER        */ 00053000
         L     @09,SSOBPTR                                      01S0150 00054000
         L     @09,SSOBINDV(,@09)                               01S0150 00055000
         L     @05,SSAGARBP(,@09)                               01S0150 00056000
         ST    @05,FIRSTARB                                     01S0150 00057000
*   SSDDRPTR=FIRSTARB;              /* INIT VARIABLE POINTER         */ 00058000
         LR    SSDDRPTR,@05                                     01S0151 00059000
*   RFY                                                         01S0152 00060000
*     SSAGARBK BASED(SSDDRPTR);     /* POINTER TO DD/TEXT UNIT  01S0152 00061000
*                                      REQUEST                       */ 00062000
*   RFY                                                         01S0153 00063000
*     SSWA BASED(SSAGSSWA);         /* PROVIDE ADDRESS OF SSWA       */ 00064000
*   ALLSTAT=0;                      /* INITIALIZE STATUS INDICATORS  */ 00065000
         SLR   @05,@05                                          01S0154 00066000
         ST    @05,ALLSTAT                                      01S0154 00067000
*   ALLRSRCE=0;                     /* INITIALIZE ENQ RESOURCE STATUS*/ 00068000
         ST    @05,ALLRSRCE                                     01S0155 00069000
*                                                               02S0156 00070000
*   /*****************************************************************/ 00071000
*   /*                                                               */ 00072000
*   /* THIS SEGMENT CREATES AN ECB LIST FOR USE FOLLOWING AN ENQ WITH*/ 00073000
*   /* ECB OPTION. IF A CANCEL ECB HAS NOT BEEN PROVIDED OR THE ECB  */ 00074000
*   /* PROVIDED IS ALREADY BEING WAITED ON, A DUMMY CANCEL ECB IS    */ 00075000
*   /* CREATED AND ITS ADDRESS IS STORED IN THE ECB LIST.            */ 00076000
*   /*                                                               */ 00077000
*   /*****************************************************************/ 00078000
*                                                               02S0156 00079000
*   DO;                             /* INSURE ENTIRE SEGMENT IS INCLD*/ 00080000
*     IF SSAGCNCL=0|                /* IF CNCLECB NOT PROVIDED OR    */ 00081000
*         SSAGCNCL->ECBWAITB=BON THEN/* CNCLECB IS BEING WIATED ON   */ 00082000
         L     @09,SSAGCNCL(,@09)                               02S0157 00083000
         CR    @09,@05                                          02S0157 00084000
         BE    @RT00157                                         02S0157 00085000
         TM    ECBWAITB(@09),B'10000000'                        02S0157 00086000
         BNO   @RF00157                                         02S0157 00087000
@RT00157 DS    0H                                               02S0158 00088000
*       DO;                         /* AUTH. DYN REQUESTS COULD WAIT */ 00089000
*         CNCLECBP=ADDR(CANCLECB);  /* PROVIDE DUMMY CANCEL ECB      */ 00090000
         LA    @09,CANCLECB                                     02S0159 00091000
         ST    @09,CNCLECBP                                     02S0159 00092000
*         CANCLECB=0;               /* AND CLEAR IT                  */ 00093000
         SLR   @09,@09                                          02S0160 00094000
         ST    @09,CANCLECB                                     02S0160 00095000
*       END;                                                    02S0161 00096000
*     ELSE                          /* OTHERWISE, USE THE CANCEL ECB */ 00097000
*       CNCLECBP=SSAGCNCL;          /* PROVIDED BY ALLOCATION.       */ 00098000
         B     @RC00157                                         02S0162 00099000
@RF00157 L     @09,SSOBPTR                                      02S0162 00100000
         L     @09,SSOBINDV(,@09)                               02S0162 00101000
         L     @09,SSAGCNCL(,@09)                               02S0162 00102000
         ST    @09,CNCLECBP                                     02S0162 00103000
*     ENQECBP=ADDR(ENQECB);         /* ADDRESS OF ENQ ECB FOR ECBLIST*/ 00104000
@RC00157 LA    @09,ENQECB                                       02S0163 00105000
         ST    @09,ENQECBP                                      02S0163 00106000
*     ACNCLECB=CNCLECBP;            /* ADDR OF CANCEL ECB TO WRK AREA*/ 00107000
         L     @09,CNCLECBP                                     02S0164 00108000
         ST    @09,ACNCLECB                                     02S0164 00109000
*     LASTECBI='1'B;                /* ADDRESSES SET, INDICATE LAST  */ 00110000
         OI    LASTECBI,B'10000000'                             02S0165 00111000
*   END;                            /* END OF SEGMENT.               */ 00112000
*   DO;                             /* BEGIN CODE SEGMENT            */ 00113000
*/*   SEGMENT DETERMINES WHETHER SS ALLOCATION HAS BEEN INVOKED BY      00114000
*     DYNAMIC OR BATCH ALLOCATION AND SETS ALLDYN = BON IF THIS IS      00115000
*     AN SVC 99 DYNAMIC ALLOCAITON.                                  */ 00116000
*                                                               02S0168 00117000
*     IF PSATOLD->TCBRBP->RBFTP='110'B THEN/* IF CURRENT TCB         */ 00118000
         L     @09,PSATOLD                                      02S0168 00119000
         L     @09,TCBRBP(,@09)                                 02S0168 00120000
         TM    RBFTP(@09),B'11000000'                           02S0168 00121000
         BNO   @RF00168                                         02S0168 00122000
         TM    RBFTP(@09),B'00100000'                           02S0168 00123000
         BNZ   @RF00168                                         02S0168 00124000
*       ALLDYN=BON;                 /* HAS SVRB AS TOP RB - DYN      */ 00125000
         OI    ALLDYN,B'10000000'                               02S0169 00126000
*   END;                            /* END OF SEGMENT                */ 00127000
@RF00168 DS    0H                                               02S0171 00128000
*                                                               02S0171 00129000
*   /*****************************************************************/ 00130000
*   /*                                                               */ 00131000
*   /* VALIDATE SUSBSYSTEM REQUEST. INSURE THAT VPSS ALLOCATION      */ 00132000
*   /* RECEIVED CONTROL FROM COMMON ALLOCATION.                      */ 00133000
*   /*                                                               */ 00134000
*   /*****************************************************************/ 00135000
*                                                               02S0171 00136000
*   DO;                             /* BEGIN CODE SEGMENT            */ 00137000
*     IF SSOBID^='SSOB' THEN        /* INVALID ENTRY - BUT           */ 00138000
         L     @09,SSOBPTR                                      02S0172 00139000
         CLC   SSOBID(4,@09),@CC03882                           02S0172 00140000
         BE    @RF00172                                         02S0172 00141000
*       DO;                         /* NOWHERE TO GO                 */ 00142000
*         ALLFAIL=BON;              /* INDICATE ALLOCATION FAILS     */ 00143000
         OI    ALLFAIL,B'10000000'                              02S0174 00144000
*         RETCODE=SSRTLERR;         /* INDICATE INVALID REQUEST VIA  */ 00145000
         MVC   RETCODE(4),@CF00288                              02S0175 00146000
*       END;                        /* REGISTER 15 RETURN CODE,NOT      00147000
*                                      RETN                          */ 00148000
*     ELSE                          /* SSOB ID IS IN FACT "SSOB"     */ 00149000
*       IF SSOBFUNC^=SSOBAGRP|      /* IF NOT THE SSOB FOR GRP ALLOC    00150000
*                                      OR                            */ 00151000
*           SSAGARBP=0 THEN         /* NO DD/TEXT UNIT REQUESTS MADE */ 00152000
         B     @RC00172                                         02S0177 00153000
@RF00172 L     @09,SSOBPTR                                      02S0177 00154000
         CLC   SSOBFUNC(2,@09),@CH00379                         02S0177 00155000
         BNE   @RT00177                                         02S0177 00156000
         L     @09,SSOBINDV(,@09)                               02S0177 00157000
         L     @09,SSAGARBP(,@09)                               02S0177 00158000
         LTR   @09,@09                                          02S0177 00159000
         BNZ   @RF00177                                         02S0177 00160000
@RT00177 DS    0H                                               02S0178 00161000
*         DO;                       /* ALLOCATION FAILS              */ 00162000
*           ALLFAIL=BON;            /* INDICATE FAILURE AND          */ 00163000
         OI    ALLFAIL,B'10000000'                              02S0179 00164000
*           RETCODE=SSRTLERR;       /* VIA R15 RETURN CODE INFORM       00165000
*                                      ALLOC                         */ 00166000
         MVC   RETCODE(4),@CF00288                              02S0180 00167000
*         END;                      /* OF FAILURE TO INTERFACE  02S0181 00168000
*                                      PROPERLY                      */ 00169000
*       ELSE                        /* OTHERWISE, REQUEST APPEARS       00170000
*                                      VALID                         */ 00171000
*         DO;                       /* INITIALIZE FOR PROCESSING     */ 00172000
         B     @RC00177                                         02S0182 00173000
@RF00177 DS    0H                                               02S0183 00174000
*           ALLRQVAL=BON;           /* INDICATE THAT REQUEST IS VALID*/ 00175000
         OI    ALLRQVAL,B'00000100'                             02S0183 00176000
*           SSOBRETN=SSAGRTOK;      /* SUCCESSFUL SSOBRETN           */ 00177000
         SLR   @09,@09                                          02S0184 00178000
         L     @05,SSOBPTR                                      02S0184 00179000
         ST    @09,SSOBRETN(,@05)                               02S0184 00180000
*           RETCODE=0;              /* SET REGISTER 15 RETURN CODE 0 */ 00181000
         ST    @09,RETCODE                                      02S0185 00182000
*         END;                      /* DO IF REQUEST VALID           */ 00183000
*   END;                            /* END OF SEGMENT                */ 00184000
@RC00177 DS    0H                                               02S0187 00185000
@RC00172 DS    0H                                               02S0188 00186000
*   IF ALLRQVAL='1'B THEN           /* REQUEST IS VALID - SO FAR     */ 00187000
         TM    ALLRQVAL,B'00000100'                             01S0188 00188000
         BNO   @RF00188                                         01S0188 00189000
*     DO;                           /* PROCEED WITH ERR RECOV   01S0189 00190000
*                                      ENVIRONMN                     */ 00191000
*       DO;                         /* START OF CODE SEGMENT         */ 00192000
*                                                               02S0190 00193000
*         /***********************************************************/ 00194000
*         /*                                                         */ 00195000
*         /* ESTABLISH ERROR RECOVERY ENVIRONMENT. SUBSYSTEM         */ 00196000
*         /* ALLOCATION REQUIRES THAT AN ERROR RECOVERY ENVIRONMENT  */ 00197000
*         /* BE ESTABLISHED PRIOR TO ANY ACQUISTITON OF SYSTEM OR    */ 00198000
*         /* SUBSYSTEM RESOURCES.                                    */ 00199000
*         /*                                                         */ 00200000
*         /***********************************************************/ 00201000
*                                                               02S0191 00202000
*                                                               02S0191 00203000
*         /***********************************************************/ 00204000
*         /*                                                         */ 00205000
*         /* ESTABLISH RECOVERY ENVIRONMENT. THE ERROR RECOVERY EXIT */ 00206000
*         /* WILL GAIN CONTROL IF AN ABEND OCCURS. A DUMP IS TAKEN,  */ 00207000
*         /* THE APPROPRIATE ERROR FLAGS SET, AND CONTROL PASSED TO  */ 00208000
*         /* THE CLEANUP ROUTINE TO RELEASE RESOURCES BEFORE         */ 00209000
*         /* RETURNING TO THE CALLER.                                */ 00210000
*         /*                                                         */ 00211000
*         /***********************************************************/ 00212000
*                                                               02S0191 00213000
*                                                               02S0191 00214000
*         /***********************************************************/ 00215000
*         /*                                                         */ 00216000
*         /* BUILD ESTAE/FRR EXIT PARAMETER LIST                     */ 00217000
*         /*                                                         */ 00218000
*         /***********************************************************/ 00219000
*                                                               02S0191 00220000
*         GEN REFS(ESTAEL,ESTAEL2,ESTAEL2L);                    02S0191 00221000
*                                            /*                         00222000
         MVC   ESTAEL(L'ESTAEL2L),ESTAEL2 MOVE LIST TO THE DYN AREA     00223000
*         APESPTR=ADDR(DAPES1);     /* POINT TO THE APES             */ 00224000
         LA    @09,DAPES1                                       02S0192 00225000
         ST    @09,APESPTR                                      02S0192 00226000
*         RFY                                                   02S0193 00227000
*           R5P RSTD;               /* WILL TEMPORARILY USE FOR APES */ 00228000
*         RFY                                                   02S0194 00229000
*           APES BASED(R5P);        /* PROVIDE TEMP BASING EXPRESSION*/ 00230000
*         R5P=APESPTR;              /* INITIALIZE THE POINTER        */ 00231000
         LR    R5P,@09                                          02S0195 00232000
*         RFY                                                   02S0196 00233000
*          (R0P,                                                02S0196 00234000
*           R1F,                                                02S0196 00235000
*           R14P,                                               02S0196 00236000
*           R15F) RSTD;             /* INITIALIZE APES BLOCK         */ 00237000
*         R0P=ADDR(APES);           /* TO ZEROS                      */ 00238000
         LR    R0P,R5P                                          02S0197 00239000
*         R1F=LENGTH(APES);         /* USE MVCL TO                   */ 00240000
         LA    R1F,648                                          02S0198 00241000
*         R15F=0;                   /* CLEAR, SINCE FIELD            */ 00242000
         SLR   R15F,R15F                                        02S0199 00243000
*         MVCL(R0P,R14P);           /* MORE THAN 216 BYTES LONG      */ 00244000
         MVCL  R0P,R14P                                         02S0200 00245000
*         RFY                                                   02S0201 00246000
*          (R0P,                                                02S0201 00247000
*           R1F,                                                02S0201 00248000
*           R14P,                                               02S0201 00249000
*           R15F) UNRSTD;           /* FINISHED WITH MVCL REGS       */ 00250000
*         APESID=APESIDC;           /* IN-CORE ID                    */ 00251000
         MVC   APESID(4,R5P),@CC03866                           02S0202 00252000
*         APESADDR=ADDR(APES);      /* ESTAE PARM VALUE              */ 00253000
         ST    R5P,APESADDR(,R5P)                               02S0203 00254000
*         APESAPMV=ADDR(CMBRXZZZ);  /* MESSAGE INDEX TABLE           */ 00255000
         LA    @09,CMBRXZZZ                                     02S0204 00256000
         ST    @09,APESAPMV(,R5P)                               02S0204 00257000
*         APESMXCD=CERRN99;         /* MAXIMUM APESCODE VALUE        */ 00258000
         MVI   APESMXCD(R5P),X'01'                              02S0205 00259000
*         APESFREC=BON;             /* RECORD ON LOGREC              */ 00260000
*         APESFSDP=BON;             /* SDUMP ON ERROR                */ 00261000
         OI    APESFREC(R5P),B'01100000'                        02S0207 00262000
*         APESMOD='IGTJH0  ';       /* MODULE IN CONTROL             */ 00263000
         MVC   APESMOD(8,R5P),@CC03889                          02S0208 00264000
*         APESPROC='IGTJH0  ';      /* CSECT IN CONTROL              */ 00265000
         MVC   APESPROC(8,R5P),@CC03889                         02S0209 00266000
*         APESFRR='        ';       /* NO FRR ID                     */ 00267000
         MVC   APESFRR(8,R5P),@CC03890                          02S0210 00268000
*         APESREGP=ADDR(APESREGZ);  /* DEFAULT REGISTER SAVEAREA     */ 00269000
         LA    @09,APESREGZ(,R5P)                               02S0211 00270000
         ST    @09,APESREGP(,R5P)                               02S0211 00271000
*         STM(R0F,R14F,APESREGZ);   /* SAVE REGISTERS FOR RETRY      */ 00272000
         STM   R0F,R14F,APESREGZ(R5P)                           02S0212 00273000
*         APESCODE=CERRN99;         /* 'UNKNOWN' ERROR               */ 00274000
         MVI   APESCODE(R5P),X'01'                              02S0213 00275000
*         RFY                                                   02S0214 00276000
*           R2P RSTD;               /* POINTS TO EXIT ROUTINE        */ 00277000
*         R2P=ADDR(APCVZUKA);       /* (IGTUKA).                     */ 00278000
         L     R2P,APCTZUKA(,APCTPTR)                           02S0215 00279000
*         APESEXRA=ADDR(JH0EXR);    /* PROVIDE EXTENDED RECOVERY EXIT*/ 00280000
         LA    @15,JH0EXR                                       02S0216 00281000
         ST    @15,APESEXRA(,R5P)                               02S0216 00282000
*         APESFEXR=BON;             /* INDICATE EXTENDED ESTAE AVAIL */ 00283000
         OI    APESFEXR(R5P),B'00000100'                        02S0217 00284000
*         APESRTRY=ADDR(IGTJHX);    /* PROVIDE (E)STAE RETRY ROUTINE */ 00285000
         LA    @15,IGTJHX                                       02S0218 00286000
         ST    @15,APESRTRY(,@09)                               02S0218 00287000
*         GEN REFS(APESADDR,APES,R5P,APSVPTR,ESTAEL,R2P,FLCCVT,PSA,     00288000
*             CVTSV60,CVTMAP) SETS(ESTAEL);                     02S0219 00289000
*                                   /*                                  00290000
         IGTYSTAE (R2P),           IGTUKA IS EXIT ROUTINE              *00291000
               PARAM=APESADDR-APES(R5P), ADDR OF APES IS PARM LIST     *00292000
               BRANCH=NO,          SVC ENTRY                           *00293000
               MF=(E,ESTAEL)       EXECUTE FORM OF THE MACRO            00294000
*         RFY                                                   02S0220 00295000
*          (R2P,                                                02S0220 00296000
*           R5P) UNRSTD;            /* DONE WITH EXIT PTR & TEMP APES*/ 00297000
*         RFY                                                   02S0221 00298000
*           APES BASED(APESPTR);    /* REVERT TO NORMAL BASING EXP   */ 00299000
*         RFY                                                   02S0222 00300000
*           R15F RSTD;              /* REGISTER USED FOR RETURN CODE */ 00301000
*         IF R15F=0 THEN            /* IF ERR RECOVERY ENV ESTABLISHD*/ 00302000
         LTR   R15F,R15F                                        02S0223 00303000
         BNZ   @RF00223                                         02S0223 00304000
*           ALLERREN=BON;           /* INDICATE SUCCESS .            */ 00305000
         OI    ALLERREN,B'00000010'                             02S0224 00306000
*         ELSE                      /* ERROR RECOVERY ENVIRONMENT NOT*/ 00307000
*           DO;                     /* ESTABLISHED. INDICATE ERROR.  */ 00308000
         B     @RC00223                                         02S0225 00309000
@RF00223 DS    0H                                               02S0226 00310000
*             RFY                                               02S0226 00311000
*               R15F UNRSTD;        /* REGISTER USED FOR RETURN CODE */ 00312000
*             ALLFAIL=BON;          /* JOB FAILS ALLOCATION          */ 00313000
*                                                               02S0227 00314000
         OI    ALLFAIL,B'10000000'                              02S0227 00315000
*             /*******************************************************/ 00316000
*             /*                                                     */ 00317000
*             /* IGTYJHE ERR(480)                                    */ 00318000
*             /*                                                     */ 00319000
*             /*******************************************************/ 00320000
*                                                               02S0228 00321000
*             RFY                                               02S0228 00322000
*               R1F RSTD;                                       02S0228 00323000
*             R1F=3;                /* PROVIDE INDEX                 */ 00324000
         LA    R1F,3                                            02S0229 00325000
*             CALL APCVZJHE;        /* SPECIFY ERROR INFORMATION        00326000
*                                      PRIOR TO RETURN               */ 00327000
         L     @15,APCTZJHE(,APCTPTR)                           02S0230 00328000
         BALR  @14,@15                                          02S0230 00329000
*             RFY                                               02S0231 00330000
*               R1F UNRSTD;         /* DONE WITH REG PROVIDE ERR INFO   00331000
*                                      FOR RETURN                    */ 00332000
*           END;                                                02S0232 00333000
*       END;                        /* ESTAE ENVIRONMENT SET         */ 00334000
@RC00223 DS    0H                                               02S0234 00335000
*       IF ALLERREN='1'B THEN       /* ERROR RECOVERY ENV ESTABLISHED*/ 00336000
         TM    ALLERREN,B'00000010'                             01S0234 00337000
         BNO   @RF00234                                         01S0234 00338000
*         DO;                       /* PROCESS SSWA SUBSYS PARMS     */ 00339000
*           DO;                                                 02S0236 00340000
*                                                               03S0237 00341000
*             /*******************************************************/ 00342000
*             /*                                                     */ 00343000
*             /* STUB FOR JCL PROCESSOR - IF NO ERRORS ARE DETECTED  */ 00344000
*             /* THEN THIS MODULE RETURNS THE ADDRESS OF AN APDS     */ 00345000
*             /* ELEMENT IN THE SSAGSSCM FIELD OF THE DD RELATED     */ 00346000
*             /* ALLOCATION REQUEST BLOCK.                           */ 00347000
*             /*                                                     */ 00348000
*             /*******************************************************/ 00349000
*                                                               03S0237 00350000
*/*  COUNT THE REQUESTS                                              */ 00351000
*                                                               03S0237 00352000
*             DO NUMALREQ=0 BY 1 WHILE SSDDRPTR^=0;/* COUNT NUMBER OF   00353000
*                                      DD/                           */ 00354000
         SLR   @09,@09                                          03S0237 00355000
         ST    @09,NUMALREQ                                     03S0237 00356000
         B     @DE00237                                         03S0237 00357000
@DL00237 DS    0H                                               03S0238 00358000
*               LASTARB=SSDDRPTR;   /* TEXT UNIT REQUESTS.           */ 00359000
         ST    SSDDRPTR,LASTARB                                 03S0238 00360000
*               SSDDRPTR=SSAGNRBP;  /* ADVANCE TO NEXT.              */ 00361000
         L     SSDDRPTR,SSAGNRBP(,SSDDRPTR)                     03S0239 00362000
*             END;                                              03S0240 00363000
         LA    @09,1                                            03S0240 00364000
         AL    @09,NUMALREQ                                     03S0240 00365000
         ST    @09,NUMALREQ                                     03S0240 00366000
@DE00237 LTR   SSDDRPTR,SSDDRPTR                                03S0240 00367000
         BNZ   @DL00237                                         03S0240 00368000
*             SSDDRPTR=FIRSTARB;    /* REINSTATE POINTER TO ALRB     */ 00369000
*                                                               03S0241 00370000
         L     SSDDRPTR,FIRSTARB                                03S0241 00371000
*             /*******************************************************/ 00372000
*             /*                                                     */ 00373000
*             /* OBTAIN STORAGE FOR APDS(S) IN ANTICIPATION THAT IT  */ 00374000
*             /* WILL BE NEEDED.                                     */ 00375000
*             /*                                                     */ 00376000
*             /*******************************************************/ 00377000
*                                                               03S0242 00378000
*             RFY                                               03S0242 00379000
*               APGF BASED(ADDR(DAPGF));/* IGTUAG INPUT PARMS FORMAT */ 00380000
*             APGF=APGF&&APGF;      /* INITIALIZE UNUSED FIELDS      */ 00381000
         LA    @09,DAPGF                                        03S0243 00382000
         XC    APGF(16,@09),APGF(@09)                           03S0243 00383000
