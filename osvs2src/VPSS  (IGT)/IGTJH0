*   END;                            /* END CODE SEGMENT              */ 02143000
*                                                               01S0741 02144000
*/* THIS SEGMENT RETURNS EITHER TO JH0EXR, THE EXTENDED RECOVERY EXIT   02145000
*   OR RETURNS TO THE CALLER, COMMON/DYNAMIC ALLOCATION, DEPENDING ON   02146000
*   THE SETTING OF ALLERRN2, WHICH IS ON IF ONLY CLEANUP IS ALLOWED. */ 02147000
*                                                               01S0741 02148000
*JHXRET:                                                        01S0741 02149000
*   ;                               /* TARGET FOR ENTRY              */ 02150000
JHXRET   DS    0H                                               01S0742 02151000
*   IF ALLERRN2=BON THEN            /* IF A SUBROUTINE OF JH0EXR     */ 02152000
         TM    ALLERRN2,B'00000010'                             01S0742 02153000
         BO    @RT00742                                         01S0742 02154000
*     RETURN;                       /* RETURN TO IT                  */ 02155000
*   ELSE                            /* MADE IT THROUGH RETRY - RETURN*/ 02156000
*     DO;                           /* BUT FIRST FILL IN ERROR INFO  */ 02157000
*                                                               01S0744 02158000
*       /*************************************************************/ 02159000
*       /*                                                           */ 02160000
*       /* IGTYJHE ERR(486)                                          */ 02161000
*       /*                                                           */ 02162000
*       /*************************************************************/ 02163000
*                                                               01S0745 02164000
*       RFY                                                     01S0745 02165000
*         R1F RSTD;                                             01S0745 02166000
*       R1F=16;                     /* PROVIDE INDEX                 */ 02167000
         LA    R1F,16                                           01S0746 02168000
*       CALL APCVZJHE;              /* SPECIFY ERROR INFORMATION        02169000
*                                      PRIOR TO RETURN               */ 02170000
         L     @15,APCTZJHE(,APCTPTR)                           01S0747 02171000
         BALR  @14,@15                                          01S0747 02172000
*       RFY                                                     01S0748 02173000
*         R1F UNRSTD;               /* DONE WITH REG PROVIDE ERROR      02174000
*                                      RETURN INFO                   */ 02175000
*       GOTO JH0RETN;               /* TO CALLER OF ALLOCATION       */ 02176000
         B     JH0RETN                                          01S0749 02177000
*     END;                          /* SHOULD NEVER GET THIS FAR     */ 02178000
*   END IGTJHX;                     /* END OF PROGRAM SEGMENT        */ 02179000
@EL00007 DS    0H                                               01S0751 02180000
@EF00007 DS    0H                                               01S0751 02181000
@ER00007 LM    @14,@12,@SA00007                                 01S0751 02182000
         BR    @14                                              01S0751 02183000
         EJECT                                                          02184000
*FIXQUEUE:                                                      01S0752 02185000
*   PROC;                           /* SEGMENT COMPLETES ADDITION OR    02186000
*                                      REMOVAL OF APDS QUEUE ELEMENTS*/ 02187000
FIXQUEUE STM   @14,@12,12(@13)                                  01S0752 02188000
*   IF ALLQADD=BON THEN             /* IF PROCESS OF ADDING TO THE   */ 02189000
         TM    ALLQADD,B'00100000'                              01S0753 02190000
         BNO   @RF00753                                         01S0753 02191000
*     DO;                           /* QUEUE WAS INTERUPTED THEN     */ 02192000
*       IF LASTAPDS^=0 THEN         /* IF THERE IS A NEW LAST THEN   */ 02193000
         L     @10,LASTAPDS                                     01S0755 02194000
         SLR   @09,@09                                          01S0755 02195000
         CR    @10,@09                                          01S0755 02196000
         BE    @RF00755                                         01S0755 02197000
*         LASTAPDS->APDSAPDS=0;     /* IT HAS NO NEXT                */ 02198000
         ST    @09,APDSAPDS(,@10)                               01S0756 02199000
*       APCTAPDS=FIRSAPDS;          /* NEW FIRST IS PREV TO OUR FIRST*/ 02200000
@RF00755 L     @10,FIRSAPDS                                     01S0757 02201000
         ST    @10,APCTAPDS(,APCTPTR)                           01S0757 02202000
*       APCTAPDS=LASTAPDS;          /* NEW LAST IS NEXT TO OUR LAST  */ 02203000
         L     @10,LASTAPDS                                     01S0758 02204000
         ST    @10,APCTAPDS(,APCTPTR)                           01S0758 02205000
*       ALLQADD=BOFF;               /* NO LONGER IN PROCESS OF ADDING*/ 02206000
         NI    ALLQADD,B'11011111'                              01S0759 02207000
*     END;                          /* OUR APDS(S) HAVE BEEN REMOVED */ 02208000
*   ELSE                            /* NOT ADDING, THEREFORE REMOVING*/ 02209000
*     DO;                           /* COMPLETE THE REMOVAL OF OUR   */ 02210000
         B     @RC00753                                         01S0761 02211000
@RF00753 DS    0H                                               01S0762 02212000
*       IF FIRSAPDS=0 THEN          /* OUR FIRST IS FIRST ON QUEUE   */ 02213000
         SLR   @10,@10                                          01S0762 02214000
         C     @10,FIRSAPDS                                     01S0762 02215000
         BNE   @RF00762                                         01S0762 02216000
*         IF LASTAPDS=0 THEN        /* OUR LAST IS LAST ON QUEUE     */ 02217000
         C     @10,LASTAPDS                                     01S0763 02218000
         BNE   @RF00763                                         01S0763 02219000
*           DO;                     /* THERE WILL BE A NULL QUEUE    */ 02220000
*             APCTAPDS=0;           /* NO FIRST APDS                 */ 02221000
         ST    @10,APCTAPDS(,APCTPTR)                           01S0765 02222000
*             APCTDSAP=0;           /* NO LAST APDS                  */ 02223000
         ST    @10,APCTDSAP(,APCTPTR)                           01S0766 02224000
*             ALLQRMV=BOFF;         /* REMOVAL IS COMPLETE           */ 02225000
         NI    ALLQRMV,B'11101111'                              01S0767 02226000
*           END;                    /* DO IF LASTAPDS = 0            */ 02227000
*         ELSE                      /* OUR LAST IS NOT LAST ON QUEUE */ 02228000
*           DO;                     /* HAVE TO DO IT ANYWAY          */ 02229000
         B     @RC00763                                         01S0769 02230000
@RF00763 DS    0H                                               01S0770 02231000
*             RFY                                               01S0770 02232000
*              (R2P,                                            01S0770 02233000
*               R3P) RSTD;          /* WILL USE THIS FOR NEW FIRST   */ 02234000
*             R3P=0;                /* USE FOR NEW FIRST'S PREVIOUS  */ 02235000
         SLR   R3P,R3P                                          01S0771 02236000
*             R2P=LASTAPDS;         /* POINTER IS IN REGISTER        */ 02237000
