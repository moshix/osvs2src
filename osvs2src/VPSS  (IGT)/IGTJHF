*               ALLSER=BON;         /* INDICATE ENQ RES (TO BE) OWNED*/ 00116000
         OI    ALLSER(ALLCWAPT),B'10000000'                     02S0094 00117000
*               IF R15P^=0&ENQRC^=8 THEN/* IF TASK DOES NOT HAVE        00118000
*                                      SHR'D                         */ 00119000
         LTR   R15P,R15P                                        02S0095 00120000
         BZ    @RF00095                                         02S0095 00121000
