TJHF     TITLE 'IGTJHF - 3838 SUBSYSTEM ALLOCATION RECOVERY ROUTINE.   *00001000
                        '                                               00002000
IGTJHF   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTJHF    77.092'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
*   SSDDRPTR=FIRSTARB;              /* POINT TO FIRST/ONLY REQUEST IN   00022000
*                                      CASE WAITING NOT ALLOWED      */ 00023000
         L     SSDDRPTR,FIRSTARB(,ALLCWAPT)                     01S0063 00024000
*                                                               02S0064 00025000
*   /*****************************************************************/ 00026000
*   /*                                                               */ 00027000
*   /* THIS SEGMENT CREATES AN ECB LIST FOR USE FOLLOWING AN ENQ WITH*/ 00028000
*   /* ECB OPTION.                                                   */ 00029000
*   /*                                                               */ 00030000
*   /*****************************************************************/ 00031000
*                                                               02S0064 00032000
*   DO;                             /* INSURE ENTIRE SEGMENT IS INCLD*/ 00033000
*     THEECBP=ADDR(ENQECB);         /* POINTER TO ENQ ECB            */ 00034000
         LA    @10,ENQECB                                       02S0065 00035000
         ST    @10,THEECBP                                      02S0065 00036000
*     CNCLECBP=ACNCLECB;            /* POINTER TO CANCEL ECB OR DUMMY*/ 00037000
         L     @10,ACNCLECB(,ALLCWAPT)                          02S0066 00038000
         ST    @10,CNCLECBP                                     02S0066 00039000
*     LASTECBI=BON;                 /* ADDRESSES SET, INDICATE LAST  */ 00040000
         OI    LASTECBI,B'10000000'                             02S0067 00041000
*   END;                            /* END OF SEGMENT.               */ 00042000
*   IF SSAGWAIT=BON THEN            /* IF WAITING IS ALLOWED         */ 00043000
