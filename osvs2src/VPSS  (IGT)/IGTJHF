*                       GPR01P) RSTD;/* REGISTERS USED               */ 00131000
*                     GPR01P=-ADDR(ECBLIST);/* POINT TO ECB LIST     */ 00132000
         LA    GPR01P,ECBLIST                                   02S0100 00133000
         LCR   GPR01P,GPR01P                                    02S0100 00134000
*                     GPR00F=1|'80000000'X;/* WAIT COUNT             */ 00135000
         L     GPR00F,@CF00846                                  02S0101 00136000
*                     SVC(1);       /* WAIT FOR ECB POSTING          */ 00137000
         SVC   1                                                02S0102 00138000
*                     RFY                                       02S0103 00139000
*                      (GPR00F,                                 02S0103 00140000
*                       GPR01P) UNRSTD;/* FINISHED WITH REGISTERS    */ 00141000
*                   END;            /* ECB WAIT COMPLETE LONG WAIT      00142000
*                                      WAIT FOR ONE TO COMPLETE      */ 00143000
*                   IF CNCLECBP->ECBPOSTB=BON THEN/* IF POSTED,      */ 00144000
         L     @10,CNCLECBP                                     02S0105 00145000
         TM    ECBPOSTB(@10),B'01000000'                        02S0105 00146000
         BNO   @RF00105                                         02S0105 00147000
*                     DO;           /* CANCELED. SO INDICATE.        */ 00148000
*                       ALLFAIL=BON;/* ALLOCATION & JOB FAIL         */ 00149000
*                                                               02S0107 00150000
         OI    ALLFAIL(ALLCWAPT),B'10000000'                    02S0107 00151000
*                       /*********************************************/ 00152000
*                       /*                                           */ 00153000
*                       /* IGTYJHE ERR(491)                          */ 00154000
*                       /*                                           */ 00155000
*                       /*********************************************/ 00156000
*                                                               02S0108 00157000
*                       RFY                                     02S0108 00158000
*                         R1F RSTD;                             02S0108 00159000
