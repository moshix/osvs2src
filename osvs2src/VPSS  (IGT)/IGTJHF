         L     @10,SSOBINDV(,@10)                               01S0069 00045000
         TM    SSAGWAIT(@10),B'10000000'                        01S0069 00046000
         BNO   @RF00069                                         01S0069 00047000
*     DO;                           /* THEN WE COULD WAIT            */ 00048000
*       IF ALLWAIT=BOFF THEN        /* AND WE HAVE NOT YET WAITED    */ 00049000
         TM    ALLWAIT(ALLCWAPT),B'01000000'                    01S0071 00050000
         BNZ   @RF00071                                         01S0071 00051000
*         CALL APCVZJHT;            /* TEST REASONABLENESS OF REQUEST   00052000
*                                      & ADEQUACY OF RESOURCES       */ 00053000
         L     @15,APCTZJHT(,APCTPTR)                           01S0072 00054000
         BALR  @14,@15                                          01S0072 00055000
*       IF ALLWAIT=BON&ALLFAIL=BOFF THEN/* WILL WAIT AGAIN OR FOR FOR   00056000
*                                      THE FIRST TIME                */ 00057000
@RF00071 TM    ALLWAIT(ALLCWAPT),B'01000000'                    01S0073 00058000
         BNO   @RF00073                                         01S0073 00059000
         TM    ALLFAIL(ALLCWAPT),B'10000000'                    01S0073 00060000
         BNZ   @RF00073                                         01S0073 00061000
*         DO;                       /* THEN EITHER HOLD OR NO HOLD   */ 00062000
*           IF ALLHOLD=BON THEN     /* IF WAIT HOLDING RESOURCES THEN*/ 00063000
         TM    ALLHOLD(ALLCWAPT),B'01000000'                    01S0075 00064000
         BNO   @RF00075                                         01S0075 00065000
*             DO;                   /* DO IT AGAIN. RESOURCES NOT YET   00066000
*                                      AVAILABLE.                    */ 00067000
*               DO;                 /* INSURE ENTIRE SEGMENT INCLUDED*/ 00068000
*                 GEN REFS(DEQLIST1,DQLIST1L,APCTENQM,APUBENQR) SETS(   00069000
*                     DYNDEQLA);                                02S0078 00070000
*                                   /*                                  00071000
         MVC   DYNDEQLA(L'DQLIST1L),DEQLIST1 INIT DYN DEQ AREA          00072000
         DEQ   (APCTENQM-APCT(,APCTPTR),APUBENQR,4,SYSTEM),            *00073000
               MF=(E,DYNDEQLA)                                          00074000
*                 ALLAPUB=BOFF;     /* NO LONGER CONTROL RESOURCE    */ 00075000
         NI    ALLAPUB(ALLCWAPT),B'11011111'                    02S0079 00076000
*               END;                                            02S0080 00077000
*               CALL APCVZJHW;      /* LOGICAL WAIT ON ALLOCATION       00078000
*                                      PENDING ECB.                  */ 00079000
         L     @15,APCTZJHW(,APCTPTR)                           01S0081 00080000
         BALR  @14,@15                                          01S0081 00081000
*             END;                                              01S0082 00082000
*           ELSE                    /* WAIT, NOT HOLDING RESOURCES   */ 00083000
*             DO;                   /* WAS SPECIFIED EARILIER. DO IT */ 00084000
         B     @RC00075                                         01S0083 00085000
@RF00075 DS    0H                                               01S0084 00086000
*               CALL APCVZJHO;      /* AGAIN. RELEASE RESOURCES.     */ 00087000
         L     @15,APCTZJHO(,APCTPTR)                           01S0084 00088000
         BALR  @14,@15                                          01S0084 00089000
*               DO;                 /* INSURE ENTIRE SEGMENT INCLUDED*/ 00090000
