         BZ    @RF00186                                         01S0186 00429000
*       DO;                         /* ATTEMPT TO SATISFY IT         */ 00430000
*         SMLAPUB=0;                /* PTR TO SMALLEST SUITABLE APUB */ 00431000
         SLR   SMLAPUB,SMLAPUB                                  01S0188 00432000
*         SMLREG='7FFF'X;           /* PARTITION SIZE OF SMLAPUB        00433000
*                                      INITIALIZED TO MAX VALUE      */ 00434000
