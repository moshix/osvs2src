TJHT     TITLE 'IGTJHT - SS ALLOCATION RECOVERY SUBROUTINE- TEST       *00001000
                        '                                               00002000
IGTJHT   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTJHT    77.092'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
*   DO;                             /* GET ENRTIRE SEGMENT           */ 00022000
*/*   SEGMENT DETERMINES THEN NUMBER OF PORTS REQUIRED BY JOB AND THE   00023000
*     BULK STORAGE REQUIREMENT FOR THOSE PORTS.                      */ 00024000
*                                                               02S0059 00025000
*     LRGSTSPR=0;                   /* LARGEST SHARED PORT REGION       00026000
*                                      REQUESTED                     */ 00027000
         SLR   LRGSTSPR,LRGSTSPR                                02S0059 00028000
*     NPNTOT=0;                     /* TOTAL # PORTS REQUIRED BY THIS   00029000
*                                      JOB                           */ 00030000
         SLR   @15,@15                                          02S0060 00031000
         STH   @15,NPNTOT(,ALLCWAPT)                            02S0060 00032000
*     BSRTOT=0;                     /* TOTAL BULK STORAGE REQUIRED BY   00033000
*                                      BY THIS JOB                   */ 00034000
         ST    @15,BSRTOT(,ALLCWAPT)                            02S0061 00035000
*     NUMIER=0;                     /* NUMBER OF REQUESTS FOR WHICH     00036000
*                                      AN INSTALLATION EXIT     02S0062 00037000
*                                      PREVIOUSLY DISAPPROVED AN        00038000
*                                      AVAILABLE PORT                */ 00039000
         STH   @15,NUMIER(,ALLCWAPT)                            02S0062 00040000
*     SSDDRPTR=FIRSTARB;            /* START WITH FIRST REQUEST BLK  */ 00041000
         L     SSDDRPTR,FIRSTARB(,ALLCWAPT)                     02S0063 00042000
*     DO UNTIL SSDDRPTR=0;          /* AND CONTINUE THROUGH LAST     */ 00043000
@DL00064 DS    0H                                               02S0065 00044000
*       APDSPTR=SSAGSSCM;           /* POINT TO APDS ASSOC W/ REQUEST*/ 00045000
         L     APDSPTR,SSAGSSCM(,SSDDRPTR)                      02S0065 00046000
*       IF APDSOWN=SHR THEN         /* IF THIS IS A SHARED PORT REQST*/ 00047000
         TM    APDSOWN(APDSPTR),B'10000000'                     02S0066 00048000
         BNZ   @RF00066                                         02S0066 00049000
*         IF APDSREG>=LRGSTSPR THEN /* AND THE LARGEST ONE YET       */ 00050000
         L     @15,APDSREG(,APDSPTR)                            02S0067 00051000
         CR    @15,LRGSTSPR                                     02S0067 00052000
         BL    @RF00067                                         02S0067 00053000
*           DO;                     /* SAVE RELEVANT INFORMATION     */ 00054000
*             LRGSTSPR=APDSREG;     /* SIZE OF REGION REQUESTED      */ 00055000
         LR    LRGSTSPR,@15                                     02S0069 00056000
*             LRGSTAPD=APDSPTR;     /* ADDRESS OF APDS               */ 00057000
         LR    LRGSTAPD,APDSPTR                                 02S0070 00058000
*           END;                    /* DO IF APDSREG >= LRGSTSPR     */ 00059000
*         ELSE                                                  02S0072 00060000
*           ;                       /* NO ACTION REQUIRED            */ 00061000
@RF00067 DS    0H                                               02S0073 00062000
*       ELSE                        /* EXCLUSIVE REQUEST             */ 00063000
*         DO;                       /* COLLECT THE DATA WE CAME FOR  */ 00064000
         B     @RC00066                                         02S0073 00065000
@RF00066 DS    0H                                               02S0074 00066000
*           NPNTOT=NPNTOT+1;        /* ONE PORT NEEDED FOR EACN EXC  */ 00067000
         LA    @15,1                                            02S0074 00068000
         AH    @15,NPNTOT(,ALLCWAPT)                            02S0074 00069000
         STH   @15,NPNTOT(,ALLCWAPT)                            02S0074 00070000
*           BSRTOT=BSRTOT+APDSREG;  /* REQUEST AND MORE BULK STORAGE */ 00071000
         L     @15,BSRTOT(,ALLCWAPT)                            02S0075 00072000
         AL    @15,APDSREG(,APDSPTR)                            02S0075 00073000
         ST    @15,BSRTOT(,ALLCWAPT)                            02S0075 00074000
*         END;                      /* ELSE EXCLUSIVE REQUEST        */ 00075000
*       IF APDSIER=BON THEN         /* INSTALLATION EXIT REJECTED    */ 00076000
@RC00066 TM    APDSIER(APDSPTR),B'00000100'                     02S0077 00077000
         BNO   @RF00077                                         02S0077 00078000
*         NUMIER=NUMIER+1;          /* ON MORE                       */ 00079000
         LA    @15,1                                            02S0078 00080000
         AH    @15,NUMIER(,ALLCWAPT)                            02S0078 00081000
         STH   @15,NUMIER(,ALLCWAPT)                            02S0078 00082000
*       SSDDRPTR=SSAGNRBP;          /* ADVANCE TO NEXT REQUEST / DONE*/ 00083000
@RF00077 L     SSDDRPTR,SSAGNRBP(,SSDDRPTR)                     02S0079 00084000
*     END;                          /* DO UNTIL SSDDRPTR = 0         */ 00085000
@DE00064 SLR   @15,@15                                          02S0080 00086000
         CR    SSDDRPTR,@15                                     02S0080 00087000
         BNE   @DL00064                                         02S0080 00088000
*     IF LRGSTSPR^=0 THEN           /* AT LEAST ONE SHARED REQUEST   */ 00089000
         CR    LRGSTSPR,@15                                     02S0081 00090000
         BE    @RF00081                                         02S0081 00091000
*       DO;                         /* INCLUDE DATA FOR IT           */ 00092000
*         NPNTOT=NPNTOT+1;          /* ONE MORE PORT TO HANDLE SHR'D    00093000
*                                      RESOURCE REQUESTS             */ 00094000
         LA    @05,1                                            02S0083 00095000
         AH    @05,NPNTOT(,ALLCWAPT)                            02S0083 00096000
         STH   @05,NPNTOT(,ALLCWAPT)                            02S0083 00097000
*         LRGSTAPD->APDSLSR=BON;    /* INDICATE LARGEST SHARED REQST */ 00098000
         OI    APDSLSR(LRGSTAPD),B'00000010'                    02S0084 00099000
*         BSRTOT=BSRTOT+LRGSTAPD->APDSREG;/* ADD BULK STOR REQUIRED  */ 00100000
         L     @05,BSRTOT(,ALLCWAPT)                            02S0085 00101000
         AL    @05,APDSREG(,LRGSTAPD)                           02S0085 00102000
         ST    @05,BSRTOT(,ALLCWAPT)                            02S0085 00103000
*       END;                        /* DO IF LRGSTSPR ^= 0           */ 00104000
*     IF NUMIER=NUMALREQ THEN       /* IF INST EXIT REJECTED EACH 1  */ 00105000
@RF00081 LH    @05,NUMIER(,ALLCWAPT)                            02S0087 00106000
         CL    @05,NUMALREQ(,ALLCWAPT)                          02S0087 00107000
         BNE   @RF00087                                         02S0087 00108000
*       EXITREJ=BON;                /* LET 1JHT KNOW ABOUT IT        */ 00109000
         OI    EXITREJ(ALLCWAPT),B'10000000'                    02S0088 00110000
*   END;                            /* END OF SEGMENT                */ 00111000
@RF00087 DS    0H                                               02S0090 00112000
*   DO;                             /* GET ENTIRE SEGMENT            */ 00113000
*/*   SEGMENT DETERMINES THE AVAILABILITY, ACTUAL AND POTENTIAL, OF     00114000
*     ALL APUBS ANCHORED BY THE SUBSYSTEM APCT.  IN ADDITION, THE       00115000
*     SIZES OF THE AVAILABLE PARTITONS ARE SUMMED TO DETERMINE WHETHER  00116000
*     THE JOB'S REQUESTS COULD BE SATISFIES WITHOUT RECONFIGURATION. */ 00117000
*                                                               02S0091 00118000
*     MSTRAPUB=APCTAPUB;            /* ADDRESS INITIAL APUB (MSTR)   */ 00119000
         L     MSTRAPUB,APCTAPUB(,APCTPTR)                      02S0091 00120000
*     DO UNTIL MSTRAPUB=0;          /* DO ONCE FOR EACH MASTER APUB  */ 00121000
@DL00092 DS    0H                                               02S0093 00122000
*       APUBPTR=MSTRAPUB->APUBAPUB; /* POINT TO FIRST USER SUBCHANNEL*/ 00123000
         L     APUBPTR,APUBAPUB(,MSTRAPUB)                      02S0093 00124000
*       IF APUBUNLD=BON THEN        /* IF THE 3838 IS UNLOADED OR    */ 00125000
         TM    APUBUNLD(APUBPTR),B'00100000'                    02S0094 00126000
         BNO   @RF00094                                         02S0094 00127000
*         DO;                       /* ABOUT TO BE                   */ 00128000
*           USCUNLD=USCUNLD+USCP3838;/* ASSUME MAX # SUBS PER 3838   */ 00129000
         LA    @05,7                                            02S0096 00130000
         AH    @05,USCUNLD(,ALLCWAPT)                           02S0096 00131000
         STH   @05,USCUNLD(,ALLCWAPT)                           02S0096 00132000
*           BSUNLD=BSUNLD+BSP3838;  /* ASSUME MAX BULK STORAGE SIZE  */ 00133000
         LA    @05,1024                                         02S0097 00134000
         AH    @05,BSUNLD(,ALLCWAPT)                            02S0097 00135000
         STH   @05,BSUNLD(,ALLCWAPT)                            02S0097 00136000
*         END;                      /* DO IF APUBUNLD = BON          */ 00137000
*       ELSE                        /* 3838 IS IPLED. EXAMINE EACH SC*/ 00138000
*         DO UNTIL APUBPTR=0;       /* ONCE PER NON-CNTL SUBCHANNEL  */ 00139000
         B     @RC00094                                         02S0099 00140000
@RF00094 DS    0H                                               02S0099 00141000
@DL00099 DS    0H                                               02S0100 00142000
*           APUBALF3=0;             /* CLEAR TEMPORARY INDICATORS    */ 00143000
         MVI   APUBALF3(APUBPTR),X'00'                          02S0100 00144000
*           IF APUBZRGN=BOFF&       /* IF PORT HAS BULK STOR ASSIGND */ 00145000
*               APUBBERR=BOFF THEN  /* AND NOT TOO MANY ERRORS       */ 00146000
         TM    APUBZRGN(APUBPTR),B'10010000'                    02S0101 00147000
         BNZ   @RF00101                                         02S0101 00148000
*             DO;                   /* PORT IS AVAILABLE             */ 00149000
*               USCAVAIL=USCAVAIL+1;/* ONE MORE PORT AVAILABLE       */ 00150000
         LA    @05,1                                            02S0103 00151000
         AH    @05,USCAVAIL(,ALLCWAPT)                          02S0103 00152000
         STH   @05,USCAVAIL(,ALLCWAPT)                          02S0103 00153000
*               BSAVAIL=BSAVAIL+APUBRGN;/* INCLUDE PORT'S BULK STOR  */ 00154000
         LH    @05,BSAVAIL(,ALLCWAPT)                           02S0104 00155000
         AH    @05,APUBRGN(,APUBPTR)                            02S0104 00156000
         STH   @05,BSAVAIL(,ALLCWAPT)                           02S0104 00157000
*               APUBSCAV=BON;       /* SUBCHANNEL AVAILABLE FOR ALLOC*/ 00158000
         L     @05,APUBSCAV-3(,APUBPTR)                         02S0105 00159000
@GL00001 LR    @03,@05                                          02S0105 00160000
         O     @03,@CF00902                                     02S0105 00161000
         CS    @05,@03,APUBSCAV-3(APUBPTR)                      02S0105 00162000
         BNE   @GL00001                                         02S0105 00163000
*             END;                  /* DO IF PORT IS AVAILABLE       */ 00164000
*           ELSE                    /* PORT IS NOT "AVAILABLE"       */ 00165000
*             DO;                   /* INCLUDE INFO IN DATA FOR UNLD */ 00166000
         B     @RC00101                                         02S0107 00167000
@RF00101 DS    0H                                               02S0108 00168000
*               USCUNLD=USCUNLD+1;  /* ONE MORE PORT                 */ 00169000
         LA    @05,1                                            02S0108 00170000
         AH    @05,USCUNLD(,ALLCWAPT)                           02S0108 00171000
         STH   @05,USCUNLD(,ALLCWAPT)                           02S0108 00172000
*               BSUNLD=BSUNLD+APUBRGN;/* RGN IS REAL OR ZERO         */ 00173000
         LH    @05,BSUNLD(,ALLCWAPT)                            02S0109 00174000
         AH    @05,APUBRGN(,APUBPTR)                            02S0109 00175000
         STH   @05,BSUNLD(,ALLCWAPT)                            02S0109 00176000
*             END;                  /* ELSE PORT IS NOT AVAILABLE    */ 00177000
*           APUBPTR=APUBAPUB;       /* ON TO NEXT OR DONE            */ 00178000
@RC00101 L     APUBPTR,APUBAPUB(,APUBPTR)                       02S0111 00179000
*         END;                      /* DO UNTIL APUBPTR = 0          */ 00180000
@DE00099 LTR   APUBPTR,APUBPTR                                  02S0112 00181000
         BNZ   @DL00099                                         02S0112 00182000
*       MSTRAPUB=MSTRAPUB->APUBMSTR;/* ADVANCE TO NEXT MASTER / DON  */ 00183000
@RC00094 L     MSTRAPUB,APUBMSTR(,MSTRAPUB)                     02S0113 00184000
*     END;                          /* DO UNTIL MSTRAPUB = 0         */ 00185000
@DE00092 LTR   MSTRAPUB,MSTRAPUB                                02S0114 00186000
         BNZ   @DL00092                                         02S0114 00187000
*   END;                            /* END OF SEGMENT                */ 00188000
*   IF NPNTOT>USCAVAIL+USCUNLD THEN /* IF TOTAL # OF PORTS NEEDED >  */ 00189000
         LH    @05,USCAVAIL(,ALLCWAPT)                          01S0116 00190000
         AH    @05,USCUNLD(,ALLCWAPT)                           01S0116 00191000
         CH    @05,NPNTOT(,ALLCWAPT)                            01S0116 00192000
         BNL   @RF00116                                         01S0116 00193000
*     DO;                           /* TOTAL # OF NON-CONTROL SUBCHAN*/ 00194000
*       EXITIMPP=BON;               /* NELS IT IS IMPOSSIBLE TO      */ 00195000
         OI    EXITIMPP(ALLCWAPT),B'10000000'                   01S0118 00196000
*       ALLFAIL=BON;                /* SATISFY THE USER'S REQUESTS   */ 00197000
*                                                               01S0119 00198000
         OI    ALLFAIL(ALLCWAPT),B'10000000'                    01S0119 00199000
*       /*************************************************************/ 00200000
*       /*                                                           */ 00201000
*       /* IGTYJHE ERR(478)                                          */ 00202000
*       /*                                                           */ 00203000
*       /*************************************************************/ 00204000
*                                                               01S0120 00205000
*       RFY                                                     01S0120 00206000
*         R1F RSTD;                                             01S0120 00207000
*       R1F=19;                     /* PROVIDE INDEX                 */ 00208000
         LA    R1F,19                                           01S0121 00209000
*       CALL APCVZJHE;              /* SPECIFY ERROR INFORMATION        00210000
*                                      PRIOR TO RETURN               */ 00211000
         L     @15,APCTZJHE(,APCTPTR)                           01S0122 00212000
         BALR  @14,@15                                          01S0122 00213000
*       RFY                                                     01S0123 00214000
*         R1F UNRSTD;               /* DONE WITH REG INDICATE PROBLEM   00215000
*                                      TO USER                       */ 00216000
*     END;                          /* DO IF INSUFFICIENT PORTS      */ 00217000
*   ELSE                            /* SUFFICIENT # PORTS MAY EXIST  */ 00218000
*     IF BSRTOT>BSAVAIL+BSUNLD THEN /* NOT ENOUGH BULK STORAGE TO    */ 00219000
         B     @RC00116                                         01S0125 00220000
@RF00116 LH    @05,BSAVAIL(,ALLCWAPT)                           01S0125 00221000
         AH    @05,BSUNLD(,ALLCWAPT)                            01S0125 00222000
         C     @05,BSRTOT(,ALLCWAPT)                            01S0125 00223000
         BNL   @RF00125                                         01S0125 00224000
*       DO;                         /* SATISFY REQUESTS              */ 00225000
*         ALLFAIL=BON;              /* INDICATE ALLOCATION FAILS     */ 00226000
         OI    ALLFAIL(ALLCWAPT),B'10000000'                    01S0127 00227000
*         EXITIMPS=BON;             /* INSUFFICIENT STORAGE          */ 00228000
*                                                               01S0128 00229000
         OI    EXITIMPS(ALLCWAPT),B'01000000'                   01S0128 00230000
*         /***********************************************************/ 00231000
*         /*                                                         */ 00232000
*         /* IGTYJHE ERR(479)                                        */ 00233000
*         /*                                                         */ 00234000
*         /***********************************************************/ 00235000
*                                                               01S0129 00236000
*         RFY                                                   01S0129 00237000
*           R1F RSTD;                                           01S0129 00238000
*         R1F=20;                   /* PROVIDE INDEX                 */ 00239000
         LA    R1F,20                                           01S0130 00240000
*         CALL APCVZJHE;            /* SPECIFY ERROR INFORMATION        00241000
*                                      PRIOR TO RETURN               */ 00242000
         L     @15,APCTZJHE(,APCTPTR)                           01S0131 00243000
         BALR  @14,@15                                          01S0131 00244000
*         RFY                                                   01S0132 00245000
*           R1F UNRSTD;             /* DONE WITH REG INDICATE PROBLEM   00246000
*                                      TO USER                       */ 00247000
*       END;                        /* DO IF BSRTOT > ...            */ 00248000
*     ELSE                          /* SUFFICIENT BULK STOR MAY EXIST*/ 00249000
*       DO;                         /* TO SATISFY THE REQUESTS       */ 00250000
*                                                               01S0134 00251000
         B     @RC00125                                         01S0134 00252000
@RF00125 DS    0H                                               01S0135 00253000
*         /***********************************************************/ 00254000
*         /*                                                         */ 00255000
*         /* ATTEMPT TO SATISFY THE USER'S REQUEST ASSUMING THAT ALL */ 00256000
*         /* LOADED PORTS NOT HAVING ZERO REGIONS OR EXCESSIVE ERRORS*/ 00257000
*         /* ARE AVAILABLE.                                          */ 00258000
*         /*                                                         */ 00259000
*         /***********************************************************/ 00260000
*                                                               01S0135 00261000
*         ALLMARK=BON;              /* TRYALLOC IS TO PROVIDE DD        00262000
*                                      LEVEL ERROR INFORMATION       */ 00263000
         OI    ALLMARK(ALLCWAPT),B'00010000'                    01S0135 00264000
*         CALL TRYALLOC;            /* PERFORM HYPOTHETICAL ALLOCATN */ 00265000
         BAL   @14,TRYALLOC                                     01S0136 00266000
*         IF NUMNTSAT=0 THEN        /* ALL REQUESTS WERE SATISFIED   */ 00267000
         LH    @05,NUMNTSAT(,ALLCWAPT)                          01S0137 00268000
         LTR   @05,@05                                          01S0137 00269000
         BNZ   @RF00137                                         01S0137 00270000
*           DO;                     /* ALLOCATION COULD SUCCEED W/0  */ 00271000
*             EXITALOK=BON;         /* RECONFIGURATION OF RESOURCES  */ 00272000
         OI    EXITALOK(ALLCWAPT),B'00100000'                   01S0139 00273000
*             DO;                   /* GET ENTIRE SEGMENT            */ 00274000
*/*   SEGMENT DETERMINES THE ACTUAL AVAILABILITY OF ALL APUBS.       */ 00275000
*                                                               02S0141 00276000
*               MSTRAPUB=APCTAPUB;  /* ADDRESS INITIAL APUB (MSTR)   */ 00277000
         L     MSTRAPUB,APCTAPUB(,APCTPTR)                      02S0141 00278000
*               DO UNTIL MSTRAPUB=0;/* DO ONCE FOR EACH MASTER APUB  */ 00279000
@DL00142 DS    0H                                               02S0143 00280000
*                 APUBPTR=MSTRAPUB->APUBAPUB;/* POINT TO FIRST USER     00281000
*                                      SUBCHANNEL                    */ 00282000
         L     APUBPTR,APUBAPUB(,MSTRAPUB)                      02S0143 00283000
*                 IF APUBUNLD=BOFF THEN/* IF THE 3838 IS UNLOADED DO */ 00284000
         TM    APUBUNLD(APUBPTR),B'00100000'                    02S0144 00285000
         BNZ   @RF00144                                         02S0144 00286000
*                   DO UNTIL APUBPTR=0;/* ONCE PER NON-CNTL     02S0145 00287000
*                                      SUBCHANNEL                    */ 00288000
@DL00145 DS    0H                                               02S0146 00289000
*                     APUBALF3=0;   /* CLEAR TEMPORARY INDICATORS    */ 00290000
         MVI   APUBALF3(APUBPTR),X'00'                          02S0146 00291000
*                     IF APUBZRGN=BOFF&/* IF PORT HAS BULK STOR 02S0147 00292000
*                                      ASSIGND                       */ 00293000
*                         APUBDSBL=BOFF&/* AND IS NOT DISABLED AND   */ 00294000
*                         APUBBERR=BOFF THEN/* AND NOT TOO MANY 02S0147 00295000
*                                      ERRORS                        */ 00296000
         TM    APUBZRGN(APUBPTR),B'11010000'                    02S0147 00297000
         BNZ   @RF00147                                         02S0147 00298000
*                       APUBSCAV=BON;/* SUBCHANNEL AVAILABLE FOR        00299000
*                                      ALLOC                         */ 00300000
         L     @05,APUBSCAV-3(,APUBPTR)                         02S0148 00301000
@GL00002 LR    @03,@05                                          02S0148 00302000
         O     @03,@CF00902                                     02S0148 00303000
         CS    @05,@03,APUBSCAV-3(APUBPTR)                      02S0148 00304000
         BNE   @GL00002                                         02S0148 00305000
*                     APUBPTR=APUBAPUB;/* ON TO NEXT OR DONE         */ 00306000
@RF00147 L     APUBPTR,APUBAPUB(,APUBPTR)                       02S0149 00307000
*                   END;            /* DO UNTIL APUBPTR = 0          */ 00308000
@DE00145 LTR   APUBPTR,APUBPTR                                  02S0150 00309000
         BNZ   @DL00145                                         02S0150 00310000
*                 MSTRAPUB=MSTRAPUB->APUBMSTR;/* ADVANCE TO NEXT        00311000
*                                      MASTER / DON                  */ 00312000
@RF00144 L     MSTRAPUB,APUBMSTR(,MSTRAPUB)                     02S0151 00313000
*               END;                /* DO UNTIL MSTRAPUB = 0         */ 00314000
@DE00142 LTR   MSTRAPUB,MSTRAPUB                                02S0152 00315000
         BNZ   @DL00142                                         02S0152 00316000
*             END;                  /* END OF SEGMENT                */ 00317000
*             ALLMARK=BOFF;         /* TRYALLOC IS NOT TO PROVIDE MSG*/ 00318000
         NI    ALLMARK(ALLCWAPT),B'11101111'                    01S0154 00319000
*             CALL TRYALLOC;        /* PERFORM HYPOTHETICAL ALLOCATN */ 00320000
         BAL   @14,TRYALLOC                                     01S0155 00321000
*             IF NUMNTSAT^=0 THEN   /* IF ENABLING OF 1 OR MORE PORTS*/ 00322000
         LH    @05,NUMNTSAT(,ALLCWAPT)                          01S0156 00323000
         LTR   @05,@05                                          01S0156 00324000
         BZ    @RF00156                                         01S0156 00325000
*               EXITENRQ=BON;       /* IS REQUIRED THEN SO INDICATE  */ 00326000
         OI    EXITENRQ(ALLCWAPT),B'00010000'                   01S0157 00327000
*           END;                    /* FOR INSTALLATION EXIT         */ 00328000
*         ELSE                      /* NOT ALL REQUESTS WERE SATISFID*/ 00329000
*           IF USCUNLD=0 THEN       /* IF NO UNLOADED 3838S EXIST    */ 00330000
         B     @RC00137                                         01S0159 00331000
@RF00137 LH    @05,USCUNLD(,ALLCWAPT)                           01S0159 00332000
         LTR   @05,@05                                          01S0159 00333000
         BNZ   @RF00159                                         01S0159 00334000
*             EXITRCON=BON;         /* RECONFIGURATION IS REQUIRED   */ 00335000
         OI    EXITRCON(ALLCWAPT),B'00001000'                   01S0160 00336000
*       END;                        /* DO IF SUFFICIENT BS MAY EXIST */ 00337000
@RF00159 DS    0H                                               01S0161 00338000
@RC00137 DS    0H                                               01S0162 00339000
*   ALLINST=BON;                    /* INDICATE INST EXIT IN CONTROL */ 00340000
@RC00125 DS    0H                                               01S0162 00341000
@RC00116 OI    ALLINST(ALLCWAPT),B'00100000'                    01S0162 00342000
*   CALL APCT1JHT(ALLCWA);          /* FINALLY CALL INST EXIT        */ 00343000
         ST    ALLCWAPT,@AL00001                                01S0163 00344000
         L     @15,APCT1JHT(,APCTPTR)                           01S0163 00345000
         LA    @01,@AL00001                                     01S0163 00346000
         BALR  @14,@15                                          01S0163 00347000
*   ALLINST=BOFF;                   /* IND INST EXIT NOT IN CONTROL  */ 00348000
         NI    ALLINST(ALLCWAPT),B'11011111'                    01S0164 00349000
*   IF EXITCOMM=BOFF THEN           /* IF OPERATOR DIALOGUE TO BE    */ 00350000
