*                       R10RSTD,                                02S0296 00676000
*                       R12RSTD,                                02S0296 00677000
*                       R14RSTD,                                02S0296 00678000
*                       R15RSTD) UNRSTD;                        02S0296 00679000
*                   END;            /* END OF CODE SEGMENT           */ 00680000
*DOVUSE:                                                        01S0298 00681000
*                   APESRTRY=ADDR(APUBDEQ);/* ADVANCE RETRY POINT    */ 00682000
DOVUSE   L     @03,APESPTR                                      01S0298 00683000
         L     @15,APESREGP(,@03)                               01S0298 00684000
         LA    @00,APUBDEQ                                      01S0298 00685000
         ST    @00,APESRTRY(,@15)                               01S0298 00686000
*                   DO;             /* ERROR RECOVERY COMPLETE       */ 00687000
*                     APESFPRV=BOFF;/* RESET RECURSION FLAG.         */ 00688000
         NI    APESFPRV(@03),B'10111111'                        02S0300 00689000
*                     APESCODE=CERRN99;/* RESET ERROR CODE           */ 00690000
         MVI   APESCODE(@03),X'01'                              02S0301 00691000
*                     APESFDMP=BON; /* RESET DUMP/LOG FLAGS          */ 00692000
*                     APESFREC=BON; /* TO THEIR NORMAL VALUES        */ 00693000
*                     APESFSDP=BON; /* (ALL ON)                      */ 00694000
         OI    APESFDMP(@03),B'11100000'                        02S0304 00695000
*                   END;            /* ALL INDICATORS RESET          */ 00696000
*                   IF ALLNLAA=BOFF THEN/* IF LAST ALLOCATD THEN ADDR   00697000
*                                      SPACE HAS NO SUBSYS RESOURCES    00698000
*                                      SO INDICATE IN APVU.          */ 00699000
         TM    ALLNLAA,B'10000000'                              01S0306 00700000
         BNZ   @RF00306                                         01S0306 00701000
*                     APVUVUSE(PSAAOLD->ASCBASID)=BOFF;/* NO    01S0307 00702000
*                                      RESOURCES                     */ 00703000
         L     @09,PSAAOLD                                      01S0307 00704000
         LH    @09,ASCBASID(,@09)                               01S0307 00705000
         N     @09,@CF03140                                     01S0307 00706000
