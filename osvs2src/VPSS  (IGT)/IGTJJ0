*                       R10RSTD,                                02S0310 00745000
*                       R12RSTD,                                02S0310 00746000
*                       R14RSTD,                                02S0310 00747000
*                       R15RSTD) UNRSTD;                        02S0310 00748000
*                   END;            /* END OF CODE SEGMENT           */ 00749000
*APUBDEQ:                                                       01S0312 00750000
*                   APESRTRY=ADDR(NOTIFY);/* ADVANCE RETRY POINT     */ 00751000
APUBDEQ  L     @03,APESPTR                                      01S0312 00752000
         L     @15,APESREGP(,@03)                               01S0312 00753000
