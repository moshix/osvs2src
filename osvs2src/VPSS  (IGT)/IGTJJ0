         OI    ALLFAIL,B'10000000'                              02S0156 00221000
*             SSOBRETN=SSALUNAL;    /* PROVIDE NON ZERO RETURN CODE  */ 00222000
         L     @01,SSOBPTR                                      02S0157 00223000
         MVC   SSOBRETN(4,@01),@CF00193                         02S0157 00224000
*           END;                                                02S0158 00225000
*       END;                        /* ESTAE ENVIRONMENT SET         */ 00226000
@RC00152 DS    0H                                               02S0160 00227000
*       IF ALLERREN='1'B THEN       /* ERROR RECOVERY ENV ESTABLISHED*/ 00228000
         TM    ALLERREN,B'00100000'                             01S0160 00229000
         BNO   @RF00160                                         01S0160 00230000
*         DO;                       /* SERIALIZE ACCESS TO      01S0161 00231000
*                                      APUBS/APDSS                   */ 00232000
*           APESRTRY=ADDR(FINDAPDS);/* ADVANCE POINT OF RETRY        */ 00233000
         L     @01,APESPTR                                      01S0162 00234000
         L     @01,APESREGP(,@01)                               01S0162 00235000
         LA    @00,FINDAPDS                                     01S0162 00236000
         ST    @00,APESRTRY(,@01)                               01S0162 00237000
*           ENQECB=0;               /* CLEAR ENQ ECB                 */ 00238000
         SLR   @15,@15                                          02S0163 00239000
         ST    @15,ENQECB                                       02S0163 00240000
*           RFY                                                 02S0164 00241000
*             R15P RSTD;            /* NEED REGISTER FOR ENQ RET CODE*/ 00242000
*           GEN REFS(APCTENQM,APUBENQR,NQLISTEL,ENQLISTE) SETS(DYNQLIST 00243000
*               );                                              02S0165 00244000
*                                   /*                                  00245000
         MVC   DYNQLIST(L'NQLISTEL),ENQLISTE INIT ENQ DYN LIST AREA     00246000
         ENQ   (APCTENQM-APCT(,APCTPTR),APUBENQR,E,4,SYSTEM),          *00247000
               ECB=ENQECB,MF=(E,DYNQLIST)                               00248000
*           IF R15P^=0&ENQRC^=8 THEN/* IF TASK DOES NOT HAVE SHR'D   */ 00249000
         LTR   R15P,R15P                                        02S0166 00250000
         BZ    @RF00166                                         02S0166 00251000
         CLI   ENQRC(R15P),8                                    02S0166 00252000
         BE    @RF00166                                         02S0166 00253000
*             DO;                   /* CONTROL OR RESOURCE THEN WAIT */ 00254000
*               RFY                                             02S0168 00255000
*                 R15P UNRSTD;      /* THROUGH WITH ENQ RETURN CODE  */ 00256000
*               DO;                 /* IGTYWAIT ECB(ENQECB)LONG(NO)  */ 00257000
*                 RFY                                           02S0170 00258000
*                  (GPR00F,                                     02S0170 00259000
*                   GPR01P) RSTD;   /* REGISTERS USED                */ 00260000
*                 DCL                                           02S0171 00261000
*                   BIT0015X BIT(8) BASED(ADDR(ENQECB));/* OVERLAY      00262000
*                                      BYTE-0 OF ECB                 */ 00263000
*                 IF BIT0015X(2)='0'B THEN/* IF ECB NOT ALREADY 02S0172 00264000
*                                      POSTED                        */ 00265000
         LA    @15,ENQECB                                       02S0172 00266000
         TM    BIT0015X(@15),B'01000000'                        02S0172 00267000
         BNZ   @RF00172                                         02S0172 00268000
*                   DO;             /* THEN ISSUE WAIT SVC           */ 00269000
*                     GPR01P=ADDR(ENQECB);/* POINT TO ECB            */ 00270000
         LR    GPR01P,@15                                       02S0174 00271000
*                     GPR00F=1;     /* WAIT COUNT                    */ 00272000
         LA    GPR00F,1                                         02S0175 00273000
*                     SVC(1);       /* WAIT FOR ECB POSTING          */ 00274000
         SVC   1                                                02S0176 00275000
*                   END;            /* ECB WAIT COMPLETE             */ 00276000
*                 RFY                                           02S0178 00277000
*                  (GPR00F,                                     02S0178 00278000
*                   GPR01P) UNRSTD; /* FINISHED WITH REGISTERS       */ 00279000
@RF00172 DS    0H                                               02S0179 00280000
*               END;                /* ECB WAIT COMPLETE WAIT FOR       00281000
*                                      APUB ENQ TO COMPLETE          */ 00282000
*             END;                                              02S0180 00283000
*           ALLAPUB=BON;            /* INDICATE ENQ RES (TO BE) OWNED*/ 00284000
@RF00166 OI    ALLAPUB,B'00100000'                              02S0181 00285000
*           IF ALLAPUB=BON THEN     /* CURRENT TASK CONTROLS RESOURCE*/ 00286000
         TM    ALLAPUB,B'00100000'                              01S0182 00287000
         BNO   @RF00182                                         01S0182 00288000
*             DO;                   /* LOOK FOR APDS ON APDS QUEUE   */ 00289000
*                                                               01S0183 00290000
*               /*****************************************************/ 00291000
*               /*                                                   */ 00292000
*               /* WAITING IN SS ALLOC FOR 3838 RESOURCES            */ 00293000
*               /*                                                   */ 00294000
*               /*****************************************************/ 00295000
*                                                               01S0184 00296000
*               DO;                 /* BEGIN CODE SEGMENT            */ 00297000
*                 RFY                                           02S0185 00298000
*                  (R0RSTD,                                     02S0185 00299000
*                   R1RSTD,                                     02S0185 00300000
*                   R2RSTD,                                     02S0185 00301000
*                   R3RSTD,                                     02S0185 00302000
*                   R4RSTD,                                     02S0185 00303000
*                   R5RSTD,         /* 6 & 7 BASES                   */ 00304000
*                   R8RSTD,                                     02S0185 00305000
*                   R9RSTD,                                     02S0185 00306000
*                   R10RSTD,                                    02S0185 00307000
*                   R12RSTD,                                    02S0185 00308000
*                   R14RSTD,                                    02S0185 00309000
*                   R15RSTD) RSTD;                              02S0185 00310000
*                 RFY                                           02S0186 00311000
*                  (R0RSTD,                                     02S0186 00312000
*                   R1RSTD,                                     02S0186 00313000
*                   R2RSTD,                                     02S0186 00314000
*                   R3RSTD,                                     02S0186 00315000
*                   R4RSTD,                                     02S0186 00316000
*                   R5RSTD,         /* 6 & 7 BASES                   */ 00317000
*                   R8RSTD,                                     02S0186 00318000
*                   R9RSTD,                                     02S0186 00319000
*                   R10RSTD,                                    02S0186 00320000
*                   R12RSTD,                                    02S0186 00321000
*                   R14RSTD,                                    02S0186 00322000
*                   R15RSTD) UNRSTD;                            02S0186 00323000
*               END;                /* END OF CODE SEGMENT           */ 00324000
*FINDAPDS:                                                      01S0188 00325000
*               APESRTRY=ADDR(FIXQUEUE);/* ADVANCE RETRY POINT       */ 00326000
FINDAPDS L     @15,APESPTR                                      01S0188 00327000
         L     @01,APESREGP(,@15)                               01S0188 00328000
         LA    @00,FIXQUEUE                                     01S0188 00329000
         ST    @00,APESRTRY(,@01)                               01S0188 00330000
*               DO;                 /* ERROR RECOVERY COMPLETE       */ 00331000
*                 APESFPRV=BOFF;    /* RESET RECURSION FLAG.         */ 00332000
         NI    APESFPRV(@15),B'10111111'                        02S0190 00333000
*                 APESCODE=CERRN99; /* RESET ERROR CODE              */ 00334000
         MVI   APESCODE(@15),X'01'                              02S0191 00335000
*                 APESFDMP=BON;     /* RESET DUMP/LOG FLAGS          */ 00336000
*                 APESFREC=BON;     /* TO THEIR NORMAL VALUES        */ 00337000
*                 APESFSDP=BON;     /* (ALL ON)                      */ 00338000
         OI    APESFDMP(@15),B'11100000'                        02S0194 00339000
*               END;                /* ALL INDICATORS RESET          */ 00340000
*               DO;                 /* INSURE ENTIRE SEGMENT INCLUDED*/ 00341000
*                 RFY                                           02S0197 00342000
*                  (R1P,                                        02S0197 00343000
*                   R2P) RSTD;      /* USES REGS FOR APDS QUE, ASCB  */ 00344000
*                 R1P=APCTAPDS;     /* START SCAN FOR APDS WITH FIRST*/ 00345000
         L     R1P,APCTAPDS(,APCTPTR)                           02S0198 00346000
*                 R2P=APDSASCB;     /* ASCB TO WHICH 3838 IS ALLOCATD*/ 00347000
         L     R2P,APDSASCB(,APDSPTR)                           02S0199 00348000
*                 DO WHILE R1P^=0;  /* DO ONCE FOR EACH APDS ON QUEUE*/ 00349000
         B     @DE00200                                         02S0200 00350000
@DL00200 DS    0H                                               02S0201 00351000
*                   IF R1P=APDSPTR THEN/* IF WE'VE LOCATED THE APDS     00352000
*                                      THEN                          */ 00353000
         CR    R1P,APDSPTR                                      02S0201 00354000
         BNE   @RF00201                                         02S0201 00355000
*                     DO;           /* WE CAN                        */ 00356000
*                       ALLQUE=BON; /* INDICATE THAT IT'S ON THE QUE */ 00357000
         OI    ALLQUE,B'01000000'                               02S0203 00358000
*                       ALLAPDS=BON;/* AND THAT THE STORAGE EXISTS   */ 00359000
         OI    ALLAPDS,B'00010000'                              02S0204 00360000
*                     END;          /* DO IF R1P = APDSPTR           */ 00361000
*                   ELSE            /* OTHERWISE, IF CURRENT APDS IS */ 00362000
*                     IF R2P=R1P->APDSASCB THEN/* ALLOCATED TO THE      00363000
*                                      SAME ADDRESS                  */ 00364000
         B     @RC00201                                         02S0206 00365000
@RF00201 C     R2P,APDSASCB(,R1P)                               02S0206 00366000
         BNE   @RF00206                                         02S0206 00367000
*                       ALLNLAA=BON;/* SPACE, INDICATE NOT LAST APDS    00368000
*                                      ALLOCATED TO ADDRESS SPACE    */ 00369000
         OI    ALLNLAA,B'10000000'                              02S0207 00370000
*                   R1P=R1P->APDSAPDS;/* ADVANCE TO NEXT APDS OR DONE*/ 00371000
@RF00206 DS    0H                                               02S0208 00372000
@RC00201 L     R1P,APDSAPDS(,R1P)                               02S0208 00373000
*                 END;              /* DO WHILE APDSS TO LOOK AT     */ 00374000
@DE00200 LTR   R1P,R1P                                          02S0209 00375000
         BNZ   @DL00200                                         02S0209 00376000
*                 ALLSRCH=BON;      /* INDICATE SEARCH FOR APDS DONE */ 00377000
         OI    ALLSRCH,B'00000100'                              02S0210 00378000
*               END;                /* END OF SEGMENT                */ 00379000
*               GOTO UNALLOC;       /* ON TO NEXT SEGMENT            */ 00380000
         B     UNALLOC                                          01S0212 00381000
*               DO;                 /* BEGIN CODE SEGMENT            */ 00382000
*                 RFY                                           02S0214 00383000
*                  (R0RSTD,                                     02S0214 00384000
*                   R1RSTD,                                     02S0214 00385000
*                   R2RSTD,                                     02S0214 00386000
*                   R3RSTD,                                     02S0214 00387000
*                   R4RSTD,                                     02S0214 00388000
*                   R5RSTD,         /* 6 & 7 BASES                   */ 00389000
*                   R8RSTD,                                     02S0214 00390000
*                   R9RSTD,                                     02S0214 00391000
*                   R10RSTD,                                    02S0214 00392000
*                   R12RSTD,                                    02S0214 00393000
*                   R14RSTD,                                    02S0214 00394000
*                   R15RSTD) RSTD;                              02S0214 00395000
*                 RFY                                           02S0215 00396000
*                  (R0RSTD,                                     02S0215 00397000
*                   R1RSTD,                                     02S0215 00398000
*                   R2RSTD,                                     02S0215 00399000
*                   R3RSTD,                                     02S0215 00400000
*                   R4RSTD,                                     02S0215 00401000
*                   R5RSTD,         /* 6 & 7 BASES                   */ 00402000
*                   R8RSTD,                                     02S0215 00403000
*                   R9RSTD,                                     02S0215 00404000
*                   R10RSTD,                                    02S0215 00405000
*                   R12RSTD,                                    02S0215 00406000
*                   R14RSTD,                                    02S0215 00407000
*                   R15RSTD) UNRSTD;                            02S0215 00408000
*               END;                /* END OF CODE SEGMENT           */ 00409000
*FIXQUEUE:                                                      01S0217 00410000
*               APESRTRY=ADDR(UNALLOC);/* ADVANCE RETRY POINT        */ 00411000
FIXQUEUE L     @03,APESPTR                                      01S0217 00412000
         L     @15,APESREGP(,@03)                               01S0217 00413000
         LA    @00,UNALLOC                                      01S0217 00414000
         ST    @00,APESRTRY(,@15)                               01S0217 00415000
*               DO;                 /* ERROR RECOVERY COMPLETE       */ 00416000
*                 APESFPRV=BOFF;    /* RESET RECURSION FLAG.         */ 00417000
         NI    APESFPRV(@03),B'10111111'                        02S0219 00418000
*                 APESCODE=CERRN99; /* RESET ERROR CODE              */ 00419000
         MVI   APESCODE(@03),X'01'                              02S0220 00420000
*                 APESFDMP=BON;     /* RESET DUMP/LOG FLAGS          */ 00421000
*                 APESFREC=BON;     /* TO THEIR NORMAL VALUES        */ 00422000
*                 APESFSDP=BON;     /* (ALL ON)                      */ 00423000
         OI    APESFDMP(@03),B'11100000'                        02S0223 00424000
*               END;                /* ALL INDICATORS RESET          */ 00425000
*               CALL APDSQREP;      /* REPAIR THE QUEUE              */ 00426000
         BAL   @14,APDSQREP                                     01S0225 00427000
*               DO;                 /* BEGIN CODE SEGMENT            */ 00428000
*                 RFY                                           02S0227 00429000
*                  (R0RSTD,                                     02S0227 00430000
*                   R1RSTD,                                     02S0227 00431000
*                   R2RSTD,                                     02S0227 00432000
*                   R3RSTD,                                     02S0227 00433000
*                   R4RSTD,                                     02S0227 00434000
*                   R5RSTD,         /* 6 & 7 BASES                   */ 00435000
*                   R8RSTD,                                     02S0227 00436000
*                   R9RSTD,                                     02S0227 00437000
*                   R10RSTD,                                    02S0227 00438000
*                   R12RSTD,                                    02S0227 00439000
*                   R14RSTD,                                    02S0227 00440000
*                   R15RSTD) RSTD;                              02S0227 00441000
*                 RFY                                           02S0228 00442000
*                  (R0RSTD,                                     02S0228 00443000
*                   R1RSTD,                                     02S0228 00444000
*                   R2RSTD,                                     02S0228 00445000
*                   R3RSTD,                                     02S0228 00446000
*                   R4RSTD,                                     02S0228 00447000
*                   R5RSTD,         /* 6 & 7 BASES                   */ 00448000
*                   R8RSTD,                                     02S0228 00449000
*                   R9RSTD,                                     02S0228 00450000
*                   R10RSTD,                                    02S0228 00451000
*                   R12RSTD,                                    02S0228 00452000
*                   R14RSTD,                                    02S0228 00453000
*                   R15RSTD) UNRSTD;                            02S0228 00454000
*               END;                /* END OF CODE SEGMENT           */ 00455000
*UNALLOC:                                                       01S0230 00456000
*               APESRTRY=ADDR(DEQAPDS);/* ADVANCE RETRY POINT        */ 00457000
UNALLOC  L     @03,APESPTR                                      01S0230 00458000
         L     @15,APESREGP(,@03)                               01S0230 00459000
         LA    @00,DEQAPDS                                      01S0230 00460000
         ST    @00,APESRTRY(,@15)                               01S0230 00461000
*               DO;                 /* ERROR RECOVERY COMPLETE       */ 00462000
*                 APESFPRV=BOFF;    /* RESET RECURSION FLAG.         */ 00463000
         NI    APESFPRV(@03),B'10111111'                        02S0232 00464000
*                 APESCODE=CERRN99; /* RESET ERROR CODE              */ 00465000
         MVI   APESCODE(@03),X'01'                              02S0233 00466000
*                 APESFDMP=BON;     /* RESET DUMP/LOG FLAGS          */ 00467000
*                 APESFREC=BON;     /* TO THEIR NORMAL VALUES        */ 00468000
*                 APESFSDP=BON;     /* (ALL ON)                      */ 00469000
         OI    APESFDMP(@03),B'11100000'                        02S0236 00470000
*               END;                /* ALL INDICATORS RESET          */ 00471000
*               IF ALLQUE=BON THEN  /* IF APDS LOCATED ON APDS QUEUE */ 00472000
         TM    ALLQUE,B'01000000'                               01S0238 00473000
         BNO   @RF00238                                         01S0238 00474000
*                 DO;               /* IT CAN BE REMOVED FROM QUEUE  */ 00475000
*                   IF APDSARS=BON THEN/* IF IT IS A SATISFIED REQUST*/ 00476000
         TM    APDSARS(APDSPTR),B'00001000'                     01S0240 00477000
         BNO   @RF00240                                         01S0240 00478000
*                     DO;           /* IT MUST BE UNALLOCATED        */ 00479000
*                       IF APDSOWN=EXC&/* IF EXCLUSIVE PORT ALLOCATED*/ 00480000
*                           APDSAPUB^=0 THEN/* AND NOT UNALLOCATED      00481000
*                                      YET                           */ 00482000
         TM    APDSOWN(APDSPTR),B'10000000'                     01S0242 00483000
         BNO   @RF00242                                         01S0242 00484000
         L     @03,APDSAPUB(,APDSPTR)                           01S0242 00485000
         LTR   @03,@03                                          01S0242 00486000
         BZ    @RF00242                                         01S0242 00487000
*                         DO;       /* PUT APUB BACK ON SHARED QUEUE */ 00488000
*                           APUBPTR=APDSAPUB;/* POINT TO PROPER APUB */ 00489000
         LR    APUBPTR,@03                                      01S0244 00490000
*                           APUBOWN=SHR;/* INDICATE AVAILABILITY OF     00491000
*                                      APUB                          */ 00492000
         L     @03,APUBOWN(,APUBPTR)                            01S0245 00493000
@GL00003 LR    @00,@03                                          01S0245 00494000
         N     @00,@CF03125                                     01S0245 00495000
         CS    @03,@00,APUBOWN(APUBPTR)                         01S0245 00496000
         BNE   @GL00003                                         01S0245 00497000
*                           APUBIEXC=BOFF;/* INDICATE NO LONGER OWNED*/ 00498000
         L     @03,APUBIEXC-2(,APUBPTR)                         01S0246 00499000
@GL00004 LR    @00,@03                                          01S0246 00500000
         N     @00,@CF03128                                     01S0246 00501000
         CS    @03,@00,APUBIEXC-2(APUBPTR)                      01S0246 00502000
         BNE   @GL00004                                         01S0246 00503000
*                           APUBISHR=BON;/* INDICATE NOW AVAIL FOR      00504000
*                                      SHR                           */ 00505000
         L     @03,APUBISHR-3(,APUBPTR)                         01S0247 00506000
@GL00005 LR    @00,@03                                          01S0247 00507000
         O     @00,@CF03131                                     01S0247 00508000
         CS    @03,@00,APUBISHR-3(APUBPTR)                      01S0247 00509000
         BNE   @GL00005                                         01S0247 00510000
*                           APDSAPUB=0;/* INDICATE RESOURCE     01S0248 00511000
*                                      UNALLOCATED                   */ 00512000
         SLR   @03,@03                                          01S0248 00513000
         ST    @03,APDSAPUB(,APDSPTR)                           01S0248 00514000
*                         END;      /* DONE WITH EXC PORTION UNALLOC */ 00515000
*                       ELSE                                    01S0250 00516000
*                         ;         /* SHARED PORT ALLOCATION        */ 00517000
@RF00242 DS    0H                                               01S0251 00518000
*                       APDSARS=BOFF;/* INDICATE NO LONGER ALLOCATED */ 00519000
         NI    APDSARS(APDSPTR),B'11110111'                     01S0251 00520000
*                     END;          /* IF APDSARS = BON              */ 00521000
*                   ELSE                                        01S0253 00522000
*                     ;             /* REQUEST NOT SATISFIED         */ 00523000
@RF00240 DS    0H                                               01S0254 00524000
*                   DO;             /* BEGIN CODE SEGMENT            */ 00525000
*                     RFY                                       02S0255 00526000
*                      (R0RSTD,                                 02S0255 00527000
*                       R1RSTD,                                 02S0255 00528000
*                       R2RSTD,                                 02S0255 00529000
*                       R3RSTD,                                 02S0255 00530000
*                       R4RSTD,                                 02S0255 00531000
*                       R5RSTD,     /* 6 & 7 BASES                   */ 00532000
*                       R8RSTD,                                 02S0255 00533000
*                       R9RSTD,                                 02S0255 00534000
*                       R10RSTD,                                02S0255 00535000
*                       R12RSTD,                                02S0255 00536000
*                       R14RSTD,                                02S0255 00537000
*                       R15RSTD) RSTD;                          02S0255 00538000
*                     RFY                                       02S0256 00539000
*                      (R0RSTD,                                 02S0256 00540000
*                       R1RSTD,                                 02S0256 00541000
*                       R2RSTD,                                 02S0256 00542000
*                       R3RSTD,                                 02S0256 00543000
*                       R4RSTD,                                 02S0256 00544000
*                       R5RSTD,     /* 6 & 7 BASES                   */ 00545000
*                       R8RSTD,                                 02S0256 00546000
*                       R9RSTD,                                 02S0256 00547000
*                       R10RSTD,                                02S0256 00548000
*                       R12RSTD,                                02S0256 00549000
*                       R14RSTD,                                02S0256 00550000
*                       R15RSTD) UNRSTD;                        02S0256 00551000
*                   END;            /* END OF CODE SEGMENT           */ 00552000
*DEQAPDS:                                                       01S0258 00553000
*                   APESRTRY=ADDR(DOVUSE);/* ADVANCE RETRY POINT     */ 00554000
DEQAPDS  L     @03,APESPTR                                      01S0258 00555000
         L     @15,APESREGP(,@03)                               01S0258 00556000
         LA    @00,DOVUSE                                       01S0258 00557000
         ST    @00,APESRTRY(,@15)                               01S0258 00558000
*                   DO;             /* ERROR RECOVERY COMPLETE       */ 00559000
*                     APESFPRV=BOFF;/* RESET RECURSION FLAG.         */ 00560000
         NI    APESFPRV(@03),B'10111111'                        02S0260 00561000
*                     APESCODE=CERRN99;/* RESET ERROR CODE           */ 00562000
         MVI   APESCODE(@03),X'01'                              02S0261 00563000
*                     APESFDMP=BON; /* RESET DUMP/LOG FLAGS          */ 00564000
*                     APESFREC=BON; /* TO THEIR NORMAL VALUES        */ 00565000
*                     APESFSDP=BON; /* (ALL ON)                      */ 00566000
         OI    APESFDMP(@03),B'11100000'                        02S0264 00567000
*                   END;            /* ALL INDICATORS RESET          */ 00568000
*                   IF ALLAPUB=BON THEN/* ONLY IF WE HAVE APUB DO WE */ 00569000
         TM    ALLAPUB,B'00100000'                              01S0266 00570000
         BNO   @RF00266                                         01S0266 00571000
*DQAPDSDO:                          /* APDS TO BE DEQUEUED HAS PTR   */ 00572000
*                     DO;           /* IN APDSPTR                    */ 00573000
DQAPDSDO DS    0H                                               02S0268 00574000
*                       FIRSAPDS=APDSDSAP;/* OUR APDS'S PREVIOUS     */ 00575000
         L     @03,APDSDSAP(,APDSPTR)                           02S0268 00576000
         LR    FIRSAPDS,@03                                     02S0268 00577000
*                       LASTAPDS=APDSAPDS;/* OUR APDS'S NEXT         */ 00578000
         L     @15,APDSAPDS(,APDSPTR)                           02S0269 00579000
         LR    LASTAPDS,@15                                     02S0269 00580000
*                       APESRTRY=ADDR(FINSQRMV);/* FINISH QUEUE 02S0270 00581000
*                                      REMOVAL OPERATION             */ 00582000
         L     @14,APESPTR                                      02S0270 00583000
         L     @14,APESREGP(,@14)                               02S0270 00584000
         LA    @00,FINSQRMV                                     02S0270 00585000
         ST    @00,APESRTRY(,@14)                               02S0270 00586000
*                       ALLQRMV=BON;/* INDICATE REMOVAL BEGUN        */ 00587000
*                       ALLQUE=BOFF;/* INDICATE QUEUE POSSIBLEY NOT     00588000
*                                      INTACT OR BL0CK NOT ON QUEUE  */ 00589000
         OI    ALLQRMV,B'00100000'                              02S0272 00590000
         NI    ALLQUE,B'10111111'                               02S0272 00591000
*                       IF APDSAPDS=0 THEN/* THIS IS LAST BLK ON        00592000
*                                      QUEUE                         */ 00593000
         SLR   @14,@14                                          02S0273 00594000
         CR    @15,@14                                          02S0273 00595000
         BNE   @RF00273                                         02S0273 00596000
*                         IF APDSDSAP=0 THEN/* THIS IS ONLY BLK ON      00597000
*                                      QUEUE                         */ 00598000
         CR    @03,@14                                          02S0274 00599000
         BNE   @RF00274                                         02S0274 00600000
*                           DO;     /* RESET FIRST AND LAST POINTERS */ 00601000
*                             APCTAPDS=0;/* FIRST = 0                */ 00602000
         ST    @14,APCTAPDS(,APCTPTR)                           02S0276 00603000
*                             APCTDSAP=0;/* LAST = 0                 */ 00604000
         ST    @14,APCTDSAP(,APCTPTR)                           02S0277 00605000
*                           END;                                02S0278 00606000
*                         ELSE      /* (OTHERS BEFORE THIS ONE)      */ 00607000
*                           DO;                                 02S0279 00608000
         B     @RC00274                                         02S0279 00609000
@RF00274 DS    0H                                               02S0280 00610000
*                             APDSDSAP->APDSAPDS=0;/* NEW LAST HAS 0    00611000
*                                      NEXT                          */ 00612000
         L     @03,APDSDSAP(,APDSPTR)                           02S0280 00613000
         SLR   @00,@00                                          02S0280 00614000
         ST    @00,APDSAPDS(,@03)                               02S0280 00615000
*                             APCTDSAP=APDSDSAP;/* NEW LAST POINTER  */ 00616000
         ST    @03,APCTDSAP(,APCTPTR)                           02S0281 00617000
*                           END;                                02S0282 00618000
*                       ELSE        /* (FIRST OR INTERIOR QUE BLK &  */ 00619000
*                         IF APDSDSAP=0 THEN/* OTHERS FOLLOW. IF PTR    00620000
*                                      TO PREV                       */ 00621000
         B     @RC00273                                         02S0283 00622000
@RF00273 SLR   @03,@03                                          02S0283 00623000
         C     @03,APDSDSAP(,APDSPTR)                           02S0283 00624000
         BNE   @RF00283                                         02S0283 00625000
*                           DO;     /* IOUS IS ZERO THEN THIS IS 1ST */ 00626000
*                             APCTAPDS=APDSAPDS;/* NEW FIRST POINTER */ 00627000
         L     @15,APDSAPDS(,APDSPTR)                           02S0285 00628000
         ST    @15,APCTAPDS(,APCTPTR)                           02S0285 00629000
*                             APDSAPDS->APDSDSAP=0;/* NEW FIRST HAS     00630000
*                                      NO PREVIOUS                   */ 00631000
         ST    @03,APDSDSAP(,@15)                               02S0286 00632000
*                           END;                                02S0287 00633000
*                         ELSE      /* (INTERIOR QUEUE BLK)          */ 00634000
*                           DO;     /*                               */ 00635000
         B     @RC00283                                         02S0288 00636000
@RF00283 DS    0H                                               02S0289 00637000
*                             APDSDSAP->APDSAPDS=APDSAPDS;/* PREVIOUS   00638000
*                                      POINTS TO NEXT                */ 00639000
         L     @03,APDSDSAP(,APDSPTR)                           02S0289 00640000
         L     @15,APDSAPDS(,APDSPTR)                           02S0289 00641000
         ST    @15,APDSAPDS(,@03)                               02S0289 00642000
*                             APDSAPDS->APDSDSAP=APDSDSAP;/* NEXT       00643000
*                                      POINTS BACK TO PREV           */ 00644000
         ST    @03,APDSDSAP(,@15)                               02S0290 00645000
*                           END;                                02S0291 00646000
*                       ALLQRMV=BOFF;/* INDICATE REMOVAL OPERATION      00647000
*                                      DUN                           */ 00648000
@RC00283 DS    0H                                               02S0292 00649000
@RC00273 NI    ALLQRMV,B'11011111'                              02S0292 00650000
*                     END DQAPDSDO;                             02S0293 00651000
*                   DO;             /* BEGIN CODE SEGMENT            */ 00652000
@RF00266 DS    0H                                               02S0295 00653000
*                     RFY                                       02S0295 00654000
*                      (R0RSTD,                                 02S0295 00655000
*                       R1RSTD,                                 02S0295 00656000
*                       R2RSTD,                                 02S0295 00657000
*                       R3RSTD,                                 02S0295 00658000
*                       R4RSTD,                                 02S0295 00659000
*                       R5RSTD,     /* 6 & 7 BASES                   */ 00660000
*                       R8RSTD,                                 02S0295 00661000
*                       R9RSTD,                                 02S0295 00662000
*                       R10RSTD,                                02S0295 00663000
*                       R12RSTD,                                02S0295 00664000
