         BZ    @RF00663                                         02S0663 01831000
*     DO;                           /* THEN TERMINATE PROCESSING     */ 01832000
*       APESCODE=CERRN09;           /* SET ERROR CODE                */ 01833000
         MVI   APESCODE,X'09'                                   02S0665 01834000
*       DO;                         /* FORCE ABEND TO LOG ERROR &       01835000
*                                      PASS CONTROL TO THE (E)STAE OR   01836000
*                                      FRR EXIT ROUTINE.             */ 01837000
*         DO;                       /* ABEND(APCTABND)               */ 01838000
*           RESPECIFY                                           02S0668 01839000
*            (GPR01F) RESTRICTED;                               02S0668 01840000
*           GPR01F=APCTABND;        /* COMP CODE IN REG 1            */ 01841000
         L     GPR01F,APCTABND(,APCTPTR)                        02S0669 01842000
*           SVC(13);                /* ISSUE ABEND SVC               */ 01843000
         SVC   13                                               02S0670 01844000
*           RESPECIFY                                           02S0671 01845000
*            (GPR01F) UNRESTRICTED;                             02S0671 01846000
*         END;                      /* ABEND(APCTABND) FORCE ABEND   */ 01847000
*         GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);          02S0673 01848000
*                                                  /* NO CODE           01849000
*       END;                        /* ABEND ISSUED                  */ 01850000
*     END;                          /* ERROR PROCESSED               */ 01851000
*   RFY                                                         02S0676 01852000
*     R15F UNRSTD;                  /* FINISHED WITH RETURN CODE     */ 01853000
@RF00663 DS    0H                                               02S0677 01854000
*   APESFLLK=BON;                   /* SHOW LOCAL LOCK OWNED         */ 01855000
*                                                               02S0677 01856000
         OI    APESFLLK,B'00100000'                             02S0677 01857000
*   /*****************************************************************/ 01858000
*   /*                                                               */ 01859000
*   /* ISSUE SETFRR TO COVER THE PROCESSING. PARAMETER LIST IS 'APES'*/ 01860000
*   /* BLOCK. THE EXIT ROUTINE CAN PROCESS BOTH (E)STAE AND FRR      */ 01861000
*   /* ENTRIES.                                                      */ 01862000
*   /*                                                               */ 01863000
*   /*****************************************************************/ 01864000
*                                                               02S0678 01865000
*   APESCODE=CERRN13;               /* SETFRR ERROR CODE             */ 01866000
         MVI   APESCODE,X'0D'                                   02S0678 01867000
*   RFY                                                         02S0679 01868000
*     R15P RSTD;                    /* SETFRR SETS TO PARM LIST      */ 01869000
*   GEN REFS(PSA,PSACSTK,PSALSFCC,APCTZUKA,APCT,APCTPTR,FRRSFRRA,       01870000
*       FRRSENTR,FRRSPARM,FRRSCURR,FRRSLAST,FRRS) SETS(R14F,R15F,R15P); 01871000
*                                   /*                                  01872000
         MODESET EXTKEY=ZERO       SETFRR REQUIRES KEY-0                01873000
         SETFRR A,FRRAD=APCTZUKA-APCT(,APCTPTR), IGTUKA IS EXIT RTN    *01874000
               PARMAD=(R15P),      * SAVE @ 6-WORD PARM FIELD HERE     *01875000
               CLEAR=NO,           * DO NOT CLEAR PARM FIELD           *01876000
               WRKREGS=(R14F,R15F), * WORK REGISTERS                   *01877000
               RELATED=(FREELLCK,APES) * APES IS INPUT                  01878000
*   R15P->I031P=ADDR(APES);         /* 1ST WORD OF PARM LIST IS 02S0681 01879000
*                                      ADDRESS OF 'APES'             */ 01880000
         LA    @14,APES                                         02S0681 01881000
