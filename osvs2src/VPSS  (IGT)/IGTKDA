TKDA     TITLE 'IGTKDA -- RELEASE VPSS CHANNEL PROGRAM                 *00001000
                        '                                               00002000
IGTKDA   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTKDA    77.093'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @07,0                                            01S0001 00010000
@PSTART  DS    0H                                               01S0001 00011000
         USING @PSTART,@07                                      01S0001 00012000
RBPRFXLN EQU 64                                                         00013000
RBPRFLNA EQU 32                                                         00014000
PRBLEN   EQU 136                                                        00015000
SIRBLEN  EQU 200                                                        00016000
TIRBLEN  EQU 136                                                        00017000
IRBLEN   EQU 128                                                        00018000
SVRBLEN  EQU 224                                                        00019000
*   RAPRLPTR=R1P;                   /* SAVE APRL ADDRESS             */ 00020000
         LR    RAPRLPTR,R1P                                     01S0180 00021000
*   RFY                                                         01S0181 00022000
*     APCT BASED(APCTPTR);          /* TELL                          */ 00023000
*   RFY                                                         01S0182 00024000
*     APDX BASED(RAPDXPTR);         /* COMPILER                      */ 00025000
*   RFY                                                         01S0183 00026000
*     APRL BASED(RAPRLPTR);         /* OF POINTERS.                  */ 00027000
*   RFY                                                         01S0184 00028000
*     DEBBASIC BASED(RDEBPTR);      /* DEB,APDX,APCT OK BY DEBCHK    */ 00029000
*   RFY                                                         01S0185 00030000
*     R1P UNRSTD;                   /* FINISHED WITH INPUT REG       */ 00031000
*   GEN(MODESET EXTKEY=DATAMGT)                                 01S0186 00032000
*   NOREFS NOSETS;                  /* RUN IN DATA MANAGEMENT KEY    */ 00033000
         MODESET EXTKEY=DATAMGT                                         00034000
         EJECT                                                          00035000
*                                                               01S0187 00036000
*   /*****************************************************************/ 00037000
*   /*                                                               */ 00038000
*   /* ACQUIRE SAVEAREA/WORKAREA. INITIALIZE PL/S WORKAREA.          */ 00039000
*   /*                                                               */ 00040000
*   /*****************************************************************/ 00041000
*                                                               01S0187 00042000
*   DO;                             /* START OF CODE SEGMENT         */ 00043000
*                                                               02S0187 00044000
*     /***************************************************************/ 00045000
*     /*                                                             */ 00046000
*     /* BUILD AND INITIALIZE A SAVEAREA/WORKAREA. ISSUE A           */ 00047000
*     /* CONDITIONAL GETMAIN FOR THE REQUESTED SA/WA SIZE. IF THE    */ 00048000
*     /* STORAGE IS UNAVAILABLE, ISSUE A UNCONDITIONAL GETMAIN IF THE*/ 00049000
*     /* CALLER WAS IN PROBLEM PROGRAM STATE AND KEY 8-15. IF        */ 00050000
*     /* PRIVLEDGED, USE CSA. INITIALIZE THE SAVEAREA/WORKAREA (VIA  */ 00051000
*     /* IGTUAF) AND SAVE THE INPUT REGISTER VALUES.                 */ 00052000
*     /*                                                             */ 00053000
*     /***************************************************************/ 00054000
*                                                               02S0188 00055000
*     RFY                           /* REQUIRED REGISTERS            */ 00056000
*      (RSP,                        /* DYNAMIC AREA SUBPOOL          */ 00057000
*       RLV,                        /* DYNAMIC AREA LENGTH           */ 00058000
*       RADDR,                      /* DYNAMIC AREA ADDRESS          */ 00059000
*       R1P,                        /* GETMAIN RETURNED AREA ADDRESS */ 00060000
*       R15F) RSTD;                 /* GETMAIN RETURN CODE           */ 00061000
*     RSP=CNORMSP;                  /* NORMAL SUBPOOL                */ 00062000
         LA    RSP,230                                          02S0189 00063000
*     RLV=CNORMLV;                  /* NORMAL LENGTH REQUIREMENT     */ 00064000
         L     RLV,@CF04591                                     02S0190 00065000
*     GEN REFS(RSP,RLV);                                        02S0191 00066000
*                                   /*                                  00067000
         GETMAIN RC,               ISSUE CONDITIONAL GETMAIN           *00068000
               LV=(RLV),           * REQUIRED LENGTH                   *00069000
               SP=(RSP),           * REQUIRED SUBPOOL                  *00070000
               BNDRY=PAGE          * START ON PAGE BOUNDARY            *00071000
                                   * R1P->STORAGE. R15F HAS RETURN CODE 00072000
*     IF R15F^=0 THEN               /* IS STORAGE NOT ACQUIRED,      */ 00073000
         LTR   R15F,R15F                                        02S0192 00074000
         BZ    @RF00192                                         02S0192 00075000
*       DO;                         /* TRY A DIFFERENT SUBPOOL.      */ 00076000
*         IF RBLINK->PSWPP=BOFF|    /* IF SUPERVISOR STATE OR        */ 00077000
*             RBLINK->PSWUKEY=BOFF THEN/* OR PRIVLEDGED KEY (0-7)    */ 00078000
         L     @14,TCBPTR                                       02S0194 00079000
         L     @14,RBSECPTR(,@14)                               02S0194 00080000
         L     @14,RBLINK(,@14)                                 02S0194 00081000
         TM    PSWPP(@14),B'10000001'                           02S0194 00082000
         BO    @RF00194                                         02S0194 00083000
*           RSP=CEMERSP;            /* THEN USE CSA.                 */ 00084000
         LA    RSP,241                                          02S0195 00085000
*         GEN REFS(RSP,RLV);                                    02S0196 00086000
@RF00194 DS    0H                                               02S0196 00087000
*                                   /*                                  00088000
         GETMAIN RU,               ISSUE UNCONDITIONAL GETMAIN         *00089000
               LV=(RLV),           * REQUIRED LENGTH                   *00090000
               SP=(RSP),           * REQUIRED SUBPOOL                  *00091000
               BNDRY=DBLWD         * NO SPECIAL ALIGNMENT              *00092000
                                   * R1 -> ACQUIRED STORAGE             00093000
*       END;                                                    02S0197 00094000
*     RADDR=R1P;                    /* SAVE ADDR ACQUIRED STORAGE    */ 00095000
@RF00192 LR    RADDR,R1P                                        02S0198 00096000
*     RFY                                                       02S0199 00097000
*      (R1P,                                                    02S0199 00098000
*       R15F) UNRSTD;               /* GETMAIN REGISTERS NOT NEEDED. */ 00099000
*                                                               02S0199 00100000
*     /***************************************************************/ 00101000
*     /*                                                             */ 00102000
*     /* STORAGE NOW OBTAINED. 'RADDR' CONTAINS ITS ADDRESS, 'RSP'   */ 00103000
*     /* ITS SUBPOOL, AND 'RLV' ITS LENGTH. CONSTRUCT A              */ 00104000
*     /* SAVEAREA/WORKAREA USING IGTUAF. ON INPUT TO THIS PROGRAM,   */ 00105000
*     /* REGISTER 1 MUST CONTAIN THE DYNAMIC STORAGE ADDRESS.        */ 00106000
*     /* REGISTER 0 MUST HAVE THE SUBPOOL NUMBER IN BYTE 0, AND THE  */ 00107000
*     /* LENGTH OF THE AREA IN BYTES 1-3. ON RETURN, REGISTER 1      */ 00108000
*     /* CONTAINS THE ADDRESS OF THE APSC FOR THE NEW SAVEAREA.      */ 00109000
*     /*                                                             */ 00110000
*     /***************************************************************/ 00111000
*                                                               02S0200 00112000
*     RFY                                                       02S0200 00113000
*      (R0F,                                                    02S0200 00114000
*       R1P) RSTD;                  /* NEEDED REGISTERS              */ 00115000
*     R0F=RSP;                      /* SUBPOOL                       */ 00116000
         LR    R0F,RSP                                          02S0201 00117000
*     SLL(R0F,24);                  /* MOVE TO BYTE 0                */ 00118000
         SLL   R0F,24                                           02S0202 00119000
*     R0F=R0F|RLV;                  /* LENGTH IN BYTES 1-3.          */ 00120000
         OR    R0F,RLV                                          02S0203 00121000
*     R1P=RADDR;                    /* ADDRESS OF DYNAMIC STORAGE    */ 00122000
         LR    R1P,RADDR                                        02S0204 00123000
*     RFY                                                       02S0205 00124000
*      (RSP,                                                    02S0205 00125000
*       RLV,                                                    02S0205 00126000
*       RADDR) UNRSTD;              /* FINISHED WITH GETMAIN REGS    */ 00127000
*     CALL APCVZUAF;                /* INITIALIZE SA-WA. ON RETURN,     00128000
*                                      R1 -> APSC.                   */ 00129000
         L     @15,APCTZUAF(,APCTPTR)                           02S0206 00130000
         BALR  @14,@15                                          02S0206 00131000
*     R0F=(APSVLN+APSCLN+@SIZDATD+7)/* ALLOW FOR APSV, APSC, AND     */ 00132000
*         &'0000FFF8'X;             /* DYNAMIC AREA. ROUND TO DWORD. */ 00133000
         LA    R0F,95                                           02S0207 00134000
         AL    R0F,@SIZDATD                                     02S0207 00135000
         N     R0F,@CF04637                                     02S0207 00136000
*     R1P->APSCCRSZ=R0F;            /* SET USED SIZE IN APSC         */ 00137000
         STH   R0F,APSCCRSZ(,R1P)                               02S0208 00138000
*     R1P=R1P+APSCLN;               /* SKIP APSC. NOW AT APSV.       */ 00139000
         LA    R1P,8(,R1P)                                      02S0209 00140000
*     RFY                                                       02S0210 00141000
*       R0F UNRSTD;                 /* FINISHED WITH REGISTER 0      */ 00142000
*                                                               02S0210 00143000
*     /***************************************************************/ 00144000
*     /*                                                             */ 00145000
*     /* BUILD SAVEAREA. POINT TO DYNAMIC AREA.                      */ 00146000
*     /*                                                             */ 00147000
*     /***************************************************************/ 00148000
*                                                               02S0211 00149000
*     R1P->APSVPREV=0;              /* NO PREVIOUS SAVEAREA          */ 00150000
         SLR   @10,@10                                          02S0211 00151000
         ST    @10,APSVPREV(,R1P)                               02S0211 00152000
*     APSVPTR=R1P;                  /* POINT TO NEW SAVEAREA         */ 00153000
         LR    APSVPTR,R1P                                      02S0212 00154000
*     RFY                                                       02S0213 00155000
*       R1P UNRSTD;                 /* FINISHED WITH REGISTER 1      */ 00156000
*     RFY                                                       02S0214 00157000
*       APDATA RSTD;                /* SET APDATA TO ADDRESS         */ 00158000
*     APDATA=ADDR(APSV)+APSVLN;     /* OF PL/S DYNAMIC AREA          */ 00159000
         LA    APDATA,80(,APSVPTR)                              02S0215 00160000
*     GEN(USING @DATD,APDATA)                                   02S0216 00161000
*     NOSETS REFS(APDATA);          /* (AFTER SAVEAREA).             */ 00162000
*                                                               02S0216 00163000
         USING @DATD,APDATA                                             00164000
*     /***************************************************************/ 00165000
*     /*                                                             */ 00166000
*     /* SAVE REGISTER POINTERS IN WORKAREA. THIS ALLOWS FOR PL/S    */ 00167000
*     /* REGISTER OPTIMIZATION.                                      */ 00168000
*     /*                                                             */ 00169000
*     /***************************************************************/ 00170000
*                                                               02S0217 00171000
*     DAPRLPTR=RAPRLPTR;            /* ADDRESS OF INPUT APRL         */ 00172000
         ST    RAPRLPTR,DAPRLPTR                                02S0217 00173000
*     DDEBPTR=RDEBPTR;              /* ADDRESS OF ASSOCIATED DEB     */ 00174000
         ST    RDEBPTR,DDEBPTR                                  02S0218 00175000
*     DAPDXPTR=RAPDXPTR;            /* ADDRESS OF APDX FOR DEB       */ 00176000
         ST    RAPDXPTR,DAPDXPTR                                02S0219 00177000
*     RFY                                                       02S0220 00178000
*       APRL BASED(DAPRLPTR);       /* TELL COMPILER OF              */ 00179000
*     RFY                                                       02S0221 00180000
*       DEBBASIC BASED(DDEBPTR);    /* THESE POINTERS                */ 00181000
*     RFY                                                       02S0222 00182000
*       APDX BASED(DAPDXPTR);       /* FOR LATER USE.                */ 00183000
*     DUSERKEY=DEBPROTG;            /* SAVE USER PROTECT KEY (THIS IS   00184000
*                                      KEY OF OPEN REQUESTOR).       */ 00185000
         L     @10,DDEBPTR                                      02S0223 00186000
         MVC   DUSERKEY(1),DEBPROTG(@10)                        02S0223 00187000
*     RFY                                                       02S0224 00188000
*      (RDEBPTR,                                                02S0224 00189000
*       RAPDXPTR,                                               02S0224 00190000
*       RAPRLPTR) UNRSTD;           /* DONE WITH REGS                */ 00191000
*   END;                            /* END OF CODE SEGMENT           */ 00192000
         EJECT                                                          00193000
*                                                               01S0226 00194000
*   /*****************************************************************/ 00195000
*   /*                                                               */ 00196000
*   /* INITIALIZE THE RECOVERY AREA AND ISSUE A SETFRR TO COVER THIS */ 00197000
*   /* PROGRAM'S PROCESSING.                                         */ 00198000
*   /*                                                               */ 00199000
*   /*****************************************************************/ 00200000
*                                                               01S0226 00201000
*   DO;                             /* START OF CODE SEGMENT         */ 00202000
*                                                               02S0226 00203000
*     /***************************************************************/ 00204000
*     /*                                                             */ 00205000
*     /* ESTABLISH RECOVERY ENVIRONMENT. THIS EXIT WILL GAIN CONTROL */ 00206000
*     /* IF AN ABEND OCCURS. A DUMP IS TAKEN, THE APPROPRIATE ERROR  */ 00207000
*     /* FLAGS SET, AND CONTROL PASSED TO THE CLEANUP ROUTINE TO     */ 00208000
*     /* RELEASE RESOURCES BEFORE RETURNING TO THE CALLER.           */ 00209000
*     /*                                                             */ 00210000
*     /***************************************************************/ 00211000
*                                                               02S0227 00212000
*                                                               02S0227 00213000
*     /***************************************************************/ 00214000
*     /*                                                             */ 00215000
*     /* BUILD ESTAE/FRR EXIT PARAMETER LIST (APES)                  */ 00216000
*     /*                                                             */ 00217000
*     /***************************************************************/ 00218000
*                                                               02S0227 00219000
*     RFY                                                       02S0227 00220000
*      (R0P,                                                    02S0227 00221000
*       R1F,                                                    02S0227 00222000
*       R14P,                                                   02S0227 00223000
*       R15F) RSTD;                 /* INITIALIZE APES BLOCK         */ 00224000
*     R0P=ADDR(APES);               /* TO ZEROS                      */ 00225000
         LA    R0P,APES                                         02S0228 00226000
*     R1F=LENGTH(APES);             /* USE MVCL TO                   */ 00227000
         LA    R1F,648                                          02S0229 00228000
*     R15F=0;                       /* CLEAR, SINCE FIELD            */ 00229000
         SLR   R15F,R15F                                        02S0230 00230000
*     MVCL(R0P,R14P);               /* MORE THAN 256 BYTES LONG      */ 00231000
         MVCL  R0P,R14P                                         02S0231 00232000
*     RFY                                                       02S0232 00233000
*      (R0P,                                                    02S0232 00234000
*       R1F,                                                    02S0232 00235000
*       R14P,                                                   02S0232 00236000
*       R15F) UNRSTD;               /* FINISHED WITH MVCL REGS       */ 00237000
*     APESID=APESIDC;               /* IN-CORE ID                    */ 00238000
         MVC   APESID(4),@CC04469                               02S0233 00239000
*     APESADDR=ADDR(APES);          /* ESTAE/SETFRR PARM VALUE       */ 00240000
         LA    @15,APES                                         02S0234 00241000
         ST    @15,APESADDR                                     02S0234 00242000
*     APESAPMV=ADDR(CMBRXZZZ);      /* MESSAGE INDEX VALUES          */ 00243000
         LA    @15,CMBRXZZZ                                     02S0235 00244000
         ST    @15,APESAPMV                                     02S0235 00245000
*     APESFDMP=BON;                 /* DUMP ON ERROR                 */ 00246000
*     APESFREC=BON;                 /* RECORD ON LOGREC              */ 00247000
*     APESFSDP=BON;                 /* SDUMP ON ERROR                */ 00248000
         OI    APESFDMP,B'11100000'                             02S0238 00249000
*     APESMXCD=CERRN99;             /* NUMBER OF CMBRXZZZ ENTRIES    */ 00250000
         MVI   APESMXCD,X'0E'                                   02S0239 00251000
*     APESMOD='IGTKDA  ';           /* MODULE IN CONTROL             */ 00252000
         MVC   APESMOD(8),@CC04646                              02S0240 00253000
*     APESPROC='IGTKDA  ';          /* CSECT IN CONTROL              */ 00254000
         MVC   APESPROC(8),@CC04646                             02S0241 00255000
*     APESFRR='        ';           /* NO FRR ID                     */ 00256000
         MVC   APESFRR(8),@CC04647                              02S0242 00257000
*     APESREGP=ADDR(APESREGZ);      /* DEFAULT REGISTER SAVEAREA     */ 00258000
         LA    @15,APESREGZ                                     02S0243 00259000
         ST    @15,APESREGP                                     02S0243 00260000
*     RFY                                                       02S0244 00261000
*      (R15P) RSTD;                 /* SET REGISTER 15 TO            */ 00262000
*     R15P=ADDR(EXIT);              /* BACKOUT ROUTINE               */ 00263000
         LA    R15P,EXIT                                        02S0245 00264000
*     STM(R0F,R15F,APESREGZ);       /* SAVE REGISTERS FOR RETRY      */ 00265000
         STM   R0F,R15F,APESREGZ                                02S0246 00266000
*     RFY                                                       02S0247 00267000
*       R15P UNRSTD;                /* FINISHED WITH 15              */ 00268000
*     APESCODE=CERRN99;             /* 'UNKNOWN' ERROR               */ 00269000
         MVI   APESCODE,X'0E'                                   02S0248 00270000
*     CALL GETLLCK;                 /* GET LOCAL LOCK & ISSUE SETFRR */ 00271000
         BAL   @14,GETLLCK                                      02S0249 00272000
*   END;                            /* RECOVERY ENVIRONMENT SET      */ 00273000
         EJECT                                                          00274000
*                                                               01S0251 00275000
*   /*****************************************************************/ 00276000
*   /*                                                               */ 00277000
*   /* COPY THE REQUIRED PORTIONS OF THE USER'S APRL TO A PROTECTED  */ 00278000
*   /* AREA.                                                         */ 00279000
*   /*                                                               */ 00280000
*   /*****************************************************************/ 00281000
*                                                               01S0251 00282000
*   DO;                                                         02S0251 00283000
*                                                               02S0251 00284000
*     /***************************************************************/ 00285000
*     /*                                                             */ 00286000
*     /* COPY THE USER'S APRL TO A PROTECTED COPY. THIS ALLOWS       */ 00287000
*     /* REFERENCE TO A STABLE COPY AND ELIMINATES HAVING TO SWITCH  */ 00288000
*     /* PROTECT KEYS TO REFERENCE THE APRL. ONLY THOSE FIELDS       */ 00289000
*     /* REQUIRED FOR PROCESSING ARE COPIED. IF AN ERROR OCCURS      */ 00290000
*     /* REFERENCING THE APRL, THE FRR WILL INTERCEPT THE FAILURE. AN*/ 00291000
*     /* ERROR CODE WILL BE RETURNED TO THE CALLER INDICATING THE    */ 00292000
*     /* FAILURE.                                                    */ 00293000
*     /*                                                             */ 00294000
*     /***************************************************************/ 00295000
*                                                               02S0252 00296000
*     DAPRL=DAPRL&&DAPRL;           /* CLEAR INTERNAL APRL AREA      */ 00297000
         XC    DAPRL(80),DAPRL                                  02S0252 00298000
*     APESCODE=CERRN06;             /* ERROR CODE - BAD APRL         */ 00299000
         MVI   APESCODE,X'06'                                   02S0253 00300000
*     APESFSDP=BOFF;                /* NO SDUMP ON FAILURE           */ 00301000
         NI    APESFSDP,B'11011111'                             02S0254 00302000
*     SPKA(DUSERKEY);               /* SWITCH TO USER'S KEY          */ 00303000
         SLR   @01,@01                                          02S0255 00304000
         IC    @01,DUSERKEY                                     02S0255 00305000
         SPKA  0(@01)                                           02S0255 00306000
*     RFY                                                       02S0256 00307000
*      (R0F,                                                    02S0256 00308000
*       R1F,                                                    02S0256 00309000
*       R14F,                                                   02S0256 00310000
*       R15F) RSTD;                 /* COPY REGISTERS                */ 00311000
*     R0F=APRLOPTS;                 /* HOLD OPTION SWITCHES          */ 00312000
         L     @11,DAPRLPTR                                     02S0257 00313000
         L     R0F,APRLOPTS(,@11)                               02S0257 00314000
*     R1F=APRLOPTZ;                 /* HOLD OPTION SWITCHES          */ 00315000
         L     R1F,APRLOPTZ(,@11)                               02S0258 00316000
*     R14F=APRLAPIO;                /* INDEX INTO APDX FOR APIO ADDR */ 00317000
         L     R14F,APRLAPIO(,@11)                              02S0259 00318000
*     R15F=APRLUSER;                /* HOLD INSTALLATION-USE FIELD   */ 00319000
         L     R15F,APRLUSER(,@11)                              02S0260 00320000
*     GEN(MODESET EXTKEY=DATAMGT)                               02S0261 00321000
*     NOREFS NOSETS;                /* RESUME NORMAL KEY             */ 00322000
         MODESET EXTKEY=DATAMGT                                         00323000
*     APESCODE=CERRN99;             /* IS VPSS FAULT.                */ 00324000
         MVI   APESCODE,X'0E'                                   02S0262 00325000
*     APESFSDP=BON;                 /* SDUMP ON FAILURE              */ 00326000
         OI    APESFSDP,B'00100000'                             02S0263 00327000
*     RFY                                                       02S0264 00328000
*       APRL BASED(ADDR(DAPRL));    /* APRL BASE                     */ 00329000
*     APRLOPTS=R0F;                 /* SAVE OPTION SWITCHES          */ 00330000
         LA    @11,DAPRL                                        02S0265 00331000
         ST    R0F,APRLOPTS(,@11)                               02S0265 00332000
*     APRLOPTZ=R1F;                 /* SAVE OPTION SWITCHES          */ 00333000
         ST    R1F,APRLOPTZ(,@11)                               02S0266 00334000
*     APRLAPIO=R14F;                /* SAVE APIO INDEX               */ 00335000
         ST    R14F,APRLAPIO(,@11)                              02S0267 00336000
*     APRLUSER=R15F;                /* SAVE INSTALLATION-USE FIELD   */ 00337000
         ST    R15F,APRLUSER(,@11)                              02S0268 00338000
*     RFY                                                       02S0269 00339000
*      (R0F,                                                    02S0269 00340000
*       R1F,                                                    02S0269 00341000
*       R14F,                                                   02S0269 00342000
*       R15F) UNRSTD;               /* FINISHED WITH REGISTERS       */ 00343000
*     APRLID=APRLIDC;               /* IN-CORE ID FOR DUMP           */ 00344000
         MVC   APRLID(4,@11),@CC03160                           02S0270 00345000
*     APESRTRY=ADDR(RESTAPRL);      /* MOVE APRL TO USER IF ERROR       00346000
*                                      SINCE USER'S NOW COPIED.      */ 00347000
         LA    @15,RESTAPRL                                     02S0271 00348000
         L     @14,APESREGP                                     02S0271 00349000
         ST    @15,APESRTRY(,@14)                               02S0271 00350000
*   END;                            /* APRL COPIED                   */ 00351000
         EJECT                                                          00352000
*                                                               01S0273 00353000
*   /*****************************************************************/ 00354000
*   /*                                                               */ 00355000
*   /* INSURE THE DEB IS STILL VALID (ISSUE DEBCHK)                  */ 00356000
*   /*                                                               */ 00357000
*   /*****************************************************************/ 00358000
*                                                               01S0273 00359000
*   DO;                             /* START OF CODE SEGMENT         */ 00360000
*                                                               02S0273 00361000
*     /***************************************************************/ 00362000
*     /*                                                             */ 00363000
*     /* ISSUE DEBCHK TO INSURE DEB STILL EXISTS. IF DEB DOES NOT    */ 00364000
*     /* EXIST, SET ERROR FLAG AND EXIT.                             */ 00365000
*     /*                                                             */ 00366000
*     /***************************************************************/ 00367000
*                                                               02S0274 00368000
*     STM(R10F,R11F,DSUPVSVE);      /* SAVE DESTROYED REGISTERS      */ 00369000
         STM   R10F,R11F,DSUPVSVE                               02S0274 00370000
*     RFY                                                       02S0275 00371000
*      (R10P,                                                   02S0275 00372000
*       R1P,                                                    02S0275 00373000
*       R15P) RSTD;                 /* DEBCHK INPUT REGISTERS        */ 00374000
*     R1P=DDEBPTR;                  /* ADDRESS OF DEB TO VERIFY      */ 00375000
         L     R1P,DDEBPTR                                      02S0276 00376000
*     R10P=PSATOLD;                 /* TCB TO CHECK. MAY NOT BE TCB     00377000
*                                      WHICH OPENED ACB, BUT IS SAME    00378000
*                                      TCB STRUCTURE.                */ 00379000
         L     R10P,PSATOLD                                     02S0277 00380000
*     R15P=CVTDEBVR;                /* DEBCHK BRANCH ENTRY POINT     */ 00381000
         L     @11,CVTPTR                                       02S0278 00382000
         L     @11,CVTEXT2(,@11)                                02S0278 00383000
         L     R15P,CVTDEBVR(,@11)                              02S0278 00384000
*     APESCODE=CERRN03;             /* DEBCHK FAILURE CODE           */ 00385000
         MVI   APESCODE,X'03'                                   02S0279 00386000
*     GEN REFS(R15P,R1P,R10P) SETS(R10P,R11P,R15P);             02S0280 00387000
*                                   /*                                  00388000
         CALL  (15)                CALL DEBCHK ROUTINE                  00389000
         NOP   *-*                 + 0 -- DEB VALID (R15=0)             00390000
*                                  + 4 -- DEB INVALID (R15=4)           00391000
*     APESCODE=CERRN99;             /* RESET ERROR CODE              */ 00392000
         MVI   APESCODE,X'0E'                                   02S0281 00393000
*     LM(R10F,R11F,DSUPVSVE);       /* RELOAD DESTROYED REGISTERS    */ 00394000
         LM    R10F,R11F,DSUPVSVE                               02S0282 00395000
*     IF R15F^=0 THEN               /* IF DEBCHK FAILED,             */ 00396000
         LTR   R15F,R15F                                        02S0283 00397000
         BZ    @RF00283                                         02S0283 00398000
*       DO;                         /* THEN SET ERROR CODE AND STOP  */ 00399000
*         APESCODE=CERRN03;         /* ERROR NUMBER                  */ 00400000
         MVI   APESCODE,X'03'                                   02S0285 00401000
*         DO;                       /* FORCE ABEND TO LOG ERROR &       00402000
*                                      PASS CONTROL TO THE (E)STAE OR   00403000
*                                      FRR EXIT ROUTINE.             */ 00404000
*           DO;                     /* ABEND(APCTABND)               */ 00405000
*             RESPECIFY                                         02S0288 00406000
