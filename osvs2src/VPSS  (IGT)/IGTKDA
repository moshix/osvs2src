*             GEN(MODESET EXTKEY=DATAMGT)                       03S0385 00761000
*             NOREFS NOSETS;        /* RESUME STANDARD KEY           */ 00762000
         MODESET EXTKEY=DATAMGT                                         00763000
*             RFY                                               03S0386 00764000
*              (R0RSTD,                                         03S0386 00765000
*               R1RSTD,                                         03S0386 00766000
*               R2RSTD,                                         03S0386 00767000
*               R3RSTD,                                         03S0386 00768000
*               R4RSTD,                                         03S0386 00769000
*               R5RSTD,             /* 6-7 BASES                     */ 00770000
*               R8RSTD,                                         03S0386 00771000
*               R9RSTD,                                         03S0386 00772000
*               R10RSTD,                                        03S0386 00773000
*               R11RSTD,            /* 12-13 APCT/APSV PTRS          */ 00774000
*               R14RSTD,                                        03S0386 00775000
*               R15RSTD) UNRSTD;    /* ALLOW PL/S TO USE REGS AGAIN  */ 00776000
*           END;                    /* RETRY ENVIRONMENT SET         */ 00777000
*           APESRTRY=DRETRY(04);    /* RESET RETRY REGISTER          */ 00778000
         L     @15,DRETRY+12                                    03S0388 00779000
         L     @14,APESREGP                                     03S0388 00780000
         ST    @15,APESRTRY(,@14)                               03S0388 00781000
*           DO;                     /* ERROR RECOVERY COMPLETE       */ 00782000
*             APESFPRV=BOFF;        /* RESET RECURSION FLAG.         */ 00783000
         NI    APESFPRV,B'10111111'                             03S0390 00784000
*             APESCODE=CERRN99;     /* RESET ERROR CODE              */ 00785000
         MVI   APESCODE,X'0E'                                   03S0391 00786000
*             APESFDMP=BON;         /* RESET DUMP/LOG FLAGS          */ 00787000
*             APESFREC=BON;         /* TO THEIR NORMAL VALUES        */ 00788000
*             APESFSDP=BON;         /* (ALL ON)                      */ 00789000
         OI    APESFDMP,B'11100000'                             03S0394 00790000
*           END;                    /* ALL INDICATORS RESET          */ 00791000
*           APESCOD1=0;             /* CLEAR ERROR CODE              */ 00792000
         MVI   APESCOD1,X'00'                                   03S0396 00793000
*           APESMOD='IGTKDA  ';     /* RESET MODULE IN CONTROL       */ 00794000
         MVC   APESMOD(8),@CC04646                              03S0397 00795000
*           APESPROC='IGTKDA  ';    /* TO BE IGTKDA.                 */ 00796000
         MVC   APESPROC(8),@CC04646                             03S0398 00797000
*           APIOACTV=0;             /* INSURE I/O NOT LONGER ACTIVE  */ 00798000
         L     @01,DAPIOPTR                                     03S0399 00799000
         MVI   APIOACTV(@01),X'00'                              03S0399 00800000
*         END;                      /* I/O CLEARED                   */ 00801000
*       ELSE                        /* IF NOT IN CLOSE PROCESSING,   */ 00802000
*         DO;                       /* LOG (USER ERROR) AND STOP     */ 00803000
         B     @RC00344                                         02S0401 00804000
@RF00344 DS    0H                                               02S0402 00805000
*           APESCODE=CERRN07;       /* ERROR CODE                    */ 00806000
         MVI   APESCODE,X'07'                                   02S0402 00807000
*           APESFSDP=BOFF;          /* NO SDUMP -- USER ERROR        */ 00808000
         NI    APESFSDP,B'11011111'                             02S0403 00809000
*           DO;                     /* FORCE ABEND TO LOG ERROR &       00810000
*                                      PASS CONTROL TO THE (E)STAE OR   00811000
*                                      FRR EXIT ROUTINE.             */ 00812000
*             DO;                   /* ABEND(APCTABND)               */ 00813000
*               RESPECIFY                                       02S0406 00814000
*                (GPR01F) RESTRICTED;                           02S0406 00815000
*               GPR01F=APCTABND;    /* COMP CODE IN REG 1            */ 00816000
         L     GPR01F,APCTABND(,APCTPTR)                        02S0407 00817000
*               SVC(13);            /* ISSUE ABEND SVC               */ 00818000
         SVC   13                                               02S0408 00819000
*               RESPECIFY                                       02S0409 00820000
*                (GPR01F) UNRESTRICTED;                         02S0409 00821000
*             END;                  /* ABEND(APCTABND) FORCE ABEND   */ 00822000
*             GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);      02S0411 00823000
*                                                  /* NO CODE           00824000
*           END;                    /* ABEND ISSUED                  */ 00825000
*         END;                      /* ACTIVE APIO PROCESSED         */ 00826000
*                                                               02S0413 00827000
*     /***************************************************************/ 00828000
*     /*                                                             */ 00829000
*     /* RESET THE APRL (PROTECTED COPY) AND APDX FIELDS ASSOCIATED  */ 00830000
*     /* WITH THIS APIO.                                             */ 00831000
*     /*                                                             */ 00832000
*     /***************************************************************/ 00833000
*                                                               02S0414 00834000
*     APDXAPIO(APRLAPIO)=0;         /* APIO PTR IN APDX CLEARED      */ 00835000
@RC00344 DS    0H                                               02S0414 00836000
@RF00343 SLR   @15,@15                                          02S0414 00837000
         LA    @14,DAPRL                                        02S0414 00838000
         L     @11,APRLAPIO(,@14)                               02S0414 00839000
         SLA   @11,2                                            02S0414 00840000
         L     @01,DAPDXPTR                                     02S0414 00841000
         ST    @15,APDXAPIO-4(@11,@01)                          02S0414 00842000
*     APRLAPIO=0;                   /* UPDATE APRL TO SHOW           */ 00843000
         ST    @15,APRLAPIO(,@14)                               02S0415 00844000
*     APRLXIO=BOFF;                 /* NO TRANSLATED PROGRAM         */ 00845000
         NI    APRLXIO(@14),B'11111011'                         02S0416 00846000
*   END;                            /* APIO FOUND                    */ 00847000
         EJECT                                                          00848000
*                                                               01S0418 00849000
*   /*****************************************************************/ 00850000
*   /*                                                               */ 00851000
*   /* RELEASE THE APIO. COPY THE APIO TO A PROTECTED WORKAREA. THIS */ 00852000
*   /* ALLOWS THE REMAINDER OF THIS PROGRAM TO RELEASE THE RESOURCES */ 00853000
*   /* ATTACHED TO THIS APIO WITHOUT ACTUALLY 'LOCKING' ON THE APIO  */ 00854000
*   /* CHAIN.                                                        */ 00855000
*   /*                                                               */ 00856000
*   /*****************************************************************/ 00857000
*                                                               01S0418 00858000
*   DO;                             /* START OF CODE SEGMENT         */ 00859000
*                                                               02S0418 00860000
*     /***************************************************************/ 00861000
*     /*                                                             */ 00862000
*     /* RELEASE THE APIO ASSOCIATED WITH THE APRL. THE APIO IS FREED*/ 00863000
*     /* BY CLEARING THE DEB/ASCB ADDRESSES (NOTE: THE ASCB ADDRESS  */ 00864000
*     /* MUST BE RESET LAST SINCE A NEW USER MAY OBTAIN THIS APIO AS */ 00865000
*     /* SOON AS APIOASCB = 0.) A COPY OF THE APIO IS MADE TO BE USED*/ 00866000
*     /* FOR FURTHER PROCESSING (IE RELEASING THE RESOURCES          */ 00867000
*     /* ASSOCIATED WITH THIS APIO).                                 */ 00868000
*     /*                                                             */ 00869000
*     /***************************************************************/ 00870000
*                                                               02S0419 00871000
*                                                               02S0419 00872000
*     /***************************************************************/ 00873000
*     /*                                                             */ 00874000
*     /* SAVE A COPY OF THE APIO IN THE DYNAMIC STORAGE AREA. THIS   */ 00875000
*     /* ALLOWS RELEASE TO COMPLETE PROCESSING EVEN THOUGH THE REAL  */ 00876000
*     /* APIO IS RELEASED (ANOTHER USER MAY ACQUIRE THE JUST RELEASED*/ 00877000
*     /* APIO).                                                      */ 00878000
*     /*                                                             */ 00879000
*     /***************************************************************/ 00880000
*                                                               02S0419 00881000
*     RFY                                                       02S0419 00882000
*      (R0P,                                                    02S0419 00883000
*       R14P,                                                   02S0419 00884000
*       R1F,                                                    02S0419 00885000
*       R15F) RSTD;                 /* MVCL REGISTERS                */ 00886000
*     R0P=ADDR(DAPIO);              /* ADDRESS OF THE APIO COPY      */ 00887000
         LA    R0P,DAPIO                                        02S0420 00888000
*     R14P=ADDR(APIO);              /* ADDRESS OF THE REAL APIO      */ 00889000
         L     R14P,DAPIOPTR                                    02S0421 00890000
*     R1F=APIOLN;                   /* LENGTH OF APIO FOR MVCL       */ 00891000
         LA    R1F,424                                          02S0422 00892000
*     R15F=R1F;                     /* MOVE THE ENTIRE APIO          */ 00893000
         LR    R15F,R1F                                         02S0423 00894000
*     MVCL(R0P,R14P);               /* SAVE THE APIO                 */ 00895000
         MVCL  R0P,R14P                                         02S0424 00896000
*     RFY                                                       02S0425 00897000
*      (R0P,                                                    02S0425 00898000
*       R14P,                                                   02S0425 00899000
*       R1F,                                                    02S0425 00900000
*       R15F) UNRSTD;               /* DONE WITH MVCL REGS           */ 00901000
*                                                               02S0425 00902000
*     /***************************************************************/ 00903000
*     /*                                                             */ 00904000
*     /* FREE THE REAL APIO                                          */ 00905000
*     /*                                                             */ 00906000
*     /***************************************************************/ 00907000
*                                                               02S0426 00908000
*     APIODEB=0;                    /* CLEAR OWNERSHIP FLAGS         */ 00909000
         SLR   @15,@15                                          02S0426 00910000
         L     @14,DAPIOPTR                                     02S0426 00911000
         ST    @15,APIODEB(,@14)                                02S0426 00912000
*     APIOASCB=0;                   /* (DEB AND ASCB)                */ 00913000
         ST    @15,APIOASCB(,@14)                               02S0427 00914000
*     DAPIOPTR=ADDR(DAPIO);         /* CHANGE APIO POINTER TO COPY   */ 00915000
         LA    @15,DAPIO                                        02S0428 00916000
         ST    @15,DAPIOPTR                                     02S0428 00917000
*   END;                            /* END OF CODE                   */ 00918000
         EJECT                                                          00919000
*                                                               01S0430 00920000
*   /*****************************************************************/ 00921000
*   /*                                                               */ 00922000
*   /* PGFREE THE STORAGE FIXED BY THE VPSS TRANSLATE FUNCTION       */ 00923000
*   /* (IGTKCA).                                                     */ 00924000
*   /*                                                               */ 00925000
*   /*****************************************************************/ 00926000
*                                                               01S0430 00927000
*   DO;                             /* START OF CODE SEGMENT         */ 00928000
*                                                               02S0430 00929000
*     /***************************************************************/ 00930000
*     /*                                                             */ 00931000
*     /* IF THE CHANNEL PROGRAM WAS FIXED AT TRANSLATION BY IGTKCA   */ 00932000
*     /* (APIOFXED=1), THEN PGFREE THE USER'S BUFFERS AND THE CHANNEL*/ 00933000
*     /* PROGRAM.                                                    */ 00934000
*     /*                                                             */ 00935000
*     /***************************************************************/ 00936000
*                                                               02S0431 00937000
*     IF APIOFXED=BON THEN          /* PAGE FIX AT TRANSLATION ?     */ 00938000
         TM    APIOFXED(@15),B'10000000'                        02S0431 00939000
         BNO   @RF00431                                         02S0431 00940000
*       DO;                         /* YES-- THEN FREE NOW.          */ 00941000
*         DRETRY(02)=APESRTRY;      /* SAVE CURRENT RETRY ADDRESS    */ 00942000
         L     @14,APESREGP                                     02S0433 00943000
         L     @11,APESRTRY(,@14)                               02S0433 00944000
         ST    @11,DRETRY+4                                     02S0433 00945000
*         APESRTRY=ADDR(PGFREEY);   /* NEW RETRY ADDRESS             */ 00946000
         LA    @11,PGFREEY                                      02S0434 00947000
         ST    @11,APESRTRY(,@14)                               02S0434 00948000
*         RFY                                                   02S0435 00949000
*          (R0P,                                                02S0435 00950000
*           R1P,                                                02S0435 00951000
*           R4P) RSTD;              /* IEAVPSI INTERFACE REGISTERS   */ 00952000
*         R0P=0;                    /* NO ECB (FIX COMPLETED)        */ 00953000
         SLR   R0P,R0P                                          02S0436 00954000
*         R1P=APIOPGFX              /* POINT TO 1ST VSL &            */ 00955000
*             |'80000000'X;         /* SHOW LIST FORMAT.             */ 00956000
         L     R1P,APIOPGFX(,@15)                               02S0437 00957000
         O     R1P,@CF04684                                     02S0437 00958000
*         R1P->VSLFLAG1='00'X;      /* CLEAR FLAGS                   */ 00959000
         MVI   VSLFLAG1(R1P),X'00'                              02S0438 00960000
*         R1P->VSLFREE=BON;         /* SHOW PGFREE REQUEST.          */ 00961000
         OI    VSLFREE(R1P),B'00100000'                         02S0439 00962000
*         R4P=DEBTCBB;              /* ELIMINATE FOE. BELONGS TO TCB    00963000
*                                      WHICH OPENED ACB.             */ 00964000
         L     @15,DDEBPTR                                      02S0440 00965000
         L     R4P,DEBTCBB-1(,@15)                              02S0440 00966000
         LA    R4P,0(,R4P)                                      02S0440 00967000
*         APESCODE=CERRN05;         /* PGFIX ERROR CODE              */ 00968000
         MVI   APESCODE,X'05'                                   02S0441 00969000
*         GEN(MODESET EXTKEY=ZERO)                              02S0442 00970000
*         NOREFS NOSETS;            /* PSI REQUIRES KEY-0            */ 00971000
         MODESET EXTKEY=ZERO                                            00972000
*         CALL IEAVPSI;             /* FREE REQUIRED PAGES           */ 00973000
         L     @05,CVTPTR                                       02S0443 00974000
         L     @15,CVTVPSIB(,@05)                               02S0443 00975000
         BALR  @14,@15                                          02S0443 00976000
*         RFY                                                   02S0444 00977000
*          (R0P,                                                02S0444 00978000
*           R1P,                                                02S0444 00979000
*           R4P) UNRSTD;            /* FINISHED WITH REGISTERS       */ 00980000
*         RFY                                                   02S0445 00981000
*          (R15F) RSTD;             /* PSI RETURN CODE               */ 00982000
*         GEN(MODESET EXTKEY=DATAMGT)                           02S0446 00983000
*         NOREFS NOSETS;            /* RESUME NORMAL KEY             */ 00984000
         MODESET EXTKEY=DATAMGT                                         00985000
*         APESCODE=CERRN99;         /* RESET ERROR CODE              */ 00986000
         MVI   APESCODE,X'0E'                                   02S0447 00987000
*         IF R15F^=0 THEN           /* IF PGFREE FAILED, THEN        */ 00988000
         LTR   R15F,R15F                                        02S0448 00989000
         BZ    @RF00448                                         02S0448 00990000
*           DO;                     /* FORCE ERROR TO BE LOGGED.     */ 00991000
*             APESCODE=CERRN05;     /* SET ERROR CODE                */ 00992000
         MVI   APESCODE,X'05'                                   02S0450 00993000
*             DO;                   /* FORCE ABEND TO LOG ERROR &       00994000
*                                      PASS CONTROL TO THE (E)STAE OR   00995000
*                                      FRR EXIT ROUTINE.             */ 00996000
*               DO;                 /* ABEND(APCTABND)               */ 00997000
*                 RESPECIFY                                     02S0453 00998000
*                  (GPR01F) RESTRICTED;                         02S0453 00999000
*                 GPR01F=APCTABND;  /* COMP CODE IN REG 1            */ 01000000
         L     GPR01F,APCTABND(,APCTPTR)                        02S0454 01001000
*                 SVC(13);          /* ISSUE ABEND SVC               */ 01002000
         SVC   13                                               02S0455 01003000
*                 RESPECIFY                                     02S0456 01004000
*                  (GPR01F) UNRESTRICTED;                       02S0456 01005000
*               END;                /* ABEND(APCTABND) FORCE ABEND   */ 01006000
*               GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);    02S0458 01007000
*                                                  /* NO CODE           01008000
*             END;                  /* ABEND ISSUED                  */ 01009000
*           END;                    /* ERROR LOGGED                  */ 01010000
*         RFY                                                   02S0461 01011000
*           R15F UNRSTD;            /* FINISHED WITH REGISTER        */ 01012000
@RF00448 DS    0H                                               02S0462 01013000
*PGFREEY:                           /* FRR RETRY POINT               */ 01014000
*         DO;                       /* ENTRY FROM ESTAE/FRR RETRY    */ 01015000
*                                                               02S0462 01016000
PGFREEY  DS    0H                                               02S0463 01017000
*           /*********************************************************/ 01018000
*           /*                                                       */ 01019000
*           /* ENTRY FROM ESTAE/FRR RETRY (IGTUKA). RETRY DOES NOT   */ 01020000
*           /* INSURE REGISTERS 0-5, 8-11, 14-15 VALID SINCE NOT     */ 01021000
*           /* SAVED IN APESREGS. ISSUE DUMMY RESTRICT TO WIPE PL/S  */ 01022000
*           /* OPTIMIZATION MEMORY OF CURRENT REGISTER VALUES        */ 01023000
*           /*                                                       */ 01024000
*           /*********************************************************/ 01025000
*                                                               02S0463 01026000
*           RFY                                                 02S0463 01027000
*            (R0RSTD,                                           02S0463 01028000
*             R1RSTD,                                           02S0463 01029000
*             R2RSTD,                                           02S0463 01030000
*             R3RSTD,                                           02S0463 01031000
*             R4RSTD,                                           02S0463 01032000
*             R5RSTD,               /* 6-7 BASES                     */ 01033000
*             R8RSTD,                                           02S0463 01034000
*             R9RSTD,                                           02S0463 01035000
*             R10RSTD,                                          02S0463 01036000
*             R11RSTD,              /* 12-13 APCT/APSV PTRS          */ 01037000
*             R14RSTD,                                          02S0463 01038000
*             R15RSTD) RSTD;        /* KEEP PL/S FROM CARRYING DATA  */ 01039000
*           GEN(MODESET EXTKEY=DATAMGT)                         02S0464 01040000
*           NOREFS NOSETS;          /* RESUME STANDARD KEY           */ 01041000
         MODESET EXTKEY=DATAMGT                                         01042000
*           RFY                                                 02S0465 01043000
*            (R0RSTD,                                           02S0465 01044000
*             R1RSTD,                                           02S0465 01045000
*             R2RSTD,                                           02S0465 01046000
*             R3RSTD,                                           02S0465 01047000
*             R4RSTD,                                           02S0465 01048000
*             R5RSTD,               /* 6-7 BASES                     */ 01049000
*             R8RSTD,                                           02S0465 01050000
*             R9RSTD,                                           02S0465 01051000
*             R10RSTD,                                          02S0465 01052000
*             R11RSTD,              /* 12-13 APCT/APSV PTRS          */ 01053000
*             R14RSTD,                                          02S0465 01054000
*             R15RSTD) UNRSTD;      /* ALLOW PL/S TO USE REGS AGAIN  */ 01055000
*         END;                      /* RETRY ENVIRONMENT SET         */ 01056000
*         APESRTRY=DRETRY(02);      /* RESTORE RETRY POINT           */ 01057000
         L     @15,APESREGP                                     02S0467 01058000
         L     @14,DRETRY+4                                     02S0467 01059000
         ST    @14,APESRTRY(,@15)                               02S0467 01060000
*         DO;                       /* ERROR RECOVERY COMPLETE       */ 01061000
*           APESFPRV=BOFF;          /* RESET RECURSION FLAG.         */ 01062000
         NI    APESFPRV,B'10111111'                             02S0469 01063000
*           APESCODE=CERRN99;       /* RESET ERROR CODE              */ 01064000
         MVI   APESCODE,X'0E'                                   02S0470 01065000
*           APESFDMP=BON;           /* RESET DUMP/LOG FLAGS          */ 01066000
*           APESFREC=BON;           /* TO THEIR NORMAL VALUES        */ 01067000
*           APESFSDP=BON;           /* (ALL ON)                      */ 01068000
         OI    APESFDMP,B'11100000'                             02S0473 01069000
*         END;                      /* ALL INDICATORS RESET          */ 01070000
*         APESCOD1=0;               /* CLEAR ERROR INDICATOR         */ 01071000
*                                                               02S0475 01072000
         MVI   APESCOD1,X'00'                                   02S0475 01073000
*         /***********************************************************/ 01074000
*         /*                                                         */ 01075000
*         /* PGFREE VPSS VSL ENTRIES                                 */ 01076000
*         /*                                                         */ 01077000
*         /***********************************************************/ 01078000
*                                                               02S0476 01079000
*         DRETRY(02)=APESRTRY;      /* SAVE CURRENT RETRY ADDRESS    */ 01080000
         ST    @14,DRETRY+4                                     02S0476 01081000
*         APESRTRY=ADDR(PGFREEZ);   /* NEW RETRY ADDRESS             */ 01082000
         LA    @14,PGFREEZ                                      02S0477 01083000
         ST    @14,APESRTRY(,@15)                               02S0477 01084000
*         RFY                                                   02S0478 01085000
*          (R0P,                                                02S0478 01086000
*           R1P,                                                02S0478 01087000
*           R2P,                                                02S0478 01088000
*           R4P) RSTD;              /* IEAVPSI INTERFACE REGISTERS   */ 01089000
*         R0P=0;                    /* NO ECB -- PGFIX COMPLETE      */ 01090000
         SLR   R0P,R0P                                          02S0479 01091000
*         R1P=APIOPGFX              /* ADDRESS OF AREA TO PGFREE     */ 01092000
*             |'20000000'X;         /* SHOW REGISTER FORM, PGFREE.   */ 01093000
         L     @05,DAPIOPTR                                     02S0480 01094000
         L     R1P,APIOPGFX(,@05)                               02S0480 01095000
         O     R1P,@CF04688                                     02S0480 01096000
*         R2P=APIOPGFL+LENGTH(VSL)  /* LAST BYTE TO PGFREE           */ 01097000
*             |'80000000'X;         /* SET END-OF-LIST FLAG          */ 01098000
         LA    R2P,8                                            02S0481 01099000
         AL    R2P,APIOPGFL(,@05)                               02S0481 01100000
         O     R2P,@CF04684                                     02S0481 01101000
*         R4P=DEBTCBB;              /* ELIMINATE FOE. BELONGS TO TCB    01102000
*                                      WHICH OPENED ACB.             */ 01103000
         L     @03,DDEBPTR                                      02S0482 01104000
         L     R4P,DEBTCBB-1(,@03)                              02S0482 01105000
         LA    R4P,0(,R4P)                                      02S0482 01106000
*         APESCODE=CERRN05;         /* PGFIX ERROR CODE              */ 01107000
         MVI   APESCODE,X'05'                                   02S0483 01108000
*         GEN(MODESET EXTKEY=ZERO)                              02S0484 01109000
*         NOREFS NOSETS;            /* PSI REQUIRES KEY-0            */ 01110000
         MODESET EXTKEY=ZERO                                            01111000
*         CALL IEAVPSI;             /* FIX REQUIRED PAGES            */ 01112000
         L     @05,CVTPTR                                       02S0485 01113000
         L     @15,CVTVPSIB(,@05)                               02S0485 01114000
         BALR  @14,@15                                          02S0485 01115000
*         RFY                                                   02S0486 01116000
*          (R0P,                                                02S0486 01117000
*           R1P,                                                02S0486 01118000
*           R2P,                                                02S0486 01119000
*           R4P) UNRSTD;            /* FINISHED WITH REGISTERS       */ 01120000
*         RFY                                                   02S0487 01121000
*          (R15F) RSTD;             /* PSI RETURN CODE               */ 01122000
*         GEN(MODESET EXTKEY=DATAMGT)                           02S0488 01123000
*         NOREFS NOSETS;            /* RESUME NORMAL KEY             */ 01124000
         MODESET EXTKEY=DATAMGT                                         01125000
*         APESCODE=CERRN99;         /* RESET ERROR CODE              */ 01126000
         MVI   APESCODE,X'0E'                                   02S0489 01127000
*         IF R15F^=0 THEN           /* IF PGFREE FAILED, THEN        */ 01128000
         LTR   R15F,R15F                                        02S0490 01129000
         BZ    @RF00490                                         02S0490 01130000
*           DO;                     /* FORCE ERROR TO BE LOGGED.     */ 01131000
*             APESCODE=CERRN05;     /* SET ERROR CODE                */ 01132000
         MVI   APESCODE,X'05'                                   02S0492 01133000
*             DO;                   /* FORCE ABEND TO LOG ERROR &       01134000
*                                      PASS CONTROL TO THE (E)STAE OR   01135000
