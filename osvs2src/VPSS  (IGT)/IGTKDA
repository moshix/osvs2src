         ST    @15,APESRTRY(,@14)                               03S0365 00691000
*           DO;                     /* ERROR RECOVERY COMPLETE       */ 00692000
*             APESFPRV=BOFF;        /* RESET RECURSION FLAG.         */ 00693000
         NI    APESFPRV,B'10111111'                             03S0367 00694000
*             APESCODE=CERRN99;     /* RESET ERROR CODE              */ 00695000
