*   END SETAPRL;                    /* END OF SUBROUTINE             */ 01780000
         B     @EL00002                                         02S0654 01781000
         EJECT                                                          01782000
*GETLLCK:                           /* GET THE LOCAL LOCK AND        */ 01783000
*   PROC OPTIONS(NOSAVEAREA);       /* ESTABLISH FRR ENVIRONMENT     */ 01784000
*                                                               02S0655 01785000
GETLLCK  STM   @14,@12,12(@13)                                  02S0655 01786000
*   /*****************************************************************/ 01787000
*   /*                                                               */ 01788000
*   /* GET THE LOCAL LOCK. THEN ISSUE A SETFRR TO PROVIDE RECOVERY   */ 01789000
*   /* SUPPORT. THE SAME PARAMETERS AS AVAILABLE TO AN ESTAE (IF ANY)*/ 01790000
*   /* ARE PASSED ('APES' BLOCK)                                     */ 01791000
*   /*                                                               */ 01792000
*   /*****************************************************************/ 01793000
*                                                               02S0656 01794000
*                                                               02S0656 01795000
*   /*****************************************************************/ 01796000
*   /*                                                               */ 01797000
*   /* GET THE LOCAL LOCK                                            */ 01798000
*   /*                                                               */ 01799000
*   /*****************************************************************/ 01800000
*                                                               02S0656 01801000
*   STM(R11F,R13F,DSUPVSVE);        /* SAVE DESTROYED REGISTERS      */ 01802000
         STM   R11F,R13F,DSUPVSVE                               02S0656 01803000
*   APESCODE=CERRN09;               /* SET ERROR CODE                */ 01804000
         MVI   APESCODE,X'09'                                   02S0657 01805000
*   GEN REFS(PSALITA,FLC) SETS(R11F,R12F,R13F,R14F);            02S0658 01806000
*                                   /*                                  01807000
         MODESET EXTKEY=ZERO       SETLOCK REQUIRES KEY-0               01808000
