*       RFY                                                     02S0701 01965000
*         R15F RSTD;                /* WILL HOLD SETLOCK RETURN CODE */ 01966000
*       APESCODE=CERRN99;           /* RESET ERROR CODE              */ 01967000
         MVI   APESCODE,X'0E'                                   02S0702 01968000
*       R15F=R13F;                  /* HOLD RETURN CODE              */ 01969000
         LR    R15F,R13F                                        02S0703 01970000
*       LM(R11F,R13F,DSUPVSVE);     /* RESTORE DESTROYED REGISTERS   */ 01971000
         LM    R11F,R13F,DSUPVSVE                               02S0704 01972000
*       IF R15F^=0 THEN             /* IF SETLOCK FAILED,            */ 01973000
         LTR   R15F,R15F                                        02S0705 01974000
         BZ    @RF00705                                         02S0705 01975000
*         DO;                       /* THEN LOG AND TERMINATE.       */ 01976000
*           APESCODE=CERRN10;       /* SET ERROR CODE                */ 01977000
         MVI   APESCODE,X'0A'                                   02S0707 01978000
*           DO;                     /* FORCE ABEND TO LOG ERROR &       01979000
*                                      PASS CONTROL TO THE (E)STAE OR   01980000
*                                      FRR EXIT ROUTINE.             */ 01981000
*             DO;                   /* ABEND(APCTABND)               */ 01982000
*               RESPECIFY                                       02S0710 01983000
*                (GPR01F) RESTRICTED;                           02S0710 01984000
*               GPR01F=APCTABND;    /* COMP CODE IN REG 1            */ 01985000
         L     GPR01F,APCTABND(,APCTPTR)                        02S0711 01986000
*               SVC(13);            /* ISSUE ABEND SVC               */ 01987000
         SVC   13                                               02S0712 01988000
*               RESPECIFY                                       02S0713 01989000
*                (GPR01F) UNRESTRICTED;                         02S0713 01990000
*             END;                  /* ABEND(APCTABND) FORCE ABEND   */ 01991000
*             GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);      02S0715 01992000
*                                                  /* NO CODE           01993000
*           END;                    /* ABEND ISSUED                  */ 01994000
*         END;                      /* ERROR PROCESSED               */ 01995000
*       RFY                                                     02S0718 01996000
*         R15F UNRSTD;              /* FINISHED WITH RETURN CODE     */ 01997000
@RF00705 DS    0H                                               02S0719 01998000
*       APESFLLK=BOFF;              /* SHOW LOCK NOT OWNED           */ 01999000
