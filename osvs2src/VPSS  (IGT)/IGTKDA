*           RFY                                                 03S0375 00715000
*             R1P RSTD;             /* R1 MUST POINT                 */ 00716000
*           R1P=ADDR(APIO);         /* TO APIO (ALSO IOSB)           */ 00717000
         LR    R1P,@15                                          03S0376 00718000
*           APESMOD='IGTKBC  ';     /* SHOW ROUTINE IN CONTROL       */ 00719000
