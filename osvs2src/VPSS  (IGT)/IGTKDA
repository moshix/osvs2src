*           GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);        02S0324 00517000
*                                                  /* NO CODE           00518000
*         END;                      /* ABEND ISSUED                  */ 00519000
*       END;                        /* ERROR PROCESSED               */ 00520000
*                                                               02S0326 00521000
*     /***************************************************************/ 00522000
*     /*                                                             */ 00523000
*     /* SAVE THE ADDRESS OF THE APIO TO PROCESS.                    */ 00524000
*     /*                                                             */ 00525000
*     /***************************************************************/ 00526000
*                                                               02S0327 00527000
*     DAPIOPTR=APDXAPIO(APRLAPIO);  /* SAVE THE ADDRESS OF THE APIO  */ 00528000
@RF00313 LA    @15,DAPRL                                        02S0327 00529000
         L     @15,APRLAPIO(,@15)                               02S0327 00530000
         SLA   @15,2                                            02S0327 00531000
         L     @01,DAPDXPTR                                     02S0327 00532000
         L     @15,APDXAPIO-4(@15,@01)                          02S0327 00533000
         ST    @15,DAPIOPTR                                     02S0327 00534000
*     RFY                                                       02S0328 00535000
*       APIO BASED(DAPIOPTR);       /* TELL PL/S                     */ 00536000
*                                                               02S0328 00537000
*     /***************************************************************/ 00538000
*     /*                                                             */ 00539000
*     /* ALTHOUGH IT SHOULD NOT OCCUR, A CHECK IS FIRST MADE TO      */ 00540000
*     /* INSURE THAT THE APIO IS ACTUALLY 'OWNED' BY THIS DEB AND    */ 00541000
*     /* THAT IT IS A VALID APIO. IF NOT, THE ERROR IS RECORDED AND  */ 00542000
*     /* PROCESSING TERMINATED (THIS MINIMIZES THE IMPACT OF THE     */ 00543000
*     /* ERROR).                                                     */ 00544000
*     /*                                                             */ 00545000
*     /***************************************************************/ 00546000
*                                                               02S0329 00547000
*     IF APIOID^=APIOIDC|           /* IF NOT AN APIO OR             */ 00548000
*         (APIOASCB^=PSAAOLD|       /* APIO NOT "OWNED" BY THE DEB   */ 00549000
*         APIODEB^=DDEBPTR) THEN    /* (ASCB/DEB ADDRS MATCH)        */ 00550000
         CLC   APIOID(4,@15),@CC04308                           02S0329 00551000
         BNE   @RT00329                                         02S0329 00552000
         CLC   APIOASCB(4,@15),PSAAOLD                          02S0329 00553000
         BNE   @RT00329                                         02S0329 00554000
         CLC   APIODEB(4,@15),DDEBPTR                           02S0329 00555000
         BE    @RF00329                                         02S0329 00556000
@RT00329 DS    0H                                               02S0330 00557000
*       DO;                         /* IS ERROR.                     */ 00558000
*         APDXAPIO(APRLAPIO)=0;     /* PREVENT RECURSION             */ 00559000
         LA    @15,DAPRL                                        02S0331 00560000
         L     @15,APRLAPIO(,@15)                               02S0331 00561000
         SLA   @15,2                                            02S0331 00562000
