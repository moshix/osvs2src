         ST    @14,DRETRY+12                                    03S0347 00619000
*           APESRTRY=ADDR(IOACTV1); /* NEW RETRY POINT (IN-LINE)     */ 00620000
         LA    @14,IOACTV1                                      03S0348 00621000
