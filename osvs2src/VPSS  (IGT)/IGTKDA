*                                                               02S0523 01264000
*     RFY                                                       02S0523 01265000
*      (R1P) RSTD;                  /* INTERFACE REGISTER            */ 01266000
*     R1P=ADDR(DAPGF);              /* POINT TO IGTUAG INPUT         */ 01267000
         LA    R1P,DAPGF                                        02S0524 01268000
*     RFY                                                       02S0525 01269000
*       APGF BASED(R1P);            /* TELL THE COMPILER             */ 01270000
*     APGF=APGF&&APGF;              /* CLEAR UNUSED FIELDS           */ 01271000
