*               DO;                 /* ABEND(APCTABND)               */ 01137000
*                 RESPECIFY                                     02S0495 01138000
*                  (GPR01F) RESTRICTED;                         02S0495 01139000
*                 GPR01F=APCTABND;  /* COMP CODE IN REG 1            */ 01140000
         L     GPR01F,APCTABND(,APCTPTR)                        02S0496 01141000
*                 SVC(13);          /* ISSUE ABEND SVC               */ 01142000
         SVC   13                                               02S0497 01143000
*                 RESPECIFY                                     02S0498 01144000
*                  (GPR01F) UNRESTRICTED;                       02S0498 01145000
*               END;                /* ABEND(APCTABND) FORCE ABEND   */ 01146000
*               GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);    02S0500 01147000
*                                                  /* NO CODE           01148000
*             END;                  /* ABEND ISSUED                  */ 01149000
*           END;                    /* ERROR LOGGED                  */ 01150000
*         RFY                                                   02S0503 01151000
*           R15F UNRSTD;            /* FINISHED WITH REGISTER        */ 01152000
