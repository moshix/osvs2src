         TM    APRLNRC(@15),B'00000001'                         02S0589 01497000
         BNZ   @RF00589                                         02S0589 01498000
*       DO;                         /* CODE (NOT SYSTEM INITIATED)-- */ 01499000
*         APESCODE=CERRN06;         /* ERROR CODE -- BAD APRL        */ 01500000
         MVI   APESCODE,X'06'                                   02S0591 01501000
*         APESFSDP=BOFF;            /* NO SDUMP -- USER ERR IF ABEND */ 01502000
