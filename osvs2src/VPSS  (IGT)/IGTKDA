         OI    APESFFRR,B'10000000'                             02S0685 01892000
*   /*****************************************************************/ 01893000
*   /*                                                               */ 01894000
*   /* PROCESSING COMPLETE. RETURN TO CALLER                         */ 01895000
*   /*                                                               */ 01896000
*   /*****************************************************************/ 01897000
*                                                               02S0686 01898000
*   RETURN;                         /* RETURN TO CALLER              */ 01899000
@EL00003 DS    0H                                               02S0686 01900000
@EF00003 DS    0H                                               02S0686 01901000
@ER00003 LM    @14,@12,12(@13)                                  02S0686 01902000
         BR    @14                                              02S0686 01903000
*   END GETLLCK;                    /* END OF SUBROUTINE             */ 01904000
         EJECT                                                          01905000
*FREELLCK:                          /* RELEASE THE LOCAL LOCK        */ 01906000
*   PROC OPTIONS(NOSAVEAREA);       /* CANCEL THE FRR ENVIRONMENT    */ 01907000
*                                                               02S0688 01908000
FREELLCK STM   @14,@12,12(@13)                                  02S0688 01909000
*   /*****************************************************************/ 01910000
*   /*                                                               */ 01911000
*   /* CANCEL THE CURRENT FRR (SET BY GETLLCK). THEN RELEASE THE     */ 01912000
*   /* LOCAL LOCK.                                                   */ 01913000
*   /*                                                               */ 01914000
*   /*****************************************************************/ 01915000
*                                                               02S0689 01916000
*                                                               02S0689 01917000
*   /*****************************************************************/ 01918000
*   /*                                                               */ 01919000
*   /* CANCEL THE RECOVERY ENVIRONMENT                               */ 01920000
*   /*                                                               */ 01921000
*   /*****************************************************************/ 01922000
*                                                               02S0689 01923000
*   IF APESFFRR=BON THEN            /* IF SETFRR ISSUED,             */ 01924000
         TM    APESFFRR,B'10000000'                             02S0689 01925000
         BNO   @RF00689                                         02S0689 01926000
*     DO;                           /* THEN CANCEL IT.               */ 01927000
*       APESFFRR=BOFF;              /* SHOW NOT IN FRR               */ 01928000
         NI    APESFFRR,B'01111111'                             02S0691 01929000
*       APESCODE=CERRN13;           /* ERROR CODE                    */ 01930000
         MVI   APESCODE,X'0D'                                   02S0692 01931000
*       GEN REFS(PSA,PSACSTK,FRRS,FRRSCURR,FRRSEMP,FRRSELEN,FRRSCURR)   01932000
*           SETS(R14F,R15F);                                    02S0693 01933000
*                                   /*                                  01934000
         MODESET EXTKEY=ZERO       SETFRR REQUIRES KEY-0                01935000
         SETFRR D,                 CANCEL THE CURRENT ENTRY            *01936000
               WRKREGS=(R14F,R15F), * WORK REGISTERS                   *01937000
               RELATED=(GETLLCK,APES) * APES WAS INPUT                  01938000
         MODESET EXTKEY=DATAMGT    RESUME STANDARD KEY                  01939000
*       APESCODE=CERRN99;           /* RESET ERROR CODE              */ 01940000
         MVI   APESCODE,X'0E'                                   02S0694 01941000
*     END;                          /* FRR CANCELLED                 */ 01942000
*                                                               02S0695 01943000
*   /*****************************************************************/ 01944000
*   /*                                                               */ 01945000
*   /* RELEASE THE LOCAL LOCK                                        */ 01946000
*   /*                                                               */ 01947000
*   /*****************************************************************/ 01948000
*                                                               02S0696 01949000
*   IF APESFLLK=BON THEN            /* IF LOCAL LOCK OWNED,          */ 01950000
@RF00689 TM    APESFLLK,B'00100000'                             02S0696 01951000
         BNO   @RF00696                                         02S0696 01952000
*     DO;                           /* THEN RELEASE IT.              */ 01953000
*       STM(R11F,R13F,DSUPVSVE);    /* SAVE DESTROYED REGISTERS      */ 01954000
         STM   R11F,R13F,DSUPVSVE                               02S0698 01955000
*       APESCODE=CERRN10;           /* SET ERROR CODE                */ 01956000
         MVI   APESCODE,X'0A'                                   02S0699 01957000
*       GEN REFS(PSALITA,FLC) SETS(R11F,R12F,R13F,R14F);        02S0700 01958000
*                                      /*                               01959000
         MODESET EXTKEY=ZERO       SETLOCK REQUIRES KEY-0               01960000
         SETLOCK RELEASE,          RELEASE THE                         *01961000
               TYPE=LOCAL,         * LOCAL LOCK                        *01962000
