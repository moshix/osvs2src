*             GPR01F=APCTABND;      /* COMP CODE IN REG 1            */ 00574000
         L     GPR01F,APCTABND(,APCTPTR)                        02S0336 00575000
*             SVC(13);              /* ISSUE ABEND SVC               */ 00576000
         SVC   13                                               02S0337 00577000
*             RESPECIFY                                         02S0338 00578000
*              (GPR01F) UNRESTRICTED;                           02S0338 00579000
*           END;                    /* ABEND(APCTABND) FORCE ABEND   */ 00580000
*           GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);        02S0340 00581000
*                                                  /* NO CODE           00582000
*         END;                      /* ABEND ISSUED                  */ 00583000
*       END;                        /* ERROR PROCESSED               */ 00584000
*                                                               02S0342 00585000
*     /***************************************************************/ 00586000
*     /*                                                             */ 00587000
*     /* INSURE I/O NOT OUTSTANDING FOR THIS APIO. IF I/O CURRENTLY  */ 00588000
*     /* ACTIVE, AND CLOSE NOT IN PROGRESS, DO NOT PROCESS THE       */ 00589000
*     /* RELEASE. IF CLOSE IN PROGRESS, LOG THE ERROR (CLOSE ISSED   */ 00590000
*     /* PURGE TO STOP ALL I/O) AND CONTINUE AFTER CLEARING THE APIO.*/ 00591000
*     /*                                                             */ 00592000
*     /***************************************************************/ 00593000
*                                                               02S0343 00594000
*     IF APIOACTV^=0 THEN           /* IF I/O ACTIVE, IS ERROR       */ 00595000
