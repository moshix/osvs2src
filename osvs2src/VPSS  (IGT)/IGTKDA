*   RFY                                                         02S0682 01883000
*     R15P UNRSTD;                  /* SETFRR PARM FIELD SET         */ 01884000
*   GEN(MODESET EXTKEY=DATAMGT)                                 02S0683 01885000
*   NOREFS NOSETS;                  /* RESUME STANDARD KEY           */ 01886000
         MODESET EXTKEY=DATAMGT                                         01887000
*   APESCODE=CERRN99;               /* RESET ERROR CODE              */ 01888000
