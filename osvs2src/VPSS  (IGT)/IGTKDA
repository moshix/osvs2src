         SLA   @14,3                                            02S0647 01759000
         SLR   @11,@11                                          02S0647 01760000
         IC    @11,CMBXRCT-8(@14)                               02S0647 01761000
         STC   @11,APRLRCT(,@15)                                02S0647 01762000
*       APRLRCC=APCCI;              /* ERROR COMPONENT (IO)          */ 01763000
         MVI   APRLRCC(@15),X'05'                               02S0648 01764000
*       APRLRCN=CMBXRCN(APESCOD1);  /* UNIQUE ERROR NUMBER           */ 01765000
         LH    @11,CMBXRCN-8(@14)                               02S0649 01766000
         STC   @11,APRLRCN(,@15)                                02S0649 01767000
*       APRLMSGN=CMBRMT(APESCOD1);  /* IGTMBR MESSAGE NUMBER         */ 01768000
         LH    @11,CMBRMT-8(@14)                                02S0650 01769000
         STH   @11,APRLMSGN(,@15)                               02S0650 01770000
*       APRLVALN=CMBRKT(APESCOD1);  /* IGTMBR KEYWORD NUMBER         */ 01771000
         LH    @14,CMBRKT-8(@14)                                02S0651 01772000
