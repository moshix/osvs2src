         L     @01,DAPDXPTR                                     02S0331 00564000
         ST    @14,APDXAPIO-4(@15,@01)                          02S0331 00565000
*         APESCODE=CERRN08;         /* SET ERROR CODE                */ 00566000
         MVI   APESCODE,X'08'                                   02S0332 00567000
*         DO;                       /* FORCE ABEND TO LOG ERROR &       00568000
*                                      PASS CONTROL TO THE (E)STAE OR   00569000
*                                      FRR EXIT ROUTINE.             */ 00570000
*           DO;                     /* ABEND(APCTABND)               */ 00571000
*             RESPECIFY                                         02S0335 00572000
