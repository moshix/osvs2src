*   /*****************************************************************/ 01732000
*                                                               01S0642 01733000
*SETAPRL:                           /* CONVERT APESCOD1 INTO APRL    */ 01734000
*   PROC;                           /* ERROR INDICATORS              */ 01735000
*                                                               02S0642 01736000
         B     @PB00002                                         02S0642 01737000
SETAPRL  STM   @14,@12,12(@13)                                  02S0642 01738000
*   /*****************************************************************/ 01739000
*   /*                                                               */ 01740000
*   /* TAKE THE ERROR CODE (APESCOD1) AND CONVERT IT INTO ERRORS     */ 01741000
*   /* CODES WHICH ARE MOVED INTO THE APRL (PROTECTED COPY).         */ 01742000
*   /*                                                               */ 01743000
*   /*****************************************************************/ 01744000
*                                                               02S0643 01745000
*   IF APESCOD1^=0 THEN             /* IF NON-ZERO CODE, THEN        */ 01746000
         CLI   APESCOD1,0                                       02S0643 01747000
         BE    @RF00643                                         02S0643 01748000
*     DO;                           /* BUILD APRL ERROR VALUES.      */ 01749000
*       IF APESCOD1>CERRN99 THEN    /* IF INVALID VALUE, USE         */ 01750000
         CLI   APESCOD1,14                                      02S0645 01751000
         BNH   @RF00645                                         02S0645 01752000
*         APESCOD1=CERRN99;         /* 'INDETERMINATE' ERROR VALUE.  */ 01753000
         MVI   APESCOD1,X'0E'                                   02S0646 01754000
*       APRLRCT=CMBXRCT(APESCOD1);  /* ERROR TYPE                    */ 01755000
