*         DAPRLPTR->APRLFDBK=APRLFDBK;/* RESTORE ERROR CODE (OR 0)   */ 01519000
         L     @11,APRLFDBK(,@15)                               02S0598 01520000
         ST    @11,APRLFDBK(,@14)                               02S0598 01521000
*         DAPRLPTR->APRLMSGN=APRLMSGN;/* ERROR MSG NUMBER (OR 0)     */ 01522000
         LH    @11,APRLMSGN(,@15)                               02S0599 01523000
         STH   @11,APRLMSGN(,@14)                               02S0599 01524000
*         DAPRLPTR->APRLVALN=APRLVALN;/* KEYWORD FOR ERROR MSG-OR 0  */ 01525000
         LH    @15,APRLVALN(,@15)                               02S0600 01526000
         STH   @15,APRLVALN(,@14)                               02S0600 01527000
*         GEN(MODESET EXTKEY=DATAMGT)                           02S0601 01528000
*         NOREFS NOSETS;            /* RESUME THE NORMAL KEY         */ 01529000
         MODESET EXTKEY=DATAMGT                                         01530000
*         APESCODE=CERRN99;         /* DEFAULT ERROR CODE            */ 01531000
         MVI   APESCODE,X'0E'                                   02S0602 01532000
*         APESFSDP=BON;             /* SDUMP IF ERROR                */ 01533000
         OI    APESFSDP,B'00100000'                             02S0603 01534000
*       END;                        /* USER APRL UPDATED             */ 01535000
*UPDAPRLZ:                          /* FRR/ESTAE RECOVERY POINT      */ 01536000
