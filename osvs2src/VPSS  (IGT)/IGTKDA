*           GEN(MODESET EXTKEY=DATAMGT)                         03S0381 00730000
*           NOREFS NOSETS;          /* RESUME NORMAL KEY             */ 00731000
         MODESET EXTKEY=DATAMGT                                         00732000
*           RFY                                                 03S0382 00733000
*             R1P UNRSTD;           /* DONE WITH REG 1               */ 00734000
