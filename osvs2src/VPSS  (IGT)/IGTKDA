*                                                               01S0298 00426000
*   /*****************************************************************/ 00427000
*   /*                                                               */ 00428000
*   /* FIND THE APIO TO RELEASE. UPDATE THE APDX AND THE PROTECTED   */ 00429000
*   /* COPY OF THE APRL TO REFLECT THE FREED APIO.                   */ 00430000
*   /*                                                               */ 00431000
*   /*****************************************************************/ 00432000
*                                                               01S0298 00433000
*   DO;                             /* START OF CODE SEGMENT         */ 00434000
*                                                               02S0298 00435000
*     /***************************************************************/ 00436000
*     /*                                                             */ 00437000
*     /* LOCATE THE APIO BASED ON APRLAPIO. RESET THE APDX AND THE   */ 00438000
*     /* APRL (PROTECTED COPY). SAVE THE APIO ADDRESS IN 'DAPIOPTR'. */ 00439000
*     /*                                                             */ 00440000
*     /***************************************************************/ 00441000
*                                                               02S0299 00442000
*                                                               02S0299 00443000
*     /***************************************************************/ 00444000
*     /*                                                             */ 00445000
*     /* INSURE THE APRL HAS AN ASSOCIATED APIO ('XLTE' ISSUED).     */ 00446000
*     /*                                                             */ 00447000
*     /***************************************************************/ 00448000
*                                                               02S0299 00449000
*     IF APRLXIO=BOFF THEN          /* IF APRL INDICATES THAT        */ 00450000
         LA    @15,DAPRL                                        02S0299 00451000
         TM    APRLXIO(@15),B'00000100'                         02S0299 00452000
         BNZ   @RF00299                                         02S0299 00453000
*       DO;                         /* APIO DOES NOT EXIST, IS ERROR */ 00454000
*         APESCODE=CERRN02;         /* SET ERROR CODE                */ 00455000
         MVI   APESCODE,X'02'                                   02S0301 00456000
*         APESFSDP=BOFF;            /* NO SDUMP                      */ 00457000
         NI    APESFSDP,B'11011111'                             02S0302 00458000
*         DO;                       /* FORCE ABEND TO LOG ERROR &       00459000
*                                      PASS CONTROL TO THE (E)STAE OR   00460000
*                                      FRR EXIT ROUTINE.             */ 00461000
*           DO;                     /* ABEND(APCTABND)               */ 00462000
*             RESPECIFY                                         02S0305 00463000
*              (GPR01F) RESTRICTED;                             02S0305 00464000
*             GPR01F=APCTABND;      /* COMP CODE IN REG 1            */ 00465000
         L     GPR01F,APCTABND(,APCTPTR)                        02S0306 00466000
*             SVC(13);              /* ISSUE ABEND SVC               */ 00467000
         SVC   13                                               02S0307 00468000
*             RESPECIFY                                         02S0308 00469000
*              (GPR01F) UNRESTRICTED;                           02S0308 00470000
*           END;                    /* ABEND(APCTABND) FORCE ABEND   */ 00471000
*           GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);        02S0310 00472000
*                                                  /* NO CODE           00473000
*         END;                      /* ABEND ISSUED                  */ 00474000
*       END;                        /* ERROR CONDITION PROCESSED     */ 00475000
*                                                               02S0312 00476000
*     /***************************************************************/ 00477000
*     /*                                                             */ 00478000
*     /* USE APRLAPIO TO INDEX INTO APDX FOR APIO LOCATION. INSURE   */ 00479000
*     /* APRLAPIO WITHIN RANGE AND THAT THE APIO ENTRY EXISTS.       */ 00480000
*     /*                                                             */ 00481000
*     /***************************************************************/ 00482000
*                                                               02S0313 00483000
*     IF APRLAPIO<=0|               /* IF INDEX NOT WITHIN RANGE     */ 00484000
*         APRLAPIO>APDXIOCT|        /* (1 <= APRLAPIO <= APDXAPIO)   */ 00485000
*         APDXAPIO(APRLAPIO)=0 THEN /* OR NO APIO FOR THIS INDEX     */ 00486000
@RF00299 SLR   @15,@15                                          02S0313 00487000
         LA    @14,DAPRL                                        02S0313 00488000
         L     @14,APRLAPIO(,@14)                               02S0313 00489000
         CR    @14,@15                                          02S0313 00490000
         BNH   @RT00313                                         02S0313 00491000
         L     @11,DAPDXPTR                                     02S0313 00492000
         CH    @14,APDXIOCT(,@11)                               02S0313 00493000
         BH    @RT00313                                         02S0313 00494000
         SLA   @14,2                                            02S0313 00495000
         C     @15,APDXAPIO-4(@14,@11)                          02S0313 00496000
         BNE   @RF00313                                         02S0313 00497000
@RT00313 DS    0H                                               02S0314 00498000
*       DO;                         /* IS ERROR --                   */ 00499000
*         APESCODE=CERRN02;         /* SET ERROR CODE                */ 00500000
         MVI   APESCODE,X'02'                                   02S0315 00501000
*         APESFSDP=BOFF;            /* NO SDUMP -- USER ERROR        */ 00502000
         NI    APESFSDP,B'11011111'                             02S0316 00503000
*         DO;                       /* FORCE ABEND TO LOG ERROR &       00504000
*                                      PASS CONTROL TO THE (E)STAE OR   00505000
*                                      FRR EXIT ROUTINE.             */ 00506000
*           DO;                     /* ABEND(APCTABND)               */ 00507000
*             RESPECIFY                                         02S0319 00508000
*              (GPR01F) RESTRICTED;                             02S0319 00509000
*             GPR01F=APCTABND;      /* COMP CODE IN REG 1            */ 00510000
         L     GPR01F,APCTABND(,APCTPTR)                        02S0320 00511000
*             SVC(13);              /* ISSUE ABEND SVC               */ 00512000
