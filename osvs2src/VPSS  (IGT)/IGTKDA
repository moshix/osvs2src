*         R10RSTD,                                              02S0606 01566000
*         R11RSTD,                  /* 12-13 APCT/APSV PTRS          */ 01567000
*         R14RSTD,                                              02S0606 01568000
*         R15RSTD) RSTD;            /* KEEP PL/S FROM CARRYING DATA  */ 01569000
*       GEN(MODESET EXTKEY=DATAMGT)                             02S0607 01570000
*       NOREFS NOSETS;              /* RESUME STANDARD KEY           */ 01571000
         MODESET EXTKEY=DATAMGT                                         01572000
*       RFY                                                     02S0608 01573000
*        (R0RSTD,                                               02S0608 01574000
*         R1RSTD,                                               02S0608 01575000
*         R2RSTD,                                               02S0608 01576000
*         R3RSTD,                                               02S0608 01577000
*         R4RSTD,                                               02S0608 01578000
*         R5RSTD,                   /* 6-7 BASES                     */ 01579000
*         R8RSTD,                                               02S0608 01580000
*         R9RSTD,                                               02S0608 01581000
*         R10RSTD,                                              02S0608 01582000
*         R11RSTD,                  /* 12-13 APCT/APSV PTRS          */ 01583000
*         R14RSTD,                                              02S0608 01584000
*         R15RSTD) UNRSTD;          /* ALLOW PL/S TO USE REGS AGAIN  */ 01585000
*     END;                          /* RETRY ENVIRONMENT SET         */ 01586000
*     APESRTRY=DRETRY(03);          /* RESTORE RETRY POINT           */ 01587000
         L     @15,DRETRY+8                                     02S0610 01588000
         L     @14,APESREGP                                     02S0610 01589000
         ST    @15,APESRTRY(,@14)                               02S0610 01590000
*     DO;                           /* ERROR RECOVERY COMPLETE       */ 01591000
*       APESFPRV=BOFF;              /* RESET RECURSION FLAG.         */ 01592000
         NI    APESFPRV,B'10111111'                             02S0612 01593000
*       APESCODE=CERRN99;           /* RESET ERROR CODE              */ 01594000
         MVI   APESCODE,X'0E'                                   02S0613 01595000
*       APESFDMP=BON;               /* RESET DUMP/LOG FLAGS          */ 01596000
*       APESFREC=BON;               /* TO THEIR NORMAL VALUES        */ 01597000
