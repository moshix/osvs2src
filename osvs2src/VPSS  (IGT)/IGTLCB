*                                                               01S0628 01323000
ERRDUMP  DS    0H                                               01S0629 01324000
*     /***************************************************************/ 01325000
*     /*                                                             */ 01326000
*     /* ENTRY FROM ESTAE/FRR RETRY (IGTUKA). RETRY DOES NOT INSURE  */ 01327000
*     /* REGISTERS 0-5, 9-11, 14-15 VALID SINCE NOT SAVED IN         */ 01328000
*     /* APESREGS. ISSUE DUMMY RESTRICT TO WIPE PL/S OPTIMIZATION    */ 01329000
*     /* MEMORY OF CURRENT REGISTER VALUES                           */ 01330000
*     /*                                                             */ 01331000
*     /***************************************************************/ 01332000
*                                                               01S0629 01333000
*     RFY                                                       01S0629 01334000
*      (R0RSTD,                                                 01S0629 01335000
*       R1RSTD,                                                 01S0629 01336000
*       R2RSTD,                                                 01S0629 01337000
*       R3RSTD,                                                 01S0629 01338000
*       R4RSTD,                                                 01S0629 01339000
*       R5RSTD,                     /* 6-8 BASES                     */ 01340000
*       R9RSTD,                                                 01S0629 01341000
*       R10RSTD,                                                01S0629 01342000
*       R11RSTD,                    /* 12-13 APCT/APSV PTRS          */ 01343000
*       R14RSTD,                                                01S0629 01344000
*       R15RSTD) RSTD;              /* KEEP PL/S FROM CARRYING DATA  */ 01345000
