         LA    DAPDXPTR,0(,DAPDXPTR)                            02S0577 01185000
*                   IF DAPDXPTR^=0 THEN/* IF APDX EXISTS,            */ 01186000
         LTR   DAPDXPTR,DAPDXPTR                                02S0578 01187000
         BZ    @RF00578                                         02S0578 01188000
*                     DO;           /* THEN CONTINUE.                */ 01189000
*                       RFY                                     02S0580 01190000
*                         APDX BASED(DAPDXPTR);/* TELL PL/S          */ 01191000
*                       DXBLKID=CXAPDS;/* SEE IF EXCLUSIVE USER      */ 01192000
         MVI   DXBLKID,X'03'                                    02S0581 01193000
*                       DBLKADDR=ADDR(APDS);/* BY CHECKING           */ 01194000
         ST    DAPDSPTR,DBLKADDR                                02S0582 01195000
*                       RMEMORY=ADDR(APDSOWN);/* APDS                */ 01196000
         LA    RMEMORY,APDSOWN(,DAPDSPTR)                       02S0583 01197000
*                       CALL XLTE;  /* RMEMORY POINTS TO FLAG        */ 01198000
         L     @15,DXLATOR                                      02S0584 01199000
         BALR  @14,@15                                          02S0584 01200000
*                       IF EVAL(RMEMORY-/* IF THIS IS EXCLUSIVE      */ 01201000
*                           (ADDR(APDSOWN)-ADDR(APDS)))->APDSOWN=BON    01202000
*                         THEN      /* USER, APUB DEDICATED.         */ 01203000
         LR    @04,RMEMORY                                      02S0585 01204000
         SL    @04,@CF00215                                     02S0585 01205000
         TM    APDSOWN(@04),B'10000000'                         02S0585 01206000
         BNO   @RF00585                                         02S0585 01207000
*                         RMEMORY=0;/* SET INDICATOR.                */ 01208000
         SLR   RMEMORY,RMEMORY                                  02S0586 01209000
*                       IF RMEMORY=0 THEN/* IF EXCLUSIVE USER,       */ 01210000
@RF00585 LTR   RMEMORY,RMEMORY                                  02S0587 01211000
         BNZ   @RF00587                                         02S0587 01212000
*                         DO;       /* FORMAT THE APUB.              */ 01213000
*                           DXBLKID=CXAPDS;/* APDS POINTS TO         */ 01214000
         MVI   DXBLKID,X'03'                                    02S0589 01215000
*                           DBLKADDR=ADDR(APDS);/* APUB              */ 01216000
         ST    DAPDSPTR,DBLKADDR                                02S0590 01217000
*                           RMEMORY=ADDR(APDSAPUB);/* POINTER        */ 01218000
         LA    RMEMORY,APDSAPUB(,DAPDSPTR)                      02S0591 01219000
*                           CALL XLTE;/* FIND IT.                    */ 01220000
         L     @15,DXLATOR                                      02S0592 01221000
         BALR  @14,@15                                          02S0592 01222000
*                           DAPUBPTR=RMEMORY->I031P;/* FIND APUB     */ 01223000
         L     @04,I031P(,RMEMORY)                              02S0593 01224000
         ST    @04,DAPUBPTR                                     02S0593 01225000
*                           IF DAPUBPTR^=0 THEN/* IF APUB EXISTS     */ 01226000
         LTR   @04,@04                                          02S0594 01227000
         BZ    @RF00594                                         02S0594 01228000
*                             CALL DMPAPUB;/* FORMAT THE APUB        */ 01229000
         BAL   @14,DMPAPUB                                      02S0595 01230000
*                         END;      /* APUB FORMATTED                */ 01231000
@RF00594 DS    0H                                               02S0597 01232000
*                       CALL DMPAPDX;/* FORMAT THE APDX              */ 01233000
@RF00587 BAL   @14,DMPAPDX                                      02S0597 01234000
*                       DXBLKID=CXAPDX;/* BLOCK BEING REFERENCED     */ 01235000
         MVI   DXBLKID,X'08'                                    02S0598 01236000
*                       DBLKADDR=ADDR(APDX);/* ADDRESS               */ 01237000
         ST    DAPDXPTR,DBLKADDR                                02S0599 01238000
*                       RMEMORY=ADDR(APDXIOCT);/* APIO CT            */ 01239000
         LA    RMEMORY,APDXIOCT(,DAPDXPTR)                      02S0600 01240000
*                       CALL XLTE;  /* FIND COUNT                    */ 01241000
         L     @15,DXLATOR                                      02S0601 01242000
         BALR  @14,@15                                          02S0601 01243000
*                       DAPIOCT1=RMEMORY->I015F;/* SAVE COUNT        */ 01244000
         LH    @04,I015F(,RMEMORY)                              02S0602 01245000
         STH   @04,DAPIOCT1                                     02S0602 01246000
*                       DO DAPIOCT1=DAPIOCT1 TO 1 BY-1;/* APIOS      */ 01247000
         B     @DE00603                                         02S0603 01248000
@DL00603 DS    0H                                               02S0604 01249000
*                         DXBLKID=CXAPDX;/* REFERENCE BLOCK ID       */ 01250000
         MVI   DXBLKID,X'08'                                    02S0604 01251000
*                         DBLKADDR=ADDR(APDX);/* BLOCK ADDR          */ 01252000
         ST    DAPDXPTR,DBLKADDR                                02S0605 01253000
*                         RMEMORY=ADDR(APDXAPIO(DAPIOCT1));     02S0606 01254000
         SLA   @04,2                                            02S0606 01255000
