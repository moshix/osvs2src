*                         CALL XLTE;/* FIND APDXAPIO ADDRESS         */ 01257000
         L     @15,DXLATOR                                      02S0607 01258000
         BALR  @14,@15                                          02S0607 01259000
*                         DAPIOPTR=RMEMORY->I031P;/* APIO ADDRESS    */ 01260000
         L     DAPIOPTR,I031P(,RMEMORY)                         02S0608 01261000
*                         IF DAPIOPTR^=0 THEN/* IF APIO EXISTS,      */ 01262000
         LTR   DAPIOPTR,DAPIOPTR                                02S0609 01263000
         BZ    @RF00609                                         02S0609 01264000
*                           DO;     /* THEN FORMAT BLOCKS            */ 01265000
*                             RFY                               02S0611 01266000
*                               APIO BASED(DAPIOPTR);/* TELL PL/S    */ 01267000
*                             CALL DMPAPIO;/* APIO                   */ 01268000
         BAL   @14,DMPAPIO                                      02S0612 01269000
*                             CALL DMPAPCI;/* 3838 CONTROL TABLE     */ 01270000
         BAL   @14,DMPAPCI                                      02S0613 01271000
*                             CALL DMPIOFD;/* CCW, APXL, VSL, ETC    */ 01272000
         BAL   @14,DMPIOFD                                      02S0614 01273000
*                             RFY                               02S0615 01274000
*                               APIO;/* FINISHED WITH APIO           */ 01275000
*                           END;    /* APIO DATA FORMATTED           */ 01276000
*                       END;        /* ALL APIO ENTRIES PROCESSED    */ 01277000
@RF00609 SLR   @04,@04                                          02S0617 01278000
         BCTR  @04,0                                            02S0617 01279000
         AH    @04,DAPIOCT1                                     02S0617 01280000
         STH   @04,DAPIOCT1                                     02S0617 01281000
@DE00603 LTR   @04,@04                                          02S0617 01282000
         BP    @DL00603                                         02S0617 01283000
*                       RFY                                     02S0618 01284000
*                         APDX;     /* FINISHED WITH APDX            */ 01285000
*                     END;          /* APDX PROCESSED                */ 01286000
*                 END;              /* FINISHED WITH DEB             */ 01287000
@RF00578 DS    0H                                               02S0621 01288000
*               RFY                                             02S0621 01289000
*                 APDS;             /* FINISHED WITH APDS POINTER    */ 01290000
@RF00571 DS    0H                                               02S0622 01291000
*             END;                  /* FINISHED WITH APDS            */ 01292000
*           RFY                                                 02S0623 01293000
*             DSAB;                 /* FINISHED WITH DSAB POINTER    */ 01294000
@RF00562 DS    0H                                               02S0624 01295000
*         END;                      /* DSAB PROCESSED                */ 01296000
*     END;                          /* ALL DSAB ENTRIES PROCESSED    */ 01297000
*                                                               02S0625 01298000
@RF00554 DS    0H                                               02S0625 01299000
@DE00504 L     @15,DDSABPTR                                     02S0625 01300000
         LTR   @15,@15                                          02S0625 01301000
         BNZ   @DL00504                                         02S0625 01302000
*     /***************************************************************/ 01303000
*     /*                                                             */ 01304000
*     /* USER CONTROL BLOCKS DUMPED. GO TO THE EXIT ROUTINE.         */ 01305000
*     /*                                                             */ 01306000
