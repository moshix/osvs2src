         L     @15,DXLATOR                                      02S0858 02058000
         BALR  @14,@15                                          02S0858 02059000
*       DSIZE=RMEMORY->I015F;       /* CHANNEL PROGRAM SIZE          */ 02060000
         LH    DSIZE,I015F(,RMEMORY)                            02S0859 02061000
*       CALL FORMAT;                /* FORMAT THE CONTROL BLOCK      */ 02062000
         BAL   @14,FORMAT                                       02S0860 02063000
*       RFY                                                     02S0861 02064000
*         APIO;                     /* FINISHED WITH APIO            */ 02065000
*     END;                          /* CHANNEL PGM PRINTED IF WANTED */ 02066000
*   RETURN;                         /* RETURN TO CALLER              */ 02067000
@EL00010 DS    0H                                               02S0863 02068000
@EF00010 DS    0H                                               02S0863 02069000
@ER00010 LM    @14,@12,@SA00010                                 02S0863 02070000
         BR    @14                                              02S0863 02071000
*   END DMPIOFD;                    /* END OF INTERNAL SUBROUTINE    */ 02072000
         B     @EL00010                                         02S0864 02073000
         EJECT                                                          02074000
*ENQAPUB:                           /* ISSUE ENQ ON SYSZSSSS/APUB    */ 02075000
*   PROC;                           /* TO PREVENT APDS CHAIN MODS    */ 02076000
*                                                               02S0865 02077000
ENQAPUB  STM   @14,@12,@SA00011                                 02S0865 02078000
*   /*****************************************************************/ 02079000
*   /*                                                               */ 02080000
*   /* ISSUE SHARED ENQ ON SYSZSSSS/APUB TO PREVENT THE APDS CHAIN   */ 02081000
*   /* FROM BEING MODIFIED. THIS ALLOWS THE MONITOR TO DUMP A STATIC */ 02082000
*   /* APDS CHAIN. IF RESOURCE ALREADY OWNED, DO NOT FREE ON EXIT.   */ 02083000
*   /*                                                               */ 02084000
*   /*****************************************************************/ 02085000
*                                                               02S0866 02086000
*   IF DPRDMP=BOFF THEN             /* IF SNAP DUMP,                 */ 02087000
         TM    DPRDMP,B'10000000'                               02S0866 02088000
         BNZ   @RF00866                                         02S0866 02089000
*     DO;                           /* ISSUE ENQ.                    */ 02090000
*       APESEXRA=ADDR(CLRXENQ);     /* ESTAE EXIT MUST FREE          */ 02091000
         LA    @15,CLRXENQ                                      02S0868 02092000
         ST    @15,APESEXRA                                     02S0868 02093000
*       APESFEXR=BON;               /* SHOW EXIT EXISTS              */ 02094000
         OI    APESFEXR,B'00000100'                             02S0869 02095000
*       GEN REFS(CXENQ,APCTENQM,APCT,APCTPTR) SETS(DXENQ);      02S0870 02096000
*                                   /*                                  02097000
         MVC   DXENQ,CXENQ         INITIALIZE PARAMETER LIST            02098000
         ENQ   (APCTENQM-APCT(,APCTPTR),,,,), ISSUE ENQUEUE FOR        *02099000
               MF=(E,DXENQ)        SYSZSSSS/APUB TO KEEP STABLE APDS    02100000
*       RFY                                                     02S0871 02101000
*         R15F RSTD;                /* ENQ RETURN CODE               */ 02102000
*       IF R15F=0 THEN              /* IF ENQ WORKED,                */ 02103000
         LTR   R15F,R15F                                        02S0872 02104000
         BNZ   @RF00872                                         02S0872 02105000
*         DENQAPUB=BON;             /* SHOW DEQ REQUIRED.            */ 02106000
         OI    DENQAPUB,B'00100000'                             02S0873 02107000
*       ELSE                        /* IF ENQ FAILED, ASSUME RESOURCE*/ 02108000
*         APESFEXR=BOFF;            /* ALREADY OWNED, DO NOT FREE.   */ 02109000
         B     @RC00872                                         02S0874 02110000
@RF00872 NI    APESFEXR,B'11111011'                             02S0874 02111000
*       RFY                                                     02S0875 02112000
*         R15F UNRSTD;              /* FINISHED WITH RETURN CODE     */ 02113000
@RC00872 DS    0H                                               02S0876 02114000
*       GEN(SPACE 1)                                            02S0876 02115000
*       FLOWS(CLRXENQ) SEQFLOW;     /* SHOW CLRXENQ FLOW ALLOWED     */ 02116000
         SPACE 1                                                        02117000
*     END;                          /* ENQ ISSUED.                   */ 02118000
*   RETURN;                         /* RETURN TO THE CALLER          */ 02119000
@EL00011 DS    0H                                               02S0878 02120000
@EF00011 DS    0H                                               02S0878 02121000
@ER00011 LM    @14,@12,@SA00011                                 02S0878 02122000
         BR    @14                                              02S0878 02123000
*   END ENQAPUB;                    /* END OF INTERNAL SUBROUTINE    */ 02124000
         B     @EL00011                                         02S0879 02125000
*DEQAPUB:                           /* ISSUE DEQ ON SYSZSSSS/APUB    */ 02126000
*   PROC;                           /* SINCE FINISHED WITH APDS.     */ 02127000
*                                                               02S0880 02128000
DEQAPUB  STM   @14,@12,@SA00012                                 02S0880 02129000
*   /*****************************************************************/ 02130000
*   /*                                                               */ 02131000
*   /* ISSUE DEQ ON SYSZSSSS/APUB IF THE PREVIOUS ENQ (ENQAPUB)      */ 02132000
*   /* WORKED. THE APDS CHAINS HAS BEEN DUMPED.                      */ 02133000
*   /*                                                               */ 02134000
*   /*****************************************************************/ 02135000
*                                                               02S0881 02136000
*   IF DENQAPUB=BON THEN            /* IF ENQ ISSUED AND WORKED,     */ 02137000
         TM    DENQAPUB,B'00100000'                             02S0881 02138000
         BNO   @RF00881                                         02S0881 02139000
*     DO;                           /* ISSUE DEQ.                    */ 02140000
*       GEN REFS(DXENQ);                                        02S0883 02141000
*                                   /*                                  02142000
         DEQ   ,RET=NONE,          RETURN CODES NOT WANTED --          *02143000
               MF=(E,DXENQ)        * DEQ MUST WORK                      02144000
*       DENQAPUB=BOFF;              /* SHOW RESOURCE NOT OWNED       */ 02145000
         NI    DENQAPUB,B'11011111'                             02S0884 02146000
*       APESFEXR=BOFF;              /* ESTAE EXIT NOT NEEDED.        */ 02147000
         NI    APESFEXR,B'11111011'                             02S0885 02148000
*     END;                          /* DEQ ISSUED.                   */ 02149000
*   RETURN;                         /* RETURN TO THE CALLER          */ 02150000
@EL00012 DS    0H                                               02S0887 02151000
@EF00012 DS    0H                                               02S0887 02152000
@ER00012 LM    @14,@12,@SA00012                                 02S0887 02153000
         BR    @14                                              02S0887 02154000
*   END DEQAPUB;                    /* END OF INTERNAL SUBROUTINE    */ 02155000
         B     @EL00012                                         02S0888 02156000
*CLRXENQ:                           /* EXTENDED ESTAE EXIT ROUTINE   */ 02157000
*   PROC OPTIONS(NOSAVEAREA);       /* (CALLED BY IGTUKA).           */ 02158000
*                                                               02S0889 02159000
CLRXENQ  STM   @14,@12,12(@13)                                  02S0889 02160000
*   /*****************************************************************/ 02161000
*   /*                                                               */ 02162000
*   /* ERROR OCCURRED WHILE OWNING SYSZSSSS/APUB RESOURCE. THIS      */ 02163000
*   /* ROUTINE GAINS CONTROL AS AN EXTENSION OF THE ESTAE EXIT       */ 02164000
*   /* ROUTINE -- IGTUKA. THIS ROUTINE IS ENTERED IN KEY-0 SUPERVISOR*/ 02165000
*   /* STATE, ENABLED. REGISTERS 2-13 ARE AS SAVED IN APESREGS.      */ 02166000
*   /* REGISTER 14 HAS THE RETURN ADDRESS TO IGTUKA. ISSUE A         */ 02167000
*   /* CONDITIONAL DEQ FOR SYSZSSSS/APUB AND RETURN CONTROL TO IGTUKA*/ 02168000
*   /* FOR NORMAL PROCESSING.                                        */ 02169000
*   /*                                                               */ 02170000
*   /*****************************************************************/ 02171000
*                                                               02S0890 02172000
*   IF DENQAPUB=BON THEN            /* IF RESOURCE OWNED BY THIS     */ 02173000
         TM    DENQAPUB,B'00100000'                             02S0890 02174000
         BNO   @RF00890                                         02S0890 02175000
*     DO;                           /* MODULE, FREE IT.              */ 02176000
*       GEN(DEQ   ,RET=HAVE,MF=(E,DXENQ))                       02S0892 02177000
*       REFS(DXENQ) SETS(DXENQ);    /* ISSUE CONDITIONAL DEQUEUE.    */ 02178000
         DEQ   ,RET=HAVE,MF=(E,DXENQ)                                   02179000
*       DENQAPUB=BOFF;              /* RESOURCE NO LONGER OWNED      */ 02180000
         NI    DENQAPUB,B'11011111'                             02S0893 02181000
*     END;                          /* CRITICAL RESOURCE RELEASED    */ 02182000
*   RETURN;                         /* RETURN TO IGTUKA              */ 02183000
@EL00013 DS    0H                                               02S0895 02184000
@EF00013 DS    0H                                               02S0895 02185000
@ER00013 LM    @14,@12,12(@13)                                  02S0895 02186000
         BR    @14                                              02S0895 02187000
*   END CLRXENQ;                    /* END OF SUBROUTINE             */ 02188000
         B     @EL00013                                         02S0896 02189000
         EJECT                                                          02190000
*ENQTIOT:                           /* ISSUE ENQ ON SYSZTIOT/ASID    */ 02191000
*   PROC;                           /* TO PREVENT DSAB CHAIN MODS    */ 02192000
*                                                               02S0897 02193000
ENQTIOT  STM   @14,@12,@SA00014                                 02S0897 02194000
*   /*****************************************************************/ 02195000
*   /*                                                               */ 02196000
*   /* ISSUE SHARED ENQ ON SYSZTIOT/ASID-QDB TO PREVENT THE DSAB     */ 02197000
*   /* CHAIN FROM BEING MODIFIED. THIS ALLOWS A STATIC DSAB CHAIN TO */ 02198000
*   /* BE SEARCHED. THIS RESOURCE MAY NOT BE HELD ACCROSS PRINT      */ 02199000
*   /* PROCESSING SINCE EOV COULD BE REQUIRED (WHICH ALSO NEEDS THIS */ 02200000
*   /* RESOURCE)                                                     */ 02201000
*   /*                                                               */ 02202000
*   /*****************************************************************/ 02203000
*                                                               02S0898 02204000
*   IF DPRDMP=BOFF THEN             /* IF SNAP DUMP,                 */ 02205000
         TM    DPRDMP,B'10000000'                               02S0898 02206000
         BNZ   @RF00898                                         02S0898 02207000
*     DO;                           /* ISSUE ENQ.                    */ 02208000
*       DTIOTEQA=ADPLASID;          /* ASID BEING PROCESSED          */ 02209000
         L     @01,DABDPLPT                                     02S0900 02210000
         LH    @15,ADPLASID(,@01)                               02S0900 02211000
         STH   @15,DTIOTEQA                                     02S0900 02212000
*       DTIOTEQJ=DQDBPTR;           /* ADDRESS OF QDB                */ 02213000
         L     @15,DQDBPTR                                      02S0901 02214000
         ST    @15,DTIOTEQJ                                     02S0901 02215000
*       APESEXRA=ADDR(CLRYENQ);     /* ESTAE EXIT MUST FREE          */ 02216000
         LA    @15,CLRYENQ                                      02S0902 02217000
         ST    @15,APESEXRA                                     02S0902 02218000
*       APESFEXR=BON;               /* SHOW EXIT EXISTS              */ 02219000
         OI    APESFEXR,B'00000100'                             02S0903 02220000
*       GEN REFS(CYENQ,DTIOTEQ) SETS(DYENQ);                    02S0904 02221000
*                                   /*                                  02222000
         MVC   DYENQ,CYENQ         INITIALIZE PARAMETER LIST            02223000
         ENQ   (,DTIOTEQ,,,),      ISSUE ENQUEUE FOR SYSZTIOT/         *02224000
               MF=(E,DYENQ)        ASID-QDB TO KEEP STABLE DSAB         02225000
*       RFY                                                     02S0905 02226000
*         R15F RSTD;                /* ENQ RETURN CODE               */ 02227000
*       IF R15F=0 THEN              /* IF ENQ WORKED,                */ 02228000
         LTR   R15F,R15F                                        02S0906 02229000
         BNZ   @RF00906                                         02S0906 02230000
*         DENQTIOT=BON;             /* SHOW DEQ REQUIRED.            */ 02231000
         OI    DENQTIOT,B'00001000'                             02S0907 02232000
*       ELSE                        /* IF ENQ FAILED, ASSUME RESOURCE*/ 02233000
*         APESFEXR=BOFF;            /* ALREADY OWNED, DO NOT FREE.   */ 02234000
         B     @RC00906                                         02S0908 02235000
@RF00906 NI    APESFEXR,B'11111011'                             02S0908 02236000
*       RFY                                                     02S0909 02237000
*         R15F UNRSTD;              /* FINISHED WITH RETURN CODE     */ 02238000
@RC00906 DS    0H                                               02S0910 02239000
*       GEN(SPACE 1)                                            02S0910 02240000
*       FLOWS(CLRYENQ) SEQFLOW;     /* SHOW CLRYENQ FLOW ALLOWED     */ 02241000
         SPACE 1                                                        02242000
*     END;                          /* ENQ ISSUED.                   */ 02243000
*   RETURN;                         /* RETURN TO THE CALLER          */ 02244000
@EL00014 DS    0H                                               02S0912 02245000
@EF00014 DS    0H                                               02S0912 02246000
@ER00014 LM    @14,@12,@SA00014                                 02S0912 02247000
         BR    @14                                              02S0912 02248000
*   END ENQTIOT;                    /* END OF INTERNAL SUBROUTINE    */ 02249000
         B     @EL00014                                         02S0913 02250000
*DEQTIOT:                           /* ISSUE DEQ ON SYSZTIOT/ASID    */ 02251000
*   PROC;                           /* SINCE FINISHED WITH DSAB.     */ 02252000
*                                                               02S0914 02253000
DEQTIOT  STM   @14,@12,@SA00015                                 02S0914 02254000
*   /*****************************************************************/ 02255000
*   /*                                                               */ 02256000
*   /* ISSUE DEQ ON SYSZTIOT/ASID-QDB IF THE PREVIOUS ENQ (ENQTIOT)  */ 02257000
*   /* WORKED. THE DSAB CHAIN HAS BEEN SCANNED.                      */ 02258000
*   /*                                                               */ 02259000
*   /*****************************************************************/ 02260000
*                                                               02S0915 02261000
*   IF DENQTIOT=BON THEN            /* IF ENQ ISSUED AND WORKED,     */ 02262000
         TM    DENQTIOT,B'00001000'                             02S0915 02263000
         BNO   @RF00915                                         02S0915 02264000
*     DO;                           /* ISSUE DEQ.                    */ 02265000
*       GEN REFS(DYENQ);                                        02S0917 02266000
*                                   /*                                  02267000
         DEQ   ,RET=NONE,          RETURN CODES NOT WANTED --          *02268000
               MF=(E,DYENQ)        * DEQ MUST WORK                      02269000
*       DENQTIOT=BOFF;              /* SHOW RESOURCE NOT OWNED       */ 02270000
         NI    DENQTIOT,B'11110111'                             02S0918 02271000
*       APESFEXR=BOFF;              /* ESTAE EXIT NOT NEEDED.        */ 02272000
         NI    APESFEXR,B'11111011'                             02S0919 02273000
*     END;                          /* DEQ ISSUED.                   */ 02274000
*   RETURN;                         /* RETURN TO THE CALLER          */ 02275000
@EL00015 DS    0H                                               02S0921 02276000
@EF00015 DS    0H                                               02S0921 02277000
@ER00015 LM    @14,@12,@SA00015                                 02S0921 02278000
         BR    @14                                              02S0921 02279000
*   END DEQTIOT;                    /* END OF INTERNAL SUBROUTINE    */ 02280000
         B     @EL00015                                         02S0922 02281000
*CLRYENQ:                           /* EXTENDED ESTAE EXIT ROUTINE   */ 02282000
*   PROC OPTIONS(NOSAVEAREA);       /* (CALLED BY IGTUKA).           */ 02283000
*                                                               02S0923 02284000
CLRYENQ  STM   @14,@12,12(@13)                                  02S0923 02285000
*   /*****************************************************************/ 02286000
*   /*                                                               */ 02287000
*   /* ERROR OCCURRED WHILE OWNING SYSZTIOT/ASIS-QDB RESOURCE. THIS  */ 02288000
*   /* ROUTINE GAINS CONTROL AS AN EXTENSION OF THE ESTAE EXIT       */ 02289000
*   /* ROUTINE -- IGTUKA. THIS ROUTINE IS ENTERED IN KEY-0 SUPERVISOR*/ 02290000
*   /* STATE, ENABLED. REGISTERS 2-13 ARE AS SAVED IN APESREGS.      */ 02291000
*   /* REGISTER 14 HAS THE RETURN ADDRESS TO IGTUKA. ISSUE A         */ 02292000
*   /* CONDITIONAL DEQ FOR SYSZTIOT/ASID AND RETURN CONTROL TO IGTUKA*/ 02293000
*   /* FOR NORMAL PROCESSING.                                        */ 02294000
*   /*                                                               */ 02295000
*   /*****************************************************************/ 02296000
*                                                               02S0924 02297000
*   IF DENQTIOT=BON THEN            /* IF RESOURCE OWNED BY THIS     */ 02298000
         TM    DENQTIOT,B'00001000'                             02S0924 02299000
         BNO   @RF00924                                         02S0924 02300000
*     DO;                           /* MODULE, FREE IT.              */ 02301000
*       GEN(DEQ   ,RET=HAVE,MF=(E,DYENQ))                       02S0926 02302000
*       REFS(DYENQ) SETS(DYENQ);    /* ISSUE CONDITIONAL DEQUEUE.    */ 02303000
         DEQ   ,RET=HAVE,MF=(E,DYENQ)                                   02304000
*       DENQTIOT=BOFF;              /* RESOURCE NO LONGER OWNED      */ 02305000
         NI    DENQTIOT,B'11110111'                             02S0927 02306000
*     END;                          /* CRITICAL RESOURCE RELEASED    */ 02307000
*   RETURN;                         /* RETURN TO IGTUKA              */ 02308000
@EL00016 DS    0H                                               02S0929 02309000
@EF00016 DS    0H                                               02S0929 02310000
@ER00016 LM    @14,@12,12(@13)                                  02S0929 02311000
         BR    @14                                              02S0929 02312000
*   END CLRYENQ;                    /* END OF SUBROUTINE             */ 02313000
         B     @EL00016                                         02S0930 02314000
         EJECT                                                          02315000
*FORMAT:                            /* CONVERT CONTROL BLOCK TO      */ 02316000
*   PROC;                           /* PRINTABLE HEX & PRINT IT.     */ 02317000
*                                                               02S0931 02318000
FORMAT   STM   @14,@12,@SA00017                                 02S0931 02319000
*   /*****************************************************************/ 02320000
*   /*                                                               */ 02321000
*   /* FORMAT & PRINT A CONTROL BLOCK. ON ENTRY, DADDR HAS THE       */ 02322000
*   /* ADDRESS OF THE BLOCK TO PRINT AND DSIZE ITS LENGTH. DFMTSW HAS*/ 02323000
*   /* THE PROCESSING OPTIONS. DXBLKNM IS SET TO THE MBLKID INDEX OF */ 02324000
*   /* THE BLOCK LABEL BEING FORMATTED.                              */ 02325000
*   /*                                                               */ 02326000
*   /*****************************************************************/ 02327000
*                                                               02S0932 02328000
*   DRETRY(02)=APESRTRY;            /* SAVE CURRENT RETRY ADDRESS    */ 02329000
         L     @11,APESREGP                                     02S0932 02330000
         L     @05,APESRTRY(,@11)                               02S0932 02331000
         ST    @05,DRETRY+4                                     02S0932 02332000
*   APESRTRY=ADDR(FORMATZ);         /* SET NEW RETRY FOR THIS RTN    */ 02333000
         LA    @05,FORMATZ                                      02S0933 02334000
         ST    @05,APESRTRY(,@11)                               02S0933 02335000
*   GEN(SPACE 1)                                                02S0934 02336000
*   NOREFS NOSETS SEQFLOW FLOWS(FORMATZ);/* NO CODE -- FLOW ONLY     */ 02337000
         SPACE 1                                                        02338000
*   DSIZE=DSIZE&'00FFFFFF'X;        /* CLEAR HI ORDER BYTE           */ 02339000
         N     DSIZE,@CF05120                                   02S0935 02340000
*   DADDR=DADDR&'7FFFFFFF'X;        /* CLEAR EXTRANEOUS BIT          */ 02341000
         N     DADDR,@CF05231                                   02S0936 02342000
*   DSIZE0=DSIZE;                   /* SIZE OF CONTROL BLOCK         */ 02343000
         ST    DSIZE,DSIZE0                                     02S0937 02344000
*   DADDR0=DADDR;                   /* ADDRESS OF CONTROL BLOCK      */ 02345000
         LR    DADDR0,DADDR                                     02S0938 02346000
*   IF DSIZE0>CMXBLKSZ THEN         /* INSURE BLOCK SIZE NOT         */ 02347000
         L     @10,DSIZE0                                       02S0939 02348000
         L     @05,@CF05085                                     02S0939 02349000
         CR    @10,@05                                          02S0939 02350000
         BNH   @RF00939                                         02S0939 02351000
*     DSIZE0=CMXBLKSZ;              /* CLOBBERED.                    */ 02352000
         ST    @05,DSIZE0                                       02S0940 02353000
*   DSIZE4=0;                       /* CLEAR PRINT LINE              */ 02354000
*                                                               02S0941 02355000
@RF00939 SLR   @10,@10                                          02S0941 02356000
         ST    @10,DSIZE4                                       02S0941 02357000
*   /*****************************************************************/ 02358000
*   /*                                                               */ 02359000
*   /* PRINT CONTROL BLOCK TITLE                                     */ 02360000
*   /*                                                               */ 02361000
*   /*****************************************************************/ 02362000
*                                                               02S0942 02363000
*   IF DNONAME=BOFF THEN            /* IF TITLE LINE NOT SUPPRESSED  */ 02364000
         TM    DNONAME,B'00100000'                              02S0942 02365000
         BNZ   @RF00942                                         02S0942 02366000
*     DO;                           /* THEN PRINT IT                 */ 02367000
*       CALL SKIP(2);               /* BLANK LINES                   */ 02368000
         LA    @01,@AL00944                                     02S0944 02369000
         BAL   @14,SKIP                                         02S0944 02370000
*       DLINE2=CLINE2;              /* INITIALIZE HEADER             */ 02371000
         MVC   DLINE2(36),CLINE2                                02S0945 02372000
*       DLINE2A=MBLKID(DXBLKNM);    /* CONTROL BLOCK NAME            */ 02373000
         SLR   @10,@10                                          02S0946 02374000
         IC    @10,DXBLKNM                                      02S0946 02375000
         SLA   @10,3                                            02S0946 02376000
         LA    @05,MBLKID-8(@10)                                02S0946 02377000
         MVC   DLINE2A(8),0(@05)                                02S0946 02378000
*       IF DINFOL2=BON THEN         /* IF 'DBLKINFO' HAS VALID DATA, */ 02379000
         TM    DINFOL2,B'00010000'                              02S0947 02380000
         BNO   @RF00947                                         02S0947 02381000
*         DLINE2B=DBLKINFO;         /* THEN MOVE TO DATA LINE.       */ 02382000
         MVC   DLINE2B(8),DBLKINFO                              02S0948 02383000
*       RFY                                                     02S0949 02384000
*        (R0P,                                                  02S0949 02385000
*         R1P) RSTD;                /* PUT ADDRESS OF                */ 02386000
@RF00947 DS    0H                                               02S0950 02387000
*       R0P=DADDR0;                 /* BLOCK IN TITLE LINE           */ 02388000
         LR    R0P,DADDR0                                       02S0950 02389000
*       R1P=ADDR(DLINE2C);          /* (MAKE 6-DIGIT HEX)            */ 02390000
         LA    R1P,DLINE2C                                      02S0951 02391000
*       CALL CNVTADR;                                           02S0952 02392000
         BAL   @14,CNVTADR                                      02S0952 02393000
*       RFY                                                     02S0953 02394000
*        (R0P,                                                  02S0953 02395000
*         R1P) UNRSTD;                                          02S0953 02396000
*       CALL PRINT;                 /* PRINT TITLE                   */ 02397000
         BAL   @14,PRINT                                        02S0954 02398000
*       CALL SKIP(1);               /* SEPARATE FROM DATA            */ 02399000
         LA    @01,@AL00955                                     02S0955 02400000
         BAL   @14,SKIP                                         02S0955 02401000
*     END;                          /* TITLE PROCESSED               */ 02402000
*   DNONAME=BOFF;                   /* RESET NO-TITLE INDICATOR      */ 02403000
*                                                               02S0957 02404000
@RF00942 NI    DNONAME,B'11011111'                              02S0957 02405000
*   /*****************************************************************/ 02406000
*   /*                                                               */ 02407000
*   /* LOOP THRU BLOCK AND PRINT 20 CHARACTERS (HEX) PER LINE        */ 02408000
*   /*                                                               */ 02409000
*   /*****************************************************************/ 02410000
*                                                               02S0958 02411000
*   DO WHILE(DSIZE0>0);                                         02S0958 02412000
*                                                               02S0958 02413000
         B     @DE00958                                         02S0958 02414000
@DL00958 DS    0H                                               02S0959 02415000
*     /***************************************************************/ 02416000
*     /*                                                             */ 02417000
*     /* FIND SIZE TO PRINT THIS LINE (DSIZE2)                       */ 02418000
*     /*                                                             */ 02419000
*     /***************************************************************/ 02420000
*                                                               02S0959 02421000
*     IF DSIZE0<LENGTH(DT1LINE3) THEN/* IF FULL LINE NOT             */ 02422000
         L     @10,DSIZE0                                       02S0959 02423000
         LA    @05,32                                           02S0959 02424000
         CR    @10,@05                                          02S0959 02425000
         BNL   @RF00959                                         02S0959 02426000
*       DO;                         /* REMAINING, PRINT REST.        */ 02427000
*         DSIZE2=DSIZE0;            /* SET DSIZE4 TO AMOUNT OF LINE  */ 02428000
         ST    @10,DSIZE2                                       02S0961 02429000
*         DSIZE4=LENGTH(DT1LINE3)-DSIZE2;/* LINE NOT TO PRINT.       */ 02430000
         SLR   @05,@10                                          02S0962 02431000
         ST    @05,DSIZE4                                       02S0962 02432000
*       END;                        /* (FOR 0C4 & SECURITY)          */ 02433000
*     ELSE                          /* IF AT LEAST FULL LINE LEFT,   */ 02434000
*       DSIZE2=LENGTH(DT1LINE3);    /* PRINT ENTIRE BUFFER.          */ 02435000
*                                                               02S0964 02436000
         B     @RC00959                                         02S0964 02437000
@RF00959 MVC   DSIZE2(4),@CF00126                               02S0964 02438000
*     /***************************************************************/ 02439000
*     /*                                                             */ 02440000
*     /* MOVE DATA TO DT1LINE3                                       */ 02441000
*     /*                                                             */ 02442000
*     /***************************************************************/ 02443000
*                                                               02S0965 02444000
*     IF DPGMCHK=BON THEN           /* IF PROGRAM CHECK              */ 02445000
@RC00959 TM    DPGMCHK,B'01000000'                              02S0965 02446000
         BNO   @RF00965                                         02S0965 02447000
*       DO;                         /* ALLOWED (USER DATA)           */ 02448000
*         APESFSDP=BOFF;            /* DONOT RECORD OR TAKE          */ 02449000
*         APESFREC=BOFF;            /* SDUMP                         */ 02450000
         NI    APESFSDP,B'10011111'                             02S0968 02451000
*       END;                        /* APES UPDATED                  */ 02452000
*     IF DPRDMP=BON THEN            /* IF RUNNING FOR PRDMP,         */ 02453000
@RF00965 TM    DPRDMP,B'10000000'                               02S0970 02454000
         BNO   @RF00970                                         02S0970 02455000
*       DO;                         /* USE MEMORY ACCESS             */ 02456000
*                                                               02S0971 02457000
*         /***********************************************************/ 02458000
*         /*                                                         */ 02459000
*         /* PRDMP MOVE -- FIND MOVE SIZES ON BOTH SIDES OF PRDMP    */ 02460000
*         /* BUFFER                                                  */ 02461000
*         /*                                                         */ 02462000
*         /***********************************************************/ 02463000
*                                                               02S0972 02464000
*         DSIZE5=((DADDR0+(CPDBUFSZ-1))&CPDBUFRN)/* AMOUNT           */ 02465000
*             -DADDR0;              /* IN FIRST PRDMP BUFFER         */ 02466000
         LA    @10,2047                                         02S0972 02467000
         ALR   @10,DADDR0                                       02S0972 02468000
         N     @10,@CF05081                                     02S0972 02469000
         SLR   @10,DADDR0                                       02S0972 02470000
         ST    @10,DSIZE5                                       02S0972 02471000
*         IF DSIZE5>DSIZE2 THEN     /* SET DSIZE5 = AMOUNT TO PRINT  */ 02472000
         L     @05,DSIZE2                                       02S0973 02473000
         CR    @10,@05                                          02S0973 02474000
         BNH   @RF00973                                         02S0973 02475000
*           DSIZE5=DSIZE2;          /* IN FIRST BUFFER.              */ 02476000
         ST    @05,DSIZE5                                       02S0974 02477000
*         DSIZE6=DSIZE2-DSIZE5;     /* 2ND BUFFER AMOUNT TO PRINT    */ 02478000
@RF00973 L     @10,DSIZE5                                       02S0975 02479000
         L     @05,DSIZE2                                       02S0975 02480000
         SLR   @05,@10                                          02S0975 02481000
         ST    @05,DSIZE6                                       02S0975 02482000
*         IF DSIZE5>0 THEN          /* IF ANY IN INITIAL BUFFER      */ 02483000
         LTR   @10,@10                                          02S0976 02484000
         BNP   @RF00976                                         02S0976 02485000
*           DO;                     /* THEN SAVE IT.                 */ 02486000
*             RMEMORY=DADDR0;       /* CONVERT ADDRESS TO            */ 02487000
         LR    RMEMORY,DADDR0                                   02S0978 02488000
*             CALL XLTE;            /* PRDMP BUFFER ADDRESS          */ 02489000
         L     @15,DXLATOR                                      02S0979 02490000
         BALR  @14,@15                                          02S0979 02491000
*             MVC(DT1LINE3(1:DSIZE5),RMEMORY->I256C);/* SAVE IT      */ 02492000
         L     @10,DSIZE5                                       02S0980 02493000
         BCTR  @10,0                                            02S0980 02494000
         EX    @10,@SM05352                                     02S0980 02495000
*           END;                    /* FIRST BUFFER SAVED            */ 02496000
*         IF DSIZE6>0 THEN          /* IF ANY DATA IN 2ND BUFFER     */ 02497000
@RF00976 L     @10,DSIZE6                                       02S0982 02498000
         LTR   @10,@10                                          02S0982 02499000
         BNP   @RF00982                                         02S0982 02500000
*           DO;                     /* THEN SAVE IT.                 */ 02501000
*             RMEMORY=DADDR0+DSIZE5;/* CONVERT VIRTUAL ADDRESS       */ 02502000
         LR    RMEMORY,DADDR0                                   02S0984 02503000
         AL    RMEMORY,DSIZE5                                   02S0984 02504000
*             CALL XLTE;            /* TO BUFFER ADDRESS             */ 02505000
         L     @15,DXLATOR                                      02S0985 02506000
         BALR  @14,@15                                          02S0985 02507000
*             MVC(DT1LINE3(DSIZE5+1:DSIZE2),RMEMORY->I256C);/* SAVE  */ 02508000
         L     @10,DSIZE5                                       02S0986 02509000
         LA    @05,DT1LINE3(@10)                                02S0986 02510000
         L     @10,DSIZE2                                       02S0986 02511000
         SL    @10,DSIZE5                                       02S0986 02512000
         BCTR  @10,0                                            02S0986 02513000
         EX    @10,@SM05354                                     02S0986 02514000
*           END;                    /* 2ND BUFFER PROCESSED          */ 02515000
*       END;                        /* PRDMP DATA SAVED              */ 02516000
*     ELSE                          /* IF SNAP FORMATTING            */ 02517000
*       DO;                         /* THEN PROCESS HERE --          */ 02518000
*                                                               02S0989 02519000
         B     @RC00970                                         02S0989 02520000
@RF00970 DS    0H                                               02S0990 02521000
*         /***********************************************************/ 02522000
*         /*                                                         */ 02523000
*         /* EXECUTING UNDER SNAP -- JUST MOVE DATA                  */ 02524000
*         /*                                                         */ 02525000
*         /***********************************************************/ 02526000
*                                                               02S0990 02527000
*         IF DPROTSW=BOFF THEN      /* IF DATA NOT FETCH PROTECTED,  */ 02528000
         TM    DPROTSW,B'10000000'                              02S0990 02529000
         BNZ   @RF00990                                         02S0990 02530000
*           MVC(DT1LINE3(1:DSIZE2),DADDR0->I256C);/* JUST MOVE IT.   */ 02531000
         L     @10,DSIZE2                                       02S0991 02532000
         BCTR  @10,0                                            02S0991 02533000
         EX    @10,@SM05356                                     02S0991 02534000
*         ELSE                      /* OTHERWISE MUST LOOP USING     */ 02535000
*           DO;                     /* MODESET BETWEEN REF & STORE   */ 02536000
         B     @RC00990                                         02S0992 02537000
@RF00990 DS    0H                                               02S0993 02538000
*             RFY                                               02S0993 02539000
*              (R0P,                                            02S0993 02540000
*               R1F,                                            02S0993 02541000
*               R14P,                                           02S0993 02542000
*               R15P) RSTD;         /* LOOP REGISTERS                */ 02543000
*             R14P=ADDR(DT1LINE3);  /* 'TO' POINTER                  */ 02544000
         LA    R14P,DT1LINE3                                    02S0994 02545000
*             R15P=DADDR0;          /* 'FROM' POINTER                */ 02546000
         LR    R15P,DADDR0                                      02S0995 02547000
*             R1F=DPROTKEY;         /* STORAGE PROTECT KEY           */ 02548000
         SLR   R1F,R1F                                          02S0996 02549000
         IC    R1F,DPROTKEY                                     02S0996 02550000
*             DO DSIZE7=1 TO DSIZE2;/* SCAN 'FROM' FIELD             */ 02551000
         LA    DSIZE7,1                                         02S0997 02552000
         B     @DE00997                                         02S0997 02553000
@DL00997 DS    0H                                               02S0998 02554000
*               SPKA(R1F);          /* SWITCH TO STORAGE KEY         */ 02555000
         SPKA  0(R1F)                                           02S0998 02556000
*               GEN(IC    R0F,0(,R15P))                         02S0999 02557000
*               REFS(R15P) SETS(R0F);/* SAVE 1-BYTE                  */ 02558000
         IC    R0F,0(,R15P)                                             02559000
*               GEN(MODESET EXTKEY=SCHED)                       02S1000 02560000
*               NOREFS NOSETS;      /* RESUME OUR KEY                */ 02561000
         MODESET EXTKEY=SCHED                                           02562000
*               APESFREC=BON;       /* RESET APES. TAKE SDUMP        */ 02563000
*               APESFSDP=BON;       /* AND LOG ON ERRORS             */ 02564000
         OI    APESFREC,B'01100000'                             02S1002 02565000
*               R14P->I001C=R0F;    /* MOVE DATA TO OUR BUFFER       */ 02566000
         STC   R0F,I001C(,R14P)                                 02S1003 02567000
*               R14P=R14P+1;        /* INCREMENT DATA &              */ 02568000
         LA    @10,1                                            02S1004 02569000
         ALR   R14P,@10                                         02S1004 02570000
*               R15P=R15P+1;        /* BUFFER ADDRESSES.             */ 02571000
         ALR   R15P,@10                                         02S1005 02572000
*             END;                  /* LOOP THRU ALL OF DATA         */ 02573000
         AL    DSIZE7,@CF00068                                  02S1006 02574000
@DE00997 C     DSIZE7,DSIZE2                                    02S1006 02575000
         BNH   @DL00997                                         02S1006 02576000
*             RFY                                               02S1007 02577000
*              (R0P,                                            02S1007 02578000
*               R1F,                                            02S1007 02579000
*               R14P,                                           02S1007 02580000
*               R15P) UNRSTD;       /* LOOP DONE                     */ 02581000
*           END;                    /* PROTECTED DATA SAVED          */ 02582000
*       END;                        /* SNAP DATA SAVED               */ 02583000
@RC00990 DS    0H                                               02S1010 02584000
*     APESFREC=BON;                 /* RESET DUMP/RECORD FLAGS       */ 02585000
@RC00970 DS    0H                                               02S1011 02586000
*     APESFSDP=BON;                 /* IF PREVIOUSLY SET OFF.        */ 02587000
*                                                               02S1011 02588000
         OI    APESFREC,B'01100000'                             02S1011 02589000
*     /***************************************************************/ 02590000
*     /*                                                             */ 02591000
*     /* CONVERT HEX TO PRINTABLE HEX. MOVE INTO TEMPORARY AREA.     */ 02592000
*     /*                                                             */ 02593000
*     /***************************************************************/ 02594000
*                                                               02S1012 02595000
*     RFY                                                       02S1012 02596000
*      (R0F,                                                    02S1012 02597000
*       R14P,                                                   02S1012 02598000
*       R15P) RSTD;                 /* LOOP REGISTERS                */ 02599000
*     R14P=ADDR(DT2LINE3);          /* 'TO' LOCATION                 */ 02600000
         LA    R14P,DT2LINE3                                    02S1013 02601000
*     R15P=ADDR(DT1LINE3);          /* 'FROM' LOCATION               */ 02602000
         LA    R15P,DT1LINE3                                    02S1014 02603000
*     DO R0F=8 TO 1 BY-1;           /* PROCESS LINE                  */ 02604000
         LA    R0F,8                                            02S1015 02605000
@DL01015 DS    0H                                               02S1016 02606000
*       UNPK(R14P->I256C(1:9),R15P->I256C(1:5));/* MAKE 1->2 BYTES   */ 02607000
         UNPK  I256C(9,R14P),I256C(5,R15P)                      02S1016 02608000
*       R14P=R14P+8;                /* NEXT OUTPUT                   */ 02609000
         LA    R14P,8(,R14P)                                    02S1017 02610000
*       R15P=R15P+4;                /* NEXT INPUT                    */ 02611000
         LA    R15P,4(,R15P)                                    02S1018 02612000
*     END;                          /* CONTINUE LOOP                 */ 02613000
         BCTR  R0F,0                                            02S1019 02614000
         LTR   R0F,R0F                                          02S1019 02615000
         BP    @DL01015                                         02S1019 02616000
*     RFY                                                       02S1020 02617000
*      (R0F,                                                    02S1020 02618000
*       R14P,                                                   02S1020 02619000
*       R15P) UNRSTD;               /* FINISHED WITH LOOP REGS       */ 02620000
*     TR(DT2LINE3,CPRHEX);          /* MAKE CHARACTERS A-F, 0-9      */ 02621000
         TR    DT2LINE3(64),CPRHEX                              02S1021 02622000
*     TR(DT1LINE3,CPRCHAR);         /* ELIMINATE UNPRINTABLES        */ 02623000
*                                                               02S1022 02624000
         TR    DT1LINE3(32),CPRCHAR                             02S1022 02625000
*     /***************************************************************/ 02626000
*     /*                                                             */ 02627000
*     /* IF LAST LINE, ELIMINATE ENDING PORTION                      */ 02628000
*     /*                                                             */ 02629000
*     /***************************************************************/ 02630000
*                                                               02S1023 02631000
*     IF DSIZE4>0 THEN              /* IF PARTIAL LAST LINE          */ 02632000
         L     @10,DSIZE4                                       02S1023 02633000
         LTR   @10,@10                                          02S1023 02634000
         BNP   @RF01023                                         02S1023 02635000
*       DO;                         /* ELIMINATE EXCESS              */ 02636000
*         MVC(DT1LINE3(LENGTH(DT1LINE3)+1-DSIZE4:/* SET UNUSED       */ 02637000
*             LENGTH(DT1LINE3)),CBLANK);/* PORTIONS OF BOTH HEX      */ 02638000
         LA    @05,33                                           02S1025 02639000
         SLR   @05,@10                                          02S1025 02640000
         LA    @01,DT1LINE3-1(@05)                              02S1025 02641000
         LA    @05,33                                           02S1025 02642000
         SL    @05,DSIZE4                                       02S1025 02643000
         LCR   @05,@05                                          02S1025 02644000
         AL    @05,@CF00126                                     02S1025 02645000
         EX    @05,@SM05358                                     02S1025 02646000
*         MVC(DT2LINE3(LENGTH(DT2LINE3)+1-2*DSIZE4:/* AND CHARACTER  */ 02647000
*             LENGTH(DT2LINE3)),CBLANK);/* TO BLANKS.                */ 02648000
         ALR   @10,@10                                          02S1026 02649000
         LCR   @10,@10                                          02S1026 02650000
         AL    @10,@CF03824                                     02S1026 02651000
         LA    @05,DT2LINE3-1(@10)                              02S1026 02652000
         L     @10,DSIZE4                                       02S1026 02653000
         ALR   @10,@10                                          02S1026 02654000
         LCR   @10,@10                                          02S1026 02655000
         AL    @10,@CF03824                                     02S1026 02656000
         LCR   @10,@10                                          02S1026 02657000
         AL    @10,@CF00215                                     02S1026 02658000
         EX    @10,@SM05360                                     02S1026 02659000
*       END;                        /* LAST LINE PROCESSED           */ 02660000
*                                                               02S1027 02661000
*     /***************************************************************/ 02662000
*     /*                                                             */ 02663000
*     /* MOVE TO PRINT LINE                                          */ 02664000
*     /*                                                             */ 02665000
*     /***************************************************************/ 02666000
*                                                               02S1028 02667000
*     DLINE3=CLINE3;                /* INITIALIZE PRINT LINE         */ 02668000
*                                                               02S1028 02669000
@RF01023 MVC   DLINE3(120),CLINE3                               02S1028 02670000
*     /***************************************************************/ 02671000
*     /*                                                             */ 02672000
*     /* MOVE PRINTABLE HEX TO PRINT LINE. ALSO CHARACTER VALUES.    */ 02673000
*     /*                                                             */ 02674000
*     /***************************************************************/ 02675000
*                                                               02S1029 02676000
*     DLINE3B=DT2LINE3(01:08);      /* MOVE IN GROUPS                */ 02677000
         MVC   DLINE3B(8),DT2LINE3                              02S1029 02678000
*     DLINE3C=DT2LINE3(09:16);      /* OF 4-BYTES                    */ 02679000
         MVC   DLINE3C(8),DT2LINE3+8                            02S1030 02680000
*     DLINE3D=DT2LINE3(17:24);      /* (8-CHARACTERS).               */ 02681000
         MVC   DLINE3D(8),DT2LINE3+16                           02S1031 02682000
*     DLINE3E=DT2LINE3(25:32);      /* PUT EXTRA SPACE               */ 02683000
         MVC   DLINE3E(8),DT2LINE3+24                           02S1032 02684000
*     DLINE3F=DT2LINE3(33:40);      /* BETWEEN                       */ 02685000
         MVC   DLINE3F(8),DT2LINE3+32                           02S1033 02686000
*     DLINE3G=DT2LINE3(41:48);      /* X'10' BYTES                   */ 02687000
         MVC   DLINE3G(8),DT2LINE3+40                           02S1034 02688000
*     DLINE3H=DT2LINE3(49:56);      /* (HALFWAY).                    */ 02689000
         MVC   DLINE3H(8),DT2LINE3+48                           02S1035 02690000
*     DLINE3I=DT2LINE3(57:64);      /* UNUSED FIELDS BLANK           */ 02691000
         MVC   DLINE3I(8),DT2LINE3+56                           02S1036 02692000
*     DLINE3J=DT1LINE3;             /* SAVE CHARACTER FORMAT         */ 02693000
*                                                               02S1037 02694000
         MVC   DLINE3J(32),DT1LINE3                             02S1037 02695000
*     /***************************************************************/ 02696000
*     /*                                                             */ 02697000
*     /* PUT OFFSET INTO CONTROL BLOCK IN THE PRINT LINE             */ 02698000
*     /*                                                             */ 02699000
*     /***************************************************************/ 02700000
*                                                               02S1038 02701000
*     RFY                                                       02S1038 02702000
*      (R0F,                                                    02S1038 02703000
*       R1P) RSTD;                  /* CONTROL REGISTERS             */ 02704000
*     R1P=ADDR(DLINE3A);            /* PUT OFFSET HERE               */ 02705000
         LA    R1P,DLINE3A                                      02S1039 02706000
*     R0F=DADDR0-DADDR;             /* OFFSET                        */ 02707000
         LR    R0F,DADDR0                                       02S1040 02708000
         SLR   R0F,DADDR                                        02S1040 02709000
*     CALL CNVTADR;                 /* CONVERT TO PRINTABLE HEX      */ 02710000
         BAL   @14,CNVTADR                                      02S1041 02711000
*     DLINE3A(1:1)=' ';             /* FIRST CHARACTER MUST BE BLANK */ 02712000
         MVI   DLINE3A,C' '                                     02S1042 02713000
*     R1P=ADDR(DLINE3A)+1;          /* SCAN OFFSET & REPLACE LEADING */ 02714000
         LA    R1P,DLINE3A                                      02S1043 02715000
         LA    R1P,1(,R1P)                                      02S1043 02716000
*     DO R0F=LENGTH(DLINE3A)-2 TO 1 BY-1/* ZEROS WITH BLANKS         */ 02717000
*           WHILE(R1P->I001C='0');  /* IF ALL ZEROS, LEAVE           */ 02718000
         LA    R0F,4                                            02S1044 02719000
@DL01044 CLI   I001C(R1P),C'0'                                  02S1044 02720000
         BNE   @DC01044                                         02S1044 02721000
*       R1P->I001C=' ';             /* 1-BLANK                       */ 02722000
         MVI   I001C(R1P),C' '                                  02S1045 02723000
*       R1P=R1P+1;                                              02S1046 02724000
         LA    R1P,1(,R1P)                                      02S1046 02725000
*     END;                                                      02S1047 02726000
         BCTR  R0F,0                                            02S1047 02727000
         LTR   R0F,R0F                                          02S1047 02728000
         BP    @DL01044                                         02S1047 02729000
@DC01044 DS    0H                                               02S1048 02730000
*     R1P=R1P-1;                    /* POINT TO LAST BLANK SPOT      */ 02731000
         BCTR  R1P,0                                            02S1048 02732000
*     R1P->I001C='+';               /* SHOW OFFSET, NOT ADDRESS.     */ 02733000
         MVI   I001C(R1P),C'+'                                  02S1049 02734000
*     RFY                                                       02S1050 02735000
*      (R0F,                                                    02S1050 02736000
*       R1P) UNRSTD;                /* FINSIHED WITH REGISTERS       */ 02737000
*                                                               02S1050 02738000
*     /***************************************************************/ 02739000
*     /*                                                             */ 02740000
*     /* PRINT THE LINE                                              */ 02741000
*     /*                                                             */ 02742000
*     /***************************************************************/ 02743000
*                                                               02S1051 02744000
*     CALL PRINT;                   /* PRINT DATA LINE               */ 02745000
*                                                               02S1051 02746000
         BAL   @14,PRINT                                        02S1051 02747000
*     /***************************************************************/ 02748000
*     /*                                                             */ 02749000
*     /* PROCESS THE NEXT LINE                                       */ 02750000
*     /*                                                             */ 02751000
*     /***************************************************************/ 02752000
*                                                               02S1052 02753000
*     DADDR0=DADDR0+LENGTH(DT1LINE3);/* NEXT CNTRL BLK LOCATION      */ 02754000
         LA    @10,32                                           02S1052 02755000
         ALR   DADDR0,@10                                       02S1052 02756000
*     DSIZE0=DSIZE0-LENGTH(DT1LINE3);/* REMAINING SIZE               */ 02757000
         LCR   @10,@10                                          02S1053 02758000
         AL    @10,DSIZE0                                       02S1053 02759000
         ST    @10,DSIZE0                                       02S1053 02760000
*   END;                            /* CONTINUE PRINT LINE           */ 02761000
*                                                               02S1054 02762000
@DE00958 L     @10,DSIZE0                                       02S1054 02763000
         LTR   @10,@10                                          02S1054 02764000
         BP    @DL00958                                         02S1054 02765000
*   /*****************************************************************/ 02766000
*   /*                                                               */ 02767000
*   /* CONTROL BLOCK PRINTED. RETURN TO CALLER.                      */ 02768000
*   /*                                                               */ 02769000
*   /*****************************************************************/ 02770000
*                                                               02S1055 02771000
*FORMATX:                           /* ERROR-EXIT RETURN LABLE       */ 02772000
*   DO;                             /* ENTRY FROM ESTAE/FRR RETRY    */ 02773000
*                                                               02S1055 02774000
FORMATX  DS    0H                                               02S1056 02775000
*     /***************************************************************/ 02776000
*     /*                                                             */ 02777000
*     /* ENTRY FROM ESTAE/FRR RETRY (IGTUKA). RETRY DOES NOT INSURE  */ 02778000
*     /* REGISTERS 0-5, 9-11, 14-15 VALID SINCE NOT SAVED IN         */ 02779000
*     /* APESREGS. ISSUE DUMMY RESTRICT TO WIPE PL/S OPTIMIZATION    */ 02780000
*     /* MEMORY OF CURRENT REGISTER VALUES                           */ 02781000
*     /*                                                             */ 02782000
*     /***************************************************************/ 02783000
*                                                               02S1056 02784000
*     RFY                                                       02S1056 02785000
*      (R0RSTD,                                                 02S1056 02786000
*       R1RSTD,                                                 02S1056 02787000
*       R2RSTD,                                                 02S1056 02788000
*       R3RSTD,                                                 02S1056 02789000
*       R4RSTD,                                                 02S1056 02790000
*       R5RSTD,                     /* 6-8 BASES                     */ 02791000
*       R9RSTD,                                                 02S1056 02792000
*       R10RSTD,                                                02S1056 02793000
*       R11RSTD,                    /* 12-13 APCT/APSV PTRS          */ 02794000
*       R14RSTD,                                                02S1056 02795000
*       R15RSTD) RSTD;              /* KEEP PL/S FROM CARRYING DATA  */ 02796000
*     IF DPRDMP=BOFF THEN           /* IF RUNNING UNDER SNAP         */ 02797000
         TM    DPRDMP,B'10000000'                               02S1057 02798000
         BNZ   @RF01057                                         02S1057 02799000
*       GEN(MODESET EXTKEY=SCHED)                               02S1058 02800000
*       NOREFS NOSETS;              /* SWITCH TO STANDARD KEY (1)    */ 02801000
         MODESET EXTKEY=SCHED                                           02802000
*     RFY                                                       02S1059 02803000
*      (R0RSTD,                                                 02S1059 02804000
*       R1RSTD,                                                 02S1059 02805000
*       R2RSTD,                                                 02S1059 02806000
*       R3RSTD,                                                 02S1059 02807000
*       R4RSTD,                                                 02S1059 02808000
*       R5RSTD,                     /* 6-8 BASES                     */ 02809000
*       R9RSTD,                                                 02S1059 02810000
*       R10RSTD,                                                02S1059 02811000
*       R11RSTD,                    /* 12-13 APCT/APSV PTRS          */ 02812000
*       R14RSTD,                                                02S1059 02813000
*       R15RSTD) UNRSTD;            /* ALLOW PL/S TO USE REGS AGAIN  */ 02814000
@RF01057 DS    0H                                               02S1060 02815000
*   END;                            /* RETRY ENVIRONMENT SET         */ 02816000
*   APESRTRY=DRETRY(02);            /* RESTORE RETRY ADDRESS         */ 02817000
         L     @15,DRETRY+4                                     02S1061 02818000
         L     @14,APESREGP                                     02S1061 02819000
         ST    @15,APESRTRY(,@14)                               02S1061 02820000
*   DO;                             /* ERROR RECOVERY COMPLETE       */ 02821000
*     APESFPRV=BOFF;                /* RESET RECURSION FLAG.         */ 02822000
         NI    APESFPRV,B'10111111'                             02S1063 02823000
*     APESCODE=CERRN99;             /* RESET ERROR CODE              */ 02824000
         MVI   APESCODE,X'01'                                   02S1064 02825000
*     APESFREC=BON;                 /* RESET DUMP/LOG FLAGS          */ 02826000
*     APESFSDP=BON;                 /* (ALL ON)                      */ 02827000
         OI    APESFREC,B'01100000'                             02S1066 02828000
*   END;                            /* ALL INDICATORS RESET          */ 02829000
*   DFMTSW='00'X;                   /* RESET PROCESSING FLAGS        */ 02830000
         MVI   DFMTSW,X'00'                                     02S1068 02831000
*   RETURN;                         /* RETURN -- BLOCK PRINTED       */ 02832000
@EL00017 DS    0H                                               02S1069 02833000
@EF00017 DS    0H                                               02S1069 02834000
@ER00017 LM    @14,@12,@SA00017                                 02S1069 02835000
         BR    @14                                              02S1069 02836000
         EJECT                                                          02837000
*FORMATZ:                           /* RETRY -- ERROR FORMATTING BLK */ 02838000
*   DO;                             /* ENTRY FROM ESTAE/FRR RETRY    */ 02839000
*                                                               02S1070 02840000
FORMATZ  DS    0H                                               02S1071 02841000
*     /***************************************************************/ 02842000
*     /*                                                             */ 02843000
*     /* ENTRY FROM ESTAE/FRR RETRY (IGTUKA). RETRY DOES NOT INSURE  */ 02844000
*     /* REGISTERS 0-5, 9-11, 14-15 VALID SINCE NOT SAVED IN         */ 02845000
*     /* APESREGS. ISSUE DUMMY RESTRICT TO WIPE PL/S OPTIMIZATION    */ 02846000
*     /* MEMORY OF CURRENT REGISTER VALUES                           */ 02847000
*     /*                                                             */ 02848000
*     /***************************************************************/ 02849000
*                                                               02S1071 02850000
*     RFY                                                       02S1071 02851000
*      (R0RSTD,                                                 02S1071 02852000
*       R1RSTD,                                                 02S1071 02853000
*       R2RSTD,                                                 02S1071 02854000
*       R3RSTD,                                                 02S1071 02855000
*       R4RSTD,                                                 02S1071 02856000
*       R5RSTD,                     /* 6-8 BASES                     */ 02857000
*       R9RSTD,                                                 02S1071 02858000
*       R10RSTD,                                                02S1071 02859000
*       R11RSTD,                    /* 12-13 APCT/APSV PTRS          */ 02860000
*       R14RSTD,                                                02S1071 02861000
*       R15RSTD) RSTD;              /* KEEP PL/S FROM CARRYING DATA  */ 02862000
*     IF DPRDMP=BOFF THEN           /* IF RUNNING UNDER SNAP         */ 02863000
         TM    DPRDMP,B'10000000'                               02S1072 02864000
         BNZ   @RF01072                                         02S1072 02865000
*       GEN(MODESET EXTKEY=SCHED)                               02S1073 02866000
*       NOREFS NOSETS;              /* SWITCH TO STANDARD KEY (1)    */ 02867000
         MODESET EXTKEY=SCHED                                           02868000
*     RFY                                                       02S1074 02869000
*      (R0RSTD,                                                 02S1074 02870000
*       R1RSTD,                                                 02S1074 02871000
*       R2RSTD,                                                 02S1074 02872000
*       R3RSTD,                                                 02S1074 02873000
*       R4RSTD,                                                 02S1074 02874000
*       R5RSTD,                     /* 6-8 BASES                     */ 02875000
*       R9RSTD,                                                 02S1074 02876000
*       R10RSTD,                                                02S1074 02877000
*       R11RSTD,                    /* 12-13 APCT/APSV PTRS          */ 02878000
*       R14RSTD,                                                02S1074 02879000
*       R15RSTD) UNRSTD;            /* ALLOW PL/S TO USE REGS AGAIN  */ 02880000
@RF01072 DS    0H                                               02S1075 02881000
*   END;                            /* RETRY ENVIRONMENT SET         */ 02882000
*   DREFBLK=BOFF;                   /* CLEAR REFERENCE ERROR FLAG    */ 02883000
         NI    DREFBLK,B'11101111'                              02S1076 02884000
*   APESRTRY=ADDR(FORMATX);         /* JUST LEAVE IF 2ND ERROR       */ 02885000
         LA    @15,FORMATX                                      02S1077 02886000
         L     @14,APESREGP                                     02S1077 02887000
         ST    @15,APESRTRY(,@14)                               02S1077 02888000
*   DO;                             /* ERROR RECOVERY COMPLETE       */ 02889000
*     APESFPRV=BOFF;                /* RESET RECURSION FLAG.         */ 02890000
         NI    APESFPRV,B'10111111'                             02S1079 02891000
*     APESCODE=CERRN99;             /* RESET ERROR CODE              */ 02892000
         MVI   APESCODE,X'01'                                   02S1080 02893000
*     APESFREC=BON;                 /* RESET DUMP/LOG FLAGS          */ 02894000
*     APESFSDP=BON;                 /* (ALL ON)                      */ 02895000
         OI    APESFREC,B'01100000'                             02S1082 02896000
*   END;                            /* ALL INDICATORS RESET          */ 02897000
*   GEN(SPACE 1)                                                02S1084 02898000
*   NOREFS NOSETS FLOWS(FORMATX);   /* SHOW FLOW                     */ 02899000
*                                                               02S1084 02900000
         SPACE 1                                                        02901000
*   /*****************************************************************/ 02902000
*   /*                                                               */ 02903000
*   /* BUILD & PRINT ERROR MESSAGE                                   */ 02904000
*   /*                                                               */ 02905000
*   /*****************************************************************/ 02906000
*                                                               02S1085 02907000
*   CALL SKIP(1);                   /* PRINT 1 BLANK LINE            */ 02908000
         LA    @01,@AL01085                                     02S1085 02909000
         BAL   @14,SKIP                                         02S1085 02910000
*   DLINEX=CLINEX;                  /* CLEAR PRINT LINE              */ 02911000
         MVC   DLINEX(120),CLINEX                               02S1086 02912000
*   DLINE4=CLINE4;                  /* ERROR MESSAGE                 */ 02913000
         MVC   DLINE4(32),CLINE4                                02S1087 02914000
*   CALL PRINT;                     /* PRINT IT                      */ 02915000
         BAL   @14,PRINT                                        02S1088 02916000
*   GOTO FORMATX;                   /* RETURN TO CALLER              */ 02917000
         B     FORMATX                                          02S1089 02918000
*   END FORMAT;                     /* END OF FORMATTER              */ 02919000
         EJECT                                                          02920000
*CNVTADR:                                                       02S1091 02921000
*   PROC OPTIONS(PARMREG);          /* PRINTABLE HEX (6 CHARACTERS)  */ 02922000
*                                                               02S1091 02923000
CNVTADR  STM   @14,@12,12(@13)                                  02S1091 02924000
*   /*****************************************************************/ 02925000
*   /*                                                               */ 02926000
*   /* THIS SUBROUTINE CONVERTS THE ADDRESS CONTAINED IN REGISTER 0  */ 02927000
*   /* TO CHARACTER HEXADECIMAL (6 CHARACTERS) AND PLACES THE VALUE  */ 02928000
*   /* AT THE ADDRESS SPECIFIED IN GENERAL REGISTER 1.               */ 02929000
*   /*                                                               */ 02930000
*   /*****************************************************************/ 02931000
*                                                               02S1092 02932000
*   DCL                             /* INTERNAL DYNAMIC AREA VALUES  */ 02933000
*     1 DCNVT1 CHAR(5) BDY(WORD),   /* ADDRESS TO CONVERT            */ 02934000
*      2 DCNVT1A FIXED(31),         /* ADDRESS TO CONVERT            */ 02935000
*      2 * CHAR(1),                 /* EXTRA BYTE FOR UNPACK         */ 02936000
*     1 DCNVT2 CHAR(9) BDY(BYTE),   /* CONVERTED ADDRESS (8 CHAR)    */ 02937000
*      2 * CHAR(2),                 /* ALWAYS 00 -- NOT USED         */ 02938000
*      2 DCNVT2A CHAR(6),           /* CONVERTED ADDRESS (6-CHAR)    */ 02939000
*      2 * CHAR(1);                 /* EXTRA BYTE FOR UNPACK         */ 02940000
*   RFY                                                         02S1093 02941000
*    (R0F,                                                      02S1093 02942000
*     R1P) RSTD;                    /* INPUT VALUES                  */ 02943000
*   DCNVT1A=R0F;                    /* SAVE ADDRESS TO CONVERT       */ 02944000
         ST    R0F,DCNVT1A                                      02S1094 02945000
*   UNPK(DCNVT2,DCNVT1);            /* MAKE 1->2 BYTES               */ 02946000
         UNPK  DCNVT2(9),DCNVT1(5)                              02S1095 02947000
*   TR(DCNVT2A,CPRHEX);             /* MAKE PRINTABLE HEX            */ 02948000
         TR    DCNVT2A(6),CPRHEX                                02S1096 02949000
*   R1P->I256C(1:LENGTH(DCNVT2A))=DCNVT2A;/* MOVE TO OUTPUT LINE     */ 02950000
         MVC   I256C(6,R1P),DCNVT2A                             02S1097 02951000
*   RFY                                                         02S1098 02952000
*    (R0F,                                                      02S1098 02953000
*     R1P) UNRSTD;                  /* FINISHED WITH INPUT PARMS     */ 02954000
*   RETURN;                         /* RETURN TO CALLER              */ 02955000
@EL00018 DS    0H                                               02S1099 02956000
@EF00018 DS    0H                                               02S1099 02957000
@ER00018 LM    @14,@12,12(@13)                                  02S1099 02958000
         BR    @14                                              02S1099 02959000
*   END CNVTADR;                    /* END OF INTERNAL SUBROUTINE    */ 02960000
         EJECT                                                          02961000
*SKIP:                              /* SKIP 'SKIPNO' LINES           */ 02962000
*   PROC(SKIPNO);                   /* NOTE: NO CARRIAGE CNTRL       */ 02963000
*                                                               02S1101 02964000
SKIP     STM   @14,@12,@SA00019                                 02S1101 02965000
         MVC   @PC00019(4),0(@01)                               02S1101 02966000
*   /*****************************************************************/ 02967000
*   /*                                                               */ 02968000
*   /* SKIP 'SKIPNO' LINES BY PRINTING BLANK LINES. CARRIAGE CONTROL */ 02969000
*   /* CHARACTERS CANNOT BE USED FOR THIS PURPOSE.                   */ 02970000
*   /*                                                               */ 02971000
*   /*****************************************************************/ 02972000
*                                                               02S1102 02973000
*   DCL                                                         02S1102 02974000
*     SKIPNO FIXED(31);             /* INPUT PARAMETER               */ 02975000
*   RFY                                                         02S1103 02976000
*     RSKIPCT RSTD;                 /* LOOP COUNTER                  */ 02977000
*   RSKIPCT=SKIPNO;                 /* NUMBER OF LINES TO SKIP       */ 02978000
         L     @15,@PC00019                                     02S1104 02979000
         L     RSKIPCT,SKIPNO(,@15)                             02S1104 02980000
*   DO RSKIPCT=RSKIPCT TO 1 BY-1;   /* PRINT 'SKIPNO' LINES          */ 02981000
         B     @DE01105                                         02S1105 02982000
@DL01105 DS    0H                                               02S1106 02983000
*     RFY                                                       02S1106 02984000
*       R1P RSTD;                   /* INTERFACE REGISTER            */ 02985000
*     R1P=ADDR(ABDPL);              /* POINT TO PARAMETER LIST       */ 02986000
         L     R1P,DABDPLPT                                     02S1107 02987000
*     IF DPRDMP=BOFF THEN           /* IF SNAP,                      */ 02988000
         TM    DPRDMP,B'10000000'                               02S1108 02989000
         BNZ   @RF01108                                         02S1108 02990000
*       GEN(MODESET EXTKEY=ZERO)                                02S1109 02991000
*       NOREFS NOSETS;              /* SWITCH TO SNAP KEY (0).       */ 02992000
         MODESET EXTKEY=ZERO                                            02993000
*     R1P->ADPLBUF->I256C(1:LENGTH(CLINEX))=CLINEX;/* BLANK LINE     */ 02994000
@RF01108 L     @02,ADPLBUF(,R1P)                                02S1110 02995000
         MVC   I256C(120,@02),CLINEX                            02S1110 02996000
*     CALL R1P->ADPLPRNT->PRINTRTN; /* PRINT A BLANK LINE            */ 02997000
         L     @15,ADPLPRNT(,R1P)                               02S1111 02998000
         BALR  @14,@15                                          02S1111 02999000
*     IF DPRDMP=BOFF THEN           /* IF SNAP,                      */ 03000000
         TM    DPRDMP,B'10000000'                               02S1112 03001000
         BNZ   @RF01112                                         02S1112 03002000
*       GEN(MODESET EXTKEY=SCHED)                               02S1113 03003000
*       NOREFS NOSETS;              /* SWITCH TO STANDARD KEY (1).   */ 03004000
         MODESET EXTKEY=SCHED                                           03005000
*     RFY                                                       02S1114 03006000
*       R1P UNRSTD;                 /* DONE WITH REGISTER            */ 03007000
@RF01112 DS    0H                                               02S1115 03008000
*   END;                            /* CONTINUE LOOP                 */ 03009000
         BCTR  RSKIPCT,0                                        02S1115 03010000
@DE01105 LTR   RSKIPCT,RSKIPCT                                  02S1115 03011000
         BP    @DL01105                                         02S1115 03012000
*   RFY                                                         02S1116 03013000
*     RSKIPCT UNRSTD;               /* FINISHED WITH COUNTER         */ 03014000
*   RETURN;                         /* RETURN TO CALLER              */ 03015000
@EL00019 DS    0H                                               02S1117 03016000
@EF00019 DS    0H                                               02S1117 03017000
@ER00019 LM    @14,@12,@SA00019                                 02S1117 03018000
         BR    @14                                              02S1117 03019000
*   END SKIP;                       /* END OF INTERNAL SUBROUTINE    */ 03020000
         EJECT                                                          03021000
*PRINT:                             /* PRINT THE LINE                */ 03022000
*   PROC;                           /* 'DLINEX'                      */ 03023000
*                                                               02S1119 03024000
PRINT    STM   @14,@12,@SA00020                                 02S1119 03025000
*   /*****************************************************************/ 03026000
*   /*                                                               */ 03027000
*   /* MOVE THE PRINT LINE 'DLINEX' TO THE ACTUAL PRINT BUFFER AND   */ 03028000
*   /* THEN CALL THE SNAP/PRDMP PRINT ROUTINE. ON RETURN, CLEAR THE  */ 03029000
*   /* BUFFER.                                                       */ 03030000
*   /*                                                               */ 03031000
*   /*****************************************************************/ 03032000
*                                                               02S1120 03033000
*   IF DPRDMP=BOFF THEN             /* IF EXECUTING FOR SNAP,        */ 03034000
         TM    DPRDMP,B'10000000'                               02S1120 03035000
         BNZ   @RF01120                                         02S1120 03036000
*     GEN(MODESET EXTKEY=ZERO)                                  02S1121 03037000
*     NOREFS NOSETS;                /* SWITCH TO BUFFER KEY (0).     */ 03038000
         MODESET EXTKEY=ZERO                                            03039000
*   RFY                                                         02S1122 03040000
*     R1P RSTD;                     /* INTERFACE REGISTER            */ 03041000
@RF01120 DS    0H                                               02S1123 03042000
*   R1P=ADDR(ABDPL);                /* POINT TO SNAP/PRDMP PARM LIST */ 03043000
         L     R1P,DABDPLPT                                     02S1123 03044000
*   R1P->ADPLBUF->I256C(1:LENGTH(DLINEX))=DLINEX;/* MOVE DATA TO        03045000
*                                      SNAP/PRDMP OUTPUT BUFFER.     */ 03046000
         L     @15,ADPLBUF(,R1P)                                02S1124 03047000
         MVC   I256C(120,@15),DLINEX                            02S1124 03048000
*   CALL R1P->ADPLPRNT->PRINTRTN;   /* PRINT IT                      */ 03049000
         L     @15,ADPLPRNT(,R1P)                               02S1125 03050000
         BALR  @14,@15                                          02S1125 03051000
*   RFY                                                         02S1126 03052000
*     R1P UNRSTD;                   /* DONE WITH INTERFACE REGISTER  */ 03053000
*   IF DPRDMP=BOFF THEN             /* IF EXECUTING FOR SNAP,        */ 03054000
         TM    DPRDMP,B'10000000'                               02S1127 03055000
         BNZ   @RF01127                                         02S1127 03056000
*     GEN(MODESET EXTKEY=SCHED)                                 02S1128 03057000
*     NOREFS NOSETS;                /* RESUME STANDARD KEY           */ 03058000
         MODESET EXTKEY=SCHED                                           03059000
*   DLINEX=CLINEX;                  /* CLEAR INTERNAL BUFFER         */ 03060000
@RF01127 MVC   DLINEX(120),CLINEX                               02S1129 03061000
*   RETURN;                         /* RETURN TO CALLER              */ 03062000
@EL00020 DS    0H                                               02S1130 03063000
@EF00020 DS    0H                                               02S1130 03064000
@ER00020 LM    @14,@12,@SA00020                                 02S1130 03065000
         BR    @14                                              02S1130 03066000
*   END PRINT;                      /* END OF INTERNAL SUBROUTINE    */ 03067000
         EJECT                                                          03068000
*SNAPXLT:                           /* SNAP MEMORY ACCESS            */ 03069000
*                                                               02S1132 03070000
*   /*****************************************************************/ 03071000
*   /*                                                               */ 03072000
*   /* THIS IS A DUMMY ROUTINE TO CONVERT THE ADDRESS IN 'RMEMORY' TO*/ 03073000
*   /* A CURRENT VIRTUAL ADDRESS. THIS PROVIDES COMPATIBILITY WITH   */ 03074000
*   /* ROUTINE 'PRDMPXLT' WHICH INTERFACES WITH THE PRDMP MEMORY     */ 03075000
*   /* ACCESS ROUTINE. SNAPXLT REFERENCES THE INPUT STORAGE TO INSURE*/ 03076000
*   /* IT IS VALID. THEN CONTROL RETURNS TO THE CALLER WITH THE      */ 03077000
*   /* REGISTERS UNCHANGED (OUTPUT ADDRESS = INPUT ADDRESS).         */ 03078000
*   /*                                                               */ 03079000
*   /*****************************************************************/ 03080000
*                                                               02S1132 03081000
*   PROC OPTIONS(NOSAVE,            /* DO NOT SAVE INPUT REGS        */ 03082000
*       NOSAVEAREA);                /* NO SAVEAREA NEEDED            */ 03083000
SNAPXLT  DS    0H                                               02S1133 03084000
*   DREFBLK=BON;                    /* SHOW REFERENCING CONTROL BLOCK*/ 03085000
         OI    DREFBLK,B'00010000'                              02S1133 03086000
*   GEN(CLI   0(RMEMORY),*-*)                                   02S1134 03087000
*   REFS(RMEMORY) NOSETS;           /* INSURE VALID STORAGE.         */ 03088000
         CLI   0(RMEMORY),*-*                                           03089000
*   DREFBLK=BOFF;                   /* STORAGE OK IF HERE.           */ 03090000
         NI    DREFBLK,B'11101111'                              02S1135 03091000
*   RMEMORY=RMEMORY;                /* OUTPUT=INPUT                  */ 03092000
*   RETURN;                         /* RETURN TO CALLER              */ 03093000
@EL00021 DS    0H                                               02S1137 03094000
@EF00021 DS    0H                                               02S1137 03095000
@ER00021 BR    @14                                              02S1137 03096000
*   END SNAPXLT;                    /* SNAP ADDRESS CONVERTED        */ 03097000
*PRDMPXLT:                          /* PRDMP MEMORY ACCESS           */ 03098000
*   PROC OPTIONS(NOSAVE(RMEMORY));  /* RMEMORY IS OUTPUT             */ 03099000
*                                                               02S1139 03100000
PRDMPXLT STM   @14,@08,@SA00022                                 02S1139 03101000
         STM   @10,@12,@SA00022+44                              02S1139 03102000
*   /*****************************************************************/ 03103000
*   /*                                                               */ 03104000
*   /* THIS ROUTINE INTERFACES WITH THE PRDMP MEMORY ACCESS ROUTINE  */ 03105000
*   /* TO CONVERT INPUT ADDRESSES (IN 'RMEMORY') TO CURRENT PRDMP    */ 03106000
*   /* BUFFER ADDRESS. IF THE TRANSLATION FAILS (NON-ZERO RETURN CODE*/ 03107000
*   /* FROM MEMORY ACCESS) SIMULATE AN ESTAE RETRY BY LOADING        */ 03108000
*   /* 'APESREGS' AND GOING TO THE REGISTER-15 ADDRESS. IF RECURSION */ 03109000
*   /* WOULD OCCUR, ABEND. ON NORMAL EXIT, 'RMEMORY' CONTAINS THE    */ 03110000
*   /* PRDMP BUFFER ADDRESS.                                         */ 03111000
*   /*                                                               */ 03112000
*   /*****************************************************************/ 03113000
*                                                               02S1140 03114000
*   IF(RMEMORY&CPDBUFRN)=DXLATVIR THEN/* IF SAME BUFFER AS LAST      */ 03115000
         LR    @15,RMEMORY                                      02S1140 03116000
         N     @15,@CF05081                                     02S1140 03117000
         C     @15,DXLATVIR                                     02S1140 03118000
         BNE   @RF01140                                         02S1140 03119000
*     RMEMORY=(RMEMORY&CPDBUFDF)+DXLATBUF;/* ADJUST POINTER          */ 03120000
         LR    @15,RMEMORY                                      02S1141 03121000
         N     @15,@CF05079                                     02S1141 03122000
         AL    @15,DXLATBUF                                     02S1141 03123000
         LR    RMEMORY,@15                                      02S1141 03124000
*   ELSE                            /* OTHERWISE CALL PRDMP          */ 03125000
*     DO;                           /* MEMORY ACCESS ROUTINE.        */ 03126000
         B     @RC01140                                         02S1142 03127000
@RF01140 DS    0H                                               02S1143 03128000
*       RFY                                                     02S1143 03129000
*        (R0P,                                                  02S1143 03130000
*         R1P) RSTD;                /* MEMORY ACCESS INPUT           */ 03131000
*       R0P=RMEMORY&CPDBUFRN;       /* R0P = ADDRESS TO TRANSLATE    */ 03132000
         LR    R0P,RMEMORY                                      02S1144 03133000
         N     R0P,@CF05081                                     02S1144 03134000
*       DXLATVIR=R0P;               /* SAVE FOR NEXT SUBROUTINE CALL */ 03135000
         ST    R0P,DXLATVIR                                     02S1145 03136000
*       R1P=ADDR(ABDPL);            /* PRDMP PARM LIST               */ 03137000
         L     R1P,DABDPLPT                                     02S1146 03138000
*       CALL R1P->ADPLMEMA->XLTERTN;/* CALL MEMORY ACCESS            */ 03139000
         L     @15,ADPLMEMA(,R1P)                               02S1147 03140000
         BALR  @14,@15                                          02S1147 03141000
*       RFY                                                     02S1148 03142000
*        (R15F) RSTD;               /* RETURN CODE                   */ 03143000
*       RFY                                                     02S1149 03144000
*         R1P UNRSTD;               /* DONE WITH CALL INTERFACE      */ 03145000
*       IF R15F=0 THEN              /* IF TRANSLATION WORKED         */ 03146000
         LTR   R15F,R15F                                        02S1150 03147000
         BNZ   @RF01150                                         02S1150 03148000
*         DO;                       /* (BUFFER PRIMED), THEN         */ 03149000
*           DXLATBUF=R0P;           /* SAVE FOR NEXT CALL.           */ 03150000
         ST    R0P,DXLATBUF                                     02S1152 03151000
*           RMEMORY=(RMEMORY&CPDBUFDF)+R0P;/* BUFFER OFFSET          */ 03152000
         LR    @14,RMEMORY                                      02S1153 03153000
         N     @14,@CF05079                                     02S1153 03154000
         ALR   @14,R0P                                          02S1153 03155000
         LR    RMEMORY,@14                                      02S1153 03156000
*         END;                      /* 'REMORY SET                   */ 03157000
*       ELSE                        /* IF UNABLE TO TRANSLATE,       */ 03158000
*         DO;                       /* SIMULATE ESTAE EXIT (IGTUKA)  */ 03159000
         B     @RC01150                                         02S1155 03160000
@RF01150 DS    0H                                               02S1156 03161000
*           DREFBLK=BON;            /* SHOW ERROR DETECTED           */ 03162000
         OI    DREFBLK,B'00010000'                              02S1156 03163000
*           DXLATVIR='FFFFFFFF'X;   /* FORCE NEXT CALL THRU PRDMP    */ 03164000
         MVC   DXLATVIR(4),@CF05118                             02S1157 03165000
*           IF APESFPRV=BOFF THEN   /* IF THIS IS NOT RECURSION,     */ 03166000
         TM    APESFPRV,B'01000000'                             02S1158 03167000
         BNZ   @RF01158                                         02S1158 03168000
*             DO;                   /* THEN GO TO RETRY RTN.         */ 03169000
*               APESFPRV=BON;       /* SET RECURSION FLAG            */ 03170000
         OI    APESFPRV,B'01000000'                             02S1160 03171000
*               LM(R0F,R15F,APESREGS);/* RELOAD SAVED REGS           */ 03172000
         L     @14,APESREGP                                     02S1161 03173000
         LM    R0F,R15F,APESREGS(@14)                           02S1161 03174000
*               GEN(BR    R15P)                                 02S1162 03175000
*               NOSETS REFS(R15P) NOSEQFLOW EXIT;/* GOTO RETRY       */ 03176000
         BR    R15P                                                     03177000
*             END;                  /* NON-RECURSION PROCESSED       */ 03178000
*           ELSE                    /* RECURSION PROCESSING --       */ 03179000
*             DO;                   /* FORCE ABEND TO LOG ERROR &       03180000
*                                      PASS CONTROL TO THE (E)STAE OR   03181000
*                                      FRR EXIT ROUTINE.             */ 03182000
@RF01158 DS    0H                                               02S1165 03183000
*               DO;                 /* ABEND(DABNDCDE)               */ 03184000
*                 RESPECIFY                                     02S1166 03185000
*                  (GPR01F) RESTRICTED;                         02S1166 03186000
*                 GPR01F=DABNDCDE;  /* COMP CODE IN REG 1            */ 03187000
         L     GPR01F,DABNDCDE                                  02S1167 03188000
*                 SVC(13);          /* ISSUE ABEND SVC               */ 03189000
         SVC   13                                               02S1168 03190000
*                 RESPECIFY                                     02S1169 03191000
*                  (GPR01F) UNRESTRICTED;                       02S1169 03192000
*               END;                /* ABEND(DABNDCDE) FORCE ABEND   */ 03193000
*               GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);    02S1171 03194000
*                                                  /* NO CODE           03195000
*             END;                  /* ABEND ISSUED                  */ 03196000
*         END;                      /* ERROR PROCESSED               */ 03197000
*       RFY                                                     02S1174 03198000
*        (R0P,                                                  02S1174 03199000
*         R15F) UNRSTD;             /* DONE WITH MEMORY REGS.        */ 03200000
@RC01150 DS    0H                                               02S1175 03201000
*     END;                          /* RMEMORY -> DATA               */ 03202000
*   RETURN;                         /* RETURN TO CALLER              */ 03203000
@EL00022 DS    0H                                               02S1176 03204000
@EF00022 DS    0H                                               02S1176 03205000
@ER00022 LM    @14,@08,@SA00022                                 02S1176 03206000
         LM    @10,@12,@SA00022+44                              02S1176 03207000
         BR    @14                                              02S1176 03208000
*   END PRDMPXLT;                   /* PRDMP MEMORY ACCESS COMPLETE  */ 03209000
         B     @EL00022                                         02S1177 03210000
         EJECT                                                          03211000
*                                   /* IGTXEPLG                      */ 03212000
*                                                               01S1178 03213000
*   /*****************************************************************/ 03214000
*   /*                                                               */ 03215000
*   /* SYSTEM DECLARES                                               */ 03216000
*   /*                                                               */ 03217000
*   /*****************************************************************/ 03218000
*                                                               01S1178 03219000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 03220000
*     GPR01F FIXED(31) REG(1),                                  01S1178 03221000
*     GPR01P PTR(31) REG(1);                                    01S1178 03222000
*   DECLARE                         /* COMMON VARIABLES              */ 03223000
*     I256C CHAR(256) BASED,                                    01S1179 03224000
*     I031F FIXED(31) BASED,                                    01S1179 03225000
*     I031P PTR(31) BASED,                                      01S1179 03226000
*     I015F FIXED(15) BASED,                                    01S1179 03227000
*     I015P PTR(15) BASED,                                      01S1179 03228000
*     I008P PTR(8) BASED,                                       01S1179 03229000
*     I001C CHAR(1) BASED;                                      01S1179 03230000
*                                                               01S1180 03231000
*   /*****************************************************************/ 03232000
*   /*                                                               */ 03233000
*   /* MODULE PATCH AREA                                             */ 03234000
*   /*                                                               */ 03235000
*   /*****************************************************************/ 03236000
*                                                               01S1180 03237000
*   DCL                                                         01S1180 03238000
*     APPATCH CHAR(*) INT STATIC LOCAL GEND;                    01S1180 03239000
*   GENERATE DATA DEFS(APPATCH);                                01S1181 03240000
*                                                               01S1182 03241000
*   /*****************************************************************/ 03242000
*   /*                                                               */ 03243000
*   /* GENERAL REGISTER DECLARES                                     */ 03244000
*   /*                                                               */ 03245000
*   /*****************************************************************/ 03246000
*                                                               01S1182 03247000
*   DCL                                                         01S1182 03248000
*     R0F FIXED(31) REG(0),                                     01S1182 03249000
*     R0P PTR(24) REG(0),                                       01S1182 03250000
*     R0RSTD PTR(31) REG(0),                                    01S1182 03251000
*     R1F FIXED(31) REG(1),                                     01S1182 03252000
*     R1P PTR(24) REG(1),                                       01S1182 03253000
*     R1RSTD PTR(31) REG(1),                                    01S1182 03254000
*     R2F FIXED(31) REG(2),                                     01S1182 03255000
*     R2P PTR(24) REG(2),                                       01S1182 03256000
*     R2RSTD PTR(31) REG(2),                                    01S1182 03257000
*     R3F FIXED(31) REG(3),                                     01S1182 03258000
*     R3P PTR(24) REG(3),                                       01S1182 03259000
*     R3RSTD PTR(31) REG(3),                                    01S1182 03260000
*     R4F FIXED(31) REG(4),                                     01S1182 03261000
*     R4P PTR(24) REG(4),                                       01S1182 03262000
*     R4RSTD PTR(31) REG(4),                                    01S1182 03263000
*     R5F FIXED(31) REG(5),                                     01S1182 03264000
*     R5P PTR(24) REG(5),                                       01S1182 03265000
*     R5RSTD PTR(31) REG(5),                                    01S1182 03266000
*     R6F FIXED(31) REG(6),                                     01S1182 03267000
*     R6P PTR(24) REG(6),                                       01S1182 03268000
*     R6RSTD PTR(31) REG(6),                                    01S1182 03269000
*     R7F FIXED(31) REG(7),                                     01S1182 03270000
*     R7P PTR(24) REG(7),                                       01S1182 03271000
*     R7RSTD PTR(31) REG(7),                                    01S1182 03272000
*     R8F FIXED(31) REG(8),                                     01S1182 03273000
*     R8P PTR(24) REG(8),                                       01S1182 03274000
*     R8RSTD PTR(31) REG(8),                                    01S1182 03275000
*     R9F FIXED(31) REG(9),                                     01S1182 03276000
*     R9P PTR(24) REG(9),                                       01S1182 03277000
*     R9RSTD PTR(31) REG(9),                                    01S1182 03278000
*     R10F FIXED(31) REG(10),                                   01S1182 03279000
*     R10P PTR(24) REG(10),                                     01S1182 03280000
*     R10RSTD PTR(31) REG(10),                                  01S1182 03281000
*     R11F FIXED(31) REG(11),                                   01S1182 03282000
*     R11P PTR(24) REG(11),                                     01S1182 03283000
*     R11RSTD PTR(31) REG(11),                                  01S1182 03284000
*     R12F FIXED(31) REG(12),                                   01S1182 03285000
*     R12P PTR(24) REG(12),                                     01S1182 03286000
*     R12RSTD PTR(31) REG(12),                                  01S1182 03287000
*     R13F FIXED(31) REG(13),                                   01S1182 03288000
*     R13P PTR(24) REG(13),                                     01S1182 03289000
*     R13RSTD PTR(31) REG(13),                                  01S1182 03290000
*     R14F FIXED(31) REG(14),                                   01S1182 03291000
*     R14P PTR(24) REG(14),                                     01S1182 03292000
*     R14RSTD PTR(31) REG(14),                                  01S1182 03293000
*     R15F FIXED(31) REG(15),                                   01S1182 03294000
*     R15P PTR(24) REG(15),                                     01S1182 03295000
*     R15RSTD PTR(31) REG(15),                                  01S1182 03296000
*                                                               01S1182 03297000
*     /***************************************************************/ 03298000
*     /*                                                             */ 03299000
*     /* VPSS CONTROL BLOCK POINTER DECLARES                         */ 03300000
*     /*                                                             */ 03301000
*     /***************************************************************/ 03302000
*                                                               01S1182 03303000
*     APSMPTR PTR(24) REG(01),      /* STORAGE MANAGEMENT BLOCK 01S1182 03304000
*                                      (SUGGESTED POINTER)           */ 03305000
*     APSCPTR PTR(24) REG(01),      /* STORAGE MANAGEMENT CONTROL       03306000
*                                      WORD (SUGGESTED POINTER)      */ 03307000
*     APINPTR PTR(24) REG(01),      /* USER'S INPUT PARAMETER LIST      03308000
*                                      (SUGGESTED POINTER)           */ 03309000
*/*                                                                  */ 03310000
*                                                               01S1182 03311000
*     APDATA PTR(24) REG(06),       /* ADDRESS OF DYNAMIC DATA  01S1182 03312000
*                                      WORKAREA (DATAREG)            */ 03313000
*     APCODE PTR(24) REG(07),       /* PROGRAM CODE BASE REGISTER       03314000
*                                      (CODEREG)                     */ 03315000
*     APDEBPTR PTR(24) REG(08),     /* VPSS DEB (SUGGESTED POINTER)  */ 03316000
*     APNPPTR PTR(24) REG(08),      /* SUBSYSTEM INITIALIZATION 01S1182 03317000
*                                      WORKAREA BLOCK (ONLY USED BY     03318000
*                                      SUBSYSTEM INITIALIZATION)     */ 03319000
*     APRLPTR PTR(24) REG(08),      /* VPSS ACB REQUEST LIST    01S1182 03320000
*                                      (SUGGESTED POINTER)           */ 03321000
*     APCIPTR PTR(24) REG(08),      /* 3838 CONTROL TABLE (SUGGESTED    03322000
*                                      POINTER)                      */ 03323000
*     APCWPTR PTR(24) REG(08),      /* CALL INTERFACE WORKAREA  01S1182 03324000
*                                      (SUGGESTED POINTER)           */ 03325000
*     APERPTR PTR(24) REG(08),      /* ERP/IO APPENDAGE WORKAREA        03326000
*                                      (SUGGESTED POINTER)           */ 03327000
*     APACBPTR PTR(24) REG(09),     /* VPSS ACB (SUGGESTED POINTER)  */ 03328000
*     APCQPTR PTR(24) REG(09),      /* COMMAND QUEUE ELEMENT    01S1182 03329000
*                                      (SUGGESTED POINTER)           */ 03330000
*     APOTPTR PTR(24) REG(09),      /* OPTION TABLE (SUGGESTED  01S1182 03331000
*                                      POINTER)                      */ 03332000
*     APIOPTR PTR(24) REG(10),      /* I/O ELEMENT (SUGGESTED   01S1182 03333000
*                                      POINTER)                      */ 03334000
*     APEXLPTR PTR(24) REG(10),     /* VPSS EXLST (SUGGESTED POINTER)*/ 03335000
*     APUBPTR PTR(24) REG(10),      /* UNIT BLOCK (SUGGESTED POINTER)*/ 03336000
