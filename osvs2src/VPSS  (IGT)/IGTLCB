         L     @15,ADPLCVT(,@01)                                02S0439 00796000
         ST    @15,DBLKADDR                                     02S0439 00797000
*         RMEMORY=ADDR(ADPLCVT->CVTASVT);/* VECTOR TABLE POINTER     */ 00798000
         LA    RMEMORY,CVTASVT(,@15)                            02S0440 00799000
*         CALL XLTE;                /* FIND ITS ADDRESS              */ 00800000
         L     @15,DXLATOR                                      02S0441 00801000
