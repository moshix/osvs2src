         LA    @14,MBLKID-8(@15)                                01S0649 01415000
         MVC   DLINE5A(8),0(@14)                                01S0649 01416000
*         RFY                                                   01S0650 01417000
*          (R0P,                                                01S0650 01418000
*           R1P) RSTD;              /* ADDRESS CONVERTER REGISTERS   */ 01419000
*         R0P=DBLKADDR;             /* ADDRESS OF BAD CONTROL BLOCK  */ 01420000
         L     R0P,DBLKADDR                                     01S0651 01421000
*         R1P=ADDR(DLINE5B);        /* PLACE PRINTABLE ADDRESS HERE  */ 01422000
         LA    R1P,DLINE5B                                      01S0652 01423000
*         CALL CNVTADR;             /* CONVERT TO PRINTABLE HEX      */ 01424000
         BAL   @14,CNVTADR                                      01S0653 01425000
*         RFY                                                   01S0654 01426000
*          (R0P,                                                01S0654 01427000
*           R1P) UNRSTD;            /* FINISHED WITH REGISTERS       */ 01428000
*       END;                        /* ERROR MESSAGE BUILD           */ 01429000
*     CALL PRINT;                   /* PRINT IT                      */ 01430000
@RC00644 BAL   @14,PRINT                                        01S0656 01431000
*     CALL SKIP(4);                 /* LEAVE MORE SPACE              */ 01432000
         LA    @01,@AL00657                                     01S0657 01433000
         BAL   @14,SKIP                                         01S0657 01434000
*   END;                            /* ERROR RECORDED                */ 01435000
         EJECT                                                          01436000
*DONEDUMP:                          /* USER/MONITOR FORMATTING DONE  */ 01437000
*                                                               01S0659 01438000
*   /*****************************************************************/ 01439000
*   /*                                                               */ 01440000
*   /* DUMP COMPLETE                                                 */ 01441000
