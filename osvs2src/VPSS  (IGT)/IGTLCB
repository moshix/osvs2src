         L     @14,APESREGP                                     01S0634 01368000
         ST    @15,APESRTRY(,@14)                               01S0634 01369000
*   DO;                             /* ERROR RECOVERY COMPLETE       */ 01370000
*     APESFPRV=BOFF;                /* RESET RECURSION FLAG.         */ 01371000
         NI    APESFPRV,B'10111111'                             01S0636 01372000
*     APESCODE=CERRN99;             /* RESET ERROR CODE              */ 01373000
         MVI   APESCODE,X'01'                                   01S0637 01374000
*     APESFREC=BON;                 /* RESET DUMP/LOG FLAGS          */ 01375000
