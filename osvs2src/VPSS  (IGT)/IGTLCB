*       R11RSTD,                    /* 12-13 APCT/APSV PTRS          */ 01484000
*       R14RSTD,                                                01S0660 01485000
*       R15RSTD) RSTD;              /* KEEP PL/S FROM CARRYING DATA  */ 01486000
*     IF DPRDMP=BOFF THEN           /* IF RUNNING UNDER SNAP         */ 01487000
         TM    DPRDMP,B'10000000'                               01S0661 01488000
         BNZ   @RF00661                                         01S0661 01489000
*       GEN(MODESET EXTKEY=SCHED)                               01S0662 01490000
*       NOREFS NOSETS;              /* SWITCH TO STANDARD KEY (1)    */ 01491000
         MODESET EXTKEY=SCHED                                           01492000
*     RFY                                                       01S0663 01493000
*      (R0RSTD,                                                 01S0663 01494000
*       R1RSTD,                                                 01S0663 01495000
*       R2RSTD,                                                 01S0663 01496000
*       R3RSTD,                                                 01S0663 01497000
*       R4RSTD,                                                 01S0663 01498000
*       R5RSTD,                     /* 6-8 BASES                     */ 01499000
*       R9RSTD,                                                 01S0663 01500000
*       R10RSTD,                                                01S0663 01501000
*       R11RSTD,                    /* 12-13 APCT/APSV PTRS          */ 01502000
*       R14RSTD,                                                01S0663 01503000
*       R15RSTD) UNRSTD;            /* ALLOW PL/S TO USE REGS AGAIN  */ 01504000
@RF00661 DS    0H                                               01S0664 01505000
*   END;                            /* RETRY ENVIRONMENT SET         */ 01506000
*   DO;                             /* START OF SEGMENT              */ 01507000
*                                                               02S0665 01508000
*     /***************************************************************/ 01509000
*     /*                                                             */ 01510000
*     /* PROCESSING COMPLETE. DELETE THE ESTAE RECOVERY PROTECTION.  */ 01511000
*     /* THE REMAINING CODE (FREE THE WORKAREA AND SET THE REGISTER  */ 01512000
*     /* RETURN CODE) IS NOT CRITCAL TO SYSTEM PERFORMANCE AND IS NOT*/ 01513000
*     /* REQUIRED TO BE UNDER THE ESTAE UMBRELLA.                    */ 01514000
*     /*                                                             */ 01515000
*     /***************************************************************/ 01516000
*                                                               02S0666 01517000
*     IF DPRDMP=BOFF&               /* IF RUNNING FOR SNAP,          */ 01518000
*         APESFSTE=BON THEN         /* ESTAE ACTIVE,                 */ 01519000
         TM    DPRDMP,B'10000000'                               02S0666 01520000
         BNZ   @RF00666                                         02S0666 01521000
         TM    APESFSTE,B'00000010'                             02S0666 01522000
         BNO   @RF00666                                         02S0666 01523000
*       DO;                         /* THEN CANCEL ESTAE.            */ 01524000
*         APESFSTE=BOFF;            /* SHOW ESTAE NOT ACTIVE         */ 01525000
         NI    APESFSTE,B'11111101'                             02S0668 01526000
*         GEN REFS(FLCCVT,PSA,CVTSV60,CVTMAP,APSVPTR);          02S0669 01527000
*                                                    /*                 01528000
         IGTYSTAE 0,                  CANCEL THE ESTAE ENVIRONMENT     *01529000
               BRANCH=YES,            BRANCH ENTRY USED                *01530000
               SVEAREA=(APSVPTR),     USE REGISTER 13 SAVEAREA         *01531000
               KEY=1                  CURRENTLY IN KEY-1 -- RESET       01532000
*       END;                        /* ESTAE CANCELLED               */ 01533000
*     APESFSTE=BOFF;                /* SHOW ESTAE NOT ACTIVE         */ 01534000
@RF00666 NI    APESFSTE,B'11111101'                             02S0671 01535000
*                                                               02S0672 01536000
*     /***************************************************************/ 01537000
*     /*                                                             */ 01538000
*     /* FREE THE SAVEAREA/WORKAREA USED FOR PROCESSING              */ 01539000
*     /*                                                             */ 01540000
*     /***************************************************************/ 01541000
*                                                               02S0672 01542000
*     RFY                                                       02S0672 01543000
*      (R0F,                                                    02S0672 01544000
*       R1P) RSTD;                  /* FREEMAIN REGISTERS            */ 01545000
*     R1P=ADDR(APSV);               /* ADDRESS OF ACQUIRED AREA      */ 01546000
         LR    R1P,APSVPTR                                      02S0673 01547000
*     APSVPTR=APSVPREV;             /* RESTORE INPUT SAVEAREA ADDRESS*/ 01548000
         L     APSVPTR,APSVPREV(,APSVPTR)                       02S0674 01549000
*     IF DPRDMP=BON THEN            /* IF RUNNING FOR PRDMP,         */ 01550000
         TM    DPRDMP,B'10000000'                               02S0675 01551000
         BNO   @RF00675                                         02S0675 01552000
*       DO;                         /* USE FREEMAIN.                 */ 01553000
*         R0F=APSVLN+@SIZDATD;      /* ACQUIRED LENGTH               */ 01554000
         LA    R0F,80                                           02S0677 01555000
         AL    R0F,@SIZDATD                                     02S0677 01556000
*         GEN(FREEMAIN R,LV=(0),A=(1));/* FREE THE STORAGE           */ 01557000
         FREEMAIN R,LV=(0),A=(1)                                        01558000
*       END;                        /* FREEMAIN ISSUED               */ 01559000
*     ELSE                          /* IF RUNNING FOR SNAP,          */ 01560000
*       DO;                         /* USE IGTYFSV.                  */ 01561000
         B     @RC00675                                         02S0680 01562000
@RF00675 DS    0H                                               02S0681 01563000
*         DO;                       /* IGTYFSV A(R1P->APSV)          */ 01564000
*           RFY                                                 02S0682 01565000
*             GPR01P RSTD;          /* REG 1 SET TO AREA ADDRESS     */ 01566000
*           GPR01P=ADDR(R1P->APSV); /* SET REG 1 TO ADDR OF AREA TO     01567000
*                                      FREE                          */ 01568000
*           CALL APCVZUAC;          /* FREE THE SAVEAREA-WORKAREA    */ 01569000
         L     @15,APCTZUAC(,APCTPTR)                           02S0684 01570000
         BALR  @14,@15                                          02S0684 01571000
*           RFY                                                 02S0685 01572000
*             GPR01P UNRSTD;        /* FINISHED WITH REG 1           */ 01573000
*         END;                      /* SAVEAREA-WORKAREA FREED FREE     01574000
*                                      THE STORAGE                   */ 01575000
*         GEN(MODESET EXTKEY=ZERO)                              02S0687 01576000
*         NOREFS NOSETS;            /* RESUME INPUT KEY              */ 01577000
         MODESET EXTKEY=ZERO                                            01578000
*       END;                        /* SNAP STORAGE FREED.           */ 01579000
*     RFY                                                       02S0689 01580000
*      (R0F,                                                    02S0689 01581000
*       R1P) UNRSTD;                /* DONE WITH FREEMAIN REGS       */ 01582000
@RC00675 DS    0H                                               02S0690 01583000
*   END;                            /* RESOURCES RELEASED            */ 01584000
*   RETURN CODE(0);                 /* RETURN TO CALLER              */ 01585000
         SLR   @15,@15                                          01S0691 01586000
         L     @14,12(,@13)                                     01S0691 01587000
         LM    @00,@12,20(@13)                                  01S0691 01588000
         BR    @14                                              01S0691 01589000
         EJECT                                                          01590000
*                                                               01S0692 01591000
*   /*****************************************************************/ 01592000
*   /*                                                               */ 01593000
*   /* INTERNAL SUBROUTINES                                          */ 01594000
*   /*                                                               */ 01595000
*   /*****************************************************************/ 01596000
*                                                               01S0692 01597000
*DMPAPCT:                           /* FORMAT THE APCT               */ 01598000
*   PROC;                           /* NO INPUT PARM LIST            */ 01599000
*                                                               02S0692 01600000
DMPAPCT  STM   @14,@12,@SA00002                                 02S0692 01601000
*   /*****************************************************************/ 01602000
*   /*                                                               */ 01603000
*   /* FORMAT THE VPSS COMMUNICATIONS VECTOR TABLE                   */ 01604000
*   /*                                                               */ 01605000
*   /*****************************************************************/ 01606000
*                                                               02S0693 01607000
*   IF DPRTAPCT=BON THEN            /* IF APCT TO BE PRINTED,        */ 01608000
         TM    DPRTAPCT,B'10000000'                             02S0693 01609000
         BNO   @RF00693                                         02S0693 01610000
*     DO;                           /* THEN DO --                    */ 01611000
*       DXBLKNM=CXAPCT;             /* BLOCK NAME                    */ 01612000
         MVI   DXBLKNM,X'01'                                    02S0695 01613000
*       DXBLKID=CXAPCT;             /* BLOCK ID BEING REFERENCED     */ 01614000
         MVI   DXBLKID,X'01'                                    02S0696 01615000
*       DBLKADDR=ADDR(APCT);        /* ADDRESS OF BLOCK              */ 01616000
         ST    APCTPTR,DBLKADDR                                 02S0697 01617000
*       RMEMORY=ADDR(APCTSIZE);     /* APCTSIZE CONTAINS APCT SIZE   */ 01618000
         LA    RMEMORY,APCTSIZE(,APCTPTR)                       02S0698 01619000
*       CALL XLTE;                  /* FIND IT                       */ 01620000
         L     @15,DXLATOR                                      02S0699 01621000
         BALR  @14,@15                                          02S0699 01622000
*       DSIZE=RMEMORY->I015F;       /* SIZE OF BLOCK TO FORMAT       */ 01623000
         LH    DSIZE,I015F(,RMEMORY)                            02S0700 01624000
*       DADDR=ADDR(APCT);           /* ADDRESS OF BLOCK TO FORMAT    */ 01625000
         LR    DADDR,APCTPTR                                    02S0701 01626000
*       CALL FORMAT;                /* FORMAT THE CONTROL BLOCK      */ 01627000
         BAL   @14,FORMAT                                       02S0702 01628000
*     END;                          /* CONTROL BLOCK FORMATTED       */ 01629000
*   RETURN;                         /* RETURN TO CALLER              */ 01630000
@EL00002 DS    0H                                               02S0704 01631000
@EF00002 DS    0H                                               02S0704 01632000
@ER00002 LM    @14,@12,@SA00002                                 02S0704 01633000
         BR    @14                                              02S0704 01634000
*   END DMPAPCT;                    /* APCT FORMATTER                */ 01635000
         B     @EL00002                                         02S0705 01636000
         EJECT                                                          01637000
*DMPAPVU:                           /* PRINT THE VPSS USER TABLE     */ 01638000
*   PROC;                           /* (APVU)                        */ 01639000
*                                                               02S0706 01640000
DMPAPVU  STM   @14,@12,@SA00003                                 02S0706 01641000
*   /*****************************************************************/ 01642000
*   /*                                                               */ 01643000
*   /* FORMAT THE VPSS USER TABLE (APVU). POINTED TO BY APCTAPVU.    */ 01644000
*   /*                                                               */ 01645000
*   /*****************************************************************/ 01646000
*                                                               02S0707 01647000
*   IF DPRTAPVU=BON THEN            /* IF APVU TO BE PRINTED,        */ 01648000
         TM    DPRTAPVU,B'01000000'                             02S0707 01649000
         BNO   @RF00707                                         02S0707 01650000
*     DO;                           /* THEN DO --                    */ 01651000
*       DXBLKNM=CXAPVU;             /* BLOCK NAME                    */ 01652000
         MVI   DXBLKNM,X'02'                                    02S0709 01653000
*       DXBLKID=CXAPCT;             /* BLOCK ID BEING REFERENCED     */ 01654000
         MVI   DXBLKID,X'01'                                    02S0710 01655000
*       DBLKADDR=ADDR(APCT);        /* ADDRESS OF BLOCK              */ 01656000
         ST    APCTPTR,DBLKADDR                                 02S0711 01657000
*       RMEMORY=ADDR(APCTAPVU);     /* APVU POINTER                  */ 01658000
         LA    RMEMORY,APCTAPVU(,APCTPTR)                       02S0712 01659000
*       CALL XLTE;                  /* FIND IT                       */ 01660000
         L     @15,DXLATOR                                      02S0713 01661000
         BALR  @14,@15                                          02S0713 01662000
*       RMEMORY=RMEMORY->I031P;     /* POINT TO APVU (IF ANY)        */ 01663000
         L     RMEMORY,I031P(,RMEMORY)                          02S0714 01664000
*       IF RMEMORY^=0 THEN          /* IF APVU EXISTS, THEN          */ 01665000
         LTR   RMEMORY,RMEMORY                                  02S0715 01666000
         BZ    @RF00715                                         02S0715 01667000
*         DO;                       /* FORMAT THE CONTROL BLOCK      */ 01668000
*           DADDR=ADDR(RMEMORY->APVU);/* ADDRESS OF APVU             */ 01669000
         LR    DADDR,RMEMORY                                    02S0717 01670000
*           DXBLKID=CXAPVU;         /* BLOCK ID BEING REFERENCED     */ 01671000
         MVI   DXBLKID,X'02'                                    02S0718 01672000
*           DBLKADDR=ADDR(RMEMORY->APVU);/* ADDRESS OF BLOCK         */ 01673000
         ST    RMEMORY,DBLKADDR                                 02S0719 01674000
*           RMEMORY=ADDR(RMEMORY->APVUSPLN);/* APVU SIZE & SUBPOOL   */ 01675000
         LA    @05,APVUSPLN(,RMEMORY)                           02S0720 01676000
         LR    RMEMORY,@05                                      02S0720 01677000
*           CALL XLTE;              /* GET IT                        */ 01678000
         L     @15,DXLATOR                                      02S0721 01679000
         BALR  @14,@15                                          02S0721 01680000
*           DSIZE=RMEMORY->I031F&'00FFFFFF'X;/* CLEAR SUBPOOL        */ 01681000
         L     DSIZE,I031F(,RMEMORY)                            02S0722 01682000
         N     DSIZE,@CF05120                                   02S0722 01683000
*           CALL FORMAT;            /* PRINT IT                      */ 01684000
         BAL   @14,FORMAT                                       02S0723 01685000
*         END;                      /* APVU PRINTED                  */ 01686000
*     END;                          /* APVU PROCESSING COMPLETE      */ 01687000
*   RETURN;                         /* RETURN TO THE CALLER          */ 01688000
@EL00003 DS    0H                                               02S0726 01689000
@EF00003 DS    0H                                               02S0726 01690000
@ER00003 LM    @14,@12,@SA00003                                 02S0726 01691000
         BR    @14                                              02S0726 01692000
*   END DMPAPVU;                    /* END OF INTERNAL SUBROUTINE    */ 01693000
         B     @EL00003                                         02S0727 01694000
         EJECT                                                          01695000
*DMPAPUB:                           /* PRINT THE VPSS UNIT BLOCK     */ 01696000
*   PROC;                           /* (APUB)                        */ 01697000
*                                                               02S0728 01698000
DMPAPUB  STM   @14,@12,@SA00004                                 02S0728 01699000
*   /*****************************************************************/ 01700000
*   /*                                                               */ 01701000
*   /* FORMAT THE APUB POINTED TO BY 'DAPUBPTR'                      */ 01702000
*   /*                                                               */ 01703000
*   /*****************************************************************/ 01704000
*                                                               02S0729 01705000
*   IF DPRTAPUB=BON THEN            /* IF APUB TO BE PRINTED,        */ 01706000
         TM    DPRTAPUB,B'00100000'                             02S0729 01707000
         BNO   @RF00729                                         02S0729 01708000
*     DO;                           /* THEN DO --                    */ 01709000
*       DXBLKNM=CXAPUB;             /* BLOCK NAME                    */ 01710000
         MVI   DXBLKNM,X'04'                                    02S0731 01711000
*       RFY                                                     02S0732 01712000
*         APUB BASED(DAPUBPTR);     /* TELL PL/S OF POINTER          */ 01713000
*       DXBLKID=CXAPUB;             /* BLOCK ID BEING REFERENCED     */ 01714000
         MVI   DXBLKID,X'04'                                    02S0733 01715000
*       DBLKADDR=ADDR(APUB);        /* ADDRESS OF BLOCK              */ 01716000
         L     @15,DAPUBPTR                                     02S0734 01717000
         ST    @15,DBLKADDR                                     02S0734 01718000
*       RMEMORY=ADDR(APUBSIZE);     /* LENGTH OF APUB                */ 01719000
         LA    RMEMORY,APUBSIZE(,@15)                           02S0735 01720000
*       CALL XLTE;                  /* FIND IT                       */ 01721000
         L     @15,DXLATOR                                      02S0736 01722000
         BALR  @14,@15                                          02S0736 01723000
*       DSIZE=RMEMORY->I015F;       /* APUB SIZE                     */ 01724000
         LH    DSIZE,I015F(,RMEMORY)                            02S0737 01725000
*       RMEMORY=ADDR(APUBUCB);      /* ADDRESS OF ASSOCIATED UCB     */ 01726000
         L     RMEMORY,DAPUBPTR                                 02S0738 01727000
         LA    RMEMORY,APUBUCB(,RMEMORY)                        02S0738 01728000
*       CALL XLTE;                  /* FIND IT                       */ 01729000
         L     @15,DXLATOR                                      02S0739 01730000
         BALR  @14,@15                                          02S0739 01731000
*       RMEMORY=RMEMORY->I031P;     /* POINT TO IT                   */ 01732000
         L     RMEMORY,I031P(,RMEMORY)                          02S0740 01733000
*       DXBLKID=CXUCB;              /* BLOCK ID BEING REFERENCED     */ 01734000
         MVI   DXBLKID,X'09'                                    02S0741 01735000
*       DBLKADDR=ADDR(RMEMORY->UCBOB);/* ADDRESS OF BLOCK            */ 01736000
         ST    RMEMORY,DBLKADDR                                 02S0742 01737000
*       DUCBPTR=ADDR(RMEMORY->UCBOB);/* SAVE FOR UCB PRINT           */ 01738000
         ST    RMEMORY,DUCBPTR                                  02S0743 01739000
*       RMEMORY=ADDR(RMEMORY->UCBNAME);/* ADDRESS OF UCB NAME        */ 01740000
         LA    @11,UCBNAME(,RMEMORY)                            02S0744 01741000
         LR    RMEMORY,@11                                      02S0744 01742000
*       CALL XLTE;                  /* FIND IT                       */ 01743000
         L     @15,DXLATOR                                      02S0745 01744000
         BALR  @14,@15                                          02S0745 01745000
*       DBLKINFO=RMEMORY->I256C(1:LENGTH(UCBNAME));/* SAVE CUU       */ 01746000
         MVI   DBLKINFO+3,C' '                                  02S0746 01747000
         MVC   DBLKINFO+4(4),DBLKINFO+3                         02S0746 01748000
         MVC   DBLKINFO(3),I256C(RMEMORY)                       02S0746 01749000
*       DINFOL2=BON;                /* 'DBLKINFO' HAS VALID DATA     */ 01750000
         OI    DINFOL2,B'00010000'                              02S0747 01751000
*       DADDR=ADDR(APUB);           /* POINT TO APUB                 */ 01752000
         L     DADDR,DAPUBPTR                                   02S0748 01753000
*       CALL FORMAT;                /* FORMAT THE CONTROL BLOCK      */ 01754000
         BAL   @14,FORMAT                                       02S0749 01755000
*       CALL DMPUCB;                /* PRINT THE UCB POINTED TO BY      01756000
*                                      'DUCBPTR'.                    */ 01757000
         BAL   @14,DMPUCB                                       02S0750 01758000
*       RFY                                                     02S0751 01759000
*         APUB;                     /* FINISHED WITH APUB            */ 01760000
*     END;                          /* APUB PRINTED IF WANTED        */ 01761000
*   RETURN;                         /* RETURN TO CALLER              */ 01762000
@EL00004 DS    0H                                               02S0753 01763000
@EF00004 DS    0H                                               02S0753 01764000
@ER00004 LM    @14,@12,@SA00004                                 02S0753 01765000
         BR    @14                                              02S0753 01766000
*   END DMPAPUB;                    /* END OF INTERNAL SUBROUTINE    */ 01767000
         B     @EL00004                                         02S0754 01768000
         EJECT                                                          01769000
*DMPUCB:                            /* PRINT THE UNIT CONTROL BLOCK  */ 01770000
*   PROC;                           /* AND EXTENSION                 */ 01771000
*                                                               02S0755 01772000
DMPUCB   STM   @14,@12,@SA00005                                 02S0755 01773000
*   /*****************************************************************/ 01774000
*   /*                                                               */ 01775000
*   /* FORMAT THE UCB POINTED TO BY 'DUCBPTR'                        */ 01776000
*   /*                                                               */ 01777000
*   /*****************************************************************/ 01778000
*                                                               02S0756 01779000
*   IF DPRTUCB=BON THEN             /* IF UCB TO BE PRINTED,         */ 01780000
         TM    DPRTUCB,B'10000000'                              02S0756 01781000
         BNO   @RF00756                                         02S0756 01782000
*     DO;                           /* THEN DO --                    */ 01783000
*       DXBLKNM=CXUCB;              /* BLOCK NAME                    */ 01784000
         MVI   DXBLKNM,X'09'                                    02S0758 01785000
*       RFY                                                     02S0759 01786000
*         UCBOB BASED(DUCBPTR);     /* TELL PL/S OF POINTER          */ 01787000
*       DADDR=ADDR(UCBOB);          /* POINT TO UCB PROPER           */ 01788000
         L     DADDR,DUCBPTR                                    02S0760 01789000
*       DSIZE=LENGTH(UCBOB)+        /* LENGTH OF CONTROL BLOCK       */ 01790000
*           8;                      /* (ALLOW FOR UCBIOSBA)          */ 01791000
         LA    DSIZE,32                                         02S0761 01792000
*       CALL FORMAT;                /* PRINT THE UCB                 */ 01793000
         BAL   @14,FORMAT                                       02S0762 01794000
*       DXBLKNM=CXUCBX;             /* BLOCK NAME                    */ 01795000
         MVI   DXBLKNM,X'14'                                    02S0763 01796000
*       DXBLKID=CXUCB;              /* BLOCK ID BEING REFERENCED     */ 01797000
         MVI   DXBLKID,X'09'                                    02S0764 01798000
*       DBLKADDR=ADDR(UCBOB);       /* ADDRESS OF BLOCK              */ 01799000
         L     @15,DUCBPTR                                      02S0765 01800000
         ST    @15,DBLKADDR                                     02S0765 01801000
*       RMEMORY=ADDR(UCBEXTPT);     /* ADDRESS OF EXTENSION          */ 01802000
         LA    RMEMORY,UCBEXTPT(,@15)                           02S0766 01803000
*       CALL XLTE;                  /* FIND IT                       */ 01804000
         L     @15,DXLATOR                                      02S0767 01805000
         BALR  @14,@15                                          02S0767 01806000
*       DADDR=EVAL(RMEMORY-         /* ADDRESS OF UCB EXTENSION      */ 01807000
*           (ADDR(UCBEXTPT)-ADDR(UCBOB)))/* CONTAINED IN             */ 01808000
*           ->UCBEXTP;              /* UCB PROPER.                   */ 01809000
         LA    @11,20                                           02S0768 01810000
         LR    @15,RMEMORY                                      02S0768 01811000
         SLR   @15,@11                                          02S0768 01812000
         L     DADDR,UCBEXTP-1(,@15)                            02S0768 01813000
         LA    DADDR,0(,DADDR)                                  02S0768 01814000
*       DSIZE=LENGTH(UCBCMEXT);     /* SIZE OF EXTENSION             */ 01815000
         LR    DSIZE,@11                                        02S0769 01816000
*       CALL FORMAT;                /* FORMAT THE CONTROL BLOCK      */ 01817000
         BAL   @14,FORMAT                                       02S0770 01818000
*       RFY                                                     02S0771 01819000
*         UCBOB;                    /* FINISHED WITH UCB             */ 01820000
*     END;                          /* UCB PRINTED IF WANTED         */ 01821000
*   RETURN;                         /* RETURN TO CALLER              */ 01822000
@EL00005 DS    0H                                               02S0773 01823000
@EF00005 DS    0H                                               02S0773 01824000
@ER00005 LM    @14,@12,@SA00005                                 02S0773 01825000
         BR    @14                                              02S0773 01826000
*   END DMPUCB;                     /* END OF INTERNAL SUBROUTINE    */ 01827000
         B     @EL00005                                         02S0774 01828000
         EJECT                                                          01829000
*DMPAPDS:                           /* PRINT THE VPSS DATASET ENTRY  */ 01830000
*   PROC;                           /* (APDS)                        */ 01831000
*                                                               02S0775 01832000
DMPAPDS  STM   @14,@12,@SA00006                                 02S0775 01833000
*   /*****************************************************************/ 01834000
*   /*                                                               */ 01835000
*   /* FORMAT THE APDS POINTED TO BY 'DAPDSPTR'                      */ 01836000
*   /*                                                               */ 01837000
*   /*****************************************************************/ 01838000
*                                                               02S0776 01839000
*   IF DPRTAPDS=BON THEN            /* IF APDS TO BE PRINTED,        */ 01840000
         TM    DPRTAPDS,B'00010000'                             02S0776 01841000
         BNO   @RF00776                                         02S0776 01842000
*     DO;                           /* THEN DO --                    */ 01843000
*       DXBLKNM=CXAPDS;             /* BLOCK NAME                    */ 01844000
         MVI   DXBLKNM,X'03'                                    02S0778 01845000
*       RFY                                                     02S0779 01846000
*         APDS BASED(DAPDSPTR);     /* TELL PL/S OF POINTER          */ 01847000
*       DXBLKID=CXAPDS;             /* BLOCK ID BEING REFERENCED     */ 01848000
