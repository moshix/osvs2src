*       DSIZE=RMEMORY->I015F;       /* APDS SIZE                     */ 01857000
         LH    DSIZE,I015F(,RMEMORY)                            02S0784 01858000
*       DADDR=ADDR(APDS);           /* POINT TO APDS                 */ 01859000
         LR    DADDR,DAPDSPTR                                   02S0785 01860000
*       CALL FORMAT;                /* FORMAT THE CONTROL BLOCK      */ 01861000
         BAL   @14,FORMAT                                       02S0786 01862000
*       RFY                                                     02S0787 01863000
*         APDS;                     /* FINISHED WITH APDS            */ 01864000
*     END;                          /* APDS PRINTED IF WANTED        */ 01865000
*   RETURN;                         /* RETURN TO CALLER              */ 01866000
@EL00006 DS    0H                                               02S0789 01867000
@EF00006 DS    0H                                               02S0789 01868000
@ER00006 LM    @14,@12,@SA00006                                 02S0789 01869000
         BR    @14                                              02S0789 01870000
*   END DMPAPDS;                    /* END OF INTERNAL SUBROUTINE    */ 01871000
         B     @EL00006                                         02S0790 01872000
         EJECT                                                          01873000
*DMPAPDX:                           /* PRINT THE VPSS DEB EXTENSION  */ 01874000
*   PROC;                           /* (APDX)                        */ 01875000
*                                                               02S0791 01876000
DMPAPDX  STM   @14,@12,@SA00007                                 02S0791 01877000
*   /*****************************************************************/ 01878000
*   /*                                                               */ 01879000
*   /* FORMAT THE APDX POINTED TO BY 'DAPDXPTR'                      */ 01880000
*   /*                                                               */ 01881000
*   /*****************************************************************/ 01882000
*                                                               02S0792 01883000
*   IF DPRTAPDX=BON THEN            /* IF APDX TO BE PRINTED,        */ 01884000
         TM    DPRTAPDX,B'00000001'                             02S0792 01885000
         BNO   @RF00792                                         02S0792 01886000
*     DO;                           /* THEN DO --                    */ 01887000
*       DXBLKNM=CXAPDX;             /* BLOCK NAME                    */ 01888000
         MVI   DXBLKNM,X'08'                                    02S0794 01889000
*       RFY                                                     02S0795 01890000
*         APDX BASED(DAPDXPTR);     /* TELL PL/S OF POINTER          */ 01891000
*       DXBLKID=CXAPDX;             /* BLOCK ID BEING REFERENCED     */ 01892000
         MVI   DXBLKID,X'08'                                    02S0796 01893000
*       DBLKADDR=ADDR(APDX);        /* ADDRESS OF BLOCK              */ 01894000
         ST    DAPDXPTR,DBLKADDR                                02S0797 01895000
*       RMEMORY=ADDR(APDXSIZE);     /* LENGTH OF APDX                */ 01896000
         LA    RMEMORY,APDXSIZE(,DAPDXPTR)                      02S0798 01897000
*       CALL XLTE;                  /* FIND IT                       */ 01898000
         L     @15,DXLATOR                                      02S0799 01899000
         BALR  @14,@15                                          02S0799 01900000
*       DSIZE=RMEMORY->I015F;       /* APDX SIZE                     */ 01901000
         LH    DSIZE,I015F(,RMEMORY)                            02S0800 01902000
*       DADDR=ADDR(APDX);           /* POINT TO APDX                 */ 01903000
         LR    DADDR,DAPDXPTR                                   02S0801 01904000
*       CALL FORMAT;                /* FORMAT THE CONTROL BLOCK      */ 01905000
         BAL   @14,FORMAT                                       02S0802 01906000
*       RFY                                                     02S0803 01907000
*         APDX;                     /* FINISHED WITH APDX            */ 01908000
*     END;                          /* APDX PRINTED IF WANTED        */ 01909000
*   RETURN;                         /* RETURN TO CALLER              */ 01910000
@EL00007 DS    0H                                               02S0805 01911000
@EF00007 DS    0H                                               02S0805 01912000
@ER00007 LM    @14,@12,@SA00007                                 02S0805 01913000
         BR    @14                                              02S0805 01914000
*   END DMPAPDX;                    /* END OF INTERNAL SUBROUTINE    */ 01915000
         B     @EL00007                                         02S0806 01916000
         EJECT                                                          01917000
*DMPAPIO:                           /* PRINT THE VPSS STARTIO BLOCK  */ 01918000
*   PROC;                           /* (APIO)                        */ 01919000
*                                                               02S0807 01920000
DMPAPIO  STM   @14,@12,@SA00008                                 02S0807 01921000
*   /*****************************************************************/ 01922000
*   /*                                                               */ 01923000
*   /* FORMAT THE APIO POINTED TO BY 'DAPIOPTR'                      */ 01924000
*   /*                                                               */ 01925000
*   /*****************************************************************/ 01926000
