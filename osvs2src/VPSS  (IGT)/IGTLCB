         BALR  @14,@15                                          02S0432 00757000
*     DPRTFLGS=RMEMORY->I031F;      /* SAVE OPTIONS                  */ 00758000
*                                                               02S0433 00759000
         L     @15,I031F(,RMEMORY)                              02S0433 00760000
         ST    @15,DPRTFLGS                                     02S0433 00761000
*     /***************************************************************/ 00762000
*     /*                                                             */ 00763000
*     /* PRINT THE APCT                                              */ 00764000
