*           END;                    /* NOW AT NEXT DSAB              */ 01105000
*       END;                        /* MATCH FOUND OR END OF CHAIN   */ 01106000
@RF00539 DS    0H                                               02S0549 01107000
@DE00521 LTR   RMEMORY,RMEMORY                                  02S0549 01108000
         BNZ   @DL00521                                         02S0549 01109000
*       DDSABCT1=DDSABCT1+1;        /* SHOW NEXT ENTRY PROCESSED     */ 01110000
         LH    @02,DDSABCT1                                     02S0550 01111000
         LA    @02,1(,@02)                                      02S0550 01112000
         STH   @02,DDSABCT1                                     02S0550 01113000
*       DDSABPTR=RDSABPTR;          /* SAVE NEW DSAB ADDRESS         */ 01114000
         ST    RDSABPTR,DDSABPTR                                02S0551 01115000
*       RFY                                                     02S0552 01116000
*         RDSABPTR UNRSTD;          /* DONE WITH SCAN REGISTER       */ 01117000
*       CALL DEQTIOT;               /* RELEASE THE DSAB CHAIN        */ 01118000
         BAL   @14,DEQTIOT                                      02S0553 01119000
         EJECT                                                          01120000
*       IF DDSABPTR^=0 THEN         /* IF DSAB ENTRY EXISTS,         */ 01121000
         L     @15,DDSABPTR                                     02S0554 01122000
         LTR   @15,@15                                          02S0554 01123000
         BZ    @RF00554                                         02S0554 01124000
*         DO;                       /* THEN PROCESS.                 */ 01125000
*                                                               02S0555 01126000
*           /*********************************************************/ 01127000
*           /*                                                       */ 01128000
*           /* ENTRY FOR THIS SUBSYSTEM FOUND. FORMAT THE ASSOCIATED */ 01129000
*           /* 3838-SUBYSTEM CONTROL BLOCKS                          */ 01130000
*           /*                                                       */ 01131000
*           /*********************************************************/ 01132000
*                                                               02S0556 01133000
*           RFY                                                 02S0556 01134000
*             DSAB BASED(DDSABPTR); /* NEW DSAB POINTER              */ 01135000
*           DXBLKID=CXDSAB;         /* BLOCK BEING REFERENCED        */ 01136000
         MVI   DXBLKID,X'11'                                    02S0557 01137000
*           DBLKADDR=ADDR(DSAB);    /* BLOCK ADDRESS                 */ 01138000
         ST    @15,DBLKADDR                                     02S0558 01139000
*           RMEMORY=ADDR(DSABSSCM); /* SUBSYSTEM DATA                */ 01140000
         LA    RMEMORY,DSABSSCM(,@15)                           02S0559 01141000
*           CALL XLTE;              /* SUBSYSTEM DATA IS ADDRESS OF  */ 01142000
         L     @15,DXLATOR                                      02S0560 01143000
         BALR  @14,@15                                          02S0560 01144000
*           DAPDSPTR=RMEMORY->I031P;/* APDS.                         */ 01145000
         L     DAPDSPTR,I031P(,RMEMORY)                         02S0561 01146000
*           IF DAPDSPTR^=0 THEN     /* IF APDS EXISTS,               */ 01147000
         LTR   DAPDSPTR,DAPDSPTR                                02S0562 01148000
         BZ    @RF00562                                         02S0562 01149000
*             DO;                   /* THEN PROCESS IT.              */ 01150000
*               RFY                                             02S0564 01151000
*                 APDS BASED(DAPDSPTR);/* TELL PL/S                  */ 01152000
*               CALL DMPAPDS;       /* FORMAT THE APDS               */ 01153000
         BAL   @14,DMPAPDS                                      02S0565 01154000
*               DXBLKID=CXAPDS;     /* BLOCK ID BEING REFERENCED     */ 01155000
         MVI   DXBLKID,X'03'                                    02S0566 01156000
*               DBLKADDR=ADDR(APDS);/* BLOCK ADDRESS                 */ 01157000
         ST    DAPDSPTR,DBLKADDR                                02S0567 01158000
*               RMEMORY=ADDR(APDSDEB);/* DEB ADDRESS                 */ 01159000
         LA    RMEMORY,APDSDEB(,DAPDSPTR)                       02S0568 01160000
*               CALL XLTE;          /* FIND ADDRESS OF DEB           */ 01161000
         L     @15,DXLATOR                                      02S0569 01162000
         BALR  @14,@15                                          02S0569 01163000
*               DAPDXPTR=RMEMORY->I031P;/* (ONLY IF OPEN)            */ 01164000
         L     DAPDXPTR,I031P(,RMEMORY)                         02S0570 01165000
*               IF DAPDXPTR^=0 THEN /* IF DEB EXISTS,                */ 01166000
         LTR   DAPDXPTR,DAPDXPTR                                02S0571 01167000
         BZ    @RF00571                                         02S0571 01168000
*                 DO;               /* THEN FIND APDX.               */ 01169000
*                   DXBLKID=CXDEB;  /* BLOCK ID BEING REFERENCED     */ 01170000
         MVI   DXBLKID,X'13'                                    02S0573 01171000
*                   DBLKADDR=ADDR(DAPDXPTR->DEB);/* BLOCK ADDRESS    */ 01172000
         ST    DAPDXPTR,DBLKADDR                                02S0574 01173000
*                   RMEMORY=ADDR(DAPDXPTR->DEBIRBAD);/* APDX ADDRESS */ 01174000
         LA    RMEMORY,DEBIRBAD(,DAPDXPTR)                      02S0575 01175000
*                   CALL XLTE;      /* FIND ADDRESS OF APDX          */ 01176000
         L     @15,DXLATOR                                      02S0576 01177000
         BALR  @14,@15                                          02S0576 01178000
*                   DAPDXPTR=EVAL(RMEMORY-/* (ONLY IF OPEN). NOTE    */ 01179000
*                       (ADDR(RMEMORY->DEBIRBAD)-/* LOW 3 BYTES      */ 01180000
*                       ADDR(RMEMORY->DEBBASIC)))->DEBIRBB;     02S0577 01181000
         LR    @02,RMEMORY                                      02S0577 01182000
         SL    @02,@CF00037                                     02S0577 01183000
