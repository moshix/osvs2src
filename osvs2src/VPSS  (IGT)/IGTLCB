TLCB     TITLE 'IGTLCB -- VPSS SNAP FORMATTER                          *00001000
                        '                                               00002000
IGTLCB   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTLCB    77.093'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  LA    @08,4095(,@07)                                   01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         USING @PSTART+4095,@08                                 01S0001 00014000
JSCBDISP EQU 188                                                        00015000
JSCBAOS1 EQU  72                                                        00016000
JSCBAOS2 EQU 192                                                        00017000
*   RSSNAME=R0F;                    /* SAVE SUBSYSTEM NAME           */ 00018000
         LR    RSSNAME,R0F                                      01S0223 00019000
*   RABDPLPT=R1P;                   /* SAVE ABDPL ADDRESS            */ 00020000
         LR    RABDPLPT,R1P                                     01S0224 00021000
*   RFY                                                         01S0225 00022000
*    (R0F,                                                      01S0225 00023000
*     R1P) UNRSTD;                  /* FINISHED WITH INPUT REGISTERS */ 00024000
*   RFY                                                         01S0226 00025000
*     ABDPL BASED(RABDPLPT);        /* TELL PL/S OF POINTER          */ 00026000
         EJECT                                                          00027000
*                                                               01S0227 00028000
*   /*****************************************************************/ 00029000
*   /*                                                               */ 00030000
*   /* INITIALIZE SAVEAREA/WORKAREA AND SAVE INPUT VALUES            */ 00031000
*   /*                                                               */ 00032000
*   /*****************************************************************/ 00033000
*                                                               01S0227 00034000
*   DO;                             /* START OF SEGMENT              */ 00035000
*                                                               02S0227 00036000
*     /***************************************************************/ 00037000
*     /*                                                             */ 00038000
*     /* BUILD A PLS SAVEAREA/WORKAREA. THEN SAVE THE INPUT          */ 00039000
*     /* PARAMETERS.                                                 */ 00040000
*     /*                                                             */ 00041000
*     /***************************************************************/ 00042000
*                                                               02S0228 00043000
*     RFY                                                       02S0228 00044000
*       R1P RSTD;                   /* GETMAIN INTERFACE             */ 00045000
*     IF ADPLSNPR=BOFF THEN         /* IF SNAP ENTRY, USE            */ 00046000
         TM    ADPLSNPR(RABDPLPT),B'10000000'                   02S0229 00047000
         BNZ   @RF00229                                         02S0229 00048000
*       DO;                         /* VPSS STORAGE MANAGEMENT.      */ 00049000
*         GEN(MODESET EXTKEY=SCHED)                             02S0231 00050000
*         NOREFS NOSETS;            /* RUN KEY-1                     */ 00051000
*                                                               02S0231 00052000
         MODESET EXTKEY=SCHED                                           00053000
*         /***********************************************************/ 00054000
*         /*                                                         */ 00055000
*         /* BUILD A STANDARD SAVEAREA/WORKAREA USING IGTUAF. INPUT  */ 00056000
*         /* TO IGTUAF IS: BYTE-0 OF R0 = SUBPOOL, BYTES 1-3 OF R0 = */ 00057000
*         /* LENGTH, R1 HAS THE DYNAMIC STORAGE ADDRESS. ON RETURN,  */ 00058000
*         /* R1 HAS THE ADDRESS OF THE APSC.                         */ 00059000
*         /*                                                         */ 00060000
*         /***********************************************************/ 00061000
*                                                               02S0232 00062000
*         RFY                                                   02S0232 00063000
*          (R0F,                                                02S0232 00064000
*           R1P) RSTD;              /* WORK REGISTERS                */ 00065000
*         R0F=CPGSIZE|(CSP*'01000000'X);/* GETMAIN REQUIRED          */ 00066000
         L     R0F,@CF05331                                     02S0233 00067000
*         GEN(GETMAIN R,LV=(0));    /* OBTAIN THE STORAGE            */ 00068000
         GETMAIN R,LV=(0)                                               00069000
*         R0F=CPGSIZE|(CSP*'01000000'X);/* INPUT TO IGTUAF.          */ 00070000
         L     R0F,@CF05331                                     02S0235 00071000
*         CALL APCVZUAF;            /* BUILD SAVEAREA/WORKAREA       */ 00072000
         L     @15,APCTZUAF(,APCTPTR)                           02S0236 00073000
         BALR  @14,@15                                          02S0236 00074000
*         RFY                                                   02S0237 00075000
*           R0F UNRSTD;             /* FINISHED                      */ 00076000
*         R1P->APSCCRSZ=(APSVLN+APSCLN+@SIZDATD+7)/* REQUIRED        */ 00077000
*             &'0000FFF8'X;         /* SIZE ROUNDED TO DOUBLEWORD    */ 00078000
         LA    @06,95                                           02S0238 00079000
         AL    @06,@SIZDATD                                     02S0238 00080000
         N     @06,@CF05102                                     02S0238 00081000
         STH   @06,APSCCRSZ(,R1P)                               02S0238 00082000
*         R1P=R1P+APSCLN;           /* SKIP THE APSC                 */ 00083000
         LA    R1P,8(,R1P)                                      02S0239 00084000
*         GEN(MODESET EXTKEY=ZERO)                              02S0240 00085000
*         NOREFS NOSETS;            /* PREVIOUS SAVEAREA KEY-0       */ 00086000
         MODESET EXTKEY=ZERO                                            00087000
*         APSVNEXT=R1P;             /* CHAIN SAVEAREA                */ 00088000
         ST    R1P,APSVNEXT(,APSVPTR)                           02S0241 00089000
*         GEN(MODESET EXTKEY=SCHED)                             02S0242 00090000
*         NOREFS NOSETS;            /* NEW KEY                       */ 00091000
         MODESET EXTKEY=SCHED                                           00092000
*         R1P->APSVPREV=APSVPTR;    /* BACK POINTER                  */ 00093000
         ST    APSVPTR,APSVPREV(,R1P)                           02S0243 00094000
*         APSVPTR=R1P;              /* POINT TO NEW SAVEAREA         */ 00095000
         LR    APSVPTR,R1P                                      02S0244 00096000
*       END;                        /* SAVEAREA BUILT                */ 00097000
*     ELSE                          /* IF PRDMP ENTRY                */ 00098000
*       DO;                         /* USE NORMAL GETMAIN.           */ 00099000
         B     @RC00229                                         02S0246 00100000
@RF00229 DS    0H                                               02S0247 00101000
*         RFY                                                   02S0247 00102000
*           R0F RSTD;               /* SET R0F = REQUIRED SIZE       */ 00103000
*         R0F=APSVLN+@SIZDATD;      /* (SAVEAREA + DYNAMIC AREA)     */ 00104000
         LA    R0F,80                                           02S0248 00105000
         AL    R0F,@SIZDATD                                     02S0248 00106000
*         GEN(GETMAIN R,LV=(0));    /* GET STORAGE. R1P->AREA.       */ 00107000
         GETMAIN R,LV=(0)                                               00108000
*         RFY                                                   02S0250 00109000
*           R0F UNRSTD;                                         02S0250 00110000
*         APSVNEXT=R1P;             /* CHAIN THE                     */ 00111000
         ST    R1P,APSVNEXT(,APSVPTR)                           02S0251 00112000
*         R1P->APSVPREV=APSVPTR;    /* SAVEAREAS TOGETHER            */ 00113000
         ST    APSVPTR,APSVPREV(,R1P)                           02S0252 00114000
*         APSVPTR=R1P;              /* POINT TO NEW SAVEAREA         */ 00115000
         LR    APSVPTR,R1P                                      02S0253 00116000
*       END;                                                    02S0254 00117000
*     RFY                                                       02S0255 00118000
*       R1P UNRSTD;                 /* FINISHED WITH POINTER         */ 00119000
*                                                               02S0255 00120000
@RC00229 DS    0H                                               02S0256 00121000
*     /***************************************************************/ 00122000
*     /*                                                             */ 00123000
*     /* BUILD DYNAMIC AREA                                          */ 00124000
*     /*                                                             */ 00125000
*     /***************************************************************/ 00126000
*                                                               02S0256 00127000
*     RFY                                                       02S0256 00128000
*       APDATA RSTD;                /* PL/S DYNAMIC AREA POINTER     */ 00129000
*     APDATA=ADDR(APSV)+APSVLN;     /* AREA AFTER EXTENDED SAVEAREA  */ 00130000
         LA    APDATA,80(,APSVPTR)                              02S0257 00131000
*     GEN(USING @DATD,APDATA)                                   02S0258 00132000
*     REFS(APDATA) NOSETS;          /* TELL THE ASSEMBLER            */ 00133000
*                                                               02S0258 00134000
         USING @DATD,APDATA                                             00135000
*     /***************************************************************/ 00136000
*     /*                                                             */ 00137000
*     /* SAVE INPUT VALUES                                           */ 00138000
*     /*                                                             */ 00139000
*     /***************************************************************/ 00140000
*                                                               02S0259 00141000
*     DSSNAME=RSSNAME;              /* SUBSYSTEM NAME                */ 00142000
         STCM  RSSNAME,15,DSSNAME                               02S0259 00143000
*     DABDPLPT=ADDR(ABDPL);         /* SNAP/PRDMP COMPONENT PARM LIST*/ 00144000
         ST    RABDPLPT,DABDPLPT                                02S0260 00145000
*     RFY                                                       02S0261 00146000
*       ABDPL BASED(DABDPLPT);      /* TELL PLS                      */ 00147000
*     RFY                                                       02S0262 00148000
*      (RSSNAME,                                                02S0262 00149000
*       RABDPLPT) UNRSTD;           /* DONE WITH REGISTER POINTERS   */ 00150000
*                                                               02S0262 00151000
*     /***************************************************************/ 00152000
*     /*                                                             */ 00153000
*     /* INITIALIZE DYNAMIC AREA VARIABLES                           */ 00154000
*     /*                                                             */ 00155000
*     /***************************************************************/ 00156000
*                                                               02S0263 00157000
*     DFMTSW=''B;                   /* CLEAR FORMAT SWITCHES         */ 00158000
         MVI   DFMTSW,X'00'                                     02S0263 00159000
*     DLINEX=CLINEX;                /* CLEAR PRINT LINE              */ 00160000
         MVC   DLINEX(120),CLINEX                               02S0264 00161000
*     DSW=''B;                      /* CLEAR PROCESSING SWITCHES     */ 00162000
         MVI   DSW,X'00'                                        02S0265 00163000
*     IF ADPLSNPR=BON THEN          /* IF PRDMP ENTRY, THEN          */ 00164000
         L     @15,DABDPLPT                                     02S0266 00165000
         TM    ADPLSNPR(@15),B'10000000'                        02S0266 00166000
         BNO   @RF00266                                         02S0266 00167000
*       DPRDMP=BON;                 /* SHOW RUNNING FOR PRDMP        */ 00168000
         OI    DPRDMP,B'10000000'                               02S0267 00169000
*     DABNDCDE='80000FFF'X;         /* DEFAULT ABEND CODE            */ 00170000
@RF00266 MVC   DABNDCDE(4),@CF05106                             02S0268 00171000
*   END;                            /* SAVEAREA BUILT                */ 00172000
         EJECT                                                          00173000
*                                                               01S0270 00174000
*   /*****************************************************************/ 00175000
*   /*                                                               */ 00176000
*   /* PROVIDE ESTAE PROTECTION                                      */ 00177000
*   /*                                                               */ 00178000
*   /*****************************************************************/ 00179000
*                                                               01S0270 00180000
*   DO;                             /* START OF CODE SEGMENT         */ 00181000
*                                                               02S0270 00182000
*     /***************************************************************/ 00183000
*     /*                                                             */ 00184000
*     /* ESTABLISH RECOVERY ENVIRONMENT. THIS EXIT WILL GAIN CONTROL */ 00185000
*     /* IF AN ABEND OCCURS. A DUMP IS TAKEN, THE APPROPRIATE ERROR  */ 00186000
*     /* FLAGS SET, AND CONTROL PASSED TO THE CLEANUP ROUTINE TO     */ 00187000
*     /* RELEASE RESOURCES BEFORE RETURNING TO THE CALLER.           */ 00188000
*     /*                                                             */ 00189000
*     /***************************************************************/ 00190000
*                                                               02S0271 00191000
*                                                               02S0271 00192000
*     /***************************************************************/ 00193000
*     /*                                                             */ 00194000
*     /* BUILD ESTAE/FRR EXIT PARAMETER LIST                         */ 00195000
*     /*                                                             */ 00196000
*     /***************************************************************/ 00197000
*                                                               02S0271 00198000
*     GEN(MVC   DXSTAE,CXSTAE)                                  02S0271 00199000
*     REFS(CXSTAE) SETS(DXSTAE);    /* INITIALIZE ESTAE PARM LIST    */ 00200000
         MVC   DXSTAE,CXSTAE                                            00201000
*     RFY                                                       02S0272 00202000
*      (R0P,                                                    02S0272 00203000
*       R1F,                                                    02S0272 00204000
*       R14P,                                                   02S0272 00205000
*       R15F) RSTD;                 /* INITIALIZE APES BLOCK         */ 00206000
*     R0P=ADDR(APES);               /* TO ZEROS                      */ 00207000
         LA    R0P,APES                                         02S0273 00208000
*     R1F=LENGTH(APES);             /* USE MVCL TO                   */ 00209000
         LA    R1F,648                                          02S0274 00210000
*     R15F=0;                       /* CLEAR, SINCE FIELD            */ 00211000
         SLR   R15F,R15F                                        02S0275 00212000
*     MVCL(R0P,R14P);               /* MORE THAN 256 BYTES LONG      */ 00213000
         MVCL  R0P,R14P                                         02S0276 00214000
*     RFY                                                       02S0277 00215000
*      (R0P,                                                    02S0277 00216000
*       R1F,                                                    02S0277 00217000
*       R14P,                                                   02S0277 00218000
*       R15F) UNRSTD;               /* FINISHED WITH MVCL REGS       */ 00219000
*     APESID=APESIDC;               /* IN-CORE ID                    */ 00220000
         MVC   APESID(4),@CC04824                               02S0278 00221000
*     APESADDR=ADDR(APES);          /* ESTAE PARM VALUE              */ 00222000
         LA    @15,APES                                         02S0279 00223000
         ST    @15,APESADDR                                     02S0279 00224000
*     APESAPMV=ADDR(CMBRXZZZ);      /* MESSAGE INDEX TABLE           */ 00225000
         LA    @15,CMBRXZZZ                                     02S0280 00226000
         ST    @15,APESAPMV                                     02S0280 00227000
*     APESMXCD=CERRN99;             /* MAXIMUM APESCODE VALUE        */ 00228000
         MVI   APESMXCD,X'01'                                   02S0281 00229000
*     APESFREC=BON;                 /* RECORD ON LOGREC              */ 00230000
*     APESFSDP=BON;                 /* SDUMP ON ERROR                */ 00231000
         OI    APESFREC,B'01100000'                             02S0283 00232000
*     APESMOD='IGTLCB  ';           /* MODULE IN CONTROL             */ 00233000
         MVC   APESMOD(8),@CC05112                              02S0284 00234000
*     APESPROC='IGTLCB  ';          /* CSECT IN CONTROL              */ 00235000
         MVC   APESPROC(8),@CC05112                             02S0285 00236000
*     APESFRR='        ';           /* NO FRR ID                     */ 00237000
         MVC   APESFRR(8),@CC04846                              02S0286 00238000
*     APESREGP=ADDR(APESREGZ);      /* DEFAULT REGISTER SAVEAREA     */ 00239000
         LA    @15,APESREGZ                                     02S0287 00240000
         ST    @15,APESREGP                                     02S0287 00241000
*     RFY                                                       02S0288 00242000
*      (R15P) RSTD;                 /* SET REGISTER 15 TO            */ 00243000
*     R15P=ADDR(ERRDUMP);           /* BACKOUT ROUTINE               */ 00244000
         LA    R15P,ERRDUMP                                     02S0289 00245000
*     STM(R0F,R15F,APESREGZ);       /* SAVE REGISTERS FOR RETRY      */ 00246000
         STM   R0F,R15F,APESREGZ                                02S0290 00247000
*     RFY                                                       02S0291 00248000
*       R15P UNRSTD;                /* FINISHED WITH 15              */ 00249000
*     APESCODE=CERRN99;             /* 'UNKNOWN' ERROR               */ 00250000
         MVI   APESCODE,X'01'                                   02S0292 00251000
*     IF DPRDMP=BOFF THEN           /* IF RUNNING FOR SNAP,          */ 00252000
         TM    DPRDMP,B'10000000'                               02S0293 00253000
         BNZ   @RF00293                                         02S0293 00254000
*       DO;                         /* ISSUE ESTAE.                  */ 00255000
*         RFY                                                   02S0295 00256000
*           RIGTUKA RSTD;           /* POINTS TO EXIT ROUTINE        */ 00257000
*         RIGTUKA=ADDR(APCVZUKA);   /* (IGTUKA).                     */ 00258000
         L     RIGTUKA,APCTZUKA(,APCTPTR)                       02S0296 00259000
*         GEN REFS(APESADDR,APSVPTR,DXSTAE,RIGTUKA,FLCCVT,PSA,CVTSV60,  00260000
*             CVTMAP) SETS(DXSTAE);                             02S0297 00261000
*                                   /*                                  00262000
         IGTYSTAE (RIGTUKA),       IGTUKA IS EXIT ROUTINE              *00263000
               PARAM=APESADDR,     ADDR OF APES IS PARAMETER LIST      *00264000
               BRANCH=YES,         BRANCH ENTRY                        *00265000
               SVEAREA=(APSVPTR),  USE REGISTER 13 SAVEAREA            *00266000
               KEY=1,              CURRENTLY IN KEY-1 - RESET          *00267000
               MF=(E,DXSTAE)       EXECUTE FORM OF THE MACRO            00268000
*         RFY                                                   02S0298 00269000
*          (R15F) RSTD;             /* ESTAE RETURN CODE             */ 00270000
*         RFY                                                   02S0299 00271000
*           RIGTUKA UNRSTD;         /* DONE WITH EXIT POINTER        */ 00272000
*         IF R15F^=0 THEN           /* IF ESTAE FAILED,              */ 00273000
         LTR   R15F,R15F                                        02S0300 00274000
         BNZ   @RT00300                                         02S0300 00275000
*           GOTO NODUMP;            /* THEN DO NOT FORMAT BLOCKS     */ 00276000
*         ELSE                      /* IF ESTAE WORKED,              */ 00277000
*           APESFSTE=BON;           /* SHOW IT.                      */ 00278000
         OI    APESFSTE,B'00000010'                             02S0302 00279000
*         RFY                                                   02S0303 00280000
*           R15F UNRSTD;            /* ESTAE RETURN CODE PROCESSED   */ 00281000
*       END;                        /* SNAP'S ESTAE ISSUED.          */ 00282000
*   END;                            /* ESTAE ENVIRONMENT SET         */ 00283000
@RF00293 DS    0H                                               02S0306 00284000
         EJECT                                                          00285000
*                                                               01S0306 00286000
*   /*****************************************************************/ 00287000
*   /*                                                               */ 00288000
*   /* BUILD SNAP/PRDMP MEMORY ACCESS BLOCKS                         */ 00289000
*   /*                                                               */ 00290000
*   /*****************************************************************/ 00291000
*                                                               01S0306 00292000
*   DO;                             /* START OF SEGMENT              */ 00293000
*                                                               02S0306 00294000
*     /***************************************************************/ 00295000
*     /*                                                             */ 00296000
*     /* BUILD CONTROL BLOCKS REQUIRED TO ACCESS MEMORY FOR SNAP OR  */ 00297000
*     /* PRDMP EXECUTION. THIS ALLOWS THE MAINLINE CODE TO RUN       */ 00298000
*     /* INDEPENDANT OF THE CALLER (SNAP.PRDMP).                     */ 00299000
*     /*                                                             */ 00300000
*     /***************************************************************/ 00301000
*                                                               02S0307 00302000
*                                                               02S0307 00303000
*     /***************************************************************/ 00304000
*     /*                                                             */ 00305000
*     /* SET DXLATOR TO ADDRESS OF 'SNAPXLT' OR 'PRDMPXLT'. THESE    */ 00306000
*     /* ROUTINES TRANSLATE THE SPECIFIED 'RMEMORY' ADDRESS TO A     */ 00307000
*     /* CURRENT ADDRESS (PRDMP COMPATIBILITY).                      */ 00308000
*     /*                                                             */ 00309000
*     /***************************************************************/ 00310000
*                                                               02S0307 00311000
*     IF DPRDMP=BOFF THEN           /* IF SNAP ENTRY,                */ 00312000
         TM    DPRDMP,B'10000000'                               02S0307 00313000
         BNZ   @RF00307                                         02S0307 00314000
*       DXLATOR=ADDR(SNAPXLT);      /* USE SNAPXLT TO TRANSLATE.     */ 00315000
         LA    @15,SNAPXLT                                      02S0308 00316000
         ST    @15,DXLATOR                                      02S0308 00317000
*     ELSE                          /* IS PRDMP, USE PRDMPXLT TO     */ 00318000
*       DO;                         /* CONVERT DUMP ADDRESS TO       */ 00319000
         B     @RC00307                                         02S0309 00320000
@RF00307 DS    0H                                               02S0310 00321000
*         DXLATOR=ADDR(PRDMPXLT);   /* PRDMP BUFFER ADDRESS.         */ 00322000
         LA    @15,PRDMPXLT                                     02S0310 00323000
         ST    @15,DXLATOR                                      02S0310 00324000
*         DXLATVIR='FFFFFFFF'X;     /* FORCE CALL TO PRDMP MEMORY       00325000
*                                      ACCESS ROUTINE ON 1ST CALL.   */ 00326000
         MVC   DXLATVIR(4),@CF05118                             02S0311 00327000
*       END;                        /* TRANSLATE RTN SET             */ 00328000
*     RFY                                                       02S0313 00329000
*       RMEMORY RSTD;               /* ADDRESS TRANSLATION REGISTER  */ 00330000
*                                                               02S0313 00331000
@RC00307 DS    0H                                               02S0314 00332000
*     /***************************************************************/ 00333000
*     /*                                                             */ 00334000
*     /* SAVE THE BOUNDS OF GLOABAL STORAGE. THE FOLLOWING TWO RANGES*/ 00335000
*     /* DENOTE GLOBAL (ADDRESSABLE FROM ALL ADDRESS SPACES) AREAS:  */ 00336000
*     /* (1) 0-CVTNUCB (1ST BYTE PAST NUCLEUS) AND (2)               */ 00337000
*     /* CVTSHRVM-'00FFFFFF'X (CSA THRU END OF VIRTUAL MEMORY).      */ 00338000
*     /*                                                             */ 00339000
*     /***************************************************************/ 00340000
*                                                               02S0314 00341000
*     DASID=ADPLASID;               /* SAVE INPUT ASID               */ 00342000
         L     @15,DABDPLPT                                     02S0314 00343000
         LH    DASID,ADPLASID(,@15)                             02S0314 00344000
*     DNUCEND='00FFFFFF'X;          /* FORCE CURRENT ACCESSES TO     */ 00345000
         L     DNUCEND,@CF05120                                 02S0315 00346000
*     DCSABEG=0;                    /* ASSUME GLOBAL MEMORY          */ 00347000
         SLR   DCSABEG,DCSABEG                                  02S0316 00348000
*     DXBLKID=CXCVT;                /* FIND RANGE OF GLOBAL          */ 00349000
         MVI   DXBLKID,X'0A'                                    02S0317 00350000
*     DBLKADDR=ADDR(ADPLCVT->CVT);  /* MEMORY FROM THE CVT           */ 00351000
         L     @15,ADPLCVT(,@15)                                02S0318 00352000
         ST    @15,DBLKADDR                                     02S0318 00353000
*     RMEMORY=ADDR(ADPLCVT->CVTNUCB);/* SHOULD BE NO PROBLEM         */ 00354000
         LA    RMEMORY,CVTNUCB(,@15)                            02S0319 00355000
*     CALL XLTE;                    /* REFERENCING CVT SINCE         */ 00356000
         L     @15,DXLATOR                                      02S0320 00357000
         BALR  @14,@15                                          02S0320 00358000
*     DNUCEND=RMEMORY->I031P-1;     /* PREVIOUS PROGRAMS HAVE        */ 00359000
         L     DNUCEND,I031P(,RMEMORY)                          02S0321 00360000
         BCTR  DNUCEND,0                                        02S0321 00361000
*     RMEMORY=ADDR(ADPLCVT->CVTSHRVM);/* REFERENCED IT.              */ 00362000
         L     @01,DABDPLPT                                     02S0322 00363000
         L     RMEMORY,ADPLCVT(,@01)                            02S0322 00364000
         LA    RMEMORY,CVTSHRVM(,RMEMORY)                       02S0322 00365000
*     CALL XLTE;                    /* (FORMAT COMMAND REQUIRES      */ 00366000
         L     @15,DXLATOR                                      02S0323 00367000
         BALR  @14,@15                                          02S0323 00368000
*     DCSABEG=RMEMORY->I031P;       /* CVT TO EXIST).                */ 00369000
*                                                               02S0324 00370000
         L     DCSABEG,I031P(,RMEMORY)                          02S0324 00371000
*     /***************************************************************/ 00372000
*     /*                                                             */ 00373000
*     /* OBTAIN ABEND CODE FROM APCT. USED FOR RECURSION             */ 00374000
*     /*                                                             */ 00375000
*     /***************************************************************/ 00376000
*                                                               02S0325 00377000
*     DXBLKID=CXAPCT;               /* BLOCK BEING REFERENCED        */ 00378000
         MVI   DXBLKID,X'01'                                    02S0325 00379000
*     DBLKADDR=ADDR(APCT);          /* ADDRESS OF BLOCK              */ 00380000
         ST    APCTPTR,DBLKADDR                                 02S0326 00381000
*     RMEMORY=ADDR(APCTABND);       /* FIND ABEND CODE               */ 00382000
         LA    RMEMORY,APCTABND(,APCTPTR)                       02S0327 00383000
*     CALL XLTE;                    /* SET RMEMORY = ADDR(APCTABND)  */ 00384000
         L     @15,DXLATOR                                      02S0328 00385000
         BALR  @14,@15                                          02S0328 00386000
*     DABNDCDE=RMEMORY->I031F;      /* SAVE FOR RECURSION ERROR      */ 00387000
         L     @15,I031F(,RMEMORY)                              02S0329 00388000
         ST    @15,DABNDCDE                                     02S0329 00389000
*   END;                            /* END OF SEGMENT                */ 00390000
         EJECT                                                          00391000
*                                                               01S0331 00392000
*   /*****************************************************************/ 00393000
*   /*                                                               */ 00394000
*   /* DETERMINE IF USER OR MONITOR. IF NEITHER, EXIT.               */ 00395000
*   /*                                                               */ 00396000
*   /*****************************************************************/ 00397000
*                                                               01S0331 00398000
*   DO;                             /* START OF SEGMENT              */ 00399000
*                                                               02S0331 00400000
*     /***************************************************************/ 00401000
*     /*                                                             */ 00402000
*     /* DETERMINE IF CURRENT ADDRESS SPACE IS USER OR MONITOR. ALSO */ 00403000
*     /* SAVE INDICATOR FOR USER OR MONITOR.                         */ 00404000
*     /*                                                             */ 00405000
*     /***************************************************************/ 00406000
*                                                               02S0332 00407000
*     DXBLKID=CXAPCT;               /* BLOCK BEING REFERENCED        */ 00408000
         MVI   DXBLKID,X'01'                                    02S0332 00409000
*     DBLKADDR=ADDR(APCT);          /* ADDRESS OF BLOCK              */ 00410000
         ST    APCTPTR,DBLKADDR                                 02S0333 00411000
*     RMEMORY=ADDR(APCTAPVU);       /* APVU POINTER OFFSET           */ 00412000
         LA    RMEMORY,APCTAPVU(,APCTPTR)                       02S0334 00413000
*     CALL XLTE;                    /* TRANSLATE IT                  */ 00414000
         L     @15,DXLATOR                                      02S0335 00415000
         BALR  @14,@15                                          02S0335 00416000
*     RMEMORY=RMEMORY->I031F;       /* POINT TO APVU                 */ 00417000
         L     RMEMORY,I031F(,RMEMORY)                          02S0336 00418000
*     DXBLKID=CXAPVU;               /* BLOCK BEING REFERENCED        */ 00419000
         MVI   DXBLKID,X'02'                                    02S0337 00420000
*     DBLKADDR=RMEMORY;             /* ADDRESS OF BLOCK              */ 00421000
         ST    RMEMORY,DBLKADDR                                 02S0338 00422000
*     RMEMORY=ADDR(RMEMORY->APVUAUSE(ADPLASID));/* FIND APVU ENTRY   */ 00423000
         L     @01,DABDPLPT                                     02S0339 00424000
         LH    @15,ADPLASID(,@01)                               02S0339 00425000
         SLA   @15,2                                            02S0339 00426000
         LA    @15,APVUAUSE-4(@15,RMEMORY)                      02S0339 00427000
         LR    RMEMORY,@15                                      02S0339 00428000
*     CALL XLTE;                    /* TRANSLATE IT                  */ 00429000
         L     @15,DXLATOR                                      02S0340 00430000
         BALR  @14,@15                                          02S0340 00431000
*     DVUAUSE=(RMEMORY->I031F);     /* SAVE APVU ENTRY (1-WORD)      */ 00432000
         L     @15,I031F(,RMEMORY)                              02S0341 00433000
         ST    @15,DVUAUSE                                      02S0341 00434000
*     IF DVUSER=BOFF&               /* IF BOTH FLAGS OFF,            */ 00435000
*         DVUMON=BOFF THEN          /* JUST EXIT.                    */ 00436000
         TM    DVUSER,B'11000000'                               02S0342 00437000
         BZ    @RT00342                                         02S0342 00438000
*       GOTO NODUMP;                /* (NO FORMATTING REQUIRED)      */ 00439000
*     IF DVUMON=BON THEN            /* IF MONITOR, SET INDICATOR     */ 00440000
@RF00342 TM    DVUMON,B'01000000'                               02S0344 00441000
         BNO   @RF00344                                         02S0344 00442000
*       DMONITOR=BON;               /* ELSE IS USER.                 */ 00443000
         OI    DMONITOR,B'01000000'                             02S0345 00444000
*   END;                            /* ASCB USES 3838 SUBSYSTEM      */ 00445000
@RF00344 DS    0H                                               02S0347 00446000
         EJECT                                                          00447000
*                                                               01S0347 00448000
*   /*****************************************************************/ 00449000
*   /*                                                               */ 00450000
*   /* MEMORY EITHER USER OR MONITOR. PRINT TITLE.                   */ 00451000
*   /*                                                               */ 00452000
*   /*****************************************************************/ 00453000
*                                                               01S0347 00454000
*   DO;                             /* START OF SEGMENT              */ 00455000
*                                                               02S0347 00456000
*     /***************************************************************/ 00457000
*     /*                                                             */ 00458000
*     /* PRINT THE HEADER LINE: SSSS CONTROL BLOCKS                  */ 00459000
*     /*                                                             */ 00460000
*     /***************************************************************/ 00461000
*                                                               02S0348 00462000
*     CALL SKIP(3);                 /* SKIP 3 LINES                  */ 00463000
         LA    @01,@AL00348                                     02S0348 00464000
         BAL   @14,SKIP                                         02S0348 00465000
*     DLINE1=CLINE1;                /* SSSS CONTROL BLOCKS           */ 00466000
         MVC   DLINE1(19),CLINE1                                02S0349 00467000
*     DLINE1A=DSSNAME;              /* REPLACE SSSS WITH SUBSYS NAME */ 00468000
         MVC   DLINE1A(4),DSSNAME                               02S0350 00469000
*     CALL PRINT;                   /* PRINT TITLE                   */ 00470000
         BAL   @14,PRINT                                        02S0351 00471000
*     CALL SKIP(2);                 /* SKIP 2 MORE LINES             */ 00472000
         LA    @01,@AL00352                                     02S0352 00473000
         BAL   @14,SKIP                                         02S0352 00474000
*   END;                            /* TITLE PRINTED                 */ 00475000
*   APESRTRY=ADDR(ERRDUMP);         /* PRINT MSG IF FAILURE          */ 00476000
         LA    @15,ERRDUMP                                      01S0354 00477000
         L     @14,APESREGP                                     01S0354 00478000
         ST    @15,APESRTRY(,@14)                               01S0354 00479000
*   GEN(SPACE 1)                                                01S0355 00480000
*   NOREFS NOSETS FLOWS(ERRDUMP) SEQFLOW;/* SHOW PL/S ERROR FLOW     */ 00481000
         SPACE 1                                                        00482000
*                                                               01S0356 00483000
*   /*****************************************************************/ 00484000
*   /*                                                               */ 00485000
*   /* PROCESS EITHER MONITOR OR USER CONTROL BLOCKS                 */ 00486000
*   /*                                                               */ 00487000
*   /*****************************************************************/ 00488000
*                                                               01S0356 00489000
*   IF DMONITOR=BOFF THEN           /* IF NOT MONITOR, MUST          */ 00490000
         TM    DMONITOR,B'01000000'                             01S0356 00491000
         BZ    @RT00356                                         01S0356 00492000
*     GOTO USERDUMP;                /* BE USER SO GO PROCESS.        */ 00493000
         EJECT                                                          00494000
*                                                               01S0358 00495000
*   /*****************************************************************/ 00496000
*   /*                                                               */ 00497000
