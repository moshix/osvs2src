*                                                               02S0435 00778000
         BAL   @14,DMPAPVU                                      02S0435 00779000
*     /***************************************************************/ 00780000
*     /*                                                             */ 00781000
*     /* IF RUNNING FOR PRDMP, FIND THE USER'S JOBSTEP TCB (LOWEST   */ 00782000
*     /* JOBSTEP TCB IN THE ADDRESS SPACE). IF EXECUTING FOR SNAP,   */ 00783000
*     /* THE TCB ADDRESS IS IN THE INPUT PARAMETER LIST (ADPLTCB).   */ 00784000
*     /*                                                             */ 00785000
*     /***************************************************************/ 00786000
*                                                               02S0436 00787000
*     IF DPRDMP=BON THEN            /* IF EXECUTING FOR PRDMP,       */ 00788000
         TM    DPRDMP,B'10000000'                               02S0436 00789000
         BNO   @RF00436                                         02S0436 00790000
*       DO;                         /* THEN FIND JOBSTEP TCB.        */ 00791000
*         DXBLKID=CXCVT;            /* BLOCK ID BEING REFERENCED     */ 00792000
         MVI   DXBLKID,X'0A'                                    02S0438 00793000
*         DBLKADDR=ADDR(ADPLCVT->CVT);/* BLOCK ADDRESS               */ 00794000
