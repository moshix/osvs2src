         OI    APESFREC,B'01100000'                             01S0639 01377000
*   END;                            /* ALL INDICATORS RESET          */ 01378000
*   DO;                             /* START OF SEGMENT              */ 01379000
*                                                               01S0641 01380000
*     /***************************************************************/ 01381000
*     /*                                                             */ 01382000
*     /* ERROR TAKING FORMATTED 3838 DUMP OR SCANNING CONTROL BLOCK  */ 01383000
*     /* CHAIN. OUTPUT AN ERROR MESSAGE GIVING THE CONTROL BLOCK     */ 01384000
*     /* BEING ACCESSED AND ITS ADDRESS. IF NOT REFERENCING A CONTROL*/ 01385000
*     /* BLOCK (DREFBLK=0), GIVE AN 'INDETERMINATE ERROR' MESSAGE.   */ 01386000
*     /* THIS MESSAGE IS PLACED IN THE NORMAL SYSOUT DATASET USING   */ 01387000
*     /* THE SNAP.PRDMP WRITER.                                      */ 01388000
*     /*                                                             */ 01389000
*     /***************************************************************/ 01390000
*                                                               01S0642 01391000
*     DLINEX=CLINEX;                /* CLEAR THE PRINT LINE          */ 01392000
         MVC   DLINEX(120),CLINEX                               01S0642 01393000
*     CALL SKIP(2);                 /* SPACING FOR BETTER NOTICE     */ 01394000
         LA    @01,@AL00643                                     01S0643 01395000
         BAL   @14,SKIP                                         01S0643 01396000
*     IF DREFBLK=BOFF THEN          /* IF NOT REFERENCING CONTROL    */ 01397000
         TM    DREFBLK,B'00010000'                              01S0644 01398000
         BNZ   @RF00644                                         01S0644 01399000
*       DLINE6=CLINE6;              /* BLOCK, GIVE GENERAL ERROR MSG */ 01400000
         MVC   DLINE6(27),CLINE6                                01S0645 01401000
*     ELSE                          /* OTHERWISE BUILD MESSAGE GIVING*/ 01402000
*       DO;                         /* CONTROL BLOCK BEING REFERENCED*/ 01403000
         B     @RC00644                                         01S0646 01404000
@RF00644 DS    0H                                               01S0647 01405000
*         DREFBLK=BOFF;             /* RESET FLAG - BLOCK NOT BEING     01406000
*                                      REF                           */ 01407000
         NI    DREFBLK,B'11101111'                              01S0647 01408000
*         DLINE5=CLINE5;            /* INITIALIZE THE MESSAGE        */ 01409000
         MVC   DLINE5(44),CLINE5                                01S0648 01410000
*         DLINE5A=MBLKID(DXBLKID);  /* BLOCK BEING REFERENCED        */ 01411000
         SLR   @15,@15                                          01S0649 01412000
