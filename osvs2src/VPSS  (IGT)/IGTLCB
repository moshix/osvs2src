         TM    DPRDMP,B'10000000'                               01S0630 01347000
         BNZ   @RF00630                                         01S0630 01348000
*       GEN(MODESET EXTKEY=SCHED)                               01S0631 01349000
*       NOREFS NOSETS;              /* SWITCH TO STANDARD KEY (1)    */ 01350000
         MODESET EXTKEY=SCHED                                           01351000
*     RFY                                                       01S0632 01352000
*      (R0RSTD,                                                 01S0632 01353000
*       R1RSTD,                                                 01S0632 01354000
*       R2RSTD,                                                 01S0632 01355000
*       R3RSTD,                                                 01S0632 01356000
*       R4RSTD,                                                 01S0632 01357000
*       R5RSTD,                     /* 6-8 BASES                     */ 01358000
*       R9RSTD,                                                 01S0632 01359000
*       R10RSTD,                                                01S0632 01360000
*       R11RSTD,                    /* 12-13 APCT/APSV PTRS          */ 01361000
