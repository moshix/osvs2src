*DMPAPCI:                           /* PRINT THE 3838 CONTROL TABLE  */ 01962000
*   PROC;                           /* ASSOCIATED WITH THE APIO.     */ 01963000
*                                                               02S0823 01964000
DMPAPCI  STM   @14,@12,@SA00009                                 02S0823 01965000
*   /*****************************************************************/ 01966000
*   /*                                                               */ 01967000
*   /* FORMAT THE 3838 CONTROL TABLE (APCI) ASSOCIATED WITH THE APIO */ 01968000
*   /* POINTED TO BY 'DAPIOPTR'.                                     */ 01969000
*   /*                                                               */ 01970000
*   /*****************************************************************/ 01971000
*                                                               02S0824 01972000
*   IF DPRTAPCI=BON THEN            /* IF APCI TO BE PRINTED,        */ 01973000
         TM    DPRTAPCI,B'00000010'                             02S0824 01974000
         BNO   @RF00824                                         02S0824 01975000
*     DO;                           /* THEN DO --                    */ 01976000
*       DXBLKNM=CXAPCI;             /* BLOCK ID BEING PRINTED        */ 01977000
         MVI   DXBLKNM,X'07'                                    02S0826 01978000
*       DPGMCHK=BON;                /* PGM CHECK VALID SINCE USER    */ 01979000
         OI    DPGMCHK,B'01000000'                              02S0827 01980000
*       RFY                                                     02S0828 01981000
*         APIO BASED(DAPIOPTR);     /* TELL PL/S OF POINTER          */ 01982000
*       DXBLKID=CXAPIO;             /* BLOCK ID BEING REFERENCED     */ 01983000
         MVI   DXBLKID,X'05'                                    02S0829 01984000
*       DBLKADDR=ADDR(APIO);        /* ADDRESS OF BLOCK              */ 01985000
         ST    DAPIOPTR,DBLKADDR                                02S0830 01986000
*       RMEMORY=ADDR(IOSCKEY);      /* APCI (USER) KEY IN IOSB       */ 01987000
         LA    RMEMORY,IOSCKEY(,DAPIOPTR)                       02S0831 01988000
*       CALL XLTE;                  /* (PART OF APIO).               */ 01989000
         L     @15,DXLATOR                                      02S0832 01990000
         BALR  @14,@15                                          02S0832 01991000
*       DPROTKEY=RMEMORY->I001C;    /* SAVE PROTECT KEY              */ 01992000
         IC    @03,I001C(,RMEMORY)                              02S0833 01993000
         STC   @03,DPROTKEY                                     02S0833 01994000
*       RMEMORY=ADDR(APIOAPCI);     /* ADDRESS OF APCI               */ 01995000
         LA    RMEMORY,APIOAPCI(,DAPIOPTR)                      02S0834 01996000
*       CALL XLTE;                  /* FIND IT                       */ 01997000
         L     @15,DXLATOR                                      02S0835 01998000
         BALR  @14,@15                                          02S0835 01999000
*       DADDR=RMEMORY->I031P;       /* APCI ADDRESS                  */ 02000000
         L     DADDR,I031P(,RMEMORY)                            02S0836 02001000
*       RMEMORY=ADDR(APIOCISZ);     /* APCI SIZE IN APIO TOO         */ 02002000
         LA    RMEMORY,APIOCISZ(,DAPIOPTR)                      02S0837 02003000
*       CALL XLTE;                  /* POINT TO APCI SIZE            */ 02004000
         L     @15,DXLATOR                                      02S0838 02005000
         BALR  @14,@15                                          02S0838 02006000
*       DSIZE=RMEMORY->I015F;       /* APCI SIZE                     */ 02007000
         LH    DSIZE,I015F(,RMEMORY)                            02S0839 02008000
*       DPROTSW=BON;                /* APCI FETCH PROTECTED          */ 02009000
*       DPGMCHK=BON;                /* PGM CHECK VALID SINCE USER    */ 02010000
         OI    DPROTSW,B'11000000'                              02S0841 02011000
*       CALL FORMAT;                /* FORMAT THE CONTROL BLOCK      */ 02012000
         BAL   @14,FORMAT                                       02S0842 02013000
*       RFY                                                     02S0843 02014000
*         APIO;                     /* FINISHED WITH APIO            */ 02015000
*     END;                          /* APCI PRINTED IF WANTED        */ 02016000
*   RETURN;                         /* RETURN TO CALLER              */ 02017000
@EL00009 DS    0H                                               02S0845 02018000
@EF00009 DS    0H                                               02S0845 02019000
@ER00009 LM    @14,@12,@SA00009                                 02S0845 02020000
         BR    @14                                              02S0845 02021000
*   END DMPAPCI;                    /* END OF INTERNAL SUBROUTINE    */ 02022000
         B     @EL00009                                         02S0846 02023000
         EJECT                                                          02024000
*DMPIOFD:                           /* PRINT THE FIELDS ASSOCIATED   */ 02025000
*   PROC;                           /* WITH THE APIO (CCWS, APXL)    */ 02026000
*                                                               02S0847 02027000
DMPIOFD  STM   @14,@12,@SA00010                                 02S0847 02028000
*   /*****************************************************************/ 02029000
*   /*                                                               */ 02030000
*   /* FORMAT THE FIELDS ASSOCIATED WITH THE APIO POINTED TO BY      */ 02031000
*   /* 'DAPIOPTR'.                                                   */ 02032000
*   /*                                                               */ 02033000
*   /*****************************************************************/ 02034000
*                                                               02S0848 02035000
*   IF DPRTCCW=BON THEN             /* IF CCW, APXL, VSL TO BE       */ 02036000
         TM    DPRTCCW,B'00000100'                              02S0848 02037000
         BNO   @RF00848                                         02S0848 02038000
*     DO;                           /* PRINTED, THEN DO --           */ 02039000
*       DXBLKNM=CXCPGM;             /* BLOCK NAME                    */ 02040000
         MVI   DXBLKNM,X'06'                                    02S0850 02041000
*       RFY                                                     02S0851 02042000
*         APIO BASED(DAPIOPTR);     /* TELL PL/S OF POINTER          */ 02043000
*       DXBLKID=CXAPIO;             /* BLOCK ID BEING REFERENCED     */ 02044000
         MVI   DXBLKID,X'05'                                    02S0852 02045000
