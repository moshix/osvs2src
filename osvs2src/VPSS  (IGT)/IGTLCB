*     /***************************************************************/ 00531000
*     /*                                                             */ 00532000
*     /* PRINT THE APCT                                              */ 00533000
*     /*                                                             */ 00534000
*     /***************************************************************/ 00535000
*                                                               02S0364 00536000
*     CALL DMPAPCT;                 /* PRINT THE APCT                */ 00537000
*                                                               02S0364 00538000
         BAL   @14,DMPAPCT                                      02S0364 00539000
*     /***************************************************************/ 00540000
*     /*                                                             */ 00541000
*     /* PRINT THE APVU                                              */ 00542000
*     /*                                                             */ 00543000
*     /***************************************************************/ 00544000
*                                                               02S0365 00545000
*     CALL DMPAPVU;                 /* PRINT THE APVU                */ 00546000
*                                                               02S0365 00547000
         BAL   @14,DMPAPVU                                      02S0365 00548000
*     /***************************************************************/ 00549000
*     /*                                                             */ 00550000
