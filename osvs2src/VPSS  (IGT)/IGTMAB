*     APCQPTR PTR(24) REG(09),      /* COMMAND QUEUE ELEMENT    01S0091 00365000
*                                      (SUGGESTED POINTER)           */ 00366000
*     APOTPTR PTR(24) REG(09),      /* OPTION TABLE (SUGGESTED  01S0091 00367000
*                                      POINTER)                      */ 00368000
*     APIOPTR PTR(24) REG(10),      /* I/O ELEMENT (SUGGESTED   01S0091 00369000
