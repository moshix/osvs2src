*     APEXLPTR PTR(24) REG(10),     /* VPSS EXLST (SUGGESTED POINTER)*/ 00371000
*     APUBPTR PTR(24) REG(10),      /* UNIT BLOCK (SUGGESTED POINTER)*/ 00372000
*     APAXPTR PTR(24) REG(10),      /* VPSS ACB EXTENSION (SUGGESTED    00373000
*                                      POINTER)                      */ 00374000
*     APCPPTR PTR(24) REG(10),      /* VPSS CALL PROCESSING     01S0091 00375000
*                                      INDICATORS (SUGGESTED POINTER)*/ 00376000
*     APOCPTR PTR(24) REG(10),      /* VPSS OPEN/CLOSE SUBSYSTEM        00377000
*                                      WORKAREA (REQUIRED POINTER)   */ 00378000
*     APATPTR PTR(24) REG(11),      /* VPSS ACB TABLE (SUGGESTED        00379000
*                                      POINTER)                      */ 00380000
*     APLBPTR PTR(24) REG(11),      /* LABEL RESOLUTION TABLE   01S0091 00381000
*                                      (SUGGESTED POINTER)           */ 00382000
*     APUTPTR PTR(24) REG(11),      /* USER TABLE (SUGGESTED POINTER)*/ 00383000
*     APDSPTR PTR(24) REG(11),      /* VPSS DATASET ENTRY (SUGGESTED    00384000
*                                      POINTER)                      */ 00385000
*     APXLPTR PTR(24) REG(11),      /* TRANSLATION TABLE (SUGGESTED     00386000
