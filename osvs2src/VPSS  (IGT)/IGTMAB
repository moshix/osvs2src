TMAB     TITLE 'IGTMAB -- USER/VPSS CONNECTION                         *00001000
                        '                                               00002000
IGTMAB   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTMAB    77.093'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
         MVC   @PC00001(16),0(@01)                              01S0001 00022000
         MVC LINKMFLD,LINKMFL                                           00023000
*   R14P=CVTLINK;                   /* R14 -> DCB FOR SYS1.LINKLIB   */ 00024000
         L     @15,CVTPTR                                       01S0040 00025000
         L     R14P,CVTLINK(,@15)                               01S0040 00026000
*   R1P=ADDR(SSNAME);               /* R1 -> SUBSYSTEM NAME          */ 00027000
         L     R1P,@PC00001+4                                   01S0041 00028000
*   GEN(LINK DCB=(14),SF=(E,LINKMFLD))/* FIND THE SSCVT FOR          */ 00029000
*   REFS(R14P,R1P,SSNAME);          /* SUBSYSTEM 'SSNAME'.           */ 00030000
         LINK DCB=(14),SF=(E,LINKMFLD)                                  00031000
*   RFY                                                         01S0043 00032000
*    (R14P,                                                     01S0043 00033000
*     R1P) UNRSTD;                  /* FINISHED WITH R14,R1          */ 00034000
*   RFY                                                         01S0044 00035000
*    (SSCTPTR) RSTD;                /* R15 -> SSCVT (OR 0)           */ 00036000
*                                                               01S0044 00037000
*   /*****************************************************************/ 00038000
*   /*                                                               */ 00039000
*   /* SSCTPTR = ADDRESS OF SSCVT FOR SUBSYSTEM 'SSNAME'. IF NOT     */ 00040000
*   /* FOUND IT IS SET TO 0. USE SSCVT TO LOCATE APCT.               */ 00041000
*   /*                                                               */ 00042000
*   /*****************************************************************/ 00043000
*                                                               01S0045 00044000
*   IF SSCTPTR=0 THEN               /* IF SSCVT NOT FOUND, THEN      */ 00045000
         LTR   SSCTPTR,SSCTPTR                                  01S0045 00046000
         BNZ   @RF00045                                         01S0045 00047000
*     ERRCODE=APCCRCRT;             /* GIVE ERROR CODE               */ 00048000
         LA    ERRCODE,12                                       01S0046 00049000
*   ELSE                            /* OTHERWISE R15 -> SSCVT        */ 00050000
*     DO;                           /* THEREFORE, CONTINUE           */ 00051000
         B     @RC00045                                         01S0047 00052000
@RF00045 DS    0H                                               01S0048 00053000
*       APCTPTR=SSCTSUSE;           /* POINT TO APCT FOR THIS SYSTEM */ 00054000
         L     APCTPTR,SSCTSUSE(,SSCTPTR)                       01S0048 00055000
*       RFY                                                     01S0049 00056000
*        (SSCTPTR) UNRSTD;          /* DONE WITH REG 15              */ 00057000
*       IF APCTID^=APCTIDC THEN     /* IF NOT POINTING TO APCT,      */ 00058000
         CLC   APCTID(4,APCTPTR),@CC01609                       01S0050 00059000
         BE    @RF00050                                         01S0050 00060000
*         ERRCODE=APCCRCRT;         /* IS NOT VPSS SUBSYSTEM. ERROR. */ 00061000
         LA    ERRCODE,12                                       01S0051 00062000
*       ELSE                        /* IF VPSS SUBSYSTEM SO CONTINUE */ 00063000
*         DO;                       /* INITIALIZATION LOGIC.         */ 00064000
*                                                               01S0052 00065000
         B     @RC00050                                         01S0052 00066000
@RF00050 DS    0H                                               01S0053 00067000
*           /*********************************************************/ 00068000
*           /*                                                       */ 00069000
*           /* APCT FOUND. INITIALIZE APUT/APAT.                     */ 00070000
*           /*                                                       */ 00071000
*           /*********************************************************/ 00072000
*                                                               01S0053 00073000
*           DO;                     /* START OF CODE                 */ 00074000
*                                                               02S0053 00075000
*             /*******************************************************/ 00076000
*             /*                                                     */ 00077000
*             /* LOAD APUT (MODULE IGT1VPSS) & INITIALIZE. THIS      */ 00078000
*             /* MODULE ALSO CONTAINS THE APAT. SERIALIZE ACROSS THIS*/ 00079000
*             /* PROCESSING TO PREVENT CONCURRENTLY EXECUTING        */ 00080000
*             /* SUBTASKS FROM CREATING MULTIPLE APUT/APAT CONTROL   */ 00081000
*             /* BLOCKS.                                             */ 00082000
*             /*                                                     */ 00083000
*             /*******************************************************/ 00084000
*                                                               02S0054 00085000
*                                                               02S0054 00086000
*             /*******************************************************/ 00087000
*             /*                                                     */ 00088000
*             /* ISSUE EXCLUSIVE ENQ ON USERVPSS/APUT TO PREVENT     */ 00089000
*             /* MULTIPLE BUILDS OF THE APUT                         */ 00090000
*             /*                                                     */ 00091000
*             /*******************************************************/ 00092000
*                                                               02S0054 00093000
*             GEN(MVC ENQMFLD,ENQMFL)                           02S0054 00094000
*             REFS(ENQMFL) SETS(ENQMFLD);/* MOVE ENQ LIST TO DYNAMIC    00095000
*                                      AREA                          */ 00096000
         MVC ENQMFLD,ENQMFL                                             00097000
*             ENQMJ1=ENQ1;          /* BUILD ENQ MAJOR NAME OF       */ 00098000
         MVC   ENQMJ1(4),ENQ1                                   02S0055 00099000
*             ENQMJ2=APCTSSID;      /* 'USER' || SSNAME -- USERVPSS  */ 00100000
         MVC   ENQMJ2(4),APCTSSID(APCTPTR)                      02S0056 00101000
*             GEN(ENQ (ENQMJ),MF=(E,ENQMFLD))                   02S0057 00102000
*             REFS(ENQMJ) SETS(ENQMFLD,R0F,R1F,R14F,R15F);/*    02S0057 00103000
*                                      EXCLUSIVE ENQ                 */ 00104000
*                                                               02S0057 00105000
         ENQ (ENQMJ),MF=(E,ENQMFLD)                                     00106000
*             /*******************************************************/ 00107000
*             /*                                                     */ 00108000
*             /* DETERMINE IF APUT PLACED IN USER'S PARM LIST. IF SO,*/ 00109000
*             /* THEN SIMULTANEOUS CALL (SUBTASKING ENVIRONMENT) HAS */ 00110000
*             /* BUILD APAT/APUT. THEREFORE JUST RETURN. OTHERWISE   */ 00111000
*             /* LOAD THE APUT (MODULE IGT1VPSS) AND RETURN ITS      */ 00112000
*             /* ADDRESS TO THE CALLER.                              */ 00113000
*             /*                                                     */ 00114000
*             /*******************************************************/ 00115000
*                                                               02S0058 00116000
*             IF ADRAPUT>0 THEN     /* IF USER NOW HAS APUT ADDRESS, */ 00117000
         L     @15,@PC00001                                     02S0058 00118000
         L     @15,ADRAPUT(,@15)                                02S0058 00119000
         LTR   @15,@15                                          02S0058 00120000
         BNP   @RF00058                                         02S0058 00121000
*               ERRCODE=APCCROK;    /* RETURN SINCE CONTROL BLOCKS   */ 00122000
         SLR   ERRCODE,ERRCODE                                  02S0059 00123000
*             ELSE                  /* BUILT ALREADY. OTHERWISE      */ 00124000
*               DO;                 /* CONSTRUCT THE APUT.           */ 00125000
         B     @RC00058                                         02S0060 00126000
@RF00058 DS    0H                                               02S0061 00127000
*                 DO;               /* START OF CODE                 */ 00128000
*                                                               03S0061 00129000
*                   /*************************************************/ 00130000
*                   /*                                               */ 00131000
*                   /* LOAD & INITIALIZE THE APUT-APAT (IF NOT       */ 00132000
*                   /* ALREADY BUILT). THEN CALL AN INSTALLATION EXIT*/ 00133000
*                   /* TO PERFORM ANY REQUIRED INSTALLATION-DEPENDENT*/ 00134000
*                   /* PROCESSING.                                   */ 00135000
*                   /*                                               */ 00136000
*                   /*************************************************/ 00137000
*                                                               03S0062 00138000
*                   MODNAME1='IGT1';/* BUILD APUT/APAT MODULE NAME:  */ 00139000
         MVC   MODNAME1(4),@CC01832                             03S0062 00140000
*                   MODNAME2=APCTSSID;/* 'IGT1'||SSNAME = 'IGT1VPSS' */ 00141000
         MVC   MODNAME2(4),APCTSSID(APCTPTR)                    03S0063 00142000
*                   RFY                                         03S0064 00143000
*                    (R0P,                                      03S0064 00144000
*                     R1P) RSTD;    /* USING REG 0 & 1 FOR LOAD      */ 00145000
*                   R1P=CVTLINK;    /* R1 -> DCB FOR SYS1.LINKLIB    */ 00146000
         L     @15,CVTPTR                                       03S0065 00147000
         L     R1P,CVTLINK(,@15)                                03S0065 00148000
*                   GEN(LOAD EPLOC=MODNAME,DCB=(1))/* LOAD      03S0066 00149000
*                                      APUT-IGT1VPSS                 */ 00150000
*                   REFS(R1P,MODNAME);/* ON RETURN, R0 -> IGT1VPSS.  */ 00151000
         LOAD EPLOC=MODNAME,DCB=(1)                                     00152000
*                   RFY                                         03S0067 00153000
*                    (R1P) UNRSTD;  /* FINISHED WITH REGISTER 1      */ 00154000
*                   RFY                                         03S0068 00155000
*                    (APUTPTR) RSTD;/* USING APUTPTR THROUGHOUT      */ 00156000
*                   APUTPTR=R0P;    /* SAVE APUT ADDRESS             */ 00157000
         LR    APUTPTR,R0P                                      03S0069 00158000
*                   RFY                                         03S0070 00159000
*                    (APUT) BASED(APUTPTR);/* TELL COMPILER OF  03S0070 00160000
*                                      POINTER                       */ 00161000
*                   RFY                                         03S0071 00162000
*                    (R0P) UNRSTD;  /* FINISHED WITH REG 0           */ 00163000
*                   APUTAPCT=ADDR(APCT);/* SAVE CONTROL TABLE ADDRESS*/ 00164000
         ST    APCTPTR,APUTAPCT(,APUTPTR)                       03S0072 00165000
*                   RFY                                         03S0073 00166000
*                    (APAT) BASED(APUTAPAT);/* APUT POINTS TO APAT   */ 00167000
*                   ADRAPUT=ADDR(APUT);/* RETURN APUT ADDR TO USER   */ 00168000
*                                                               03S0074 00169000
         L     @10,@PC00001                                     03S0074 00170000
         ST    APUTPTR,ADRAPUT(,@10)                            03S0074 00171000
*                   /*************************************************/ 00172000
*                   /*                                               */ 00173000
*                   /* CALL INSTALLATION EXIT. PASS APUT & USER PARM */ 00174000
*                   /* ADDRESSES.                                    */ 00175000
*                   /*                                               */ 00176000
*                   /*************************************************/ 00177000
*                                                               03S0075 00178000
*                   CALL APCV1MAB(APUT,USER);/* CALL INSTALLATION       00179000
*                                      EXIT                          */ 00180000
         ST    APUTPTR,@AL00001                                 03S0075 00181000
         L     @10,@PC00001+12                                  03S0075 00182000
         ST    @10,@AL00001+4                                   03S0075 00183000
         MVI   @AL00001+4,X'80'                                 03S0075 00184000
         L     @15,APCT1MAB(,APCTPTR)                           03S0075 00185000
         LA    @01,@AL00001                                     03S0075 00186000
         BALR  @14,@15                                          03S0075 00187000
*                   IF R15F>0 THEN  /* IF RETURN CODE 0,             */ 00188000
         LTR   R15F,R15F                                        03S0076 00189000
         BNP   @RF00076                                         03S0076 00190000
*                     ERRCODE=APCCRCRT;/* THEN SHOW ERROR CONDITION. */ 00191000
         LA    ERRCODE,12                                       03S0077 00192000
*                   ELSE            /* OTHERWISE SHOW THAT           */ 00193000
*                     ERRCODE=APCCROK;/* INITIALIZATION OK.          */ 00194000
*                                                               03S0078 00195000
         B     @RC00076                                         03S0078 00196000
@RF00076 SLR   ERRCODE,ERRCODE                                  03S0078 00197000
*                   /*************************************************/ 00198000
*                   /*                                               */ 00199000
*                   /* SHOW APUT/APAT NOW CONSTRUCTED.               */ 00200000
*                   /*                                               */ 00201000
*                   /*************************************************/ 00202000
*                                                               03S0079 00203000
*                   APUTACT=BON;    /* SET ACTIVE SWITCH ON          */ 00204000
@RC00076 OI    APUTACT(APUTPTR),B'10000000'                     03S0079 00205000
*                 END;              /* END OF CODE                   */ 00206000
*               END;                /* INITIALIZATION COMPLETE.      */ 00207000
*                                                               02S0081 00208000
*             /*******************************************************/ 00209000
*             /*                                                     */ 00210000
*             /* INITIALIZATION COMPLETE. RETURN ADDRESS OF FAST-PATH*/ 00211000
*             /* ROUTINE (MODULE IGTMBA) TO USER. THIS MUST BE THE   */ 00212000
*             /* LAST ACTION TAKEN SINCE FUTURE CALLS TO VPSS        */ 00213000
*             /* (POSSIBLY BEFORE THIS ROUTINE TERMINATES) GO        */ 00214000
*             /* DIRECTLY TO THE FAST-PATH ROUTINE WHICH ASSUMES THAT*/ 00215000
*             /* ALL CONTROL BLOCKS HAVE BEEN BUILT.                 */ 00216000
*             /*                                                     */ 00217000
*             /*******************************************************/ 00218000
*                                                               02S0082 00219000
*             ADRRTN=APCTZMBA;      /* ADDRESS OF INTERFACE ROUTINE  */ 00220000
@RC00058 L     @10,@PC00001+8                                   02S0082 00221000
         L     @03,APCTZMBA(,APCTPTR)                           02S0082 00222000
         ST    @03,ADRRTN(,@10)                                 02S0082 00223000
*             GEN(DEQ MF=(E,ENQMFLD))/* RELEASE SERIALIZATION SINCE  */ 00224000
*             REFS(ENQMFLD);        /* APUT/APAT INITIALIZED         */ 00225000
         DEQ MF=(E,ENQMFLD)                                             00226000
*           END;                    /* APUT/APAT INITIALIZED         */ 00227000
*         END;                      /* APUT/APAT BUILT               */ 00228000
*     END;                          /* APUT/APAT BUILT               */ 00229000
*                                                               01S0086 00230000
@RC00050 DS    0H                                               01S0087 00231000
*   /*****************************************************************/ 00232000
*   /*                                                               */ 00233000
*   /* APUT/APAT BUILT. ADDRESS OF VPSS FAST-PATH PROCESSOR RETURNED */ 00234000
*   /* TO THE USER. FUTURE CALLS GO DIRECTLY TO THE FAST-PATH ROUTINE*/ 00235000
*   /* (IGTMBA). THEREFORE RETURN TO CALLER. REGISTER 15 HAS THE     */ 00236000
*   /* RETURN CODE: =0 IF NO ERROR, ^=0 IF ERROR.                    */ 00237000
*   /*                                                               */ 00238000
*   /*****************************************************************/ 00239000
*                                                               01S0087 00240000
*   RETURN CODE(ERRCODE);           /* RETURN TO CALLER (R15 SET)    */ 00241000
@RC00045 L     @13,4(,@13)                                      01S0087 00242000
         L     @00,@SIZDATD                                     01S0087 00243000
         LR    @01,@06                                          01S0087 00244000
         FREEMAIN R,LV=(0),A=(1)                                        00245000
         LR    @15,@02                                          01S0087 00246000
         L     @14,12(,@13)                                     01S0087 00247000
         LM    @00,@12,20(@13)                                  01S0087 00248000
         BR    @14                                              01S0087 00249000
         EJECT                                                          00250000
*                                   /* IGTXEPLG                      */ 00251000
*                                                               01S0088 00252000
*   /*****************************************************************/ 00253000
*   /*                                                               */ 00254000
*   /* SYSTEM DECLARES                                               */ 00255000
*   /*                                                               */ 00256000
*   /*****************************************************************/ 00257000
*                                                               01S0088 00258000
*   DECLARE                         /* COMMON VARIABLES              */ 00259000
*     I256C CHAR(256) BASED,                                    01S0088 00260000
*     I031F FIXED(31) BASED,                                    01S0088 00261000
*     I031P PTR(31) BASED,                                      01S0088 00262000
*     I015F FIXED(15) BASED,                                    01S0088 00263000
*     I015P PTR(15) BASED,                                      01S0088 00264000
*     I008P PTR(8) BASED,                                       01S0088 00265000
*     I001C CHAR(1) BASED;                                      01S0088 00266000
*                                                               01S0089 00267000
*   /*****************************************************************/ 00268000
*   /*                                                               */ 00269000
*   /* MODULE PATCH AREA                                             */ 00270000
