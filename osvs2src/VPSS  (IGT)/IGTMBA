TMBA     TITLE 'IGTMBA -- FAST PATH                                    *00001000
                        '                                               00002000
IGTMBA   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTMBA    77.092'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
*   RFUNC=APINFUNC;                 /* RFUNC CONTAINS FUNCTION NAME     00014000
*                                      APRLPTR -> APRL BEFORE USING.    00015000
*                                      LOAD ADDRESS IF ONE OF THREE  */ 00016000
         L     @09,APINAFNC(,APINPTR)                           01S0036 00017000
         L     RFUNC,APINFUNC(,@09)                             01S0036 00018000
*   RFY                                                         01S0037 00019000
*    (APRL) BASED(APRLPTR);         /* FUNCTIONS PROCESSED BY RTN.   */ 00020000
*   RFY                                                         01S0038 00021000
*    (RCODE) RSTD;                  /* SET TO RETURN CODE            */ 00022000
*   RCODE=APCCROK;                  /* SET TO NORMAL RETURN CODE (0) */ 00023000
         SLR   RCODE,RCODE                                      01S0039 00024000
*   RFY                                                         01S0040 00025000
*    (ECB) BASED(ADDR(APRLECB));    /* ECB WITHIN APRL               */ 00026000
*                                                               01S0040 00027000
*   /*****************************************************************/ 00028000
*   /*                                                               */ 00029000
*   /* CHECK FOR 'FAST PATH' REQUESTS (EXCW,EXC,WAIT) AND PROCESS.   */ 00030000
*   /* OTHERWISE GO TO MODULE IGTMBD.                                */ 00031000
*   /*                                                               */ 00032000
*   /*****************************************************************/ 00033000
*                                                               01S0041 00034000
         EJECT                                                          00035000
*   IF RFUNC=APFNEXCW THEN          /* CHECK FOR EXECUTE & WAIT      */ 00036000
         CL    RFUNC,@CF01009                                   01S0041 00037000
         BNE   @RF00041                                         01S0041 00038000
*     DO;                           /* 'EXCW' LOGIC                  */ 00039000
*                                                               02S0042 00040000
*       /*************************************************************/ 00041000
*       /*                                                           */ 00042000
*       /* 'EXCW' -- EXECUTE-AND-WAIT. THIS IS SYNCHRONOUS           */ 00043000
*       /* PROCESSING. CONTROL IS NOT RETURNED TO THE CALLER UNTIL   */ 00044000
*       /* THE I/O IS COMPLETE.                                      */ 00045000
*       /*                                                           */ 00046000
*       /*************************************************************/ 00047000
*                                                               02S0043 00048000
*       APRLPTR=APINAPRL;           /* APRLPTR -> APRL               */ 00049000
         L     @06,APINAAR(,APINPTR)                            02S0043 00050000
         L     APRLPTR,APINAPRL(,@06)                           02S0043 00051000
*       TS(APRLACTV);               /* SEE IF APRL CURRENTLY ACTIVE  */ 00052000
         TS    APRLACTV(APRLPTR)                                02S0044 00053000
*       BC(TSOFFMSK,PEXCW01);       /* IF ACTIVE, THEN               */ 00054000
         BC    8,PEXCW01                                        02S0045 00055000
*       RCODE=APCCRBSY;             /* SET ERROR CODE TO SHOW BUSY.  */ 00056000
         LA    RCODE,4                                          02S0046 00057000
*       GOTO EXIT4;                 /* AND GO TO EXIT LOGIC.         */ 00058000
         B     EXIT4                                            02S0047 00059000
*PEXCW01:                                                       02S0048 00060000
*       ;                           /* APRL MARKED ACTIVE NOW        */ 00061000
PEXCW01  DS    0H                                               02S0049 00062000
*       APRLASY=BOFF;               /* AFTER SHOWING SYNCHRONOUS,    */ 00063000
         NI    APRLASY(APRLPTR),B'11110111'                     02S0049 00064000
*       DO;                         /* IGTYIO APRL(APRL)FUNC(EXCP)   */ 00065000
*         RFY                                                   02S0051 00066000
*          (GPR00F,                                             02S0051 00067000
*           GPR01P) RSTD;           /* RESTRICT REQUIRED REGISTERS   */ 00068000
*         GPR00F=0;                 /* SET R0 = FUNCTION REQUEST ID  */ 00069000
         SLR   GPR00F,GPR00F                                    02S0052 00070000
*         GPR01P=ADDR(APRL);        /* POINT TO APRL TO PROCESS      */ 00071000
         LR    GPR01P,APRLPTR                                   02S0053 00072000
*         SVC(111);                 /* ISSUE VPSS SVC REQUEST        */ 00073000
         SVC   111                                              02S0054 00074000
*         RFY                                                   02S0055 00075000
*          (GPR00F,                                             02S0055 00076000
*           GPR01P) UNRSTD;         /* RELEASE REGISTERS             */ 00077000
*       END;                        /* IGTYIO APRL(APRL)FUNC(EXCP)      00078000
*                                      ISSUE I/O REQUEST.            */ 00079000
*       DO;                         /* IGTYWAIT ECB(APRLECB)         */ 00080000
*         RFY                                                   02S0058 00081000
*          (GPR00F,                                             02S0058 00082000
*           GPR01P) RSTD;           /* REGISTERS USED                */ 00083000
*         DCL                                                   02S0059 00084000
*           BIT0013X BIT(8) BASED(ADDR(APRLECB));/* OVERLAY BYTE-0 OF   00085000
*                                      ECB                           */ 00086000
*         IF BIT0013X(2)='0'B THEN  /* IF ECB NOT ALREADY POSTED     */ 00087000
         TM    BIT0013X+8(APRLPTR),B'01000000'                  02S0060 00088000
         BNZ   @RF00060                                         02S0060 00089000
*           DO;                     /* THEN ISSUE WAIT SVC           */ 00090000
*             GPR01P=ADDR(APRLECB); /* POINT TO ECB                  */ 00091000
         LA    GPR01P,APRLECB(,APRLPTR)                         02S0062 00092000
*             GPR00F=1;             /* WAIT COUNT                    */ 00093000
         LA    GPR00F,1                                         02S0063 00094000
*             SVC(1);               /* WAIT FOR ECB POSTING          */ 00095000
         SVC   1                                                02S0064 00096000
*           END;                    /* ECB WAIT COMPLETE             */ 00097000
*         RFY                                                   02S0066 00098000
*          (GPR00F,                                             02S0066 00099000
*           GPR01P) UNRSTD;         /* FINISHED WITH REGISTERS       */ 00100000
@RF00060 DS    0H                                               02S0067 00101000
*       END;                        /* ECB WAIT COMPLETE WAIT FOR I/O   00102000
*                                      COMPLETION                    */ 00103000
*       APRLACTV=TSOFF;             /* RESET ACTIVE INDICATOR        */ 00104000
         MVI   APRLACTV(APRLPTR),X'00'                          02S0068 00105000
*       IF APRLECBC='7F'X THEN      /* IF NORMAL COMPLETION,         */ 00106000
         CLI   APRLECBC(APRLPTR),X'7F'                          02S0069 00107000
         BE    @RT00069                                         02S0069 00108000
*         GOTO EXIT0;               /* THEN GO TO EXIT ROUTINE       */ 00109000
*       ELSE                        /* OTHERWISE --                  */ 00110000
*         GOTO EXITXCTL;            /* GO TO 'XCTL' LOGIC (ERROR)    */ 00111000
         B     EXITXCTL                                         02S0071 00112000
*     END;                          /* END OF 'EXCW' LOGIC           */ 00113000
         EJECT                                                          00114000
*   IF RFUNC=APFNEXC THEN           /* CHECK FOR EXECUTE             */ 00115000
@RF00041 CL    RFUNC,@CF01007                                   01S0073 00116000
         BNE   @RF00073                                         01S0073 00117000
*     DO;                           /* 'EXC ' LOGIC                  */ 00118000
*                                                               02S0074 00119000
*       /*************************************************************/ 00120000
*       /*                                                           */ 00121000
*       /* 'EXC ' -- EXECUTE. THIS IS ASYNCHRONOUS PROCESSING.       */ 00122000
*       /* CONTROL IS RETURNED AFTER THE I/O REQUEST IS ISSUED. THE  */ 00123000
*       /* I/O MIGHT NOT BE COMPLETED (IT MIGHT NOT EVEN BE STARTED).*/ 00124000
*       /*                                                           */ 00125000
*       /*************************************************************/ 00126000
*                                                               02S0075 00127000
*       APRLPTR=APINAPRL;           /* APRLPTR -> APRL               */ 00128000
         L     @06,APINAAR(,APINPTR)                            02S0075 00129000
         L     APRLPTR,APINAPRL(,@06)                           02S0075 00130000
*       TS(APRLACTV);               /* SEE IF APRL CURRENTLY ACTIVE  */ 00131000
         TS    APRLACTV(APRLPTR)                                02S0076 00132000
*       BC(TSOFFMSK,PEXC01);        /* IF ACTIVE, THEN               */ 00133000
         BC    8,PEXC01                                         02S0077 00134000
*       RCODE=APCCRBSY;             /* SET ERROR CODE TO SHOW BUSY.  */ 00135000
         LA    RCODE,4                                          02S0078 00136000
*       GOTO EXIT4;                 /* AND GO TO EXIT LOGIC.         */ 00137000
         B     EXIT4                                            02S0079 00138000
*PEXC01:                                                        02S0080 00139000
*       ;                           /* APRL MARKED ACTIVE NOW        */ 00140000
PEXC01   DS    0H                                               02S0081 00141000
*       APRLASY=BON;                /* AFTER SHOWING ASYNCHRONOUS,   */ 00142000
         OI    APRLASY(APRLPTR),B'00001000'                     02S0081 00143000
*       DO;                         /* IGTYIO APRL(APRL)FUNC(EXCP)   */ 00144000
*         RFY                                                   02S0083 00145000
*          (GPR00F,                                             02S0083 00146000
*           GPR01P) RSTD;           /* RESTRICT REQUIRED REGISTERS   */ 00147000
*         GPR00F=0;                 /* SET R0 = FUNCTION REQUEST ID  */ 00148000
         SLR   GPR00F,GPR00F                                    02S0084 00149000
*         GPR01P=ADDR(APRL);        /* POINT TO APRL TO PROCESS      */ 00150000
         LR    GPR01P,APRLPTR                                   02S0085 00151000
*         SVC(111);                 /* ISSUE VPSS SVC REQUEST        */ 00152000
         SVC   111                                              02S0086 00153000
*         RFY                                                   02S0087 00154000
*          (GPR00F,                                             02S0087 00155000
*           GPR01P) UNRSTD;         /* RELEASE REGISTERS             */ 00156000
*       END;                        /* IGTYIO APRL(APRL)FUNC(EXCP)      00157000
*                                      ISSUE I/O REQUEST.            */ 00158000
*       GOTO EXIT0;                 /* GOTO EXIT ROUTINE.            */ 00159000
         B     EXIT0                                            02S0089 00160000
*     END;                          /* GOTO EXIT LOGIC               */ 00161000
         EJECT                                                          00162000
*   IF RFUNC=APFNWAIT THEN          /* CHECK FOR WAIT.               */ 00163000
@RF00073 CL    RFUNC,@CF01011                                   01S0091 00164000
         BNE   @RF00091                                         01S0091 00165000
*     DO;                           /* WAIT FOR I/O COMPLETION       */ 00166000
*                                                               02S0092 00167000
*       /*************************************************************/ 00168000
*       /*                                                           */ 00169000
*       /* 'WAIT' -- WAIT. THIS IS ASYNCHRONOUS PROCESSING. CONTROL  */ 00170000
*       /* IS RETURNED TO THE USER AFTER THE I/O (ISSUED BY 'EXC '   */ 00171000
*       /* CALL) HAS COMPLETED.                                      */ 00172000
*       /*                                                           */ 00173000
*       /*************************************************************/ 00174000
*                                                               02S0093 00175000
*       APRLPTR=APINAPRL;           /* APRLPTR -> APRL               */ 00176000
         L     @06,APINAAR(,APINPTR)                            02S0093 00177000
         L     APRLPTR,APINAPRL(,@06)                           02S0093 00178000
*       IF APRLACTV=TSOFF THEN      /* IF 'EXC ' REQUEST NOT         */ 00179000
         CLI   APRLACTV(APRLPTR),X'00'                          02S0094 00180000
         BNE   @RF00094                                         02S0094 00181000
*         DO;                       /* OUTSTANDING, THEN IS ERROR    */ 00182000
*           RCODE=APCCRBSY;         /* SET ERROR CODE                */ 00183000
         LA    RCODE,4                                          02S0096 00184000
*           GOTO EXIT4;             /* AND GO TO EXIT LOGIC.         */ 00185000
         B     EXIT4                                            02S0097 00186000
*         END;                      /* NON-ACTIVE APRL PROCESSED     */ 00187000
*       DO;                         /* IGTYWAIT ECB(APRLECB)         */ 00188000
@RF00094 DS    0H                                               02S0100 00189000
*         RFY                                                   02S0100 00190000
*          (GPR00F,                                             02S0100 00191000
*           GPR01P) RSTD;           /* REGISTERS USED                */ 00192000
*         DCL                                                   02S0101 00193000
*           BIT0017X BIT(8) BASED(ADDR(APRLECB));/* OVERLAY BYTE-0 OF   00194000
*                                      ECB                           */ 00195000
*         IF BIT0017X(2)='0'B THEN  /* IF ECB NOT ALREADY POSTED     */ 00196000
         TM    BIT0017X+8(APRLPTR),B'01000000'                  02S0102 00197000
         BNZ   @RF00102                                         02S0102 00198000
*           DO;                     /* THEN ISSUE WAIT SVC           */ 00199000
*             GPR01P=ADDR(APRLECB); /* POINT TO ECB                  */ 00200000
         LA    GPR01P,APRLECB(,APRLPTR)                         02S0104 00201000
*             GPR00F=1;             /* WAIT COUNT                    */ 00202000
         LA    GPR00F,1                                         02S0105 00203000
*             SVC(1);               /* WAIT FOR ECB POSTING          */ 00204000
         SVC   1                                                02S0106 00205000
*           END;                    /* ECB WAIT COMPLETE             */ 00206000
*         RFY                                                   02S0108 00207000
*          (GPR00F,                                             02S0108 00208000
*           GPR01P) UNRSTD;         /* FINISHED WITH REGISTERS       */ 00209000
@RF00102 DS    0H                                               02S0109 00210000
*       END;                        /* ECB WAIT COMPLETE WAIT FOR I/O   00211000
*                                      TO COMPLETE                   */ 00212000
*       APRLACTV=TSOFF;             /* RESET BUSY INDICATOR          */ 00213000
         MVI   APRLACTV(APRLPTR),X'00'                          02S0110 00214000
*       IF APRLECBC='7F'X THEN      /* IF NORMAL COMPLETION,         */ 00215000
         CLI   APRLECBC(APRLPTR),X'7F'                          02S0111 00216000
         BE    @RT00111                                         02S0111 00217000
*         GOTO EXIT0;               /* THEN GO TO EXIT ROUTINE       */ 00218000
*       ELSE                        /* OTHERWISE --                  */ 00219000
*         GOTO EXITXCTL;            /* GO TO 'XCTL' LOGIC (ERROR)    */ 00220000
         B     EXITXCTL                                         02S0113 00221000
*     END;                          /* END OF 'WAIT' LOGIC           */ 00222000
         EJECT                                                          00223000
*                                                               01S0115 00224000
*   /*****************************************************************/ 00225000
*   /*                                                               */ 00226000
*   /* IF RCODE > 4 (CAUSED BY AN ERROR CONDITION) OR NOT A FAST-PATH*/ 00227000
*   /* REQUEST, 'XCTL' TO IGTMBD.                                    */ 00228000
*   /*                                                               */ 00229000
*   /*****************************************************************/ 00230000
*                                                               01S0115 00231000
*EXITXCTL:                          /* ERROR OR NOT FAST-PATH        */ 00232000
*   DO;                             /* GO TO VPSS ROUTER ROUTINE     */ 00233000
*                                                               02S0115 00234000
@RF00091 DS    0H                                               02S0115 00235000
EXITXCTL DS    0H                                               02S0116 00236000
*     /***************************************************************/ 00237000
*     /*                                                             */ 00238000
*     /* NOT FAST-PATH REQUEST (EXCW, EXC, WAIT) OR ERROR CONDITION  */ 00239000
*     /* DETECTED. TRANSFER CONTROL TO IGTMBD TO CONTINUE PROCESSING.*/ 00240000
*     /* CONTROL WILL BE RETURNED DIRECTLY TO THE USER, NOT TO THIS  */ 00241000
*     /* PROGRAM (IE: THIS IS AN XCTL, NOT A CALL, TRANSFER OF       */ 00242000
*     /* CONTROL). ON ENTRY TO IGTMBD, ALL REGISTERS MUST BE THE SAME*/ 00243000
*     /* AS ON ENTRY TO THIS PROGRAM EXCEPT REGISTER 15, WHICH MUST  */ 00244000
*     /* CONTAIN THE ADDRESS OF IGTMBD.                              */ 00245000
*     /*                                                             */ 00246000
*     /***************************************************************/ 00247000
*                                                               02S0116 00248000
*     APSVRG15=                     /* SET SAVED REGISTER 15         */ 00249000
*         APSVRG0->                 /* SAVED REGISTER 0              */ 00250000
*         I031P->                   /* POINTS TO WORD WITH @ APUT    */ 00251000
*         APUTAPCT->                /* POINTS TO APCT                */ 00252000
*         APCTZMBD;                 /* POINTS TO @ IGTMBD            */ 00253000
         L     @09,APSVRG0(,APSVPTR)                            02S0116 00254000
         L     @09,I031P(,@09)                                  02S0116 00255000
         L     @09,APUTAPCT(,@09)                               02S0116 00256000
         L     @09,APCTZMBD(,@09)                               02S0116 00257000
         ST    @09,APSVRG15(,APSVPTR)                           02S0116 00258000
*     LM(R14P,R12P,APSVREGS);       /* RELOAD INPUT REGISTERS NOTE:     00259000
*                                      R15 -> IGTMBD                 */ 00260000
         LM    R14P,R12P,APSVREGS(APSVPTR)                      02S0117 00261000
*     RFY                                                       02S0118 00262000
*      (R15P) RSTD;                 /* USING REGISTER 15             */ 00263000
*     GOTO R15P->APCVZMBD;          /* GO TO MODULE IGTMBD           */ 00264000
         BR    R15P                                             02S0119 00265000
*     RFY                                                       02S0120 00266000
*      (R15P) UNRSTD;               /* FINISHED WITH REGISTER 15     */ 00267000
*                                                               02S0120 00268000
*     /***************************************************************/ 00269000
*     /*                                                             */ 00270000
*     /* NOTE -- CONTROL DOES NOT RETURN                             */ 00271000
*     /*                                                             */ 00272000
*     /***************************************************************/ 00273000
*                                                               02S0121 00274000
*   END;                            /* 'XCTL' ACCOMPLISHED           */ 00275000
         EJECT                                                          00276000
*                                                               01S0122 00277000
*   /*****************************************************************/ 00278000
*   /*                                                               */ 00279000
*   /* IF FAST-PATH REQUEST AND RETURN CODE IS 0 (OK) OR 4 (INVALID  */ 00280000
*   /* APRL STATUS BUSY/FREE) THEN RETURN TO CALLER WITH REGISTER 15 */ 00281000
*   /* SET.                                                          */ 00282000
*   /*                                                               */ 00283000
*   /*****************************************************************/ 00284000
*                                                               01S0122 00285000
*EXIT0:                             /* FAST PATH RETURN              */ 00286000
*   DO;                             /* 0 RETURN CODE                 */ 00287000
*                                                               02S0122 00288000
EXIT0    DS    0H                                               02S0123 00289000
*     /***************************************************************/ 00290000
*     /*                                                             */ 00291000
*     /* FAST PATH REQUEST AND NO ERROR CONDITION DETECTED. IF THE   */ 00292000
*     /* USER HAS SPECIFIED A RETURN WORD (APRLURCP NOT ZERO), THEN  */ 00293000
*     /* SET THE WORD TO ZERO. FALL THROUGH TO THE RETURN-TO-CALLER  */ 00294000
*     /* LOGIC.                                                      */ 00295000
*     /*                                                             */ 00296000
*     /***************************************************************/ 00297000
*                                                               02S0123 00298000
*     IF APRLURCP^=RCODE THEN       /* IF USER WANTS RETURN CODE     */ 00299000
         L     @09,APRLURCP(,APRLPTR)                           02S0123 00300000
         CR    @09,RCODE                                        02S0123 00301000
         BE    @RF00123                                         02S0123 00302000
*       APRLURCP->I031F=RCODE;      /* THEN SET TO ZERO.             */ 00303000
         ST    RCODE,I031F(,@09)                                02S0124 00304000
*   END;                            /* FALL THRU TO RETURN LOGIC     */ 00305000
@RF00123 DS    0H                                               02S0126 00306000
*EXIT4:                             /* FAST PATH RETURN              */ 00307000
*   DO;                             /* 4 RETURN CODE                 */ 00308000
*                                                               02S0126 00309000
EXIT4    DS    0H                                               02S0127 00310000
*     /***************************************************************/ 00311000
*     /*                                                             */ 00312000
*     /* FAST PATH REQUEST AND APRL BUSY/FREE SET WRONG. ALSO USED BY*/ 00313000
*     /* EXIT0 TO RETURN CONTROL TO THE USER. ON ENTRY, RCODE HAS    */ 00314000
*     /* BEEN SET TO THE DESIRED REGISTER 15 RETURN CODE.            */ 00315000
*     /*                                                             */ 00316000
*     /***************************************************************/ 00317000
*                                                               02S0127 00318000
*     RETURN CODE(RCODE);           /* RETURN TO CALLER WITH REGISTER   00319000
*                                      15 SET TO 0 OR 4.             */ 00320000
         LR    @15,RCODE                                        02S0127 00321000
         L     @14,12(,@13)                                     02S0127 00322000
         LM    @00,@12,20(@13)                                  02S0127 00323000
         BR    @14                                              02S0127 00324000
*   END;                            /* END OF RETURN LOGIC.          */ 00325000
         EJECT                                                          00326000
*                                   /* IGTXEPLG                      */ 00327000
*                                                               01S0129 00328000
*   /*****************************************************************/ 00329000
*   /*                                                               */ 00330000
*   /* SYSTEM DECLARES                                               */ 00331000
*   /*                                                               */ 00332000
*   /*****************************************************************/ 00333000
*                                                               01S0129 00334000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 00335000
*     GPR00F FIXED(31) REG(0),                                  01S0129 00336000
*     GPR01P PTR(31) REG(1);                                    01S0129 00337000
*   DECLARE                         /* COMMON VARIABLES              */ 00338000
*     I256C CHAR(256) BASED,                                    01S0130 00339000
*     I031F FIXED(31) BASED,                                    01S0130 00340000
*     I031P PTR(31) BASED,                                      01S0130 00341000
*     I015F FIXED(15) BASED,                                    01S0130 00342000
*     I015P PTR(15) BASED,                                      01S0130 00343000
*     I008P PTR(8) BASED,                                       01S0130 00344000
*     I001C CHAR(1) BASED;                                      01S0130 00345000
*                                                               01S0131 00346000
*   /*****************************************************************/ 00347000
*   /*                                                               */ 00348000
*   /* MODULE PATCH AREA                                             */ 00349000
*   /*                                                               */ 00350000
*   /*****************************************************************/ 00351000
*                                                               01S0131 00352000
*   DCL                                                         01S0131 00353000
*     APPATCH CHAR(*) INT STATIC LOCAL GEND;                    01S0131 00354000
*   GENERATE DATA DEFS(APPATCH);                                01S0132 00355000
*                                                               01S0133 00356000
*   /*****************************************************************/ 00357000
*   /*                                                               */ 00358000
*   /* GENERAL REGISTER DECLARES                                     */ 00359000
*   /*                                                               */ 00360000
*   /*****************************************************************/ 00361000
*                                                               01S0133 00362000
*   DCL                                                         01S0133 00363000
*     R0F FIXED(31) REG(0),                                     01S0133 00364000
*     R0P PTR(24) REG(0),                                       01S0133 00365000
*     R0RSTD PTR(31) REG(0),                                    01S0133 00366000
*     R1F FIXED(31) REG(1),                                     01S0133 00367000
*     R1P PTR(24) REG(1),                                       01S0133 00368000
*     R1RSTD PTR(31) REG(1),                                    01S0133 00369000
*     R2F FIXED(31) REG(2),                                     01S0133 00370000
*     R2P PTR(24) REG(2),                                       01S0133 00371000
*     R2RSTD PTR(31) REG(2),                                    01S0133 00372000
*     R3F FIXED(31) REG(3),                                     01S0133 00373000
*     R3P PTR(24) REG(3),                                       01S0133 00374000
*     R3RSTD PTR(31) REG(3),                                    01S0133 00375000
*     R4F FIXED(31) REG(4),                                     01S0133 00376000
*     R4P PTR(24) REG(4),                                       01S0133 00377000
*     R4RSTD PTR(31) REG(4),                                    01S0133 00378000
*     R5F FIXED(31) REG(5),                                     01S0133 00379000
*     R5P PTR(24) REG(5),                                       01S0133 00380000
*     R5RSTD PTR(31) REG(5),                                    01S0133 00381000
*     R6F FIXED(31) REG(6),                                     01S0133 00382000
*     R6P PTR(24) REG(6),                                       01S0133 00383000
*     R6RSTD PTR(31) REG(6),                                    01S0133 00384000
*     R7F FIXED(31) REG(7),                                     01S0133 00385000
*     R7P PTR(24) REG(7),                                       01S0133 00386000
*     R7RSTD PTR(31) REG(7),                                    01S0133 00387000
*     R8F FIXED(31) REG(8),                                     01S0133 00388000
*     R8P PTR(24) REG(8),                                       01S0133 00389000
*     R8RSTD PTR(31) REG(8),                                    01S0133 00390000
*     R9F FIXED(31) REG(9),                                     01S0133 00391000
*     R9P PTR(24) REG(9),                                       01S0133 00392000
*     R9RSTD PTR(31) REG(9),                                    01S0133 00393000
