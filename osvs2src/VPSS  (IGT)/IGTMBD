         BNM   @RF00237                                         03S0237 00621000
*         IF APCWR15B<APCCRERR THEN /* IF ERROR CODE HIGHER THAN     */ 00622000
         CLI   APCWR15B(APCWPTR),8                              03S0238 00623000
         BNL   @RF00238                                         03S0238 00624000
*           DO;                     /* CURRENT CODE, LOG ERROR.      */ 00625000
*             APEMVALN=APCMK011;    /* NAME OF FIELD IN ERROR        */ 00626000
         LA    @11,112                                          03S0240 00627000
         ALR   @11,APCWPTR                                      03S0240 00628000
         MVC   APEMVALN(4,@11),@CF00561                         03S0240 00629000
*             APEMMSGN=APCMC003;    /* ERROR MESSAGE NUMBER          */ 00630000
         MVC   APEMMSGN(4,@11),@CF00065                         03S0241 00631000
*             APCWRCN=APCCC003;     /* USER RETURN CODE              */ 00632000
         MVI   APCWRCN(APCWPTR),X'03'                           03S0242 00633000
*             APCWR15B=APCCRERR;    /* SET REGISTER 15 TO ERROR      */ 00634000
         MVI   APCWR15B(APCWPTR),X'08'                          03S0243 00635000
*           END;                    /* ERROR LOGGED.                 */ 00636000
*         ELSE                                                  03S0245 00637000
*           ;                       /* COMPILER REQUIREMENT          */ 00638000
@RF00238 DS    0H                                               03S0246 00639000
*       ELSE                        /* IF OK SO FAR --               */ 00640000
*         IF APCWACB>0&             /* IF ACB EXISTS BUT             */ 00641000
*             APCPSACN=BON THEN     /* MAY NOT, IS ERROR.            */ 00642000
         B     @RC00237                                         03S0246 00643000
@RF00237 L     @11,APCWACB(,APCWPTR)                            03S0246 00644000
