*             DO;                   /* (UNSUPPORTED OR UNKNOWN)      */ 00587000
*               APCWRCN=APCCC001;   /* RETURN CODE                   */ 00588000
         MVI   APCWRCN(APCWPTR),X'01'                           03S0227 00589000
