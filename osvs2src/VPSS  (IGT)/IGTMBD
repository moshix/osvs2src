         TM    ACBOPEN(@11),B'00010000'                         03S0270 00743000
         BNO   @RF00270                                         03S0270 00744000
*         IF(APCWR15B<APCCRWRN)|    /* IF THIS ERROR CODE IS         */ 00745000
*             (APCWR15B<APCCRERR&   /* HIGHER THAN CURRENT ERROR     */ 00746000
*             APCPSOPW=BOFF) THEN   /* CODE, THEN                    */ 00747000
         CLI   APCWR15B(APCWPTR),4                              03S0271 00748000
         BL    @RT00271                                         03S0271 00749000
