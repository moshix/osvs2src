TMBD     TITLE 'IGTMBD -- VPSS CALL ROUTER                             *00001000
                        '                                               00002000
IGTMBD   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTMBD    77.092'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
*                                                               01S0089 00014000
*   /*****************************************************************/ 00015000
*   /*                                                               */ 00016000
*   /* SAVEAREA/WORKAREA NOT YET ESTABLISHED. THEREFORE ALL          */ 00017000
*   /* CALCULATIONS MUST BE PERFORMED USING ONLY THE REGISTERS.      */ 00018000
*   /*                                                               */ 00019000
*   /*****************************************************************/ 00020000
*                                                               01S0090 00021000
*                                                               01S0090 00022000
*   /*****************************************************************/ 00023000
*   /*                                                               */ 00024000
*   /* FIND THE APUT. SET APUTPTR TO ADDRESS OF THIS CONTROL BLOCK.  */ 00025000
*   /*                                                               */ 00026000
*   /*****************************************************************/ 00027000
*                                                               01S0090 00028000
*   DO;                             /* START OF CODE                 */ 00029000
*                                                               02S0090 00030000
*     /***************************************************************/ 00031000
*     /*                                                             */ 00032000
*     /* FIND THE APUT. ON ENTRY, REG 0 -> WORD WHICH CONTAINS THE   */ 00033000
*     /* ADDRESS OF THE APUT. SET APUTPTR TO THE ADDRESS.            */ 00034000
*     /*                                                             */ 00035000
*     /***************************************************************/ 00036000
*                                                               02S0091 00037000
*     RFY                                                       02S0091 00038000
*      (R0P,                                                    02S0091 00039000
*       APUTPTR) RSTD;              /* NEED THESE REGS               */ 00040000
*     APUTPTR=R0P;                  /* USEFULL POINTER               */ 00041000
         LR    APUTPTR,R0P                                      02S0092 00042000
*     RFY                                                       02S0093 00043000
*      (R0P) UNRSTD;                /* FINISHED WITH REG 0           */ 00044000
*     APUTPTR=APUTPTR->I031P;       /* POINT TO APUT                 */ 00045000
         L     APUTPTR,I031P(,APUTPTR)                          02S0094 00046000
*     RFY                                                       02S0095 00047000
*      (APUT) BASED(APUTPTR);       /* TELL COMPILER                 */ 00048000
*   END;                            /* END OF CODE                   */ 00049000
*   RFY                                                         01S0097 00050000
*    (APUTPTR) RSTD;                /* TELL COMPILER OF POINTER      */ 00051000
*   RFY                                                         01S0098 00052000
*    (APUT) BASED(APUTPTR);         /* TO APUT.                      */ 00053000
*                                                               01S0098 00054000
*   /*****************************************************************/ 00055000
*   /*                                                               */ 00056000
*   /* FIND THE APCT. SET APCTPTR TO ADDRESS OF THIS CONTROL BLOCK.  */ 00057000
*   /*                                                               */ 00058000
*   /*****************************************************************/ 00059000
*                                                               01S0099 00060000
*   DO;                             /* START OF CODE                 */ 00061000
*                                                               02S0099 00062000
*     /***************************************************************/ 00063000
*     /*                                                             */ 00064000
*     /* FIND THE APCT. ON ENTRY, APUTPTR -> APUT. THE APUT CONTAINS */ 00065000
*     /* THE ADDRESS OF THE APCT. SET APCTPTR TO THIS ADDRESS.       */ 00066000
*     /*                                                             */ 00067000
*     /***************************************************************/ 00068000
*                                                               02S0100 00069000
*     RFY                                                       02S0100 00070000
*      (APCTPTR) RSTD;              /* NEED THIS REG                 */ 00071000
*     APCTPTR=APUTAPCT;             /* POINT TO APCT                 */ 00072000
         L     APCTPTR,APUTAPCT(,APUTPTR)                       02S0101 00073000
*     RFY                                                       02S0102 00074000
*      (APCT) BASED(APCTPTR);       /* TELL COMPILER                 */ 00075000
*   END;                            /* END OF CODE                   */ 00076000
*   RFY                                                         01S0104 00077000
*    (APCTPTR) RSTD;                /* TELL COMPILER OF POINTER      */ 00078000
*   RFY                                                         01S0105 00079000
*    (APCT) BASED(APCTPTR);         /* TO APCT.                      */ 00080000
         EJECT                                                          00081000
*                                                               01S0106 00082000
*   /*****************************************************************/ 00083000
*   /*                                                               */ 00084000
*   /* FIND ACB, IF ANY. SET APACBPTR = @ ACB. IF NO ACB, SET        */ 00085000
*   /* APACBPTR = 0. IF INVALID UNIT, SET APACBPTR = -1.             */ 00086000
*   /*                                                               */ 00087000
*   /*****************************************************************/ 00088000
*                                                               01S0106 00089000
*   DO;                             /* START OF CODE                 */ 00090000
*                                                               02S0106 00091000
*     /***************************************************************/ 00092000
*     /*                                                             */ 00093000
*     /* FIND ACB ASSOCIATED WITH UNIT APINUNIT. ON INPUT, APINPTR   */ 00094000
*     /* CONTAINS THE ADDRESS OF THE INPUT PARAMETER LIST. ON OUTPUT,*/ 00095000
*     /* APACBPTR CONTAINS: 1) THE ADDRESS OF THE ACB, 2) ZERO IF THE*/ 00096000
*     /* ACB DOES NOT EXIST, OR 3) -1 IF APINUNIT IS INVALID.        */ 00097000
*     /*                                                             */ 00098000
*     /***************************************************************/ 00099000
*                                                               02S0107 00100000
*     RFY                                                       02S0107 00101000
*      (APACBPTR) RSTD;             /* USING REG                     */ 00102000
*     RFY                                                       02S0108 00103000
*      (APAT) BASED(APUTAPAT);      /* ACB TABLE POINTED TO BY APUT  */ 00104000
*     APACBPTR=APINUNIT;            /* UNIT NUMBER FROM CALLER       */ 00105000
         L     @10,APINAUNT(,APINPTR)                           02S0109 00106000
         L     APACBPTR,APINUNIT(,@10)                          02S0109 00107000
*     IF APACBPTR<=00|              /* IF UNIT NUMBER INVALID        */ 00108000
*         APACBPTR>99 THEN          /* (RANGE IS 1 TO 99), SET       */ 00109000
         LTR   APACBPTR,APACBPTR                                02S0110 00110000
         BNP   @RT00110                                         02S0110 00111000
         C     APACBPTR,@CF02436                                02S0110 00112000
         BNH   @RF00110                                         02S0110 00113000
@RT00110 DS    0H                                               02S0111 00114000
*       APACBPTR=-1;                /* APACBPTR = -1.                */ 00115000
         SLR   APACBPTR,APACBPTR                                02S0111 00116000
         BCTR  APACBPTR,0                                       02S0111 00117000
*     ELSE                          /* IF UNIT NUMBER VALID,         */ 00118000
*       APACBPTR=APATACB(APACBPTR); /* GET ACB PTR FROM ACB ARRAY.   */ 00119000
         B     @RC00110                                         02S0112 00120000
@RF00110 LR    @10,APACBPTR                                     02S0112 00121000
         SLA   @10,2                                            02S0112 00122000
         L     @08,APUTAPAT(,APUTPTR)                           02S0112 00123000
         L     @10,APATACB-4(@10,@08)                           02S0112 00124000
         LR    APACBPTR,@10                                     02S0112 00125000
*     RFY                                                       02S0113 00126000
*      (APAT);                      /* FINISHED WITH APAT            */ 00127000
@RC00110 DS    0H                                               02S0114 00128000
*     RFY                                                       02S0114 00129000
*      (IFGACB) BASED(APACBPTR);    /* APACBPTR = -1, 0, OR @ACB     */ 00130000
*   END;                            /* END OF CODE                   */ 00131000
*   RFY                                                         01S0116 00132000
*    (APACBPTR) RSTD;               /* TELL COMPILER OF POINTER      */ 00133000
*   RFY                                                         01S0117 00134000
*    (IFGACB) BASED(APACBPTR);      /* TO ACB FOR UNIT 'APINUNIT'    */ 00135000
         EJECT                                                          00136000
*                                                               01S0118 00137000
*   /*****************************************************************/ 00138000
*   /*                                                               */ 00139000
*   /* FIND THE APCP ENTRY FOR THE REQUESTED FUNCTION, APINFUNC. IF  */ 00140000
*   /* REQUESTED FUNCTION DOES NOT EXIST, SET POINTER TO DUMMY APCP  */ 00141000
*   /* ENTRY SHOWING INVALID FUNCTION.                               */ 00142000
*   /*                                                               */ 00143000
*   /*****************************************************************/ 00144000
*                                                               01S0118 00145000
*   DO;                             /* START OF CODE                 */ 00146000
*                                                               02S0118 00147000
*     /***************************************************************/ 00148000
*     /*                                                             */ 00149000
*     /* FIND THE APCP ENTRY FOR THE REQUESTED FUNCTION, APINFUNC. IF*/ 00150000
*     /* THE REQUESTED FUNCTION DOES NOT EXIST, SET THE RETURN       */ 00151000
*     /* POINTER TO A DUMMY APCP LOCATED AT END OF APCP TABLE. THIS  */ 00152000
*     /* ENTRY IS ALSO SELECTED IF APINFUNC IS NOT SPECIFIED (CALL   */ 00153000
*     /* INPUT HAS ONLY 1 PARAMETER). THIS DUMMY ENTRY IS FLAGGED AS */ 00154000
*     /* AN UNKNOWN FUNCTION REQUEST.                                */ 00155000
*     /*                                                             */ 00156000
*     /***************************************************************/ 00157000
*                                                               02S0119 00158000
*     RFY                                                       02S0119 00159000
*      (R0F) RSTD;                  /* REQUIRED REGISTERS            */ 00160000
*     IF APINAP1E=BON THEN          /* IF ONLY 1-PARM, THEN FUNCTION */ 00161000
         TM    APINAP1E(APINPTR),B'10000000'                    02S0120 00162000
         BNO   @RF00120                                         02S0120 00163000
*       R0F=0;                      /* NOT SPECIFIED SO USE DUMMY.   */ 00164000
         SLR   R0F,R0F                                          02S0121 00165000
*     ELSE                          /* OTHERWISE SET REGISTER-0 =    */ 00166000
*       R0F=APINFUNC;               /* FUNCTION MNEMONIC.            */ 00167000
         B     @RC00120                                         02S0122 00168000
@RF00120 L     @10,APINAFNC(,APINPTR)                           02S0122 00169000
         L     R0F,APINFUNC(,@10)                               02S0122 00170000
*     RFY                                                       02S0123 00171000
*      (R14F,                                                   02S0123 00172000
*       R15P,                                                   02S0123 00173000
*       APCPPTR) RSTD;              /* REQUIRED REGISTERS            */ 00174000
@RC00120 DS    0H                                               02S0124 00175000
*     RFY                                                       02S0124 00176000
*      (APCP) BASED(APCPPTR);       /* TELL COMPILER OF POINTER      */ 00177000
*     APCPPTR=APCTAPCP;             /* POINT TO APCP HEADER          */ 00178000
         L     APCPPTR,APCTAPCP(,APCTPTR)                       02S0125 00179000
*     R14F=APCPLN1;                 /* LENGTH OF APCP ENTRY          */ 00180000
         LA    R14F,96                                          02S0126 00181000
*     R15P=APCPLAST+ADDR(APCP);     /* POINT TO LAST ENTRY PRIOR TO     00182000
*                                      DUMMY ENTRY                   */ 00183000
         LR    R15P,APCPPTR                                     02S0127 00184000
         AL    R15P,APCPLAST(,APCPPTR)                          02S0127 00185000
*     APCPPTR=ADDR(APCPFRST);       /* POINT TO FIRST APCP ENTRY     */ 00186000
         LA    @08,APCPFRST(,APCPPTR)                           02S0128 00187000
         LR    APCPPTR,@08                                      02S0128 00188000
*                                                               02S0129 00189000
*/*                                                                  */ 00190000
*/* EQUIVALENT CODE ----                                             */ 00191000
*/* DO APCPPTR = APCPPTR TO R15P BY R14F                             */ 00192000
*/*    WHILE(R0F ^= APCPAPFN)                                        */ 00193000
*/*                                                                  */ 00194000
*                                                               02S0129 00195000
*FNDAPCP1:                          /* SCAN APCP FOR MATCHING        */ 00196000
*     IF R0F^=APCPAPFN THEN         /* APCPAPFN (FUNCTION MNEMONIC). */ 00197000
FNDAPCP1 CL    R0F,APCPAPFN(,APCPPTR)                           02S0129 00198000
         BE    @RF00129                                         02S0129 00199000
*       GEN(BXLE APCPPTR,R14F,FNDAPCP1)/* IF NONE FOUND, THEN POINT  */ 00200000
*       REFS(R14F,R15P)             /* AT DUMMY ENTRY AT END OF      */ 00201000
*           SETS(APCPPTR)           /* APCP TABLE.                   */ 00202000
*           FLOWS(FNDAPCP1)         /* APCPPTR NOW SET TO ADDRESS    */ 00203000
*           SEQFLOW;                /* OF SELECTED ENTRY.            */ 00204000
         BXLE APCPPTR,R14F,FNDAPCP1                                     00205000
*     RFY                                                       02S0131 00206000
*      (R0F,                                                    02S0131 00207000
*       R14F,                                                   02S0131 00208000
*       R15P) UNRSTD;               /* FINISHED WITH TEMPORARY REGS  */ 00209000
@RF00129 DS    0H                                               02S0132 00210000
*     RFY                                                       02S0132 00211000
*      (APCP) BASED(APCPPTR);       /* APCPPTR POINTS TO APCP ENTRY  */ 00212000
*   END;                            /* END OF CODE                   */ 00213000
*   RFY                                                         01S0134 00214000
*    (APCPPTR) RSTD;                /* TELL COMPILER OF POINTER      */ 00215000
*   RFY                                                         01S0135 00216000
*    (APCP) BASED(APCPPTR);         /* TO THE APCP ENTRY.            */ 00217000
         EJECT                                                          00218000
*                                                               01S0136 00219000
*   /*****************************************************************/ 00220000
*   /*                                                               */ 00221000
*   /* OBTAIN A SAVEAREA/WORKAREA. USE THE WORKAREA POINTED TO BY THE*/ 00222000
*   /* ACB IF POSSIBLE.                                              */ 00223000
*   /*                                                               */ 00224000
*   /*****************************************************************/ 00225000
*                                                               01S0136 00226000
*   RFY                                                         01S0136 00227000
*    (RAPINPTR) RSTD;               /* REGISTER 1 REQUIRED TO        */ 00228000
*   RAPINPTR=APINPTR;               /* OBTAIN SA-WA. SAVE INPUT      */ 00229000
         LR    RAPINPTR,APINPTR                                 01S0137 00230000
*   RFY                                                         01S0138 00231000
*    (APINPTR) UNRSTD;              /* PARAMETERS IN REGISTER        */ 00232000
*   RFY                                                         01S0139 00233000
*    (APIN) BASED(RAPINPTR);        /* RAPINPTR. TELL COMPILER.      */ 00234000
*   DO;                             /* START OF CODE                 */ 00235000
*                                                               02S0140 00236000
*     /***************************************************************/ 00237000
*     /*                                                             */ 00238000
*     /* OBTAIN A SAVEAREA-WORKAREA (SA-WA). RETURN THE ADDRESS OF   */ 00239000
*     /* THIS WA-SA IN REGISTER-1. USE THE WORKAREA POINTED TO BY THE*/ 00240000
*     /* ACB IF THE FOLLOWING CONDITIONS EXIST: 1) THE APCP SHOWS THE*/ 00241000
*     /* ACB'S SA-WA MAY BE USED (APCPSPSA=1), 2) THE ACB EXISTS     */ 00242000
*     /* (APACBPTR>0), 3) THE ACB IS OPEN (ACBOPEN=1), AND 4) THE    */ 00243000
*     /* SA-WA IS NOT IN USE (APAXSVSW=00). IF ALL THESE CONDITIONS  */ 00244000
*     /* DO NOT EXIST, GETMAIN A TEMPORARY SA-WA.                    */ 00245000
*     /*                                                             */ 00246000
*     /***************************************************************/ 00247000
*                                                               02S0141 00248000
*     RFY                                                       02S0141 00249000
*      (R0F,                                                    02S0141 00250000
*       R1P) RSTD;                  /* REQUIRED REGISTERS            */ 00251000
*     IF APCPSUSV=BON&              /* INSURE SA-WA MAY BE USED      */ 00252000
*         APACBPTR>0&               /* ACB MUST EXIST                */ 00253000
*         ACBOPEN=BON THEN          /* ACB MUST BE OPEN              */ 00254000
         TM    APCPSUSV(APCPPTR),B'01000000'                    02S0142 00255000
         BNO   @RF00142                                         02S0142 00256000
         LTR   APACBPTR,APACBPTR                                02S0142 00257000
         BNP   @RF00142                                         02S0142 00258000
         TM    ACBOPEN(APACBPTR),B'00010000'                    02S0142 00259000
         BNO   @RF00142                                         02S0142 00260000
*       DO;                         /* THEN SA-WA EXISTS. CHECK 02S0143 00261000
*                                      IN-USE INDICATOR              */ 00262000
*         TS(ACBAMWAP->APAXSVSW);   /* DETERMINE IF SA-WA IN-USE. IF    00263000
*                                      NOT, MARK IN-USE. USE TS TO      00264000
*                                      INSURE TASK & CPU        02S0144 00265000
*                                      SERIALIZATION.                */ 00266000
         L     @08,ACBAMWAP(,APACBPTR)                          02S0144 00267000
         TS    APAXSVSW(@08)                                    02S0144 00268000
*         BC(TSONMSK,FNDSAWA1);     /* IF NOT AVAILABLE, GO GETMAIN     00269000
*                                      NEW SA-WA.                    */ 00270000
         BC    4,FNDSAWA1                                       02S0145 00271000
*         R0F=(APSVLN+APSCLN+@SIZDATD+7)/* R0 = LENGTH OF:           */ 00272000
*             &'0000FFF8'X;         /* SAVEAREA + CONTROL WORD +        00273000
*                                      DYNAMIC AREA ROUNDED UPWARD TO   00274000
*                                      DOUBLEWORD BOUNDARY.          */ 00275000
         LA    R0F,95                                           02S0146 00276000
         AL    R0F,@SIZDATD                                     02S0146 00277000
         N     R0F,@CF02455                                     02S0146 00278000
*         R1P=ACBAMWAP->APAXAPSV;   /* POINT TO ACB'S SA-WA. (AT        00279000
*                                      APSC)                         */ 00280000
         L     @08,ACBAMWAP(,APACBPTR)                          02S0147 00281000
         L     R1P,APAXAPSV(,@08)                               02S0147 00282000
*         R1P->APSCCRSZ=R0F;        /* SET REQUIRED SIZE IN APSC     */ 00283000
         STH   R0F,APSCCRSZ(,R1P)                               02S0148 00284000
*         R1P=R1P+APSCLN;           /* SKIP APSC. POINT AT APSV.     */ 00285000
         LA    R1P,8(,R1P)                                      02S0149 00286000
*         R0F=CACBSAWA;             /* SHOW ACB'S SA-WA USED         */ 00287000
         SLR   R0F,R0F                                          02S0150 00288000
*       END;                        /* ACB'S SA-WA SELECTED          */ 00289000
*     ELSE                          /* IF ACB'S SA-WA NOT AVAILABLE, */ 00290000
*FNDSAWA1:                          /* THEN GETMAIN A SA-WA          */ 00291000
*       DO;                         /* USING IGTYGSV.                */ 00292000
         B     @RC00142                                         02S0152 00293000
@RF00142 DS    0H                                               02S0152 00294000
FNDSAWA1 DS    0H                                               02S0153 00295000
*         DO;                       /* IGTYGSV LV(APSVLN+@SIZDATD)INI   00296000
*                                      TIAL(YES)                     */ 00297000
*           RFY                                                 02S0154 00298000
*            (GPR00F,                                           02S0154 00299000
*             GPR01F) RSTD;         /* NEEDED REGISTERS              */ 00300000
*           GPR00F=APSVLN+@SIZDATD; /* SET REG 0 TO SUBPOOL/LENGTH   */ 00301000
         LA    GPR00F,80                                        02S0155 00302000
         AL    GPR00F,@SIZDATD                                  02S0155 00303000
*           GPR01F=0;               /* NO EXTRA STORAGE REQUIRED     */ 00304000
         SLR   GPR01F,GPR01F                                    02S0156 00305000
*           CALL APCVZUAA;          /* OBTAIN INITIAL           02S0157 00306000
*                                      SAVEAREA-WORKAREA             */ 00307000
         L     @15,APCTZUAA(,APCTPTR)                           02S0157 00308000
         BALR  @14,@15                                          02S0157 00309000
*           RFY                                                 02S0158 00310000
*            (GPR00F,                                           02S0158 00311000
*             GPR01F) UNRSTD;       /* FINISHED WITH REGISTERS       */ 00312000
*         END;                      /* REG_1 -> SAVEAREA/WORKAREA       00313000
*                                      OBTAINED R1P -> OBTAINED SA-WA*/ 00314000
*         R0F=CNEWSAWA;             /* SHOW NEW SA-WA USED           */ 00315000
         LA    R0F,1                                            02S0160 00316000
*       END;                        /* NEW SA-WA SELECTED.           */ 00317000
*                                                               02S0161 00318000
*     /***************************************************************/ 00319000
*     /*                                                             */ 00320000
*     /* SET R1P = @SA-WA & R0F TO INDICATE IF THE ACB'S SA-WA WAS   */ 00321000
*     /* USED (=CACBSAWA IF ACB SA-WA USED & =CNEWSAWA IF NEW SA-WA  */ 00322000
*     /* OBTAINED. R1P -> SA-WA. R0F = CACBSAWA|CNEWSAWA             */ 00323000
*     /*                                                             */ 00324000
*     /***************************************************************/ 00325000
*                                                               02S0162 00326000
*   END;                            /* END OF CODE                   */ 00327000
@RC00142 DS    0H                                               02S0163 00328000
*   RFY                                                         01S0163 00329000
*    (R0F,                                                      01S0163 00330000
*     R1P) RSTD;                    /* R1->SA-WA,               01S0163 00331000
*                                      R0=CACBSAWA|CNEWSAWA          */ 00332000
*                                                               01S0163 00333000
*   /*****************************************************************/ 00334000
*   /*                                                               */ 00335000
*   /* INITIALIZE THE SAVEAREA - WORKAREA. CHAIN NEW SAVEAREA INTO   */ 00336000
*   /* CHAIN. INITIALIZE DYNAMIC AREA POINTER.                       */ 00337000
*   /*                                                               */ 00338000
*   /*****************************************************************/ 00339000
*                                                               01S0164 00340000
*   DO;                             /* START OF CODE                 */ 00341000
*                                                               02S0164 00342000
*     /***************************************************************/ 00343000
*     /*                                                             */ 00344000
*     /* ON ENTRY, R1->SA-WA & R0=CACBSAWA|CNEWSAWA. CHAIN NEW       */ 00345000
*     /* SAVEAREA INTO OS CHAIN & POINT R13 AT NEW SAVEAREA. POINT   */ 00346000
*     /* APDATA AT DYNAMIC WORKAREA. SET DSAWA TO INDICATE IF THE    */ 00347000
*     /* SA-WA BELONGS TO THE ACB OR HAS BEEN GETMAIN'ED.            */ 00348000
*     /*                                                             */ 00349000
*     /***************************************************************/ 00350000
*                                                               02S0165 00351000
*     RFY                                                       02S0165 00352000
*      (R0F,                                                    02S0165 00353000
*       R1P,                                                    02S0165 00354000
*       APDATA) RSTD;               /* REQUIRED REGISTERS            */ 00355000
*     APSVNEXT=R1P;                 /* FORWARD CHAIN                 */ 00356000
         ST    R1P,APSVNEXT(,APSVPTR)                           02S0166 00357000
*     R1P->APSVPREV=APSVPTR;        /* BACKWARD CHAIN                */ 00358000
         ST    APSVPTR,APSVPREV(,R1P)                           02S0167 00359000
*     APSVPTR=R1P;                  /* POINT TO NEW SAVEAREA         */ 00360000
         LR    APSVPTR,R1P                                      02S0168 00361000
*     RFY                                                       02S0169 00362000
*      (R1P) UNRSTD;                /* FINISHED WITH R1              */ 00363000
*     APDATA=ADDR(APSV)+APSVLN;     /* POINT TO DYNAMIC AREA (AFTER     00364000
*                                      SAVEAREA).                    */ 00365000
         LA    APDATA,80(,APSVPTR)                              02S0170 00366000
*     GEN(USING @DATD,APDATA)       /* TELL ASSEMBLER                */ 00367000
*     NOSETS REFS(APDATA);                                      02S0171 00368000
         USING @DATD,APDATA                                             00369000
*     DSAWA=R0F;                    /* SAVE CACBSAWA|CNEWSAWA        */ 00370000
         LR    DSAWA,R0F                                        02S0172 00371000
*     RFY                                                       02S0173 00372000
*      (R0F) UNRSTD;                /* INDICATOR IN DYNAMIC AREA.       00373000
*                                      SA-WA ESTABLISHED             */ 00374000
*   END;                            /* END OF CODE                   */ 00375000
*                                                               01S0175 00376000
*   /*****************************************************************/ 00377000
*   /*                                                               */ 00378000
*   /* DYNAMIC AREA NOW ESTABLISHED. IT MAY BE USED FOR FURTHER      */ 00379000
*   /* CALCULATIONS                                                  */ 00380000
*   /*                                                               */ 00381000
*   /*****************************************************************/ 00382000
*                                                               01S0175 00383000
*   RFY                                                         01S0175 00384000
*    (APDATA) RSTD;                 /* RESTRICT DYNAMIC POINTER      */ 00385000
*   GEN(USING @DATD,APDATA)         /* TELL ASSEMBLER OF ADDRESSING  */ 00386000
*   REFS(APDATA)                    /* TELL COMPILER WHAT THE        */ 00387000
*       NOSETS                      /* GENERATE STATEMENT REQUIRES   */ 00388000
*       SEQFLOW;                    /* OF COMPILER.                  */ 00389000
*                                                               01S0176 00390000
         USING @DATD,APDATA                                             00391000
*   /*****************************************************************/ 00392000
*   /*                                                               */ 00393000
*   /* BUILD & INITIALIZE THE APCW. THIS CONTROL BLOCK IS BUILT IN   */ 00394000
*   /* THE DYNAMIC AREA.                                             */ 00395000
*   /*                                                               */ 00396000
*   /*****************************************************************/ 00397000
*                                                               01S0177 00398000
*   DO;                             /* START OF CODE                 */ 00399000
*                                                               02S0177 00400000
*     /***************************************************************/ 00401000
*     /*                                                             */ 00402000
*     /* INITIALIZE PART OF THE APCW. THE REST OF THE APCW IS BUILT  */ 00403000
*     /* AS THE PARAMETERS ARE VALIDATED.                            */ 00404000
*     /*                                                             */ 00405000
*     /***************************************************************/ 00406000
*                                                               02S0178 00407000
*     RFY                                                       02S0178 00408000
*      (APCWPTR) RSTD;              /* SET REGISTER APCWPTR TO       */ 00409000
*     APCWPTR=ADDR(DAPCW);          /* ADDRESS OF APCW               */ 00410000
         LA    APCWPTR,DAPCW                                    02S0179 00411000
*     RFY                                                       02S0180 00412000
*      (APCW) BASED(APCWPTR);       /* TELL COMPILER                 */ 00413000
*     APCW(1:LENGTH(APCW)-LENGTH(APCWAPEM))=/* ZERO HEADER PORTION   */ 00414000
*         APCW(1:LENGTH(APCW)-LENGTH(APCWAPEM))&&/* OF APCW CONTAINED*/ 00415000
*         APCW(1:LENGTH(APCW)-LENGTH(APCWAPEM));/* IN WORKAREA.      */ 00416000
         XC    APCW(112,APCWPTR),APCW(APCWPTR)                  02S0181 00417000
*     APCWID=APCWIDC;               /* SET IN-CORE ID                */ 00418000
         MVC   APCWID(4,APCWPTR),@CC01690                       02S0182 00419000
*     APCWAPEM=IGTMBREM;            /* BUILD SKELETON APEM           */ 00420000
         L     @01,APCTZMBR(,APCTPTR)                           02S0183 00421000
         L     @01,IGTMBRAE(,@01)                               02S0183 00422000
         MVC   APCWAPEM(128,APCWPTR),IGTMBREM(@01)              02S0183 00423000
*     APCWAPIN=ADDR(APIN);          /* USER'S INPUT PARM ADDRESS     */ 00424000
         ST    RAPINPTR,APCWAPIN(,APCWPTR)                      02S0184 00425000
*     APCWAPUT=ADDR(APUT);          /* APUT ADDRESS                  */ 00426000
         ST    APUTPTR,APCWAPUT(,APCWPTR)                       02S0185 00427000
*     APCWAPSV=APSVPREV;            /* USER'S SAVEAREA ADDRESS       */ 00428000
         L     @15,APSVPREV(,APSVPTR)                           02S0186 00429000
         ST    @15,APCWAPSV(,APCWPTR)                           02S0186 00430000
*     APCWACB=ADDR(IFGACB);         /* ACB ADDRESS (OR 0)            */ 00431000
         ST    APACBPTR,APCWACB(,APCWPTR)                       02S0187 00432000
*     APCWAPCP=ADDR(APCP);          /* APCP ENTRY ADDRESS            */ 00433000
         ST    APCPPTR,APCWAPCP(,APCWPTR)                       02S0188 00434000
*     APCWAPOP=APCPAPOP;            /* FUNCTION OP-CODE              */ 00435000
         MVC   APCWAPOP(2,APCWPTR),APCPAPOP(APCPPTR)            02S0189 00436000
*     APCWAPAL=ADDR(APCPAPAL);      /* ALGORITHM-DEPENDENT SWITCHES     00437000
*                                      (SEE MEMBER 'IGTXAPAL').      */ 00438000
*                                                               02S0190 00439000
         LA    @15,APCPAPAL(,APCPPTR)                           02S0190 00440000
         ST    @15,APCWAPAL(,APCWPTR)                           02S0190 00441000
*     /***************************************************************/ 00442000
*     /*                                                             */ 00443000
*     /* USE APCW TO POINT TO CONTROL BLOCKS INSTEAD OF REGISTERS.   */ 00444000
*     /* THIS ALLOWS PL/S COMPILER OPTIMIZATION OF REGISTERS.        */ 00445000
*     /*                                                             */ 00446000
*     /***************************************************************/ 00447000
*                                                               02S0191 00448000
*     RFY                                                       02S0191 00449000
*      (RAPINPTR,                                               02S0191 00450000
*       APUTPTR,                                                02S0191 00451000
*       APACBPTR) UNRSTD;           /* FINISHED WITH POINTERS        */ 00452000
*     RFY                                                       02S0192 00453000
*      (APCPPTR) RSTD;              /* USE FOR PERFORMANCE           */ 00454000
*     RFY                                                       02S0193 00455000
*       APIN BASED(APCWAPIN);       /* USER'S INPUT PARAMETER LIST   */ 00456000
*     RFY                                                       02S0194 00457000
*       APUT BASED(APCWAPUT);       /* VPSS USER TABLE               */ 00458000
*     RFY                                                       02S0195 00459000
*       APCP BASED(APCPPTR);        /* VPSS CALL PROCESSING SWITCHES */ 00460000
*     RFY                                                       02S0196 00461000
*       APEM BASED(APCWPTR+(ADDR(APCWAPEM)-ADDR(APCW)));/* ERR MSG   */ 00462000
*     RFY                                                       02S0197 00463000
*       IFGACB BASED(APCWACB);      /* VPSS ACB                      */ 00464000
*     RFY                                                       02S0198 00465000
*       APCX BASED(ACBAMAX);        /* VPSS ACB CALL EXTENSION       */ 00466000
*     RFY                                                       02S0199 00467000
*       APAX BASED(ACBAMWAP);       /* VPSS ACCESS METHOD EXTENSION  */ 00468000
*   END;                            /* END OF CODE                   */ 00469000
*   RFY                                                         01S0201 00470000
*    (APCWPTR) RSTD;                /* TELL COMPILER OF POINTER      */ 00471000
*   RFY                                                         01S0202 00472000
*    (APCW) BASED(APCWPTR);         /* TO THE APCW                   */ 00473000
         EJECT                                                          00474000
*                                                               01S0203 00475000
*   /*****************************************************************/ 00476000
*   /*                                                               */ 00477000
*   /* VALIDATE THE INPUT PARAMETERS & THE CURRENT ENVIRONMENT.      */ 00478000
*   /*                                                               */ 00479000
*   /*****************************************************************/ 00480000
*                                                               01S0203 00481000
*   DO;                             /* START OF CODE                 */ 00482000
*                                                               02S0203 00483000
*     /***************************************************************/ 00484000
*     /*                                                             */ 00485000
*     /* VALIDATE THE INPUT DATA & THE CURRENT ENVIRONMENT. IF AN    */ 00486000
*     /* ERROR IS DETECTED, SET APCWRCDE, APCWR15C, & APEMMSGN. SAVE */ 00487000
*     /* THE FIRST OCCURENCE OF THE HIGHEST ERROR CODE.              */ 00488000
*     /*                                                             */ 00489000
*     /***************************************************************/ 00490000
*                                                               02S0204 00491000
*     DO;                           /* START OF CODE                 */ 00492000
*                                                               03S0204 00493000
*       /*************************************************************/ 00494000
*       /*                                                           */ 00495000
*       /* DETERMINE NUMBER OF INPUT PARAMETERS. PLACE IN APCWNBRI.  */ 00496000
*       /* THEN INSURE NUMBER OF PARAMETERS IS WITHIN RANGE FOR THE  */ 00497000
*       /* SPECIFIED FUNCTION.                                       */ 00498000
*       /*                                                           */ 00499000
*       /*************************************************************/ 00500000
*                                                               03S0205 00501000
*       DAPINPTR=ADDR(APIN);        /* POINT TO INPUT LIST           */ 00502000
         L     @11,APCWAPIN(,APCWPTR)                           03S0205 00503000
         LR    DAPINPTR,@11                                     03S0205 00504000
*       DO APCWNBRI=1 BY 1          /* DETERMINE NUMBER OF           */ 00505000
*             WHILE DAPINPTR->APINAP1E=BOFF;/* PARMS. LAST ENTRY HAS */ 00506000
         LA    @11,1                                            03S0206 00507000
         ST    @11,APCWNBRI(,APCWPTR)                           03S0206 00508000
         B     @DE00206                                         03S0206 00509000
@DL00206 DS    0H                                               03S0207 00510000
*         DAPINPTR=DAPINPTR+4;      /* X'80' IN HI-ORDER BYTE.       */ 00511000
         AL    DAPINPTR,@CF00091                                03S0207 00512000
*       END;                        /* APCWNBRI SET                  */ 00513000
         LA    @11,1                                            03S0208 00514000
         AL    @11,APCWNBRI(,APCWPTR)                           03S0208 00515000
         ST    @11,APCWNBRI(,APCWPTR)                           03S0208 00516000
@DE00206 TM    APINAP1E(DAPINPTR),B'10000000'                   03S0208 00517000
         BZ    @DL00206                                         03S0208 00518000
*       IF(APCWNBRI<APCPMINP)|      /* IF TOO FEW PARMS OR           */ 00519000
*           (APCWNBRI>APCPMAXP) THEN/* TOO MANY PARMS, THEN ERROR    */ 00520000
         L     @11,APCWNBRI(,APCWPTR)                           03S0209 00521000
         CH    @11,APCPMINP(,APCPPTR)                           03S0209 00522000
         BL    @RT00209                                         03S0209 00523000
         CH    @11,APCPMAXP(,APCPPTR)                           03S0209 00524000
         BNH   @RF00209                                         03S0209 00525000
@RT00209 DS    0H                                               03S0210 00526000
*         IF APCWR15B<APCCRERR THEN /* IF THIS IS HIGHEST ERROR      */ 00527000
         CLI   APCWR15B(APCWPTR),8                              03S0210 00528000
         BNL   @RF00210                                         03S0210 00529000
*           DO;                     /* CODE, THEN LOG ERROR.         */ 00530000
*             APCWR15B=APCCRERR;    /* REG 15 ERROR CODE             */ 00531000
         MVI   APCWR15B(APCWPTR),X'08'                          03S0212 00532000
*             APCWRCN=APCCC009;     /* RETURN CODE                   */ 00533000
         MVI   APCWRCN(APCWPTR),X'09'                           03S0213 00534000
*             APEMMSGN=APCMC009;    /* SYNAD MESSAGE CODE APEMVALN      00535000
*                                      NOT USED                      */ 00536000
         MVC   APEMMSGN+112(4,APCWPTR),@CF00558                 03S0214 00537000
*           END;                    /* ERROR INDICATED               */ 00538000
*     END;                          /* END OF CHKNBRI CODE           */ 00539000
@RF00210 DS    0H                                               03S0216 00540000
@RF00209 DS    0H                                               03S0217 00541000
*     DO;                           /* START OF CHKAPFN CODE         */ 00542000
*                                                               03S0217 00543000
*       /*************************************************************/ 00544000
*       /*                                                           */ 00545000
*       /* SET APCWAPFN TO FUNCTION MNEMONIC                         */ 00546000
*       /*                                                           */ 00547000
*       /*************************************************************/ 00548000
*                                                               03S0218 00549000
*       IF APCWNBRI>                /* IF AN LEAST ENOUGH PARMS      */ 00550000
*           ((ADDR(APINAFNC)-ADDR(APIN))/4) THEN/* TO HAVE FUNCTION  */ 00551000
         L     @11,APCWNBRI(,APCWPTR)                           03S0218 00552000
         C     @11,@CF00053                                     03S0218 00553000
         BNH   @RF00218                                         03S0218 00554000
*         APCWAPFN=APINFUNC;        /* THEN SAVE, IN APCW            */ 00555000
         L     @11,APCWAPIN(,APCWPTR)                           03S0219 00556000
         L     @11,APINAFNC(,@11)                               03S0219 00557000
         MVC   APCWAPFN(4,APCWPTR),APINFUNC(@11)                03S0219 00558000
*       ELSE                        /* IF TOO FEW PARMS, THEN        */ 00559000
