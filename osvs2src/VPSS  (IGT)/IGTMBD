         BNP   @RF00246                                         03S0246 00646000
         TM    APCPSACN(APCPPTR),B'00100000'                    03S0246 00647000
         BNO   @RF00246                                         03S0246 00648000
*           IF(APCWR15B<APCCRWRN)|  /* IF THIS ERROR CODE IS         */ 00649000
*               (APCWR15B<APCCRERR& /* HIGHER THAN CURRENT ERROR     */ 00650000
*               APCPSACW=BOFF) THEN /* CODE, THEN                    */ 00651000
