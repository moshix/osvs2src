TMCA     TITLE ' IGTMCA -- INTERPRET I/O ERROR                         *00001000
                        '                                               00002000
IGTMCA   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTMCA    77.231'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
         EJECT                                                          00022000
*                                                               01S0126 00023000
*   /*****************************************************************/ 00024000
*   /*                                                               */ 00025000
*   /* INITIALIZE POINTERS TO MAJOR CONTROL BLOCKS                   */ 00026000
*   /*                                                               */ 00027000
*   /*****************************************************************/ 00028000
*                                                               01S0126 00029000
*   DO;                             /* START OF SEGMENT              */ 00030000
*                                                               02S0126 00031000
*     /***************************************************************/ 00032000
*     /*                                                             */ 00033000
*     /* FIND APIO AND INSURE THAT AN ERROR HAS OCCURRED             */ 00034000
*     /* (APRLECBC^='7F'X). THEN POINT TO OTHER MAJOR CONTROL BLOCKS */ 00035000
*     /* TO IMPROVE PL/S GENERATED CODE.                             */ 00036000
*     /*                                                             */ 00037000
*     /***************************************************************/ 00038000
*                                                               02S0127 00039000
*     IF APRLECBC='7F'X THEN        /* IF COMPLETION CODE = '7F'X    */ 00040000
         L     @09,APCWAPRL(,APCWPTR)                           02S0127 00041000
         CLI   APRLECBC(@09),X'7F'                              02S0127 00042000
         BE    @RT00127                                         02S0127 00043000
*       RETURN;                     /* THEN NO ERROR -- SO EXIT      */ 00044000
*     RFY                                                       02S0129 00045000
*       RAPIOPTR RSTD;              /* USE REGISTER POINTER          */ 00046000
*     RAPIOPTR=APCWAPIO;            /* FOR APIO/IOSB TO IMPROVE      */ 00047000
         L     RAPIOPTR,APCWAPIO(,APCWPTR)                      02S0130 00048000
*     RFY                                                       02S0131 00049000
*       APIO BASED(RAPIOPTR);       /* PL/S CODE.                    */ 00050000
*     RFY                                                       02S0132 00051000
*       RAPEMPTR RSTD;              /* POINT TO VPSS ERROR MESSAGE   */ 00052000
*     RAPEMPTR=ADDR(APEM);          /* IN THE APCW. USE REGISTER TO  */ 00053000
         LA    RAPEMPTR,112                                     02S0133 00054000
         ALR   RAPEMPTR,APCWPTR                                 02S0133 00055000
*     RFY                                                       02S0134 00056000
*       APEM BASED(RAPEMPTR);       /* IMPROVE PL/S CODE.            */ 00057000
*     RFY                                                       02S0135 00058000
*       RAPSNPTR RSTD;              /* POINT TO 3838 SENSE DATA      */ 00059000
*     RAPSNPTR=ADDR(APSN);          /* IN THE APIO. USE REGISTER TO  */ 00060000
         LA    RAPSNPTR,256                                     02S0136 00061000
         ALR   RAPSNPTR,RAPIOPTR                                02S0136 00062000
*     RFY                                                       02S0137 00063000
*       APSN BASED(RAPSNPTR);       /* IMPROVE PL/S CODE.            */ 00064000
*   END;                            /* MAJOR CONTROL BLOCKS LOCATED  */ 00065000
*                                                               01S0139 00066000
*   /*****************************************************************/ 00067000
*   /*                                                               */ 00068000
*   /* PLACE ADDRESS OF ASSOCIATED APRL IN THE PARAMETER LIST        */ 00069000
*   /*                                                               */ 00070000
*   /*****************************************************************/ 00071000
*                                                               01S0139 00072000
*   DO;                                                         02S0139 00073000
*                                                               02S0139 00074000
*     /***************************************************************/ 00075000
*     /*                                                             */ 00076000
*     /* PLACE THE ADDRESS OF THE ASSOCIATED APRL IN THE ERROR       */ 00077000
*     /* MESSAGE.                                                    */ 00078000
*     /*                                                             */ 00079000
*     /***************************************************************/ 00080000
*                                                               02S0140 00081000
*     IF APCWAPRL>0 THEN            /* IF APRL EXISTS, THEN          */ 00082000
         LTR   @09,@09                                          02S0140 00083000
         BNP   @RF00140                                         02S0140 00084000
*       DO;                         /* MOVE TO MESSAGE.              */ 00085000
*         DFIXED4=ADDR(APRL);       /* MAKE ADDRESS FIELD            */ 00086000
         ST    @09,DFIXED4                                      02S0142 00087000
*         UNPK(XAPEMRL,DWORK8(1:5));/* (6 CHARACTERS)                */ 00088000
         UNPK  XAPEMRL(7,RAPEMPTR),DWORK8(5)                    02S0143 00089000
*         TR(APEMAPRL,CCNVTHEX);    /* PRINTABLE HEX.                */ 00090000
         TR    APEMAPRL(6,RAPEMPTR),CCNVTHEX                    02S0144 00091000
*         APEMCM17=',';             /* RESET FIELD SEPARATOR.        */ 00092000
         MVI   APEMCM17(RAPEMPTR),C','                          02S0145 00093000
*       END;                        /* APRL IDENTIFIED               */ 00094000
*   END;                            /* APRL ADDR IN MSG IF AVAILABLE */ 00095000
@RF00140 DS    0H                                               02S0148 00096000
*                                                               01S0148 00097000
*   /*****************************************************************/ 00098000
*   /*                                                               */ 00099000
*   /* PLACE THE CHANNEL/UNIT NUMBER OF THE ASSOCIATED 3838 PORT IN  */ 00100000
*   /* THE ERROR MESSAGE (APEM).                                     */ 00101000
*   /*                                                               */ 00102000
*   /*****************************************************************/ 00103000
*                                                               01S0148 00104000
*   DO;                                                         02S0148 00105000
*                                                               02S0148 00106000
*     /***************************************************************/ 00107000
*     /*                                                             */ 00108000
*     /* PLACE THE UNIT NUMBER (CUU) OF THE 3838 SUBCHANNEL WHICH    */ 00109000
*     /* DETECTED THE ERROR IN THE ERROR MESSAGE                     */ 00110000
*     /*                                                             */ 00111000
*     /***************************************************************/ 00112000
*                                                               02S0149 00113000
*     IF APIOAPUB->APUBID=APUBIDC THEN/* IF APIO POINTS TO APUB,     */ 00114000
         L     @09,APIOAPUB(,RAPIOPTR)                          02S0149 00115000
         CLC   APUBID(4,@09),@CC02740                           02S0149 00116000
         BNE   @RF00149                                         02S0149 00117000
*       APEMUNIT=APIOAPUB->APUBUCB->UCBNAME;/* MOVE CUU FROM UCB     */ 00118000
         L     @09,APUBUCB(,@09)                                02S0150 00119000
         MVC   APEMUNIT(3,RAPEMPTR),UCBNAME(@09)                02S0150 00120000
*   END;                            /* 3838 SUBCHANNEL IDENTIFIED    */ 00121000
@RF00149 DS    0H                                               02S0152 00122000
*                                                               01S0152 00123000
*   /*****************************************************************/ 00124000
*   /*                                                               */ 00125000
*   /* BASED ON THE I/O COMPLETION CODE (APRLECBC), SET AN INITIAL   */ 00126000
*   /* DESCRIPTION OF THE ERROR.                                     */ 00127000
*   /*                                                               */ 00128000
*   /*****************************************************************/ 00129000
*                                                               01S0152 00130000
*   DO;                             /* START OF SEGMENT              */ 00131000
*                                                               02S0152 00132000
*     /***************************************************************/ 00133000
*     /*                                                             */ 00134000
*     /* FIND THE GENERAL CATEGORY OF ERROR BASED ON THE I/O ECB     */ 00135000
*     /* COMPLETION CODE (APRLECBC). SET DINFINDX TO THE VALUE WHICH */ 00136000
*     /* DESCRIBES THE ERROR, AND DFLAGS TO FURTHER PROCESSING       */ 00137000
*     /* OPTIONS. IF NO MATCH IS FOUND, TREAT AS UNKNOWN. THE COMPARE*/ 00138000
*     /* VALUES ARE IN THE ARRAY 'CECBTAB'.                          */ 00139000
*     /*                                                             */ 00140000
*     /***************************************************************/ 00141000
*                                                               02S0153 00142000
*     RFY                                                       02S0153 00143000
*      (R1P,                                                    02S0153 00144000
*       R15F) RSTD;                 /* USE REGISTER POINTER TO       */ 00145000
*     R1P=ADDR(APRL);               /* IMPROVE PL/S CODE             */ 00146000
*                                                               02S0154 00147000
         L     R1P,APCWAPRL(,APCWPTR)                           02S0154 00148000
*     /***************************************************************/ 00149000
*     /*                                                             */ 00150000
*     /* SCAN 'CECBTAB'                                              */ 00151000
*     /*                                                             */ 00152000
*     /***************************************************************/ 00153000
*                                                               02S0155 00154000
*     DO R15F=DIM(CECBTAB) TO 2 BY-1/* LOOK FOR MATCH. POINT         */ 00155000
*           WHILE(R1P->APRLECBC^=CECBTECB(R15F));/* TO ENTRY-1       */ 00156000
         LA    R15F,5                                           02S0155 00157000
@DL00155 LR    @14,R15F                                         02S0155 00158000
         SLA   @14,2                                            02S0155 00159000
         SLR   @09,@09                                          02S0155 00160000
         IC    @09,APRLECBC(,R1P)                               02S0155 00161000
         SLR   @02,@02                                          02S0155 00162000
         IC    @02,CECBTECB-4(@14)                              02S0155 00163000
         CR    @09,@02                                          02S0155 00164000
         BE    @DC00155                                         02S0155 00165000
*     END;                          /* IF NO MATCH (UNKNOWN COND).   */ 00166000
         BCTR  R15F,0                                           02S0156 00167000
         C     R15F,@CF00099                                    02S0156 00168000
         BNL   @DL00155                                         02S0156 00169000
@DC00155 DS    0H                                               02S0157 00170000
*     RFY                                                       02S0157 00171000
*       R1P UNRSTD;                 /* FINISHED WITH APRL POINTER    */ 00172000
*                                                               02S0157 00173000
*     /***************************************************************/ 00174000
*     /*                                                             */ 00175000
*     /* MATCH FOUND OR POINTING AT CECBTAB(1)                       */ 00176000
*     /*                                                             */ 00177000
*     /***************************************************************/ 00178000
*                                                               02S0158 00179000
*     DFLAGS=CECBTFLG(R15F);        /* FURTHER PROCESSING FLAGS      */ 00180000
         LR    @14,R15F                                         02S0158 00181000
         SLA   @14,2                                            02S0158 00182000
         LH    @10,CECBTFLG-4(@14)                              02S0158 00183000
         N     @10,@CF03412                                     02S0158 00184000
         STH   @10,DFLAGS                                       02S0158 00185000
*     DINFINDX=CECBTINF(R15F);      /* CINFTAB INDEX                 */ 00186000
         SLR   DINFINDX,DINFINDX                                02S0159 00187000
         IC    DINFINDX,CECBTINF-4(@14)                         02S0159 00188000
*     RFY                                                       02S0160 00189000
*       R15F UNRSTD;                /* FINISHED WITH LOOP COUNTER    */ 00190000
*   END;                            /* ECB INTERROGATED              */ 00191000
*                                                               01S0162 00192000
*   /*****************************************************************/ 00193000
*   /*                                                               */ 00194000
*   /* FURTHER REFINEMENT OF ERROR POSSIBLE IF VALID CSW             */ 00195000
*   /*                                                               */ 00196000
*   /*****************************************************************/ 00197000
*                                                               01S0162 00198000
*   IF DFECSW=BON THEN              /* IF CSW EXISTS                 */ 00199000
         TM    DFECSW,B'10000000'                               01S0162 00200000
         BNO   @RF00162                                         01S0162 00201000
*     DO;                           /* CONTINUE REFINEMENT.          */ 00202000
*                                                               01S0163 00203000
*       /*************************************************************/ 00204000
*       /*                                                           */ 00205000
*       /* CHECK THE CSW ERROR FLAGS (CSW BYTES 4-5)                 */ 00206000
*       /*                                                           */ 00207000
*       /*************************************************************/ 00208000
*                                                               01S0164 00209000
*       DO;                         /* START OF SEGMENT              */ 00210000
*                                                               02S0164 00211000
*         /***********************************************************/ 00212000
*         /*                                                         */ 00213000
*         /* BASED OF THE CSW BITS SET, DEVELOP AN INFTAB INDEX AND  */ 00214000
*         /* SET FURTHER PROCESSING SWITCHES. THE CSW BITS AND       */ 00215000
*         /* CHECKED IN THE SAME ORDER AS THE ERP AND ONLY THE MOST  */ 00216000
*         /* SEVERE ERROR REPORTED.                                  */ 00217000
*         /*                                                         */ 00218000
*         /***********************************************************/ 00219000
*                                                               02S0165 00220000
*         DMATCH(1:2)=DMATCH(1:2)&&DMATCH(1:2);/* 0-1 NOT USED       */ 00221000
         XC    DMATCH(2),DMATCH                                 02S0165 00222000
*         DMATCH(3:4)=CSWFLAGS;     /* UNIT/CHANNEL ERRORS           */ 00223000
         L     @10,APIOVCSW(,RAPIOPTR)                          02S0166 00224000
         MVC   DMATCH+2(2),CSWFLAGS(@10)                        02S0166 00225000
*         DLOOKPTR=ADDR(CCSWTAB);   /* LOOKUP TABLE FOR CSW          */ 00226000
         LA    DLOOKPTR,CCSWTAB                                 02S0167 00227000
*         CALL FNDMATCH;            /* LOOK FOR BIT-SET MATCH        */ 00228000
         BAL   @14,FNDMATCH                                     02S0168 00229000
*       END;                        /* CSW INTERROGATED              */ 00230000
*                                                               01S0170 00231000
*       /*************************************************************/ 00232000
*       /*                                                           */ 00233000
*       /* IF CCW ADDRESS IN THE CSW VALID, FORMAT THE CCW COMMAND   */ 00234000
*       /* CODE (BYTE-0 OF THE CCW).                                 */ 00235000
*       /*                                                           */ 00236000
*       /*************************************************************/ 00237000
*                                                               01S0170 00238000
*       DO;                         /* START OF SEGMENT              */ 00239000
*                                                               02S0170 00240000
*         /***********************************************************/ 00241000
*         /*                                                         */ 00242000
*         /* IF THE CCW ADDRESS IN THE CSW IS VALID, SAVE THE OP-CODE*/ 00243000
*         /* IN THE ERROR MESSAGE (APEM). MAKE THE COMMAND CODE      */ 00244000
*         /* PRINTABLE HEX AND FOLLOW BY A 1-3 CHARACTER DESCRIPTION */ 00245000
*         /* OF THE OPERATION (EG RD,WR).                            */ 00246000
*         /*                                                         */ 00247000
*         /***********************************************************/ 00248000
*                                                               02S0171 00249000
*         IF DFCCCW=BON&            /* IF CSW CAN HAVE VALID CCW AND */ 00250000
*             CSWCCW^=0 THEN        /* CCW ADDRESS NOT ZERO, THEN    */ 00251000
         TM    DFCCCW,B'10000000'                               02S0171 00252000
         BNO   @RF00171                                         02S0171 00253000
         L     @10,APIOVCSW(,RAPIOPTR)                          02S0171 00254000
         L     @10,CSWCCW-1(,@10)                               02S0171 00255000
         LA    @10,0(,@10)                                      02S0171 00256000
         LTR   @10,@10                                          02S0171 00257000
         BZ    @RF00171                                         02S0171 00258000
*           DO;                     /* PRINT IT                      */ 00259000
*             RFY                                               02S0173 00260000
*               R1P RSTD;           /* POINT TO LAST CCW.            */ 00261000
*             R1P=CSWCCW-           /* CSW HAS NEXT CCW ADDRESS      */ 00262000
*                 LENGTH(CCW);      /* SO BACK OFF BY 1 CCW          */ 00263000
         SL    @10,@CF00040                                     02S0174 00264000
         LR    R1P,@10                                          02S0174 00265000
*             RFY                                               02S0175 00266000
*               CCW BASED(R1P);     /* TELL PL/S                     */ 00267000
*                                                               02S0175 00268000
*             /*******************************************************/ 00269000
*             /*                                                     */ 00270000
*             /* OP CODE PRINTABLE HEX                               */ 00271000
*             /*                                                     */ 00272000
*             /*******************************************************/ 00273000
*                                                               02S0176 00274000
*             IF ADDR(CCW)<APIOGMAD|/* IF FAILING CCW NOT IN         */ 00275000
*                 ADDR(CCW)>=APIOGMAD+APIOGMS1/* ORIGINAL CHANNEL PGM*/ 00276000
*               THEN                /* (ERP FAILED, ETC.)            */ 00277000
         L     @10,APIOGMAD(,RAPIOPTR)                          02S0176 00278000
         CR    R1P,@10                                          02S0176 00279000
         BL    @RT00176                                         02S0176 00280000
         AH    @10,APIOGMS1(,RAPIOPTR)                          02S0176 00281000
         CR    R1P,@10                                          02S0176 00282000
         BL    @RF00176                                         02S0176 00283000
@RT00176 DS    0H                                               02S0177 00284000
*               APEMCCWC(1:2)='**'; /* THEN CANNOT LOCATE.           */ 00285000
         MVC   APEMCCWC(2,RAPEMPTR),@CC03313                    02S0177 00286000
*             ELSE                  /* IF FAILING CCW IN THE         */ 00287000
*               DO;                 /* USER'S PGM, THEN FORMAT       */ 00288000
         B     @RC00176                                         02S0178 00289000
@RF00176 DS    0H                                               02S0179 00290000
*                 UNPK(APEMCCWC(1:3),CCW(1:2));/* MAKE THE 1-BYTE CMD*/ 00291000
         UNPK  APEMCCWC(3,RAPEMPTR),CCW(2,R1P)                  02S0179 00292000
*                 TR(APEMCCWC(1:2),CCNVTHEX);/* CODE INTO 2     02S0180 00293000
*                                      CHARACTERS.                   */ 00294000
         TR    APEMCCWC(2,RAPEMPTR),CCNVTHEX                    02S0180 00295000
*               END;                /* CMD CODE FORMATTED            */ 00296000
*                                                               02S0181 00297000
*             /*******************************************************/ 00298000
*             /*                                                     */ 00299000
*             /* ENGLISH DESCRIPTION OP CCW OPCODE                   */ 00300000
*             /*                                                     */ 00301000
*             /*******************************************************/ 00302000
*                                                               02S0182 00303000
*             APEMCCWC(3:3)='-';    /* SEPERATOR                     */ 00304000
@RC00176 MVI   APEMCCWC+2(RAPEMPTR),C'-'                        02S0182 00305000
*             IF CCWOP=CWRITEOP THEN/* DESCRIBE OP CODE. FOLLOWING   */ 00306000
         CLI   CCWOP(R1P),1                                     02S0183 00307000
         BNE   @RF00183                                         02S0183 00308000
*               APEMCCWC(4:6)='WR ';/* VALUES ALLOWED --             */ 00309000
         MVC   APEMCCWC+3(3,RAPEMPTR),@CC03315                  02S0184 00310000
*             ELSE                  /* 01-WR WRITE                   */ 00311000
*               IF CCWOP=CREADOP THEN/* 02-RD READ                   */ 00312000
         B     @RC00183                                         02S0185 00313000
@RF00183 CLI   CCWOP(R1P),2                                     02S0185 00314000
         BNE   @RF00185                                         02S0185 00315000
*                 APEMCCWC(4:6)='RD ';/* 03-NOP CONTROL NO OPERATION */ 00316000
         MVC   APEMCCWC+3(3,RAPEMPTR),@CC03316                  02S0186 00317000
*               ELSE                /* OF-PRC PROCESS (CONTROL)      */ 00318000
*                 IF CCWOP=CNOPOP THEN/* ALL OTHER OPCODES ARE       */ 00319000
         B     @RC00185                                         02S0187 00320000
@RF00185 CLI   CCWOP(R1P),3                                     02S0187 00321000
         BNE   @RF00187                                         02S0187 00322000
*                   APEMCCWC(4:6)='NOP';/* INVALID, AND CAN NOT      */ 00323000
         MVC   APEMCCWC+3(3,RAPEMPTR),@CC03317                  02S0188 00324000
*                 ELSE              /* OCCUR. IF FOUND, USE          */ 00325000
*                   IF CCWOP=CPROCOP THEN/* 'UNK' FOR UNKNOWN.       */ 00326000
         B     @RC00187                                         02S0189 00327000
@RF00187 CLI   CCWOP(R1P),15                                    02S0189 00328000
         BNE   @RF00189                                         02S0189 00329000
*                     APEMCCWC(4:6)='PRC';                      02S0190 00330000
         MVC   APEMCCWC+3(3,RAPEMPTR),@CC03318                  02S0190 00331000
*                   ELSE                                        02S0191 00332000
*                     APEMCCWC(4:6)='UNK';                      02S0191 00333000
         B     @RC00189                                         02S0191 00334000
@RF00189 MVC   APEMCCWC+3(3,RAPEMPTR),@CC03319                  02S0191 00335000
*             RFY                                               02S0192 00336000
*               CCW;                /* OP CODE PROCESSED             */ 00337000
@RC00189 DS    0H                                               02S0192 00338000
@RC00187 DS    0H                                               02S0192 00339000
@RC00185 DS    0H                                               02S0192 00340000
@RC00183 DS    0H                                               02S0193 00341000
*             RFY                                               02S0193 00342000
*               R1P UNRSTD;         /* RELEASE PL/S RESOURCES        */ 00343000
*           END;                    /* VALID CCW COMMAND CODE PRINTED*/ 00344000
*       END;                        /* CCW PROCESSED                 */ 00345000
@RF00171 DS    0H                                               02S0196 00346000
*                                                               01S0196 00347000
*       /*************************************************************/ 00348000
*       /*                                                           */ 00349000
*       /* IF UNIT CHECK AND VALID SENSE (STANDARD USER-ERROR CASE), */ 00350000
*       /* CONTINUE THE ANALYSIS BY INSPECTING BYTES 0-1 OF THE SENSE*/ 00351000
*       /* DATA.                                                     */ 00352000
*       /*                                                           */ 00353000
*       /*************************************************************/ 00354000
*                                                               01S0196 00355000
*       IF DFCUCHK=BON&             /* IF UNIT-CHECK AND             */ 00356000
*           APIOVSNS=BON THEN       /* THE 3838 GAVE VALID SENSE,    */ 00357000
         TM    DFCUCHK,B'01000000'                              01S0196 00358000
         BNO   @RF00196                                         01S0196 00359000
         TM    APIOVSNS(RAPIOPTR),B'01000000'                   01S0196 00360000
         BNO   @RF00196                                         01S0196 00361000
*         DO;                       /* CONTINUE PROCESSING.          */ 00362000
*           DO;                     /* START OF SEGMENT              */ 00363000
*                                                               02S0198 00364000
*             /*******************************************************/ 00365000
*             /*                                                     */ 00366000
*             /* CHECK BYTES 0-1 OF SENSE. DEVELOP AN INFTAB INDEX   */ 00367000
*             /* AND SET FURTHER PROCESSING SWITCHES. THE SENSE DATA */ 00368000
*             /* IS CHECKED IN THE SAME ORDER AS THE ERP AND ONLY THE*/ 00369000
*             /* MOST SEVERE ERROR IS REPORTED.                      */ 00370000
*             /*                                                     */ 00371000
*             /*******************************************************/ 00372000
*                                                               02S0199 00373000
*             DMATCH(1:2)=DMATCH(1:2)&&DMATCH(1:2);/* 0-1 NOT USED   */ 00374000
         XC    DMATCH(2),DMATCH                                 02S0199 00375000
*             DMATCH(3:4)=APSN(1:2);/* CHECK STANDARD SENSE          */ 00376000
         MVC   DMATCH+2(2),APSN(RAPSNPTR)                       02S0200 00377000
*             DLOOKPTR=ADDR(CSN0TAB);/* LOOKUP TABLE FOR BYTES 0-1   */ 00378000
         LA    DLOOKPTR,CSN0TAB                                 02S0201 00379000
*             CALL FNDMATCH;        /* FIND BIT-MATCH ENTRY          */ 00380000
         BAL   @14,FNDMATCH                                     02S0202 00381000
*           END;                    /* SENSE BYTES 0-1 CHECKED       */ 00382000
*                                                               01S0204 00383000
*           /*********************************************************/ 00384000
*           /*                                                       */ 00385000
*           /* IF DATA DEPENDANT (USER) ERROR (FORMAT-2 SENSE),      */ 00386000
*           /* CONTINUE THE REFINEMENT.                              */ 00387000
*           /*                                                       */ 00388000
*           /*********************************************************/ 00389000
*                                                               01S0204 00390000
*           IF DFSUSER=BON&         /* IF BYTES 0-1 OF SENSE SHOW       00391000
*                                      DATA                          */ 00392000
*               APSNFMT=APSNFUSR THEN/* & FORMAT-2 SENSE GIVEN,      */ 00393000
         TM    DFSUSER,B'10000000'                              01S0204 00394000
         BNO   @RF00204                                         01S0204 00395000
         CLI   APSNFMT(RAPSNPTR),2                              01S0204 00396000
         BNE   @RF00204                                         01S0204 00397000
*             DO;                   /* THEN CHECK SENSE BYTES 8-11.  */ 00398000
*               DO;                 /* START OF SEGMENT              */ 00399000
*                                                               02S0206 00400000
*                 /***************************************************/ 00401000
*                 /*                                                 */ 00402000
*                 /* CHECK FORMAT-2 (DATA ERRORS) BYTES 8-11 OF THE  */ 00403000
*                 /* SENSE. DEVELOP AN INFTAB INDEX AND SET FURTHER  */ 00404000
*                 /* PROCESSING SWITCHES. IF MORE THAN 1 ERROR IS    */ 00405000
*                 /* DETECTED, ONLY ONE IS REPORTED.                 */ 00406000
*                 /*                                                 */ 00407000
*                 /***************************************************/ 00408000
*                                                               02S0207 00409000
*                 MVC(DMATCH,APSNFMT2);/* SENSE BYTES 8-11           */ 00410000
         MVC   DMATCH(4),APSNFMT2(RAPSNPTR)                     02S0207 00411000
*                 DLOOKPTR=ADDR(CSNUTAB);/* LOOKUP TABLE USER SENSE     00412000
*                                      ERROR                         */ 00413000
