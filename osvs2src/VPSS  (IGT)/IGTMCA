*   /*                                                               */ 00678000
*   /*****************************************************************/ 00679000
*                                                               01S0262 00680000
*   DO;                             /* START OF SEGMENT              */ 00681000
*                                                               02S0262 00682000
*     /***************************************************************/ 00683000
*     /*                                                             */ 00684000
*     /* MOVE APRL ERROR VALUES TO THE APCW. ALSO SET RETURN CODE,   */ 00685000
*     /* AND TYPE OF ERROR USING 'DINFINDX' TO INDEX INTO ERROR      */ 00686000
*     /* TABLE.                                                      */ 00687000
*     /*                                                             */ 00688000
*     /***************************************************************/ 00689000
*                                                               02S0263 00690000
*     APCWR15B=APCCRERR;            /* REGISTER-15 ERROR CODE (ALWAYS   00691000
*                                      8).                           */ 00692000
