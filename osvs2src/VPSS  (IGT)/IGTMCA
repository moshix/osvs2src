*                                                               02S0220 00464000
         LA    R1P,4(,R1P)                                      02S0220 00465000
*             /*******************************************************/ 00466000
*             /*                                                     */ 00467000
*             /* COUNT NUMBER OF ENTRIES TO FAILING ONE. WHEN        */ 00468000
*             /* COMPLETE, R15P SHOWS WHICH ENTRY FAILED (NTH        */ 00469000
*             /* OCCURANCE OF THE OP CODE). FOR EXAMPLE IF R15=3 AND */ 00470000
*             /* THE FAILING OPERATION WAS AN SVE, THIS MEANS THE 3RD*/ 00471000
*             /* SVE IN THE USER'S REQUEST FAILED.                   */ 00472000
*             /*                                                     */ 00473000
*             /*******************************************************/ 00474000
*                                                               02S0221 00475000
*             R15P=1;               /* ALSO INCLUDE FAILING ENTRY    */ 00476000
