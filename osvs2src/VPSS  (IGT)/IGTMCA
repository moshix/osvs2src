*       DO;                         /* BY IGTKBA, THEN DO NOW.       */ 00650000
*         APRLRCC=APCCI;            /* ERROR COMPONENT IS ALWAYS I/O.*/ 00651000
         MVI   APRLRCC(@10),X'05'                               02S0255 00652000
*         APRLRCT=CINFRCT(DINFINDX);/* RETURN CODE TYPE FROM ARRAY.     00653000
*                                      (LOGICAL/INVALID_CNTRL_BLK)   */ 00654000
