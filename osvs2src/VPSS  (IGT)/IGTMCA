*                 CALL FNDMATCH;    /* CHECK BITS                    */ 00415000
         BAL   @14,FNDMATCH                                     02S0209 00416000
*               END;                /* SENSE BYTES 8-11 CHECKED      */ 00417000
*             END;                  /* FORMAT-2 SENSE DATA CHECKED   */ 00418000
*         END;                      /* SENSE CHECKED FOR ERROR INDIC */ 00419000
*                                                               01S0212 00420000
@RF00204 DS    0H                                               01S0213 00421000
*       /*************************************************************/ 00422000
*       /*                                                           */ 00423000
*       /* DESCRIBE THE APCI ENTRY THAT FAILED. (REQUIRES VALID      */ 00424000
*       /* SENSE). AVAILABILITY OF SENSE IN APIO IF CSW VALID.       */ 00425000
*       /*                                                           */ 00426000
*       /*************************************************************/ 00427000
*                                                               01S0213 00428000
*       DO;                         /* START OF SEGMENT              */ 00429000
*                                                               02S0213 00430000
@RF00196 DS    0H                                               02S0214 00431000
*         /***********************************************************/ 00432000
*         /*                                                         */ 00433000
*         /* IF THE SENSE DATA DESCRIBES THE FAILING OPERATION       */ 00434000
*         /* (APSNCIDS ^= 0), DETERMINE THE NUMBER OF THE ENTRY AND  */ 00435000
*         /* THE USER'S MNEMONIC WHICH CREATED THE ENTRY.            */ 00436000
*         /*                                                         */ 00437000
*         /***********************************************************/ 00438000
*                                                               02S0214 00439000
*         IF APIOVSNS=BON&          /* INSURE SENSE IS VALID         */ 00440000
