*   END;                            /* APCW ERROR CODES UPDATED      */ 00719000
*                                                               01S0271 00720000
*   /*****************************************************************/ 00721000
*   /*                                                               */ 00722000
*   /* PROCESSING COMPLETE. RETURN TO THE CALLER.                    */ 00723000
*   /*                                                               */ 00724000
*   /*****************************************************************/ 00725000
*                                                               01S0271 00726000
*   RETURN;                         /* EXIT -- PROCESSING COMPLETE   */ 00727000
@EL00001 L     @13,4(,@13)                                      01S0271 00728000
@EF00001 L     @00,@SIZDATD                                     01S0271 00729000
         LR    @01,@06                                          01S0271 00730000
         IGTYFSV  R,LV=(0),A=(1)                                        00731000
@ER00001 LM    @14,@12,12(@13)                                  01S0271 00732000
         BR    @14                                              01S0271 00733000
         EJECT                                                          00734000
*FNDMATCH:                          /* FIND DLOOKUP ENTRY AND        */ 00735000
*   PROC;                           /* SET DINFINDX AND DFLAGS.      */ 00736000
*                                                               01S0272 00737000
FNDMATCH STM   @14,@10,12(@13)                                  01S0272 00738000
         ST    @12,68(,@13)                                     01S0272 00739000
*   /*****************************************************************/ 00740000
*   /*                                                               */ 00741000
*   /* SCAN THE 'DLOOKUP' TABLE TO FIND A 'DLOOKFND' WITH A BIT SET  */ 00742000
*   /* THAT IS ALSO SET IS 'DMATCH'. IF A MATCH IS FOUND, SAVE THE   */ 00743000
*   /* CORRESPONDING 'DLOOKINX' AND 'DLOOKFLG' VALUES IN 'DINFINDX'  */ 00744000
*   /* AND 'DFLAGS'. IF A MATCH IS NOT FOUND, SET 'DFLAGS' TO ZERO,  */ 00745000
*   /* BUT DO NOT CHANGE 'DINFINDX'. THE 'DLOOKUP' TABLE IS POINTED  */ 00746000
*   /* TO BY 'DLOOKPTR'. THE END OF THE TABLE IS DENOTED BY A        */ 00747000
*   /* 'DLOOKINX' VALUE OF ALL ZEROS.                                */ 00748000
*   /*                                                               */ 00749000
*   /*****************************************************************/ 00750000
*                                                               01S0273 00751000
*   RFY                                                         01S0273 00752000
*    (R0F,                          /* HOLDS DMATCH                  */ 00753000
*     R1P) RSTD;                    /* DLOOKUP SCANNER               */ 00754000
*   R0F=DMATCH;                                                 01S0274 00755000
         L     R0F,DMATCH                                       01S0274 00756000
*   RFY                                                         01S0275 00757000
*     DLOOKUP BASED(R1P);           /* SCAN TABLE BASE               */ 00758000
*   DO R1P=DLOOKPTR BY LENGTH(DLOOKUP)/* SCAN DLOOKUP UNTIL          */ 00759000
*         WHILE(((DMATCH&DLOOKFND)=0)&/* BIT-SET FOUND               */ 00760000
*         (DLOOKFND^=0));           /* OR END OF TABLE               */ 00761000
         LR    R1P,DLOOKPTR                                     01S0276 00762000
         B     @DE00276                                         01S0276 00763000
@DL00276 DS    0H                                               01S0277 00764000
*   END;                            /* CONTINUE LOOP                 */ 00765000
         AL    R1P,@CF00040                                     01S0277 00766000
@DE00276 SLR   @10,@10                                          01S0277 00767000
         L     @02,DLOOKFND(,R1P)                               01S0277 00768000
         L     @15,DMATCH                                       01S0277 00769000
         NR    @15,@02                                          01S0277 00770000
         CLR   @15,@10                                          01S0277 00771000
         BNE   @DC00276                                         01S0277 00772000
         CLR   @02,@10                                          01S0277 00773000
         BNE   @DL00276                                         01S0277 00774000
@DC00276 DS    0H                                               01S0278 00775000
*   IF DLOOKFND=0 THEN              /* IF NO MATCH FOUND,            */ 00776000
         SLR   @10,@10                                          01S0278 00777000
         CL    @10,DLOOKFND(,R1P)                               01S0278 00778000
         BNE   @RF00278                                         01S0278 00779000
*     DFLAGS=0;                     /* SET FLAGS TO ZERO.            */ 00780000
         STH   @10,DFLAGS                                       01S0279 00781000
*   ELSE                            /* IF MATCH FOUND,               */ 00782000
*     DO;                                                       01S0280 00783000
         B     @RC00278                                         01S0280 00784000
@RF00278 DS    0H                                               01S0281 00785000
*       DFLAGS=DLOOKFLG;            /* SAVE NEW FLAGS AND            */ 00786000
         MVC   DFLAGS(2),DLOOKFLG(R1P)                          01S0281 00787000
*       DINFINDX=DLOOKINX;          /* IMPROVE MESSAGE RESOLUTION.   */ 00788000
