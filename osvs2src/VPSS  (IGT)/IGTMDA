*     DESCRVEC=DESCRVEC&&DESCRVEC;  /* ZERO DESCRIPTOR WORK AREA     */ 00075000
         XC    DESCRVEC(88,DESCRPTR),DESCRVEC(DESCRPTR)         01S0060 00076000
*   END;                                                        01S0061 00077000
*                                                               01S0061 00078000
*   /*****************************************************************/ 00079000
*   /*                                                               */ 00080000
*   /* BUILD HEADER PORTION OF ALGORITHM CIT ENTRY                   */ 00081000
*   /*                                                               */ 00082000
