*     APCIAV3=APCIAV3&&APCIAV3;     /* ZERO U VECTOR                 */ 00071000
         XC    APCIAV3(16,@09),APCIAV3(@09)                     01S0058 00072000
*     DESCRPTR=ADDR(DESCRWRK);      /* POINT TO DESCRIPTOR WORK AREA */ 00073000
