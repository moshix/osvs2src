*             APCWRCN=APCCZ004;     /* GIVE ERROR NUMBER             */ 00363000
         MVI   APCWRCN(APCWPTR),X'04'                           01S0108 00364000
*             APCWZAM=ON;           /* INDICATE ERROR DETECTED       */ 00365000
