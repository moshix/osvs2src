TMDC     TITLE 'IGTMDC - PROCESS NTH ZERO CROSSING REQUEST             *00001000
                        '                                               00002000
IGTMDC   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTMDC    77.231'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
         MVC   @PC00001(4),0(@01)                               01S0001 00022000
*   APCWPTR=ADDR(DAPCW);            /* GET APCW PTR                  */ 00023000
         L     APCWPTR,@PC00001                                 01S0046 00024000
*   RFY                                                         01S0047 00025000
*     APCW BASED(APCWPTR);          /* MAP APCW                      */ 00026000
*   RFY                                                         01S0048 00027000
*     APEM BASED(ADDR(APCWEMSG));   /* MAP APEM - MSG FORMAT         */ 00028000
*   INPUTPTR=APCWAPIN;              /* POINTER TO USER PARAMETER LIST*/ 00029000
         L     INPUTPTR,APCWAPIN(,APCWPTR)                      01S0049 00030000
*   RFY                                                         01S0050 00031000
*     APCI BASED(ADDR(CITENTRY(1)));/* MAP APCI ON WORKAREA          */ 00032000
*   RFY                                                         01S0051 00033000
*     APIN BASED(INPUTPTR);         /* MAP APIN - CALL FORMAT        */ 00034000
*   RFY                                                         01S0052 00035000
*     APAL BASED(APCWAPAL);         /* MAP APAL - SWITCHES           */ 00036000
*                                                               01S0052 00037000
*   /*****************************************************************/ 00038000
*   /*                                                               */ 00039000
*   /* ZERO CIT WORKAREA                                             */ 00040000
*   /*                                                               */ 00041000
*   /*****************************************************************/ 00042000
*                                                               01S0053 00043000
*   APCIZDTA=APCIZDTA&&APCIZDTA;    /* ZERO NZC CIT WORKAREA         */ 00044000
*                                                               01S0053 00045000
         LA    @09,CITENTRY                                     01S0053 00046000
         XC    APCIZDTA(36,@09),APCIZDTA(@09)                   01S0053 00047000
*   /*****************************************************************/ 00048000
*   /*                                                               */ 00049000
*   /* BUILD HEADER PORTION OF CIT ENTRY                             */ 00050000
*   /*                                                               */ 00051000
*   /*****************************************************************/ 00052000
*                                                               01S0054 00053000
*                                                               02S0054 00054000
*/********************************************************************/ 00055000
*/*                                                                  */ 00056000
*/* THIS SEGMENT INSERTS THE OP CODE AND ENTRY LENGTH(HALFWORDS)     */ 00057000
*/* INTO THE APPROPRIATE SLOTS IN THE CIT ENTRY.                     */ 00058000
*/*                                                                  */ 00059000
*/********************************************************************/ 00060000
*                                                               02S0054 00061000
*   DO;                             /* BUILD NZC CIT HEADER          */ 00062000
*     APCIOPID=APCWAPOP;            /* INSERT OP CODE                */ 00063000
         MVC   APCIOPID(2,@09),APCWAPOP(APCWPTR)                02S0055 00064000
*     APCIOPSZ=APALCISZ;            /* INSERT ENTRY LENGTH(HALFWORDS */ 00065000
         L     @03,APCWAPAL(,APCWPTR)                           02S0056 00066000
         LH    @03,APALCISZ(,@03)                               02S0056 00067000
         STH   @03,APCIOPSZ(,@09)                               02S0056 00068000
*   END;                            /* END HEADER PROCESSING         */ 00069000
*                                                               01S0058 00070000
*   /*****************************************************************/ 00071000
*   /*                                                               */ 00072000
*   /* VALIDATE RESULT INDEXES IVAL1 AND IVAL2. INSERT INTO CIT IF OK*/ 00073000
*   /*                                                               */ 00074000
*   /*****************************************************************/ 00075000
*                                                               01S0058 00076000
*                                                               02S0058 00077000
*/********************************************************************/ 00078000
*/*                                                                  */ 00079000
*/* THIS SEGMENT VERIFIES THAT THE RESULT INDEXES IVAL1 AND IVAL2    */ 00080000
*/* AS PASSED BY THE USER ARE VALID AND EXCLUSIVE. IF INCORRECT, THE */ 00081000
*/* STANDARD VPSS ERROR INDICATORS ARE SET AND A RETURN IS MADE TO   */ 00082000
*/* THE CALLER. IF OK, INSERT THE INDEX IDS INTO THE CIT ENTRY.      */ 00083000
*/*                                                                  */ 00084000
*/********************************************************************/ 00085000
*                                                               02S0058 00086000
*   DO;                             /* VALIDATE & INSERT RESULT INDEX   00087000
*                                      IDS INTO CIT ENTRY            */ 00088000
*                                                               02S0058 00089000
*     /***************************************************************/ 00090000
*     /*                                                             */ 00091000
*     /* ESTABLISH ADDRESSABILITY ON FIRST FOUR PARAMETERS. PARM1 IS */ 00092000
*     /* IFUNC, PARM2 IS IUNIT, PARM3 IS IVAL1, PARM4 IS IVAL2. NOTE */ 00093000
*     /* THAT THE IUNIT AND IFUNC HAVE ALREADY BEEN VALIDATED BY THE */ 00094000
*     /* CALL ROUTER                                                 */ 00095000
*     /*                                                             */ 00096000
*     /***************************************************************/ 00097000
*                                                               02S0059 00098000
*     RFY                                                       02S0059 00099000
*       PARM1 BASED(APINAP1);       /* APINAP1 POINTS TO IUNIT. PARM1   00100000
*                                      IS NOW IUNIT                  */ 00101000
*     RFY                                                       02S0060 00102000
*       PARM2 BASED(APINAP2);       /* APINAP2 POINTS TO IFUNC. PARM2   00103000
*                                      IS NOW IFUNC                  */ 00104000
*     RFY                                                       02S0061 00105000
*       PARM3 BASED(APINAP3);       /* APINAP3 POINTS TO IVAL1. PARM3   00106000
*                                      IS NOW IVAL1                  */ 00107000
*     RFY                                                       02S0062 00108000
*       PARM4 BASED(APINAP4);       /* APINAP4 POINTS TO IFUNC. PARM4   00109000
*                                      IS NOW IVAL2                  */ 00110000
*                                                               02S0062 00111000
*     /***************************************************************/ 00112000
*     /*                                                             */ 00113000
*     /* VALIDATE IVAL1 - INSERT INTO CIT IF VALID                   */ 00114000
*     /*                                                             */ 00115000
*     /***************************************************************/ 00116000
*                                                               02S0063 00117000
*     IF(PARM3<1)|(PARM3>15) THEN   /* VALIDITY CHECK IVAL1 RANGE    */ 00118000
         L     @09,APINAP3(,INPUTPTR)                           02S0063 00119000
         L     @09,PARM3(,@09)                                  02S0063 00120000
         LTR   @09,@09                                          02S0063 00121000
         BNP   @RT00063                                         02S0063 00122000
         C     @09,@CF00109                                     02S0063 00123000
         BNH   @RF00063                                         02S0063 00124000
@RT00063 DS    0H                                               02S0064 00125000
*       DO;                         /* IVAL1 IS NOT A VALID INDEX ID    00126000
*                                      - SET ERROR INDICATORS        */ 00127000
*         APCWRCN=APCCA018;         /* SET ERROR NUMBER FOR IVAL1 OUT   00128000
*                                      OF RANGE                      */ 00129000
         MVI   APCWRCN(APCWPTR),X'12'                           02S0065 00130000
*         APEMMSGN=APCMA018;        /* SET MESSAGE NUMBER FOR OUT OF    00131000
*                                      RANGE                         */ 00132000
         LA    @09,112                                          02S0066 00133000
         ALR   @09,APCWPTR                                      02S0066 00134000
         MVC   APEMMSGN(4,@09),@CF00089                         02S0066 00135000
*         APEMVALN=APCMK038;        /* SET KEYWORD NUMBER FOR IVAL1  */ 00136000
         MVC   APEMVALN(4,@09),@CF00208                         02S0067 00137000
*         CALL NZCERR;              /* NZCERR TO SET COMMON FLAGS AND   00138000
*                                      INDICATORS                    */ 00139000
         BAL   @14,NZCERR                                       02S0068 00140000
*         RETURN;                   /* RETURN TO CALLER              */ 00141000
@EL00001 L     @13,4(,@13)                                      02S0069 00142000
@EF00001 L     @00,@SIZDATD                                     02S0069 00143000
         LR    @01,@06                                          02S0069 00144000
         IGTYFSV  R,LV=(0),A=(1)                                        00145000
@ER00001 LM    @14,@12,12(@13)                                  02S0069 00146000
         BR    @14                                              02S0069 00147000
*       END;                        /* END INVALID IVAL1 PROCESSING  */ 00148000
*     ELSE                          /* IVAL1 IS VALID INDEX ID       */ 00149000
*       APCIZX1=PARM3;              /* INSERT IVAL1 INTO CIT ENTRY   */ 00150000
*                                                               02S0071 00151000
@RF00063 LA    @09,CITENTRY                                     02S0071 00152000
         L     @03,APINAP3(,INPUTPTR)                           02S0071 00153000
         L     @03,PARM3(,@03)                                  02S0071 00154000
         STH   @03,APCIZX1(,@09)                                02S0071 00155000
*     /***************************************************************/ 00156000
*     /*                                                             */ 00157000
*     /* VALIDATE IVAL2 - INSERT INTO CIT IF VALID                   */ 00158000
*     /*                                                             */ 00159000
*     /***************************************************************/ 00160000
*                                                               02S0072 00161000
*     IF(PARM4<1)|(PARM4>15) THEN   /* VALIDITY CHECK IVAL2 RANGE    */ 00162000
         L     @09,APINAP4(,INPUTPTR)                           02S0072 00163000
         L     @09,PARM4(,@09)                                  02S0072 00164000
         LTR   @09,@09                                          02S0072 00165000
         BNP   @RT00072                                         02S0072 00166000
         C     @09,@CF00109                                     02S0072 00167000
         BNH   @RF00072                                         02S0072 00168000
@RT00072 DS    0H                                               02S0073 00169000
*       DO;                         /* IVAL2 IS NOT A VALID INDEX ID    00170000
*                                      - SET ERROR INDICATORS        */ 00171000
*         APCWRCN=APCCA019;         /* SET ERROR NUMBER FOR IVAL2 OUT   00172000
*                                      OF RANGE                      */ 00173000
         MVI   APCWRCN(APCWPTR),X'13'                           02S0074 00174000
*         APEMMSGN=APCMA019;        /* SET MESSAGE NUMBER FOR OUT OF    00175000
*                                      RANGE                         */ 00176000
         LA    @09,112                                          02S0075 00177000
         ALR   @09,APCWPTR                                      02S0075 00178000
         MVC   APEMMSGN(4,@09),@CF00089                         02S0075 00179000
*         APEMVALN=APCMK039;        /* SET KEYWORD NUMBER FOR IVAL2  */ 00180000
         MVC   APEMVALN(4,@09),@CF00210                         02S0076 00181000
*         CALL NZCERR;              /* NZCERR TO SET COMMON FLAGS AND   00182000
*                                      INDICATORS                    */ 00183000
         BAL   @14,NZCERR                                       02S0077 00184000
*         RETURN;                   /* RETURN TO CALLER              */ 00185000
         B     @EL00001                                         02S0078 00186000
*       END;                        /* END INVALID IVAL2 PROCESSING  */ 00187000
*     ELSE                          /* IVAL2 IS VALID INDEX ID       */ 00188000
*       IF(PARM4=PARM3) THEN        /* IF IVAL2 = IVAL1, THE CALL IS    00189000
*                                      IN ERROR SINCE RESULTS ARE       00190000
*                                      RETURNED TO THESE INDEXES     */ 00191000
@RF00072 L     @09,APINAP4(,INPUTPTR)                           02S0080 00192000
         L     @03,APINAP3(,INPUTPTR)                           02S0080 00193000
         CLC   PARM4(4,@09),PARM3(@03)                          02S0080 00194000
         BNE   @RF00080                                         02S0080 00195000
*         DO;                       /* ERROR - IVAL2 = IVAL1, SET       00196000
*                                      ERROR INDICATORS FOR IVAL2       00197000
*                                      INVALID VALUE                 */ 00198000
*           APCWRCN=APCCA023;       /* SET RETURN CODE NUMBER FOR       00199000
*                                      IVAL2 INVALID VALUE           */ 00200000
         MVI   APCWRCN(APCWPTR),X'17'                           02S0082 00201000
*           APEMMSGN=APCMA023;      /* SET MSG. NO. FOR INVALID VALUE*/ 00202000
         LA    @09,112                                          02S0083 00203000
         ALR   @09,APCWPTR                                      02S0083 00204000
         MVC   APEMMSGN(4,@09),@CF00040                         02S0083 00205000
*           APEMVALN=APCMK039;      /* SET KYWD. NO. FOR IVAL2       */ 00206000
         MVC   APEMVALN(4,@09),@CF00210                         02S0084 00207000
*           CALL NZCERR;            /* NZCERR SETS COMMON FLAGS &       00208000
*                                      VALUES                        */ 00209000
         BAL   @14,NZCERR                                       02S0085 00210000
*           RETURN;                 /* RETURN TO CALLER              */ 00211000
         B     @EL00001                                         02S0086 00212000
*         END;                      /* END IVAL2 = IVAL1 ERROR  02S0087 00213000
*                                      PROCESSING                    */ 00214000
*       ELSE                        /* GOOD INDEXES - IVAL2 NE IVAL1 */ 00215000
*         APCIZX2=PARM4;            /* MOVE IVAL2 FROM USER PARAMETER   00216000
*                                      LIST TO CIT                   */ 00217000
@RF00080 LA    @09,CITENTRY                                     02S0088 00218000
         L     @03,APINAP4(,INPUTPTR)                           02S0088 00219000
         L     @03,PARM4(,@03)                                  02S0088 00220000
         STH   @03,APCIZX2(,@09)                                02S0088 00221000
*   END;                            /* END RESULT INDEX PROCESSING   */ 00222000
*                                                               01S0090 00223000
*   /*****************************************************************/ 00224000
*   /*                                                               */ 00225000
*   /* BUMP INPUT POINTER TO ACCESS NEXT FOUR PARAMETERS. PARM1 WILL */ 00226000
*   /* CONTAIN IFMX - X VECTOR FORMAT SPECIFICATION. PARM2 WILL      */ 00227000
*   /* CONTAIN XARRV - X VECTOR BULK STORE ADDRESS. PARM3 WILL       */ 00228000
*   /* CONTAIN ICTXV - X VECTOR LENGTH. PARM4 WILL POINT TO EITHER   */ 00229000
*   /* XARRVX - BULK STORE ADDRESS INDEX, ICTXX - X VECTOR COUNT     */ 00230000
*   /* INDEX, OR IFMU - U VECTOR FORMAT SPECIFICATION DEPENDING ON   */ 00231000
*   /* WHAT IS SPECIFIED IN IFMX.                                    */ 00232000
*   /*                                                               */ 00233000
*   /*****************************************************************/ 00234000
*                                                               01S0090 00235000
*   INPUTPTR=INPUTPTR+16;           /* BUMP INPUT POINTER - NOW 01S0090 00236000
*                                      POINTS TO 5TH PARAMETER - IFMX*/ 00237000
*                                                               01S0090 00238000
         AL    INPUTPTR,@CF00111                                01S0090 00239000
*   /*****************************************************************/ 00240000
*   /*                                                               */ 00241000
*   /* VALIDATE X VECTOR PARAMETERS AND BUILD X VECTOR PORTION OF CIT*/ 00242000
*   /* ENTRY IF ALL PARAMETERS ARE VALID                             */ 00243000
*   /*                                                               */ 00244000
*   /*****************************************************************/ 00245000
*                                                               01S0091 00246000
*                                                               02S0091 00247000
*/********************************************************************/ 00248000
*/*                                                                  */ 00249000
*/* THIS SEGMENT VERIFIES INPUT PARAMETERS WHICH APPLY TO THE X      */ 00250000
*/* VECTOR AND INSERTS THE APPROPRIATE VALUES INTO THE NZC CIT ENTRY */ 00251000
*/*                                                                  */ 00252000
*/********************************************************************/ 00253000
*                                                               02S0091 00254000
*   DO;                             /* VALIDATE & INSERT X VECTOR       00255000
*                                      PARMS INTO CIT ENTRY          */ 00256000
*     RFY                                                       02S0092 00257000
*       IFMFMT BASED(ADDR(PARM1));  /* MAP IFMX                      */ 00258000
*                                                               02S0092 00259000
*     /***************************************************************/ 00260000
*     /*                                                             */ 00261000
*     /* VERIFY NO INVALID BITS ARE SET IN IFMX FOR THIS OPERATION   */ 00262000
*     /*                                                             */ 00263000
*     /***************************************************************/ 00264000
*                                                               02S0093 00265000
*     IF((DELTANDX=ON)&(APALXFXD=OFF))|((COUNTNDX=ON)&(APALXFXC=OFF))|( 00266000
*         (STARTNDX=ON)&(APALXFXS=OFF))|((APALXFSO=OFF)&(FORCENEG=ON|   00267000
*         REVSIGN=ON|FORCEPOS=ON))|((FXPTTRUE=ON)&(APALXFFT=OFF))|((    00268000
*         FXPTTWOS=ON)&(APALXFFX=OFF)) THEN/* DETERMINE IF INVALID      00269000
*                                      BITS ARE SET IN IFMX USING THE   00270000
*                                      SWITCHES SET IN APAL FOR NZC  */ 00271000
         L     @09,APINAP1(,INPUTPTR)                           02S0093 00272000
         TM    DELTANDX(@09),B'10000000'                        02S0093 00273000
         BNO   @GL00007                                         02S0093 00274000
         L     @09,APCWAPAL(,APCWPTR)                           02S0093 00275000
         TM    APALXFXD(@09),B'00000001'                        02S0093 00276000
         BZ    @RT00093                                         02S0093 00277000
@GL00007 L     @09,APINAP1(,INPUTPTR)                           02S0093 00278000
         TM    COUNTNDX(@09),B'01000000'                        02S0093 00279000
         BNO   @GL00006                                         02S0093 00280000
         L     @09,APCWAPAL(,APCWPTR)                           02S0093 00281000
         TM    APALXFXC(@09),B'00000010'                        02S0093 00282000
         BZ    @RT00093                                         02S0093 00283000
@GL00006 L     @09,APINAP1(,INPUTPTR)                           02S0093 00284000
         TM    STARTNDX(@09),B'00100000'                        02S0093 00285000
         BNO   @GL00005                                         02S0093 00286000
         L     @09,APCWAPAL(,APCWPTR)                           02S0093 00287000
         TM    APALXFXS(@09),B'00000100'                        02S0093 00288000
         BZ    @RT00093                                         02S0093 00289000
@GL00005 L     @09,APCWAPAL(,APCWPTR)                           02S0093 00290000
         TM    APALXFSO(@09),B'00001000'                        02S0093 00291000
         BNZ   @GL00004                                         02S0093 00292000
         L     @09,APINAP1(,INPUTPTR)                           02S0093 00293000
         TM    FORCENEG(@09),B'00011100'                        02S0093 00294000
         BNZ   @RT00093                                         02S0093 00295000
@GL00004 L     @09,APINAP1(,INPUTPTR)                           02S0093 00296000
         TM    FXPTTRUE(@09),B'00000010'                        02S0093 00297000
         BNO   @GL00003                                         02S0093 00298000
         L     @09,APCWAPAL(,APCWPTR)                           02S0093 00299000
         TM    APALXFFT(@09),B'00100000'                        02S0093 00300000
         BZ    @RT00093                                         02S0093 00301000
@GL00003 L     @09,APINAP1(,INPUTPTR)                           02S0093 00302000
         TM    FXPTTWOS(@09),B'00000001'                        02S0093 00303000
         BNO   @RF00093                                         02S0093 00304000
         L     @09,APCWAPAL(,APCWPTR)                           02S0093 00305000
         TM    APALXFFX(@09),B'01000000'                        02S0093 00306000
         BNZ   @RF00093                                         02S0093 00307000
@RT00093 DS    0H                                               02S0094 00308000
*       DO;                         /* IFMX IS INVALID - SET ERROR      00309000
*                                      INDICATORS AND RETURN         */ 00310000
*         APCWRCN=APCCA013;         /* SET ERROR NUMBER FOR IFMX        00311000
*                                      INVALID                       */ 00312000
         MVI   APCWRCN(APCWPTR),X'0D'                           02S0095 00313000
*         APEMMSGN=APCMA013;        /* SET MESSAGE NUMBER FOR INVALID   00314000
*                                      VALUE                         */ 00315000
         LA    @09,112                                          02S0096 00316000
         ALR   @09,APCWPTR                                      02S0096 00317000
         MVC   APEMMSGN(4,@09),@CF00040                         02S0096 00318000
*         APEMVALN=APCMK023;        /* SET KEYWORD NUMBER FOR IFMX   */ 00319000
         MVC   APEMVALN(4,@09),@CF00125                         02S0097 00320000
*         CALL NZCERR;              /* NZCERR TO SET COMMON FLAGS AND   00321000
*                                      INDICATORS                    */ 00322000
         BAL   @14,NZCERR                                       02S0098 00323000
*         RETURN;                   /* RETURN TO CALLER              */ 00324000
         B     @EL00001                                         02S0099 00325000
*       END;                        /* END INVALID IFMX PROCESSING   */ 00326000
*     ELSE                                                      02S0101 00327000
*       ;                           /* IFMX IS OK                    */ 00328000
*                                                               02S0101 00329000
@RF00093 DS    0H                                               02S0102 00330000
*     /***************************************************************/ 00331000
*     /*                                                             */ 00332000
*     /* SET APPROPRIATE FLAGS IN CIT FOR IFMX FLAGS                 */ 00333000
*     /*                                                             */ 00334000
*     /***************************************************************/ 00335000
*                                                               02S0102 00336000
*     DO;                           /* SET FORMAT SPECIFICATION FLAGS*/ 00337000
*       IF STARTNDX=ON THEN         /* IS START INDEX USE INDICATED? */ 00338000
         L     @09,APINAP1(,INPUTPTR)                           02S0103 00339000
         TM    STARTNDX(@09),B'00100000'                        02S0103 00340000
         BNO   @RF00103                                         02S0103 00341000
*         USESTART=ON;              /* YES - SET FLAG                */ 00342000
         OI    USESTART,B'10000000'                             02S0104 00343000
*       ELSE                        /* START INDEX NOT USED          */ 00344000
*         USESTART=OFF;             /* CLEAR FLAG                    */ 00345000
         B     @RC00103                                         02S0105 00346000
@RF00103 NI    USESTART,B'01111111'                             02S0105 00347000
*       IF COUNTNDX=ON THEN         /* IS COUNT INDEX USE INDICATED? */ 00348000
@RC00103 L     @09,APINAP1(,INPUTPTR)                           02S0106 00349000
         TM    COUNTNDX(@09),B'01000000'                        02S0106 00350000
         BNO   @RF00106                                         02S0106 00351000
*         USECOUNT=ON;              /* YES - SET FLAG                */ 00352000
         OI    USECOUNT,B'10000000'                             02S0107 00353000
*       ELSE                        /* COUNT INDEX NOT USED          */ 00354000
*         USECOUNT=OFF;             /* CLEAR FLAG                    */ 00355000
         B     @RC00106                                         02S0108 00356000
@RF00106 NI    USECOUNT,B'01111111'                             02S0108 00357000
*       IF FORCENEG=ON THEN         /* IS FORCE NEGATIVE REQUESTED   */ 00358000
@RC00106 L     @09,APINAP1(,INPUTPTR)                           02S0109 00359000
         TM    FORCENEG(@09),B'00010000'                        02S0109 00360000
         BNO   @RF00109                                         02S0109 00361000
*         APCIZFN1=ON;              /* YES - SET BIT IN CIT          */ 00362000
         LA    @09,CITENTRY                                     02S0110 00363000
         OI    APCIZFN1(@09),B'00100000'                        02S0110 00364000
*       IF REVSIGN=ON THEN          /* IS REVERSE SIGN REQUESTED?    */ 00365000
@RF00109 L     @09,APINAP1(,INPUTPTR)                           02S0111 00366000
         TM    REVSIGN(@09),B'00001000'                         02S0111 00367000
         BNO   @RF00111                                         02S0111 00368000
*         APCIZRS1=ON;              /* YES - SET BIT IN CIT          */ 00369000
         LA    @09,CITENTRY                                     02S0112 00370000
         OI    APCIZRS1(@09),B'10000000'                        02S0112 00371000
*       IF FORCEPOS=ON THEN         /* IS FORCE POSITIVE REQUESTED   */ 00372000
@RF00111 L     @09,APINAP1(,INPUTPTR)                           02S0113 00373000
         TM    FORCEPOS(@09),B'00000100'                        02S0113 00374000
         BNO   @RF00113                                         02S0113 00375000
*         APCIZFP1=ON;              /* YES - SET BIT IN CIT          */ 00376000
         LA    @09,CITENTRY                                     02S0114 00377000
         OI    APCIZFP1(@09),B'01000000'                        02S0114 00378000
*       IF FXPTTRUE=ON THEN         /* IS X FIXED POINT SIGNED TRUE? */ 00379000
@RF00113 L     @09,APINAP1(,INPUTPTR)                           02S0115 00380000
         TM    FXPTTRUE(@09),B'00000010'                        02S0115 00381000
         BNO   @RF00115                                         02S0115 00382000
*         APCIZST1=ON;              /* YES - SET BIT IN CIT          */ 00383000
         LA    @09,CITENTRY                                     02S0116 00384000
         OI    APCIZST1(@09),B'00000100'                        02S0116 00385000
*       IF FXPTTWOS=ON THEN         /* IS X FIXED POINT 2'S COMP?    */ 00386000
@RF00115 L     @09,APINAP1(,INPUTPTR)                           02S0117 00387000
