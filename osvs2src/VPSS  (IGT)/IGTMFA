*NDEXBULK:                                                      02S0072 00119000
*       DO;                         /* SUBCASE INDEX - BULK STORE       00120000
*                                      TRANSFER                      */ 00121000
NDEXBULK DS    0H                                               02S0073 00122000
*/*********************************************************          */ 00123000
*/*                                                                  */ 00124000
*/* THIS CASE BUILDS A CIT ENTRY FOR INDEX - BULK STORE              */ 00125000
*/* TRANSFERS. USER CALL FORMAT FOR INDEX - BULK STORE               */ 00126000
*/* TRANSFERS IS:                                                    */ 00127000
*/*                                                                  */ 00128000
*/*         CALL VPSS(IUNIT,IFUNC,YARRV,ICTY,YARRVX,IVAL1)           */ 00129000
*/*                                                                  */ 00130000
*/*         WHERE:                                                   */ 00131000
*/*                                                                  */ 00132000
*/*         IUNIT - UNIT REFERENCE NUMBER                            */ 00133000
*/*         IFUNC - FUNCTION MNEMONIC -                              */ 00134000
*/*                   'XMVX' - MOVE INDEX TO BULK STORE              */ 00135000
*/*                   'XMVS' - MOVE BULK STORE TO INDEX              */ 00136000
*/*        YARRV - BULK STORE ADDRESS                                */ 00137000
*/*        ICTY  - NUMBER OF INDEXES TO LOAD/STORE                   */ 00138000
*/*        YARRVX- BULK STORE ADDRESS INDEX                          */ 00139000
*/*        IVAL1 - FIRST INDEX TO LOAD/STORE                         */ 00140000
*/*                                                                  */ 00141000
*/* THE CIT FORMAT FOR INDEX - BULK STORE TRANSFER IS:               */ 00142000
*/*                                                                  */ 00143000
*/*      <-------------- 4 BYTES ----------------->                  */ 00144000
*/*      ------------------------------------------                  */ 00145000
*/*      |  OPERATION ID     |ENTRY LENGTH(HWORDS)|                  */ 00146000
*/*      ------------------------------------------                  */ 00147000
*/*      |  FIRST INDEX ID   | LAST INDEX ID        |                */ 00148000
*/*      ------------------------------------------                  */ 00149000
*/*      |            BULK STORE ADDRESS          |                  */ 00150000
*/*      ------------------------------------------                  */ 00151000
*/*      |  BULK STORE INDEX |      UNUSED          |                */ 00152000
*/*      ____________________________________________                */ 00153000
*/*                                                                  */ 00154000
*/*********************************************************          */ 00155000
*                                                               02S0073 00156000
*         DO;                       /* BUILD CIT ENTRY FOR INDEX/BULK   00157000
*                                      STORE TRANSFER OPERATIONS     */ 00158000
*           IF(APINXID>0)&(APINXID<=15) THEN/* IVAL1 VALIDITY TEST   */ 00159000
         L     @04,APCWAPIN(,APCWPTR)                           02S0074 00160000
         L     @01,APINAXID(,@04)                               02S0074 00161000
         L     @15,APINXID(,@01)                                02S0074 00162000
         LTR   @15,@15                                          02S0074 00163000
         BNP   @RF00074                                         02S0074 00164000
         LA    @14,15                                           02S0074 00165000
         CR    @15,@14                                          02S0074 00166000
         BH    @RF00074                                         02S0074 00167000
*             DO;                   /* IVAL1 OK - CONTINUE           */ 00168000
*               APCIXID1=APINXID;   /* MOVE IVAL1 FROM USER PARM LIST   00169000
*                                      TO CIT WORK AREA              */ 00170000
         LA    @11,CITENTRY                                     02S0076 00171000
         STH   @15,APCIXID1(,@11)                               02S0076 00172000
*               APCIXID2=APCIXID1+APINXCT-1;/* CALCULATE LAST INDEX     00173000
*                                      TO TRANSFER USING IVAL1 AND      00174000
*                                      ICTY                          */ 00175000
         L     @01,APINAXCT(,@04)                               02S0077 00176000
