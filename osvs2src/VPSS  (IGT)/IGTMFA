         SLA   @04,3                                            02S0253 00969000
         L     @01,APCWACB(,APCWPTR)                            02S0253 00970000
         L     @01,ACBAMWAP(,@01)                               02S0253 00971000
         L     @15,APAXAPLB(,@01)                               02S0253 00972000
         ST    @04,@TF00001                                     02S0253 00973000
         ALR   @04,@15                                          02S0253 00974000
         TM    APLBGOOD-8(@04),B'11000000'                      02S0253 00975000
         BNO   @RF00253                                         02S0253 00976000
         L     @04,APCWAPIN(,APCWPTR)                           02S0253 00977000
         L     @04,APINADLB(,@04)                               02S0253 00978000
