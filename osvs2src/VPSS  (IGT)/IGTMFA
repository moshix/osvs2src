         SLA   @11,3                                            02S0214 00797000
         L     @04,APCWACB(,APCWPTR)                            02S0214 00798000
         L     @04,ACBAMWAP(,@04)                               02S0214 00799000
         L     @15,APAXAPLB(,@04)                               02S0214 00800000
         ST    @11,@TF00001                                     02S0214 00801000
         ALR   @11,@15                                          02S0214 00802000
         TM    APLBGOOD-8(@11),B'10000000'                      02S0214 00803000
         BNZ   @RF00214                                         02S0214 00804000
*                           DO;     /* NO                            */ 00805000
*                             APLBGOOD(I)=ON;/* SHOW ENTRY AS VALID  */ 00806000
         LR    @11,@15                                          02S0216 00807000
         AL    @11,@TF00001                                     02S0216 00808000
         OI    APLBGOOD-8(@11),B'10000000'                      02S0216 00809000
*                             APLBLBL(I)=APINXLB;/* INSERT LABEL     */ 00810000
         LR    @11,@15                                          02S0217 00811000
         AL    @11,@TF00001                                     02S0217 00812000
         L     @01,APCWAPIN(,APCWPTR)                           02S0217 00813000
         L     @01,APINAXLB(,@01)                               02S0217 00814000
         MVC   APLBLBL-8(4,@11),APINXLB(@01)                    02S0217 00815000
*                             APLBDEF(I)=OFF;/* MARK AS REFERENCE    */ 00816000
         LR    @11,@15                                          02S0218 00817000
         AL    @11,@TF00001                                     02S0218 00818000
         NI    APLBDEF-8(@11),B'10111111'                       02S0218 00819000
*                             APLBLOC(I)=(APAXCIN-APAXCIS)/2;/* 02S0219 00820000
*                                      INSERT HALFWORD OFFSET OF THIS   00821000
*                                      CIT ENTRY                     */ 00822000
         L     @00,APAXCIN(,@04)                                02S0219 00823000
         SL    @00,APAXCIS(,@04)                                02S0219 00824000
         SRDA  @00,32                                           02S0219 00825000
         D     @00,@CF00064                                     02S0219 00826000
         L     @11,@TF00001                                     02S0219 00827000
