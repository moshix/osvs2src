*                                      RANGE                         */ 00890000
         LA    @04,112                                          02S0240 00891000
         ALR   @04,APCWPTR                                      02S0240 00892000
         MVC   APEMMSGN(4,@04),@CF00628                         02S0240 00893000
*               APEMVALN=APCMK038;  /* SET KEYWORD VALUE FOR IVAL2   */ 00894000
         MVC   APEMVALN(4,@04),@CF01148                         02S0241 00895000
*             END;                                              02S0242 00896000
*         END;                                                  02S0243 00897000
*         GOTO CE0001@;             /* BRANCH TO END OF CASE         */ 00898000
         B     CE0001@                                          02S0244 00899000
*       END CDBRANCH;               /* END OF SUBCASE                */ 00900000
*XGO:                                                           02S0246 00901000
*       DO;                         /* SUBCASE                       */ 00902000
XGO      DS    0H                                               02S0247 00903000
*/* THIS CASE BUILDS A CIT ENTRY FOR THE UNCONDITIONAL               */ 00904000
*/* BRANCH OPERATION. THE CALL FORMAT FOR AN UNCONDITIONAL           */ 00905000
*/* BRANCH IS:                                                       */ 00906000
*/*                                                                  */ 00907000
*/*       CALL VPSS(IUNIT,'XGO ',LABEL)                              */ 00908000
*/*                                                                  */ 00909000
*/*       WHERE:                                                     */ 00910000
*/*                                                                  */ 00911000
*/*         IUNIT - UNIT REFERENCE NUMBER                            */ 00912000
*/*         XGO   - FUNCTION MNEMONIC                                */ 00913000
*/*         LABEL - BRANCH TO LOCATION - DEFINED BY 'XID '           */ 00914000
*/*                                                                  */ 00915000
*/* THE CIT FORMAT FOR AN UNCONDITIONAL BRANCH IS:                   */ 00916000
*/*                                                                  */ 00917000
*/*       <-------------- 4 BYTES ----------------->                 */ 00918000
*/*       ------------------------------------------                 */ 00919000
*/*       |   OPERATION ID    |ENTRY LENGTH(HWORDS)|                 */ 00920000
*/*       ------------------------------------------                 */ 00921000
*/*       |  ANY INDEX ID     |     UNUSED         |                 */ 00922000
*/*       ------------------------------------------                 */ 00923000
*/*       |                UNUSED                  |                 */ 00924000
*/*       ------------------------------------------                 */ 00925000
*/*       |  CONDITION MASK   |REL CIT OFFSET-HWORD|                 */ 00926000
*/*       ------------------------------------------                 */ 00927000
*/*                                                                  */ 00928000
*/*   CONDITION MASK IS:                                             */ 00929000
*/*     X'FFFF' - UNCONDITIONAL BRANCH                               */ 00930000
*/*                                                                  */ 00931000
*/************************************************************       */ 00932000
*                                                               02S0247 00933000
*         DO;                                                   02S0247 00934000
*           APCIXCC=APCIXCGO;       /* INSERT CONDITION MASK INTO THE   00935000
*                                      CIT ENTRY                     */ 00936000
         LA    @11,CITENTRY                                     02S0248 00937000
         MVC   APCIXCC(2,@11),@CB00470                          02S0248 00938000
*           APCIXID1=1;             /* SUPPLY AN INDEX TO KEEP CP       00939000
*                                      HAPPY                         */ 00940000
*                                                               02S0249 00941000
         LA    @04,1                                            02S0249 00942000
         STH   @04,APCIXID1(,@11)                               02S0249 00943000
*           /*********************************************************/ 00944000
*           /*                                                       */ 00945000
*           /* SEARCH THE LABEL TABLE TO SEE IF THE LABEL HAS BEEN   */ 00946000
*           /* DEFINED. IF SO, GET THE BRANCH TO OFFSET AND INSERT IT*/ 00947000
*           /* INTO THE CIT.                                         */ 00948000
*           /*                                                       */ 00949000
*           /*********************************************************/ 00950000
*                                                               02S0250 00951000
*           LOOPCTRL=OFF;           /* INITIALIZE LOOP CONTROL FLAG     00952000
*                                      TO OFF                        */ 00953000
