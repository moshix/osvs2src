         BH    @RF00084                                         02S0084 00217000
*                         APCIXBSX=APINXBX;/* YARRVX OK - MOVE TO CIT   00218000
*                                      WORKAREA                      */ 00219000
         LA    @04,CITENTRY                                     02S0085 00220000
         STH   @15,APCIXBSX(,@04)                               02S0085 00221000
*                       ELSE        /* YARRVX INVALID - TELL USER    */ 00222000
*                         DO;       /* YARRVX ERROR PROCESSING       */ 00223000
         B     @RC00084                                         02S0086 00224000
@RF00084 DS    0H                                               02S0087 00225000
*                           ERRFLAG=ON;/* FLAG ERROR TO MAIN SEGMENT */ 00226000
         LA    ERRFLAG,1                                        02S0087 00227000
*                           APCWRCN=APCCX008;/* ERROR NUMBER FOR OUT    00228000
*                                      OF RANGE - YARRVX             */ 00229000
         MVI   APCWRCN(APCWPTR),X'08'                           02S0088 00230000
*                           APEMMSGN=APCMX008;/* MESSAGE NUMBER FOR     00231000
*                                      OUT OF RANGE                  */ 00232000
         LA    @04,112                                          02S0089 00233000
         ALR   @04,APCWPTR                                      02S0089 00234000
         MVC   APEMMSGN(4,@04),@CF00628                         02S0089 00235000
*                           APEMVALN=APCMK020;/* KEYWORD NUMBER FOR     00236000
*                                      YARRVX                        */ 00237000
         MVC   APEMVALN(4,@04),@CF00961                         02S0090 00238000
*                         END;      /* END OF YARRVX ERROR PROCESSING*/ 00239000
*                     END;          /* END OF SEGMENT                */ 00240000
*                   ELSE            /* MIF RETURNED ERROR FROM  02S0093 00241000
*                                      ATTEMPT TO RESOLVE THE BULK      00242000
*                                      STORE ADDRESS                 */ 00243000
*                     DO;           /* YARRV ERROR PROCESSING        */ 00244000
         B     @RC00082                                         02S0093 00245000
@RF00082 DS    0H                                               02S0094 00246000
*                       ERRFLAG=ON; /* FLAG ERROR TO MAIN            */ 00247000
         LA    ERRFLAG,1                                        02S0094 00248000
*                       APEMVALN=APCMK017;/* KEYWORD NUMBER FOR 02S0095 00249000
*                                      YARRVX                        */ 00250000
         LA    @04,112                                          02S0095 00251000
         ALR   @04,APCWPTR                                      02S0095 00252000
         MVC   APEMVALN(4,@04),@CF01068                         02S0095 00253000
*                       IF RSOLVERR=BDYERROR THEN/* TEST IF ERROR       00254000
*                                      BECAUSE YARRV WAS NOT ALIGNED    00255000
*                                      ON A FULLWORD BULKSTORE  02S0096 00256000
*                                      BOUNDARY                      */ 00257000
