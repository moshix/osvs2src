*                             LOOPCTRL=ON;/* GET OUT OF LOOP         */ 00829000
         LA    @11,1                                            02S0220 00830000
         LR    LOOPCTRL,@11                                     02S0220 00831000
*                             GOODONE=ON;/* INDICATE SUCCESS         */ 00832000
         LR    GOODONE,@11                                      02S0221 00833000
*                           END;                                02S0222 00834000
*                         ELSE                                  02S0223 00835000
*                           ;                                   02S0223 00836000
@RF00214 DS    0H                                               02S0224 00837000
*                       END;                                    02S0224 00838000
         AL    I,@CF00061                                       02S0224 00839000
@DE00213 L     @11,APCWACB(,APCWPTR)                            02S0224 00840000
         L     @11,ACBAMWAP(,@11)                               02S0224 00841000
         L     @11,APAXAPLB(,@11)                               02S0224 00842000
         C     I,APLBNUMB(,@11)                                 02S0224 00843000
         BNH   @DL00213                                         02S0224 00844000
@DC00213 DS    0H                                               02S0225 00845000
*                     END;                                      02S0225 00846000
*                   ELSE                                        02S0226 00847000
*                     ;                                         02S0226 00848000
*                                                               02S0226 00849000
@RF00210 DS    0H                                               02S0227 00850000
*                   /*************************************************/ 00851000
*                   /*                                               */ 00852000
*                   /* IF UNABLE TO MAKE ENTRY INTO THE LABEL TABLE, */ 00853000
*                   /* IT IS FULL AND AN ERROR EXISTS.               */ 00854000
*                   /*                                               */ 00855000
*                   /*************************************************/ 00856000
*                                                               02S0227 00857000
*                   IF GOODONE=OFF THEN/* TEST SUCCESS INDICATOR     */ 00858000
         LTR   GOODONE,GOODONE                                  02S0227 00859000
         BNZ   @RF00227                                         02S0227 00860000
*                     DO;           /* UNSUCCESSFUL - SET ERROR 02S0228 00861000
*                                      INDICATORS                    */ 00862000
*                       ERRFLAG=ON; /* SET ERROR FLAG                */ 00863000
         LA    ERRFLAG,1                                        02S0229 00864000
*                       APCWRCN=APCCX004;/* SET ERROR NUMBER         */ 00865000
         MVI   APCWRCN(APCWPTR),X'04'                           02S0230 00866000
