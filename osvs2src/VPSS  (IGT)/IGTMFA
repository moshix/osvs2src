*                       APEMVALN=APCMK051;/* SET KEYWORD NUMBER FOR     00872000
*                                      APLB                          */ 00873000
         MVC   APEMVALN(4,@04),@CF01173                         02S0232 00874000
*                     END;                                      02S0233 00875000
*                   ELSE                                        02S0234 00876000
*                     ;                                         02S0234 00877000
@RF00227 DS    0H                                               02S0235 00878000
*                 END;                                          02S0235 00879000
*             END;                                              02S0236 00880000
*           ELSE                    /* IVAL1 INVALID                 */ 00881000
*             DO;                                               02S0237 00882000
         B     @RC00164                                         02S0237 00883000
@RF00164 DS    0H                                               02S0238 00884000
*               ERRFLAG=ON;         /* SET ERROR FLAG                */ 00885000
