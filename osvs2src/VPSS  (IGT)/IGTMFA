*                 GOODONE=ON;       /* INDICATE SUCCESS              */ 00992000
         LA    GOODONE,1                                        02S0257 00993000
*               END;                                            02S0258 00994000
*             ELSE                                              02S0259 00995000
*               ;                                               02S0259 00996000
@RF00253 DS    0H                                               02S0260 00997000
*           END;                                                02S0260 00998000
*                                                               02S0260 00999000
